11:21:09 562  WHEN PAYPAL_ALREADY_INACTIVE THEN
11:21:09 563  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.STATE_ERROR,
11:21:09 564  	  SPROC_NAME, 'Paypal already inactive');
11:21:09 565  WHEN PENDING_TRANSACTIONS_FOUND THEN
11:21:09 566  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.STATE_ERROR,
11:21:09 567  	  SPROC_NAME, 'Account has pending charge which are using this paypal');
11:21:09 568  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
11:21:09 569  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 570  	  SPROC_NAME, 'Could not get current default financial instrument', EXCEPTION_MESSAGE);
11:21:09 571  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
11:21:09 572  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 573  	  SPROC_NAME, 'Could not delete information about default financial instrument from account', EXCEPTION_MESSAGE);
11:21:09 574  WHEN OTHERS THEN
