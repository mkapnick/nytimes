11:21:08 1160  WHEN OTHERS THEN
11:21:08 1161  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:08 1162  	   SPROC_NAME, 'Unknown error', SQLERRM);
11:21:08 1163  END GET_BILLING_CYCLE;
11:21:08 1164  
11:21:08 1165  /******************************************************************************/
11:21:08 1166  
11:21:08 1167  PROCEDURE REFUND_SUBSCRIPTION (
11:21:08 1168  /*
11:21:08 1169  Throws exceptions:
11:21:08 1170  APP_EXCEPTION_CODES_V18.NOT_FOUND
11:21:08 1171  APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR
11:21:08 1172  */
11:21:08 1173  	 in_subscription_id IN NUMBER,
11:21:08 1174  	 in_invoice_id	    IN NUMBER,
11:21:08 1175  	 in_refund_amount   IN NUMBER,
11:21:08 1176  	 in_note	    IN VARCHAR2,
11:21:08 1177  	 in_created_by	    IN VARCHAR2,
11:21:08 1178  	 out_charge_id	    OUT NUMBER
11:21:08 1179  ) AS
11:21:08 1180  -- VARIABLES
11:21:08 1181  SPROC_NAME	      CONSTANT VARCHAR2(19) := 'REFUND_SUBSCRIPTION';
11:21:08 1182  var_invoice_status_id  NUMBER;
11:21:08 1183  var_account_id	      NUMBER;
11:21:08 1184  var_account_status_id  NUMBER;
11:21:08 1185  var_new_transaction_id NUMBER;
11:21:08 1186  var_instrument_type_id NUMBER;
11:21:08 1187  var_instrument_id      NUMBER;
11:21:08 1188  var_new_charge_id      NUMBER;
11:21:08 1189  var_invoice_amount     NUMBER(10,2);
11:21:08 1190  var_refunds_before     NUMBER(10,2);
11:21:08 1191  var_charges_amount     NUMBER(10,2);
11:21:08 1192  -- EXCEPTIONS
11:21:08 1193  BAD_SUBSCRIPTION_ID	     EXCEPTION;
11:21:08 1194  ACCOUNT_IS_FROZEN	     EXCEPTION;
11:21:08 1195  BAD_INVOICE_ID		     EXCEPTION;
11:21:08 1196  CAN_NOT_CREATE_TRANSACTION    EXCEPTION;
11:21:08 1197  CAN_NOT_CREATE_CHARGE	     EXCEPTION;
11:21:08 1198  CAN_NOT_CALC_INVOICE_AMOUNT   EXCEPTION;
11:21:08 1199  REFUND_IS_GREATER_THAN_ANOUNT EXCEPTION;
11:21:08 1200  CAN_NOT_ANNOTATE_SUBSCRIPTION EXCEPTION;
11:21:08 1201  TOT_REF_IS_GREATER_THAN_ANOUNT EXCEPTION;
11:21:08 1202  INVOICE_IS_NOT_CLOSED	     EXCEPTION;
11:21:08 1203  TOT_REF_IS_GRATER_THAN_CHARGES EXCEPTION;
11:21:08 1204  EXCEPTION_MESSAGE	      VARCHAR2(1024);
11:21:08 1205  BEGIN
11:21:08 1206  
11:21:08 1207  	 BEGIN
11:21:08 1208  	   SELECT
11:21:08 1209  	     SUBSCRIPTION.INSTRUMENT_ID,
11:21:08 1210  	     SUBSCRIPTION.INSTRUMENT_TYPE_ID,
11:21:08 1211  	     SUBSCRIPTION.ACCOUNT_ID
11:21:08 1212  	     into
11:21:08 1213  	     var_instrument_id,
11:21:08 1214  	     var_instrument_type_id,
11:21:08 1215  	     var_account_id
11:21:08 1216  	   FROM
11:21:08 1217  	     SUBSCRIPTION
11:21:08 1218  	   WHERE
11:21:08 1219  	     SUBSCRIPTION.ID = in_subscription_id;
11:21:08 1220  	   EXCEPTION
11:21:08 1221  	     WHEN NO_DATA_FOUND THEN
11:21:08 1222  	       RAISE BAD_SUBSCRIPTION_ID;
11:21:08 1223  	 END;
11:21:08 1224  
11:21:08 1225  	 -- Check account status. It should not to be frozen
11:21:08 1226  	 SELECT
11:21:08 1227  	   ACCOUNT.ACCOUNT_STATUS_ID into var_account_status_id
11:21:08 1228  	 FROM
11:21:08 1229  	   ACCOUNT
11:21:08 1230  	 WHERE
11:21:08 1231  	   ACCOUNT.ID = var_account_id;
11:21:08 1232  
11:21:08 1233  	 IF var_account_status_id = GLOBAL_STATUSES_V18.ACCOUNT_FROZEN THEN
11:21:08 1234  	   RAISE ACCOUNT_IS_FROZEN;
11:21:08 1235  	 END IF;
11:21:08 1236  
11:21:08 1237  	 BEGIN
11:21:08 1238  	   SELECT
11:21:08 1239  	     INVOICE.INVOICE_STATUS_ID into var_invoice_status_id
11:21:08 1240  	   FROM
11:21:08 1241  	     INVOICE
11:21:08 1242  	   WHERE
11:21:08 1243  	     INVOICE.ID = in_invoice_id;
11:21:08 1244  	   EXCEPTION
11:21:08 1245  	     WHEN NO_DATA_FOUND THEN
11:21:08 1246  	       RAISE BAD_INVOICE_ID;
11:21:08 1247  	 END;
11:21:08 1248  
11:21:08 1249  	 IF var_invoice_status_id != GLOBAL_STATUSES_V18.INVOICE_CLOSED THEN
11:21:08 1250  	   RAISE INVOICE_IS_NOT_CLOSED;
11:21:08 1251  	 END IF;
11:21:08 1252  
11:21:08 1253  	 BEGIN
11:21:08 1254  	   PROCS_INVOICE_V18.CALCULATE_INVOICE_AMOUNT (
11:21:08 1255  	     in_invoice_id => in_invoice_id,
11:21:08 1256  	     out_amount    => var_invoice_amount
11:21:08 1257  	   );
11:21:08 1258  	   EXCEPTION
11:21:08 1259  	     WHEN OTHERS THEN
11:21:08 1260  	       EXCEPTION_MESSAGE := SQLERRM;
11:21:08 1261  	       RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
11:21:08 1262  	 END;
11:21:08 1263  
11:21:08 1264  	 IF ( in_refund_amount > var_invoice_amount ) THEN
11:21:08 1265  	   RAISE REFUND_IS_GREATER_THAN_ANOUNT;
11:21:08 1266  	 END IF;
11:21:08 1267  
11:21:08 1268  	 SELECT /*+ STAR_TRANSFORMATION */
11:21:08 1269  	   SUM(CHARGE.CHARGE_AMOUNT) into var_refunds_before
11:21:08 1270  	 FROM
11:21:08 1271  	   CHARGE
11:21:08 1272  	 WHERE
11:21:08 1273  	   CHARGE.INVOICE_ID = in_invoice_id
11:21:08 1274  	   AND (
11:21:08 1275  	     CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V18.CHARGE_OPENED
11:21:08 1276  	     OR CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V18.CHARGE_PROCESSED
11:21:08 1277  	   )
11:21:08 1278  	   AND CHARGE.CHARGE_AMOUNT < 0;
11:21:08 1279  
11:21:08 1280  	 -- Refunds are negative
11:21:08 1281  	 var_refunds_before := -var_refunds_before;
11:21:08 1282  
11:21:08 1283  	 var_charges_amount := 0;
11:21:08 1284  
11:21:08 1285  	 FOR f_processed_charges IN (
11:21:08 1286  	   SELECT
11:21:08 1287  	     CHARGE.CHARGE_AMOUNT
11:21:08 1288  	   FROM
11:21:08 1289  	     CHARGE
11:21:08 1290  	   WHERE
11:21:08 1291  	     CHARGE.INVOICE_ID = in_invoice_id
11:21:08 1292  	     AND CHARGE.CHARGE_STATUS_ID IN (SELECT GLOBAL_STATUSES_V18.CHARGE_PROCESSED FROM DUAL)
11:21:08 1293  	 )
11:21:08 1294  	 LOOP
11:21:08 1295  	   IF f_processed_charges.CHARGE_AMOUNT > 0 THEN
11:21:08 1296  	     var_charges_amount := var_charges_amount + f_processed_charges.CHARGE_AMOUNT;
11:21:08 1297  	   END IF;
11:21:08 1298  	 END LOOP;
11:21:08 1299  
11:21:08 1300  	 IF (in_refund_amount + var_refunds_before > var_invoice_amount) THEN
11:21:08 1301  	   RAISE TOT_REF_IS_GREATER_THAN_ANOUNT;
11:21:08 1302  	 END IF;
11:21:08 1303  
11:21:08 1304  	 IF (in_refund_amount + var_refunds_before > var_charges_amount) THEN
11:21:08 1305  	   RAISE TOT_REF_IS_GRATER_THAN_CHARGES;
11:21:08 1306  	 END IF;
11:21:08 1307  
11:21:08 1308  	 BEGIN
11:21:08 1309  	   PROCS_TRANSACTION_V18.CREATE_TRANSACTION(
11:21:08 1310  	     in_transaction_id	       => NULL,
11:21:08 1311  	     in_status_id	       => GLOBAL_STATUSES_V18.TRANSACTION_PREPARE,
11:21:08 1312  	     in_amount		       => -in_refund_amount,
11:21:08 1313  	     in_created_by	       => in_created_by,
11:21:08 1314  	     in_order_id	       => NULL,
11:21:08 1315  	     in_is_refund	       => GLOBAL_CONSTANTS_V18.TRUE,
11:21:08 1316  	     in_transaction_type_code  => 'REFUND',
11:21:08 1317  	     out_transaction_id        => var_new_transaction_id
11:21:08 1318  	   );
11:21:08 1319  	   EXCEPTION
11:21:08 1320  	     WHEN OTHERS THEN
11:21:08 1321  	       EXCEPTION_MESSAGE := SQLERRM;
11:21:08 1322  	       RAISE CAN_NOT_CREATE_TRANSACTION;
11:21:08 1323  	 END;
11:21:08 1324  
11:21:08 1325  	 BEGIN
11:21:08 1326  	   PROCS_CHARGE_V18.CREATE_CHARGE(
11:21:08 1327  	     in_invoice_id	   => in_invoice_id,
11:21:08 1328  	     in_transaction_id	   => var_new_transaction_id,
11:21:08 1329  	     in_instrument_type_id => var_instrument_type_id,
11:21:08 1330  	     in_instrument_id	   => var_instrument_id,
11:21:08 1331  	     in_charge_amount	   => -in_refund_amount,
11:21:08 1332  	     in_created_by	   => in_created_by,
11:21:08 1333  	     in_charge_status_id   => GLOBAL_STATUSES_V18.CHARGE_OPENED,
11:21:08 1334  	     out_charge_id	   => var_new_charge_id
11:21:08 1335  	   );
11:21:08 1336  	   EXCEPTION
11:21:08 1337  	     WHEN OTHERS THEN
11:21:08 1338  	       EXCEPTION_MESSAGE := SQLERRM;
11:21:08 1339  	       RAISE CAN_NOT_CREATE_CHARGE;
11:21:08 1340  	 END;
11:21:08 1341  
11:21:08 1342  	 IF in_note IS NOT NULL THEN
11:21:08 1343  	   BEGIN
11:21:08 1344  	     PROCS_SUBSCRIPTION_V18.ANNOTATE_SUBSCRIPTION(
11:21:08 1345  	       in_subscription_id => in_subscription_id,
11:21:08 1346  	       in_agent_id	  => 0, -- AGENT_ID??
11:21:08 1347  	       in_note		  => in_note,
11:21:08 1348  	       in_created_by	  => in_created_by
11:21:08 1349  	     );
11:21:08 1350  	     EXCEPTION
11:21:08 1351  	       WHEN OTHERS THEN
11:21:08 1352  		 EXCEPTION_MESSAGE := SQLERRM;
11:21:08 1353  		 RAISE CAN_NOT_ANNOTATE_SUBSCRIPTION;
11:21:08 1354  	   END;
11:21:08 1355  	 END IF;
11:21:08 1356  
11:21:08 1357  	 out_charge_id := var_new_charge_id;
11:21:08 1358  
11:21:08 1359  EXCEPTION
11:21:08 1360  WHEN INVOICE_IS_NOT_CLOSED THEN
11:21:08 1361  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.STATE_ERROR,
11:21:08 1362  	   SPROC_NAME, 'Invoice is not closed');
11:21:08 1363  WHEN BAD_SUBSCRIPTION_ID THEN
11:21:08 1364  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:08 1365  	   SPROC_NAME, 'No such subscription');
11:21:08 1366  WHEN ACCOUNT_IS_FROZEN THEN
11:21:08 1367  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.STATE_ERROR,
11:21:08 1368  	   SPROC_NAME, 'Could not refund subscription for frozen account');
11:21:08 1369  WHEN BAD_INVOICE_ID THEN
11:21:08 1370  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:08 1371  	   SPROC_NAME, 'No such invoice');
11:21:08 1372  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
11:21:08 1373  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:08 1374  	   SPROC_NAME, 'Could not calculate invoice amount', EXCEPTION_MESSAGE);
11:21:08 1375  WHEN REFUND_IS_GREATER_THAN_ANOUNT THEN
11:21:08 1376  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INVALID_PARAMETER,
11:21:08 1377  	   SPROC_NAME, 'Refund is greater than amount');
11:21:08 1378  WHEN TOT_REF_IS_GREATER_THAN_ANOUNT THEN
11:21:08 1379  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INVALID_PARAMETER,
11:21:08 1380  	   SPROC_NAME, 'There were refunds before and sum of all refunds and new refund more than invoice amount');
11:21:08 1381  WHEN TOT_REF_IS_GRATER_THAN_CHARGES THEN
11:21:08 1382  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INVALID_PARAMETER,
11:21:08 1383  	   SPROC_NAME, 'Total refund amount is greater than sum of processed charges');
11:21:08 1384  WHEN CAN_NOT_CREATE_TRANSACTION THEN
11:21:08 1385  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:08 1386  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
11:21:08 1387  WHEN CAN_NOT_CREATE_CHARGE THEN
11:21:08 1388  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:08 1389  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
11:21:08 1390  WHEN CAN_NOT_ANNOTATE_SUBSCRIPTION THEN
11:21:08 1391  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:08 1392  	   SPROC_NAME, 'Could not annotate subscription', EXCEPTION_MESSAGE);
11:21:08 1393  WHEN OTHERS THEN
11:21:08 1394  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:08 1395  	   SPROC_NAME, 'Unknown error', SQLERRM);
11:21:08 1396  END REFUND_SUBSCRIPTION;
