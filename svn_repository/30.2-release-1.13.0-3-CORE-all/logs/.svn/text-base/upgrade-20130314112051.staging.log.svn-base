11:21:08 659  
11:21:08 660  PROCEDURE GET_SUBSCRIPTION_INFO (
11:21:08 661  /*
11:21:08 662  Throws exceptions:
11:21:08 663  APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR
11:21:08 664  */
11:21:08 665  	  in_subscription_id  IN  NUMBER,
11:21:08 666  	  out_result_set      OUT SYS_REFCURSOR
11:21:08 667  ) AS
11:21:08 668  SPROC_NAME CONSTANT VARCHAR2(21) := 'GET_SUBSCRIPTION_INFO';
11:21:08 669  -- VARIABLES
11:21:08 670  temp_subscription_id NUMBER;
11:21:08 671  -- EXCEPTIONS
11:21:08 672  BAD_SUBSCRIPTION_ID EXCEPTION;
11:21:08 673  BEGIN
11:21:08 674  	-- Find subscription by id
11:21:08 675  	-- Return its details
11:21:08 676  
11:21:08 677  	BEGIN
11:21:08 678  	  SELECT
11:21:08 679  	    SUBSCRIPTION.ID into temp_subscription_id
11:21:08 680  	  FROM
11:21:08 681  	    SUBSCRIPTION
11:21:08 682  	  WHERE
11:21:08 683  	    SUBSCRIPTION.ID = in_subscription_id;
11:21:08 684  	  EXCEPTION
11:21:08 685  	    WHEN NO_DATA_FOUND THEN
11:21:08 686  	      RAISE BAD_SUBSCRIPTION_ID;
11:21:08 687  	END;
11:21:08 688  
11:21:08 689  	OPEN out_result_set FOR
11:21:08 690  	SELECT
11:21:08 691  	  SUBSCRIPTION.ID AS "SUBSCRIPTION_ID",
11:21:08 692  	  SUBSCRIPTION.SUBSCRIPTION_STATUS_ID,
11:21:08 693  	  SUBSCRIPTION.PURCHASE_DATE,
11:21:08 694  	  SUBSCRIPTION.SUSPEND_DATE,
11:21:08 695  	  SUBSCRIPTION.REACTIVATION_DATE,
11:21:08 696  	  SUBSCRIPTION.CANCELLATION_DATE,
11:21:08 697  	  SUBSCRIPTION_CANCEL_REASON.VALUE as "CANCEL_TYPE",
11:21:08 698  	  OFFER_CHAIN.ID AS "OFFER_CHAIN_ID",
11:21:08 699  	  OFFER_CHAIN.NAME,
11:21:08 700  	  OFFER_CHAIN.DESCRIPTION,
11:21:08 701  	  OFFER_CHAIN.PRODUCT_URI,
11:21:08 702  	  SUBSCRIPTION.INSTRUMENT_ID,
11:21:08 703  	  SUBSCRIPTION.INSTRUMENT_TYPE_ID,
11:21:08 704  	  PROCS_SUBSCRIPTION_V18.CALC_SUBSCRIPTION_END_DATE(SUBSCRIPTION.ID) as "END_DATE",
11:21:08 705  	  PROCS_SUBSCRIPTION_V18.GET_RECENT_CHARGE(SUBSCRIPTION.ID) AS "RECENT_CHARGE",
11:21:08 706  	  PROCS_SUBSCRIPTION_V18.GET_RENEWAL_DATE(SUBSCRIPTION.ID) AS "RENEWAL_DATE",
11:21:08 707  	  PROCS_SUBSCRIPTION_V18.GET_BILLING_CYCLE(SUBSCRIPTION.ID) AS "BILLING_CYCLE",
11:21:08 708  	  (
11:21:08 709  	    SELECT
11:21:08 710  	      ACCOUNT.GROUP_ID
11:21:08 711  	      FROM ACCOUNT
11:21:08 712  	      WHERE ACCOUNT.ID = SUBSCRIPTION.ACCOUNT_ID
11:21:08 713  	  ) as "GROUP_ID",
11:21:08 714  	  (
11:21:08 715  	    SELECT
11:21:08 716  	      MAX(ENTITLEMENT_END_DATE)
11:21:08 717  	      FROM LICENSE
11:21:08 718  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
11:21:08 719  	  )
11:21:08 720  	  as "ENT_END_DATE",
11:21:08 721  	  (
11:21:08 722  	    SELECT
11:21:08 723  	      MIN(START_DATE)
11:21:08 724  	      FROM LICENSE
11:21:08 725  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
11:21:08 726  	  )
11:21:08 727  	  as "ENT_START_DATE",
11:21:08 728  	  PROCS_SUBSCRIPTION_V18.IS_SUBSCRIPTION_CANCELABLE(SUBSCRIPTION.ID) AS "IS_CANCELABLE",
11:21:08 729  	  ITUNES_RECEIPT.ID AS "ITUNES_RECEIPT_ID",
11:21:08 730  	  GROUP_ACCOUNT.ID GA_ID,
11:21:08 731  	  GROUP_ACCOUNT.SUBSCRIPTION_ID GA_SUBSCRIPTION_ID,
11:21:08 732  	  GROUP_ACCOUNT.GROUP_NAME GA_GROUP_NAME,
11:21:08 733  	  GROUP_ACCOUNT.FIRST_NAME GA_FIRST_NAME,
11:21:08 734  	  GROUP_ACCOUNT.LAST_NAME GA_LAST_NAME,
11:21:08 735  	  GROUP_ACCOUNT.EMAIL GA_EMAIL,
11:21:08 736  	  GROUP_ACCOUNT.PHONE GA_PHONE,
11:21:08 737  	  GROUP_ACCOUNT.ORGANIZATION_TYPE GA_ORGANIZATION_TYPE,
11:21:08 738  	  GROUP_ACCOUNT.SEATS GA_SEATS,
11:21:08 739  	  PROCS_GROUP_ACCOUNT_V18.F_GET_NUM_OCCUPIED_GROUP_SEATS(GROUP_ACCOUNT.ID) GA_SEATS_USED,
11:21:08 740  	  GROUP_ACCOUNT.IP GA_IP,
11:21:08 741  	  PROCS_SUBSCRIPTION_V18.GET_GIFT_CERT_CODE_BY_SUB_ID(SUBSCRIPTION.ID) gift_certificate_code,
11:21:08 742  	  PROCS_ACCOUNT_V18.GET_GRACE_START_DATE(SUBSCRIPTION.ID) GRACE_START_DATE,
11:21:08 743  	  PROCS_ACCOUNT_V18.GET_GRACE_END_DATE(SUBSCRIPTION.ID) GRACE_END_DATE
11:21:08 744  	FROM
11:21:08 745  	  SUBSCRIPTION
11:21:08 746  	  INNER JOIN OFFER_CHAIN ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
11:21:08 747  	  LEFT JOIN SUBSCRIPTION_CANCEL_REASON ON SUBSCRIPTION.SCT_ID = SUBSCRIPTION_CANCEL_REASON.ID
11:21:08 748  	  LEFT JOIN ITUNES_RECEIPT ON SUBSCRIPTION.ID = ITUNES_RECEIPT.SUBSCRIPTION_ID
11:21:08 749  	  LEFT JOIN GROUP_ACCOUNT ON SUBSCRIPTION.ID = GROUP_ACCOUNT.SUBSCRIPTION_ID
11:21:08 750  	WHERE
11:21:08 751  	  SUBSCRIPTION.ID = in_subscription_id;
11:21:08 752  
11:21:08 753  EXCEPTION
11:21:08 754  WHEN BAD_SUBSCRIPTION_ID THEN
11:21:08 755  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:08 756  	  SPROC_NAME, 'No such subscription');
11:21:08 757  WHEN OTHERS THEN
11:21:08 758  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:08 759  	  SPROC_NAME, 'Unknown error', SQLERRM);
11:21:08 760  END GET_SUBSCRIPTION_INFO;
11:21:08 761  
