11:21:09 317  
11:21:09 318  PROCEDURE DISABLE_CREDIT_CARD (
11:21:09 319  /*
11:21:09 320  Throws exceptions:
11:21:09 321  APP_EXCEPTION_CODES_V18.NOT_FOUND
11:21:09 322  APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR
11:21:09 323  APP_EXCEPTION_CODES_V18.INTERNAL_ERROR
11:21:09 324  */
11:21:09 325  	in_credit_card_id IN NUMBER,
11:21:09 326  	in_updated_by	  IN VARCHAR2
11:21:09 327  ) AS
11:21:09 328  SPROC_NAME		   CONSTANT VARCHAR2(19) := 'DISABLE_CREDIT_CARD';
11:21:09 329  -- VARIBLES
11:21:09 330  var_account_id		     NUMBER;
11:21:09 331  var_group_id		     NUMBER;
11:21:09 332  var_credit_card_status	     NUMBER;
11:21:09 333  var_pending_transactions_num   NUMBER;
11:21:09 334  var_pending_invoices_num	     NUMBER;
11:21:09 335  current_def_instrument_type_id NUMBER;
11:21:09 336  current_def_instrument_id      NUMBER;
11:21:09 337  -- EXCEPTIONS
11:21:09 338  BAD_CC_STATUS		   EXCEPTION;
11:21:09 339  PENDING_TRANSACTIONS_FOUNDED EXCEPTION;
11:21:09 340  CAN_NOT_GET_DEF_FINANCIAL    EXCEPTION;
11:21:09 341  CAN_NOT_DEL_DEF_FINANCIAL    EXCEPTION;
11:21:09 342  CAN_NOT_DISABLE_CREDIT_CARD  EXCEPTION;
11:21:09 343  EXCEPTION_MESSAGE 	   VARCHAR2(1024);
11:21:09 344  BEGIN
11:21:09 345  
11:21:09 346  	-- Get credit card status
11:21:09 347  	-- Get account id
11:21:09 348  	SELECT
11:21:09 349  	  CREDIT_CARD.CREDIT_CARD_STATUS_ID,
11:21:09 350  	  CREDIT_CARD.ACCOUNT_ID
11:21:09 351  	  into
11:21:09 352  	  var_credit_card_status,
11:21:09 353  	  var_account_id
11:21:09 354  	FROM
11:21:09 355  	  CREDIT_CARD
11:21:09 356  	WHERE
11:21:09 357  	  CREDIT_CARD.ID = in_credit_card_id;
11:21:09 358  
11:21:09 359  	-- Check that we can disable this credit card (STUB)
11:21:09 360  	IF F_CAN_DISABLE_CREDIT_CARD(in_credit_card_id) = GLOBAL_CONSTANTS_V18.FALSE THEN
11:21:09 361  	  RAISE CAN_NOT_DISABLE_CREDIT_CARD;
11:21:09 362  	END IF;
11:21:09 363  
11:21:09 364  	-- Get account id
11:21:09 365  	SELECT
11:21:09 366  	  ACCOUNT.GROUP_ID into var_group_id
11:21:09 367  	FROM
11:21:09 368  	  ACCOUNT
11:21:09 369  	WHERE
11:21:09 370  	  ACCOUNT.ID = var_account_id;
11:21:09 371  
11:21:09 372  	-- Card should to be active
11:21:09 373  	IF var_credit_card_status != GLOBAL_STATUSES_V18.CREDIT_CARD_ACTIVE THEN
11:21:09 374  	  RAISE BAD_CC_STATUS;
11:21:09 375  	END IF;
11:21:09 376  
11:21:09 377  	-- Looking for pending transactions associated with given credit card
11:21:09 378  	SELECT
11:21:09 379  	  COUNT(*) into var_pending_invoices_num
11:21:09 380  	FROM
11:21:09 381  	  CHARGE
11:21:09 382  	  INNER JOIN TRANSACTION ON CHARGE.TRANSACTION_ID = TRANSACTION.ID
11:21:09 383  	WHERE
11:21:09 384  	  CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V18.INSTRUMENT_CREDIT_CARD
11:21:09 385  	  AND CHARGE.INSTRUMENT_ID = in_credit_card_id
11:21:09 386  	  AND TRANSACTION.TRANSACTION_STATUS_ID = GLOBAL_STATUSES_V18.TRANSACTION_PENDING;
11:21:09 387  
11:21:09 388  	IF var_pending_invoices_num > 0 THEN
11:21:09 389  	  RAISE PENDING_TRANSACTIONS_FOUNDED;
11:21:09 390  	END IF;
11:21:09 391  
11:21:09 392  	-- Getting current default financial instrument
11:21:09 393  	BEGIN
11:21:09 394  	  GET_DEF_FINANCIAL_INSTRUMENT(
11:21:09 395  	    in_group_id 	   => var_group_id,
11:21:09 396  	    out_instrument_type_id => current_def_instrument_type_id,
11:21:09 397  	    out_instrument_id	   => current_def_instrument_id
11:21:09 398  	  );
11:21:09 399  	  EXCEPTION
11:21:09 400  	    WHEN OTHERS THEN
11:21:09 401  	      EXCEPTION_MESSAGE := SQLERRM;
11:21:09 402  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
11:21:09 403  	END;
11:21:09 404  
11:21:09 405  	-- Checking that credit card is not default
11:21:09 406  	IF current_def_instrument_type_id = GLOBAL_ENUMS_V18.INSTRUMENT_CREDIT_CARD
11:21:09 407  	  AND current_def_instrument_id = in_credit_card_id THEN
11:21:09 408  	  BEGIN
11:21:09 409  	    DEL_DEF_FINANCIAL_INSTRUMENT(
11:21:09 410  	      in_group_id => var_group_id
11:21:09 411  	    );
11:21:09 412  	  EXCEPTION
11:21:09 413  	    WHEN OTHERS THEN
11:21:09 414  	      EXCEPTION_MESSAGE := SQLERRM;
11:21:09 415  	      RAISE CAN_NOT_DEL_DEF_FINANCIAL;
11:21:09 416  	  END;
11:21:09 417  	END IF;
11:21:09 418  
11:21:09 419  	-- Update credit card status
11:21:09 420  	PROCS_FIN_INSTRUMENTS_V18.UPDATE_CREDIT_CARD_STATUS(
11:21:09 421  	  in_credit_card_id	   => in_credit_card_id,
11:21:09 422  	  in_updated_by 	   => in_updated_by,
11:21:09 423  	  in_credit_card_status_id => GLOBAL_STATUSES_V18.CREDIT_CARD_DISABLED
11:21:09 424  	);
11:21:09 425  
11:21:09 426  EXCEPTION
11:21:09 427  WHEN NO_DATA_FOUND THEN
11:21:09 428  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 429  	  SPROC_NAME, 'No such credit card');
11:21:09 430  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
11:21:09 431  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 432  	  SPROC_NAME, 'Could not get current default financial instrument', EXCEPTION_MESSAGE);
11:21:09 433  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
11:21:09 434  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 435  	  SPROC_NAME, 'Could not delete information about default financial instrument from account', EXCEPTION_MESSAGE);
11:21:09 436  WHEN BAD_CC_STATUS THEN
11:21:09 437  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.STATE_ERROR,
11:21:09 438  	  SPROC_NAME, 'Credit card is not active');
11:21:09 439  WHEN PENDING_TRANSACTIONS_FOUNDED THEN
11:21:09 440  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.STATE_ERROR,
11:21:09 441  	  SPROC_NAME, 'Account has pending charge which is using this card');
11:21:09 442  WHEN CAN_NOT_DISABLE_CREDIT_CARD THEN
11:21:09 443  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 444  	  SPROC_NAME, 'Could not disable this credit card', EXCEPTION_MESSAGE);
11:21:09 445  WHEN OTHERS THEN
11:21:09 446  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:09 447  	  SPROC_NAME, 'Unknown error', SQLERRM);
11:21:09 448  END DISABLE_CREDIT_CARD;
