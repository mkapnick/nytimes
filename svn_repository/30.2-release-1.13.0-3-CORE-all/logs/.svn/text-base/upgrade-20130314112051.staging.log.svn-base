11:21:09 148  
11:21:09 149  PROCEDURE DISABLE_ACCOUNT (
11:21:09 150  /*
11:21:09 151  Throws exceptions:
11:21:09 152  APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 153  APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:09 154  */
11:21:09 155  	in_group_id   IN NUMBER,
11:21:09 156  	in_updated_by IN VARCHAR2,
11:21:09 157  	in_note       IN VARCHAR2,
11:21:09 158  	in_agent_id   IN NUMBER
11:21:09 159  ) AS
11:21:09 160  SPROC_NAME	      CONSTANT VARCHAR2(15) := 'DISABLE_ACCOUNT';
11:21:09 161  var_account_id	      NUMBER;
11:21:09 162  current_account_status  NUMBER;
11:21:09 163  
11:21:09 164  var_active_subscriptions_num NUMBER;
11:21:09 165  var_pending_invoices_num	   NUMBER;
11:21:09 166  
11:21:09 167  -- EXCEPTIONS
11:21:09 168  BAD_ACOUNT_ID		EXCEPTION;
11:21:09 169  BAD_CURRENT_ACC_STATUS	EXCEPTION;
11:21:09 170  PENDING_INVOICES_FOUND	EXCEPTION;
11:21:09 171  ACCOUNT_HAS_ACIVE_SUBSCRS EXCEPTION;
11:21:09 172  CAN_NOT_ANNOTATE_ACCOUNT	EXCEPTION;
11:21:09 173  EXCEPTION_MESSAGE 	VARCHAR2(1024);
11:21:09 174  BEGIN
11:21:09 175  
11:21:09 176  	-- Get account's status and id
11:21:09 177  	BEGIN
11:21:09 178  	  SELECT
11:21:09 179  	    ACCOUNT.ACCOUNT_STATUS_ID,
11:21:09 180  	    ACCOUNT.ID
11:21:09 181  	  INTO
11:21:09 182  	    current_account_status,
11:21:09 183  	    var_account_id
11:21:09 184  	  FROM ACCOUNT
11:21:09 185  	  WHERE
11:21:09 186  	    ACCOUNT.GROUP_ID = in_group_id;
11:21:09 187  	  EXCEPTION
11:21:09 188  	    WHEN NO_DATA_FOUND THEN
11:21:09 189  	      RAISE BAD_ACOUNT_ID;
11:21:09 190  	END;
11:21:09 191  
11:21:09 192  	-- For now, we can disable account whenever
11:21:09 193  	IF current_account_status = GLOBAL_STATUSES_V18.ACCOUNT_DISABLED THEN
11:21:09 194  	  RAISE BAD_CURRENT_ACC_STATUS;
11:21:09 195  	END IF;
11:21:09 196  
11:21:09 197  	-- Checks for out outstanding balances
11:21:09 198  	-- CHECK: No outstanding balances. If monies are due, then we can not cancel account. Return ERROR.
11:21:09 199  	SELECT
11:21:09 200  	  COUNT(*) into var_pending_invoices_num
11:21:09 201  	FROM
11:21:09 202  	  LICENSE
11:21:09 203  	    INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
11:21:09 204  	    INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
11:21:09 205  	WHERE
11:21:09 206  	  SUBSCRIPTION.ACCOUNT_ID = var_account_id
11:21:09 207  	  AND INVOICE.INVOICE_STATUS_ID IN ( SELECT GLOBAL_STATUSES_V18.INVOICE_OPEN FROM DUAL );
11:21:09 208  
11:21:09 209  	IF var_pending_invoices_num > 0 THEN
11:21:09 210  	  RAISE PENDING_INVOICES_FOUND;
11:21:09 211  	END IF;
11:21:09 212  
11:21:09 213  	SELECT
11:21:09 214  	  COUNT(*) into var_active_subscriptions_num
11:21:09 215  	FROM
11:21:09 216  	  SUBSCRIPTION
11:21:09 217  	WHERE
11:21:09 218  	  SUBSCRIPTION.ACCOUNT_ID = var_account_id
11:21:09 219  	  AND (
11:21:09 220  	    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V18.SUBSCRIPTION_ACTIVE
11:21:09 221  	    OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V18.SUBSCRIPTION_SUSPENDED
11:21:09 222  	    OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V18.SUBSCRIPTION_IN_GRACE_PERIOD);
11:21:09 223  
11:21:09 224  	IF var_active_subscriptions_num > 0 THEN
11:21:09 225  	  RAISE ACCOUNT_HAS_ACIVE_SUBSCRS;
11:21:09 226  	END IF;
11:21:09 227  
11:21:09 228  	PROCS_ACCOUNT_V18.UPDATE_ACCOUNT_STATUS(
11:21:09 229  	  in_account_id        => var_account_id,
11:21:09 230  	  in_account_status_id => GLOBAL_STATUSES_V18.ACCOUNT_DISABLED,
11:21:09 231  	  in_updated_by        => in_updated_by
11:21:09 232  	);
11:21:09 233  
11:21:09 234  	-- Annotate account
11:21:09 235  	IF in_note IS NOT NULL THEN
11:21:09 236  	  BEGIN
11:21:09 237  	    PROCS_ACCOUNT_V18.ANNOTATE_ACCOUNT(
11:21:09 238  	      in_group_id   => in_group_id,
11:21:09 239  	      in_agent_id   => in_agent_id,
11:21:09 240  	      in_note	    => in_note,
11:21:09 241  	      in_created_by => in_updated_by
11:21:09 242  	    );
11:21:09 243  	    EXCEPTION
11:21:09 244  	      WHEN OTHERS THEN
11:21:09 245  		EXCEPTION_MESSAGE := SQLERRM;
11:21:09 246  		RAISE CAN_NOT_ANNOTATE_ACCOUNT;
11:21:09 247  	  END;
11:21:09 248  	END IF;
11:21:09 249  
11:21:09 250  EXCEPTION
11:21:09 251  WHEN ACCOUNT_HAS_ACIVE_SUBSCRS THEN
11:21:09 252  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 253  	  SPROC_NAME, 'Account has active or suspended subsciptions');
11:21:09 254  WHEN BAD_CURRENT_ACC_STATUS THEN
11:21:09 255  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.STATE_ERROR,
11:21:09 256  	  SPROC_NAME, 'Current account status is "disabled". Can not disable it one more time.');
11:21:09 257  WHEN PENDING_INVOICES_FOUND THEN
11:21:09 258  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 259  	  SPROC_NAME, 'Opened/Pending invoices founded');
11:21:09 260  WHEN BAD_ACOUNT_ID THEN
11:21:09 261  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 262  	  SPROC_NAME, 'No such account');
11:21:09 263  WHEN CAN_NOT_ANNOTATE_ACCOUNT THEN
11:21:09 264  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 265  	  SPROC_NAME, 'Could not annotate account', EXCEPTION_MESSAGE);
11:21:09 266  WHEN OTHERS THEN
11:21:09 267  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:09 268  	  SPROC_NAME, 'Unknown error', SQLERRM);
11:21:09 269  END DISABLE_ACCOUNT;
11:21:09 270  
11:21:09 271  /******************************************************************************/
11:21:09 272  
11:21:09 273  PROCEDURE CREATE_ACTIVE_ACCOUNT(
11:21:09 274  /*
11:21:09 275  Throws exceptions:
11:21:09 276  APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:09 277  APP_EXCEPTION_CODES_V18.DUPLICATE_ERROR
11:21:09 278  */
11:21:09 279  	  in_created_by IN VARCHAR2,
11:21:09 280  	  in_group_id	IN NUMBER
11:21:09 281  ) AS
11:21:09 282  -- VARIABLES
11:21:09 283  SPROC_NAME      CONSTANT VARCHAR2(21) := 'CREATE_ACTIVE_ACCOUNT';
11:21:09 284  new_account_id  NUMBER;
11:21:09 285  temp_group_id   NUMBER;
11:21:09 286  -- EXCEPTIONS
11:21:09 287  GROUP_EXISTS_EXCEPTION EXCEPTION;
11:21:09 288  EXCEPTION_MESSAGE      VARCHAR2(1024);
11:21:09 289  BEGIN
11:21:09 290  	-- Check if account already exists
11:21:09 291  	BEGIN
11:21:09 292  	  SELECT
11:21:09 293  	    ACCOUNT.GROUP_ID into temp_group_id
11:21:09 294  	  FROM
11:21:09 295  	    ACCOUNT
11:21:09 296  	  WHERE
11:21:09 297  	    ACCOUNT.GROUP_ID = in_group_id
11:21:09 298  	    AND ROWNUM <= 1;
11:21:09 299  
11:21:09 300  	  IF SQL%ROWCOUNT = 1 THEN
11:21:09 301  	    RAISE GROUP_EXISTS_EXCEPTION;
11:21:09 302  	  END IF;
11:21:09 303  
11:21:09 304  	  EXCEPTION
11:21:09 305  	    WHEN NO_DATA_FOUND THEN
11:21:09 306  	      NULL;
11:21:09 307  	END;
11:21:09 308  
11:21:09 309  	-- Insert new row into ACCOUNT table
11:21:09 310  	PROCS_ACCOUNT_CRU_V18.CREATE_ACCOUNT(
11:21:09 311  	  out_account_id	=> new_account_id,
11:21:09 312  	  in_account_status_id	=> GLOBAL_STATUSES_V18.ACCOUNT_ACTIVE,
11:21:09 313  	  in_group_id		=> in_group_id,
11:21:09 314  	  in_created_by 	=> in_created_by,
11:21:09 315  	  in_system_category_id => GLOBAL_ENUMS_V18.SYSTEM_CATEGORY_LIVE
11:21:09 316  	);
11:21:09 317  
11:21:09 318  EXCEPTION
11:21:09 319  WHEN GROUP_EXISTS_EXCEPTION THEN
11:21:09 320  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.DUPLICATE_ERROR,
11:21:09 321  	  SPROC_NAME, 'Group already exists');
11:21:09 322  WHEN OTHERS THEN
11:21:09 323  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:09 324  	  SPROC_NAME, 'Unknown error', SQLERRM);
11:21:09 325  END CREATE_ACTIVE_ACCOUNT;
