11:21:09 534  
11:21:09 535  PROCEDURE GET_ACCOUNT_INFO  (
11:21:09 536  /*
11:21:09 537  Throws exceptions:
11:21:09 538  APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 539  APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR
11:21:09 540  */
11:21:09 541  	  in_group_id	    IN	NUMBER,
11:21:09 542  	  out_account_info  OUT SYS_REFCURSOR
11:21:09 543  ) AS
