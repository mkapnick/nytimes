11:21:08 212  
11:21:08 213  	-- if user have any active existing subscriptions to the offer chain
11:21:08 214  	-- and if MAX_CONCURRENT_SUBS <= [user's subscription count for the offer chain]
11:21:08 215  	-- then deny purchase
11:21:08 216  	var_max_concurrent_subscrs := PROCS_OFFER_CHAIN_V18.GET_OFFER_CHAIN_MAX_CONC_SUBSC(in_offer_chain_id);
11:21:08 217  	IF var_max_concurrent_subscrs != GLOBAL_CONSTANTS_V18.INFINITY
11:21:08 218  	  AND var_max_concurrent_subscrs <= var_same_offer_chains_num THEN
11:21:08 219  	  -- Find first concurrent subscription id:
11:21:08 220  	  SELECT
11:21:08 221  	    ID into var_concur_subscription_id
