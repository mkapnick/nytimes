11:21:09 487  -- norlov: #38796
11:21:09 488  PROCEDURE GET_TRANSACTION_INVOICE (
11:21:09 489  /*
11:21:09 490  Throws exceptions:
11:21:09 491  APP_EXCEPTION_CODES_V18.NOT_FOUND
11:21:09 492  APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR
11:21:09 493  */
11:21:09 494  	in_transaction_id  IN  NUMBER,
11:21:09 495  	out_result_set	      OUT SYS_REFCURSOR
11:21:09 496  ) AS
11:21:09 497  SPROC_NAME CONSTANT  VARCHAR2(23) := 'GET_TRANSACTION_INVOICE';
11:21:09 498  -- VARIABLES
11:21:09 499  temp_transaction_id  NUMBER;
11:21:09 500  var_invoice_id	   NUMBER;
11:21:09 501  var_subscription_id  NUMBER;
11:21:09 502  var_offer_chain_id   NUMBER;
11:21:09 503  var_offer_chain_name VARCHAR2(255);
11:21:09 504  -- EXCEPTIONS
11:21:09 505  BAD_TRANSACTION_ID     EXCEPTION;
11:21:09 506  EXCEPTION_MESSAGE      VARCHAR2(1024);
11:21:09 507  BEGIN
11:21:09 508   -- check if there is the transaction
11:21:09 509  	BEGIN
11:21:09 510  	  SELECT
11:21:09 511  	    TRANSACTION.ID into temp_transaction_id
11:21:09 512  	  FROM
11:21:09 513  	    TRANSACTION
11:21:09 514  	  WHERE
11:21:09 515  	    TRANSACTION.ID = in_transaction_id;
11:21:09 516  	  EXCEPTION
11:21:09 517  	    WHEN NO_DATA_FOUND THEN
11:21:09 518  	      RAISE BAD_TRANSACTION_ID;
11:21:09 519  	END;
11:21:09 520  
11:21:09 521  	-- Get invoice id
11:21:09 522  	SELECT DISTINCT
11:21:09 523  	  CHARGE.INVOICE_ID into var_invoice_id
11:21:09 524  	FROM
11:21:09 525  	  CHARGE
11:21:09 526  	WHERE
11:21:09 527  	  CHARGE.TRANSACTION_ID = in_transaction_id;
11:21:09 528  
11:21:09 529  	-- Get subscription id if exists
11:21:09 530  	BEGIN
11:21:09 531  	  SELECT DISTINCT
11:21:09 532  	    LICENSE.SUBSCRIPTION_ID into var_subscription_id
11:21:09 533  	  FROM
11:21:09 534  	    LICENSE
11:21:09 535  	  WHERE
11:21:09 536  	    LICENSE.INVOICE_ID = var_invoice_id;
11:21:09 537  	  EXCEPTION
11:21:09 538  	    WHEN NO_DATA_FOUND THEN
11:21:09 539  	      var_subscription_id := NULL;
11:21:09 540  	END;
11:21:09 541  
11:21:09 542  	IF var_subscription_id IS NOT NULL THEN
11:21:09 543  	  -- Fetch offer chain from subscription
11:21:09 544  	  SELECT
11:21:09 545  	    OFFER_CHAIN.ID,
11:21:09 546  	    OFFER_CHAIN.NAME
11:21:09 547  	    into
11:21:09 548  	    var_offer_chain_id,
11:21:09 549  	    var_offer_chain_name
11:21:09 550  	  FROM
11:21:09 551  	    OFFER_CHAIN
11:21:09 552  	    INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
11:21:09 553  	  WHERE
11:21:09 554  	    SUBSCRIPTION.ID = var_subscription_id;
11:21:09 555  	ELSE
11:21:09 556  	  -- Fetch offer chain from GC
11:21:09 557  	  SELECT
11:21:09 558  	    OFFER_CHAIN.ID,
11:21:09 559  	    OFFER_CHAIN.NAME
11:21:09 560  	    into
11:21:09 561  	    var_offer_chain_id,
11:21:09 562  	    var_offer_chain_name
11:21:09 563  	  FROM
11:21:09 564  	    OFFER_CHAIN
11:21:09 565  	    INNER JOIN GIFT_CERTIFICATE ON GIFT_CERTIFICATE.OFFER_CHAIN_ID = OFFER_CHAIN.ID
11:21:09 566  	  WHERE
11:21:09 567  	    GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = var_invoice_id;
11:21:09 568  	END IF;
11:21:09 569  
11:21:09 570  	OPEN out_result_set FOR
11:21:09 571  	SELECT DISTINCT
11:21:09 572  	  var_invoice_id       AS "INVOICE_ID",
11:21:09 573  	  var_subscription_id  AS "SUBSCRIPTION_ID",
11:21:09 574  	  var_offer_chain_id   AS "OFFER_CHAIN_ID",
11:21:09 575  	  var_offer_chain_name AS "OFFER_CHAIN_NAME"
11:21:09 576  	FROM
11:21:09 577  	  DUAL;
11:21:09 578  
11:21:09 579  EXCEPTION
11:21:09 580  WHEN BAD_TRANSACTION_ID THEN
11:21:09 581  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 582  	  SPROC_NAME, 'No such transaction');
11:21:09 583  WHEN OTHERS THEN
11:21:09 584  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:09 585  	  SPROC_NAME, 'Unknown error', SQLERRM);
11:21:09 586  END GET_TRANSACTION_INVOICE;
11:21:09 587  
