11:21:09 749  
11:21:09 750  PROCEDURE CALCULATE_INVOICE_CHARGEBACK (
11:21:09 751  	in_invoice_id	      IN NUMBER,
11:21:09 752  	in_chargeback_date    IN DATE,
11:21:09 753  	out_chargeback_amount OUT NUMBER
11:21:09 754  ) AS
11:21:09 755  -- VARIABLES
11:21:09 756  SPROC_NAME	     CONSTANT VARCHAR2(28) := 'CALCULATE_INVOICE_CHARGEBACK';
11:21:09 757  var_chargeback_date    DATE;
11:21:09 758  temp_invoice_id	     NUMBER;
11:21:09 759  var_licenses_number    NUMBER;
11:21:09 760  var_invoice_start_date DATE;
11:21:09 761  var_invoice_end_date   DATE;
11:21:09 762  var_offer_id	     NUMBER;
11:21:09 763  var_offer_days_interval NUMBER;
11:21:09 764  var_license_days_used  NUMBER;
11:21:09 765  var_invoice_amount     NUMBER(10,2);
11:21:09 766  var_offer_chain_id     NUMBER;
11:21:09 767  var_offer_chain_meta_data_val VARCHAR2(1024);
11:21:09 768  var_offer_chain_full_refund   NUMBER;
11:21:09 769  var_offer_chain_prorated_ref  NUMBER;
11:21:09 770  var_chargeback_calculated NUMBER;
11:21:09 771  var_max_invoice_refund	NUMBER;
11:21:09 772  -- EXCEPTIONS
11:21:09 773  BAD_INVOICE_ID		  EXCEPTION;
11:21:09 774  NO_LICENSES_FOUND_EXCEPTION EXCEPTION;
11:21:09 775  OFFER_LENGTH_IS_ZERO	  EXCEPTION;
11:21:09 776  BEGIN
11:21:09 777  
11:21:09 778  	IF in_chargeback_date IS NULL THEN
11:21:09 779  	  var_chargeback_date := PROCS_COMMON_V18.NORMALIZE_DATE(current_date);
11:21:09 780  	ELSE
11:21:09 781  	  var_chargeback_date := PROCS_COMMON_V18.NORMALIZE_DATE(in_chargeback_date);
11:21:09 782  	END IF;
11:21:09 783  
11:21:09 784  	-- Check that invoice exists
11:21:09 785  	BEGIN
11:21:09 786  	  SELECT
11:21:09 787  	    INVOICE.ID into temp_invoice_id
11:21:09 788  	  FROM
11:21:09 789  	    INVOICE
11:21:09 790  	  WHERE
11:21:09 791  	    INVOICE.ID = in_invoice_id;
11:21:09 792  	  EXCEPTION
11:21:09 793  	    WHEN NO_DATA_FOUND THEN
11:21:09 794  	      RAISE BAD_INVOICE_ID;
11:21:09 795  	END;
11:21:09 796  
11:21:09 797  	-- Check that invoice has at least one license
11:21:09 798  	SELECT
11:21:09 799  	  COUNT(*) into var_licenses_number
11:21:09 800  	FROM
11:21:09 801  	  LICENSE
11:21:09 802  	WHERE
11:21:09 803  	  LICENSE.INVOICE_ID = in_invoice_id;
11:21:09 804  
11:21:09 805  	IF var_licenses_number = 0 THEN
11:21:09 806  	  RAISE NO_LICENSES_FOUND_EXCEPTION;
11:21:09 807  	END IF;
11:21:09 808  
11:21:09 809  	SELECT
11:21:09 810  	  MIN(LICENSE.START_DATE) into var_invoice_start_date
11:21:09 811  	FROM
11:21:09 812  	  LICENSE
11:21:09 813  	WHERE
11:21:09 814  	  LICENSE.INVOICE_ID = in_invoice_id;
11:21:09 815  
11:21:09 816  	var_invoice_start_date := PROCS_COMMON_V18.NORMALIZE_DATE(var_invoice_start_date);
11:21:09 817  
11:21:09 818  	SELECT DISTINCT
11:21:09 819  	  LICENSE.OFFER_ID into var_offer_id
11:21:09 820  	FROM
11:21:09 821  	  LICENSE
11:21:09 822  	WHERE
11:21:09 823  	  LICENSE.INVOICE_ID = in_invoice_id;
11:21:09 824  
11:21:09 825  	SELECT
11:21:09 826  	  MAX (LICENSE.END_DATE) into var_invoice_end_date
11:21:09 827  	FROM
11:21:09 828  	  LICENSE
11:21:09 829  	WHERE
11:21:09 830  	  LICENSE.INVOICE_ID = in_invoice_id;
11:21:09 831  
11:21:09 832  	-- All licenses for given invoice should point into the same offer
11:21:09 833  
11:21:09 834  	PROCS_OFFER_CHAIN_V18.GET_OFFER_LENGTH_IN_DAYS(
11:21:09 835  	  in_offer_id	=> var_offer_id,
11:21:09 836  	  in_start_date => var_invoice_start_date,
11:21:09 837  	  out_days	=> var_offer_days_interval
11:21:09 838  	);
11:21:09 839  
11:21:09 840  	IF var_offer_days_interval = 0 THEN
11:21:09 841  	  RAISE OFFER_LENGTH_IS_ZERO;
11:21:09 842  	END IF;
11:21:09 843  
11:21:09 844  	PROCS_INVOICE_V18.GET_INVOICE_DAYS_USED_NUMBER(
11:21:09 845  	  in_invoice_id      => in_invoice_id,
11:21:09 846  	  in_chargeback_date => var_chargeback_date,
11:21:09 847  	  out_days_num	     => var_license_days_used
11:21:09 848  	);
11:21:09 849  
11:21:09 850  	PROCS_INVOICE_V18.CALCULATE_INVOICE_AMOUNT(
11:21:09 851  	  in_invoice_id => in_invoice_id,
11:21:09 852  	  out_amount	=> var_invoice_amount
11:21:09 853  	);
11:21:09 854  
11:21:09 855  	-- 39437
11:21:09 856  	-- Get offer chain id by invoice id
11:21:09 857  	SELECT DISTINCT
11:21:09 858  	  SUBSCRIPTION.OFFER_CHAIN_ID into var_offer_chain_id
11:21:09 859  	FROM
11:21:09 860  	  SUBSCRIPTION
11:21:09 861  	  INNER JOIN LICENSE ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
11:21:09 862  	WHERE
11:21:09 863  	  LICENSE.INVOICE_ID = in_invoice_id;
11:21:09 864  
11:21:09 865  	-- Get offer chain's meta data full amount value
11:21:09 866  	PROCS_OFFER_CHAIN_V18.GET_OFFER_CHAIN_MD_VALUE(
11:21:09 867  	  in_offer_chain_id => var_offer_chain_id,
11:21:09 868  	  in_meta_data_name => GLOBAL_CONSTANTS_V18.OFFER_CHAIN_FULL_REFUND,
11:21:09 869  	  out_value	    => var_offer_chain_meta_data_val
11:21:09 870  	);
11:21:09 871  	IF var_offer_chain_meta_data_val IS NULL THEN
11:21:09 872  	  var_offer_chain_full_refund := NULL;
11:21:09 873  	ELSE
11:21:09 874  	  var_offer_chain_full_refund := TO_NUMBER(var_offer_chain_meta_data_val);
11:21:09 875  	END IF;
11:21:09 876  
11:21:09 877  	-- Get offer chain's meta data prorated amount value
11:21:09 878  	PROCS_OFFER_CHAIN_V18.GET_OFFER_CHAIN_MD_VALUE(
11:21:09 879  	  in_offer_chain_id => var_offer_chain_id,
11:21:09 880  	  in_meta_data_name => GLOBAL_CONSTANTS_V18.OFFER_CHAIN_PRORATED_REFUND,
11:21:09 881  	  out_value	    => var_offer_chain_meta_data_val
11:21:09 882  	);
11:21:09 883  	IF var_offer_chain_meta_data_val IS NULL THEN
11:21:09 884  	  var_offer_chain_prorated_ref := NULL;
11:21:09 885  	ELSE
11:21:09 886  	  var_offer_chain_prorated_ref := TO_NUMBER(var_offer_chain_meta_data_val);
11:21:09 887  	END IF;
11:21:09 888  
11:21:09 889  	var_chargeback_calculated := GLOBAL_CONSTANTS_V18.FALSE;
11:21:09 890  
11:21:09 891  	IF var_offer_chain_full_refund IS NOT NULL
11:21:09 892  	   AND var_chargeback_calculated = GLOBAL_CONSTANTS_V18.FALSE THEN
11:21:09 893  	  IF var_license_days_used < var_offer_chain_full_refund THEN
11:21:09 894  	    out_chargeback_amount := var_invoice_amount;
11:21:09 895  	    var_chargeback_calculated := GLOBAL_CONSTANTS_V18.TRUE;
11:21:09 896  	  END IF;
11:21:09 897  	END IF;
11:21:09 898  
11:21:09 899  	IF var_offer_chain_prorated_ref IS NOT NULL
11:21:09 900  	   AND var_chargeback_calculated = GLOBAL_CONSTANTS_V18.FALSE THEN
11:21:09 901  	  IF var_license_days_used < var_offer_chain_prorated_ref THEN
11:21:09 902  	    out_chargeback_amount := ( var_invoice_amount * (var_offer_days_interval - var_license_days_used) ) / var_offer_days_interval;
11:21:09 903  	    var_chargeback_calculated := GLOBAL_CONSTANTS_V18.TRUE;
11:21:09 904  	  END IF;
11:21:09 905  	END IF;
11:21:09 906  
11:21:09 907  	IF var_chargeback_calculated = GLOBAL_CONSTANTS_V18.TRUE THEN
11:21:09 908  	  PROCS_INVOICE_V18.GET_MAX_REFUND(
11:21:09 909  	    in_invoice_id => in_invoice_id,
11:21:09 910  	    out_amount	  => var_max_invoice_refund
11:21:09 911  	  );
11:21:09 912  	  IF var_max_invoice_refund < out_chargeback_amount THEN
11:21:09 913  	    out_chargeback_amount := var_max_invoice_refund;
11:21:09 914  	  END IF;
11:21:09 915  	END IF;
11:21:09 916  
11:21:09 917  	IF var_chargeback_calculated = GLOBAL_CONSTANTS_V18.FALSE THEN
11:21:09 918  	  out_chargeback_amount := 0;
11:21:09 919  	END IF;
11:21:09 920  
11:21:09 921  EXCEPTION
11:21:09 922  WHEN BAD_INVOICE_ID THEN
11:21:09 923  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 924  	  SPROC_NAME, 'No such invoice');
11:21:09 925  WHEN NO_LICENSES_FOUND_EXCEPTION THEN
11:21:09 926  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 927  	  SPROC_NAME, 'No licenses found');
11:21:09 928  WHEN OFFER_LENGTH_IS_ZERO THEN
11:21:09 929  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 930  	  SPROC_NAME, 'Offer length is zero');
11:21:09 931  WHEN OTHERS THEN
11:21:09 932  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:09 933  	  SPROC_NAME, 'Unknown error', SQLERRM);
11:21:09 934  END CALCULATE_INVOICE_CHARGEBACK;
