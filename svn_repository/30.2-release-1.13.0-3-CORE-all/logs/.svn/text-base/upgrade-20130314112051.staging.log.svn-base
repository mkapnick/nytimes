11:21:08 147  	  , PROCS_REPORTING_1A.getRefundAmount(line_item.id) refund_amount
11:21:08 148  	  , decode(gclicense.sct_id,null,0, 1)		 is_refund_cancel
11:21:08 149  	  , 0						 purchase_tax_amount
