11:21:09 493  	    WHEN NO_DATA_FOUND THEN
11:21:09 494  	      RAISE BAD_PAYPAL_ID;
11:21:09 495  	END;
11:21:09 496  
11:21:09 497  	-- Get group id
11:21:09 498  	SELECT
11:21:09 499  	  ACCOUNT.GROUP_ID into var_group_id
11:21:09 500  	FROM
11:21:09 501  	  ACCOUNT
11:21:09 502  	WHERE
11:21:09 503  	  ACCOUNT.ID = var_account_id;
11:21:09 504  
11:21:09 505  	-- Card should not be disabled
11:21:09 506  	IF var_paypal_status_id = GLOBAL_STATUSES_V18.PAYPAL_INACTIVE THEN
11:21:09 507  	  RAISE PAYPAL_ALREADY_INACTIVE;
11:21:09 508  	END IF;
11:21:09 509  
11:21:09 510  	-- Looking for pending transactions associated with given credit card
11:21:09 511  	SELECT
11:21:09 512  	  COUNT(*) into var_pending_invoices_num
11:21:09 513  	FROM
11:21:09 514  	  CHARGE
11:21:09 515  	  INNER JOIN TRANSACTION ON CHARGE.TRANSACTION_ID = TRANSACTION.ID
11:21:09 516  	WHERE
11:21:09 517  	  CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V18.INSTRUMENT_PAYPAL
11:21:09 518  	  AND CHARGE.INSTRUMENT_ID = in_paypal_id
11:21:09 519  	  AND TRANSACTION.TRANSACTION_STATUS_ID = GLOBAL_STATUSES_V18.TRANSACTION_PENDING;
11:21:09 520  
