11:21:09 1404  /******************************************************************************/
11:21:09 1405  
11:21:09 1406  PROCEDURE REDEEM_GIFT_CERTIFICATE (
11:21:09 1407  	 in_group_id			 IN NUMBER,
11:21:09 1408  	 in_gift_certificate_code	 IN VARCHAR2,
11:21:09 1409  	 in_created_by			 IN VARCHAR2,
11:21:09 1410  	 in_redeemer_address_id 	 IN NUMBER,
11:21:09 1411  	 in_fin_instrument_id		 IN NUMBER,
11:21:09 1412  	 in_fin_instrument_type_id	 IN NUMBER,
11:21:09 1413  	 in_redemption_offer_chain_id	 IN NUMBER,
11:21:09 1414  	 out_subscription_id		 OUT NUMBER,
11:21:09 1415  	 out_license_id 		 OUT NUMBER
11:21:09 1416  ) AS
11:21:09 1417  SPROC_NAME	       CONSTANT VARCHAR2(23) := 'REDEEM_GIFT_CERTIFICATE';
11:21:09 1418  -- VARIABLES
11:21:09 1419  var_gift_certificate_id NUMBER;
11:21:09 1420  -- norlov: #38151 var_offer_chain_id replaced by var_purchased_oc_id and var_oc_id_to_redeem
11:21:09 1421  var_purchased_oc_id     NUMBER;
11:21:09 1422  var_oc_id_to_redeem     NUMBER := in_redemption_offer_chain_id;
11:21:09 1423  var_offer_duration      VARCHAR2(30);
11:21:09 1424  var_invoice_id	       NUMBER;
11:21:09 1425  var_succ_purch_attempts_num NUMBER;
11:21:09 1426  var_subscription_id     NUMBER;
11:21:09 1427  var_license_id	       NUMBER;
11:21:09 1428  var_account_id	       NUMBER;
11:21:09 1429  var_gc_status_id        NUMBER;
11:21:09 1430  var_gc_charges_amount   NUMBER;
11:21:09 1431  var_gc_expiration_date  DATE;
11:21:09 1432  var_gc_redeemer_group_id NUMBER;
11:21:09 1433  var_gc_purchase_invoice_id NUMBER;
11:21:09 1434  var_gc_purchase_inv_status_id NUMBER;
11:21:09 1435  var_offer_index		     NUMBER;
