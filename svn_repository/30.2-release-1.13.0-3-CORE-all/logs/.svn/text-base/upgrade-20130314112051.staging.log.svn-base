11:21:09 177  	  )
11:21:09 178  	  AND INVOICE.ID NOT IN
11:21:09 179  	    (
11:21:09 180  	      SELECT GENERIC_ID
11:21:09 181  	      FROM PROCESS_RETRY_THROTTLE
11:21:09 182  	      WHERE
