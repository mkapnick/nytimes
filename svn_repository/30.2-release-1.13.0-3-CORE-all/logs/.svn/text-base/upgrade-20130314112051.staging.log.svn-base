11:21:07  94  	  AND SS.GROUP_ACCOUNT_ID = GA.ID
11:21:07  95  	  AND SYSDATE BETWEEN SS.START_DATE AND SS.END_DATE
11:21:07  96  	  AND SYSDATE BETWEEN LIC.START_DATE AND LIC.ENTITLEMENT_END_DATE
11:21:07  97  	  AND C.SHAREABLE = 1
11:21:07  98  	GROUP BY
