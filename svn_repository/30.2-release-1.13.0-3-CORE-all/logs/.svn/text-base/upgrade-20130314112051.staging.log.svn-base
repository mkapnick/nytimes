11:21:08 223  	    SELECT
11:21:08 224  	      ID
11:21:08 225  	    FROM
11:21:08 226  	      SUBSCRIPTION
11:21:08 227  	    WHERE
11:21:08 228  	      SUBSCRIPTION.ACCOUNT_ID = var_account_id
11:21:08 229  	      AND SUBSCRIPTION.OFFER_CHAIN_ID = in_offer_chain_id
