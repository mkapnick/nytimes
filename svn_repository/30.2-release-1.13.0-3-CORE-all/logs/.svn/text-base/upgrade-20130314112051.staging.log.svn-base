11:21:09 158  	      FROM PROCESS_RETRY_THROTTLE
11:21:09 159  	      WHERE
11:21:09 160  		PROCESS_NAME = SPROC_NAME
11:21:09 161  	    )
11:21:09 162  	  AND ROWNUM <= var_row_number;
11:21:09 163  
11:21:09 164  	-- Invoices not marked as CLOSED with no payments(charges).
11:21:09 165  	OPEN out_result_set3 FOR
11:21:09 166  	SELECT
11:21:09 167  	  INVOICE.ID
11:21:09 168  	FROM
11:21:09 169  	  INVOICE
11:21:09 170  	WHERE
11:21:09 171  	  1 = 2 AND
11:21:09 172  	  (
