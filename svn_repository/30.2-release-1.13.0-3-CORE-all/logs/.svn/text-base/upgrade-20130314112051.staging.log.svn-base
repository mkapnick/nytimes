11:21:08 745  	    INVOICE.ID into temp_invoice_id
11:21:08 746  	  FROM
11:21:08 747  	    INVOICE
11:21:08 748  	  WHERE
11:21:08 749  	    INVOICE.ID = in_invoice_id;
11:21:08 750  	  EXCEPTION
