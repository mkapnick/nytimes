11:21:09 739  PROCEDURE P_IS_INVOICE_PAYING_STARTED (
11:21:09 740  	in_invoice_id  IN NUMBER,
11:21:09 741  	out_is_started OUT NUMBER
11:21:09 742  ) AS
11:21:09 743  BEGIN
11:21:09 744  	-- Just a wrapper
11:21:09 745  	out_is_started := PROCS_INVOICE_V18.IS_INVOICE_PAYING_STARTED(in_invoice_id);
