11:21:07 439  /******************************************************************************/
11:21:07 440  
11:21:07 441  FUNCTION CALCULATE_OFFER_CHAIN_END_DATE (
11:21:07 442  /*
11:21:07 443  Throws exceptions (codes):
11:21:07 444  APP_EXCEPTION_CODES_V18.NOT_FOUND
11:21:07 445  APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR
11:21:07 446  */
11:21:07 447  	in_offer_chain_id	  IN NUMBER,
11:21:07 448  	in_offer_chain_start_date IN DATE
11:21:07 449  ) RETURN DATE AS
11:21:07 450  -- VARIABLES
11:21:07 451  SPROC_NAME	     CONSTANT VARCHAR2(30) := 'CALCULATE_OFFER_CHAIN_END_DATE';
11:21:07 452  temp_offer_chain_id    NUMBER;
11:21:07 453  var_offer_chain_length NUMBER;
11:21:07 454  var_offer_duration     VARCHAR2(30);
11:21:07 455  var_offer_recurrences  NUMBER;
11:21:07 456  var_end_date	     DATE;
11:21:07 457  
11:21:07 458  var_offer_ym_interval INTERVAL YEAR TO MONTH;
11:21:07 459  var_offer_ds_interval INTERVAL DAY(3) TO SECOND;
11:21:07 460  var_offer_years	    NUMBER;
11:21:07 461  var_offer_months	    NUMBER;
11:21:07 462  var_offer_days	    NUMBER;
11:21:07 463  var_infinity_offers_count NUMBER;
11:21:07 464  
11:21:07 465  var_offers_set SYS_REFCURSOR;
11:21:07 466  
11:21:07 467  -- EXCEPTIONS
11:21:07 468  BAD_OFFER_CHAIN_ID EXCEPTION;
11:21:07 469  BEGIN
11:21:07 470  
11:21:07 471  	var_end_date := in_offer_chain_start_date;
11:21:07 472  
11:21:07 473  	-- Check that offer chain exists
11:21:07 474  	BEGIN
11:21:07 475  	  SELECT
11:21:07 476  	    OFFER_CHAIN.ID into temp_offer_chain_id
11:21:07 477  	  FROM
11:21:07 478  	    OFFER_CHAIN
11:21:07 479  	  WHERE
11:21:07 480  	    OFFER_CHAIN.ID = in_offer_chain_id;
11:21:07 481  	  EXCEPTION
11:21:07 482  	    WHEN NO_DATA_FOUND
11:21:07 483  	      THEN RAISE BAD_OFFER_CHAIN_ID;
11:21:07 484  	END;
11:21:07 485  
11:21:07 486  	SELECT
11:21:07 487  	  COUNT(*) into var_infinity_offers_count
11:21:07 488  	FROM
11:21:07 489  	  OFFER_OFFER_CHAIN
11:21:07 490  	WHERE
11:21:07 491  	  OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
11:21:07 492  	  AND OFFER_OFFER_CHAIN.NUM_RECURRENCES = GLOBAL_ENUMS_V18.OFFER_REC_INFINITY;
11:21:07 493  
11:21:07 494  	IF var_infinity_offers_count > 0 THEN
11:21:07 495  	  -- Offer chain contains offers with infinity num of recurrences
11:21:07 496  	  RETURN NULL;
11:21:07 497  	END IF;
11:21:07 498  
11:21:07 499  	-- Select offers durations
11:21:07 500  	OPEN var_offers_set FOR
11:21:07 501  	SELECT
11:21:07 502  	  OFFER.ENTITLEMENT_DURATION,
11:21:07 503  	  OFFER_OFFER_CHAIN.NUM_RECURRENCES
11:21:07 504  	FROM
11:21:07 505  	  OFFER_OFFER_CHAIN
11:21:07 506  	  INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
11:21:07 507  	WHERE
11:21:07 508  	  OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id;
11:21:07 509  
11:21:07 510  	-- Calculate sum of offers durations
11:21:07 511  	LOOP
11:21:07 512  	  FETCH var_offers_set INTO var_offer_duration, var_offer_recurrences;
11:21:07 513  	  EXIT WHEN var_offers_set%NOTFOUND;
11:21:07 514  	  PROCS_COMMON_V18.ISO8601DURATION_TO_INTERVALS(var_offer_duration, var_offer_years, var_offer_months, var_offer_days);
11:21:07 515  	  var_offer_ym_interval := var_offer_years||'-'||var_offer_months;
11:21:07 516  	  var_offer_ds_interval := var_offer_days||' 0:0:0';
11:21:07 517  	  var_end_date := var_end_date + ( var_offer_ym_interval * ( var_offer_recurrences + 1) ) + ( var_offer_ds_interval * ( var_offer_recurrences + 1) );
11:21:07 518  	END LOOP;
11:21:07 519  
11:21:07 520  	RETURN var_end_date;
11:21:07 521  
11:21:07 522  EXCEPTION
11:21:07 523  WHEN BAD_OFFER_CHAIN_ID THEN
11:21:07 524  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:07 525  	  SPROC_NAME, 'No such offer chain');
11:21:07 526  WHEN OTHERS THEN
11:21:07 527  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:07 528  	  SPROC_NAME, 'Unknown error', SQLERRM);
11:21:07 529  END CALCULATE_OFFER_CHAIN_END_DATE;
11:21:07 530  
11:21:07 531  /******************************************************************************/
11:21:07 532  
11:21:07 533  FUNCTION CALCULATE_OFFER_AMOUNT (
11:21:07 534  	in_offer_id IN NUMBER
11:21:07 535  ) RETURN NUMBER AS
11:21:07 536  -- VARIABLES
11:21:07 537  SPROC_NAME    CONSTANT VARCHAR2(22) := 'CALCULATE_OFFER_AMOUNT';
11:21:07 538  temp_offer_id NUMBER;
11:21:07 539  
11:21:07 540  var_product_offering_set	    SYS_REFCURSOR;
11:21:07 541  var_product_offering_id	    NUMBER;
11:21:07 542  var_product_offering_price    NUMBER(10,6);
11:21:07 543  var_product_offering_t_amount NUMBER(10,6);
11:21:07 544  var_product_offering_quantity NUMBER;
11:21:07 545  
11:21:07 546  var_total_amount NUMBER(10,6);
11:21:07 547  var_final_amount NUMBER(10,2);
11:21:07 548  
11:21:07 549  var_percent_discount NUMBER(10,2);
11:21:07 550  var_fixed_discount NUMBER(10,6);
11:21:07 551  
11:21:07 552  -- EXCEPTIONS
11:21:07 553  BAD_OFFER_ID EXCEPTION;
11:21:07 554  BEGIN
11:21:07 555  
11:21:07 556  	BEGIN
11:21:07 557  	  SELECT
11:21:07 558  	    OFFER.ID into temp_offer_id
11:21:07 559  	  FROM
11:21:07 560  	    OFFER
11:21:07 561  	  WHERE
11:21:07 562  	    OFFER.ID = in_offer_id;
11:21:07 563  	  EXCEPTION
11:21:07 564  	    WHEN NO_DATA_FOUND THEN
11:21:07 565  	      RAISE BAD_OFFER_ID;
11:21:07 566  	END;
11:21:07 567  
11:21:07 568  	OPEN var_product_offering_set FOR
11:21:07 569  	SELECT
11:21:07 570  	  OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID,
11:21:07 571  	  PRODUCT_OFFERING.UNIT_PRICE,
11:21:07 572  	  PRODUCT_OFFERING.QUANTITY
11:21:07 573  	FROM
11:21:07 574  	  OFFER_PRODUCT_OFFERING
11:21:07 575  	  INNER JOIN PRODUCT_OFFERING ON OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = PRODUCT_OFFERING.ID
11:21:07 576  	WHERE
11:21:07 577  	  OFFER_PRODUCT_OFFERING.OFFER_ID = in_offer_id;
11:21:07 578  
11:21:07 579  	var_total_amount := 0;
11:21:07 580  
11:21:07 581  	LOOP
11:21:07 582  	  FETCH var_product_offering_set into
11:21:07 583  	    var_product_offering_id,
11:21:07 584  	    var_product_offering_price,
11:21:07 585  	    var_product_offering_quantity;
11:21:07 586  	  EXIT WHEN var_product_offering_set%NOTFOUND;
11:21:07 587  
11:21:07 588  	  SELECT
11:21:07 589  	    SUM(DISCOUNT.FIXED_AMOUNT) into var_fixed_discount
11:21:07 590  	  FROM
11:21:07 591  	    DISCOUNT_PRODUCT_OFFERING
11:21:07 592  	    INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
11:21:07 593  	  WHERE
11:21:07 594  	    DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
11:21:07 595  	    AND DISCOUNT.FIXED_AMOUNT IS NOT NULL;
11:21:07 596  
11:21:07 597  	  SELECT
11:21:07 598  	    SUM(DISCOUNT.PERCENT_AMOUNT) into var_percent_discount
11:21:07 599  	  FROM
11:21:07 600  	    DISCOUNT_PRODUCT_OFFERING
11:21:07 601  	    INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
11:21:07 602  	  WHERE
11:21:07 603  	    DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
11:21:07 604  	    AND DISCOUNT.PERCENT_AMOUNT IS NOT NULL;
11:21:07 605  
11:21:07 606  	  var_product_offering_t_amount := var_product_offering_price * var_product_offering_quantity;
11:21:07 607  
11:21:07 608  	  IF var_percent_discount IS NOT NULL THEN
11:21:07 609  	    var_product_offering_t_amount := var_product_offering_t_amount - ( var_product_offering_t_amount * var_percent_discount / 100 );
11:21:07 610  	  END IF;
11:21:07 611  
11:21:07 612  	  IF var_fixed_discount IS NOT NULL THEN
11:21:07 613  	    var_product_offering_t_amount := var_product_offering_t_amount - var_fixed_discount;
11:21:07 614  	  END IF;
11:21:07 615  
11:21:07 616  	  var_total_amount := var_total_amount + var_product_offering_t_amount;
11:21:07 617  	END LOOP;
11:21:07 618  	var_final_amount := var_total_amount;
11:21:07 619  	RETURN var_final_amount;
11:21:07 620  
11:21:07 621  EXCEPTION
11:21:07 622  WHEN BAD_OFFER_ID THEN
11:21:07 623  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:07 624  	  SPROC_NAME, 'No such offer');
11:21:07 625  WHEN OTHERS THEN
11:21:07 626  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:07 627  	  SPROC_NAME, 'Unknown error', SQLERRM);
11:21:07 628  END CALCULATE_OFFER_AMOUNT;
11:21:07 629  
11:21:07 630  /******************************************************************************/
11:21:07 631  
11:21:07 632  FUNCTION CALCULATE_OFFER_CHAIN_AMOUNT (
11:21:07 633  	in_offer_chain_id IN NUMBER
11:21:07 634  ) RETURN NUMBER AS
11:21:07 635  -- VARIABLES
11:21:07 636  SPROC_NAME	     CONSTANT VARCHAR2(28) := 'CALCULATE_OFFER_CHAIN_AMOUNT';
11:21:07 637  temp_offer_chain_id    NUMBER;
11:21:07 638  var_first_offer_id     NUMBER;
11:21:07 639  -- EXCEPTIONS
11:21:07 640  BAD_OFFER_CHAIN_ID      EXCEPTION;
11:21:07 641  CAN_NOT_GET_FIRST_OFFER EXCEPTION;
11:21:07 642  EXCEPTION_MESSAGE       VARCHAR2(1024);
11:21:07 643  BEGIN
11:21:07 644  
11:21:07 645  	BEGIN
11:21:07 646  	  SELECT
11:21:07 647  	    OFFER_CHAIN.ID into temp_offer_chain_id
11:21:07 648  	  FROM
11:21:07 649  	    OFFER_CHAIN
11:21:07 650  	  WHERE
11:21:07 651  	    OFFER_CHAIN.ID = in_offer_chain_id;
11:21:07 652  	  EXCEPTION
11:21:07 653  	    WHEN NO_DATA_FOUND THEN
11:21:07 654  	      RAISE BAD_OFFER_CHAIN_ID;
11:21:07 655  	END;
11:21:07 656  
11:21:07 657  	BEGIN
11:21:07 658  	  PROCS_OFFER_CHAIN_V18.GET_FIRST_OFFER(
11:21:07 659  	    in_offer_chain_id => in_offer_chain_id,
11:21:07 660  	    out_offer_id      => var_first_offer_id
11:21:07 661  	  );
11:21:07 662  	  EXCEPTION
