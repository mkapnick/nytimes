11:21:09 2703  PROCEDURE SWITCH_FINANCIAL_INSTRUMENT (
11:21:09 2704  	 in_old_fin_instrument_id   IN NUMBER,
11:21:09 2705  	 in_old_fin_instrument_type IN NUMBER,
11:21:09 2706  	 in_new_fin_instrument_id   IN NUMBER,
11:21:09 2707  	 in_new_fin_instrument_type IN NUMBER,
11:21:09 2708  	 in_updated_by		    IN VARCHAR2
11:21:09 2709  ) AS
11:21:09 2710  SPROC_NAME CONSTANT VARCHAR2(27) := 'SWITCH_FINANCIAL_INSTRUMENT';
11:21:09 2711  -- variables
11:21:09 2712  temp_out_transaction_id NUMBER;
11:21:09 2713  temp_out_charge_id      NUMBER;
11:21:09 2714  var_accounts_count      NUMBER;
11:21:09 2715  var_transaction_type_old  "TRANSACTION".TRANSACTION_TYPE_CODE%TYPE;
11:21:09 2716  var_transaction_type	 "TRANSACTION".TRANSACTION_TYPE_CODE%TYPE;
11:21:09 2717  -- EXCEPTIONS
11:21:09 2718  BAD_OLD_CC   EXCEPTION;
11:21:09 2719  BAD_OLD_PP   EXCEPTION;
11:21:09 2720  BAD_OLD_TYPE EXCEPTION;
11:21:09 2721  BAD_NEW_CC   EXCEPTION;
11:21:09 2722  BAD_NEW_PP   EXCEPTION;
11:21:09 2723  BAD_NEW_TYPE EXCEPTION;
11:21:09 2724  DIFFERENT_OWNERS EXCEPTION;
11:21:09 2725  BEGIN
11:21:09 2726  
11:21:09 2727  	 IF in_old_fin_instrument_type = GLOBAL_ENUMS_V18.INSTRUMENT_CREDIT_CARD THEN
11:21:09 2728  	   IF PROCS_FIN_INSTRUMENTS_V18.IS_CREDIT_CARD_EXISTS(in_old_fin_instrument_id) = GLOBAL_CONSTANTS_V18.FALSE THEN
11:21:09 2729  	     -- throw exception: bad old credit card
11:21:09 2730  	     RAISE BAD_OLD_CC;
11:21:09 2731  	   END IF;
11:21:09 2732  	 ELSIF in_old_fin_instrument_type = GLOBAL_ENUMS_V18.INSTRUMENT_PAYPAL THEN
11:21:09 2733  	   IF PROCS_FIN_INSTRUMENTS_V18.IS_PAYPAL_EXISTS(in_old_fin_instrument_id) = GLOBAL_CONSTANTS_V18.FALSE THEN
11:21:09 2734  	     -- throw exception: bad old paypal
11:21:09 2735  	     RAISE BAD_OLD_PP;
11:21:09 2736  	   END IF;
11:21:09 2737  	 ELSE
11:21:09 2738  	   -- throw exception: bad instrument type
11:21:09 2739  	   RAISE BAD_OLD_TYPE;
11:21:09 2740  	 END IF;
11:21:09 2741  
11:21:09 2742  	 IF in_new_fin_instrument_type = GLOBAL_ENUMS_V18.INSTRUMENT_CREDIT_CARD THEN
11:21:09 2743  	   IF PROCS_FIN_INSTRUMENTS_V18.IS_CREDIT_CARD_EXISTS(in_new_fin_instrument_id) = GLOBAL_CONSTANTS_V18.FALSE THEN
11:21:09 2744  	     -- throw exception: bad new credit card
11:21:09 2745  	     RAISE BAD_NEW_CC;
11:21:09 2746  	   END IF;
11:21:09 2747  	 ELSIF in_new_fin_instrument_type = GLOBAL_ENUMS_V18.INSTRUMENT_PAYPAL THEN
11:21:09 2748  	   IF PROCS_FIN_INSTRUMENTS_V18.IS_PAYPAL_EXISTS(in_new_fin_instrument_id) = GLOBAL_CONSTANTS_V18.FALSE THEN
11:21:09 2749  	     -- throw exception: bad new paypal
11:21:09 2750  	     RAISE BAD_NEW_PP;
11:21:09 2751  	   END IF;
11:21:09 2752  	 ELSE
11:21:09 2753  	   -- throw exception: bad new instrument type
11:21:09 2754  	   RAISE BAD_NEW_TYPE;
11:21:09 2755  	 END IF;
11:21:09 2756  
11:21:09 2757  	 -- Check that owner of both instruments - same man
11:21:09 2758  
11:21:09 2759  	 SELECT count(1) into var_accounts_count FROM (
11:21:09 2760  	   SELECT
11:21:09 2761  	     CC.ACCOUNT_ID
11:21:09 2762  	   FROM
11:21:09 2763  	     CREDIT_CARD CC
11:21:09 2764  	   WHERE
11:21:09 2765  	     (
11:21:09 2766  	       CC.ID = in_old_fin_instrument_id
11:21:09 2767  	       AND in_old_fin_instrument_type = GLOBAL_ENUMS_V18.INSTRUMENT_CREDIT_CARD
11:21:09 2768  	     )
11:21:09 2769  	     OR
11:21:09 2770  	     (
11:21:09 2771  	       CC.ID = in_new_fin_instrument_id
11:21:09 2772  	       AND in_new_fin_instrument_type = GLOBAL_ENUMS_V18.INSTRUMENT_CREDIT_CARD
11:21:09 2773  	     )
11:21:09 2774  	   UNION
11:21:09 2775  	   SELECT
11:21:09 2776  	     PP.ACCOUNT_ID
11:21:09 2777  	   FROM
11:21:09 2778  	     PAYPAL PP
11:21:09 2779  	   WHERE
11:21:09 2780  	     (
11:21:09 2781  	       PP.ID = in_old_fin_instrument_id
11:21:09 2782  	       AND in_old_fin_instrument_type = GLOBAL_ENUMS_V18.INSTRUMENT_PAYPAL
11:21:09 2783  	     )
11:21:09 2784  	     OR
11:21:09 2785  	     (
11:21:09 2786  	       PP.ID = in_new_fin_instrument_id
11:21:09 2787  	       AND in_new_fin_instrument_type = GLOBAL_ENUMS_V18.INSTRUMENT_PAYPAL
11:21:09 2788  	     )
11:21:09 2789  	 )
11:21:09 2790  	 WHERE
11:21:09 2791  	   account_id IS NOT NULL;
11:21:09 2792  
11:21:09 2793  	 IF (var_accounts_count > 1) THEN
11:21:09 2794  	   -- Throw exception: different owners of instruments
11:21:09 2795  	   RAISE DIFFERENT_OWNERS;
11:21:09 2796  	 END IF;
11:21:09 2797  
11:21:09 2798  	 FOR f_sub IN (
11:21:09 2799  	   select
11:21:09 2800  	     s.id
11:21:09 2801  	   FROM
11:21:09 2802  	     subscription s
11:21:09 2803  	   WHERE
11:21:09 2804  	     (
11:21:09 2805  	       s.subscription_status_id = GLOBAL_STATUSES_V18.SUBSCRIPTION_ACTIVE OR
11:21:09 2806  	       s.subscription_status_id = GLOBAL_STATUSES_V18.SUBSCRIPTION_SUSPENDED OR
11:21:09 2807  	       s.subscription_status_id = GLOBAL_STATUSES_V18.SUBSCRIPTION_IN_GRACE_PERIOD
11:21:09 2808  	     )
11:21:09 2809  	     AND
11:21:09 2810  	     s.instrument_type_id = in_old_fin_instrument_type AND
11:21:09 2811  	     s.instrument_id = in_old_fin_instrument_id
11:21:09 2812  	 ) LOOP
11:21:09 2813  	   PROCS_SUBSCRIPTION_CRU_V18.UPDATE_SUBSCRIPTION(
11:21:09 2814  	     in_subscription_id => f_sub.id,
11:21:09 2815  	     in_instrument_type_id => in_new_fin_instrument_type,
11:21:09 2816  	     in_instrument_id => in_new_fin_instrument_id,
11:21:09 2817  	     in_updated_by => in_updated_by
11:21:09 2818  	   );
11:21:09 2819  	 END LOOP;
11:21:09 2820  
11:21:09 2821  	 FOR f_open_charge IN (
11:21:09 2822  	   select
11:21:09 2823  	     ch.id,
11:21:09 2824  	     ch.invoice_id,
11:21:09 2825  	     ch.transaction_id,
11:21:09 2826  	     ch.charge_amount
11:21:09 2827  	   FROM
11:21:09 2828  	     charge ch
11:21:09 2829  	   WHERE
11:21:09 2830  	     ch.instrument_type_id = in_old_fin_instrument_type
11:21:09 2831  	     AND ch.instrument_id = in_old_fin_instrument_id
11:21:09 2832  	     AND ch.charge_status_id = GLOBAL_STATUSES_V18.CHARGE_OPENED
11:21:09 2833  	 ) LOOP
11:21:09 2834  
11:21:09 2835  	   FOR f_pending_transaction IN (
11:21:09 2836  	     select
11:21:09 2837  	       id, transaction_amount, order_id, is_refund
11:21:09 2838  	     from
11:21:09 2839  	       transaction
11:21:09 2840  	     where
11:21:09 2841  	       id = f_open_charge.transaction_id
11:21:09 2842  	       and transaction_status_id = GLOBAL_STATUSES_V18.TRANSACTION_PENDING
11:21:09 2843  	   ) LOOP
11:21:09 2844  
11:21:09 2845  	     SELECT
11:21:09 2846  	       DECODE(TRANSACTION_TYPE_CODE, 'RECURRING_BILLING', 'RECURRING_BILLING_USER_UPDATE',
11:21:09 2847  					     'GRACE_PERIOD_FINAL', 'GRACE_PERIOD_USER_UPDATE',
11:21:09 2848  					     TRANSACTION_TYPE_CODE)
11:21:09 2849  	     INTO var_transaction_type
11:21:09 2850  	     FROM
11:21:09 2851  	       Transaction
11:21:09 2852  	     WHERE
11:21:09 2853  	       id = f_pending_transaction.id
11:21:09 2854  	       AND ROWNUM <= 1;
11:21:09 2855  
11:21:09 2856  	     PROCS_TRANSACTION_V18.CREATE_TRANSACTION (
11:21:09 2857  	       in_transaction_id => NULL,
11:21:09 2858  	       in_status_id  => GLOBAL_STATUSES_V18.TRANSACTION_PENDING,
11:21:09 2859  	       in_amount     => f_pending_transaction.transaction_amount,
11:21:09 2860  	       in_created_by => in_updated_by,
11:21:09 2861  	       in_order_id   => null,
11:21:09 2862  	       in_is_refund  => f_pending_transaction.is_refund,
11:21:09 2863  	       in_transaction_type_code => var_transaction_type,
11:21:09 2864  	       out_transaction_id => temp_out_transaction_id
11:21:09 2865  	     );
11:21:09 2866  
11:21:09 2867  	     PROCS_TRANSACTION_V18.UPDATE_TRANSACTION_STATUS(
11:21:09 2868  	       in_transaction_id => f_pending_transaction.id,
11:21:09 2869  	       in_updated_by	 => in_updated_by,
11:21:09 2870  	       in_transaction_status_id  => GLOBAL_STATUSES_V18.TRANSACTION_CLOSED
11:21:09 2871  	     );
11:21:09 2872  
11:21:09 2873  	     -- Create new charge
11:21:09 2874  	     PROCS_CHARGE_V18.CREATE_CHARGE (
11:21:09 2875  	       in_invoice_id	     => f_open_charge.invoice_id,
11:21:09 2876  	       in_transaction_id     => temp_out_transaction_id,
11:21:09 2877  	       in_instrument_type_id => in_new_fin_instrument_type,
11:21:09 2878  	       in_instrument_id      => in_new_fin_instrument_id,
11:21:09 2879  	       in_charge_amount      => f_open_charge.charge_amount,
11:21:09 2880  	       in_created_by	     => in_updated_by,
11:21:09 2881  	       in_charge_status_id   => GLOBAL_STATUSES_V18.CHARGE_OPENED,
11:21:09 2882  	       out_charge_id	     => temp_out_charge_id
11:21:09 2883  	     );
11:21:09 2884  	     -- Cancel old charge
11:21:09 2885  	     PROCS_CHARGE_V18.UPDATE_CHARGE_STATUS(
11:21:09 2886  	       in_charge_id	   => f_open_charge.id,
11:21:09 2887  	       in_updated_by	   => in_updated_by,
11:21:09 2888  	       in_charge_status_id => GLOBAL_STATUSES_V18.CHARGE_CANCELED
11:21:09 2889  	     );
11:21:09 2890  
11:21:09 2891  	     PROCS_ADJUSTMENTS_V18.UPDATE_INVOICE_ADJUSTMENT(
11:21:09 2892  	       IN_INVOICE_ID => f_open_charge.invoice_id,
11:21:09 2893  	       IN_ORIGINAL_CHARGE_ID => f_open_charge.id,
11:21:09 2894  	       IN_CHARGE_ID => temp_out_charge_id,
11:21:09 2895  	       IN_UPDATED_BY => in_updated_by
11:21:09 2896  	     );
11:21:09 2897  
11:21:09 2898  	   END LOOP;
11:21:09 2899  	 END LOOP;
11:21:09 2900  
11:21:09 2901  EXCEPTION
11:21:09 2902  WHEN BAD_OLD_CC THEN
11:21:09 2903  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 2904  	   SPROC_NAME, 'Trying to switch from non existing credit card');
11:21:09 2905  WHEN BAD_OLD_PP THEN
11:21:09 2906  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 2907  	   SPROC_NAME, 'Trying to switch from non existing paypal');
11:21:09 2908  WHEN BAD_OLD_TYPE THEN
11:21:09 2909  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 2910  	   SPROC_NAME, 'Trying to switch from unknown/unsupported financial instrument');
11:21:09 2911  WHEN BAD_NEW_CC THEN
11:21:09 2912  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 2913  	   SPROC_NAME, 'Trying to switch to non existing credit card');
11:21:09 2914  WHEN BAD_NEW_PP THEN
11:21:09 2915  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 2916  	   SPROC_NAME, 'Trying to switch to non existing paypal');
11:21:09 2917  WHEN BAD_NEW_TYPE THEN
11:21:09 2918  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 2919  	   SPROC_NAME, 'Trying to switch to unknown/unsupported financial instrument');
11:21:09 2920  WHEN DIFFERENT_OWNERS THEN
