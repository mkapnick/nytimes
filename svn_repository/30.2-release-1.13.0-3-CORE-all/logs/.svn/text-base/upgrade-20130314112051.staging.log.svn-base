11:21:08 2289  PROCEDURE FINALIZE_CANCELATION (
11:21:08 2290  /*
11:21:08 2291  APP_EXCEPTION_CODES_V18.NOT_FOUND
11:21:08 2292  APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR
11:21:08 2293  APP_EXCEPTION_CODES_V18.INTERNAL_ERROR
11:21:08 2294  */
11:21:08 2295  --  in_subscription_id	 IN SUBSCRIPTION.ID%TYPE,
11:21:08 2296  --  in_cancelation_reason IN SUBSCRIPTION_CANCEL_REASON.VALUE%TYPE,
11:21:08 2297  --  in_cancelation_date	 IN SUBSCRIPTION.CANCELLATION_DATE%TYPE,
11:21:08 2298  --  in_note		 IN SUBSCRIPTION_NOTE.NOTE%TYPE,
11:21:08 2299  --  in_agent_id		 IN SUBSCRIPTION_NOTE.AGENT_ID%TYPE,
11:21:08 2300  --  in_updated_by	 IN SUBSCRIPTION.UPDATED_BY%TYPE
11:21:08 2301  	 in_subscription_id    IN NUMBER,
11:21:08 2302  	 in_cancelation_reason IN VARCHAR2,
11:21:08 2303  	 in_cancelation_date   IN DATE,
11:21:08 2304  	 in_note	       IN VARCHAR2,
11:21:08 2305  	 in_agent_id	       IN NUMBER,
11:21:08 2306  	 in_updated_by	       IN VARCHAR2
11:21:08 2307  ) AS
11:21:08 2308  SPROC_NAME CONSTANT VARCHAR2(20) := 'FINALIZE_CANCELATION';
11:21:08 2309  -- VARIABLES
11:21:08 2310  var_current_subscr_status SUBSCRIPTION.SUBSCRIPTION_STATUS_ID%TYPE;
11:21:08 2311  var_sct_id		 SUBSCRIPTION.SCT_ID%TYPE;
11:21:08 2312  var_active_invoices_count NUMBER;
11:21:08 2313  var_license_to_disgrace	 LICENSE.ID%TYPE;
11:21:08 2314  var_now			 DATE := SYSDATE;
11:21:08 2315  -- EXCEPTIONS
11:21:08 2316  BAD_SUBSCRIPTION_ID	   EXCEPTION;
11:21:08 2317  BAD_SUBSCRIPTION_STATUS	   EXCEPTION;
11:21:08 2318  BAD_CANCELATION_REASON	   EXCEPTION;
11:21:08 2319  CAN_NOT_UPDATE_SUBSCRIPTION EXCEPTION;
11:21:08 2320  ACTIVE_INVOICES_FOUND	   EXCEPTION;
11:21:08 2321  CAN_NOT_CREATE_NOTE	   EXCEPTION;
11:21:08 2322  EXCEPTION_MESSAGE	   VARCHAR2(1024);
11:21:08 2323  BEGIN
11:21:08 2324  
11:21:08 2325  	 -- Get current subscription status
11:21:08 2326  	 BEGIN
11:21:08 2327  	   SELECT
11:21:08 2328  	     SUBSCRIPTION.SUBSCRIPTION_STATUS_ID into var_current_subscr_status
11:21:08 2329  	   FROM
11:21:08 2330  	     SUBSCRIPTION
11:21:08 2331  	   WHERE
11:21:08 2332  	     SUBSCRIPTION.ID = in_subscription_id;
11:21:08 2333  	   EXCEPTION
11:21:08 2334  	     WHEN NO_DATA_FOUND THEN
11:21:08 2335  	       RAISE BAD_SUBSCRIPTION_ID;
11:21:08 2336  	 END;
11:21:08 2337  
11:21:08 2338  	 -- Check that subscription reason is correct
11:21:08 2339  	 BEGIN
11:21:08 2340  	   SELECT
11:21:08 2341  	     SUBSCRIPTION_CANCEL_REASON.ID into var_sct_id
11:21:08 2342  	   FROM
11:21:08 2343  	     SUBSCRIPTION_CANCEL_REASON
11:21:08 2344  	   WHERE
11:21:08 2345  	     SUBSCRIPTION_CANCEL_REASON.VALUE LIKE in_cancelation_reason
11:21:08 2346  	     AND ROWNUM <= 1;
11:21:08 2347  	   EXCEPTION
11:21:08 2348  	     WHEN NO_DATA_FOUND THEN
11:21:08 2349  	       RAISE BAD_CANCELATION_REASON;
11:21:08 2350  	 END;
11:21:08 2351  
11:21:08 2352  	 -- Check for invoices with active licenses
11:21:08 2353  	 SELECT
11:21:08 2354  	   COUNT(*) into var_active_invoices_count
11:21:08 2355  	 FROM
11:21:08 2356  	   LICENSE
11:21:08 2357  	   INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
11:21:08 2358  	 WHERE
11:21:08 2359  	   LICENSE.LICENSE_STATUS_ID in (GLOBAL_STATUSES_V18.LICENSE_ACTIVE, GLOBAL_STATUSES_V18.LICENSE_IN_GRACE_PERIOD)
11:21:08 2360  	   AND LICENSE.SUBSCRIPTION_ID = in_subscription_id;
11:21:08 2361  
11:21:08 2362  	 IF var_active_invoices_count > 0 THEN
11:21:08 2363  	   RAISE ACTIVE_INVOICES_FOUND;
11:21:08 2364  	 END IF;
11:21:08 2365  
11:21:08 2366  	 -- Check that subscription is active
11:21:08 2367  	 IF var_current_subscr_status != GLOBAL_STATUSES_V18.SUBSCRIPTION_ACTIVE AND
11:21:08 2368  	    var_current_subscr_status != GLOBAL_STATUSES_V18.SUBSCRIPTION_SUSPENDED AND
11:21:08 2369  	    var_current_subscr_status != GLOBAL_STATUSES_V18.SUBSCRIPTION_IN_GRACE_PERIOD THEN
11:21:08 2370  	   RAISE BAD_SUBSCRIPTION_STATUS;
11:21:08 2371  	 END IF;
11:21:08 2372  
11:21:08 2373  	 -- Update subscription data
11:21:08 2374  	 BEGIN
11:21:08 2375  	   PROCS_SUBSCRIPTION_CRU_V18.UPDATE_SUBSCRIPTION(
11:21:08 2376  	     in_subscription_id        => in_subscription_id,
11:21:08 2377  	     in_subscription_status_id => GLOBAL_STATUSES_V18.SUBSCRIPTION_CANCELED,
11:21:08 2378  	     in_cancelation_date       => in_cancelation_date,
11:21:08 2379  	     in_updated_by	       => in_updated_by,
11:21:08 2380  	     in_sct_id		       => var_sct_id
11:21:08 2381  	   );
11:21:08 2382  	   EXCEPTION
11:21:08 2383  	     WHEN OTHERS THEN
11:21:08 2384  	       EXCEPTION_MESSAGE := SQLERRM;
11:21:08 2385  	       RAISE CAN_NOT_UPDATE_SUBSCRIPTION;
11:21:08 2386  	 END;
11:21:08 2387  
11:21:08 2388  	 -- Terminate grace period for licenses in grace [SAR-31]
11:21:08 2389  	 BEGIN
11:21:08 2390  	   SELECT
11:21:08 2391  	     LICENSE.ID into var_license_to_disgrace
11:21:08 2392  	   FROM
11:21:08 2393  	     LICENSE
11:21:08 2394  	   WHERE
11:21:08 2395  	     LICENSE.SUBSCRIPTION_ID = in_subscription_id
11:21:08 2396  	     AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V18.LICENSE_IN_GRACE_PERIOD
11:21:08 2397  	     AND ROWNUM <= 1
11:21:08 2398  	   ORDER BY
11:21:08 2399  	     CREATE_DATE DESC;
11:21:08 2400  	 EXCEPTION
11:21:08 2401  	   WHEN NO_DATA_FOUND THEN
11:21:08 2402  	     var_license_to_disgrace := NULL;
11:21:08 2403  	 END;
11:21:08 2404  
11:21:08 2405  	 IF var_license_to_disgrace IS NOT NULL THEN
11:21:08 2406  	   PROCS_LICENSE_CRU_V18.UPDATE_LICENSE(
11:21:08 2407  	     in_license_id	     => var_license_to_disgrace,
11:21:08 2408  	     in_license_status_id    => GLOBAL_STATUSES_V18.LICENSE_CLOSED,
11:21:08 2409  	     in_updated_by	     => in_updated_by
11:21:08 2410  	   );
11:21:08 2411  	 END IF;
11:21:08 2412  
11:21:08 2413  	 -- Annotate subscription
11:21:08 2414  	 IF in_note IS NOT NULL THEN
11:21:08 2415  	   BEGIN
11:21:08 2416  	     PROCS_SUBSCRIPTION_V18.ANNOTATE_SUBSCRIPTION(
11:21:08 2417  	       in_subscription_id,
11:21:08 2418  	       in_agent_id,
11:21:08 2419  	       in_note,
11:21:08 2420  	       in_updated_by
11:21:08 2421  	     );
11:21:08 2422  	     EXCEPTION
11:21:08 2423  	      WHEN OTHERS THEN
11:21:08 2424  		EXCEPTION_MESSAGE := SQLERRM;
11:21:08 2425  		RAISE CAN_NOT_CREATE_NOTE;
11:21:08 2426  	   END;
11:21:08 2427  	 END IF;
11:21:08 2428  
11:21:08 2429  EXCEPTION
11:21:08 2430  WHEN BAD_SUBSCRIPTION_ID THEN
11:21:08 2431  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:08 2432  	   SPROC_NAME, 'No such subscription');
11:21:08 2433  WHEN BAD_SUBSCRIPTION_STATUS THEN
11:21:08 2434  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INVALID_PARAMETER,
11:21:08 2435  	   SPROC_NAME, 'Bad current subscription status');
11:21:08 2436  WHEN BAD_CANCELATION_REASON THEN
11:21:08 2437  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INVALID_PARAMETER,
11:21:08 2438  	   SPROC_NAME, 'Bad cancellation reason');
11:21:08 2439  WHEN CAN_NOT_UPDATE_SUBSCRIPTION THEN
11:21:08 2440  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:08 2441  	   SPROC_NAME, 'Could not update subscription data', EXCEPTION_MESSAGE);
11:21:08 2442  WHEN ACTIVE_INVOICES_FOUND THEN
11:21:08 2443  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.STATE_ERROR,
11:21:08 2444  	   SPROC_NAME, 'Invoices with active licenses found');
11:21:08 2445  WHEN CAN_NOT_CREATE_NOTE THEN
11:21:08 2446  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:08 2447  	   SPROC_NAME, 'Could not annotate subscription', EXCEPTION_MESSAGE);
11:21:08 2448  --WHEN OTHERS THEN
11:21:08 2449  --  PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:08 2450  --    SPROC_NAME, 'Unknown error', SQLERRM);
11:21:08 2451  END FINALIZE_CANCELATION;
