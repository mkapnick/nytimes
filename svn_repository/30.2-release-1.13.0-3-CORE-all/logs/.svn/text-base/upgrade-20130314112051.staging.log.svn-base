11:21:08 2211  	     END IF;
11:21:08 2212  	   end if;
11:21:08 2213  	 ELSE
11:21:08 2214  
11:21:08 2215  	   FOR f_transaction_to_close IN (
11:21:08 2216  	     SELECT DISTINCT CHARGE.TRANSACTION_ID AS "ID" FROM CHARGE WHERE CHARGE.INVOICE_ID = in_invoice_id and CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V18.CHARGE_OPENED
11:21:08 2217  	   )
11:21:08 2218  	   LOOP
11:21:08 2219  	     PROCS_TRANSACTION_V18.UPDATE_TRANSACTION_STATUS(
11:21:08 2220  	       in_transaction_id	=> f_transaction_to_close.ID,
11:21:08 2221  	       in_updated_by		=> in_updated_by,
11:21:08 2222  	       in_transaction_status_id => GLOBAL_STATUSES_V18.TRANSACTION_CLOSED
11:21:08 2223  	     );
11:21:08 2224  	   END LOOP;
11:21:08 2225  	   -- Needs to close charges. No refund.
11:21:08 2226  	   FOR f_charge_to_close IN (
11:21:08 2227  	     SELECT CHARGE.ID FROM CHARGE WHERE CHARGE.INVOICE_ID = in_invoice_id and CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V18.CHARGE_OPENED
11:21:08 2228  	   )
11:21:08 2229  	   LOOP
11:21:08 2230  	     PROCS_CHARGE_V18.UPDATE_CHARGE_STATUS(
11:21:08 2231  	       in_charge_id	   => f_charge_to_close.ID,
11:21:08 2232  	       in_updated_by	   => in_updated_by,
11:21:08 2233  	       in_charge_status_id => GLOBAL_STATUSES_V18.CHARGE_CANCELED
11:21:08 2234  	     );
11:21:08 2235  	   END LOOP;
11:21:08 2236  
11:21:08 2237  	   PROCS_INVOICE_V18.UPDATE_INVOICE_STATUS(
11:21:08 2238  	     in_invoice_id		    => in_invoice_id,
11:21:08 2239  	     in_updated_by		    => in_updated_by,
11:21:08 2240  	     in_invoice_status_id	    => GLOBAL_STATUSES_V18.INVOICE_CLOSED
11:21:08 2241  	   );
11:21:08 2242  
11:21:08 2243  	 END IF;
11:21:08 2244  	 -- update licenses
11:21:08 2245  	 IF(var_revoke = GLOBAL_CONSTANTS_V18.TRUE OR var_chargeback_amount > 0 OR (var_subscription_in_grace = GLOBAL_CONSTANTS_V18.FALSE AND var_billed = GLOBAL_CONSTANTS_V18.FALSE)) THEN
11:21:08 2246  	   FOR f_license_to_cancel IN (
11:21:08 2247  	     SELECT LICENSE.ID FROM LICENSE WHERE LICENSE.INVOICE_ID = in_invoice_id AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V18.LICENSE_ACTIVE
11:21:08 2248  	   )
11:21:08 2249  	   LOOP
