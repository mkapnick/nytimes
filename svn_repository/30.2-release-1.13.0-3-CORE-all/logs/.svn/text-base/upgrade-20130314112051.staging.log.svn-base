11:21:09 828  PROCEDURE SHOULD_MOVE_TO_GRACE(
11:21:09 829  	in_invoice_id  IN INVOICE.ID%TYPE,
11:21:09 830  	out_result     OUT NUMBER
11:21:09 831  ) AS
11:21:09 832  BEGIN
11:21:09 833  	-- if the invoice preceding the given invoice has no transaction attempts, then
11:21:09 834  	-- it is not billed out of Sartre. if so, then the associated subscription
11:21:09 835  	-- should be canceled after a final failed billing attempt--not moved to grace.
11:21:09 836  	SELECT DECODE(COUNT(1), 0, 0, 1) INTO out_result
11:21:09 837  	FROM CHARGE c
11:21:09 838  	INNER JOIN TRANSACTION t ON c.TRANSACTION_ID = t.ID
11:21:09 839  	INNER JOIN TRANSACTION_ATTEMPT ta ON ta.TRANSACTION_ID = t.ID
11:21:09 840  	WHERE c.INVOICE_ID = (
11:21:09 841  	  -- select previous invoice_id, or -1 if there is none
11:21:09 842  	  SELECT PREV_INVOICE_ID FROM (
11:21:09 843  	    SELECT i.ID, LAG(i.ID, 1, -1) OVER (ORDER BY i.CREATE_DATE) AS PREV_INVOICE_ID
11:21:09 844  	    FROM INVOICE i
11:21:09 845  	    INNER JOIN LICENSE l ON i.ID = l.INVOICE_ID
11:21:09 846  	    WHERE l.SUBSCRIPTION_ID = (
11:21:09 847  	      SELECT SUBSCRIPTION_ID FROM LICENSE WHERE INVOICE_ID = in_invoice_id
11:21:09 848  	    )
11:21:09 849  	  ) WHERE ID = in_invoice_id
11:21:09 850  	);
11:21:09 851  END SHOULD_MOVE_TO_GRACE;
11:21:09 852  
