11:21:08 845  PROCEDURE GET_GRACE_LICE_FOR_FINAL_TRANS (
11:21:08 846  	in_days_before_close	 IN NUMBER,
11:21:08 847  	in_num_licenses_to_fetch IN NUMBER,
11:21:08 848  	out_result_set		 OUT SYS_REFCURSOR
11:21:08 849  ) AS
11:21:08 850  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_GRACE_LICE_FOR_FINAL_TRANS';
11:21:08 851  BEGIN
11:21:08 852  	OPEN out_result_set FOR
11:21:08 853  	SELECT
11:21:08 854  	    *
11:21:08 855  	FROM
11:21:08 856  	    (
11:21:08 857  		SELECT
11:21:08 858  		    l.id
11:21:08 859  		FROM
11:21:08 860  		    license l
11:21:08 861  		JOIN
11:21:08 862  		    invoice i
11:21:08 863  		ON
11:21:08 864  		    l.invoice_id = i.id
11:21:08 865  		WHERE
11:21:08 866  		    i.invoice_status_id = GLOBAL_STATUSES_V18.INVOICE_OPEN
11:21:08 867  		AND l.license_status_id = GLOBAL_STATUSES_V18.LICENSE_IN_GRACE_PERIOD
11:21:08 868  		AND SYSDATE + in_days_before_close >= l.grace_end_date
11:21:08 869  		AND NOT EXISTS
11:21:08 870  		    (
11:21:08 871  			SELECT
11:21:08 872  			    1
11:21:08 873  			FROM
11:21:08 874  			    charge c
11:21:08 875  			WHERE
11:21:08 876  			    c.invoice_id = i.id
11:21:08 877  			AND c.charge_status_id = GLOBAL_STATUSES_V18.CHARGE_OPENED)
11:21:08 878  		AND rownum <= in_num_licenses_to_fetch * 10
11:21:08 879  		ORDER BY
11:21:08 880  		    dbms_random.value)
11:21:08 881  	WHERE
11:21:08 882  	    rownum <= in_num_licenses_to_fetch;
11:21:08 883  END GET_GRACE_LICE_FOR_FINAL_TRANS;
