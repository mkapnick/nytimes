11:21:09 671  
11:21:09 672  PROCEDURE GET_ACCOUNT_SUBSCRIPTIONS (
11:21:09 673  	  in_group_id	 IN  NUMBER,
11:21:09 674  	  in_start_date  IN DATE,
11:21:09 675  	  in_end_date	 IN DATE,
11:21:09 676  	  in_status	 IN NUMBER,
11:21:09 677  	  in_group_account_type IN VARCHAR2,
11:21:09 678  	  out_result_set OUT SYS_REFCURSOR
11:21:09 679  ) AS
11:21:09 680  -- VARIABLES
11:21:09 681  SPROC_NAME     CONSTANT VARCHAR2(25) := 'GET_ACCOUNT_SUBSCRIPTIONS';
11:21:09 682  var_account_id NUMBER;
11:21:09 683  -- EXCEPTIONS
11:21:09 684  BAD_GROUP_ID	      EXCEPTION;
11:21:09 685  BEGIN
11:21:09 686  	-- Get account id
11:21:09 687  	BEGIN
11:21:09 688  	  SELECT
11:21:09 689  	    ACCOUNT.ID INTO var_account_id
11:21:09 690  	  FROM
11:21:09 691  	    ACCOUNT
11:21:09 692  	  WHERE
11:21:09 693  	    ACCOUNT.GROUP_ID = in_group_id;
11:21:09 694  	  EXCEPTION
11:21:09 695  	    WHEN NO_DATA_FOUND THEN
11:21:09 696  	      RAISE BAD_GROUP_ID;
11:21:09 697  	END;
11:21:09 698  
11:21:09 699  	-- Get information about account subscriptions
11:21:09 700  	OPEN out_result_set FOR
11:21:09 701  	SELECT
11:21:09 702  	  in_group_id AS "GROUP_ID",
11:21:09 703  	  SUBSCRIPTION.ID AS "SUBSCRIPTION_ID",
11:21:09 704  	  SUBSCRIPTION.SUBSCRIPTION_STATUS_ID,
11:21:09 705  	  SUBSCRIPTION.PURCHASE_DATE,
11:21:09 706  	  SUBSCRIPTION.SUSPEND_DATE,
11:21:09 707  	  SUBSCRIPTION.REACTIVATION_DATE,
11:21:09 708  	  SUBSCRIPTION.CANCELLATION_DATE,
11:21:09 709  	  SUBSCRIPTION_CANCEL_REASON.VALUE as "CANCEL_TYPE",
11:21:09 710  	  SUBSCRIPTION.INSTRUMENT_ID,
11:21:09 711  	  SUBSCRIPTION.INSTRUMENT_TYPE_ID,
11:21:09 712  	  OFFER_CHAIN.ID AS "OFFER_CHAIN_ID",
11:21:09 713  	  OFFER_CHAIN.NAME,
11:21:09 714  	  OFFER_CHAIN.DESCRIPTION,
11:21:09 715  	  OFFER_CHAIN.PRODUCT_URI,
11:21:09 716  	  PROCS_SUBSCRIPTION_V18.CALC_SUBSCRIPTION_END_DATE(SUBSCRIPTION.ID) as "END_DATE",
11:21:09 717  	  PROCS_SUBSCRIPTION_V18.GET_RECENT_CHARGE(SUBSCRIPTION.ID) AS "RECENT_CHARGE",
11:21:09 718  	  PROCS_SUBSCRIPTION_V18.GET_RENEWAL_DATE(SUBSCRIPTION.ID) AS "RENEWAL_DATE",
11:21:09 719  	  PROCS_SUBSCRIPTION_V18.GET_BILLING_CYCLE(SUBSCRIPTION.ID) AS "BILLING_CYCLE",
11:21:09 720  	  PROCS_SUBSCRIPTION_V18.IS_SUBSCRIPTION_CANCELABLE(SUBSCRIPTION.ID) AS "IS_CANCELABLE",
11:21:09 721  	  ITUNES_RECEIPT.ID AS "ITUNES_RECEIPT_ID",
11:21:09 722  	  (
11:21:09 723  	    SELECT
11:21:09 724  	      MAX(ENTITLEMENT_END_DATE)
11:21:09 725  	      FROM LICENSE
11:21:09 726  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
11:21:09 727  	  )
11:21:09 728  	  as "ENT_END_DATE",
11:21:09 729  	  (
11:21:09 730  	    SELECT
11:21:09 731  	      MIN(START_DATE)
11:21:09 732  	      FROM LICENSE
11:21:09 733  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
11:21:09 734  	  )
11:21:09 735  	  as "ENT_START_DATE",
11:21:09 736  	  GROUP_ACCOUNT.SUBSCRIPTION_ID GA_SUBSCRIPTION_ID,
11:21:09 737  	  GROUP_ACCOUNT.ID GA_ID,
11:21:09 738  	  GROUP_ACCOUNT.GROUP_NAME GA_GROUP_NAME,
11:21:09 739  	  GROUP_ACCOUNT.FIRST_NAME GA_FIRST_NAME,
11:21:09 740  	  GROUP_ACCOUNT.LAST_NAME GA_LAST_NAME,
11:21:09 741  	  GROUP_ACCOUNT.EMAIL GA_EMAIL,
11:21:09 742  	  GROUP_ACCOUNT.PHONE GA_PHONE,
11:21:09 743  	  GROUP_ACCOUNT.ORGANIZATION_TYPE GA_ORGANIZATION_TYPE,
11:21:09 744  	  GROUP_ACCOUNT.SEATS GA_SEATS,
11:21:09 745  	  PROCS_GROUP_ACCOUNT_V18.F_GET_NUM_OCCUPIED_GROUP_SEATS(GROUP_ACCOUNT.ID) GA_SEATS_USED,
11:21:09 746  	  GROUP_ACCOUNT.IP GA_IP,
11:21:09 747  	  PROCS_SUBSCRIPTION_V18.GET_GIFT_CERT_CODE_BY_SUB_ID(SUBSCRIPTION.ID) GIFT_CERTIFICATE_CODE,
11:21:09 748  	  PROCS_ACCOUNT_V18.GET_GRACE_START_DATE(SUBSCRIPTION.ID) GRACE_START_DATE,
11:21:09 749  	  PROCS_ACCOUNT_V18.GET_GRACE_END_DATE(SUBSCRIPTION.ID) GRACE_END_DATE
11:21:09 750  	FROM
11:21:09 751  	  SUBSCRIPTION
11:21:09 752  	  INNER JOIN OFFER_CHAIN ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
11:21:09 753  	  LEFT JOIN SUBSCRIPTION_CANCEL_REASON ON SUBSCRIPTION.SCT_ID = SUBSCRIPTION_CANCEL_REASON.ID
11:21:09 754  	  LEFT JOIN ITUNES_RECEIPT ON SUBSCRIPTION.ID = ITUNES_RECEIPT.SUBSCRIPTION_ID
11:21:09 755  	  LEFT JOIN GROUP_ACCOUNT ON SUBSCRIPTION.ID = GROUP_ACCOUNT.SUBSCRIPTION_ID
11:21:09 756  	WHERE
11:21:09 757  	  SUBSCRIPTION.ACCOUNT_ID = var_account_id
11:21:09 758  	  AND (SUBSCRIPTION.SCT_ID IS NULL OR SUBSCRIPTION.SCT_ID != GLOBAL_STATUSES_V18.REAL_TIME_CANCEL_REASON)
11:21:09 759  	  AND SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = NVL(in_status, SUBSCRIPTION.SUBSCRIPTION_STATUS_ID)
11:21:09 760  	  AND PROCS_COMMON_V18.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE) >= NVL(in_start_date, PROCS_COMMON_V18.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE))
11:21:09 761  	  AND PROCS_COMMON_V18.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE) <= NVL(in_end_date, PROCS_COMMON_V18.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE))
11:21:09 762  	  AND (in_group_account_type IS NULL OR OFFER_CHAIN.GROUP_ACCOUNT_TYPE_ID = in_group_account_type);
11:21:09 763  
11:21:09 764  EXCEPTION
11:21:09 765  WHEN BAD_GROUP_ID THEN
11:21:09 766  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 767  	  SPROC_NAME, 'Can not find account with given group id');
11:21:09 768  WHEN OTHERS THEN
11:21:09 769  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:09 770  	  SPROC_NAME, 'Unknown error', SQLERRM);
11:21:09 771  END GET_ACCOUNT_SUBSCRIPTIONS;
11:21:09 772  
11:21:09 773  /******************************************************************************/
11:21:09 774  
11:21:09 775  PROCEDURE FREEZE_ACCOUNT (
11:21:09 776  /*
11:21:09 777  Throws exceptions:
11:21:09 778  APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 779  APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 780  APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR
11:21:09 781  */
11:21:09 782  	in_group_id   IN NUMBER,
11:21:09 783  	in_updated_by IN VARCHAR2,
11:21:09 784  	in_note       IN VARCHAR2,
11:21:09 785  	in_agent_id   IN NUMBER
11:21:09 786  ) AS
11:21:09 787  SPROC_NAME	    CONSTANT VARCHAR2(14) := 'FREEZE_ACCOUNT';
11:21:09 788  -- VARIABLES
11:21:09 789  var_account_id	    NUMBER;
11:21:09 790  var_account_status_id NUMBER;
11:21:09 791  -- EXCEPTIONS
11:21:09 792  BAD_GROUP_ID	    EXCEPTION;
11:21:09 793  BAD_ACCOUNT_STATUS_ID EXCEPTION;
11:21:09 794  CAN_NOT_CREATE_NOTE   EXCEPTION;
11:21:09 795  EXCEPTION_MESSAGE     VARCHAR2(1024);
11:21:09 796  BEGIN
11:21:09 797  
11:21:09 798  	-- Get account status, account id
11:21:09 799  	BEGIN
11:21:09 800  	  SELECT
11:21:09 801  	    ACCOUNT.ID,
11:21:09 802  	    ACCOUNT.ACCOUNT_STATUS_ID
11:21:09 803  	    into
11:21:09 804  	    var_account_id,
11:21:09 805  	    var_account_status_id
11:21:09 806  	  FROM
11:21:09 807  	    ACCOUNT
11:21:09 808  	  WHERE
11:21:09 809  	    ACCOUNT.GROUP_ID = in_group_id;
11:21:09 810  	  EXCEPTION
11:21:09 811  	    WHEN NO_DATA_FOUND THEN
11:21:09 812  	      RAISE BAD_GROUP_ID;
11:21:09 813  	END;
11:21:09 814  
11:21:09 815  	-- We can freeze only ACTIVE accounts
11:21:09 816  	IF var_account_status_id != GLOBAL_STATUSES_V18.ACCOUNT_ACTIVE
11:21:09 817  	  AND var_account_status_id != GLOBAL_STATUSES_V18.ACCOUNT_FROZEN THEN
11:21:09 818  	  RAISE BAD_ACCOUNT_STATUS_ID;
11:21:09 819  	END IF;
11:21:09 820  
11:21:09 821  	-- Set account status
11:21:09 822  	PROCS_ACCOUNT_V18.UPDATE_ACCOUNT_STATUS(
11:21:09 823  	  in_account_id        => var_account_id,
11:21:09 824  	  in_updated_by        => in_updated_by,
11:21:09 825  	  in_account_status_id => GLOBAL_STATUSES_V18.ACCOUNT_FROZEN
11:21:09 826  	);
11:21:09 827  
11:21:09 828  	-- Annotate account
11:21:09 829  	BEGIN
11:21:09 830  	  PROCS_ACCOUNT_V18.ANNOTATE_ACCOUNT(
11:21:09 831  	    in_group_id   => in_group_id,
11:21:09 832  	    in_agent_id   => in_agent_id,
11:21:09 833  	    in_note	  => in_note,
11:21:09 834  	    in_created_by => in_updated_by
11:21:09 835  	  );
11:21:09 836  	  EXCEPTION
11:21:09 837  	    WHEN OTHERS THEN
11:21:09 838  	      EXCEPTION_MESSAGE := SQLERRM;
11:21:09 839  	      RAISE CAN_NOT_CREATE_NOTE;
11:21:09 840  	END;
11:21:09 841  
11:21:09 842  EXCEPTION
11:21:09 843  WHEN BAD_GROUP_ID THEN
11:21:09 844  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 845  	  SPROC_NAME, 'No such group id');
11:21:09 846  WHEN BAD_ACCOUNT_STATUS_ID THEN
11:21:09 847  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.STATE_ERROR,
11:21:09 848  	  SPROC_NAME, 'Could not update this account. Status should to be active or frozen');
11:21:09 849  WHEN CAN_NOT_CREATE_NOTE THEN
11:21:09 850  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 851  	  SPROC_NAME, 'Could not annotate account', EXCEPTION_MESSAGE);
11:21:09 852  WHEN OTHERS THEN
11:21:09 853  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:09 854  	  SPROC_NAME, 'Unknown error', SQLERRM);
11:21:09 855  END FREEZE_ACCOUNT;
11:21:09 856  
11:21:09 857  /******************************************************************************/
11:21:09 858  
11:21:09 859  PROCEDURE GET_ACCOUNT_SUBSCR_INVOICES (
11:21:09 860  /*
11:21:09 861  Throws exceptions:
11:21:09 862  APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 863  APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR
11:21:09 864  */
11:21:09 865  	in_group_id	   IN  NUMBER,
11:21:09 866  	in_subscription_id IN NUMBER,
11:21:09 867  	out_result_set	   OUT SYS_REFCURSOR
11:21:09 868  ) AS
11:21:09 869  SPROC_NAME	   CONSTANT VARCHAR2(27) := 'GET_ACCOUNT_SUBSCR_INVOICES';
11:21:09 870  -- VARIABLES
11:21:09 871  var_account_id	   NUMBER;
11:21:09 872  temp_subscription_id NUMBER;
11:21:09 873  -- EXCEPTIONS
11:21:09 874  BAD_GROUP_ID	  EXCEPTION;
11:21:09 875  BAD_SUBSCRIPTION_ID EXCEPTION;
11:21:09 876  BEGIN
11:21:09 877  	-- Get account id
11:21:09 878  	BEGIN
11:21:09 879  	  SELECT
11:21:09 880  	    ACCOUNT.ID into var_account_id
11:21:09 881  	  FROM
11:21:09 882  	    ACCOUNT
11:21:09 883  	  WHERE
11:21:09 884  	    ACCOUNT.GROUP_ID = in_group_id;
11:21:09 885  	  EXCEPTION
11:21:09 886  	    WHEN NO_DATA_FOUND THEN
11:21:09 887  	      RAISE BAD_GROUP_ID;
11:21:09 888  	END;
11:21:09 889  
11:21:09 890  	-- Check that subscription exists
11:21:09 891  	BEGIN
11:21:09 892  	  IF in_subscription_id IS NOT NULL THEN
11:21:09 893  	    SELECT
11:21:09 894  	      SUBSCRIPTION.ID into temp_subscription_id
11:21:09 895  	    FROM
11:21:09 896  	      SUBSCRIPTION
11:21:09 897  	    WHERE
11:21:09 898  	      SUBSCRIPTION.ID = in_subscription_id;
11:21:09 899  	  END IF;
11:21:09 900  	  EXCEPTION
11:21:09 901  	    WHEN NO_DATA_FOUND THEN
11:21:09 902  	      RAISE BAD_SUBSCRIPTION_ID;
11:21:09 903  	END;
11:21:09 904  
11:21:09 905  	OPEN out_result_set FOR
11:21:09 906  	SELECT DISTINCT
11:21:09 907  	  INVOICE.ID as "INVOICE_ID",
11:21:09 908  	  INVOICE.CREATE_DATE,
11:21:09 909  	  INVOICE.INVOICE_STATUS_ID,
11:21:09 910  	  PROCS_INVOICE_V18.F_CALCULATE_INVOICE_AMOUNT(INVOICE.ID) as "AMOUNT",
11:21:09 911  	  OFFER_CHAIN.ID as "OFFER_CHAIN_ID",
11:21:09 912  	  OFFER_CHAIN.NAME as "OFFER_CHAIN_NAME",
11:21:09 913  	  SUBSCRIPTION.ID as "SUBSCRIPTION_ID",
11:21:09 914  	  NULL as "GC_CODE",
11:21:09 915  	  NULL as "GC_ID"
11:21:09 916  	FROM
11:21:09 917  	  LICENSE
11:21:09 918  	  INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
11:21:09 919  	  INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
11:21:09 920  	  INNER JOIN OFFER_CHAIN ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
11:21:09 921  	WHERE
11:21:09 922  	  SUBSCRIPTION.ACCOUNT_ID = var_account_id
11:21:09 923  	  AND (SUBSCRIPTION.SCT_ID IS NULL OR SUBSCRIPTION.SCT_ID != GLOBAL_STATUSES_V18.REAL_TIME_CANCEL_REASON)
11:21:09 924  	  AND SUBSCRIPTION.ID = NVL(in_subscription_id, SUBSCRIPTION.ID);
11:21:09 925  
11:21:09 926  EXCEPTION
11:21:09 927  WHEN BAD_GROUP_ID THEN
11:21:09 928  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 929  	  SPROC_NAME, 'No such account');
11:21:09 930  WHEN BAD_SUBSCRIPTION_ID THEN
11:21:09 931  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
