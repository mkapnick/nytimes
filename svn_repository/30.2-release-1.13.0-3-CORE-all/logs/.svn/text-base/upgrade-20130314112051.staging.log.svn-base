11:21:08 1144  	   INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
11:21:08 1145  	   INNER JOIN OFFER ON LICENSE.OFFER_ID = OFFER.ID
11:21:08 1146  	 WHERE
11:21:08 1147  	   --LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V18.LICENSE_PROCESSED
11:21:08 1148  	   --AND
11:21:08 1149  	   SUBSCRIPTION.ID = in_subscription_id
11:21:08 1150  	   AND ROWNUM <= 1
11:21:08 1151  	 ORDER BY
