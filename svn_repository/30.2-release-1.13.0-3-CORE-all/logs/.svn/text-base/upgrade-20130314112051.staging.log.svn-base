11:21:07 699  	  WHERE
11:21:07 700  	    OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
11:21:07 701  	  ORDER BY
11:21:07 702  	    OFFER_OFFER_CHAIN.INDEX_VALUE ASC
11:21:07 703  	)
11:21:07 704  	WHERE
