11:21:09 580  
11:21:09 581  PROCEDURE UPDATE_CREDIT_CARD (
11:21:09 582  /*
11:21:09 583  Throws exceptions:
11:21:09 584  APP_EXCEPTION_CODES_V18.NOT_FOUND
11:21:09 585  APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR
11:21:09 586  */
11:21:09 587  	in_credit_card_id	  IN NUMBER,
11:21:09 588  	in_updated_by		  IN VARCHAR2,
11:21:09 589  	in_instrument_name	  IN VARCHAR2,
11:21:09 590  	in_is_default		  IN NUMBER
11:21:09 591  ) AS
11:21:09 592  SPROC_NAME CONSTANT VARCHAR2(18) := 'UPDATE_CREDIT_CARD';
11:21:09 593  -- VARIABLES
11:21:09 594  var_account_id NUMBER;
11:21:09 595  var_group_id   NUMBER;
11:21:09 596  temp_cc_rownum NUMBER;
11:21:09 597  current_def_instrument_type_id NUMBER;
11:21:09 598  current_def_instrument_id      NUMBER;
11:21:09 599  -- EXCEPTION
11:21:09 600  CAN_NOT_SET_DEF_FINANCIAL  EXCEPTION;
11:21:09 601  BAD_IS_DEFAULT_VALUE	 EXCEPTION;
11:21:09 602  CAN_NOT_GET_DEF_FINANCIAL  EXCEPTION;
11:21:09 603  CAN_NOT_DEL_DEF_FINANCIAL  EXCEPTION;
11:21:09 604  EXCEPTION_MESSAGE 	 VARCHAR2(1024);
11:21:09 605  BEGIN
11:21:09 606  
11:21:09 607  	-- Get account id
11:21:09 608  	SELECT
11:21:09 609  	  CREDIT_CARD.ACCOUNT_ID
11:21:09 610  	  into
11:21:09 611  	  var_account_id
11:21:09 612  	FROM
11:21:09 613  	  CREDIT_CARD
11:21:09 614  	WHERE
11:21:09 615  	  CREDIT_CARD.ID = in_credit_card_id;
11:21:09 616  
11:21:09 617  	-- Get group id
11:21:09 618  	SELECT
11:21:09 619  	  ACCOUNT.GROUP_ID into var_group_id
11:21:09 620  	FROM
11:21:09 621  	  ACCOUNT
11:21:09 622  	WHERE
11:21:09 623  	  ACCOUNT.ID = var_account_id;
11:21:09 624  
11:21:09 625  	-- Check that passed data is correct
11:21:09 626  	IF in_is_default != GLOBAL_CONSTANTS_V18.TRUE
11:21:09 627  	  AND in_is_default != GLOBAL_CONSTANTS_V18.FALSE
11:21:09 628  	  AND in_is_default IS NOT NULL THEN
11:21:09 629  	  RAISE BAD_IS_DEFAULT_VALUE;
11:21:09 630  	END IF;
11:21:09 631  
11:21:09 632  	-- Update credit card
11:21:09 633  	IF in_instrument_name IS NOT NULL THEN
11:21:09 634  	  PROCS_FIN_INSTRUMENTS_CRU_V18.UPDATE_CREDIT_CARD(
11:21:09 635  	    in_credit_card_id  => in_credit_card_id,
11:21:09 636  	    in_updated_by      => in_updated_by,
11:21:09 637  	    in_instrument_name => in_instrument_name
11:21:09 638  	  );
11:21:09 639  	END IF;
11:21:09 640  
11:21:09 641  	-- Set default financial instrument
11:21:09 642  	IF in_is_default = GLOBAL_CONSTANTS_V18.TRUE THEN
11:21:09 643  	  BEGIN
11:21:09 644  	    PROCS_FIN_INSTRUMENTS_V18.SET_DEF_FINANCIAL_INSTRUMENT(
11:21:09 645  	      in_group_id	    => var_group_id,
11:21:09 646  	      in_instrument_type_id => GLOBAL_ENUMS_V18.INSTRUMENT_CREDIT_CARD,
11:21:09 647  	      in_instrument_id	    => in_credit_card_id,
11:21:09 648  	      in_updated_by	    => in_updated_by
11:21:09 649  	    );
11:21:09 650  	    EXCEPTION
11:21:09 651  	      WHEN OTHERS THEN
11:21:09 652  		EXCEPTION_MESSAGE := SQLERRM;
11:21:09 653  		RAISE CAN_NOT_SET_DEF_FINANCIAL;
11:21:09 654  	  END;
11:21:09 655  	END IF;
11:21:09 656  
11:21:09 657  	-- Set default financial instrument
11:21:09 658  	IF in_is_default = GLOBAL_CONSTANTS_V18.FALSE THEN
11:21:09 659  	  BEGIN
11:21:09 660  	    GET_DEF_FINANCIAL_INSTRUMENT(
11:21:09 661  	      in_group_id	     => var_group_id,
11:21:09 662  	      out_instrument_type_id => current_def_instrument_type_id,
11:21:09 663  	      out_instrument_id      => current_def_instrument_id
11:21:09 664  	    );
11:21:09 665  	  EXCEPTION
11:21:09 666  	    WHEN OTHERS THEN
11:21:09 667  	      EXCEPTION_MESSAGE := SQLERRM;
11:21:09 668  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
11:21:09 669  	  END;
11:21:09 670  	  IF current_def_instrument_type_id = GLOBAL_ENUMS_V18.INSTRUMENT_CREDIT_CARD
11:21:09 671  	    AND current_def_instrument_id = in_credit_card_id THEN
11:21:09 672  	    BEGIN
11:21:09 673  	      DEL_DEF_FINANCIAL_INSTRUMENT(
11:21:09 674  		in_group_id => var_group_id
11:21:09 675  	      );
11:21:09 676  	      EXCEPTION
11:21:09 677  		WHEN OTHERS THEN
11:21:09 678  		  EXCEPTION_MESSAGE := SQLERRM;
11:21:09 679  		  RAISE CAN_NOT_DEL_DEF_FINANCIAL;
11:21:09 680  	    END;
11:21:09 681  	  END IF;
11:21:09 682  	END IF;
11:21:09 683  
11:21:09 684  EXCEPTION
11:21:09 685  WHEN NO_DATA_FOUND THEN
11:21:09 686  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 687  	  SPROC_NAME, 'No such credit card');
11:21:09 688  WHEN CAN_NOT_SET_DEF_FINANCIAL THEN
11:21:09 689  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 690  	  SPROC_NAME, 'Could not set default financial instrument for account', EXCEPTION_MESSAGE);
11:21:09 691  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
11:21:09 692  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 693  	  SPROC_NAME, 'Could not get default financial instrument for account', EXCEPTION_MESSAGE);
11:21:09 694  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
11:21:09 695  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 696  	  SPROC_NAME, 'Could not delete information about default financial instrument', EXCEPTION_MESSAGE);
11:21:09 697  WHEN OTHERS THEN
11:21:09 698  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:09 699  	  SPROC_NAME, 'Unknown error', SQLERRM);
11:21:09 700  END UPDATE_CREDIT_CARD;
