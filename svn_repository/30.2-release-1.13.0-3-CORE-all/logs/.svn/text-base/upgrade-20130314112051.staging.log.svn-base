11:21:08 1398  /******************************************************************************/
11:21:08 1399  
11:21:08 1400  PROCEDURE ADD_SUBSCRIPTION_EXTENSION (
11:21:08 1401  /*
11:21:08 1402  APP_EXCEPTION_CODES_V18.NOT_FOUND
11:21:08 1403  APP_EXCEPTION_CODES_V18.INTERNAL_ERROR
11:21:08 1404  APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR
11:21:08 1405  */
11:21:08 1406  	 in_subscription_id	 IN NUMBER,
11:21:08 1407  	 in_effective_start_date IN DATE,
11:21:08 1408  	 in_effective_end_date	 IN DATE,
11:21:08 1409  	 in_note		 IN VARCHAR2,
11:21:08 1410  	 in_updated_by		 IN VARCHAR2
11:21:08 1411  ) AS
11:21:08 1412  -- VARIABLES
11:21:08 1413  SPROC_NAME		    CONSTANT VARCHAR2(26) := 'ADD_SUBSCRIPTION_EXTENSION';
11:21:08 1414  temp_subscription_id	    NUMBER;
11:21:08 1415  var_current_license_id	    NUMBER;
11:21:08 1416  var_current_license_start_date DATE;
11:21:08 1417  var_current_license_end_date DATE;
11:21:08 1418  var_current_offer_id	    NUMBER;
11:21:08 1419  var_current_invoice_id	    NUMBER;
11:21:08 1420  var_current_date 	    DATE;
11:21:08 1421  var_current_offer_index	    NUMBER;
11:21:08 1422  var_current_offer_recurr_num NUMBER;
11:21:08 1423  var_account_tax_exempt_id    VARCHAR2(255);
11:21:08 1424  
11:21:08 1425  var_free_invoice_id NUMBER;
11:21:08 1426  var_free_license_id NUMBER;
11:21:08 1427  var_new_license_id  NUMBER;
11:21:08 1428  var_ext_license_id  NUMBER;
11:21:08 1429  -- EXCEPTIONS
11:21:08 1430  BAD_SUBSCRIPTION_ID	     EXCEPTION;
11:21:08 1431  CAN_NOT_FIND_OFFER_OR_LICENSE EXCEPTION;
11:21:08 1432  CAN_NOT_CHANGE_LICENSE_STATUS EXCEPTION;
11:21:08 1433  CAN_NOT_CREATE_INVOICE	     EXCEPTION;
11:21:08 1434  CAN_NOT_CREATE_NEW_LICENSE    EXCEPTION;
11:21:08 1435  CAN_NOT_CREATE_END_LICENSE    EXCEPTION;
11:21:08 1436  CAN_NOT_ANNOTATE_SUBSCRIPTION EXCEPTION;
11:21:08 1437  EXTENS_START_DATE_IS_TOO_FAR  EXCEPTION;
11:21:08 1438  EXT_START_DATE_LATER_THEN_END EXCEPTION;
11:21:08 1439  EXTENS_START_DATE_IS_TOO_SMALL EXCEPTION;
11:21:08 1440  EXCEPTION_MESSAGE	     VARCHAR2(1024);
11:21:08 1441  BEGIN
11:21:08 1442  
11:21:08 1443  	 var_current_date := PROCS_COMMON_V18.NORMALIZE_DATE(SYSDATE);
11:21:08 1444  
11:21:08 1445  	 -- Check that subscription exists
11:21:08 1446  	 BEGIN
11:21:08 1447  	   SELECT
11:21:08 1448  	     SUBSCRIPTION.ID into temp_subscription_id
11:21:08 1449  	   FROM
11:21:08 1450  	     SUBSCRIPTION
11:21:08 1451  	   WHERE
11:21:08 1452  	     SUBSCRIPTION.ID = in_subscription_id;
11:21:08 1453  	   EXCEPTION
11:21:08 1454  	     WHEN NO_DATA_FOUND THEN
11:21:08 1455  	       RAISE BAD_SUBSCRIPTION_ID;
11:21:08 1456  	 END;
11:21:08 1457  
11:21:08 1458  	 -- Get account tax exempt id
11:21:08 1459  	 SELECT DISTINCT
11:21:08 1460  	   ACCOUNT.TAX_EXEMPT_ID into var_account_tax_exempt_id
11:21:08 1461  	 FROM
11:21:08 1462  	   ACCOUNT
11:21:08 1463  	   INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.ACCOUNT_ID = ACCOUNT.ID
11:21:08 1464  	 WHERE
11:21:08 1465  	   SUBSCRIPTION.ID = in_subscription_id;
11:21:08 1466  
11:21:08 1467  	 -- Select current data
11:21:08 1468  	 BEGIN
11:21:08 1469  	   SELECT
11:21:08 1470  	     LICENSE.ID,
11:21:08 1471  	     LICENSE.START_DATE,
11:21:08 1472  	     LICENSE.END_DATE,
11:21:08 1473  	     LICENSE.CURRENT_OFFER_INDEX,
11:21:08 1474  	     LICENSE.CURRENT_OFFER_RECURR_NUM,
11:21:08 1475  	     OFFER.ID,
11:21:08 1476  	     INVOICE.ID
11:21:08 1477  	   INTO
11:21:08 1478  	     var_current_license_id,
11:21:08 1479  	     var_current_license_start_date,
11:21:08 1480  	     var_current_license_end_date,
11:21:08 1481  	     var_current_offer_index,
11:21:08 1482  	     var_current_offer_recurr_num,
11:21:08 1483  	     var_current_offer_id,
11:21:08 1484  	     var_current_invoice_id
11:21:08 1485  	   FROM
11:21:08 1486  	     LICENSE
11:21:08 1487  	     INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
11:21:08 1488  	     INNER JOIN OFFER ON LICENSE.OFFER_ID = OFFER.ID
11:21:08 1489  	     INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
11:21:08 1490  	   WHERE
11:21:08 1491  	     SUBSCRIPTION.ID = in_subscription_id
11:21:08 1492  	     AND PROCS_COMMON_V18.NORMALIZE_DATE(LICENSE.END_DATE) > var_current_date
11:21:08 1493  	     AND PROCS_COMMON_V18.NORMALIZE_DATE(LICENSE.START_DATE) <= var_current_date
11:21:08 1494  	     AND ROWNUM <= 1
11:21:08 1495  	   ORDER BY
11:21:08 1496  	     LICENSE.ID DESC;
11:21:08 1497  	   EXCEPTION
11:21:08 1498  	     WHEN NO_DATA_FOUND THEN
11:21:08 1499  	       RAISE CAN_NOT_FIND_OFFER_OR_LICENSE;
11:21:08 1500  	 END;
11:21:08 1501  
11:21:08 1502  	 IF var_current_license_end_date < in_effective_start_date THEN
11:21:08 1503  	   RAISE EXTENS_START_DATE_IS_TOO_FAR;
11:21:08 1504  	 END IF;
11:21:08 1505  
11:21:08 1506  	 IF var_current_license_start_date > in_effective_start_date THEN
11:21:08 1507  	   RAISE EXTENS_START_DATE_IS_TOO_SMALL;
11:21:08 1508  	 END IF;
11:21:08 1509  
11:21:08 1510  	 IF in_effective_start_date > in_effective_end_date THEN
11:21:08 1511  	   RAISE EXT_START_DATE_LATER_THEN_END;
11:21:08 1512  	 END IF;
11:21:08 1513  
11:21:08 1514  	 -- Closing curent license
11:21:08 1515  	 BEGIN
11:21:08 1516  	   PROCS_LICENSE_CRU_V18.UPDATE_LICENSE(
11:21:08 1517  	     in_license_id	   => var_current_license_id,
11:21:08 1518  	     in_updated_by	   => in_updated_by,
11:21:08 1519  	     in_license_status_id  => GLOBAL_STATUSES_V18.LICENSE_CLOSED,
11:21:08 1520  	     in_end_date	   => in_effective_start_date,
11:21:08 1521  	     in_needs_entitlements => GLOBAL_CONSTANTS_V18.TRUE
11:21:08 1522  	   );
11:21:08 1523  	   EXCEPTION
11:21:08 1524  	     WHEN OTHERS THEN
11:21:08 1525  	       EXCEPTION_MESSAGE := SQLERRM;
11:21:08 1526  	       RAISE CAN_NOT_CHANGE_LICENSE_STATUS;
11:21:08 1527  	 END;
11:21:08 1528  
11:21:08 1529  	 -- Creating new "free" invoice
11:21:08 1530  	 BEGIN
11:21:08 1531  	   PROCS_INVOICE_V18.CREATE_INVOICE(
11:21:08 1532  	     in_invoice_status => GLOBAL_STATUSES_V18.INVOICE_CLOSED,
11:21:08 1533  	     in_created_by     => in_updated_by,
11:21:08 1534  	     in_tax_exempt_id  => var_account_tax_exempt_id,
11:21:08 1535  	     out_invoice_id    => var_free_invoice_id
11:21:08 1536  	   );
11:21:08 1537  	   EXCEPTION
11:21:08 1538  	     WHEN OTHERS THEN
11:21:08 1539  	       EXCEPTION_MESSAGE := SQLERRM;
11:21:08 1540  	       RAISE CAN_NOT_CREATE_INVOICE;
11:21:08 1541  	 END;
11:21:08 1542  
11:21:08 1543  	 -- Creating new "free" license
11:21:08 1544  	 BEGIN
11:21:08 1545  	   PROCS_LICENSE_V18.CREATE_LICENSE (
11:21:08 1546  	     in_status_id		 => GLOBAL_STATUSES_V18.LICENSE_ACTIVE,
11:21:08 1547  	     in_needs_entitlements	 => GLOBAL_CONSTANTS_V18.TRUE,
11:21:08 1548  	     in_start_date		 => in_effective_start_date,
11:21:08 1549  	     in_end_date		 => in_effective_end_date,
11:21:08 1550  	     in_offer_id		 => var_current_offer_id,
11:21:08 1551  	     in_subscription_id 	 => in_subscription_id,
11:21:08 1552  	     in_invoice_id		 => var_free_invoice_id,
11:21:08 1553  	     in_created_by		 => in_updated_by,
11:21:08 1554  	     in_is_extension		 => GLOBAL_CONSTANTS_V18.TRUE,
11:21:08 1555  	     in_current_offer_index	 => var_current_offer_index,
11:21:08 1556  	     in_current_offer_recurr_num => var_current_offer_recurr_num,
11:21:08 1557  	     out_license_id		 => var_free_license_id
11:21:08 1558  	   );
11:21:08 1559  	   EXCEPTION
11:21:08 1560  	     WHEN OTHERS THEN
11:21:08 1561  	       EXCEPTION_MESSAGE := SQLERRM;
11:21:08 1562  	       RAISE CAN_NOT_CREATE_NEW_LICENSE;
11:21:08 1563  	 END;
11:21:08 1564  
11:21:08 1565  	 -- Creating new license
11:21:08 1566  	 IF PROCS_COMMON_V18.NORMALIZE_DATE(var_current_license_end_date) >
11:21:08 1567  	    PROCS_COMMON_V18.NORMALIZE_DATE(in_effective_start_date) THEN
11:21:08 1568  	   BEGIN
11:21:08 1569  	     PROCS_LICENSE_V18.CREATE_LICENSE (
11:21:08 1570  	       in_status_id		   => GLOBAL_STATUSES_V18.LICENSE_ACTIVE,
11:21:08 1571  	       in_needs_entitlements	   => GLOBAL_CONSTANTS_V18.TRUE,
11:21:08 1572  	       in_start_date		   => in_effective_end_date,
11:21:08 1573  	       in_end_date		   => var_current_license_end_date + (in_effective_end_date - in_effective_start_date),
11:21:08 1574  	       in_offer_id		   => var_current_offer_id,
11:21:08 1575  	       in_subscription_id	   => in_subscription_id,
11:21:08 1576  	       in_invoice_id		   => var_current_invoice_id,
11:21:08 1577  	       in_created_by		   => in_updated_by,
11:21:08 1578  	       in_is_extension		   => GLOBAL_CONSTANTS_V18.FALSE,
11:21:08 1579  	       in_current_offer_index	   => var_current_offer_index,
11:21:08 1580  	       in_current_offer_recurr_num => var_current_offer_recurr_num,
11:21:08 1581  	       out_license_id		   => var_ext_license_id
11:21:08 1582  	     );
11:21:08 1583  	     EXCEPTION
11:21:08 1584  	       WHEN OTHERS THEN
11:21:08 1585  		 EXCEPTION_MESSAGE := SQLERRM;
11:21:08 1586  		 RAISE CAN_NOT_CREATE_END_LICENSE;
11:21:08 1587  	   END;
11:21:08 1588  	 END IF;
11:21:08 1589  
11:21:08 1590  	 -- Create new note for subscription
11:21:08 1591  	 BEGIN
11:21:08 1592  	   PROCS_SUBSCRIPTION_V18.ANNOTATE_SUBSCRIPTION (
11:21:08 1593  	     in_subscription_id => in_subscription_id,
11:21:08 1594  	     in_agent_id	=> 0, -- FIXME: What should to be here (agent id)?
11:21:08 1595  	     in_note		=> in_note,
11:21:08 1596  	     in_created_by	=> in_updated_by
11:21:08 1597  	   );
11:21:08 1598  	   EXCEPTION
11:21:08 1599  	     WHEN OTHERS THEN
11:21:08 1600  	       EXCEPTION_MESSAGE := SQLERRM;
11:21:08 1601  	       RAISE CAN_NOT_ANNOTATE_SUBSCRIPTION;
11:21:08 1602  	 END;
11:21:08 1603  
11:21:08 1604  EXCEPTION
11:21:08 1605  WHEN EXT_START_DATE_LATER_THEN_END THEN
11:21:08 1606  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INVALID_PARAMETER,
11:21:08 1607  	   SPROC_NAME, 'Extension start date is bigger then end date');
11:21:08 1608  WHEN EXTENS_START_DATE_IS_TOO_FAR THEN
11:21:08 1609  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INVALID_PARAMETER,
11:21:08 1610  	   SPROC_NAME, 'Extension start date is too far');
11:21:08 1611  WHEN EXTENS_START_DATE_IS_TOO_SMALL THEN
11:21:08 1612  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INVALID_PARAMETER,
11:21:08 1613  	   SPROC_NAME, 'Extension start date is too small');
11:21:08 1614  WHEN BAD_SUBSCRIPTION_ID THEN
11:21:08 1615  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:08 1616  	   SPROC_NAME, 'No such subscription');
11:21:08 1617  WHEN CAN_NOT_FIND_OFFER_OR_LICENSE THEN
11:21:08 1618  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:08 1619  	   SPROC_NAME, 'Could not find license and/or offer for given subscription');
11:21:08 1620  WHEN CAN_NOT_CHANGE_LICENSE_STATUS THEN
11:21:08 1621  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:08 1622  	   SPROC_NAME, 'Could not change license status', EXCEPTION_MESSAGE);
11:21:08 1623  WHEN CAN_NOT_CREATE_INVOICE THEN
11:21:08 1624  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:08 1625  	   SPROC_NAME, 'Could not create new invoice', EXCEPTION_MESSAGE);
11:21:08 1626  WHEN CAN_NOT_CREATE_NEW_LICENSE THEN
11:21:08 1627  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:08 1628  	   SPROC_NAME, 'Could not create new license', EXCEPTION_MESSAGE);
11:21:08 1629  WHEN CAN_NOT_CREATE_END_LICENSE THEN
11:21:08 1630  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:08 1631  	   SPROC_NAME, 'Could not create last license', EXCEPTION_MESSAGE);
11:21:08 1632  WHEN CAN_NOT_ANNOTATE_SUBSCRIPTION THEN
11:21:08 1633  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:08 1634  	   SPROC_NAME, 'Could not create new note for subscription', EXCEPTION_MESSAGE);
11:21:08 1635  WHEN OTHERS THEN
11:21:08 1636  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:08 1637  	   SPROC_NAME, 'Unknown error', SQLERRM);
11:21:08 1638  END ADD_SUBSCRIPTION_EXTENSION;
11:21:08 1639  
11:21:08 1640  /******************************************************************************/
11:21:08 1641  
11:21:08 1642  FUNCTION CALC_SUBSCRIPTION_END_DATE (
11:21:08 1643  /*
11:21:08 1644  APP_EXCEPTION_CODES_V18.NOT_FOUND
11:21:08 1645  APP_EXCEPTION_CODES_V18.INTERNAL_ERROR
11:21:08 1646  APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR
11:21:08 1647  Returns:
11:21:08 1648  NULL if it is impossible to calculate end date (for example,
11:21:08 1649  	 offer chain includes offer with infinity recurrences number)
11:21:08 1650  DATE else
11:21:08 1651  */
11:21:08 1652  	 in_subscription_id IN NUMBER
11:21:08 1653  ) RETURN DATE AS
11:21:08 1654  -- VARIABLES
11:21:08 1655  SPROC_NAME		     CONSTANT VARCHAR2(26) := 'CALC_SUBSCRIPTION_END_DATE';
11:21:08 1656  last_license_id		     NUMBER;
11:21:08 1657  last_license_end_date	     DATE;
11:21:08 1658  last_license_offer_id	     NUMBER;
11:21:08 1659  last_license_offer_index      NUMBER;
11:21:08 1660  last_license_offer_recurr_num NUMBER;
11:21:08 1661  var_last_license_id	     NUMBER;
11:21:08 1662  var_offer_chain_id	     NUMBER;
11:21:08 1663  var_current_offer_rec_number  NUMBER;
11:21:08 1664  var_next_offers_set	     SYS_REFCURSOR;
11:21:08 1665  var_next_offer_duration	     VARCHAR2(30);
11:21:08 1666  var_next_offer_recur	     NUMBER;
11:21:08 1667  var_infinity_offers_number    NUMBER;
11:21:08 1668  
11:21:08 1669  var_result_date DATE;
11:21:08 1670  
11:21:08 1671  var_ym_interval	INTERVAL YEAR TO MONTH;
11:21:08 1672  var_ds_interval	INTERVAL DAY(3) TO SECOND;
11:21:08 1673  var_offer_years	NUMBER;
11:21:08 1674  var_offer_months NUMBER;
11:21:08 1675  var_offer_days	NUMBER;
11:21:08 1676  
11:21:08 1677  -- EXCEPTIONS
11:21:08 1678  BAD_SUBSCRIPTION_ID	  EXCEPTION;
11:21:08 1679  CAN_NOT_FIND_LAST_LICENSE  EXCEPTION;
11:21:08 1680  CAN_NOT_CALC_OFFER_LENGTH  EXCEPTION;
11:21:08 1681  CAN_NOT_CALC_OFFER_LENGTH2 EXCEPTION;
11:21:08 1682  EXCEPTION_MESSAGE	  VARCHAR2(1024);
11:21:08 1683  BEGIN
11:21:08 1684  
11:21:08 1685  	 BEGIN
11:21:08 1686  	   SELECT
11:21:08 1687  	     SUBSCRIPTION.OFFER_CHAIN_ID into var_offer_chain_id
11:21:08 1688  	   FROM
11:21:08 1689  	     SUBSCRIPTION
11:21:08 1690  	   WHERE
11:21:08 1691  	     SUBSCRIPTION.ID = in_subscription_id;
11:21:08 1692  	   EXCEPTION
11:21:08 1693  	     WHEN NO_DATA_FOUND THEN
11:21:08 1694  	       RAISE BAD_SUBSCRIPTION_ID;
11:21:08 1695  	 END;
11:21:08 1696  
11:21:08 1697  	 BEGIN
11:21:08 1698  	   SELECT
11:21:08 1699  	     ID into var_last_license_id
11:21:08 1700  	   FROM
11:21:08 1701  	     (
11:21:08 1702  	       SELECT
11:21:08 1703  		 LICENSE.ID
11:21:08 1704  	       FROM
11:21:08 1705  		 LICENSE
11:21:08 1706  	       WHERE
11:21:08 1707  		 LICENSE.SUBSCRIPTION_ID = in_subscription_id
11:21:08 1708  	       ORDER BY
11:21:08 1709  		 LICENSE.END_DATE DESC
11:21:08 1710  	     )
11:21:08 1711  	   WHERE
11:21:08 1712  	     ROWNUM <= 1;
11:21:08 1713  	   EXCEPTION
11:21:08 1714  	     WHEN NO_DATA_FOUND THEN
11:21:08 1715  	       RAISE CAN_NOT_FIND_LAST_LICENSE;
11:21:08 1716  	 END;
11:21:08 1717  
11:21:08 1718  	 SELECT
11:21:08 1719  	   COUNT(*) into var_infinity_offers_number
11:21:08 1720  	 FROM
11:21:08 1721  	   OFFER_OFFER_CHAIN
11:21:08 1722  	 WHERE
11:21:08 1723  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
11:21:08 1724  	   AND OFFER_OFFER_CHAIN.NUM_RECURRENCES = GLOBAL_ENUMS_V18.OFFER_REC_INFINITY;
11:21:08 1725  
11:21:08 1726  	 IF var_infinity_offers_number > 0 THEN
11:21:08 1727  	   RETURN NULL;
11:21:08 1728  	 END IF;
11:21:08 1729  
11:21:08 1730  	 BEGIN
11:21:08 1731  	   SELECT
11:21:08 1732  	     LICENSE.ID,
11:21:08 1733  	     LICENSE.END_DATE,
11:21:08 1734  	     LICENSE.CURRENT_OFFER_INDEX,
11:21:08 1735  	     LICENSE.CURRENT_OFFER_RECURR_NUM,
11:21:08 1736  	     LICENSE.OFFER_ID
11:21:08 1737  	     into
11:21:08 1738  	     last_license_id,
11:21:08 1739  	     last_license_end_date,
11:21:08 1740  	     last_license_offer_index,
11:21:08 1741  	     last_license_offer_recurr_num,
11:21:08 1742  	     last_license_offer_id
11:21:08 1743  	   FROM
11:21:08 1744  	     LICENSE
11:21:08 1745  	   WHERE
11:21:08 1746  	     LICENSE.ID = var_last_license_id;
11:21:08 1747  	   EXCEPTION
11:21:08 1748  	     WHEN NO_DATA_FOUND THEN
11:21:08 1749  	       RAISE CAN_NOT_FIND_LAST_LICENSE;
11:21:08 1750  	 END;
11:21:08 1751  
11:21:08 1752  	 var_result_date := last_license_end_date;
11:21:08 1753  
11:21:08 1754  	 -- Find current recurrence number
11:21:08 1755  	 SELECT
11:21:08 1756  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES into var_current_offer_rec_number
11:21:08 1757  	 FROM
11:21:08 1758  	   OFFER_OFFER_CHAIN
11:21:08 1759  	 WHERE
11:21:08 1760  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
11:21:08 1761  	   AND OFFER_OFFER_CHAIN.OFFER_ID = last_license_offer_id
11:21:08 1762  	   AND OFFER_OFFER_CHAIN.INDEX_VALUE = last_license_offer_index;
11:21:08 1763  
11:21:08 1764  	 IF var_current_offer_rec_number > last_license_offer_recurr_num THEN
11:21:08 1765  	   BEGIN
11:21:08 1766  	     PROCS_OFFER_CHAIN_V18.GET_OFFER_LENGTH(
11:21:08 1767  	       last_license_offer_id,
11:21:08 1768  	       var_offer_years,
11:21:08 1769  	       var_offer_months,
11:21:08 1770  	       var_offer_days
11:21:08 1771  	     );
11:21:08 1772  
11:21:08 1773  	     var_ym_interval := var_offer_years||'-'||var_offer_months;
11:21:08 1774  	     var_ds_interval := var_offer_days||' 0:0:0';
11:21:08 1775  
11:21:08 1776  	     var_result_date := var_result_date
11:21:08 1777  	       + ( var_ym_interval * ( var_current_offer_rec_number - last_license_offer_recurr_num ) )
11:21:08 1778  	       + ( var_ds_interval * ( var_current_offer_rec_number - last_license_offer_recurr_num ) );
11:21:08 1779  	     EXCEPTION
11:21:08 1780  	       WHEN OTHERS THEN
11:21:08 1781  		 EXCEPTION_MESSAGE := SQLERRM;
11:21:08 1782  		 RAISE CAN_NOT_CALC_OFFER_LENGTH;
11:21:08 1783  	   END;
11:21:08 1784  	 END IF;
11:21:08 1785  
11:21:08 1786  	 OPEN var_next_offers_set FOR
11:21:08 1787  	 SELECT
11:21:08 1788  	   OFFER.ENTITLEMENT_DURATION,
11:21:08 1789  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES
11:21:08 1790  	 FROM
11:21:08 1791  	   OFFER_OFFER_CHAIN
11:21:08 1792  	   INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
11:21:08 1793  	 WHERE
11:21:08 1794  	   OFFER_OFFER_CHAIN.INDEX_VALUE > last_license_offer_index
11:21:08 1795  	   AND OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id;
11:21:08 1796  
11:21:08 1797  	 LOOP
11:21:08 1798  	   FETCH var_next_offers_set into var_next_offer_duration, var_next_offer_recur;
11:21:08 1799  	   EXIT WHEN var_next_offers_set%NOTFOUND;
11:21:08 1800  	   BEGIN
11:21:08 1801  	     PROCS_COMMON_V18.ISO8601DURATION_TO_INTERVALS(
11:21:08 1802  	       var_next_offer_duration,
11:21:08 1803  	       var_offer_years,
11:21:08 1804  	       var_offer_months,
11:21:08 1805  	       var_offer_days
11:21:08 1806  	     );
11:21:08 1807  
11:21:08 1808  	     var_ym_interval := var_offer_years||'-'||var_offer_months;
11:21:08 1809  	     var_ds_interval := var_offer_days||' 0:0:0';
11:21:08 1810  
11:21:08 1811  	     var_result_date := var_result_date
11:21:08 1812  	       + ( var_ym_interval * var_next_offer_recur )
11:21:08 1813  	       + ( var_ds_interval * var_next_offer_recur );
11:21:08 1814  	     EXCEPTION
11:21:08 1815  	       WHEN OTHERS THEN
11:21:08 1816  		 EXCEPTION_MESSAGE := SQLERRM;
11:21:08 1817  		 RAISE CAN_NOT_CALC_OFFER_LENGTH2;
11:21:08 1818  	   END;
11:21:08 1819  	 END LOOP;
11:21:08 1820  
11:21:08 1821  	 RETURN var_result_date;
11:21:08 1822  
11:21:08 1823  EXCEPTION
11:21:08 1824  WHEN BAD_SUBSCRIPTION_ID THEN
11:21:08 1825  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:08 1826  	   SPROC_NAME, 'No such subscription');
11:21:08 1827  WHEN CAN_NOT_FIND_LAST_LICENSE THEN
11:21:08 1828  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:08 1829  	   SPROC_NAME, 'Could not find last license for given subscription');
11:21:08 1830  WHEN CAN_NOT_CALC_OFFER_LENGTH THEN
11:21:08 1831  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:08 1832  	   SPROC_NAME, 'Could not calculate offer length', EXCEPTION_MESSAGE);
11:21:08 1833  WHEN CAN_NOT_CALC_OFFER_LENGTH2 THEN
11:21:08 1834  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:08 1835  	   SPROC_NAME, 'Could not calculate last offer length', EXCEPTION_MESSAGE);
11:21:08 1836  WHEN OTHERS THEN
11:21:08 1837  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:08 1838  	   SPROC_NAME, 'Unkown error', SQLERRM);
11:21:08 1839  END CALC_SUBSCRIPTION_END_DATE;
