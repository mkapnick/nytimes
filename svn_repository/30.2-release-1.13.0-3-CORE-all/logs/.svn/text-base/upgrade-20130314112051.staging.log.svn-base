11:21:07 1682  	       INNER JOIN PRODUCT_OFFERING ON OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = PRODUCT_OFFERING.ID
11:21:07 1683  	     GROUP BY
11:21:07 1684  	       PRODUCT_OFFERING.PRODUCT_ID
11:21:07 1685  	   )
11:21:07 1686  	   INNER JOIN
11:21:07 1687  	   (
11:21:07 1688  	     SELECT
11:21:07 1689  	       PRODUCT_OFFERING.PRODUCT_ID as "PRODUCT_ID_IN_OFFER_CH_2",
11:21:07 1690  	       COUNT(*) 		   as "COUNT_2"
11:21:07 1691  	     FROM
11:21:07 1692  	       (
