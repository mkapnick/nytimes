11:21:07   9  	in_invoice_id IN NUMBER,
11:21:07  10  	in_offer_id   IN NUMBER,
11:21:07  11  	in_created_by IN VARCHAR2
11:21:07  12  ) AS
11:21:07  13  -- VARIABLES
11:21:07  14  SPROC_NAME      CONSTANT VARCHAR2(14) := 'ADD_LINE_ITEMS';
11:21:07  15  temp_invoice_id NUMBER;
11:21:07  16  temp_offer_id   NUMBER;
11:21:07  17  
11:21:07  18  var_line_item_data SYS_REFCURSOR;
11:21:07  19  var_new_line_item_id NUMBER;
11:21:07  20  var_product_unit_price NUMBER (10,6);
11:21:07  21  var_product_offering_price NUMBER(10,6);
11:21:07  22  var_product_offering_oprice NUMBER(10,6);
11:21:07  23  var_product_quantity NUMBER;
11:21:07  24  var_product_offering_id NUMBER;
11:21:07  25  
11:21:07  26  var_line_item_price	  NUMBER(10,2);
11:21:07  27  var_discount_fixed_amount   NUMBER(10,6);
11:21:07  28  var_discount_percent_amount NUMBER(10,2);
11:21:07  29  
11:21:07  30  
11:21:07  31  -- EXCEPTIONS
11:21:07  32  BAD_INVOICE_ID EXCEPTION;
11:21:07  33  BAD_OFFER_ID EXCEPTION;
11:21:07  34  BAD_DISCOUNT EXCEPTION;
11:21:07  35  BEGIN
11:21:07  36  
11:21:07  37  	-- Check that given invoice exists
11:21:07  38  	BEGIN
11:21:07  39  	  SELECT
11:21:07  40  	    INVOICE.ID into temp_invoice_id
11:21:07  41  	  FROM
11:21:07  42  	    INVOICE
11:21:07  43  	  WHERE
11:21:07  44  	    INVOICE.ID = in_invoice_id;
11:21:07  45  
11:21:07  46  	  EXCEPTION
11:21:07  47  	    WHEN NO_DATA_FOUND THEN
11:21:07  48  	      RAISE BAD_INVOICE_ID;
11:21:07  49  	END;
11:21:07  50  
11:21:07  51  	-- Check that given offer exists
11:21:07  52  	BEGIN
11:21:07  53  	  SELECT
11:21:07  54  	    OFFER.ID into temp_offer_id
11:21:07  55  	  FROM
11:21:07  56  	    OFFER
11:21:07  57  	  WHERE
11:21:07  58  	    OFFER.ID = in_offer_id;
11:21:07  59  
11:21:07  60  	  EXCEPTION
11:21:07  61  	    WHEN NO_DATA_FOUND THEN
11:21:07  62  	      RAISE BAD_OFFER_ID;
11:21:07  63  	END;
11:21:07  64  
11:21:07  65  	-- Get product_offering data
11:21:07  66  	OPEN var_line_item_data FOR
11:21:07  67  	SELECT
11:21:07  68  	  PRODUCT_OFFERING.ID,
11:21:07  69  	  PRODUCT_OFFERING.UNIT_PRICE,
11:21:07  70  	  PRODUCT_OFFERING.QUANTITY
11:21:07  71  	FROM
11:21:07  72  	  OFFER_PRODUCT_OFFERING
11:21:07  73  	  INNER JOIN PRODUCT_OFFERING ON OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = PRODUCT_OFFERING.ID
11:21:07  74  	WHERE
11:21:07  75  	  OFFER_PRODUCT_OFFERING.OFFER_ID = in_offer_id;
11:21:07  76  
11:21:07  77  	-- insert line items
11:21:07  78  	-- add discounts to line items
11:21:07  79  	LOOP
11:21:07  80  	  FETCH var_line_item_data INTO
11:21:07  81  	    var_product_offering_id,
11:21:07  82  	    var_product_unit_price,
11:21:07  83  	    var_product_quantity;
11:21:07  84  	  EXIT WHEN var_line_item_data%NOTFOUND;
11:21:07  85  
11:21:07  86  	  var_product_offering_oprice := var_product_unit_price * var_product_quantity;
11:21:07  87  	  var_product_offering_price := var_product_offering_oprice;
11:21:07  88  
11:21:07  89  	  -- Apply discounts to line_item
11:21:07  90  	  BEGIN
11:21:07  91  	    SELECT
11:21:07  92  	      SUM (DISCOUNT.FIXED_AMOUNT) into var_discount_fixed_amount
11:21:07  93  	    FROM
11:21:07  94  	      DISCOUNT_PRODUCT_OFFERING
11:21:07  95  	      INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
11:21:07  96  	    WHERE
11:21:07  97  	      DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
11:21:07  98  	      AND DISCOUNT.FIXED_AMOUNT IS NOT NULL;
11:21:07  99  	    EXCEPTION
11:21:07 100  	    WHEN NO_DATA_FOUND THEN
11:21:07 101  	      var_discount_fixed_amount := NULL;
11:21:07 102  	  END;
11:21:07 103  
11:21:07 104  	  BEGIN
11:21:07 105  	    SELECT
11:21:07 106  	      SUM (DISCOUNT.PERCENT_AMOUNT) into var_discount_percent_amount
11:21:07 107  	    FROM
11:21:07 108  	      DISCOUNT_PRODUCT_OFFERING
11:21:07 109  	      INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
11:21:07 110  	    WHERE
11:21:07 111  	      DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
11:21:07 112  	      AND DISCOUNT.PERCENT_AMOUNT IS NOT NULL;
11:21:07 113  	    EXCEPTION
11:21:07 114  	    WHEN NO_DATA_FOUND THEN
11:21:07 115  	      var_discount_percent_amount := NULL;
11:21:07 116  	  END;
11:21:07 117  
11:21:07 118  	  IF (var_discount_percent_amount IS NOT NULL) THEN
11:21:07 119  	    var_product_offering_price := var_product_offering_price * ( 1 - var_discount_percent_amount / 100 );
11:21:07 120  	  END IF;
11:21:07 121  
11:21:07 122  	  IF (var_discount_fixed_amount IS NOT NULL) THEN
11:21:07 123  	    var_product_offering_price := var_product_offering_price - var_discount_fixed_amount;
11:21:07 124  	  END IF;
11:21:07 125  
11:21:07 126  	  var_line_item_price := PROCS_COMMON_V18.ROUND_10_6_TO_10_2(var_product_offering_price);
11:21:07 127  
11:21:07 128  	  IF (var_line_item_price < 0) THEN
11:21:07 129  	      RAISE BAD_DISCOUNT;
11:21:07 130  	  END IF;
11:21:07 131  
11:21:07 132  	  var_new_line_item_id := NULL;
11:21:07 133  	  PROCS_LINE_ITEMS_CRU_V18.CREATE_LINE_ITEM(
11:21:07 134  	    inout_line_item_id	=> var_new_line_item_id,
11:21:07 135  	    in_product_offer_id => var_product_offering_id,
11:21:07 136  	    in_invoice_id	=> in_invoice_id,
11:21:07 137  	    in_amount		=> var_line_item_price,
11:21:07 138  	    in_created_by	=> in_created_by,
11:21:07 139  	    in_discount_amount	=> var_product_offering_oprice - var_line_item_price,
11:21:07 140  	    in_taxes_amount	=> NULL
11:21:07 141  	  );
11:21:07 142  
11:21:07 143  	  FOR f_discount IN (
11:21:07 144  	    SELECT
11:21:07 145  	      DISCOUNT.ID
