11:21:09 1983  	     ACCOUNT.ID into var_account_id
11:21:09 1984  	   FROM
11:21:09 1985  	     ACCOUNT
11:21:09 1986  	   WHERE
11:21:09 1987  	     ACCOUNT.GROUP_ID = in_group_id;
