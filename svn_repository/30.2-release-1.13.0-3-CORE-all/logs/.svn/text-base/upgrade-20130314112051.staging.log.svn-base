11:21:09 1066  /******************************************************************************/
11:21:09 1067  
11:21:09 1068  PROCEDURE GET_MAX_REFUND (
11:21:09 1069  	 in_invoice_id IN NUMBER,
11:21:09 1070  	 out_amount    OUT NUMBER
11:21:09 1071  ) AS
11:21:09 1072  SPROC_NAME CONSTANT VARCHAR2(14) := 'GET_MAX_REFUND';
11:21:09 1073  -- VARIABLES
11:21:09 1074  temp_invoice_id	       NUMBER;
11:21:09 1075  var_invoice_refunds_sum NUMBER(10,2);
11:21:09 1076  var_invoice_charges_sum NUMBER(10,2);
11:21:09 1077  -- EXCEPTIONS
11:21:09 1078  BAD_INVOICE_ID EXCEPTION;
11:21:09 1079  BEGIN
11:21:09 1080  
11:21:09 1081  	 BEGIN
11:21:09 1082  	   SELECT
11:21:09 1083  	     INVOICE.ID into temp_invoice_id
11:21:09 1084  	   FROM
11:21:09 1085  	     INVOICE
11:21:09 1086  	   WHERE
11:21:09 1087  	     INVOICE.ID = in_invoice_id;
11:21:09 1088  	   EXCEPTION
11:21:09 1089  	     WHEN NO_DATA_FOUND THEN
11:21:09 1090  	       RAISE BAD_INVOICE_ID;
11:21:09 1091  	 END;
11:21:09 1092  
11:21:09 1093  	 var_invoice_refunds_sum := 0;
11:21:09 1094  	 var_invoice_charges_sum := 0;
11:21:09 1095  
11:21:09 1096  	 FOR f_charge IN (
11:21:09 1097  	   SELECT
11:21:09 1098  	     CHARGE.ID,
11:21:09 1099  	     CHARGE.CHARGE_STATUS_ID,
11:21:09 1100  	     CHARGE.CHARGE_AMOUNT,
11:21:09 1101  	     CHARGE.TRANSACTION_ID
11:21:09 1102  	   FROM
11:21:09 1103  	     CHARGE
11:21:09 1104  	   WHERE
11:21:09 1105  	     CHARGE.INVOICE_ID = in_invoice_id
11:21:09 1106  	 )
11:21:09 1107  	 LOOP
11:21:09 1108  	   -- If charge.status = canceled then continue
11:21:09 1109  	   IF f_charge.CHARGE_STATUS_ID != GLOBAL_STATUSES_V18.CHARGE_CANCELED THEN
11:21:09 1110  
11:21:09 1111  	     IF f_charge.CHARGE_AMOUNT > 0 THEN
11:21:09 1112  	       IF f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V18.CHARGE_PROCESSED
11:21:09 1113  		  AND PROCS_TRANSACTION_V18.IS_TRANSACTION_COLLECTED(f_charge.TRANSACTION_ID) = GLOBAL_CONSTANTS_V18.TRUE THEN
11:21:09 1114  		 -- Transaction collected
11:21:09 1115  		 var_invoice_charges_sum := var_invoice_charges_sum + f_charge.CHARGE_AMOUNT;
11:21:09 1116  	       ELSE
11:21:09 1117  		 -- Transaction is not collected. Do nothing
11:21:09 1118  		 NULL;
11:21:09 1119  	       END IF;
11:21:09 1120  	     ELSE
11:21:09 1121  	       IF f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V18.CHARGE_PROCESSED
11:21:09 1122  		  AND PROCS_TRANSACTION_V18.IS_TRANSACTION_COLLECTED(f_charge.TRANSACTION_ID) = GLOBAL_CONSTANTS_V18.FALSE THEN
11:21:09 1123  		 -- If charge is processed transaction is not collected then do nothing
11:21:09 1124  		 NULL;
11:21:09 1125  	       ELSE
11:21:09 1126  		 var_invoice_refunds_sum := var_invoice_refunds_sum + f_charge.CHARGE_AMOUNT;
11:21:09 1127  	       END IF;
11:21:09 1128  	     END IF;
11:21:09 1129  
11:21:09 1130  	   END IF;
11:21:09 1131  	 END LOOP;
11:21:09 1132  
11:21:09 1133  	 -- Refunds are negative
11:21:09 1134  	 var_invoice_refunds_sum := 0 - var_invoice_refunds_sum;
11:21:09 1135  
11:21:09 1136  	 out_amount := var_invoice_charges_sum - var_invoice_refunds_sum;
11:21:09 1137  
11:21:09 1138  EXCEPTION
11:21:09 1139  WHEN BAD_INVOICE_ID THEN
11:21:09 1140  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 1141  	   SPROC_NAME, 'No such invoice');
11:21:09 1142  WHEN OTHERS THEN
11:21:09 1143  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:09 1144  	   SPROC_NAME, 'Unknown error', SQLERRM);
11:21:09 1145  END GET_MAX_REFUND;
11:21:09 1146  
11:21:09 1147  /******************************************************************************/
11:21:09 1148  
11:21:09 1149  PROCEDURE GET_INVOICE_DAYS_USED_NUMBER (
11:21:09 1150  	 in_invoice_id	     IN NUMBER,
11:21:09 1151  	 in_chargeback_date  IN DATE DEFAULT SYSDATE,
11:21:09 1152  	 out_days_num	     OUT NUMBER
11:21:09 1153  ) AS
11:21:09 1154  SPROC_NAME CONSTANT VARCHAR2(28) := 'GET_INVOICE_DAYS_USED_NUMBER';
11:21:09 1155  -- VARIABLES
11:21:09 1156  temp_invoice_id	      NUMBER;
11:21:09 1157  var_license_start_date DATE;
11:21:09 1158  var_license_end_date   DATE;
11:21:09 1159  var_chargeback_date    DATE;
11:21:09 1160  var_invoice_days_used  NUMBER;
11:21:09 1161  -- EXCEPTIONS
11:21:09 1162  BAD_INVOICE_ID EXCEPTION;
11:21:09 1163  BEGIN
11:21:09 1164  
11:21:09 1165  	 var_chargeback_date := NVL(in_chargeback_date, SYSDATE);
11:21:09 1166  
11:21:09 1167  	 BEGIN
11:21:09 1168  	   SELECT
11:21:09 1169  	     INVOICE.ID into temp_invoice_id
11:21:09 1170  	   FROM
11:21:09 1171  	     INVOICE
11:21:09 1172  	   WHERE
11:21:09 1173  	     INVOICE.ID = in_invoice_id;
11:21:09 1174  	   EXCEPTION
11:21:09 1175  	     WHEN NO_DATA_FOUND THEN
11:21:09 1176  	       RAISE BAD_INVOICE_ID;
11:21:09 1177  	 END;
11:21:09 1178  
11:21:09 1179  	 var_invoice_days_used := 0;
11:21:09 1180  
11:21:09 1181  	 FOR f_license IN (
11:21:09 1182  	   SELECT
11:21:09 1183  	     LICENSE.START_DATE,
11:21:09 1184  	     LICENSE.END_DATE
11:21:09 1185  	   FROM
11:21:09 1186  	     LICENSE
11:21:09 1187  	   WHERE
11:21:09 1188  	     LICENSE.INVOICE_ID = in_invoice_id
11:21:09 1189  	     AND LICENSE.IS_EXTENSION = GLOBAL_CONSTANTS_V18.FALSE
11:21:09 1190  	 )
11:21:09 1191  	 LOOP
11:21:09 1192  	   var_license_start_date := PROCS_COMMON_V18.NORMALIZE_DATE(f_license.START_DATE);
11:21:09 1193  	   var_license_end_date := PROCS_COMMON_V18.NORMALIZE_DATE(f_license.END_DATE);
11:21:09 1194  
11:21:09 1195  	   IF var_license_start_date <= var_chargeback_date THEN
11:21:09 1196  	     IF var_license_end_date <= var_chargeback_date THEN
11:21:09 1197  	       -- License is passed
11:21:09 1198  	       var_invoice_days_used := var_invoice_days_used + (var_license_end_date - var_license_start_date);
11:21:09 1199  	     ELSE
11:21:09 1200  	       -- This is current license
11:21:09 1201  	       var_invoice_days_used := var_invoice_days_used + (var_chargeback_date - var_license_start_date);
11:21:09 1202  	     END IF;
11:21:09 1203  	   ELSE
11:21:09 1204  	     -- if var_license_start_date > in_chargeback_date then do nothing
11:21:09 1205  	     NULL;
11:21:09 1206  	   END IF;
11:21:09 1207  	 END LOOP;
11:21:09 1208  
11:21:09 1209  	 out_days_num := var_invoice_days_used;
11:21:09 1210  
11:21:09 1211  EXCEPTION
