11:21:08 507  	  SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
11:21:08 508  WHEN CAN_NOT_CREATE_CHARGE THEN
11:21:08 509  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:08 510  	  SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
11:21:08 511  WHEN OTHERS THEN
11:21:08 512  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:08 513  	  SPROC_NAME, 'Unknown error', SQLERRM);
11:21:08 514  END FINALIZE_SUBSCRIPTION_CREATION;
