11:21:09  90  	      ta.transaction_attempt_status_id = GLOBAL_STATUSES_V18.TRANS_ATTEMPT_SUCCESS
11:21:09  91  	  ) AND NOT EXISTS (
11:21:09  92  	    SELECT NULL
11:21:09  93  	    FROM
11:21:09  94  	      charge c,
11:21:09  95  	      transaction t
11:21:09  96  	    WHERE
11:21:09  97  	      c.invoice_id = gc.purchase_invoice_id and
11:21:09  98  	      c.transaction_id = t.id and
11:21:09  99  	      t.is_refund = GLOBAL_CONSTANTS_V18.TRUE
11:21:09 100  	  )
11:21:09 101  	  AND ROWNUM <= in_num_rows*10
11:21:09 102  	  ORDER BY dbms_random.value
11:21:09 103  ) WHERE
11:21:09 104  	  ROWNUM <= in_num_rows;
