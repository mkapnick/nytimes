11:21:08 696  
11:21:08 697  PROCEDURE GET_ENDED_GC_LICENSES (
11:21:08 698  	out_result_set		OUT SYS_REFCURSOR,
11:21:08 699  	in_hours_number 	IN NUMBER DEFAULT 14*24,
11:21:08 700  	in_num_rows		IN NUMBER DEFAULT 10000,
11:21:08 701  	in_process_name IN PROCESS_RETRY_THROTTLE.PROCESS_NAME%TYPE
11:21:08 702  ) AS
11:21:08 703  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_ENDED_GC_INVOICES';
11:21:08 704  var_days		NUMBER;
11:21:08 705  var_hours 	NUMBER;
11:21:08 706  var_time_interval INTERVAL DAY (3) TO SECOND;
11:21:08 707  BEGIN
11:21:08 708  	var_hours := mod(in_hours_number,24);
11:21:08 709  	var_days := (in_hours_number - var_hours) / 24;
11:21:08 710  	var_time_interval := var_days||' '||var_hours||':0:0';
11:21:08 711  	OPEN out_result_set FOR
11:21:08 712  SELECT * FROM
11:21:08 713  (
11:21:08 714  	SELECT
11:21:08 715  	  l.ID,
11:21:08 716  	  l.CREATE_DATE,
11:21:08 717  	  l.CREATED_BY,
11:21:08 718  	  l.CURRENT_OFFER_INDEX,
11:21:08 719  	  l.CURRENT_OFFER_RECURR_NUM,
11:21:08 720  	  l.END_DATE,
11:21:08 721  	  l.ENTITLEMENT_END_DATE,
11:21:08 722  	  l.INVOICE_ID,
11:21:08 723  	  l.IS_EXTENSION,
11:21:08 724  	  l.LICENSE_STATUS_ID,
11:21:08 725  	  l.OFFER_ID,
11:21:08 726  	  l.START_DATE,
11:21:08 727  	  l.SUBSCRIPTION_ID,
11:21:08 728  	  l.UPDATE_DATE,
11:21:08 729  	  l.UPDATED_BY
11:21:08 730  	FROM
11:21:08 731  	  GIFT_CERTIFICATE gc
11:21:08 732  	  INNER JOIN INVOICE i ON i.id = gc.PURCHASE_INVOICE_ID
11:21:08 733  	  INNER JOIN LICENSE l ON l.invoice_id = i.id
11:21:08 734  	  LEFT JOIN SUBSCRIPTION s ON s.id = l.subscription_id
11:21:08 735  	WHERE
11:21:08 736  	  l.LICENSE_STATUS_ID != GLOBAL_STATUSES_V18.LICENSE_ACTIVE
11:21:08 737  	  AND l.ENTITLEMENT_END_DATE <= (current_timestamp)
11:21:08 738  	  AND l.ENTITLEMENT_END_DATE > (current_timestamp - var_time_interval)
11:21:08 739  	  AND s.subscription_status_id = GLOBAL_STATUSES_V18.SUBSCRIPTION_CLOSED
11:21:08 740  	  AND NOT EXISTS (
11:21:08 741  	    SELECT NULL
11:21:08 742  	    FROM PROCESS_RETRY_THROTTLE
11:21:08 743  	    WHERE PROCESS_NAME = in_process_name
11:21:08 744  	      AND GENERIC_ID = l.ID
11:21:08 745  	  )
11:21:08 746  	  AND ROWNUM <= in_num_rows*10
11:21:08 747  	  ORDER BY dbms_random.value
11:21:08 748  ) WHERE
11:21:08 749  	  ROWNUM <= in_num_rows
11:21:08 750  	  GROUP BY SUBSCRIPTION_ID;
11:21:08 751  EXCEPTION
11:21:08 752  WHEN OTHERS THEN
11:21:08 753  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:08 754  	  SPROC_NAME, 'Unknown error', SQLERRM);
11:21:08 755  END GET_ENDED_GC_LICENSES;
11:21:08 756  
