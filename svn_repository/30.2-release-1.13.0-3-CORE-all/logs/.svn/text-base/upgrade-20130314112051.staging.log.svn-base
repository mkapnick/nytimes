11:21:07 1694  	       )
11:21:07 1695  	       INNER JOIN OFFER_PRODUCT_OFFERING ON OFFER_OFFER_CHAIN_OFFER_ID = OFFER_PRODUCT_OFFERING.OFFER_ID
11:21:07 1696  	       INNER JOIN PRODUCT_OFFERING ON OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = PRODUCT_OFFERING.ID
11:21:07 1697  	     GROUP BY
11:21:07 1698  	       PRODUCT_OFFERING.PRODUCT_ID
11:21:07 1699  	   ) ON PRODUCT_ID_IN_OFFER_CH_1 = PRODUCT_ID_IN_OFFER_CH_2;
11:21:07 1700  
11:21:07 1701  END GET_OFF_CHAINS_SAME_PRODUCTS;
