11:21:09 291  PROCEDURE TEST_DELETE_USER_ACCOUNTS  (
11:21:09 292  	in_start_group_id IN NUMBER,
11:21:09 293  	in_end_group_id   IN NUMBER
11:21:09 294  ) IS
11:21:09 295   gid ACCOUNT.GROUP_ID%TYPE;
11:21:09 296   CURSOR c (v_from NUMBER, v_to NUMBER) IS SELECT ACCOUNT.GROUP_ID FROM ACCOUNT WHERE GROUP_ID BETWEEN v_from AND v_to;
11:21:09 297  BEGIN
11:21:09 298  -- arosolovskiy refactoring: call delete_user_account only "COUNT(group_id) WHERE ...." times instead of "in_end_group_id - in_start_group_id" times;
11:21:09 299  	/*
11:21:09 300  	FOR var_group_id IN in_start_group_id..in_end_group_id
11:21:09 301  	LOOP
11:21:09 302  	  TEST_DELETE_USER_ACCOUNT(var_group_id);
11:21:09 303  	END LOOP;*/
11:21:09 304  	OPEN c(in_start_group_id, in_end_group_id);
11:21:09 305  	WHILE c%ISOPEN LOOP
11:21:09 306  	  FETCH c INTO gid;
11:21:09 307  	  IF c%NOTFOUND THEN
11:21:09 308  	   CLOSE c;
11:21:09 309  	  END IF;
11:21:09 310  	  TEST_DELETE_USER_ACCOUNT(gid);
11:21:09 311  	END LOOP;
11:21:09 312  END;
