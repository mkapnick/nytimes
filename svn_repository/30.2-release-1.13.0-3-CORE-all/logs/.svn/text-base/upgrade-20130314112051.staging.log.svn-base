11:21:09 522  	  RAISE PENDING_TRANSACTIONS_FOUND;
11:21:09 523  	END IF;
11:21:09 524  
11:21:09 525  	-- Getting current default financial instrument
11:21:09 526  	BEGIN
11:21:09 527  	  GET_DEF_FINANCIAL_INSTRUMENT(
11:21:09 528  	    in_group_id 	   => var_group_id,
11:21:09 529  	    out_instrument_type_id => current_def_instrument_type_id,
11:21:09 530  	    out_instrument_id	   => current_def_instrument_id
11:21:09 531  	  );
11:21:09 532  	  EXCEPTION
11:21:09 533  	    WHEN OTHERS THEN
11:21:09 534  	      EXCEPTION_MESSAGE := SQLERRM;
11:21:09 535  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
11:21:09 536  	END;
11:21:09 537  
11:21:09 538  	-- Checking that credit card is not default
11:21:09 539  	IF current_def_instrument_type_id = GLOBAL_ENUMS_V18.INSTRUMENT_PAYPAL
11:21:09 540  	  AND current_def_instrument_id = in_paypal_id THEN
11:21:09 541  	  BEGIN
11:21:09 542  	    DEL_DEF_FINANCIAL_INSTRUMENT(
11:21:09 543  	      in_group_id => var_group_id
11:21:09 544  	    );
11:21:09 545  	  EXCEPTION
11:21:09 546  	    WHEN OTHERS THEN
11:21:09 547  	      EXCEPTION_MESSAGE := SQLERRM;
11:21:09 548  	      RAISE CAN_NOT_DEL_DEF_FINANCIAL;
11:21:09 549  	  END;
11:21:09 550  	END IF;
11:21:09 551  
11:21:09 552  	PROCS_FIN_INSTRUMENTS_V18.UPDATE_PAYPAL_STATUS(
11:21:09 553  	  in_paypal_id	      => in_paypal_id,
11:21:09 554  	  in_updated_by       => in_updated_by,
11:21:09 555  	  in_paypal_status_id => GLOBAL_STATUSES_V18.PAYPAL_INACTIVE
11:21:09 556  	);
11:21:09 557  
11:21:09 558  EXCEPTION
