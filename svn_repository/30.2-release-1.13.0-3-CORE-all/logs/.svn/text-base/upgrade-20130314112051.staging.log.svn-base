11:21:07  73  	  C.DESCRIPTION,
11:21:07  74  	  1 INHERITED,
11:21:07  75  	  C.SHAREABLE,
11:21:07  76  	  MAX(LEAST(SS.END_DATE, LIC.ENTITLEMENT_END_DATE)) EXPIRES
11:21:07  77  	FROM
11:21:07  78  	  ACCOUNT BORROWER,
11:21:07  79  	  SUBSCRIPTION S,
