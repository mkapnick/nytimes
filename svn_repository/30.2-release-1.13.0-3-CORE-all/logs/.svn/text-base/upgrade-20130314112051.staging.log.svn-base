11:21:08 417  PROCEDURE FINALIZE_SUBSCRIPTION_CREATION (
11:21:08 418  	in_subscription_id    IN NUMBER,
11:21:08 419  	in_invoice_id	      IN NUMBER,
11:21:08 420  	in_instrument_type_id IN NUMBER,
11:21:08 421  	in_instrument_id      IN NUMBER,
11:21:08 422  	in_created_by	      IN VARCHAR2
11:21:08 423  ) AS
11:21:08 424  SPROC_NAME CONSTANT VARCHAR2(30) := 'FINALIZE_SUBSCRIPTION_CREATION';
11:21:08 425  -- VARIABLES
11:21:08 426  var_invoice_amount     NUMBER(10, 2);
11:21:08 427  var_new_transaction_id NUMBER;
11:21:08 428  var_new_charge_id      NUMBER;
11:21:08 429  -- EXCEPTIONS
11:21:08 430  CAN_NOT_USE_FCINSTR	  EXCEPTION;
11:21:08 431  CAN_NOT_CALC_INVOICE_AMOUNT EXCEPTION;
11:21:08 432  CAN_NOT_CREATE_TRANSACTION  EXCEPTION;
11:21:08 433  CAN_NOT_CREATE_CHARGE	  EXCEPTION;
11:21:08 434  
11:21:08 435  EXCEPTION_MESSAGE VARCHAR2(1024);
11:21:08 436  BEGIN
11:21:08 437  
11:21:08 438  	-- Calculate invoice amount ( + discounts, taxes)
11:21:08 439  	BEGIN
11:21:08 440  	  PROCS_INVOICE_V18.CALCULATE_INVOICE_AMOUNT(
11:21:08 441  	    in_invoice_id => in_invoice_id,
11:21:08 442  	    out_amount	  => var_invoice_amount
11:21:08 443  	  );
11:21:08 444  	  EXCEPTION
11:21:08 445  	   WHEN OTHERS THEN
11:21:08 446  	     EXCEPTION_MESSAGE := SQLERRM;
11:21:08 447  	     RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
11:21:08 448  	END;
11:21:08 449  
11:21:08 450  	IF in_instrument_type_id = GLOBAL_ENUMS_V18.INSTRUMENT_ZCI_INSTRUMENT
11:21:08 451  	  AND var_invoice_amount > 0 THEN
11:21:08 452  	  RAISE CAN_NOT_USE_FCINSTR;
11:21:08 453  	END IF;
11:21:08 454  
11:21:08 455  	IF var_invoice_amount = 0 THEN
11:21:08 456  	  -- UPDATE INVOICE. SET STATUS TO PROCESSED
11:21:08 457  	  PROCS_INVOICE_CRU_V18.UPDATE_INVOICE(
