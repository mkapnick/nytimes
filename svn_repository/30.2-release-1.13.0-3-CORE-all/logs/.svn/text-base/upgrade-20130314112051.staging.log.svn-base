11:21:07  88  	PROCEDURE CHASE_PROFILE_BY_REQ_FILE_ID(
11:21:07  89  	  in_request_file_id IN CC_UPDATE.CC_REQUEST_FILE_ID%TYPE,
11:21:07  90  	  in_start	     IN NUMBER,
11:21:07  91  	  in_end	     IN NUMBER,
11:21:07  92  	  out_result_set     OUT SYS_REFCURSOR
11:21:07  93  	) AS
11:21:07  94  	SPROC_NAME CONSTANT VARCHAR2(32) := 'CHASE_PROFILE_BY_REQ_FILE_ID';
11:21:07  95  	var_range_diff	    NUMBER := 0;
11:21:07  96  	var_upper_bond_diff NUMBER := 0;
11:21:07  97  	var_l_start	    NUMBER := 0;
11:21:07  98  	var_l_end	    NUMBER := 0;
11:21:07  99  	BEGIN
11:21:07 100  	  --Normalize the end points [START]
11:21:07 101  	  IF (in_start IS NULL OR in_start < 0) Then
11:21:07 102  	    var_l_start := 0;
11:21:07 103  	  ELSE
11:21:07 104  	    var_l_start := in_start;
11:21:07 105  	  END IF;
11:21:07 106  
11:21:07 107  	  IF (in_end IS NULL) Then
11:21:07 108  	    var_l_end := 500;
11:21:07 109  	  ELSE
11:21:07 110  	    var_l_end := in_end;
11:21:07 111  	  END IF;
11:21:07 112  
11:21:07 113  	  var_l_start := var_l_start + 1;
11:21:07 114  	  var_l_end   := var_l_end   + 1;
11:21:07 115  
11:21:07 116  	  var_range_diff  := var_l_end - var_l_start;
11:21:07 117  	  var_upper_bond_diff :=  var_range_diff - 1000;
11:21:07 118  
11:21:07 119  	  IF (var_upper_bond_diff > 0) Then
11:21:07 120  	    var_l_end := var_l_end - var_upper_bond_diff;
11:21:07 121  	  END IF;
11:21:07 122  	  --Normalize the end points [END]
11:21:07 123  
11:21:07 124  	  OPEN out_result_set FOR
11:21:07 125  	    SELECT CHASE_PROFILE_ID FROM
11:21:07 126  	      (SELECT rownum rnum, q.* FROM
11:21:07 127  		 (SELECT
11:21:07 128  		    cc.CHASE_PROFILE_ID
11:21:07 129  		  FROM
11:21:07 130  		    CREDIT_CARD cc,
11:21:07 131  		    CC_UPDATE ccu
11:21:07 132  		  WHERE
11:21:07 133  		    ccu.CC_REQUEST_FILE_ID = in_request_file_id
11:21:07 134  		    AND ccu.CREDIT_CARD_ID = cc.id
11:21:07 135  		) Q
11:21:07 136  	      WHERE rownum <= var_l_end)
11:21:07 137  	    WHERE rnum >= var_l_Start;
11:21:07 138  	END CHASE_PROFILE_BY_REQ_FILE_ID;
