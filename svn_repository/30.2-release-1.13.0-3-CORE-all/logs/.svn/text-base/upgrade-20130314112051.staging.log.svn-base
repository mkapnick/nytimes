11:21:07   5  	PROCEDURE POPULATE_REQUEST_INFO(
11:21:07   6  	  in_hours_prior    IN	NUMBER,
11:21:07   7  	  in_filename	    IN	CC_REQUEST_FILE.FILE_NAME%TYPE,
11:21:07   8  	  in_creator	    IN	CC_REQUEST_FILE.UPDATED_BY%TYPE
11:21:07   9  	) AS
11:21:07  10  	SPROC_NAME CONSTANT VARCHAR2(32) := 'POPULATE_REQUEST_INFO';
11:21:07  11  	var_start_date	    DATE := SYSDATE;
11:21:07  12  	var_end_date	    DATE := var_start_date + (in_hours_prior/24);
11:21:07  13  	var_request_file_id NUMBER := 0;
11:21:07  14  	var_license_count   NUMBER := 0;
11:21:07  15  	var_cc_update_count NUMBER := 0;
11:21:07  16  	BEGIN
11:21:07  17  	    SELECT CC_REQUEST_FILE_ID_SEQ.NEXTVAL INTO var_request_file_id  FROM DUAL;
11:21:07  18  	    INSERT INTO CC_REQUEST_FILE (ID,
11:21:07  19  					 FILE_NAME,
11:21:07  20  					 CC_REQUEST_FILE_STATUS,
11:21:07  21  					 CREATE_DATE,
11:21:07  22  					 CREATED_BY,
11:21:07  23  					 UPDATE_DATE,
11:21:07  24  					 UPDATED_BY)
11:21:07  25  					 VALUES (
11:21:07  26  					 var_request_file_id,
11:21:07  27  					 in_filename,
11:21:07  28  					 'NOT_CREATED',
11:21:07  29  					 var_start_date,
11:21:07  30  					 in_creator,
11:21:07  31  					 var_start_date,
11:21:07  32  					 in_creator);
11:21:07  33  
11:21:07  34  	   FOR record IN (SELECT
11:21:07  35  			    l.ID LICENSE_ID, cc.ID CREDIT_CARD_ID
11:21:07  36  			  FROM
11:21:07  37  			    LICENSE l INNER JOIN SUBSCRIPTION s ON L.SUBSCRIPTION_ID = s.ID
11:21:07  38  				      INNER JOIN CREDIT_CARD cc ON S.INSTRUMENT_ID   = cc.ID
11:21:07  39  			  WHERE
11:21:07  40  			    s.INSTRUMENT_TYPE_ID	 = 1
11:21:07  41  			    AND cc.CREDIT_CARD_STATUS_ID = 1
11:21:07  42  			    AND s.SUBSCRIPTION_STATUS_ID = 1
11:21:07  43  			    AND l.LICENSE_STATUS_ID	 = 2
11:21:07  44  			    AND cc.CREDIT_CARD_TYPE_ID IN (2,3)
11:21:07  45  			    AND l.END_DATE BETWEEN var_start_date AND var_end_date
11:21:07  46  			    AND l.ID NOT IN (SELECT LICENSE_ID FROM CC_UPDATE))
11:21:07  47  	   LOOP
11:21:07  48  	     var_license_count := 0;
11:21:07  49  	     SELECT COUNT(1) INTO  var_license_count FROM CC_UPDATE WHERE LICENSE_ID = record.LICENSE_ID;
11:21:07  50  
11:21:07  51  	     IF var_license_count = 0 THEN
11:21:07  52  		INSERT INTO CC_UPDATE (ID,
11:21:07  53  				       CREDIT_CARD_ID,
11:21:07  54  				       LICENSE_ID,
11:21:07  55  				       CC_UPDATE_STATUS,
11:21:07  56  				       CC_REQUEST_FILE_ID,
11:21:07  57  				       CREATE_DATE,
11:21:07  58  				       UPDATE_DATE,
11:21:07  59  				       CREATED_BY,
11:21:07  60  				       UPDATED_BY
11:21:07  61  				       ) VALUES (
11:21:07  62  				       CC_UPDATE_SEQ.NEXTVAL,
11:21:07  63  				       record.CREDIT_CARD_ID,
11:21:07  64  				       record.LICENSE_ID,
11:21:07  65  				       'NOT_ADDED_TO_FILE',
11:21:07  66  				       var_request_file_id,
11:21:07  67  				       var_start_date,
11:21:07  68  				       var_start_date,
11:21:07  69  				       in_creator,
11:21:07  70  				       in_creator
11:21:07  71  				       );
11:21:07  72  	     END IF;
11:21:07  73  	   END LOOP;
11:21:07  74  
11:21:07  75  	   SELECT COUNT(1) INTO var_cc_update_count
11:21:07  76  	   FROM CC_UPDATE
11:21:07  77  	   WHERE CC_REQUEST_FILE_ID = var_request_file_id;
11:21:07  78  	   IF var_cc_update_count <= 0 THEN
11:21:07  79  	     UPDATE CC_REQUEST_FILE
11:21:07  80  	     SET CC_REQUEST_FILE_STATUS = 'EMPTY'
11:21:07  81  	     WHERE ID = var_request_file_id;
11:21:07  82  	   END IF;
11:21:07  83  
11:21:07  84  	END POPULATE_REQUEST_INFO;
