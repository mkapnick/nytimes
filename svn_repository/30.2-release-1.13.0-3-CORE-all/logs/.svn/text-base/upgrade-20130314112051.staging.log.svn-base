11:21:07 1898  PROCEDURE CHECK_PRODUCT_ELIGIBILITY (
11:21:07 1899  	 in_group_id	   IN NUMBER,
11:21:07 1900  	 in_offer_chain_id IN NUMBER,
11:21:07 1901  	 out_is_eligible   OUT NUMBER,
11:21:07 1902  	 out_concurrent_subscription_id OUT NUMBER
11:21:07 1903  ) AS
11:21:07 1904  SPROC_NAME CONSTANT VARCHAR2(25) := 'CHECK_PRODUCT_ELIGIBILITY';
11:21:07 1905  -- Variables
11:21:07 1906  var_account_id  NUMBER;
11:21:07 1907  var_is_eligible NUMBER;
11:21:07 1908  var_is_gc       NUMBER;
11:21:07 1909  -- Exceptions
11:21:07 1910  BAD_GROUP_ID		   EXCEPTION;
11:21:07 1911  CAN_NOT_CHECK_SAME_PRODUCTS EXCEPTION;
11:21:07 1912  BAD_OC_ID		   EXCEPTION;
11:21:07 1913  EXCEPTION_MESSAGE	   VARCHAR(1024);
11:21:07 1914  BEGIN
11:21:07 1915  
11:21:07 1916  	 var_is_eligible := GLOBAL_CONSTANTS_V18.TRUE;
11:21:07 1917  
11:21:07 1918  	 out_concurrent_subscription_id := NULL;
11:21:07 1919  
11:21:07 1920  	 BEGIN
11:21:07 1921  	   SELECT
11:21:07 1922  	     OC.IS_GIFT_CERTIFICATE into var_is_gc
11:21:07 1923  	   FROM
11:21:07 1924  	     OFFER_CHAIN OC
11:21:07 1925  	   WHERE
11:21:07 1926  	     OC.ID = in_offer_chain_id;
11:21:07 1927  	   EXCEPTION
11:21:07 1928  	     WHEN NO_DATA_FOUND THEN
11:21:07 1929  	       RAISE BAD_OC_ID;
11:21:07 1930  	 END;
11:21:07 1931  
11:21:07 1932  	 -- only check eligibility if this is not a gift certificate
11:21:07 1933  	 IF (var_is_gc IS NULL OR var_is_gc != 1) THEN
11:21:07 1934  	   BEGIN
11:21:07 1935  	     SELECT
11:21:07 1936  	       A.ID into var_account_id
11:21:07 1937  	     FROM
11:21:07 1938  	       ACCOUNT A
11:21:07 1939  	     WHERE
11:21:07 1940  	       A.GROUP_ID = in_group_id;
11:21:07 1941  	     EXCEPTION
11:21:07 1942  	       WHEN NO_DATA_FOUND THEN
11:21:07 1943  		 RAISE BAD_GROUP_ID;
11:21:07 1944  	   END;
11:21:07 1945  
11:21:07 1946  
11:21:07 1947  	   FOR f_offer_chain IN (
11:21:07 1948  	       SELECT
11:21:07 1949  		 S.ID as SUBSCRIPTION_ID,
11:21:07 1950  		 S.OFFER_CHAIN_ID
11:21:07 1951  	       FROM
11:21:07 1952  		 SUBSCRIPTION S
11:21:07 1953  	       WHERE
11:21:07 1954  		 S.ACCOUNT_ID = var_account_id
11:21:07 1955  		 AND (S.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V18.SUBSCRIPTION_ACTIVE
11:21:07 1956  		      OR S.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V18.SUBSCRIPTION_IN_GRACE_PERIOD)
11:21:07 1957  	   )
11:21:07 1958  	   LOOP
11:21:07 1959  	     BEGIN
11:21:07 1960  	       IF (
11:21:07 1961  		 PROCS_OFFER_CHAIN_V18.CHECK_FOR_SAME_PRODUCTS(
11:21:07 1962  		   in_offer_chain_id,
11:21:07 1963  		   f_offer_chain.offer_chain_id,
11:21:07 1964  		   GLOBAL_CONSTANTS_V18.TRUE
11:21:07 1965  		 ) = GLOBAL_CONSTANTS_V18.TRUE
11:21:07 1966  	       ) THEN
11:21:07 1967  		 var_is_eligible := GLOBAL_CONSTANTS_V18.FALSE;
11:21:07 1968  		 out_concurrent_subscription_id := f_offer_chain.SUBSCRIPTION_ID;
11:21:07 1969  	       END IF;
11:21:07 1970  	       EXCEPTION
11:21:07 1971  		 WHEN OTHERS THEN
11:21:07 1972  		   EXCEPTION_MESSAGE := SQLERRM;
11:21:07 1973  		   RAISE CAN_NOT_CHECK_SAME_PRODUCTS;
11:21:07 1974  	     END;
11:21:07 1975  	   END LOOP;
11:21:07 1976  	 END IF;
11:21:07 1977  	 out_is_eligible := var_is_eligible;
11:21:07 1978  
