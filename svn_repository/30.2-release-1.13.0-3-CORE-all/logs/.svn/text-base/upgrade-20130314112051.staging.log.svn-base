11:21:09 458  */
11:21:09 459  	in_paypal_id  IN NUMBER,
11:21:09 460  	in_updated_by IN VARCHAR2
11:21:09 461  ) AS
11:21:09 462  SPROC_NAME CONSTANT VARCHAR2(14) := 'DISABLE_PAYPAL';
11:21:09 463  -- VARIABLES
11:21:09 464  var_paypal_status_id NUMBER;
11:21:09 465  var_account_id	   NUMBER;
11:21:09 466  var_group_id	   NUMBER;
