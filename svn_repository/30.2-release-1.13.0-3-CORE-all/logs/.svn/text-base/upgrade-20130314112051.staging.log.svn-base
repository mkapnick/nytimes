11:21:09 928  var_ga_type OFFER_CHAIN.GROUP_ACCOUNT_TYPE_ID%TYPE;
11:21:09 929  var_is_dupe NUMBER(1);
11:21:09 930  NOT_GL EXCEPTION;
11:21:09 931  DUPE EXCEPTION;
11:21:09 932  BEGIN
11:21:09 933  	  SELECT OFFER_CHAIN.GROUP_ACCOUNT_TYPE_ID into var_ga_type
