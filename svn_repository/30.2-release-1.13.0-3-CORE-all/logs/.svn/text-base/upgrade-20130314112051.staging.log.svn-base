11:21:08 231  		   OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V18.SUBSCRIPTION_IN_GRACE_PERIOD)
11:21:08 232  	    ORDER BY
11:21:08 233  	      ID
11:21:08 234  	  )
11:21:08 235  	  WHERE
11:21:08 236  	    ROWNUM <= 1;
11:21:08 237  	  RAISE LIMIT_REACHED;
11:21:08 238  	END IF;
11:21:08 239  
11:21:08 240  	-- if user does not have any active existing subscriptions to the offer chain
11:21:08 241  	-- and if product from the offer chain is already owned from another offer chain
11:21:08 242  	-- then deny purchase
11:21:08 243  	IF (in_check_dupe_products != GLOBAL_CONSTANTS_V18.FALSE) THEN
11:21:08 244  	  FOR f_account_offer_chains IN (
