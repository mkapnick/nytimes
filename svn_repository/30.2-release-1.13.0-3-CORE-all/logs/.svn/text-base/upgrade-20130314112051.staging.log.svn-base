11:21:07 1671  	   COUNT_1 + COUNT_2	    AS "COUNT"
11:21:07 1672  	 FROM
11:21:07 1673  	   (
11:21:07 1674  	     SELECT
11:21:07 1675  	       PRODUCT_OFFERING.PRODUCT_ID as "PRODUCT_ID_IN_OFFER_CH_1",
