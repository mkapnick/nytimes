11:21:07 1164  PRODUCT_ELIGIBILITY_NAME CONSTANT VARCHAR2(19) := 'MAX_CONCURRENT_SUBS';
11:21:07 1165  -- VARIABLES
11:21:07 1166  temp_offer_chain_id	OFFER_CHAIN.ID%TYPE;
11:21:07 1167  var_same_products	SYS_REFCURSOR;
11:21:07 1168  var_same_product_id	NUMBER;
11:21:07 1169  same_product_count	NUMBER;
11:21:07 1170  var_product_eligibility_limit NUMBER;
11:21:07 1171  s_product_eligibility_limit   VARCHAR2(100);
11:21:07 1172  -- EXCEPTIONS
11:21:07 1173  BAD_FIRST_OFFER_CHAIN	      EXCEPTION;
11:21:07 1174  BAD_SECOND_OFFER_CHAIN	      EXCEPTION;
11:21:07 1175  BEGIN
11:21:07 1176  
11:21:07 1177  	 -- Check that first offer chain exists
11:21:07 1178  	 BEGIN
11:21:07 1179  	   SELECT
11:21:07 1180  	     OFFER_CHAIN.ID into temp_offer_chain_id
11:21:07 1181  	   FROM
11:21:07 1182  	     OFFER_CHAIN
11:21:07 1183  	   WHERE
11:21:07 1184  	     OFFER_CHAIN.ID = in_offer_chain_1;
11:21:07 1185  	   EXCEPTION
11:21:07 1186  	     WHEN NO_DATA_FOUND THEN
11:21:07 1187  	       RAISE BAD_FIRST_OFFER_CHAIN;
11:21:07 1188  	 END;
11:21:07 1189  
11:21:07 1190  	 -- Check that second offer chain exists
11:21:07 1191  	 BEGIN
11:21:07 1192  	   SELECT
11:21:07 1193  	     OFFER_CHAIN.ID into temp_offer_chain_id
11:21:07 1194  	   FROM
11:21:07 1195  	     OFFER_CHAIN
11:21:07 1196  	   WHERE
11:21:07 1197  	     OFFER_CHAIN.ID = in_offer_chain_2;
11:21:07 1198  	   EXCEPTION
11:21:07 1199  	     WHEN NO_DATA_FOUND THEN
11:21:07 1200  	       RAISE BAD_SECOND_OFFER_CHAIN;
11:21:07 1201  	 END;
11:21:07 1202  
11:21:07 1203  	 PROCS_OFFER_CHAIN_V18.GET_OFF_CHAINS_SAME_PRODUCTS(
11:21:07 1204  	   in_offer_chain_1 => in_offer_chain_1,
11:21:07 1205  	   in_offer_chain_2 => in_offer_chain_2,
11:21:07 1206  	   out_result_set   => var_same_products
11:21:07 1207  	 );
11:21:07 1208  
11:21:07 1209  	 LOOP
11:21:07 1210  	   FETCH var_same_products INTO var_same_product_id, same_product_count;
11:21:07 1211  	   EXIT WHEN var_same_products%NOTFOUND;
11:21:07 1212  
11:21:07 1213  	   IF in_use_eligibility_rules = GLOBAL_CONSTANTS_V18.FALSE THEN
11:21:07 1214  	     -- Return false because this offer chains having same products
11:21:07 1215  	     RETURN GLOBAL_CONSTANTS_V18.TRUE;
11:21:07 1216  	   ELSE
11:21:07 1217  
11:21:07 1218  	     -- Get eligibility rule for given product
11:21:07 1219  	     BEGIN
11:21:07 1220  	       SELECT
11:21:07 1221  		 PRODUCT_ELIGIBILITY.VALUE into s_product_eligibility_limit
11:21:07 1222  	       FROM
11:21:07 1223  		 PRODUCT_ELIGIBILITY
11:21:07 1224  	       WHERE
11:21:07 1225  		 PRODUCT_ELIGIBILITY.PRODUCT_ID = var_same_product_id
11:21:07 1226  		 AND PRODUCT_ELIGIBILITY.NAME = PRODUCT_ELIGIBILITY_NAME;
11:21:07 1227  
11:21:07 1228  	       -- REVU: What should to be here? 1?
11:21:07 1229  	       EXCEPTION
11:21:07 1230  		 WHEN NO_DATA_FOUND THEN
11:21:07 1231  		   s_product_eligibility_limit := '1';
11:21:07 1232  	     END;
11:21:07 1233  
11:21:07 1234  	     IF UPPER(s_product_eligibility_limit) = GLOBAL_CONSTANTS_V18.MAX_CONSURRENT_PRD_UNLIM THEN
11:21:07 1235  	       RETURN GLOBAL_CONSTANTS_V18.FALSE;
11:21:07 1236  	     END IF;
11:21:07 1237  
11:21:07 1238  	     var_product_eligibility_limit := TO_NUMBER(s_product_eligibility_limit);
11:21:07 1239  
11:21:07 1240  	     -- Check for limit
11:21:07 1241  	     IF var_product_eligibility_limit < same_product_count THEN
11:21:07 1242  	       RETURN GLOBAL_CONSTANTS_V18.TRUE;
11:21:07 1243  	     END IF;
11:21:07 1244  
11:21:07 1245  	   END IF;
11:21:07 1246  	 END LOOP;
11:21:07 1247  
11:21:07 1248  	 RETURN GLOBAL_CONSTANTS_V18.FALSE;
11:21:07 1249  
11:21:07 1250  EXCEPTION
11:21:07 1251  WHEN BAD_FIRST_OFFER_CHAIN THEN
11:21:07 1252  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:07 1253  	   SPROC_NAME, 'First offer chain not found');
11:21:07 1254  WHEN BAD_SECOND_OFFER_CHAIN THEN
11:21:07 1255  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:07 1256  	   SPROC_NAME, 'Second offer chain not found');
11:21:07 1257  WHEN OTHERS THEN
11:21:07 1258  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:07 1259  	   SPROC_NAME, 'Unknown error', SQLERRM);
11:21:07 1260  END CHECK_FOR_SAME_PRODUCTS;
