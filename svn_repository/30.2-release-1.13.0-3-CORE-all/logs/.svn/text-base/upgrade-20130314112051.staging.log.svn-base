11:21:08 254  		OR SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V18.SUBSCRIPTION_IN_GRACE_PERIOD)
11:21:08 255  	  )
11:21:08 256  	  LOOP
11:21:08 257  	    IF PROCS_OFFER_CHAIN_V18.CHECK_FOR_SAME_PRODUCTS(
11:21:08 258  	      in_offer_chain_1	       => in_offer_chain_id,
11:21:08 259  	      in_offer_chain_2	       => f_account_offer_chains.OFFER_CHAIN_ID,
11:21:08 260  	      in_use_eligibility_rules => GLOBAL_CONSTANTS_V18.TRUE
11:21:08 261  	    ) = GLOBAL_CONSTANTS_V18.TRUE THEN
11:21:08 262  
11:21:08 263  	      -- Find first concurrent subscription id:
11:21:08 264  	      SELECT
11:21:08 265  		ID into var_concur_subscription_id
11:21:08 266  	      FROM (
11:21:08 267  		SELECT
11:21:08 268  		  ID
11:21:08 269  		FROM
11:21:08 270  		  SUBSCRIPTION
11:21:08 271  		WHERE
11:21:08 272  		  SUBSCRIPTION.ACCOUNT_ID = var_account_id
11:21:08 273  		  AND SUBSCRIPTION.OFFER_CHAIN_ID = f_account_offer_chains.OFFER_CHAIN_ID
11:21:08 274  		  AND (
11:21:08 275  		    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V18.SUBSCRIPTION_ACTIVE
11:21:08 276  		    OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V18.SUBSCRIPTION_SUSPENDED
11:21:08 277  		    OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V18.SUBSCRIPTION_IN_GRACE_PERIOD)
11:21:08 278  		ORDER BY
11:21:08 279  		  ID
11:21:08 280  	      )
11:21:08 281  	      WHERE
11:21:08 282  		ROWNUM <= 1;
11:21:08 283  
11:21:08 284  	      RAISE PRODUCT_ALREADY_PURCHASED;
11:21:08 285  	    END IF;
11:21:08 286  	  END LOOP;
11:21:08 287  	END IF;
