11:21:08 2823  PROCEDURE ARE_REFUNDS_PENDING_FOR_SUBSCR (
11:21:08 2824  /*
11:21:08 2825  APP_EXCEPTION_CODES_V18.NOT_FOUND
11:21:08 2826  APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR
11:21:08 2827  */
11:21:08 2828  	 in_subscription_id IN NUMBER,
11:21:08 2829  	 out_result	    OUT NUMBER
11:21:08 2830  ) AS
11:21:08 2831  SPROC_NAME CONSTANT VARCHAR2(30) := 'ARE_REFUNDS_PENDING_FOR_SUBSCR';
11:21:08 2832  -- VARIABLES
11:21:08 2833  temp_subscription_id NUMBER;
11:21:08 2834  var_local_result     NUMBER;
11:21:08 2835  -- EXCEPTIONS
11:21:08 2836  BAD_SUBSCRIPTION_ID EXCEPTION;
11:21:08 2837  BEGIN
11:21:08 2838  
11:21:08 2839  	 -- Check that subscription exists
11:21:08 2840  	 BEGIN
11:21:08 2841  	   SELECT
11:21:08 2842  	     SUBSCRIPTION.ID into temp_subscription_id
11:21:08 2843  	   FROM
11:21:08 2844  	     SUBSCRIPTION
11:21:08 2845  	   WHERE
11:21:08 2846  	     SUBSCRIPTION.ID = in_subscription_id;
11:21:08 2847  	   EXCEPTION
11:21:08 2848  	     WHEN NO_DATA_FOUND THEN
11:21:08 2849  	       RAISE BAD_SUBSCRIPTION_ID;
11:21:08 2850  	 END;
11:21:08 2851  
11:21:08 2852  	 var_local_result := NULL;
11:21:08 2853  
11:21:08 2854  	 -- Check charges for each invoice associated with gived subscription
11:21:08 2855  	 FOR f_invoice IN (
11:21:08 2856  	   SELECT DISTINCT
11:21:08 2857  	     LICENSE.INVOICE_ID as "ID"
11:21:08 2858  	   FROM
11:21:08 2859  	     LICENSE
11:21:08 2860  	   WHERE
11:21:08 2861  	     LICENSE.SUBSCRIPTION_ID = in_subscription_id
11:21:08 2862  	     AND LICENSE.LICENSE_STATUS_ID IN ( SELECT GLOBAL_STATUSES_V18.LICENSE_ACTIVE FROM DUAL )
11:21:08 2863  	 )
11:21:08 2864  	 LOOP
11:21:08 2865  
11:21:08 2866  	   -- Check each charge in invoice
11:21:08 2867  	   FOR f_charge IN (
11:21:08 2868  	     SELECT
11:21:08 2869  	       CHARGE.ID,
11:21:08 2870  	       CHARGE.CHARGE_STATUS_ID,
11:21:08 2871  	       CHARGE.CHARGE_AMOUNT
11:21:08 2872  	     FROM
11:21:08 2873  	       CHARGE
11:21:08 2874  	     WHERE
11:21:08 2875  	       CHARGE.INVOICE_ID = f_invoice.ID
11:21:08 2876  	   )
11:21:08 2877  	   LOOP
11:21:08 2878  
11:21:08 2879  	     -- Charge amount < 0     => it is a refund
11:21:08 2880  	     -- Charge status is OPEN => means that it is not processed yet
11:21:08 2881  	     IF f_charge.CHARGE_AMOUNT < 0
11:21:08 2882  		AND f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V18.CHARGE_OPENED THEN
11:21:08 2883  	       var_local_result := GLOBAL_CONSTANTS_V18.TRUE;
11:21:08 2884  	     END IF;
11:21:08 2885  
11:21:08 2886  	   END LOOP;
11:21:08 2887  
11:21:08 2888  	 END LOOP;
11:21:08 2889  
11:21:08 2890  	 IF var_local_result IS NULL THEN
11:21:08 2891  	   out_result := GLOBAL_CONSTANTS_V18.FALSE;
11:21:08 2892  	 ELSE
11:21:08 2893  	   out_result := GLOBAL_CONSTANTS_V18.TRUE;
11:21:08 2894  	 END IF;
11:21:08 2895  
11:21:08 2896  EXCEPTION
11:21:08 2897  WHEN BAD_SUBSCRIPTION_ID THEN
11:21:08 2898  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:08 2899  	   SPROC_NAME, 'No such transaction id');
11:21:08 2900  WHEN OTHERS THEN
11:21:08 2901  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:08 2902  	   SPROC_NAME, 'Unknown error', SQLERRM);
11:21:08 2903  END ARE_REFUNDS_PENDING_FOR_SUBSCR;
