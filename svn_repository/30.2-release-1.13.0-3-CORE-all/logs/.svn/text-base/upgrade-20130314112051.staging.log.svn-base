11:21:09 374  
11:21:09 375  PROCEDURE GET_INVOICE_DETAILS (
11:21:09 376  /*
11:21:09 377  Throws exceptions:
11:21:09 378  APP_EXCEPTION_CODES_V18.NOT_FOUND
11:21:09 379  APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR
11:21:09 380  */
11:21:09 381  	in_invoice_id	   IN  NUMBER,
11:21:09 382  	out_group_id	   OUT NUMBER,
11:21:09 383  	out_status_id	   OUT NUMBER,
11:21:09 384  	out_line_items_set OUT SYS_REFCURSOR,
11:21:09 385  	out_pp_charges_set OUT SYS_REFCURSOR,
11:21:09 386  	out_cc_charges_set OUT SYS_REFCURSOR,
11:21:09 387  	out_gc_charges_set OUT SYS_REFCURSOR
11:21:09 388  ) AS
11:21:09 389  -- VARIABLES
11:21:09 390  SPROC_NAME CONSTANT VARCHAR2(19) := 'GET_INVOICE_DETAILS';
11:21:09 391  
11:21:09 392  -- EXCEPTIONS
11:21:09 393  BAD_INVOICE_ID	     EXCEPTION;
11:21:09 394  CAN_NOT_FIND_ACCOUNT   EXCEPTION;
11:21:09 395  EXCEPTION_MESSAGE      VARCHAR2(1024);
11:21:09 396  BEGIN
11:21:09 397  
11:21:09 398  	-- get invoice status
11:21:09 399  	BEGIN
11:21:09 400  	  SELECT
11:21:09 401  	    INVOICE.INVOICE_STATUS_ID into out_status_id
11:21:09 402  	  FROM
11:21:09 403  	    INVOICE
11:21:09 404  	  WHERE
11:21:09 405  	    INVOICE.ID = in_invoice_id;
11:21:09 406  	  EXCEPTION
11:21:09 407  	    WHEN NO_DATA_FOUND THEN
11:21:09 408  	      RAISE BAD_INVOICE_ID;
11:21:09 409  	END;
11:21:09 410  
11:21:09 411  	-- get group id
11:21:09 412  	BEGIN
11:21:09 413  	  PROCS_INVOICE_V18.GET_ACCOUNT_BY_INVOICE_ID(in_invoice_id, out_group_id);
11:21:09 414  	  EXCEPTION
11:21:09 415  	    WHEN OTHERS THEN
11:21:09 416  	      EXCEPTION_MESSAGE := SQLERRM;
11:21:09 417  	      RAISE CAN_NOT_FIND_ACCOUNT;
11:21:09 418  	END;
11:21:09 419  
11:21:09 420  	-- get all line items for given invoice
11:21:09 421  	OPEN out_line_items_set FOR
11:21:09 422  	SELECT
11:21:09 423  	  LINE_ITEM.AMOUNT,
11:21:09 424  	  LINE_ITEM.ID,
11:21:09 425  	  LINE_ITEM.CREATED_BY,
11:21:09 426  	  LINE_ITEM.CREATE_DATE,
11:21:09 427  	  LINE_ITEM.DISCOUNT_AMOUNT,
11:21:09 428  	  LINE_ITEM.TAXES_AMOUNT,
11:21:09 429  	  LINE_ITEM.PRODUCT_OFFER_ID,
11:21:09 430  	  LINE_ITEM.INVOICE_ID
11:21:09 431  	FROM
11:21:09 432  	  LINE_ITEM
11:21:09 433  	WHERE
11:21:09 434  	  LINE_ITEM.INVOICE_ID = in_invoice_id;
11:21:09 435  
11:21:09 436  	-- get all pp charges for given invoice
11:21:09 437  	OPEN out_pp_charges_set FOR
11:21:09 438  	SELECT
11:21:09 439  	  CHARGE.ID as "CHARGE_ID",
11:21:09 440  	  CHARGE_AMOUNT,
11:21:09 441  	  CHARGE.INSTRUMENT_ID,
11:21:09 442  	  CHARGE.CHARGE_STATUS_ID
11:21:09 443  	FROM
11:21:09 444  	  CHARGE
11:21:09 445  	WHERE
11:21:09 446  	  CHARGE.INVOICE_ID = in_invoice_id
11:21:09 447  	  AND CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V18.INSTRUMENT_PAYPAL;
11:21:09 448  
11:21:09 449  	-- get all credir cards for given invoice
11:21:09 450  	OPEN out_cc_charges_set FOR
11:21:09 451  	SELECT
11:21:09 452  	  CHARGE.ID as "CHARGE_ID",
11:21:09 453  	  CHARGE.CHARGE_AMOUNT,
11:21:09 454  	  CHARGE.INSTRUMENT_ID,
11:21:09 455  	  CHARGE.CHARGE_STATUS_ID
11:21:09 456  	FROM
11:21:09 457  	  CHARGE
11:21:09 458  	WHERE
11:21:09 459  	  CHARGE.INVOICE_ID = in_invoice_id
11:21:09 460  	  AND CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V18.INSTRUMENT_CREDIT_CARD;
11:21:09 461  
11:21:09 462  	OPEN out_gc_charges_set FOR
11:21:09 463  	SELECT
11:21:09 464  	  CHARGE.ID as "CHARGE_ID",
11:21:09 465  	  CHARGE.CHARGE_AMOUNT,
11:21:09 466  	  CHARGE.INSTRUMENT_ID,
11:21:09 467  	  CHARGE.CHARGE_STATUS_ID
11:21:09 468  	FROM
11:21:09 469  	  CHARGE
11:21:09 470  	WHERE
11:21:09 471  	  CHARGE.INVOICE_ID = in_invoice_id
11:21:09 472  	  AND CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V18.INSTRUMENT_GIFT_CERTIFICATE;
11:21:09 473  
11:21:09 474  EXCEPTION
11:21:09 475  WHEN BAD_INVOICE_ID THEN
11:21:09 476  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 477  	  SPROC_NAME, 'No such invoice');
11:21:09 478  WHEN CAN_NOT_FIND_ACCOUNT THEN
11:21:09 479  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 480  	  SPROC_NAME, 'Could not find account for given invoice id', EXCEPTION_MESSAGE);
11:21:09 481  WHEN OTHERS THEN
11:21:09 482  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:09 483  	  SPROC_NAME, 'Unknown error', SQLERRM);
11:21:09 484  END GET_INVOICE_DETAILS;
