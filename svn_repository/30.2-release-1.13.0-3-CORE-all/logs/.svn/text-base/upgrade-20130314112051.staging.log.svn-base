11:21:09 937  
11:21:09 938  PROCEDURE APPLY_REFUND (
11:21:09 939  	in_invoice_id	     IN NUMBER,
11:21:09 940  	in_chargeback_amount IN NUMBER,
11:21:09 941  	in_created_by	     IN VARCHAR2,
11:21:09 942  	out_charge_id	     OUT NUMBER
11:21:09 943  ) AS
11:21:09 944  -- VARIABLES
11:21:09 945  SPROC_NAME		 CONSTANT VARCHAR2(16) := 'APPLY_CHARGEBACK';
11:21:09 946  temp_invoice_id		 NUMBER;
11:21:09 947  var_total_charges_amount	 NUMBER(10,2);
11:21:09 948  var_charge_amount_to_apply NUMBER(10,2);
11:21:09 949  var_transaction_id	 NUMBER;
11:21:09 950  var_instrument_type_id	 NUMBER;
11:21:09 951  var_instrument_id 	 NUMBER;
11:21:09 952  var_charge_id		 NUMBER;
11:21:09 953  -- EXCEPTIONS
11:21:09 954  BAD_INVOICE_ID		 EXCEPTION;
11:21:09 955  CAN_NOT_CREATE_TRANSACTION EXCEPTION;
11:21:09 956  CAN_NOT_FIND_INSTRUMENT	 EXCEPTION;
11:21:09 957  CAN_NOT_CREATE_CHARGE	 EXCEPTION;
11:21:09 958  EXCEPTION_MESSAGE 	 VARCHAR2(1024);
11:21:09 959  BEGIN
11:21:09 960  
11:21:09 961  	BEGIN
11:21:09 962  	  SELECT
11:21:09 963  	    INVOICE.ID into temp_invoice_id
11:21:09 964  	  FROM
11:21:09 965  	    INVOICE
11:21:09 966  	  WHERE
11:21:09 967  	    INVOICE.ID = in_invoice_id;
11:21:09 968  	  EXCEPTION
11:21:09 969  	    WHEN NO_DATA_FOUND THEN
11:21:09 970  	      RAISE BAD_INVOICE_ID;
11:21:09 971  	END;
11:21:09 972  
11:21:09 973  	SELECT
11:21:09 974  	  SUM (CHARGE.CHARGE_AMOUNT) into var_total_charges_amount
11:21:09 975  	FROM
11:21:09 976  	  CHARGE
11:21:09 977  	WHERE
11:21:09 978  	  CHARGE.INVOICE_ID = in_invoice_id
11:21:09 979  	  AND CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V18.CHARGE_PROCESSED
11:21:09 980  	  AND CHARGE.INSTRUMENT_TYPE_ID != GLOBAL_ENUMS_V18.INSTRUMENT_GIFT_CERTIFICATE;
11:21:09 981  
11:21:09 982  	-- FIXME: Maybe whe should throw exception here?
11:21:09 983  	IF var_total_charges_amount < in_chargeback_amount THEN
11:21:09 984  	  var_charge_amount_to_apply := var_total_charges_amount;
11:21:09 985  	ELSE
11:21:09 986  	  var_charge_amount_to_apply := in_chargeback_amount;
11:21:09 987  	END IF;
11:21:09 988  
11:21:09 989  	BEGIN
11:21:09 990  	  PROCS_TRANSACTION_V18.CREATE_TRANSACTION(
11:21:09 991  	    in_transaction_id	      => NULL,
11:21:09 992  	    in_status_id	      => GLOBAL_STATUSES_V18.TRANSACTION_PREPARE,
11:21:09 993  	    in_amount		      => -var_charge_amount_to_apply,
11:21:09 994  	    in_created_by	      => in_created_by,
11:21:09 995  	    in_order_id 	      => NULL,
11:21:09 996  	    in_is_refund	      => GLOBAL_CONSTANTS_V18.TRUE,
11:21:09 997  	    in_transaction_type_code  => 'REFUND',
11:21:09 998  	    out_transaction_id	      => var_transaction_id
11:21:09 999  	  );
11:21:09 1000  	   EXCEPTION
11:21:09 1001  	     WHEN OTHERS THEN
11:21:09 1002  	       EXCEPTION_MESSAGE := SQLERRM;
11:21:09 1003  	       RAISE CAN_NOT_CREATE_TRANSACTION;
11:21:09 1004  	 END;
11:21:09 1005  
11:21:09 1006  	 BEGIN
11:21:09 1007  	   SELECT
11:21:09 1008  	     C.INSTRUMENT_TYPE_ID,
11:21:09 1009  	     C.INSTRUMENT_ID
11:21:09 1010  	     into
11:21:09 1011  	     var_instrument_type_id,
11:21:09 1012  	     var_instrument_id
11:21:09 1013  	   FROM
11:21:09 1014  	     CHARGE C,
11:21:09 1015  	     TRANSACTION_ATTEMPT TA,
11:21:09 1016  	     TRANSACTION T
11:21:09 1017  	   WHERE
11:21:09 1018  	     C.INVOICE_ID = in_invoice_id and
11:21:09 1019  	     C.TRANSACTION_ID = T.ID and
11:21:09 1020  	     TA.TRANSACTION_ID = T.ID and
11:21:09 1021  	     TA.TRANSACTION_ATTEMPT_STATUS_ID = GLOBAL_STATUSES_V18.TRANS_ATTEMPT_SUCCESS and
11:21:09 1022  	     T.IS_REFUND = GLOBAL_CONSTANTS_V18.FALSE and
11:21:09 1023  	     T.TRANSACTION_AMOUNT >= 0 and
11:21:09 1024  	     rownum < 2;
11:21:09 1025  	 EXCEPTION
11:21:09 1026  	     WHEN NO_DATA_FOUND THEN
11:21:09 1027  	       RAISE CAN_NOT_FIND_INSTRUMENT;
11:21:09 1028  	 END;
11:21:09 1029  	 BEGIN
11:21:09 1030  	   PROCS_CHARGE_V18.CREATE_CHARGE(
11:21:09 1031  	     in_invoice_id	   => in_invoice_id,
11:21:09 1032  	     in_transaction_id	   => var_transaction_id,
11:21:09 1033  	     in_instrument_type_id => var_instrument_type_id,
11:21:09 1034  	     in_instrument_id	   => var_instrument_id,
11:21:09 1035  	     in_charge_amount	   => -var_charge_amount_to_apply,
11:21:09 1036  	     in_created_by	   => in_created_by,
11:21:09 1037  	     in_charge_status_id   => GLOBAL_STATUSES_V18.CHARGE_OPENED,
11:21:09 1038  	     out_charge_id	   => var_charge_id
11:21:09 1039  	   );
11:21:09 1040  	   EXCEPTION
11:21:09 1041  	     WHEN OTHERS THEN
11:21:09 1042  	       EXCEPTION_MESSAGE := SQLERRM;
11:21:09 1043  	       RAISE CAN_NOT_CREATE_CHARGE;
11:21:09 1044  	 END;
11:21:09 1045  
11:21:09 1046  	 out_charge_id := var_charge_id;
11:21:09 1047  
11:21:09 1048  EXCEPTION
11:21:09 1049  WHEN BAD_INVOICE_ID THEN
11:21:09 1050  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 1051  	   SPROC_NAME, 'No such invoice');
11:21:09 1052  WHEN CAN_NOT_CREATE_TRANSACTION THEN
11:21:09 1053  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 1054  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
11:21:09 1055  WHEN CAN_NOT_FIND_INSTRUMENT THEN
11:21:09 1056  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 1057  	   SPROC_NAME, 'Could not find financial instrument');
11:21:09 1058  WHEN CAN_NOT_CREATE_CHARGE THEN
11:21:09 1059  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 1060  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
11:21:09 1061  WHEN OTHERS THEN
11:21:09 1062  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:09 1063  	   SPROC_NAME, 'Unknown error', SQLERRM);
11:21:09 1064  END APPLY_REFUND;
