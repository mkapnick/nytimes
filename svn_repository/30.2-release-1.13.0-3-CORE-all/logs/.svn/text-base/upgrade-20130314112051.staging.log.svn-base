11:21:08 763  
11:21:08 764  PROCEDURE GET_SUBSCRIPTION_INVOICES (
11:21:08 765  /*
11:21:08 766  Throws exceptions:
11:21:08 767  APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:08 768  APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR
11:21:08 769  */
11:21:08 770  	in_subscription_id  IN	NUMBER,
11:21:08 771  	out_result_set	    OUT SYS_REFCURSOR
11:21:08 772  ) AS
11:21:08 773  SPROC_NAME	   CONSTANT VARCHAR2(25) := 'GET_SUBSCRIPTION_INVOICES';
11:21:08 774  temp_subscription_id NUMBER;
11:21:08 775  BEGIN
11:21:08 776  	-- Note: A subscription has one or more associated licenses, each of which has an associated invoice.
11:21:08 777  	-- Find associated LICENSES for the subscription by "LICENSE.subscription_id"
11:21:08 778  	--   for each license
11:21:08 779  	--     get associated invoice
11:21:08 780  	--     add to results list
11:21:08 781  	--   end loop
11:21:08 782  	-- end
11:21:08 783  
11:21:08 784  	SELECT
11:21:08 785  	  SUBSCRIPTION.ID into temp_subscription_id
11:21:08 786  	FROM
11:21:08 787  	  SUBSCRIPTION
11:21:08 788  	WHERE
11:21:08 789  	  SUBSCRIPTION.ID = in_subscription_id;
11:21:08 790  
11:21:08 791  	OPEN out_result_set FOR
11:21:08 792  	SELECT
11:21:08 793  	  INVOICE.ID,
11:21:08 794  	  INVOICE.INVOICE_STATUS_ID,
11:21:08 795  	  INVOICE.CREATE_DATE,
11:21:08 796  	  INVOICE.CREATED_BY,
11:21:08 797  	  INVOICE.UPDATE_DATE,
11:21:08 798  	  INVOICE.UPDATED_BY,
11:21:08 799  	  INVOICE.TAX_EXEMPT_ID
11:21:08 800  	FROM
11:21:08 801  	  LICENSE
11:21:08 802  	    INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
11:21:08 803  	WHERE
11:21:08 804  	  LICENSE.SUBSCRIPTION_ID = in_subscription_id;
11:21:08 805  
11:21:08 806  EXCEPTION
11:21:08 807  WHEN NO_DATA_FOUND THEN
11:21:08 808  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:08 809  	  SPROC_NAME, 'Could not find subscription with given ID');
11:21:08 810  WHEN OTHERS THEN
11:21:08 811  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:08 812  	  SPROC_NAME, 'Unknown error', SQLERRM);
11:21:08 813  END GET_SUBSCRIPTION_INVOICES;
