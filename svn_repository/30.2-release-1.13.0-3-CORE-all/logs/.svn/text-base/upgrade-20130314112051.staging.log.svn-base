11:21:09 589  
11:21:09 590  PROCEDURE UPDATE_INVOICE_STATUS (
11:21:09 591  /*
11:21:09 592  Throws exceptions:
11:21:09 593  APP_EXCEPTION_CODES_V18.NOT_FOUND
11:21:09 594  APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR
11:21:09 595  */
11:21:09 596  	in_invoice_id		       IN NUMBER,
11:21:09 597  	in_invoice_status_id	       IN NUMBER,
11:21:09 598  	in_updated_by		       IN VARCHAR2
11:21:09 599  ) AS
11:21:09 600  SPROC_NAME CONSTANT VARCHAR2(21) := 'UPDATE_INVOICE_STATUS';
11:21:09 601  -- VARIABLES
