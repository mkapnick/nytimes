11:21:07 712  	  SPROC_NAME, 'No such offer chain');
11:21:07 713  WHEN OTHERS THEN
11:21:07 714  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:07 715  	  SPROC_NAME, 'Unknown error', SQLERRM);
11:21:07 716  END GET_FIRST_OFFER_INDEX;
11:21:07 717  
11:21:07 718  /******************************************************************************/
11:21:07 719  
11:21:07 720  FUNCTION GET_NEXT_OFFER_INDEX (
11:21:07 721  /*
11:21:07 722  NULL, if not exists
11:21:07 723  */
11:21:07 724  	in_offer_chain_id	     IN NUMBER,
11:21:07 725  	in_offer_chain_current_index IN NUMBER
11:21:07 726  ) RETURN NUMBER AS
11:21:07 727  -- VARIABLES
11:21:07 728  SPROC_NAME		     CONSTANT VARCHAR2(20) := 'GET_NEXT_OFFER_INDEX';
11:21:07 729  temp_offer_chain_id	     NUMBER;
11:21:07 730  temp_offer_chain_current_index NUMBER;
11:21:07 731  var_result		     NUMBER;
11:21:07 732  -- EXCEPTIONS
11:21:07 733  BAD_OFFER_CHAIN_ID      EXCEPTION;
11:21:07 734  BAD_CURRENT_INDEX_VALUE EXCEPTION;
11:21:07 735  BEGIN
11:21:07 736  
11:21:07 737  	-- Check that offer chain exists
11:21:07 738  	BEGIN
11:21:07 739  	  SELECT
11:21:07 740  	    OFFER_CHAIN.ID into temp_offer_chain_id
11:21:07 741  	  FROM
11:21:07 742  	    OFFER_CHAIN
11:21:07 743  	  WHERE
11:21:07 744  	    OFFER_CHAIN.ID = in_offer_chain_id;
11:21:07 745  	  EXCEPTION
11:21:07 746  	    WHEN NO_DATA_FOUND THEN
11:21:07 747  	      RAISE BAD_OFFER_CHAIN_ID;
11:21:07 748  	END;
11:21:07 749  
11:21:07 750  	-- Check that current offer index exists
11:21:07 751  	BEGIN
11:21:07 752  	  SELECT
11:21:07 753  	    OFFER_OFFER_CHAIN.INDEX_VALUE into temp_offer_chain_current_index
11:21:07 754  	  FROM
11:21:07 755  	    OFFER_OFFER_CHAIN
11:21:07 756  	  WHERE
11:21:07 757  	    OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
11:21:07 758  	    AND OFFER_OFFER_CHAIN.INDEX_VALUE = in_offer_chain_current_index
11:21:07 759  	    -- TODO: delete next line
11:21:07 760  	    AND ROWNUM <= 1;
11:21:07 761  	  EXCEPTION
11:21:07 762  	    WHEN NO_DATA_FOUND THEN
11:21:07 763  	      RAISE BAD_CURRENT_INDEX_VALUE;
11:21:07 764  	END;
11:21:07 765  
11:21:07 766  	-- Get next offer index
11:21:07 767  	BEGIN
11:21:07 768  	  SELECT
11:21:07 769  	    INDEX_VALUE into var_result
11:21:07 770  	  FROM (
11:21:07 771  	    SELECT
11:21:07 772  	      OFFER_OFFER_CHAIN.INDEX_VALUE
11:21:07 773  	    FROM
11:21:07 774  	      OFFER_OFFER_CHAIN
11:21:07 775  	    WHERE
11:21:07 776  	      OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
11:21:07 777  	      AND OFFER_OFFER_CHAIN.INDEX_VALUE > in_offer_chain_current_index
11:21:07 778  	    ORDER BY
11:21:07 779  	      OFFER_OFFER_CHAIN.INDEX_VALUE ASC
11:21:07 780  	  )
11:21:07 781  	  WHERE
11:21:07 782  	    ROWNUM <= 1;
11:21:07 783  	  EXCEPTION
11:21:07 784  	    WHEN NO_DATA_FOUND THEN
11:21:07 785  	      var_result := NULL;
11:21:07 786  	END;
11:21:07 787  
11:21:07 788  	RETURN var_result;
11:21:07 789  
11:21:07 790  EXCEPTION
11:21:07 791  WHEN BAD_OFFER_CHAIN_ID THEN
11:21:07 792  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:07 793  	  SPROC_NAME, 'No such offer chain');
11:21:07 794  WHEN BAD_CURRENT_INDEX_VALUE THEN
11:21:07 795  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:07 796  	  SPROC_NAME, 'Bad current index value');
11:21:07 797  WHEN OTHERS THEN
11:21:07 798  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:07 799  	  SPROC_NAME, 'Unknown error', SQLERRM);
11:21:07 800  END GET_NEXT_OFFER_INDEX;
