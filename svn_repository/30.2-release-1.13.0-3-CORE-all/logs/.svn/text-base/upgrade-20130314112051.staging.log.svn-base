11:21:08 2114  PROCEDURE CANCEL_SUBSCRIPTION_INVOICE (
11:21:08 2115  /*
11:21:08 2116  APP_EXCEPTION_CODES_V18.NOT_FOUND
11:21:08 2117  APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR
11:21:08 2118  APP_EXCEPTION_CODES_V18.INTERNAL_ERROR
11:21:08 2119  */
11:21:08 2120  	 in_invoice_id	      IN NUMBER,
11:21:08 2121  	 in_updated_by	      IN VARCHAR2,
11:21:08 2122  	 in_refundable	      IN NUMBER DEFAULT GLOBAL_CONSTANTS_V18.FALSE
11:21:08 2123  ) AS
11:21:08 2124  SPROC_NAME CONSTANT VARCHAR2(27) := 'CANCEL_SUBSCRIPTION_INVOICE';
11:21:08 2125  -- VARIABLES
11:21:08 2126  temp_invoice_id	      INVOICE.ID%TYPE;
11:21:08 2127  var_chargeback_amount  NUMBER(10,2);
11:21:08 2128  ver_refund_charge_id   NUMBER;
11:21:08 2129  -- EXCEPTIONS
11:21:08 2130  BAD_INVOICE_ID		    EXCEPTION;
11:21:08 2131  CAN_NOT_CALCULATE_CHARGEBACK EXCEPTION;
11:21:08 2132  CAN_NOT_APPLY_CHARGEBACK     EXCEPTION;
11:21:08 2133  EXCEPTION_MESSAGE	    VARCHAR2(1024);
11:21:08 2134  -- STUB
11:21:08 2135  var_now DATE;
11:21:08 2136  var_revoke NUMBER;
11:21:08 2137  var_refund NUMBER;
11:21:08 2138  var_billed NUMBER;
11:21:08 2139  var_subscription_in_grace NUMBER;
11:21:08 2140  BEGIN
11:21:08 2141  
11:21:08 2142  	 var_now := sysdate;
11:21:08 2143  
11:21:08 2144  	 -- Check that invoice exists
11:21:08 2145  	 BEGIN
11:21:08 2146  	   SELECT
11:21:08 2147  	     INVOICE.ID into temp_invoice_id
11:21:08 2148  	   FROM
11:21:08 2149  	     INVOICE
11:21:08 2150  	   WHERE
11:21:08 2151  	     INVOICE.ID = in_invoice_id;
11:21:08 2152  	   EXCEPTION
11:21:08 2153  	     WHEN NO_DATA_FOUND THEN
11:21:08 2154  	       RAISE BAD_INVOICE_ID;
11:21:08 2155  	 END;
11:21:08 2156  
11:21:08 2157  	 select decode(count(1), 0, GLOBAL_CONSTANTS_V18.FALSE, GLOBAL_CONSTANTS_V18.TRUE) into var_revoke
11:21:08 2158  	 from license l, subscription s, offer_chain oc
11:21:08 2159  	 where
11:21:08 2160  	   l.subscription_id = s.id and
11:21:08 2161  	   s.offer_chain_id = oc.id and
11:21:08 2162  	   l.invoice_id = in_invoice_id and
11:21:08 2163  	   oc.revoke_entitlements = GLOBAL_CONSTANTS_V18.TRUE and
11:21:08 2164  	   rownum < 2;
11:21:08 2165  
11:21:08 2166  	 select
11:21:08 2167  	   decode(s.subscription_status_id, GLOBAL_STATUSES_V18.SUBSCRIPTION_IN_GRACE_PERIOD, 1, 0)
11:21:08 2168  	 into
11:21:08 2169  	   var_subscription_in_grace
11:21:08 2170  	 from license l, subscription s
11:21:08 2171  	 where
11:21:08 2172  	   l.subscription_id = s.id and
11:21:08 2173  	   l.invoice_id = in_invoice_id and
11:21:08 2174  	   rownum < 2;
11:21:08 2175  
11:21:08 2176  	 var_billed := PROCS_INVOICE_V18.IS_INVOICE_PAYING_STARTED(in_invoice_id);
11:21:08 2177  	 var_refund := GLOBAL_CONSTANTS_V18.FALSE;
11:21:08 2178  
11:21:08 2179  	 -- Check that transaction for given invoice not started
11:21:08 2180  	 -- if refund enabled calculate and apply chargeback
11:21:08 2181  	 IF (
11:21:08 2182  	     var_billed = GLOBAL_CONSTANTS_V18.TRUE
11:21:08 2183  	   )THEN
11:21:08 2184  	   if (in_refundable = GLOBAL_CONSTANTS_V18.TRUE) then
11:21:08 2185  	     -- If started then we need to calculate refund
11:21:08 2186  	     BEGIN
11:21:08 2187  	       PROCS_INVOICE_V18.CALCULATE_INVOICE_CHARGEBACK(
11:21:08 2188  		 in_invoice_id,
11:21:08 2189  		 var_now,
11:21:08 2190  		 var_chargeback_amount
11:21:08 2191  	       );
11:21:08 2192  	       EXCEPTION
11:21:08 2193  		 WHEN OTHERS THEN
11:21:08 2194  		   EXCEPTION_MESSAGE := SQLERRM;
11:21:08 2195  		   RAISE CAN_NOT_CALCULATE_CHARGEBACK;
11:21:08 2196  	     END;
11:21:08 2197  	     IF var_chargeback_amount > 0 THEN
11:21:08 2198  	       BEGIN
11:21:08 2199  		 PROCS_INVOICE_V18.APPLY_REFUND(
11:21:08 2200  		   in_invoice_id,
11:21:08 2201  		   var_chargeback_amount,
11:21:08 2202  		   in_updated_by,
11:21:08 2203  		   ver_refund_charge_id
11:21:08 2204  		 );
11:21:08 2205  		 EXCEPTION
