11:21:08 517  
11:21:08 518  PROCEDURE SUSPEND_SUBSCRIPTION(
11:21:08 519  /*
11:21:08 520  Throws exceptions:
11:21:08 521  APP_EXCEPTION_CODES_V18.NOT_FOUND
11:21:08 522  APP_EXCEPTION_CODES_V18.INTERNAL_ERROR
11:21:08 523  APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR
11:21:08 524  */
11:21:08 525  	  in_subs_id	IN NUMBER,
11:21:08 526  	  in_updated_by IN VARCHAR2
11:21:08 527  ) AS
11:21:08 528  SPROC_NAME		  CONSTANT VARCHAR2(20) := 'SUSPEND_SUBSCRIPTION';
11:21:08 529  var_subscription_status_id  NUMBER;
11:21:08 530  var_license_id		  NUMBER;
11:21:08 531  var_offer_id		  NUMBER;
11:21:08 532  var_license_start_date	  DATE;
11:21:08 533  var_license_end_date	  DATE;
11:21:08 534  
11:21:08 535  var_entitlement_dupration   VARCHAR2(30);
11:21:08 536  var_d_entitlement_dupration NUMBER;
11:21:08 537  
11:21:08 538  var_ym_interval INTERVAL YEAR TO MONTH;
11:21:08 539  var_ds_interval INTERVAL DAY(3) TO SECOND;
11:21:08 540  
11:21:08 541  -- EXCEPTIONS
11:21:08 542  BAD_SUBSCRIPTION_ID     EXCEPTION;
11:21:08 543  BAD_SUBSCRIPTION_STATUS EXCEPTION;
11:21:08 544  NO_LICENSE_FOUND	      EXCEPTION;
11:21:08 545  NO_OFFER_FOUND	      EXCEPTION;
11:21:08 546  EXCEPTION_MESSAGE       VARCHAR2(1024);
11:21:08 547  BEGIN
11:21:08 548  	-- TODO: Finish this prcedure (in Phase II)
11:21:08 549  
11:21:08 550  	-- Get subscription by id. FAULT if no such subscription.
11:21:08 551  	-- begin TX
11:21:08 552  	--   Get for update associated license (subscription.license_id). FAULT if not found.
11:21:08 553  	--   Set status to PROCESSED.
11:21:08 554  	--   updated record.
11:21:08 555  	--   compute days remaining in the subscription: original end_date - today = days_remaining_adjustment
11:21:08 556  	--   new subscription status is SUSPENDED.
11:21:08 557  	--   suspend_date is now.
11:21:08 558  	--   update subscription record.
11:21:08 559  	-- end TX
11:21:08 560  
11:21:08 561  	BEGIN
11:21:08 562  	  SELECT
11:21:08 563  	    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID into var_subscription_status_id
11:21:08 564  	  FROM
11:21:08 565  	    SUBSCRIPTION
11:21:08 566  	  WHERE
11:21:08 567  	    SUBSCRIPTION.ID = in_subs_id;
11:21:08 568  	  EXCEPTION
11:21:08 569  	  WHEN NO_DATA_FOUND THEN
11:21:08 570  	    RAISE BAD_SUBSCRIPTION_ID;
11:21:08 571  	END;
11:21:08 572  
11:21:08 573  	IF var_subscription_status_id != GLOBAL_STATUSES_V18.SUBSCRIPTION_ACTIVE THEN
11:21:08 574  	  RAISE BAD_SUBSCRIPTION_STATUS;
11:21:08 575  	END IF;
11:21:08 576  
11:21:08 577  	BEGIN
11:21:08 578  	  SELECT
11:21:08 579  	    LICENSE.ID,
11:21:08 580  	    LICENSE.OFFER_ID,
11:21:08 581  	    LICENSE.START_DATE
11:21:08 582  	    into
11:21:08 583  	    var_license_id,
11:21:08 584  	    var_offer_id,
11:21:08 585  	    var_license_start_date
11:21:08 586  	  FROM
11:21:08 587  	    LICENSE
11:21:08 588  	  WHERE
11:21:08 589  	    LICENSE.SUBSCRIPTION_ID = in_subs_id
11:21:08 590  	      AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V18.LICENSE_ACTIVE;
11:21:08 591  	  EXCEPTION
11:21:08 592  	  WHEN NO_DATA_FOUND THEN
11:21:08 593  	    RAISE NO_LICENSE_FOUND;
11:21:08 594  	END;
11:21:08 595  
11:21:08 596  	PROCS_LICENSE_CRU_V18.UPDATE_LICENSE(
11:21:08 597  	  in_license_id        => var_license_id,
11:21:08 598  	  in_updated_by        => in_updated_by,
11:21:08 599  	  in_license_status_id => GLOBAL_STATUSES_V18.LICENSE_CLOSED
11:21:08 600  	);
11:21:08 601  
11:21:08 602  	BEGIN
11:21:08 603  	  SELECT
11:21:08 604  	    OFFER.ENTITLEMENT_DURATION into var_entitlement_dupration
11:21:08 605  	  FROM
11:21:08 606  	    OFFER
11:21:08 607  	  WHERE
11:21:08 608  	    OFFER.ID = var_offer_id;
11:21:08 609  	  EXCEPTION
11:21:08 610  	  WHEN NO_DATA_FOUND THEN
11:21:08 611  	    RAISE NO_OFFER_FOUND;
11:21:08 612  	END;
11:21:08 613  
11:21:08 614  	var_ym_interval := substr(var_entitlement_dupration, 0, 4);
11:21:08 615  	var_ds_interval := substr(var_entitlement_dupration, 4);
11:21:08 616  
11:21:08 617  	var_license_end_date := var_license_start_date + var_ym_interval + var_ds_interval;
11:21:08 618  
11:21:08 619  	var_d_entitlement_dupration := var_license_end_date - current_date;
11:21:08 620  
11:21:08 621  	PROCS_SUBSCRIPTION_CRU_V18.UPDATE_SUBSCRIPTION(
11:21:08 622  	  in_subscription_id	       => in_subs_id,
11:21:08 623  	  in_updated_by 	       => in_updated_by,
11:21:08 624  	  in_suspend_date	       => SYSDATE,
11:21:08 625  	  in_subscription_status_id    => GLOBAL_STATUSES_V18.SUBSCRIPTION_SUSPENDED,
11:21:08 626  	  in_days_remainning_ajustment => var_d_entitlement_dupration
11:21:08 627  	);
11:21:08 628  
11:21:08 629  EXCEPTION
11:21:08 630  WHEN BAD_SUBSCRIPTION_ID THEN
11:21:08 631  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:08 632  	  SPROC_NAME, 'No such subscription');
11:21:08 633  WHEN BAD_SUBSCRIPTION_STATUS THEN
11:21:08 634  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.STATE_ERROR,
11:21:08 635  	  SPROC_NAME, 'Subscription is not active');
11:21:08 636  WHEN NO_LICENSE_FOUND THEN
11:21:08 637  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:08 638  	  SPROC_NAME, 'Can not find license associated with given subscription ID');
11:21:08 639  WHEN NO_OFFER_FOUND THEN
11:21:08 640  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:08 641  	  SPROC_NAME, 'Can not find offer associated with given subscription ID');
11:21:08 642  WHEN OTHERS THEN
11:21:08 643  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:08 644  	  SPROC_NAME, 'Unknown error', SQLERRM);
11:21:08 645  END SUSPEND_SUBSCRIPTION;
