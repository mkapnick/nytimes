11:21:09 2006  	 -- update account information
11:21:09 2007  	 PROCS_ACCOUNT_CRU_V18.UPDATE_ACCOUNT(
11:21:09 2008  	   in_account_id	 => var_account_id,
11:21:09 2009  	   in_updated_by	 => in_updated_by,
11:21:09 2010  	   in_instrument_type_id => in_instrument_type_id,
11:21:09 2011  	   in_instrument_id	 => in_instrument_id
11:21:09 2012  	 );
11:21:09 2013  
11:21:09 2014  EXCEPTION
11:21:09 2015  WHEN BAD_GROUP_ID THEN
11:21:09 2016  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 2017  	   SPROC_NAME, 'No such account');
