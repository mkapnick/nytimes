11:21:09  82  	in_end		 IN  NUMBER,
11:21:09  83  	in_expired	 IN  NUMBER,
11:21:09  84  	out_result_set	 OUT SYS_REFCURSOR,
11:21:09  85  	out_shares_count OUT NUMBER
11:21:09  86  ) AS
11:21:09  87  SPROC_NAME CONSTANT VARCHAR2(32) := 'SUB_SHARE_BY_GROUP_ID';
11:21:09  88  range_diff NUMBER := 0;
11:21:09  89  upper_bond_diff NUMBER := 0;
11:21:09  90  l_start NUMBER := 0;
11:21:09  91  l_end   NUMBER := 0;
11:21:09  92  BEGIN
11:21:09  93  	--Normalize the end points [START]
11:21:09  94  	IF (in_start IS NULL OR in_start < 0) Then
11:21:09  95  	  l_start := 0;
11:21:09  96  	ELSE
11:21:09  97  	  l_start := in_start;
11:21:09  98  	END IF;
11:21:09  99  
11:21:09 100  	IF (in_end IS NULL) Then
11:21:09 101  	  l_end := 11;
11:21:09 102  	ELSE
11:21:09 103  	  l_end := in_end;
11:21:09 104  	END IF;
11:21:09 105  
11:21:09 106  	l_start := l_start + 1;
11:21:09 107  	l_end	:= l_end   + 1;
11:21:09 108  
11:21:09 109  	range_diff := l_end - l_start;
11:21:09 110  	upper_bond_diff :=  range_diff - GLOBAL_CONSTANTS_V18.MAX_RETURN_COUNT;
11:21:09 111  
11:21:09 112  	IF (upper_bond_diff > 0) Then
11:21:09 113  	  l_end := l_end - upper_bond_diff;
11:21:09 114  	END IF;
11:21:09 115  	--Normalize the end points [END]
11:21:09 116  
11:21:09 117  	BEGIN
11:21:09 118  	  SELECT
11:21:09 119  	    COUNT(1) INTO out_shares_count
11:21:09 120  	  FROM
11:21:09 121  	    GROUP_ACCOUNT ga,
11:21:09 122  	    SUBSCRIPTION_SHARE ss,
11:21:09 123  	    ACCOUNT a
11:21:09 124  	  WHERE
11:21:09 125  	    a.GROUP_ID		= in_group_id AND
11:21:09 126  	    a.Id		= ss.borrower_account_id And
11:21:09 127  	    ss.GROUP_ACCOUNT_ID = ga.ID;
11:21:09 128  	END;
11:21:09 129  
11:21:09 130  	IF in_expired > 0 THEN
11:21:09 131  	BEGIN
11:21:09 132  	    OPEN out_result_set FOR
11:21:09 133  	    SELECT
11:21:09 134  	      *
11:21:09 135  	    FROM
11:21:09 136  	      (SELECT rownum rnum, q.*
11:21:09 137  	       FROM
11:21:09 138  		(SELECT
11:21:09 139  		   ga.SUBSCRIPTION_ID,
11:21:09 140  		   ss.START_DATE,
11:21:09 141  		   ss.END_DATE,
11:21:09 142  		   a2.GROUP_ID AS PARENT_GROUP_ID
11:21:09 143  		 FROM
11:21:09 144  		   GROUP_ACCOUNT ga,
11:21:09 145  		   SUBSCRIPTION_SHARE ss,
11:21:09 146  		   ACCOUNT a,
11:21:09 147  		   SUBSCRIPTION s,
11:21:09 148  		   ACCOUNT a2
11:21:09 149  		 WHERE
11:21:09 150  		   a.GROUP_ID	       = in_group_id AND
11:21:09 151  		   a.ID 	       = ss.BORROWER_ACCOUNT_ID AND
11:21:09 152  		   ss.GROUP_ACCOUNT_ID = ga.ID	AND
11:21:09 153  		   ga.SUBSCRIPTION_ID  = s.ID AND
11:21:09 154  		   s.ACCOUNT_ID        = a2.ID
11:21:09 155  		) Q
11:21:09 156  	      WHERE rownum <= l_end)
11:21:09 157  	    WHERE rnum >= l_Start;
11:21:09 158  	END;
11:21:09 159  	ELSE
11:21:09 160  	BEGIN
11:21:09 161  	    OPEN out_result_set FOR
11:21:09 162  	    SELECT
11:21:09 163  	      *
11:21:09 164  	    FROM
11:21:09 165  	      (SELECT rownum rnum, q.*
11:21:09 166  	       FROM
11:21:09 167  		(SELECT
11:21:09 168  		   ga.SUBSCRIPTION_ID,
11:21:09 169  		   ss.START_DATE,
11:21:09 170  		   ss.END_DATE,
11:21:09 171  		   a2.GROUP_ID AS PARENT_GROUP_ID
11:21:09 172  		 FROM
11:21:09 173  		   GROUP_ACCOUNT ga,
11:21:09 174  		   SUBSCRIPTION_SHARE ss,
11:21:09 175  		   ACCOUNT a,
11:21:09 176  		   SUBSCRIPTION s,
11:21:09 177  		   ACCOUNT a2
11:21:09 178  		 WHERE
11:21:09 179  		   a.GROUP_ID	       = in_group_id AND
11:21:09 180  		   a.ID 	       = ss.BORROWER_ACCOUNT_ID AND
11:21:09 181  		   SYSDATE BETWEEN START_DATE AND END_DATE AND
11:21:09 182  		   ss.GROUP_ACCOUNT_ID = ga.ID AND
11:21:09 183  		   ga.SUBSCRIPTION_ID  = s.ID  AND
11:21:09 184  		   s.ACCOUNT_ID        = a2.ID
11:21:09 185  		) Q
11:21:09 186  	      WHERE rownum <= l_end)
11:21:09 187  	    WHERE rnum >= l_start;
11:21:09 188  	  END;
11:21:09 189  	END IF;
11:21:09 190  EXCEPTION
11:21:09 191  WHEN NO_DATA_FOUND THEN
11:21:09 192  	NULL;
11:21:09 193  WHEN OTHERS THEN
11:21:09 194  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:09 195  	  SPROC_NAME, 'Unknow error while retrieving subscription share info by group id', SQLERRM);
11:21:09 196  END SUB_SHARE_BY_GROUP_ID;
