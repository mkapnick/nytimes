11:21:09 1989  	     WHEN NO_DATA_FOUND THEN
11:21:09 1990  	       RAISE BAD_GROUP_ID;
11:21:09 1991  	 END;
11:21:09 1992  
11:21:09 1993  	 -- Chech that given instrument exists
11:21:09 1994  	 IF in_instrument_type_id = GLOBAL_ENUMS_V18.INSTRUMENT_CREDIT_CARD THEN
11:21:09 1995  	   IF IS_CREDIT_CARD_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V18.FALSE THEN
11:21:09 1996  	     RAISE BAD_CREDIT_CARD;
11:21:09 1997  	   END IF;
11:21:09 1998  	 ELSIF in_instrument_type_id = GLOBAL_ENUMS_V18.INSTRUMENT_PAYPAL THEN
