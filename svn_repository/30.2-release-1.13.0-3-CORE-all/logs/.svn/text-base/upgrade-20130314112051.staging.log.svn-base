11:21:09 712  	in_recipient_notify_date  IN DATE,
11:21:09 713  	in_sender_name		  IN VARCHAR2,
11:21:09 714  	in_sender_email 	  IN VARCHAR2,
11:21:09 715  	in_gift_message 	  IN  VARCHAR2,
11:21:09 716  	in_expiration_date	  IN DATE,
11:21:09 717  	out_gift_certificate_id   OUT NUMBER,
11:21:09 718  	out_invoice_id		  OUT NUMBER
11:21:09 719  ) AS
11:21:09 720  SPROC_NAME CONSTANT VARCHAR2(19) := 'START_GC_PURCHASING';
11:21:09 721  -- VARIABLES
11:21:09 722  var_account_id		NUMBER;
11:21:09 723  temp_gc_code		GIFT_CERTIFICATE.CODE%TYPE;
11:21:09 724  var_och_is_gc		NUMBER;
11:21:09 725  var_offer_chain_status_id NUMBER;
11:21:09 726  var_is_for_redemption	NUMBER;
11:21:09 727  var_new_invoice_id	NUMBER;
11:21:09 728  var_gift_cert_id		NUMBER;
11:21:09 729  var_account_tax_exempt_id VARCHAR2(255);
11:21:09 730  -- EXCEPTIONS
11:21:09 731  BAD_GROUP_ID		    EXCEPTION;
11:21:09 732  GC_CODE_ALREADY_EXISTS	    EXCEPTION;
11:21:09 733  BAD_OFFER_CHAIN_ID	    EXCEPTION;
11:21:09 734  OCH_IS_NOT_GIFT_CERTIFICATE   EXCEPTION;
11:21:09 735  BAD_OFFER_CHAIN_STATUS	    EXCEPTION;
11:21:09 736  CAN_NOT_PURCHASE_GC_FOR_RDMPN EXCEPTION;
11:21:09 737  CAN_NOT_CREATE_INVOICE	    EXCEPTION;
11:21:09 738  OFFER_REC_NUM_LESS_THAN_ONE   EXCEPTION;
11:21:09 739  CAN_NOT_CREATE_LINE_ITEMS     EXCEPTION;
11:21:09 740  
11:21:09 741  EXCEPTION_MESSAGE VARCHAR2(1024);
11:21:09 742  BEGIN
11:21:09 743  	-- Get account id
11:21:09 744  	BEGIN
11:21:09 745  	  SELECT
11:21:09 746  	    ACCOUNT.ID,
11:21:09 747  	    ACCOUNT.TAX_EXEMPT_ID
11:21:09 748  	    into
11:21:09 749  	    var_account_id,
11:21:09 750  	    var_account_tax_exempt_id
11:21:09 751  	  FROM
11:21:09 752  	    ACCOUNT
11:21:09 753  	  WHERE
11:21:09 754  	    ACCOUNT.GROUP_ID = in_group_id;
11:21:09 755  	  EXCEPTION
11:21:09 756  	  WHEN NO_DATA_FOUND THEN
11:21:09 757  	    RAISE BAD_GROUP_ID;
11:21:09 758  	END;
11:21:09 759  
11:21:09 760  	-- Check for the same code
11:21:09 761  	BEGIN
11:21:09 762  	  SELECT
11:21:09 763  	    GIFT_CERTIFICATE.CODE into temp_gc_code
11:21:09 764  	  FROM
11:21:09 765  	    GIFT_CERTIFICATE
11:21:09 766  	  WHERE
11:21:09 767  	    GIFT_CERTIFICATE.CODE = in_gift_certificate_code;
11:21:09 768  
11:21:09 769  	  RAISE GC_CODE_ALREADY_EXISTS;
11:21:09 770  
11:21:09 771  	  EXCEPTION
11:21:09 772  	    WHEN NO_DATA_FOUND THEN
11:21:09 773  	      NULL;
11:21:09 774  	END;
11:21:09 775  
11:21:09 776  	-- Get offer chain flag "is_gift_certificate"
11:21:09 777  	BEGIN
11:21:09 778  	  SELECT
11:21:09 779  	    OFFER_CHAIN.IS_GIFT_CERTIFICATE,
11:21:09 780  	    OFFER_CHAIN.OFFER_CHAIN_STATUS_ID
11:21:09 781  	    into
11:21:09 782  	    var_och_is_gc,
11:21:09 783  	    var_offer_chain_status_id
11:21:09 784  	  FROM
11:21:09 785  	    OFFER_CHAIN
11:21:09 786  	  WHERE
11:21:09 787  	    OFFER_CHAIN.ID = in_offer_chain_id;
11:21:09 788  	  EXCEPTION
11:21:09 789  	  WHEN NO_DATA_FOUND THEN
11:21:09 790  	    RAISE BAD_OFFER_CHAIN_ID;
11:21:09 791  	END;
11:21:09 792  
11:21:09 793  	IF var_och_is_gc != GLOBAL_CONSTANTS_V18.TRUE
11:21:09 794  	  OR var_och_is_gc IS NULL THEN
11:21:09 795  	  RAISE OCH_IS_NOT_GIFT_CERTIFICATE;
11:21:09 796  	END IF;
11:21:09 797  
11:21:09 798  	IF var_offer_chain_status_id != GLOBAL_STATUSES_V18.OFFER_CHAIN_ACTIVE THEN
11:21:09 799  	  RAISE BAD_OFFER_CHAIN_STATUS;
11:21:09 800  	END IF;
11:21:09 801  
11:21:09 802  	-- norlov: #38151 check if the OC is for Redemption:
11:21:09 803  	SELECT
11:21:09 804  	  COUNT(*) into var_is_for_redemption
11:21:09 805  	FROM
11:21:09 806  	  OFFER_CHAIN_ELIGIBILITY
11:21:09 807  	WHERE
11:21:09 808  	  OFFER_CHAIN_ELIGIBILITY.OFFER_CHAIN_ID = in_offer_chain_id
11:21:09 809  	  AND OFFER_CHAIN_ELIGIBILITY.NAME = GLOBAL_CONSTANTS_V18.GIFT_CERTIFICATE_REQUIRED
11:21:09 810  	  AND OFFER_CHAIN_ELIGIBILITY.VALUE = GLOBAL_CONSTANTS_V18.ELIGIBILITY_FLAG_SET;
11:21:09 811  
11:21:09 812  	IF var_is_for_redemption > 0 THEN
11:21:09 813  	  RAISE CAN_NOT_PURCHASE_GC_FOR_RDMPN;
11:21:09 814  	END IF;
11:21:09 815  
11:21:09 816  	-- Create new invoice
11:21:09 817  	BEGIN
11:21:09 818  	  PROCS_INVOICE_V18.CREATE_INVOICE(
11:21:09 819  	    in_invoice_status => GLOBAL_STATUSES_V18.INVOICE_OPEN,
11:21:09 820  	    in_created_by     => in_created_by,
11:21:09 821  	    in_tax_exempt_id  => var_account_tax_exempt_id,
11:21:09 822  	    out_invoice_id    => var_new_invoice_id
11:21:09 823  	  );
11:21:09 824  	  EXCEPTION
11:21:09 825  	    WHEN OTHERS THEN
11:21:09 826  	      EXCEPTION_MESSAGE := SQLERRM;
11:21:09 827  	      RAISE CAN_NOT_CREATE_INVOICE;
11:21:09 828  	END;
11:21:09 829  
11:21:09 830  	-- Add line items for new invoice
11:21:09 831  	BEGIN
11:21:09 832  	  FOR f_offer_data IN (
11:21:09 833  	    SELECT
11:21:09 834  	      OFFER_ID,
11:21:09 835  	      NUM_RECURRENCES
11:21:09 836  	    FROM
11:21:09 837  	      OFFER_OFFER_CHAIN
11:21:09 838  	    WHERE
11:21:09 839  	      OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
11:21:09 840  	  )
11:21:09 841  	  LOOP
11:21:09 842  	    IF f_offer_data.NUM_RECURRENCES < 1 THEN
11:21:09 843  	      RAISE OFFER_REC_NUM_LESS_THAN_ONE;
11:21:09 844  	    END IF;
11:21:09 845  	    FOR i_offer_recurrences_iterator IN 1..f_offer_data.NUM_RECURRENCES
11:21:09 846  	    LOOP
11:21:09 847  	      PROCS_LINE_ITEMS_V18.ADD_LINE_ITEMS(
11:21:09 848  		in_invoice_id => var_new_invoice_id,
11:21:09 849  		in_offer_id   => f_offer_data.OFFER_ID,
11:21:09 850  		in_created_by => in_created_by
11:21:09 851  	      );
11:21:09 852  	    END LOOP;
11:21:09 853  	  END LOOP;
11:21:09 854  
11:21:09 855  	  EXCEPTION
11:21:09 856  	    WHEN OTHERS THEN
11:21:09 857  	      EXCEPTION_MESSAGE := SQLERRM;
11:21:09 858  	      RAISE CAN_NOT_CREATE_LINE_ITEMS;
11:21:09 859  	END;
11:21:09 860  
11:21:09 861  	PROCS_FIN_INSTRUMENTS_CRU_V18.CREATE_GIFT_CERTIFICATE(
11:21:09 862  	  out_gift_certificate_id	=> var_gift_cert_id,
11:21:09 863  	  in_purchaser_group_id 	=> in_group_id,
11:21:09 864  	  in_purchaser_invoice_id	=> var_new_invoice_id,
11:21:09 865  	  in_offer_chain_id		=> in_offer_chain_id,
11:21:09 866  	  in_expiration_date		=> in_expiration_date,
11:21:09 867  	  in_purchase_date		=> SYSDATE,
11:21:09 868  	  in_gift_certificate_status_id => GLOBAL_STATUSES_V18.GIFT_CERTIFICATE_ACTIVE,
11:21:09 869  	  in_code			=> in_gift_certificate_code,
11:21:09 870  	  in_created_by 		=> in_created_by,
11:21:09 871  	  in_recipient_name		=> in_recipient_name,
11:21:09 872  	  in_gift_message		=> in_gift_message,
11:21:09 873  	  in_recipient_email		=> in_recipient_email,
11:21:09 874  	  in_sender_email		=> in_sender_email,
11:21:09 875  	  in_sender_name		=> in_sender_name,
11:21:09 876  	  in_recipient_address_id	=> in_recipient_address_id,
11:21:09 877  	  in_recipient_notify_date	=> in_recipient_notify_date
11:21:09 878  	);
11:21:09 879  
11:21:09 880  	out_gift_certificate_id := var_gift_cert_id;
11:21:09 881  	out_invoice_id := var_new_invoice_id;
11:21:09 882  
11:21:09 883  EXCEPTION
11:21:09 884  WHEN BAD_OFFER_CHAIN_STATUS THEN
11:21:09 885  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.STATE_ERROR,
11:21:09 886  	  SPROC_NAME, 'Offer chain is not active');
11:21:09 887  WHEN GC_CODE_ALREADY_EXISTS THEN
11:21:09 888  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.DUPLICATE_ERROR,
11:21:09 889  	  SPROC_NAME, 'Gift certificate with same code already exists');
11:21:09 890  WHEN OCH_IS_NOT_GIFT_CERTIFICATE THEN
11:21:09 891  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INVALID_PARAMETER,
11:21:09 892  	  SPROC_NAME, 'This offer chain can not be used for gift certificate');
11:21:09 893  WHEN CAN_NOT_PURCHASE_GC_FOR_RDMPN THEN
11:21:09 894  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INVALID_PARAMETER,
11:21:09 895  	  SPROC_NAME, 'This offer chain can not be purchased for gift certificate since it is for redemption');
11:21:09 896  WHEN CAN_NOT_CREATE_INVOICE THEN
11:21:09 897  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 898  	  SPROC_NAME, 'Could not create new invoice', EXCEPTION_MESSAGE);
11:21:09 899  WHEN CAN_NOT_CREATE_LINE_ITEMS THEN
11:21:09 900  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 901  	  SPROC_NAME, 'Could not create line items', EXCEPTION_MESSAGE);
11:21:09 902  WHEN BAD_GROUP_ID THEN
11:21:09 903  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 904  	  SPROC_NAME, 'No such group id');
11:21:09 905  WHEN BAD_OFFER_CHAIN_ID THEN
11:21:09 906  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 907  	  SPROC_NAME, 'No such offer chain');
11:21:09 908  WHEN OFFER_REC_NUM_LESS_THAN_ONE THEN
11:21:09 909  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 910  	  SPROC_NAME, 'Some offer has recurrences number less than 1');
11:21:09 911  WHEN OTHERS THEN
11:21:09 912  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:09 913  	  SPROC_NAME, 'Unknown error', SQLERRM);
11:21:09 914  END START_GC_PURCHASING;
11:21:09 915  
11:21:09 916  /******************************************************************************/
11:21:09 917  
11:21:09 918  PROCEDURE FINALIZE_GC_PURCHASING (
11:21:09 919  	in_invoice_id	      IN NUMBER,
11:21:09 920  	in_created_by	      IN VARCHAR2,
11:21:09 921  	in_instrument_id      IN NUMBER,
11:21:09 922  	in_instrument_type_id IN NUMBER,
11:21:09 923  	in_order_id	      IN VARCHAR2,
11:21:09 924  	in_transaction_id     IN NUMBER,
11:21:09 925  	out_charge_amount     OUT NUMBER
11:21:09 926  ) AS
11:21:09 927  SPROC_NAME CONSTANT VARCHAR2(22) := 'FINALIZE_GC_PURCHASING';
11:21:09 928  -- VARIABLES
11:21:09 929  temp_transaction_id_count NUMBER;
11:21:09 930  var_invoice_amount	NUMBER(10,2);
11:21:09 931  var_transaction_id	NUMBER;
11:21:09 932  var_new_charge_id 	NUMBER;
11:21:09 933  -- EXCEPTIONS
11:21:09 934  BAD_CREDIT_CARD_ID	  EXCEPTION;
11:21:09 935  BAD_PAYPAL_ID		  EXCEPTION;
11:21:09 936  BAD_INSTRUMENT_TYPE	  EXCEPTION;
11:21:09 937  TRANSACTION_EXISTS	  EXCEPTION;
11:21:09 938  CAN_NOT_CALC_INVOICE_AMOUNT EXCEPTION;
11:21:09 939  CAN_NOT_USE_FCINSTR	  EXCEPTION;
11:21:09 940  CAN_NOT_CREATE_TRANSACTION  EXCEPTION;
11:21:09 941  CAN_NOT_CREATE_CHARGE	  EXCEPTION;
11:21:09 942  EXCEPTION_MESSAGE   VARCHAR2(1024);
11:21:09 943  BEGIN
11:21:09 944  
11:21:09 945  	-- Check that instrument exists
11:21:09 946  	IF in_instrument_type_id = GLOBAL_ENUMS_V18.INSTRUMENT_CREDIT_CARD THEN
11:21:09 947  	  IF IS_CREDIT_CARD_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V18.FALSE THEN
11:21:09 948  	    RAISE BAD_CREDIT_CARD_ID;
11:21:09 949  	  END IF;
11:21:09 950  	ELSIF in_instrument_type_id = GLOBAL_ENUMS_V18.INSTRUMENT_PAYPAL THEN
11:21:09 951  	  IF IS_PAYPAL_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V18.FALSE THEN
11:21:09 952  	    RAISE BAD_PAYPAL_ID;
11:21:09 953  	  END IF;
11:21:09 954  	ELSIF in_instrument_type_id = GLOBAL_ENUMS_V18.INSTRUMENT_ZCI_INSTRUMENT THEN
11:21:09 955  	  NULL;
11:21:09 956  	ELSE
11:21:09 957  	  RAISE BAD_INSTRUMENT_TYPE;
11:21:09 958  	END IF;
11:21:09 959  
11:21:09 960  	-- Check that transaction with given id do not exists
11:21:09 961  	SELECT
11:21:09 962  	  COUNT(*) into temp_transaction_id_count
11:21:09 963  	FROM
11:21:09 964  	  TRANSACTION
11:21:09 965  	WHERE
11:21:09 966  	  TRANSACTION.ID = in_transaction_id;
11:21:09 967  
11:21:09 968  	IF temp_transaction_id_count > 0 THEN
11:21:09 969  	  RAISE TRANSACTION_EXISTS;
11:21:09 970  	END IF;
11:21:09 971  
11:21:09 972  	-- Calculate new invoice amount
11:21:09 973  	BEGIN
11:21:09 974  	  PROCS_INVOICE_V18.CALCULATE_INVOICE_AMOUNT(in_invoice_id, var_invoice_amount);
11:21:09 975  	  EXCEPTION
11:21:09 976  	    WHEN OTHERS THEN
11:21:09 977  	      EXCEPTION_MESSAGE := SQLERRM;
11:21:09 978  	      RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
11:21:09 979  	END;
11:21:09 980  
11:21:09 981  	IF var_invoice_amount > 0
11:21:09 982  	  AND in_instrument_type_id = GLOBAL_ENUMS_V18.INSTRUMENT_ZCI_INSTRUMENT THEN
11:21:09 983  	  RAISE CAN_NOT_USE_FCINSTR;
11:21:09 984  	END IF;
11:21:09 985  
11:21:09 986  	-- If invoice amount iz 0 then we need to set status for this invoice to PROCCESSED
11:21:09 987  	IF var_invoice_amount = 0 THEN
11:21:09 988  	  PROCS_INVOICE_CRU_V18.UPDATE_INVOICE(
11:21:09 989  	    in_invoice_id		   => in_invoice_id,
11:21:09 990  	    in_updated_by		   => in_created_by,
11:21:09 991  	    in_invoice_status_id	   => GLOBAL_STATUSES_V18.INVOICE_CLOSED
11:21:09 992  	  );
11:21:09 993  	END IF;
11:21:09 994  
11:21:09 995  	IF var_invoice_amount > 0 THEN
11:21:09 996  	  -- Create transaction
11:21:09 997  	  BEGIN
11:21:09 998  	    PROCS_TRANSACTION_V18.CREATE_TRANSACTION(
11:21:09 999  	      in_transaction_id        => in_transaction_id,
11:21:09 1000  	       in_status_id		=> GLOBAL_STATUSES_V18.TRANSACTION_PENDING,
11:21:09 1001  	       in_amount		=> var_invoice_amount,
11:21:09 1002  	       in_created_by		=> in_created_by,
11:21:09 1003  	       in_order_id		=> in_order_id,
11:21:09 1004  	       in_transaction_type_code => 'GIFT_CERTIFICATE_PURCHASE',
11:21:09 1005  	       out_transaction_id	=> var_transaction_id
11:21:09 1006  	     );
11:21:09 1007  	     EXCEPTION
11:21:09 1008  	       WHEN OTHERS THEN
11:21:09 1009  		 EXCEPTION_MESSAGE := SQLERRM;
11:21:09 1010  		 RAISE CAN_NOT_CREATE_TRANSACTION;
11:21:09 1011  	   END;
11:21:09 1012  
11:21:09 1013  	   -- Create charge
11:21:09 1014  	   BEGIN
11:21:09 1015  	     PROCS_CHARGE_V18.CREATE_CHARGE(
11:21:09 1016  	       in_invoice_id	     => in_invoice_id,
11:21:09 1017  	       in_transaction_id     => var_transaction_id,
11:21:09 1018  	       in_instrument_type_id => in_instrument_type_id,
11:21:09 1019  	       in_instrument_id      => in_instrument_id,
11:21:09 1020  	       in_charge_amount      => var_invoice_amount,
11:21:09 1021  	       in_created_by	     => in_created_by,
11:21:09 1022  	       in_charge_status_id   => GLOBAL_STATUSES_V18.CHARGE_OPENED,
11:21:09 1023  	       out_charge_id	     => var_new_charge_id
11:21:09 1024  	     );
11:21:09 1025  	     out_charge_amount := var_invoice_amount;
11:21:09 1026  	     EXCEPTION
11:21:09 1027  	       WHEN OTHERS THEN
11:21:09 1028  		 EXCEPTION_MESSAGE := SQLERRM;
11:21:09 1029  		 RAISE CAN_NOT_CREATE_CHARGE;
11:21:09 1030  	   END;
11:21:09 1031  	 ELSE
11:21:09 1032  	   out_charge_amount := 0;
11:21:09 1033  	 END IF;
11:21:09 1034  
11:21:09 1035  EXCEPTION
11:21:09 1036  WHEN CAN_NOT_USE_FCINSTR THEN
11:21:09 1037  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.STATE_ERROR,
11:21:09 1038  	   SPROC_NAME, 'Could not use "free charge instrument" for non-zero invoice');
11:21:09 1039  WHEN BAD_CREDIT_CARD_ID THEN
11:21:09 1040  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 1041  	   SPROC_NAME, 'Bad credit card id');
11:21:09 1042  WHEN BAD_PAYPAL_ID THEN
11:21:09 1043  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 1044  	   SPROC_NAME, 'Bad paypal id');
11:21:09 1045  WHEN BAD_INSTRUMENT_TYPE THEN
11:21:09 1046  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INVALID_PARAMETER,
11:21:09 1047  	   SPROC_NAME, 'Bad instrument type');
11:21:09 1048  WHEN TRANSACTION_EXISTS THEN
11:21:09 1049  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.DUPLICATE_ERROR,
11:21:09 1050  	   SPROC_NAME, 'Transaction with given id already exists');
11:21:09 1051  WHEN CAN_NOT_CREATE_TRANSACTION THEN
11:21:09 1052  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 1053  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
11:21:09 1054  WHEN CAN_NOT_CREATE_CHARGE THEN
11:21:09 1055  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 1056  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
11:21:09 1057  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
11:21:09 1058  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 1059  	   SPROC_NAME, 'Could not calculate amount for new invoice', EXCEPTION_MESSAGE);
11:21:09 1060  WHEN OTHERS THEN
11:21:09 1061  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:09 1062  	   SPROC_NAME, 'Unknown error', SQLERRM);
11:21:09 1063  END FINALIZE_GC_PURCHASING;
11:21:09 1064  
11:21:09 1065  /******************************************************************************/
11:21:09 1066  
11:21:09 1067  PROCEDURE PURCHASE_GIFT_CERTIFICATE (
11:21:09 1068  	 in_group_id		   IN NUMBER,
11:21:09 1069  	 in_offer_chain_id	   IN VARCHAR2,
11:21:09 1070  	 in_gift_certificate_code  IN VARCHAR2,
11:21:09 1071  	 in_created_by		   IN VARCHAR2,
11:21:09 1072  	 in_recipient_name	   IN VARCHAR2,
11:21:09 1073  	 in_recipient_email	   IN VARCHAR2,
11:21:09 1074  	 in_sender_name 	   IN VARCHAR2,
11:21:09 1075  	 in_sender_email	   IN VARCHAR2,
11:21:09 1076  	 in_gift_message	   IN VARCHAR2,
11:21:09 1077  	 in_instrument_id	   IN NUMBER,
11:21:09 1078  	 in_instrument_type_id	   IN NUMBER,
11:21:09 1079  	 in_expiration_date	   IN DATE,
11:21:09 1080  	 in_order_id		   IN VARCHAR2,
11:21:09 1081  	 in_transaction_id	   IN NUMBER
11:21:09 1082  ) AS
11:21:09 1083  SPROC_NAME	  CONSTANT VARCHAR2(25) := 'PURCHASE_GIFT_CERTIFICATE';
11:21:09 1084  -- VARIABLES
11:21:09 1085  var_gift_cert_id   NUMBER;
11:21:09 1086  var_account_id	  NUMBER;
11:21:09 1087  var_invoice_amount NUMBER (10,2);
11:21:09 1088  var_new_invoice_id NUMBER;
11:21:09 1089  var_new_charge_id  NUMBER;
11:21:09 1090  var_och_is_gc	  NUMBER;
11:21:09 1091  var_offer_chain_status_id NUMBER;
11:21:09 1092  var_is_for_redemption	 NUMBER;
11:21:09 1093  var_account_tax_exempt_id VARCHAR2(255);
11:21:09 1094  
11:21:09 1095  temp_transaction_id_count NUMBER;
11:21:09 1096  var_transaction_id	 NUMBER;
11:21:09 1097  temp_gc_code VARCHAR2(255);
11:21:09 1098  
11:21:09 1099  var_invoice_status_id NUMBER;
11:21:09 1100  -- EXCEPTIONS
11:21:09 1101  CAN_NOT_CREATE_INVOICE		   EXCEPTION;
11:21:09 1102  CAN_NOT_CREATE_TRANSACTION	   EXCEPTION;
11:21:09 1103  CAN_NOT_CREATE_CHARGE		   EXCEPTION;
11:21:09 1104  CAN_NOT_CREATE_LINE_ITEMS	   EXCEPTION;
11:21:09 1105  BAD_GROUP_ID			   EXCEPTION;
11:21:09 1106  BAD_OFFER_CHAIN_ID		   EXCEPTION;
11:21:09 1107  OCH_IS_NOT_GIFT_CERTIFICATE	   EXCEPTION;
11:21:09 1108  TRANSACTION_EXISTS		   EXCEPTION;
11:21:09 1109  GC_CODE_ALREADY_EXISTS		   EXCEPTION;
11:21:09 1110  BAD_INSTRUMENT_TYPE		   EXCEPTION;
11:21:09 1111  BAD_CREDIT_CARD_ID		   EXCEPTION;
11:21:09 1112  BAD_PAYPAL_ID			   EXCEPTION;
11:21:09 1113  CAN_NOT_CALCULATE_OCH_AMOUNT	   EXCEPTION;
11:21:09 1114  BAD_OFFER_CHAIN_STATUS		   EXCEPTION;
11:21:09 1115  OFFER_REC_NUM_LESS_THAN_ONE	   EXCEPTION;
11:21:09 1116  CAN_NOT_CALC_INVOICE_AMOUNT	   EXCEPTION;
11:21:09 1117  CAN_NOT_USE_FCINSTR		   EXCEPTION;
11:21:09 1118  CAN_NOT_PURCHASE_GC_FOR_RDMPN	   EXCEPTION;
11:21:09 1119  EXCEPTION_MESSAGE		   VARCHAR2(1024);
11:21:09 1120  BEGIN
11:21:09 1121  
11:21:09 1122  	 -- Get account id
11:21:09 1123  	 BEGIN
11:21:09 1124  	   SELECT
11:21:09 1125  	     ACCOUNT.ID,
11:21:09 1126  	     ACCOUNT.TAX_EXEMPT_ID
11:21:09 1127  	     into
11:21:09 1128  	     var_account_id,
11:21:09 1129  	     var_account_tax_exempt_id
11:21:09 1130  	   FROM
11:21:09 1131  	     ACCOUNT
11:21:09 1132  	   WHERE
11:21:09 1133  	     ACCOUNT.GROUP_ID = in_group_id;
11:21:09 1134  	   EXCEPTION
11:21:09 1135  	   WHEN NO_DATA_FOUND THEN
11:21:09 1136  	     RAISE BAD_GROUP_ID;
11:21:09 1137  	 END;
11:21:09 1138  
11:21:09 1139  	 -- Check that instrument exists
11:21:09 1140  	 IF in_instrument_type_id = GLOBAL_ENUMS_V18.INSTRUMENT_CREDIT_CARD THEN
11:21:09 1141  	   IF IS_CREDIT_CARD_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V18.FALSE THEN
11:21:09 1142  	     RAISE BAD_CREDIT_CARD_ID;
11:21:09 1143  	   END IF;
11:21:09 1144  	 ELSIF in_instrument_type_id = GLOBAL_ENUMS_V18.INSTRUMENT_PAYPAL THEN
11:21:09 1145  	   IF IS_PAYPAL_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V18.FALSE THEN
11:21:09 1146  	     RAISE BAD_PAYPAL_ID;
11:21:09 1147  	   END IF;
11:21:09 1148  	 ELSIF in_instrument_type_id = GLOBAL_ENUMS_V18.INSTRUMENT_ZCI_INSTRUMENT THEN
11:21:09 1149  	   NULL;
11:21:09 1150  	 ELSE
11:21:09 1151  	   RAISE BAD_INSTRUMENT_TYPE;
11:21:09 1152  	 END IF;
11:21:09 1153  
11:21:09 1154  	 -- Check for the same code
11:21:09 1155  	 BEGIN
11:21:09 1156  	   SELECT
11:21:09 1157  	     GIFT_CERTIFICATE.CODE into temp_gc_code
11:21:09 1158  	   FROM
11:21:09 1159  	     GIFT_CERTIFICATE
11:21:09 1160  	   WHERE
11:21:09 1161  	     GIFT_CERTIFICATE.CODE = in_gift_certificate_code;
11:21:09 1162  
11:21:09 1163  	   RAISE GC_CODE_ALREADY_EXISTS;
11:21:09 1164  
11:21:09 1165  	   EXCEPTION
11:21:09 1166  	     WHEN NO_DATA_FOUND THEN
11:21:09 1167  	       NULL;
11:21:09 1168  	 END;
11:21:09 1169  
11:21:09 1170  	 -- Get offer chain flag "is_gift_certificate"
11:21:09 1171  	 BEGIN
11:21:09 1172  	   SELECT
11:21:09 1173  	     OFFER_CHAIN.IS_GIFT_CERTIFICATE,
11:21:09 1174  	     OFFER_CHAIN.OFFER_CHAIN_STATUS_ID
11:21:09 1175  	     into
11:21:09 1176  	     var_och_is_gc,
11:21:09 1177  	     var_offer_chain_status_id
11:21:09 1178  	   FROM
11:21:09 1179  	     OFFER_CHAIN
11:21:09 1180  	   WHERE
11:21:09 1181  	     OFFER_CHAIN.ID = in_offer_chain_id;
11:21:09 1182  	   EXCEPTION
11:21:09 1183  	   WHEN NO_DATA_FOUND THEN
11:21:09 1184  	     RAISE BAD_OFFER_CHAIN_ID;
11:21:09 1185  	 END;
11:21:09 1186  
11:21:09 1187  	 IF var_och_is_gc != GLOBAL_CONSTANTS_V18.TRUE
11:21:09 1188  	   OR var_och_is_gc IS NULL THEN
11:21:09 1189  	   RAISE OCH_IS_NOT_GIFT_CERTIFICATE;
11:21:09 1190  	 END IF;
11:21:09 1191  
11:21:09 1192  	 IF var_offer_chain_status_id != GLOBAL_STATUSES_V18.OFFER_CHAIN_ACTIVE THEN
11:21:09 1193  	   RAISE BAD_OFFER_CHAIN_STATUS;
11:21:09 1194  	 END IF;
11:21:09 1195  
11:21:09 1196  	 -- norlov: #38151 check if the OC is for Redemption:
11:21:09 1197  	 SELECT
11:21:09 1198  	   COUNT(*) into var_is_for_redemption
11:21:09 1199  	 FROM
11:21:09 1200  	   OFFER_CHAIN_ELIGIBILITY
11:21:09 1201  	 WHERE
11:21:09 1202  	   OFFER_CHAIN_ELIGIBILITY.OFFER_CHAIN_ID = in_offer_chain_id
11:21:09 1203  	   AND OFFER_CHAIN_ELIGIBILITY.NAME = GLOBAL_CONSTANTS_V18.GIFT_CERTIFICATE_REQUIRED
11:21:09 1204  	   AND OFFER_CHAIN_ELIGIBILITY.VALUE = GLOBAL_CONSTANTS_V18.ELIGIBILITY_FLAG_SET;
11:21:09 1205  
11:21:09 1206  	 IF var_is_for_redemption > 0 THEN
11:21:09 1207  	   RAISE CAN_NOT_PURCHASE_GC_FOR_RDMPN;
11:21:09 1208  	 END IF;
11:21:09 1209  
11:21:09 1210  	 -- Check that transaction with given id do not exists
11:21:09 1211  	 SELECT
11:21:09 1212  	   COUNT(*) into temp_transaction_id_count
11:21:09 1213  	 FROM
11:21:09 1214  	   TRANSACTION
11:21:09 1215  	 WHERE
11:21:09 1216  	   TRANSACTION.ID = in_transaction_id;
11:21:09 1217  
11:21:09 1218  	 IF temp_transaction_id_count > 0 THEN
11:21:09 1219  	   RAISE TRANSACTION_EXISTS;
11:21:09 1220  	 END IF;
11:21:09 1221  
11:21:09 1222  	 -- Create new invoice
11:21:09 1223  	 BEGIN
11:21:09 1224  	   PROCS_INVOICE_V18.CREATE_INVOICE(
11:21:09 1225  	     in_invoice_status => GLOBAL_STATUSES_V18.INVOICE_OPEN,
11:21:09 1226  	     in_created_by     => in_created_by,
11:21:09 1227  	     in_tax_exempt_id  => var_account_tax_exempt_id,
11:21:09 1228  	     out_invoice_id    => var_new_invoice_id
11:21:09 1229  	   );
11:21:09 1230  	   EXCEPTION
11:21:09 1231  	     WHEN OTHERS THEN
11:21:09 1232  	       EXCEPTION_MESSAGE := SQLERRM;
11:21:09 1233  	       RAISE CAN_NOT_CREATE_INVOICE;
11:21:09 1234  	 END;
11:21:09 1235  
11:21:09 1236  	 -- Add line items for new invoice
11:21:09 1237  	 BEGIN
11:21:09 1238  	   FOR f_offer_data IN (
11:21:09 1239  	     SELECT
11:21:09 1240  	       OFFER_ID,
11:21:09 1241  	       NUM_RECURRENCES
11:21:09 1242  	     FROM
11:21:09 1243  	       OFFER_OFFER_CHAIN
11:21:09 1244  	     WHERE
11:21:09 1245  	       OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
11:21:09 1246  	   )
11:21:09 1247  	   LOOP
11:21:09 1248  	     IF f_offer_data.NUM_RECURRENCES < 1 THEN
11:21:09 1249  	       RAISE OFFER_REC_NUM_LESS_THAN_ONE;
11:21:09 1250  	     END IF;
11:21:09 1251  	     FOR i_offer_recurrences_iterator IN 1..f_offer_data.NUM_RECURRENCES
11:21:09 1252  	     LOOP
11:21:09 1253  	       PROCS_LINE_ITEMS_V18.ADD_LINE_ITEMS(
11:21:09 1254  		 in_invoice_id => var_new_invoice_id,
11:21:09 1255  		 in_offer_id   => f_offer_data.OFFER_ID,
11:21:09 1256  		 in_created_by => in_created_by
11:21:09 1257  	       );
11:21:09 1258  	     END LOOP;
11:21:09 1259  	   END LOOP;
11:21:09 1260  
11:21:09 1261  	   EXCEPTION
11:21:09 1262  	     WHEN OTHERS THEN
11:21:09 1263  	       EXCEPTION_MESSAGE := SQLERRM;
11:21:09 1264  	       RAISE CAN_NOT_CREATE_LINE_ITEMS;
11:21:09 1265  	 END;
11:21:09 1266  
11:21:09 1267  	 -- Calculate new invoice amount
11:21:09 1268  	 BEGIN
11:21:09 1269  	   PROCS_INVOICE_V18.CALCULATE_INVOICE_AMOUNT(var_new_invoice_id, var_invoice_amount);
11:21:09 1270  	   EXCEPTION
11:21:09 1271  	     WHEN OTHERS THEN
11:21:09 1272  	       EXCEPTION_MESSAGE := SQLERRM;
11:21:09 1273  	       RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
11:21:09 1274  	 END;
11:21:09 1275  
11:21:09 1276  	 IF var_invoice_amount > 0
11:21:09 1277  	   AND in_instrument_type_id = GLOBAL_ENUMS_V18.INSTRUMENT_ZCI_INSTRUMENT THEN
11:21:09 1278  	   RAISE CAN_NOT_USE_FCINSTR;
11:21:09 1279  	 END IF;
11:21:09 1280  
11:21:09 1281  	 -- If invoice amount iz 0 then we need to set status for this invoice to PROCCESSED
11:21:09 1282  	 IF var_invoice_amount = 0 THEN
11:21:09 1283  	   PROCS_INVOICE_CRU_V18.UPDATE_INVOICE(
11:21:09 1284  	     in_invoice_id		    => var_new_invoice_id,
11:21:09 1285  	     in_updated_by		    => in_created_by,
11:21:09 1286  	     in_invoice_status_id	    => GLOBAL_STATUSES_V18.INVOICE_CLOSED
11:21:09 1287  	   );
11:21:09 1288  	 END IF;
11:21:09 1289  
11:21:09 1290  	 IF var_invoice_amount > 0 THEN
11:21:09 1291  	   -- Create transaction
11:21:09 1292  	   BEGIN
11:21:09 1293  	     PROCS_TRANSACTION_V18.CREATE_TRANSACTION(
11:21:09 1294  	       in_transaction_id  => in_transaction_id,
11:21:09 1295  	       in_status_id	  => GLOBAL_STATUSES_V18.TRANSACTION_PENDING,
11:21:09 1296  	       in_amount	  => var_invoice_amount,
11:21:09 1297  	       in_created_by	  => in_created_by,
11:21:09 1298  	       in_order_id	  => in_order_id,
11:21:09 1299  	       out_transaction_id => var_transaction_id
11:21:09 1300  	     );
11:21:09 1301  	     EXCEPTION
11:21:09 1302  	       WHEN OTHERS THEN
11:21:09 1303  		 EXCEPTION_MESSAGE := SQLERRM;
11:21:09 1304  		 RAISE CAN_NOT_CREATE_TRANSACTION;
11:21:09 1305  	   END;
11:21:09 1306  
11:21:09 1307  	   -- Create charge
11:21:09 1308  	   BEGIN
11:21:09 1309  	     PROCS_CHARGE_V18.CREATE_CHARGE(
11:21:09 1310  	       in_invoice_id	     => var_new_invoice_id,
11:21:09 1311  	       in_transaction_id     => var_transaction_id,
11:21:09 1312  	       in_instrument_type_id => in_instrument_type_id,
11:21:09 1313  	       in_instrument_id      => in_instrument_id,
11:21:09 1314  	       in_charge_amount      => var_invoice_amount,
11:21:09 1315  	       in_created_by	     => in_created_by,
11:21:09 1316  	       in_charge_status_id   => GLOBAL_STATUSES_V18.CHARGE_OPENED,
11:21:09 1317  	       out_charge_id	     => var_new_charge_id
11:21:09 1318  	     );
11:21:09 1319  	     EXCEPTION
11:21:09 1320  	       WHEN OTHERS THEN
11:21:09 1321  		 EXCEPTION_MESSAGE := SQLERRM;
11:21:09 1322  		 RAISE CAN_NOT_CREATE_CHARGE;
11:21:09 1323  	   END;
11:21:09 1324  	 END IF;
11:21:09 1325  
11:21:09 1326  	 -- Create new row in GIFT_CERTIFICATE table
11:21:09 1327  	 PROCS_FIN_INSTRUMENTS_CRU_V18.CREATE_GIFT_CERTIFICATE(
11:21:09 1328  	   out_gift_certificate_id	 => var_gift_cert_id,
11:21:09 1329  	   in_purchaser_group_id	 => in_group_id,
11:21:09 1330  	   in_purchaser_invoice_id	 => var_new_invoice_id,
11:21:09 1331  	   in_offer_chain_id		 => in_offer_chain_id,
11:21:09 1332  	   in_expiration_date		 => in_expiration_date,
11:21:09 1333  	   in_purchase_date		 => SYSDATE,
11:21:09 1334  	   in_gift_certificate_status_id => GLOBAL_STATUSES_V18.GIFT_CERTIFICATE_ACTIVE,
11:21:09 1335  	   in_code			 => in_gift_certificate_code,
11:21:09 1336  	   in_created_by		 => in_created_by,
11:21:09 1337  	   in_recipient_name		 => in_recipient_name,
11:21:09 1338  	   in_gift_message		 => in_gift_message,
11:21:09 1339  	   in_recipient_email		 => in_recipient_email,
11:21:09 1340  	   in_sender_email		 => in_sender_email,
11:21:09 1341  	   in_sender_name		 => in_sender_name
11:21:09 1342  	 );
11:21:09 1343  
11:21:09 1344  EXCEPTION
11:21:09 1345  WHEN CAN_NOT_USE_FCINSTR THEN
11:21:09 1346  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.STATE_ERROR,
11:21:09 1347  	   SPROC_NAME, 'Could not use "free charge instrument" for non-zero invoice');
11:21:09 1348  WHEN BAD_OFFER_CHAIN_STATUS THEN
11:21:09 1349  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.STATE_ERROR,
11:21:09 1350  	   SPROC_NAME, 'Offer chain is not active');
11:21:09 1351  WHEN CAN_NOT_CALCULATE_OCH_AMOUNT THEN
11:21:09 1352  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 1353  	   SPROC_NAME, 'Could not calculate offer chain amount', EXCEPTION_MESSAGE);
11:21:09 1354  WHEN BAD_CREDIT_CARD_ID THEN
11:21:09 1355  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 1356  	   SPROC_NAME, 'Bad credit card id');
11:21:09 1357  WHEN BAD_PAYPAL_ID THEN
11:21:09 1358  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 1359  	   SPROC_NAME, 'Bad paypal id');
11:21:09 1360  WHEN BAD_INSTRUMENT_TYPE THEN
11:21:09 1361  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INVALID_PARAMETER,
11:21:09 1362  	   SPROC_NAME, 'Bad instrument type');
11:21:09 1363  WHEN GC_CODE_ALREADY_EXISTS THEN
11:21:09 1364  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.DUPLICATE_ERROR,
11:21:09 1365  	   SPROC_NAME, 'Gift certificate with same code already exists');
11:21:09 1366  WHEN OCH_IS_NOT_GIFT_CERTIFICATE THEN
11:21:09 1367  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INVALID_PARAMETER,
11:21:09 1368  	   SPROC_NAME, 'This offer chain can not be used for gift certificate');
11:21:09 1369  WHEN CAN_NOT_PURCHASE_GC_FOR_RDMPN THEN
11:21:09 1370  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INVALID_PARAMETER,
11:21:09 1371  	   SPROC_NAME, 'This offer chain can not be purchased for gift certificate since it is for redemption');
11:21:09 1372  WHEN TRANSACTION_EXISTS THEN
11:21:09 1373  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.DUPLICATE_ERROR,
11:21:09 1374  	   SPROC_NAME, 'Transaction with given id already exists');
11:21:09 1375  WHEN CAN_NOT_CREATE_INVOICE THEN
11:21:09 1376  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 1377  	   SPROC_NAME, 'Could not create new invoice', EXCEPTION_MESSAGE);
11:21:09 1378  WHEN CAN_NOT_CREATE_TRANSACTION THEN
11:21:09 1379  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 1380  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
11:21:09 1381  WHEN CAN_NOT_CREATE_CHARGE THEN
11:21:09 1382  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 1383  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
11:21:09 1384  WHEN CAN_NOT_CREATE_LINE_ITEMS THEN
11:21:09 1385  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 1386  	   SPROC_NAME, 'Could not create line items', EXCEPTION_MESSAGE);
11:21:09 1387  WHEN BAD_GROUP_ID THEN
11:21:09 1388  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 1389  	   SPROC_NAME, 'No such group id');
11:21:09 1390  WHEN BAD_OFFER_CHAIN_ID THEN
11:21:09 1391  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 1392  	   SPROC_NAME, 'No such offer chain');
11:21:09 1393  WHEN OFFER_REC_NUM_LESS_THAN_ONE THEN
11:21:09 1394  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 1395  	   SPROC_NAME, 'Some offer has recurrences number less than 1');
11:21:09 1396  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
11:21:09 1397  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 1398  	   SPROC_NAME, 'COuold not calculate amount for new invoice', EXCEPTION_MESSAGE);
11:21:09 1399  WHEN OTHERS THEN
11:21:09 1400  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:09 1401  	   SPROC_NAME, 'Unknown error', SQLERRM);
11:21:09 1402  END PURCHASE_GIFT_CERTIFICATE;
