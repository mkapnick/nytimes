11:21:09 2022  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 2023  	   SPROC_NAME, 'Could not find paypal with given ID');
11:21:09 2024  WHEN BAD_INSTRUMENT_TYPE THEN
11:21:09 2025  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INVALID_PARAMETER,
11:21:09 2026  	   SPROC_NAME, 'Bad instrument type id');
11:21:09 2027  WHEN OTHERS THEN
11:21:09 2028  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:09 2029  	   SPROC_NAME, 'Unknown error', SQLERRM);
