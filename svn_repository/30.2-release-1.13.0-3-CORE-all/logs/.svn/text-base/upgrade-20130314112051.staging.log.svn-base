11:21:07  52  	  C.CODE NAME,
11:21:07  53  	  C.DESCRIPTION,
11:21:07  54  	  0 INHERITED,
11:21:07  55  	  C.SHAREABLE,
11:21:07  56  	  MAX(LIC.ENTITLEMENT_END_DATE) EXPIRES
11:21:07  57  	FROM
11:21:07  58  	  SUBSCRIPTION SB
