11:21:09 606  EXCEPTION_MESSAGE 	      VARCHAR2(1024);
11:21:09 607  BEGIN
11:21:09 608  
11:21:09 609  	-- Check if invoice exists
11:21:09 610  	BEGIN
11:21:09 611  	  SELECT
11:21:09 612  	    INVOICE.ID into temp_invoice_id
11:21:09 613  	  FROM
11:21:09 614  	    INVOICE
11:21:09 615  	  WHERE
11:21:09 616  	    INVOICE.ID = in_invoice_id;
