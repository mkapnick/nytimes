11:21:08 382  	IF var_offer_num_recurrences = 0 THEN
11:21:08 383  	  --out_result_set := NULL;
11:21:08 384  	  -- TODO: Remove this hardcode
11:21:08 385  	  OPEN out_result_set FOR
