11:21:09 645  /****************************************************************/
11:21:09 646  
11:21:09 647  FUNCTION IS_INVOICE_PAYING_STARTED (
11:21:09 648  /*
11:21:09 649  Throws exceptions:
11:21:09 650  APP_EXCEPTION_CODES_V18.NOT_FOUND
11:21:09 651  APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR
11:21:09 652  */
11:21:09 653  	in_invoice_id IN NUMBER
11:21:09 654  ) RETURN NUMBER AS
11:21:09 655  -- VARIABLES
11:21:09 656  SPROC_NAME		 CONSTANT VARCHAR2(30) := 'IS_INVOICE_PAYING_STARTED';
11:21:09 657  temp_invoice_id		 NUMBER;
11:21:09 658  var_processed_charges_num  NUMBER;
11:21:09 659  var_processed_transac_num  NUMBER;
11:21:09 660  var_success_attempts_num	 NUMBER;
11:21:09 661  var_is_gc 		 NUMBER;
11:21:09 662  -- EXCEPTIONS
11:21:09 663  BAD_INVOICE_ID EXCEPTION;
11:21:09 664  BEGIN
11:21:09 665  
11:21:09 666  	-- Check that invoice exists
11:21:09 667  	BEGIN
11:21:09 668  	  SELECT
11:21:09 669  	    INVOICE.ID into temp_invoice_id
11:21:09 670  	  FROM
11:21:09 671  	    INVOICE
11:21:09 672  	  WHERE
11:21:09 673  	    INVOICE.ID = in_invoice_id;
11:21:09 674  	  EXCEPTION
11:21:09 675  	   WHEN NO_DATA_FOUND THEN
11:21:09 676  	     RAISE BAD_INVOICE_ID;
11:21:09 677  	END;
11:21:09 678  
11:21:09 679  	-- Check that there are tansaction attempts with status success
11:21:09 680  	SELECT
11:21:09 681  	  COUNT(1) into var_success_attempts_num
11:21:09 682  	FROM
11:21:09 683  	  TRANSACTION_ATTEMPT ta,
11:21:09 684  	  TRANSACTION t,
11:21:09 685  	  CHARGE c
11:21:09 686  	WHERE
11:21:09 687  	  ta.TRANSACTION_ATTEMPT_STATUS_ID = GLOBAL_STATUSES_V18.TRANS_ATTEMPT_SUCCESS and
11:21:09 688  	  ta.transaction_id = t.id and
11:21:09 689  	  c.transaction_id = t.id and
11:21:09 690  	  t.is_refund = GLOBAL_CONSTANTS_V18.FALSE and
11:21:09 691  	  c.invoice_id = in_invoice_id
11:21:09 692  	;
11:21:09 693  
11:21:09 694  	IF var_success_attempts_num > 0 THEN
11:21:09 695  	  RETURN GLOBAL_CONSTANTS_V18.TRUE;
11:21:09 696  	END IF;
11:21:09 697  
11:21:09 698  	SELECT
11:21:09 699  	  COUNT(1) into var_success_attempts_num
11:21:09 700  	FROM
11:21:09 701  	  TRANSACTION t,
11:21:09 702  	  CHARGE c
11:21:09 703  	WHERE
11:21:09 704  	  c.transaction_id = t.id and
11:21:09 705  	  t.is_refund = GLOBAL_CONSTANTS_V18.FALSE and
11:21:09 706  	  t.is_settled = GLOBAL_CONSTANTS_V18.TRUE and
11:21:09 707  	  c.invoice_id = in_invoice_id
11:21:09 708  	;
11:21:09 709  
11:21:09 710  	IF var_success_attempts_num > 0 THEN
11:21:09 711  	  RETURN GLOBAL_CONSTANTS_V18.TRUE;
11:21:09 712  	END IF;
11:21:09 713  
11:21:09 714  	SELECT
11:21:09 715  	  COUNT(1) into var_is_gc
11:21:09 716  	FROM
11:21:09 717  	  gift_certificate gc
11:21:09 718  	WHERE
11:21:09 719  	  gc.finalized_invoice_id = in_invoice_id
11:21:09 720  	;
11:21:09 721  
11:21:09 722  	IF var_is_gc > 0 THEN
11:21:09 723  	  RETURN GLOBAL_CONSTANTS_V18.TRUE;
11:21:09 724  	END IF;
11:21:09 725  
11:21:09 726  	RETURN GLOBAL_CONSTANTS_V18.FALSE;
11:21:09 727  
11:21:09 728  EXCEPTION
11:21:09 729  WHEN BAD_INVOICE_ID THEN
11:21:09 730  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 731  	  SPROC_NAME, 'No such invoice');
11:21:09 732  WHEN OTHERS THEN
11:21:09 733  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:09 734  	  SPROC_NAME, 'Unknown error', SQLERRM);
11:21:09 735  END IS_INVOICE_PAYING_STARTED;
11:21:09 736  
11:21:09 737  /******************************************************************************/
