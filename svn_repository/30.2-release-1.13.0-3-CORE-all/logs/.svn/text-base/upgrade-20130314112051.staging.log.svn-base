11:21:07 664  	      EXCEPTION_MESSAGE := SQLERRM;
11:21:07 665  	      RAISE CAN_NOT_GET_FIRST_OFFER;
11:21:07 666  	END;
11:21:07 667  
11:21:07 668  	RETURN CALCULATE_OFFER_AMOUNT(var_first_offer_id);
11:21:07 669  
11:21:07 670  EXCEPTION
11:21:07 671  WHEN BAD_OFFER_CHAIN_ID THEN
11:21:07 672  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:07 673  	  SPROC_NAME, 'No such offer chain');
11:21:07 674  WHEN CAN_NOT_GET_FIRST_OFFER THEN
11:21:07 675  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:07 676  	  SPROC_NAME, 'Could not find first offer', EXCEPTION_MESSAGE);
11:21:07 677  WHEN OTHERS THEN
11:21:07 678  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:07 679  	  SPROC_NAME, 'Unknown error', SQLERRM);
11:21:07 680  END CALCULATE_OFFER_CHAIN_AMOUNT;
