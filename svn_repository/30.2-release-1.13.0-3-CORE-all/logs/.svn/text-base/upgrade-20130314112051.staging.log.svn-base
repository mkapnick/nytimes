11:21:08 468  	      in_amount 		=> var_invoice_amount,
11:21:08 469  	      in_created_by		=> in_created_by,
11:21:08 470  	      in_order_id		=> NULL,
11:21:08 471  	      in_transaction_type_code	=> 'START_SUBSCRIPTION',
11:21:08 472  	      out_transaction_id	=> var_new_transaction_id
11:21:08 473  	    );
11:21:08 474  	    EXCEPTION
11:21:08 475  	      WHEN OTHERS THEN
11:21:08 476  		EXCEPTION_MESSAGE := SQLERRM;
11:21:08 477  		RAISE CAN_NOT_CREATE_TRANSACTION;
11:21:08 478  	  END;
11:21:08 479  
11:21:08 480  	  BEGIN
11:21:08 481  	    PROCS_CHARGE_V18.CREATE_CHARGE(
