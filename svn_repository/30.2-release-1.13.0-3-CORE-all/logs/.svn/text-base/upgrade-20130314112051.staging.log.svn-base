11:21:07  88  	  AND LIC.SUBSCRIPTION_ID = S.ID
11:21:07  89  	  AND OPO.OFFER_ID = LIC.OFFER_ID
11:21:07  90  	  AND PO.ID = OPO.PRODUCT_OFFERING_ID
11:21:07  91  	  AND PO.CAPABILITY_ID = C.ID
