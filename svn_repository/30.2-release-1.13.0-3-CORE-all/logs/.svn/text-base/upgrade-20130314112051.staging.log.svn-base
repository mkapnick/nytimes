11:21:08  30  	  function getRefundAmount(in_line_item_id line_item.id%type)
11:21:08  31  	      return line_item.amount%type
11:21:08  32  	  is
11:21:08  33  	      v_li_total  line_item.amount%type;
11:21:08  34  	      v_inv_total line_item.amount%type;
11:21:08  35  	      v_ref_total line_item.amount%type;
11:21:08  36  	  begin
11:21:08  37  
11:21:08  38  	      for li in (
11:21:08  39  		  select li.invoice_id, po.*
11:21:08  40  		  from line_item li
11:21:08  41  			  join core_owner.product_offering po on li.product_offer_id = po.id
11:21:08  42  		  where li.id = in_line_item_id
11:21:08  43  	      )
11:21:08  44  	      loop
11:21:08  45  		  v_li_total := (li.quantity * li.unit_price) - getDiscountAmount(in_line_item_id);
11:21:08  46  
11:21:08  47  		  v_inv_total := 0;
11:21:08  48  		  v_ref_total := 0;
11:21:08  49  		  for x in (
11:21:08  50  		      select case when c.charge_amount < 0 then -1 else 1 end type, sum(c.charge_amount) total
11:21:08  51  		      from charge c
11:21:08  52  		      where c.invoice_id = li.invoice_id
11:21:08  53  		      group by case when c.charge_amount < 0 then -1 else 1 end
11:21:08  54  		  )
11:21:08  55  		  loop
11:21:08  56  		      if (x.type = 1) then
11:21:08  57  			  v_inv_total := x.total;
11:21:08  58  		      else
11:21:08  59  			  v_ref_total := x.total;
11:21:08  60  		      end if;
11:21:08  61  		  end loop;
11:21:08  62  
11:21:08  63  		  if (v_inv_total > 0) then
11:21:08  64  		      return (v_ref_total / v_inv_total) * v_li_total;
11:21:08  65  		  else
11:21:08  66  		      return 0;
11:21:08  67  		  end if;
11:21:08  68  
11:21:08  69  	      end loop;
11:21:08  70  
