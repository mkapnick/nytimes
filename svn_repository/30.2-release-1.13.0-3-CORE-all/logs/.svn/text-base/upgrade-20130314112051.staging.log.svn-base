11:21:08   6  	  function getDiscountAmount(in_line_item_id line_item.id%type)
11:21:08   7  	      return line_item.amount%type
11:21:08   8  	  is
11:21:08   9  	      v_discount  line_item.amount%type := 0;
11:21:08  10  	  begin
11:21:08  11  	      for x in (
11:21:08  12  		  select d.id discount_id
11:21:08  13  			,nvl(d.fixed_amount, d.percent_amount * (po.quantity * po.unit_price)) discount_amount
11:21:08  14  		  from discount d
11:21:08  15  		      join discount_line_item dli	      on dli.discount_id = d.id
11:21:08  16  		      join discount_product_offering dop      on dop.discount_id = d.id
11:21:08  17  		      join product_offering po		      on po.id = dop.product_offering_id
11:21:08  18  		      join line_item li 		      on li.id = dli.line_item_id and li.product_offer_id = po.id
11:21:08  19  		  where dli.line_item_id = in_line_item_id
11:21:08  20  	      )
11:21:08  21  	      loop
11:21:08  22  		  v_discount := v_discount + x.discount_amount;
11:21:08  23  	      end loop;
11:21:08  24  
11:21:08  25  	      return v_discount;
11:21:08  26  	  end;
