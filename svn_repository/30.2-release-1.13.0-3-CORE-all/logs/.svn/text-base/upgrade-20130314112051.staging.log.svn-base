11:21:09 940  	    GROUP_ACCOUNT.SUBSCRIPTION_ID = SUBSCRIPTION.ID AND
11:21:09 941  	    SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
