11:21:09 946  
11:21:09 947  	  PROCS_GROUP_ACCOUNT_CRU_V18.ADD_IP_RANGE(
11:21:09 948  	    in_group_account_id => in_group_account_id,
11:21:09 949  	    in_minimum_ip_string => in_minimum_ip_string,
11:21:09 950  	    in_minimum_ip_low => in_minimum_ip_low,
11:21:09 951  	    in_minimum_ip_high => in_minimum_ip_high,
11:21:09 952  	    in_maximum_ip_string => in_maximum_ip_string,
11:21:09 953  	    in_maximum_ip_low => in_maximum_ip_low,
11:21:09 954  	    in_maximum_ip_high => in_maximum_ip_high,
11:21:09 955  	    in_created_by => in_created_by
11:21:09 956  	  );
11:21:09 957  
11:21:09 958  	  -- Check for overlapping ip address range after insert.  Note that if another
11:21:09 959  	  -- call to add_ip_range has not completed, overlapping ip entries can occur.
11:21:09 960  	  SELECT count(1) into var_is_dupe
11:21:09 961  	  FROM
11:21:09 962  	    GROUP_ACCOUNT_IP_RANGE
11:21:09 963  	  WHERE
11:21:09 964  	    GROUP_ACC_IP_RNG_STATUS_ID = GLOBAL_STATUSES_V18.GROUP_ACC_IP_RNG_ACTIVE AND
11:21:09 965  		((
11:21:09 966  		  (in_minimum_ip_high > minimum_ip_high and in_minimum_ip_high < maximum_ip_high) or
11:21:09 967  		  (in_minimum_ip_high = minimum_ip_high and (in_minimum_ip_low >= minimum_ip_low and in_minimum_ip_low <= maximum_ip_low)) or
11:21:09 968  		  (in_minimum_ip_high = maximum_ip_high and (in_minimum_ip_low >= minimum_ip_low and in_minimum_ip_low <= maximum_ip_low))
11:21:09 969  		) OR
11:21:09 970  
11:21:09 971  		(
11:21:09 972  		  (in_maximum_ip_high > minimum_ip_high and in_maximum_ip_high < maximum_ip_high) or
11:21:09 973  		  (in_maximum_ip_high = minimum_ip_high and (in_maximum_ip_low >= minimum_ip_low and in_maximum_ip_low <= maximum_ip_low)) or
11:21:09 974  		  (in_maximum_ip_high = maximum_ip_high and (in_maximum_ip_low >= minimum_ip_low and in_maximum_ip_low <= maximum_ip_low))
11:21:09 975  		)) AND
11:21:09 976  	    ROWNUM < 3;
11:21:09 977  
11:21:09 978  	  If(var_is_dupe > 1) THEN
11:21:09 979  	    RAISE DUPE;
11:21:09 980  	  END IF;
11:21:09 981  EXCEPTION
11:21:09 982  WHEN NOT_GL THEN
11:21:09 983  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INVALID_PARAMETER,
11:21:09 984  	  SPROC_NAME, 'Group account type does not support IPs', SQLERRM);
11:21:09 985  WHEN DUPE THEN
11:21:09 986  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INVALID_PARAMETER,
11:21:09 987  	  SPROC_NAME, 'The IP address range is already in use', SQLERRM);
11:21:09 988  WHEN OTHERS THEN
