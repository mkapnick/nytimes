11:21:08 355  	out_result_set		      OUT SYS_REFCURSOR
11:21:08 356  ) AS
11:21:08 357  -- VARIABLES
11:21:08 358  SPROC_NAME	  CONSTANT VARCHAR2(30) := 'GET_TRNSCTN_ATTEMPTS_BY_STATUS';
11:21:08 359  temp_transaction_id NUMBER;
11:21:08 360  -- EXCEPTIONS
11:21:08 361  BAD_TRANSACTION_ID       EXCEPTION;
11:21:08 362  BAD_TRANS_ATTEMPT_STATUS EXCEPTION;
11:21:08 363  BEGIN
11:21:08 364  
11:21:08 365  	-- Check that transaction exists
11:21:08 366  	BEGIN
11:21:08 367  	  SELECT
11:21:08 368  	    TRANSACTION.ID into temp_transaction_id
11:21:08 369  	  FROM
11:21:08 370  	    TRANSACTION
11:21:08 371  	  WHERE
11:21:08 372  	    TRANSACTION.ID = in_transaction_id;
11:21:08 373  	  EXCEPTION
11:21:08 374  	    WHEN OTHERS THEN
11:21:08 375  	      RAISE BAD_TRANSACTION_ID;
11:21:08 376  	END;
11:21:08 377  
11:21:08 378  	-- Check that transaction attempt status is correct
11:21:08 379  	IF in_transaction_attempt_status != GLOBAL_STATUSES_V18.TRANS_ATTEMPT_IN_PROGRESS
11:21:08 380  	  AND in_transaction_attempt_status != GLOBAL_STATUSES_V18.TRANS_ATTEMPT_SUCCESS
11:21:08 381  	  AND in_transaction_attempt_status != GLOBAL_STATUSES_V18.TRANS_ATTEMPT_FAILED THEN
11:21:08 382  	  RAISE BAD_TRANS_ATTEMPT_STATUS;
11:21:08 383  	END IF;
11:21:08 384  
11:21:08 385  	OPEN out_result_set FOR
11:21:08 386  	SELECT
11:21:08 387  	  TRANSACTION_ATTEMPT.ID,
11:21:08 388  	  TRANSACTION_ATTEMPT.EXTERNAL_STATUS_CODE,
11:21:08 389  	  TRANSACTION_ATTEMPT.EXTERNAL_STATUS_MESSAGE,
11:21:08 390  	  TRANSACTION_ATTEMPT.CREATE_DATE,
11:21:08 391  	  TRANSACTION_ATTEMPT.CREATED_BY,
11:21:08 392  	  TRANSACTION_ATTEMPT.EXTERNAL_TRANSACTION_ID,
11:21:08 393  	  TRANSACTION_ATTEMPT.TRANSACTION_START_TIME,
11:21:08 394  	  TRANSACTION.TRANSACTION_AMOUNT,
11:21:08 395  	  TRANSACTION.ID as "TRANSACTION_ID",
11:21:08 396  	  TRANSACTION.UPDATE_DATE as "TRANSACTION_UPDATE_TIME"
11:21:08 397  	FROM
11:21:08 398  	  TRANSACTION_ATTEMPT
11:21:08 399  	  INNER JOIN TRANSACTION ON TRANSACTION_ATTEMPT.TRANSACTION_ID = TRANSACTION.ID
11:21:08 400  	WHERE
11:21:08 401  	  TRANSACTION_ATTEMPT.TRANSACTION_ID = in_transaction_id
11:21:08 402  	  AND TRANSACTION_ATTEMPT.TRANSACTION_ATTEMPT_STATUS_ID = in_transaction_attempt_status;
11:21:08 403  
11:21:08 404  EXCEPTION
11:21:08 405  WHEN BAD_TRANSACTION_ID THEN
11:21:08 406  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:08 407  	  SPROC_NAME, 'No such transaction');
11:21:08 408  WHEN BAD_TRANS_ATTEMPT_STATUS THEN
11:21:08 409  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INVALID_PARAMETER,
11:21:08 410  	  SPROC_NAME, 'Bad transaction attempt status');
11:21:08 411  WHEN OTHERS THEN
11:21:08 412  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:08 413  	  SPROC_NAME, 'Unknown error', SQLERRM);
11:21:08 414  END GET_TRNSCTN_ATTEMPTS_BY_STATUS;
