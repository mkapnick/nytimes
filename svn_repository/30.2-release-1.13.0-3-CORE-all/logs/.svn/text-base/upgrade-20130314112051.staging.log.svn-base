11:21:09 1979  
11:21:09 1980  PROCEDURE GET_PAYMENT_INFO_BY_INVOICE_ID (
11:21:09 1981  	 in_invoice_id		     IN NUMBER,
11:21:09 1982  	 out_order_id		     OUT VARCHAR2,
11:21:09 1983  	 out_external_transaction_id OUT VARCHAR2
11:21:09 1984  ) AS
11:21:09 1985  SPROC_NAME CONSTANT VARCHAR2(30) := 'GET_PEYMENT_INFO_BY_INVOICE_ID';
11:21:09 1986  -- VARIABLES
11:21:09 1987  temp_invoice_id number;
11:21:09 1988  cnt_matched_instr number := 0;
11:21:09 1989  -- EXCEPTIONS
11:21:09 1990  BAD_INVOICE_ID EXCEPTION;
11:21:09 1991  BEGIN
