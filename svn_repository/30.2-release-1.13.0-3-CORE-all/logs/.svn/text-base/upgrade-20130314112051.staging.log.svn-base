11:21:08 106  FUNCTION CREATE_NEW_TRANSFER_SET
11:21:08 107  	  (
11:21:08 108  	      in_set_maximum NUMBER)
11:21:08 109  	  RETURN core_owner.polling_sync.set_id%type
11:21:08 110  IS
11:21:08 111  	  pragma autonomous_transaction;
11:21:08 112  	  v_set_id core_owner.polling_sync.set_id%type;
11:21:08 113  BEGIN
11:21:08 114  	  SELECT
11:21:08 115  	      pollsync_setid_seq.nextval
11:21:08 116  	  INTO
11:21:08 117  	      v_set_id
11:21:08 118  	  FROM
11:21:08 119  	      dual;
11:21:08 120  	  update
11:21:08 121  	    core_owner.polling_sync ps
11:21:08 122  	  set
11:21:08 123  	    ps.set_id = v_set_id
11:21:08 124  	  where
11:21:08 125  	    ps.set_id IS NULL and
11:21:08 126  	    rownum <= in_set_maximum
11:21:08 127  	  ;
11:21:08 128  	  COMMIT;
11:21:08 129  	  RETURN v_set_id;
11:21:08 130  END;
11:21:08 131  --------------------------------------------------------------------------------
11:21:08 132  FUNCTION RETREIVE_TRANSFER_SET
11:21:08 133  	  (
11:21:08 134  	      in_set_id core_owner.polling_sync.set_id%type)
11:21:08 135  	  RETURN sys_refcursor
11:21:08 136  IS
11:21:08 137  	  v_refcursor sys_refcursor;
11:21:08 138  BEGIN
11:21:08 139  	  UPDATE
11:21:08 140  	      core_owner.polling_sync ps
11:21:08 141  	  SET
11:21:08 142  	      ps.last_send_date = sysdate,
11:21:08 143  	      ps.num_calls = ps.num_calls + 1
11:21:08 144  	  WHERE
11:21:08 145  	      ps.set_id = in_set_id ;
11:21:08 146  	  OPEN v_refcursor FOR
11:21:08 147  	  SELECT
11:21:08 148  	      ps.set_id,
11:21:08 149  	      ps.group_id,
11:21:08 150  	      ps.event_type,
11:21:08 151  	      ps.event_date
11:21:08 152  	  FROM
11:21:08 153  	      core_owner.polling_sync ps
11:21:08 154  	  WHERE
11:21:08 155  	      ps.set_id = in_set_id
11:21:08 156  	  AND ps.group_id IS NOT NULL ;
11:21:08 157  	  RETURN v_refcursor;
11:21:08 158  END;
11:21:08 159  --------------------------------------------------------------------------------
11:21:08 160  PROCEDURE GET_TRANSFER_SET
11:21:08 161  	  (
11:21:08 162  	      in_set_maximum NUMBER,
11:21:08 163  	      in_max_retries NUMBER,
11:21:08 164  	      out_refcursor OUT sys_refcursor)
11:21:08 165  IS
11:21:08 166  	  v_set_id core_owner.polling_sync.set_id%type;
11:21:08 167  BEGIN
11:21:08 168  	  /* Look for previously sent but unconfirmed sets and
11:21:08 169  	  send again until max_retries calls */
11:21:08 170  	  FOR x IN
11:21:08 171  	  (
11:21:08 172  	      SELECT
11:21:08 173  		  ps.set_id,
11:21:08 174  		  COUNT( *) set_size
11:21:08 175  	      FROM
11:21:08 176  		  core_owner.polling_sync ps
11:21:08 177  	      WHERE
11:21:08 178  		  ps.event_date > sysdate - 14
11:21:08 179  	      AND ps.confirm_date IS NULL
11:21:08 180  	      AND ps.last_send_date IS NOT NULL
11:21:08 181  	      AND ps.num_calls < in_max_retries
11:21:08 182  	      GROUP BY
11:21:08 183  		  ps.set_id
11:21:08 184  	      ORDER BY
11:21:08 185  		  ps.set_id
11:21:08 186  	  )
11:21:08 187  	  LOOP
11:21:08 188  	      out_refcursor := Retreive_Transfer_Set(x.set_id) ;
11:21:08 189  	      RETURN;
11:21:08 190  	  END LOOP;
11:21:08 191  	  v_set_id := Create_New_Transfer_Set(in_set_maximum) ;
11:21:08 192  	  out_refcursor := Retreive_Transfer_Set(v_set_id) ;
11:21:08 193  	  RETURN;
11:21:08 194  END;
11:21:08 195  --------------------------------------------------------------------------------
11:21:08 196  PROCEDURE CONFIRM_TRANSFER_SET
11:21:08 197  	  (
11:21:08 198  	      in_set_id core_owner.polling_sync.set_id%type)
11:21:08 199  IS
11:21:08 200  	  v_unconfirmable EXCEPTION;
11:21:08 201  BEGIN
11:21:08 202  	  UPDATE
11:21:08 203  	      core_owner.polling_sync ps
11:21:08 204  	  SET
11:21:08 205  	      ps.confirm_date = systimestamp
11:21:08 206  	  WHERE
11:21:08 207  	      ps.set_id = in_set_id
11:21:08 208  	  AND ps.confirm_date IS NULL ;
11:21:08 209  	  IF(sql%rowcount < 1) THEN
11:21:08 210  	      raise v_unconfirmable;
11:21:08 211  	  END IF;
11:21:08 212  END;
11:21:08 213  PROCEDURE SET_LAST_RUN(ts in timestamp)
11:21:08 214  IS
11:21:08 215  BEGIN
11:21:08 216  	  UPDATE POLLING_SYNC_LASTRUN
11:21:08 217  	  SET last_run = current_timestamp;
11:21:08 218  	  IF ( sql%rowcount = 0 )
11:21:08 219  	  THEN
11:21:08 220  	    INSERT INTO POLLING_SYNC_LASTRUN VALUES (ts);
11:21:08 221  	  END if;
11:21:08 222  	  COMMIT;
11:21:08 223  END;
11:21:08 224  PROCEDURE GET_LAST_RUN(ts out timestamp)
11:21:08 225  IS
11:21:08 226  BEGIN
11:21:08 227  	  SELECT LAST_RUN INTO ts
11:21:08 228  	  FROM POLLING_SYNC_LASTRUN
11:21:08 229  	  WHERE ROWNUM < 2;
11:21:08 230  EXCEPTION
11:21:08 231  	WHEN NO_DATA_FOUND
11:21:08 232  	THEN
11:21:08 233  	  ts := current_timestamp;
11:21:08 234  END;
11:21:08 235  END PROCS_POLLING_SYNC;
