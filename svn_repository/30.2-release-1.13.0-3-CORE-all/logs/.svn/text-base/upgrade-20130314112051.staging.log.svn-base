11:21:09 477  BEGIN
11:21:09 478  
11:21:09 479  	-- Get Paypal status
11:21:09 480  	-- Get account
11:21:09 481  	BEGIN
11:21:09 482  	  SELECT
11:21:09 483  	    PAYPAL.PAYPAL_STATUS_ID,
11:21:09 484  	    PAYPAL.ACCOUNT_ID
11:21:09 485  	    into
