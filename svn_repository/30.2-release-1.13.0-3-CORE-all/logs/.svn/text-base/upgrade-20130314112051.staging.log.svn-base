11:21:08 2572  
11:21:08 2573  FUNCTION IS_SUBSCRIPTION_CANCELABLE (
11:21:08 2574  	 in_subscription_id IN NUMBER
11:21:08 2575  ) RETURN NUMBER AS
11:21:08 2576  SPROC_NAME CONSTANT VARCHAR2(26) := 'IS_SUBSCRIPTION_CANCELABLE';
11:21:08 2577  -- VARIABLES
11:21:08 2578  var_is_offer_chain_cancelable NUMBER;
11:21:08 2579  --64603
11:21:08 2580  var_end_date date;
11:21:08 2581  today_date date := current_date;
11:21:08 2582  offer_id number;
11:21:08 2583  -- EXCEPTIONS
11:21:08 2584  COULD_NOT_CHECK	   EXCEPTION;
11:21:08 2585  BAD_SUBSCRIPTION_ID EXCEPTION;
11:21:08 2586  EXCEPTION_MESSAGE   VARCHAR2(1024);
11:21:08 2587  BEGIN
11:21:08 2588  
11:21:08 2589  	 BEGIN
11:21:08 2590  	   -- find offer_chain_id for given in_subscription_id
11:21:08 2591  	   SELECT OFFER_CHAIN_ID into offer_id
11:21:08 2592  	   FROM SUBSCRIPTION
11:21:08 2593  	   WHERE ID = in_subscription_id;
11:21:08 2594  	   EXCEPTION
11:21:08 2595  	     WHEN NO_DATA_FOUND THEN
11:21:08 2596  	       RAISE BAD_SUBSCRIPTION_ID;
11:21:08 2597  	     WHEN OTHERS THEN
11:21:08 2598  	       EXCEPTION_MESSAGE := SQLERRM;
11:21:08 2599  	       RAISE COULD_NOT_CHECK;
11:21:08 2600  	 END;
11:21:08 2601  
11:21:08 2602  	 -- find if it was redeemed from a gift certificate
11:21:08 2603  	 BEGIN
11:21:08 2604  	   SELECT l.end_date INTO var_end_date
11:21:08 2605  	   FROM LICENSE l, GIFT_CERTIFICATE g
11:21:08 2606  	   WHERE l.invoice_id = g.finalized_invoice_id
11:21:08 2607  	   AND l.subscription_id = in_subscription_id;
11:21:08 2608  
11:21:08 2609  	   -- if the license end_date is bigger than today, we are in the
11:21:08 2610  	   -- first period, so we cannot cancel; otherwise can cancel
11:21:08 2611  	   IF var_end_date > today_date THEN
11:21:08 2612  	       RETURN GLOBAL_CONSTANTS_V18.FALSE;
11:21:08 2613  	   ELSE
11:21:08 2614  	       RETURN GLOBAL_CONSTANTS_V18.TRUE;
11:21:08 2615  	   END IF;
11:21:08 2616  
11:21:08 2617  	   EXCEPTION
11:21:08 2618  	       -- not coming from a gift certificate,
11:21:08 2619  	       -- use old logic
11:21:08 2620  	       WHEN NO_DATA_FOUND THEN
11:21:08 2621  		   SELECT
11:21:08 2622  		       PROCS_OFFER_CHAIN_V18.IS_OFFER_CHAIN_CANCELABLE(offer_id)
11:21:08 2623  		       INTO var_is_offer_chain_cancelable
11:21:08 2624  		   FROM DUAL;
11:21:08 2625  		   RETURN var_is_offer_chain_cancelable;
11:21:08 2626  	 END;
11:21:08 2627  
11:21:08 2628  EXCEPTION
11:21:08 2629  WHEN BAD_SUBSCRIPTION_ID THEN
11:21:08 2630  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:08 2631  	   SPROC_NAME, 'No such subscription');
11:21:08 2632  WHEN COULD_NOT_CHECK THEN
11:21:08 2633  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:08 2634  	   SPROC_NAME, 'Could not check if offer chain calcelable', EXCEPTION_MESSAGE);
11:21:08 2635  WHEN OTHERS THEN
11:21:08 2636  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:08 2637  	   SPROC_NAME, 'Unknown error', SQLERRM);
11:21:08 2638  
11:21:08 2639  END IS_SUBSCRIPTION_CANCELABLE;
