11:21:08 1016  -- norlov: #38796
11:21:08 1017  PROCEDURE GET_TRANSACTIONS (
11:21:08 1018  /*
11:21:08 1019  Throws exceptions:
11:21:08 1020  APP_EXCEPTION_CODES_V18.NOT_FOUND
11:21:08 1021  APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR
11:21:08 1022  */
11:21:08 1023  	 in_group_id	       IN  NUMBER,
11:21:08 1024  	 in_invoice_id	       IN NUMBER DEFAULT NULL,
11:21:08 1025  	 in_subscription_id    IN NUMBER DEFAULT NULL,
11:21:08 1026  	 in_start_date	       IN TRANSACTION.CREATE_DATE%TYPE DEFAULT NULL,
11:21:08 1027  	 in_end_date	       IN TRANSACTION.CREATE_DATE%TYPE DEFAULT NULL,
11:21:08 1028  	 in_transaction_status IN NUMBER DEFAULT NULL,
11:21:08 1029  	 out_result_set        OUT SYS_REFCURSOR
11:21:08 1030  ) AS
11:21:08 1031  SPROC_NAME CONSTANT VARCHAR2(16) := 'GET_TRANSACTIONS';
11:21:08 1032  -- VARIABLES
11:21:08 1033  var_account_id ACCOUNT.ID%TYPE;
11:21:08 1034  statement VARCHAR2(2000);
11:21:08 1035  -- EXCEPTIONS
11:21:08 1036  BAD_GROUP_ID   EXCEPTION;
11:21:08 1037  BEGIN
11:21:08 1038  	-- check group id
11:21:08 1039  	BEGIN
11:21:08 1040  	   SELECT
11:21:08 1041  	     ACCOUNT.ID into var_account_id
11:21:08 1042  	   FROM
11:21:08 1043  	     ACCOUNT
11:21:08 1044  	   WHERE
11:21:08 1045  	     ACCOUNT.GROUP_ID = in_group_id;
11:21:08 1046  	   EXCEPTION
11:21:08 1047  	     WHEN NO_DATA_FOUND THEN
11:21:08 1048  	       RAISE BAD_GROUP_ID;
11:21:08 1049  	 END;
11:21:08 1050  
11:21:08 1051  	 OPEN out_result_set FOR
11:21:08 1052  	 SELECT
11:21:08 1053  	   TRANSACTION.ID,
11:21:08 1054  	   TRANSACTION.TRANSACTION_STATUS_ID,
11:21:08 1055  	   TRANSACTION.TRANSACTION_AMOUNT,
11:21:08 1056  	   TRANSACTION.CREATE_DATE,
11:21:08 1057  	   TRANSACTION.CREATED_BY,
11:21:08 1058  	   TRANSACTION.IS_REFUND,
11:21:08 1059  	   GET_TRANSACTION_TAX_AMOUNT(TRANSACTION.ID) as TRANSACTION_TAX_AMOUNT,
11:21:08 1060  	   GET_TRANSACTION_INTRL_TAXES(TRANSACTION.ID) as INTERNATIONAL_TOTAL
11:21:08 1061  	 FROM
11:21:08 1062  	   TRANSACTION
11:21:08 1063  	   INNER JOIN CHARGE ON TRANSACTION.ID = CHARGE.TRANSACTION_ID
11:21:08 1064  	   INNER JOIN INVOICE ON INVOICE.ID = CHARGE.INVOICE_ID
11:21:08 1065  	 WHERE
11:21:08 1066  	   -- Filter by invoice ID
11:21:08 1067  	   (
11:21:08 1068  	     INVOICE.ID IN (
11:21:08 1069  	       -- Gift certificate invoices
11:21:08 1070  	       SELECT
11:21:08 1071  		 GIFT_CERTIFICATE.PURCHASE_INVOICE_ID
11:21:08 1072  	       FROM
11:21:08 1073  		 GIFT_CERTIFICATE
11:21:08 1074  	       WHERE
11:21:08 1075  		 GIFT_CERTIFICATE.PURCHASER_GROUP_ID = in_group_id
11:21:08 1076  		 -- If subscription_id is set then return nothing
11:21:08 1077  		 AND EXISTS (SELECT 1 FROM DUAL WHERE in_subscription_id IS NULL)
11:21:08 1078  	     )
11:21:08 1079  	     OR
11:21:08 1080  	     INVOICE.ID IN (
11:21:08 1081  	       SELECT
11:21:08 1082  		 LICENSE.INVOICE_ID
11:21:08 1083  	       FROM
11:21:08 1084  		 LICENSE
11:21:08 1085  		 INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
11:21:08 1086  	       WHERE
11:21:08 1087  		 SUBSCRIPTION.ACCOUNT_ID = var_account_id
11:21:08 1088  		 -- Filter by subscription id
11:21:08 1089  		 AND SUBSCRIPTION.ID = NVL(in_subscription_id, SUBSCRIPTION.ID)
11:21:08 1090  	     )
11:21:08 1091  	   )
11:21:08 1092  	   -- Filter by invoice id
11:21:08 1093  	   AND INVOICE.ID = NVL(in_invoice_id, INVOICE.ID)
11:21:08 1094  	   -- Filter by start date
11:21:08 1095  	   AND TRANSACTION.CREATE_DATE >= NVL(in_start_date, TRANSACTION.CREATE_DATE)
11:21:08 1096  	   -- Filter by end date
11:21:08 1097  	   AND TRANSACTION.CREATE_DATE <= NVL(in_end_date, TRANSACTION.CREATE_DATE)
11:21:08 1098  	   -- Filter by transaction status
11:21:08 1099  	   AND TRANSACTION.TRANSACTION_STATUS_ID IN ( SELECT NVL(in_transaction_status, TRANSACTION.TRANSACTION_STATUS_ID) FROM DUAL);
11:21:08 1100  
11:21:08 1101  /*
11:21:08 1102  
11:21:08 1103  statement :=  'select distinct '||CHR(10)
11:21:08 1104  || ' TRANSACTION.ID,'||CHR(10)
11:21:08 1105  || ' TRANSACTION.TRANSACTION_STATUS_ID, '||CHR(10)
11:21:08 1106  || ' TRANSACTION.TRANSACTION_AMOUNT, '||CHR(10)
11:21:08 1107  || ' TRANSACTION.CREATE_DATE, '||CHR(10)
11:21:08 1108  || ' TRANSACTION.CREATED_BY, '||CHR(10)
11:21:08 1109  || ' TRANSACTION.IS_REFUND from TRANSACTION '||CHR(10)
11:21:08 1110  || ' inner join CHARGE on (CHARGE.TRANSACTION_ID = TRANSACTION.ID)'||CHR(10)
11:21:08 1111  || ' inner join INVOICE on (INVOICE.ID = CHARGE.INVOICE_ID)'||CHR(10)
11:21:08 1112  || ' inner join LICENSE on (LICENSE.INVOICE_ID = INVOICE.ID)'||CHR(10)
11:21:08 1113  || ' inner join SUBSCRIPTION on (SUBSCRIPTION.ID = LICENSE.SUBSCRIPTION_ID)'||CHR(10)
11:21:08 1114  || ' inner join ACCOUNT on (ACCOUNT.ID = SUBSCRIPTION.ACCOUNT_ID)'||CHR(10)
11:21:08 1115  || ' where ACCOUNT.GROUP_ID = '||in_group_id;
11:21:08 1116  
11:21:08 1117  IF (in_transaction_status IS NOT NULL) THEN
11:21:08 1118  	 statement := statement || CHR(10) || 'AND TRANSACTION.TRANSACTION_STATUS_ID=' || in_transaction_status;
11:21:08 1119  END IF;
11:21:08 1120  
11:21:08 1121  IF (in_invoice_id IS NOT NULL) THEN
11:21:08 1122  	 statement := statement || CHR(10) || 'AND INVOICE.ID=' || in_invoice_id;
11:21:08 1123  END IF;
11:21:08 1124  
11:21:08 1125  IF (in_subscription_id IS NOT NULL) THEN
11:21:08 1126  	 statement := statement || CHR(10) || 'AND SUBSCRIPTION.ID=' || in_subscription_id;
11:21:08 1127  END IF;
11:21:08 1128  
11:21:08 1129  IF (in_start_date IS NOT NULL) THEN
11:21:08 1130  	 statement := statement || CHR(10) || 'AND TRANSACTION.CREATE_DATE>= TO_DATE(''' || TO_CHAR(in_start_date,'yyyy/mm/dd:hh:mi:ss') || ''',''yyyy/mm/dd:hh:mi:ss'')';  -- norlov: ??
11:21:08 1131  END IF;
11:21:08 1132  
11:21:08 1133  IF (in_end_date IS NOT NULL) THEN
11:21:08 1134  	 statement := statement || CHR(10) || 'AND TRANSACTION.CREATE_DATE<= TO_DATE(''' || TO_CHAR(in_end_date,'yyyy/mm/dd:hh:mi:ss') || ''',''yyyy/mm/dd:hh:mi:ss'')'; -- norlov: ??
11:21:08 1135  END IF;
11:21:08 1136  dbms_output.put_line(statement);
11:21:08 1137  OPEN out_result_set FOR statement;
11:21:08 1138  
11:21:08 1139  */
11:21:08 1140  
11:21:08 1141  EXCEPTION
11:21:08 1142  WHEN BAD_GROUP_ID THEN
11:21:08 1143  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:08 1144  	   SPROC_NAME, 'No such group');
11:21:08 1145  WHEN OTHERS THEN
