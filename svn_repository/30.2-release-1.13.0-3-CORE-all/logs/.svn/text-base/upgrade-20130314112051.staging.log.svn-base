11:21:08 151  	  , invoice.id					 invoice_number
11:21:08 152  	  , NVL2(transaction.id, 1, 0)			 has_transaction
11:21:08 153  	  , NVL2(credit_card.id, 1, 0)			 is_cc_transaction
