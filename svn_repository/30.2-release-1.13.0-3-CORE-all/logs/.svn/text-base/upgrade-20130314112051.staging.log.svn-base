11:21:08 1116  FUNCTION GET_BILLING_CYCLE (
11:21:08 1117  	 in_subscription_id IN NUMBER
11:21:08 1118  ) RETURN VARCHAR2 AS
11:21:08 1119  -- VARIABLES
