11:21:09 1709  
11:21:09 1710  PROCEDURE REFUND_INVOICE (
11:21:09 1711  	 in_invoice_id	    IN NUMBER,
11:21:09 1712  	 in_refund_amount   IN NUMBER,
11:21:09 1713  	 in_note	    IN VARCHAR2,
11:21:09 1714  	 in_created_by	    IN VARCHAR2,
11:21:09 1715  	 out_charge_id	    OUT NUMBER
11:21:09 1716  ) AS
11:21:09 1717  SPROC_NAME CONSTANT VARCHAR2(14) := 'REFUND_INVOICE';
11:21:09 1718  -- VARIABLES
11:21:09 1719  var_invoice_status_id  NUMBER;
11:21:09 1720  var_subscription_id    NUMBER;
11:21:09 1721  var_account_id	      NUMBER;
11:21:09 1722  var_group_id	      NUMBER;
11:21:09 1723  var_account_status_id  NUMBER;
11:21:09 1724  var_new_transaction_id NUMBER;
11:21:09 1725  var_instrument_type_id NUMBER;
11:21:09 1726  var_instrument_id      NUMBER;
11:21:09 1727  var_new_charge_id      NUMBER;
11:21:09 1728  var_invoice_amount     NUMBER(10,2);
11:21:09 1729  var_refunds_before     NUMBER(10,2);
11:21:09 1730  var_charges_amount     NUMBER(10,2);
11:21:09 1731  -- EXCEPTIONS
11:21:09 1732  CAN_NOT_FIND_SUBSCR_OR_GC     EXCEPTION;
11:21:09 1733  ACCOUNT_IS_FROZEN	     EXCEPTION;
11:21:09 1734  BAD_INVOICE_ID		     EXCEPTION;
11:21:09 1735  CAN_NOT_CREATE_TRANSACTION    EXCEPTION;
11:21:09 1736  CAN_NOT_CREATE_CHARGE	     EXCEPTION;
11:21:09 1737  CAN_NOT_CALC_INVOICE_AMOUNT   EXCEPTION;
11:21:09 1738  REFUND_IS_GREATER_THAN_ANOUNT EXCEPTION;
11:21:09 1739  CAN_NOT_ANNOTATE_SUBSCRIPTION EXCEPTION;
11:21:09 1740  TOT_REF_IS_GREATER_THAN_ANOUNT EXCEPTION;
11:21:09 1741  INVOICE_IS_NOT_CLOSED	     EXCEPTION;
11:21:09 1742  TOT_REF_IS_GRATER_THAN_CHARGES EXCEPTION;
11:21:09 1743  EXCEPTION_MESSAGE	      VARCHAR2(1024);
11:21:09 1744  BEGIN
11:21:09 1745  
11:21:09 1746  	 BEGIN
11:21:09 1747  	   SELECT
11:21:09 1748  	     INVOICE.INVOICE_STATUS_ID into var_invoice_status_id
11:21:09 1749  	   FROM
11:21:09 1750  	     INVOICE
11:21:09 1751  	   WHERE
11:21:09 1752  	     INVOICE.ID = in_invoice_id;
11:21:09 1753  	   EXCEPTION
11:21:09 1754  	     WHEN NO_DATA_FOUND THEN
11:21:09 1755  	       RAISE BAD_INVOICE_ID;
11:21:09 1756  	 END;
11:21:09 1757  
11:21:09 1758  	 -- Get instrument and subscription id if exists
11:21:09 1759  	 BEGIN
11:21:09 1760  	   SELECT
11:21:09 1761  	     SUBSCRIPTION.INSTRUMENT_ID,
11:21:09 1762  	     SUBSCRIPTION.INSTRUMENT_TYPE_ID,
11:21:09 1763  	     SUBSCRIPTION.ACCOUNT_ID,
11:21:09 1764  	     SUBSCRIPTION.ID
11:21:09 1765  	     into
11:21:09 1766  	     var_instrument_id,
11:21:09 1767  	     var_instrument_type_id,
11:21:09 1768  	     var_account_id,
11:21:09 1769  	     var_subscription_id
11:21:09 1770  	   FROM
11:21:09 1771  	     SUBSCRIPTION
11:21:09 1772  	     INNER JOIN LICENSE ON SUBSCRIPTION.ID = LICENSE.SUBSCRIPTION_ID
11:21:09 1773  	   WHERE
11:21:09 1774  	     LICENSE.INVOICE_ID = in_invoice_id
11:21:09 1775  	     AND ROWNUM <= 1;
11:21:09 1776  	   EXCEPTION
11:21:09 1777  	     WHEN NO_DATA_FOUND THEN
11:21:09 1778  	       BEGIN
11:21:09 1779  
11:21:09 1780  		 var_subscription_id := NULL;
11:21:09 1781  
11:21:09 1782  		 SELECT
11:21:09 1783  		   CHARGE.INSTRUMENT_ID,
11:21:09 1784  		   CHARGE.INSTRUMENT_TYPE_ID,
11:21:09 1785  		   GIFT_CERTIFICATE.PURCHASER_GROUP_ID
11:21:09 1786  		   into
11:21:09 1787  		   var_instrument_id,
11:21:09 1788  		   var_instrument_type_id,
11:21:09 1789  		   var_group_id
11:21:09 1790  		 FROM
11:21:09 1791  		   INVOICE
11:21:09 1792  		   INNER JOIN CHARGE ON INVOICE.ID = CHARGE.INVOICE_ID
11:21:09 1793  		   INNER JOIN GIFT_CERTIFICATE ON GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = INVOICE.ID
11:21:09 1794  		 WHERE
11:21:09 1795  		   INVOICE.ID = in_invoice_id
11:21:09 1796  		   AND ROWNUM <= 1;
11:21:09 1797  
11:21:09 1798  		 SELECT
11:21:09 1799  		   ACCOUNT.ID into var_account_id
11:21:09 1800  		 FROM
11:21:09 1801  		   ACCOUNT
11:21:09 1802  		 WHERE
11:21:09 1803  		   ACCOUNT.GROUP_ID = var_group_id;
11:21:09 1804  
11:21:09 1805  		 EXCEPTION
11:21:09 1806  		   WHEN NO_DATA_FOUND THEN
11:21:09 1807  		     RAISE CAN_NOT_FIND_SUBSCR_OR_GC;
11:21:09 1808  	       END;
11:21:09 1809  	 END;
11:21:09 1810  
11:21:09 1811  	 -- Check account status. It should not to be frozen
11:21:09 1812  	 SELECT
11:21:09 1813  	   ACCOUNT.ACCOUNT_STATUS_ID into var_account_status_id
11:21:09 1814  	 FROM
11:21:09 1815  	   ACCOUNT
11:21:09 1816  	 WHERE
11:21:09 1817  	   ACCOUNT.ID = var_account_id;
11:21:09 1818  
11:21:09 1819  	 IF var_account_status_id = GLOBAL_STATUSES_V18.ACCOUNT_FROZEN THEN
11:21:09 1820  	   RAISE ACCOUNT_IS_FROZEN;
11:21:09 1821  	 END IF;
11:21:09 1822  
11:21:09 1823  	 IF var_invoice_status_id != GLOBAL_STATUSES_V18.INVOICE_CLOSED THEN
11:21:09 1824  	   RAISE INVOICE_IS_NOT_CLOSED;
11:21:09 1825  	 END IF;
11:21:09 1826  
11:21:09 1827  	 BEGIN
11:21:09 1828  	   PROCS_INVOICE_V18.CALCULATE_INVOICE_AMOUNT (
11:21:09 1829  	     in_invoice_id => in_invoice_id,
11:21:09 1830  	     out_amount    => var_invoice_amount
11:21:09 1831  	   );
11:21:09 1832  	   EXCEPTION
11:21:09 1833  	     WHEN OTHERS THEN
11:21:09 1834  	       EXCEPTION_MESSAGE := SQLERRM;
11:21:09 1835  	       RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
11:21:09 1836  	 END;
11:21:09 1837  
11:21:09 1838  	 IF ( in_refund_amount > var_invoice_amount ) THEN
11:21:09 1839  	   RAISE REFUND_IS_GREATER_THAN_ANOUNT;
11:21:09 1840  	 END IF;
11:21:09 1841  
11:21:09 1842  	 SELECT /*+ STAR_TRANSFORMATION */
11:21:09 1843  	   SUM(CHARGE.CHARGE_AMOUNT) into var_refunds_before
11:21:09 1844  	 FROM
11:21:09 1845  	   CHARGE
11:21:09 1846  	 WHERE
11:21:09 1847  	   CHARGE.INVOICE_ID = in_invoice_id
11:21:09 1848  	   AND (
11:21:09 1849  	     CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V18.CHARGE_OPENED
11:21:09 1850  	     OR (
11:21:09 1851  	       CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V18.CHARGE_PROCESSED
11:21:09 1852  	       AND EXISTS (
11:21:09 1853  		 SELECT 1 FROM TRANSACTION_ATTEMPT ta where ta.transaction_id = CHARGE.TRANSACTION_ID and ta.transaction_attempt_status_id = GLOBAL_STATUSES_V18.TRANS_ATTEMPT_SUCCESS
11:21:09 1854  	       )
11:21:09 1855  	     )
11:21:09 1856  	   )
11:21:09 1857  	   AND CHARGE.CHARGE_AMOUNT < 0;
11:21:09 1858  
11:21:09 1859  	 -- Refunds are negative
11:21:09 1860  	 IF var_refunds_before IS NULL THEN var_refunds_before := 0; END IF;
11:21:09 1861  	 var_refunds_before := 0 - var_refunds_before;
11:21:09 1862  
11:21:09 1863  	 var_charges_amount := 0;
11:21:09 1864  
11:21:09 1865  	 FOR f_processed_charges IN (
11:21:09 1866  	   SELECT
11:21:09 1867  	     CHARGE.CHARGE_AMOUNT
11:21:09 1868  	   FROM
11:21:09 1869  	     CHARGE
11:21:09 1870  	   WHERE
11:21:09 1871  	     CHARGE.INVOICE_ID = in_invoice_id
11:21:09 1872  	     AND CHARGE.CHARGE_AMOUNT > 0
11:21:09 1873  	     AND CHARGE.CHARGE_STATUS_ID IN (SELECT GLOBAL_STATUSES_V18.CHARGE_PROCESSED FROM DUAL)
11:21:09 1874  	     AND EXISTS (SELECT 1 FROM TRANSACTION_ATTEMPT ta where ta.transaction_id = CHARGE.TRANSACTION_ID and ta.transaction_attempt_status_id = GLOBAL_STATUSES_V18.TRANS_ATTEMPT_SUCCESS)
11:21:09 1875  	 )
11:21:09 1876  	 LOOP
11:21:09 1877  	   var_charges_amount := var_charges_amount + f_processed_charges.CHARGE_AMOUNT;
11:21:09 1878  	 END LOOP;
11:21:09 1879  
11:21:09 1880  	 IF (in_refund_amount + var_refunds_before > var_invoice_amount) THEN
11:21:09 1881  	   RAISE TOT_REF_IS_GREATER_THAN_ANOUNT;
11:21:09 1882  	 END IF;
11:21:09 1883  
11:21:09 1884  	 IF (in_refund_amount + var_refunds_before > var_charges_amount) THEN
11:21:09 1885  	   RAISE TOT_REF_IS_GRATER_THAN_CHARGES;
11:21:09 1886  	 END IF;
11:21:09 1887  
11:21:09 1888  	 BEGIN
11:21:09 1889  	   PROCS_TRANSACTION_V18.CREATE_TRANSACTION(
11:21:09 1890  	     in_transaction_id	       => NULL,
11:21:09 1891  	     in_status_id	       => GLOBAL_STATUSES_V18.TRANSACTION_PREPARE,
11:21:09 1892  	     in_amount		       => -in_refund_amount,
11:21:09 1893  	     in_created_by	       => in_created_by,
11:21:09 1894  	     in_order_id	       => NULL,
11:21:09 1895  	     in_is_refund	       => GLOBAL_CONSTANTS_V18.TRUE,
11:21:09 1896  	     in_transaction_type_code  => 'REFUND',
11:21:09 1897  	     out_transaction_id        => var_new_transaction_id
11:21:09 1898  	   );
11:21:09 1899  	   EXCEPTION
11:21:09 1900  	     WHEN OTHERS THEN
11:21:09 1901  	       EXCEPTION_MESSAGE := SQLERRM;
11:21:09 1902  	       RAISE CAN_NOT_CREATE_TRANSACTION;
11:21:09 1903  	 END;
11:21:09 1904  
11:21:09 1905  	 BEGIN
11:21:09 1906  	   PROCS_CHARGE_V18.CREATE_CHARGE(
11:21:09 1907  	     in_invoice_id	   => in_invoice_id,
11:21:09 1908  	     in_transaction_id	   => var_new_transaction_id,
11:21:09 1909  	     in_instrument_type_id => var_instrument_type_id,
11:21:09 1910  	     in_instrument_id	   => var_instrument_id,
11:21:09 1911  	     in_charge_amount	   => -in_refund_amount,
11:21:09 1912  	     in_created_by	   => in_created_by,
11:21:09 1913  	     in_charge_status_id   => GLOBAL_STATUSES_V18.CHARGE_OPENED,
11:21:09 1914  	     out_charge_id	   => var_new_charge_id
11:21:09 1915  	   );
11:21:09 1916  	   EXCEPTION
11:21:09 1917  	     WHEN OTHERS THEN
11:21:09 1918  	       EXCEPTION_MESSAGE := SQLERRM;
11:21:09 1919  	       RAISE CAN_NOT_CREATE_CHARGE;
11:21:09 1920  	 END;
11:21:09 1921  
11:21:09 1922  	 out_charge_id := var_new_charge_id;
11:21:09 1923  
11:21:09 1924  	 IF in_note IS NOT NULL AND var_subscription_id IS NOT NULL THEN
11:21:09 1925  	   BEGIN
11:21:09 1926  	     PROCS_SUBSCRIPTION_V18.ANNOTATE_SUBSCRIPTION(
11:21:09 1927  	       in_subscription_id => var_subscription_id,
11:21:09 1928  	       in_agent_id	  => 0, -- AGENT_ID??
11:21:09 1929  	       in_note		  => in_note,
11:21:09 1930  	       in_created_by	  => in_created_by
11:21:09 1931  	     );
11:21:09 1932  	     EXCEPTION
11:21:09 1933  	       WHEN OTHERS THEN
11:21:09 1934  		 EXCEPTION_MESSAGE := SQLERRM;
11:21:09 1935  		 RAISE CAN_NOT_ANNOTATE_SUBSCRIPTION;
11:21:09 1936  	   END;
11:21:09 1937  	 END IF;
11:21:09 1938  
11:21:09 1939  EXCEPTION
11:21:09 1940  WHEN CAN_NOT_FIND_SUBSCR_OR_GC THEN
11:21:09 1941  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 1942  	   SPROC_NAME, 'Could not find subscription or GC for the inovice');
11:21:09 1943  WHEN INVOICE_IS_NOT_CLOSED THEN
11:21:09 1944  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.STATE_ERROR,
11:21:09 1945  	   SPROC_NAME, 'Invoice is not closed');
11:21:09 1946  WHEN ACCOUNT_IS_FROZEN THEN
11:21:09 1947  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.STATE_ERROR,
11:21:09 1948  	   SPROC_NAME, 'Could not refund subscription for frozen account');
11:21:09 1949  WHEN BAD_INVOICE_ID THEN
11:21:09 1950  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 1951  	   SPROC_NAME, 'No such invoice');
11:21:09 1952  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
11:21:09 1953  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 1954  	   SPROC_NAME, 'Could not calculate invoice amount', EXCEPTION_MESSAGE);
11:21:09 1955  WHEN REFUND_IS_GREATER_THAN_ANOUNT THEN
11:21:09 1956  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INVALID_PARAMETER,
11:21:09 1957  	   SPROC_NAME, 'Refund is greater than amount');
11:21:09 1958  WHEN TOT_REF_IS_GREATER_THAN_ANOUNT THEN
11:21:09 1959  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INVALID_PARAMETER,
11:21:09 1960  	   SPROC_NAME, 'There were refunds before and sum of all refunds and new refund more than invoice amount');
11:21:09 1961  WHEN TOT_REF_IS_GRATER_THAN_CHARGES THEN
11:21:09 1962  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INVALID_PARAMETER,
11:21:09 1963  	   SPROC_NAME, 'Total refund amount is greater than sum of processed charges');
11:21:09 1964  WHEN CAN_NOT_CREATE_TRANSACTION THEN
11:21:09 1965  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 1966  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
11:21:09 1967  WHEN CAN_NOT_CREATE_CHARGE THEN
11:21:09 1968  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 1969  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
11:21:09 1970  WHEN CAN_NOT_ANNOTATE_SUBSCRIPTION THEN
11:21:09 1971  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 1972  	   SPROC_NAME, 'Could not annotate subscription', EXCEPTION_MESSAGE);
11:21:09 1973  WHEN OTHERS THEN
11:21:09 1974  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:09 1975  	   SPROC_NAME, 'Unknown error', SQLERRM);
11:21:09 1976  END REFUND_INVOICE;
