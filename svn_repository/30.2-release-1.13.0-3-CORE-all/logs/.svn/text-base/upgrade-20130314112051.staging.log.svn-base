11:21:09 442  
11:21:09 443  PROCEDURE GET_ACCOUNT_GIFT_CERTIFICATES (
11:21:09 444  /*
11:21:09 445  IN:
11:21:09 446  instr_status:
11:21:09 447  GLOBAL_CONSTANTS_V18.TRUE - get active instruments only (default)
11:21:09 448  GLOBAL_CONSTANTS_V18.FALSE - get inactive instruments only
11:21:09 449  
11:21:09 450  Throws exceptions:
11:21:09 451  APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 452  APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR
11:21:09 453  */
11:21:09 454  	in_group_id	  IN NUMBER,
11:21:09 455  	out_result_gc_set OUT SYS_REFCURSOR,
11:21:09 456  	in_instr_status   IN NUMBER DEFAULT GLOBAL_CONSTANTS_V18.TRUE
11:21:09 457  ) AS
11:21:09 458  SPROC_NAME     CONSTANT VARCHAR2(29) := 'GET_ACCOUNT_GIFT_CERTIFICATES';
11:21:09 459  var_account_id NUMBER;
11:21:09 460  
11:21:09 461  -- Exceptions
11:21:09 462  WRONG_INSTR_EXCEPTION	 EXCEPTION;
11:21:09 463  BEGIN
11:21:09 464  
11:21:09 465  	-- Get account id
11:21:09 466  	SELECT
11:21:09 467  	  ACCOUNT.ID INTO var_account_id
11:21:09 468  	FROM
11:21:09 469  	  ACCOUNT
11:21:09 470  	WHERE
11:21:09 471  	  ACCOUNT.GROUP_ID = in_group_id;
11:21:09 472  
11:21:09 473  	-- Check that incoming data is correct
11:21:09 474  	IF in_instr_status != GLOBAL_CONSTANTS_V18.TRUE AND in_instr_status != GLOBAL_CONSTANTS_V18.FALSE THEN
11:21:09 475  	  RAISE WRONG_INSTR_EXCEPTION;
11:21:09 476  	END IF;
11:21:09 477  
11:21:09 478  	OPEN out_result_gc_set FOR
11:21:09 479  	SELECT
11:21:09 480  	  GIFT_CERTIFICATE.ID,
11:21:09 481  	  GIFT_CERTIFICATE.PURCHASER_GROUP_ID,
11:21:09 482  	  GIFT_CERTIFICATE.PURCHASE_INVOICE_ID,
11:21:09 483  	  GIFT_CERTIFICATE.PURCHASE_DATE,
11:21:09 484  	  GIFT_CERTIFICATE.OFFER_CHAIN_ID,
11:21:09 485  	  GIFT_CERTIFICATE.EXPIRATION_DATE,
11:21:09 486  	  GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID,
11:21:09 487  	  GIFT_CERTIFICATE.CODE,
11:21:09 488  	  GIFT_CERTIFICATE.CREATE_DATE,
11:21:09 489  	  GIFT_CERTIFICATE.CREATED_BY,
11:21:09 490  	  GIFT_CERTIFICATE.UPDATE_DATE,
11:21:09 491  	  GIFT_CERTIFICATE.UPDATED_BY,
11:21:09 492  	  GIFT_CERTIFICATE.RECIPIENT_NAME,
11:21:09 493  	  GIFT_CERTIFICATE.RECIPIENT_EMAIL,
11:21:09 494  	  GIFT_CERTIFICATE.SENDER_NAME,
11:21:09 495  	  GIFT_CERTIFICATE.SENDER_EMAIL,
11:21:09 496  	  GIFT_CERTIFICATE.REDEEMER_GROUP_ID,
11:21:09 497  	  GIFT_CERTIFICATE.REDEMPTION_DATE,
11:21:09 498  	  GIFT_CERTIFICATE.FINALIZED_INVOICE_ID,
11:21:09 499  	  GIFT_CERTIFICATE.GIFT_MESSAGE
11:21:09 500  	FROM
11:21:09 501  	  GIFT_CERTIFICATE
11:21:09 502  	WHERE
11:21:09 503  	  GIFT_CERTIFICATE.PURCHASER_GROUP_ID = in_group_id
11:21:09 504  	  AND (
11:21:09 505  		( in_instr_status = GLOBAL_CONSTANTS_V18.TRUE AND
11:21:09 506  		  (
11:21:09 507  		    GIFT_CERTIFICATE.EXPIRATION_DATE >= current_date
11:21:09 508  		    AND GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID = GLOBAL_STATUSES_V18.GIFT_CERTIFICATE_ACTIVE
11:21:09 509  		  )
11:21:09 510  		)
11:21:09 511  		OR
11:21:09 512  		(
11:21:09 513  		  in_instr_status = GLOBAL_CONSTANTS_V18.FALSE AND
11:21:09 514  		  (
11:21:09 515  		    GIFT_CERTIFICATE.EXPIRATION_DATE < current_date
11:21:09 516  		    OR GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID != GLOBAL_STATUSES_V18.GIFT_CERTIFICATE_ACTIVE
11:21:09 517  		  )
11:21:09 518  		)
11:21:09 519  	      );
11:21:09 520  
11:21:09 521  EXCEPTION
11:21:09 522  WHEN NO_DATA_FOUND THEN
11:21:09 523  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 524  	  SPROC_NAME, 'Can not find account with given group id');
11:21:09 525  WHEN WRONG_INSTR_EXCEPTION THEN
11:21:09 526  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INVALID_PARAMETER,
11:21:09 527  	  SPROC_NAME, 'Wrong gift certificate type');
11:21:09 528  WHEN OTHERS THEN
11:21:09 529  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:09 530  	  SPROC_NAME, 'Unknown error', SQLERRM);
11:21:09 531  END GET_ACCOUNT_GIFT_CERTIFICATES;
