11:21:08 935  /******************************************************************************/
11:21:08 936  
11:21:08 937  FUNCTION GET_RENEWAL_DATE (
11:21:08 938  /*
11:21:08 939  Throws exceptions:
11:21:08 940  APP_EXCEPTION_CODES_V18.NOT_FOUND
11:21:08 941  APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR
11:21:08 942  */
11:21:08 943  	in_subscription_id in NUMBER
11:21:08 944  ) RETURN DATE AS
11:21:08 945  -- VARIABLES
11:21:08 946  SPROC_NAME	      CONSTANT VARCHAR2(16) := 'GET_RENEWAL_DATE';
11:21:08 947  var_subscription_status NUMBER;
11:21:08 948  var_licenses_count      NUMBER;
11:21:08 949  var_license_end_date    DATE;
11:21:08 950  var_last_offer_id       NUMBER;
11:21:08 951  var_offer_chain_id      NUMBER;
11:21:08 952  var_last_license_id     NUMBER;
11:21:08 953  var_current_offer_index NUMBER;
11:21:08 954  var_current_offer_recurr_num NUMBER;
11:21:08 955  var_offer_recurr_num    NUMBER;
11:21:08 956  -- EXCEPTIONS
11:21:08 957  BAD_SUBSCRIPTION_ID EXCEPTION;
11:21:08 958  NO_LICENSES_FOUND EXCEPTION;
11:21:08 959  BEGIN
11:21:08 960  
11:21:08 961  	-- Get subscription id and offer chain id
11:21:08 962  	BEGIN
11:21:08 963  	  SELECT
11:21:08 964  	    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID,
11:21:08 965  	    SUBSCRIPTION.OFFER_CHAIN_ID
11:21:08 966  	    into
11:21:08 967  	    var_subscription_status,
11:21:08 968  	    var_offer_chain_id
11:21:08 969  	  FROM
11:21:08 970  	    SUBSCRIPTION
11:21:08 971  	  WHERE
11:21:08 972  	    SUBSCRIPTION.ID = in_subscription_id;
11:21:08 973  	  EXCEPTION
11:21:08 974  	    WHEN NO_DATA_FOUND THEN
11:21:08 975  	      RAISE BAD_SUBSCRIPTION_ID;
11:21:08 976  	END;
11:21:08 977  
11:21:08 978  	IF var_subscription_status != GLOBAL_STATUSES_V18.SUBSCRIPTION_ACTIVE THEN
11:21:08 979  	  -- TODO: Is suspended subscription has renewal date? (For the phase II)
11:21:08 980  	  -- AND var_subscription_status != GLOBAL_STATUSES_V18.SUBSCRIPTION_SUSPENDED THEN
11:21:08 981  	  RETURN NULL;
11:21:08 982  	END IF;
11:21:08 983  
11:21:08 984  	BEGIN
11:21:08 985  	  SELECT
11:21:08 986  	    LICENSE_ID,
11:21:08 987  	    END_DATE,
11:21:08 988  	    OFFER_ID,
11:21:08 989  	    CURRENT_OFFER_INDEX,
11:21:08 990  	    CURRENT_OFFER_RECURR_NUM
11:21:08 991  	    into
11:21:08 992  	    var_last_license_id,
11:21:08 993  	    var_license_end_date,
11:21:08 994  	    var_last_offer_id,
11:21:08 995  	    var_current_offer_index,
11:21:08 996  	    var_current_offer_recurr_num
11:21:08 997  	  FROM
11:21:08 998  	    (
11:21:08 999  	      SELECT
11:21:08 1000  		 LICENSE.ID as "LICENSE_ID",
11:21:08 1001  		 LICENSE.END_DATE,
11:21:08 1002  		 LICENSE.OFFER_ID,
11:21:08 1003  		 LICENSE.CURRENT_OFFER_INDEX,
11:21:08 1004  		 LICENSE.CURRENT_OFFER_RECURR_NUM
11:21:08 1005  	       FROM
11:21:08 1006  		 LICENSE
11:21:08 1007  	       WHERE
11:21:08 1008  		 LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V18.LICENSE_ACTIVE
11:21:08 1009  		 AND LICENSE.SUBSCRIPTION_ID = in_subscription_id
11:21:08 1010  	       ORDER BY END_DATE DESC
11:21:08 1011  	     )
11:21:08 1012  	     INNER JOIN OFFER ON OFFER_ID = OFFER.ID
11:21:08 1013  	   WHERE
11:21:08 1014  	     ROWNUM <= 1;
11:21:08 1015  
11:21:08 1016  	   EXCEPTION
11:21:08 1017  	     WHEN NO_DATA_FOUND THEN
11:21:08 1018  	       -- RAISE NO_LICENSES_FOUND;
11:21:08 1019  	       RETURN NULL;
11:21:08 1020  	 END;
11:21:08 1021  
11:21:08 1022  	 SELECT
11:21:08 1023  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES into var_offer_recurr_num
11:21:08 1024  	 FROM
11:21:08 1025  	   OFFER_OFFER_CHAIN
11:21:08 1026  	 WHERE
11:21:08 1027  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
11:21:08 1028  	   AND OFFER_OFFER_CHAIN.OFFER_ID = var_last_offer_id;
11:21:08 1029  
11:21:08 1030  	 IF PROCS_OFFER_CHAIN_V18.GET_NEXT_OFFER_INDEX(var_offer_chain_id, var_current_offer_index) IS NULL
11:21:08 1031  	   AND var_offer_recurr_num = var_current_offer_recurr_num THEN
11:21:08 1032  	   -- There is no next offer for this subscription
11:21:08 1033  	   RETURN NULL;
11:21:08 1034  	 END IF;
11:21:08 1035  
11:21:08 1036  	 RETURN var_license_end_date;
11:21:08 1037  
11:21:08 1038  EXCEPTION
11:21:08 1039  WHEN BAD_SUBSCRIPTION_ID THEN
11:21:08 1040  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:08 1041  	   SPROC_NAME, 'No such subscription');
11:21:08 1042  WHEN NO_LICENSES_FOUND THEN
11:21:08 1043  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:08 1044  	   SPROC_NAME, 'No licenses found');
11:21:08 1045  WHEN OTHERS THEN
11:21:08 1046  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:08 1047  	   SPROC_NAME, 'Unknown error', SQLERRM);
11:21:08 1048  END GET_RENEWAL_DATE;
11:21:08 1049  
11:21:08 1050  /******************************************************************************/
11:21:08 1051  
11:21:08 1052  FUNCTION GET_RECENT_CHARGE (
11:21:08 1053  /*
11:21:08 1054  Throws exceptions:
11:21:08 1055  APP_EXCEPTION_CODES_V18.NOT_FOUND
11:21:08 1056  APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR
11:21:08 1057  */
11:21:08 1058  	 in_subscription_id IN NUMBER
11:21:08 1059  ) RETURN NUMBER AS
11:21:08 1060  -- VARIABLES
11:21:08 1061  SPROC_NAME	    CONSTANT VARCHAR2(17) := 'GET_RECENT_CHARGE';
11:21:08 1062  temp_subscription_id NUMBER;
11:21:08 1063  var_recent_charge    NUMBER(10,2);
11:21:08 1064  
11:21:08 1065  -- EXCEPTIONS
11:21:08 1066  BAD_SUBSCRIPTION_ID EXCEPTION;
11:21:08 1067  BEGIN
11:21:08 1068  
11:21:08 1069  	 BEGIN
11:21:08 1070  	   SELECT
11:21:08 1071  	     SUBSCRIPTION.ID into temp_subscription_id
11:21:08 1072  	   FROM
11:21:08 1073  	     SUBSCRIPTION
11:21:08 1074  	   WHERE
11:21:08 1075  	     SUBSCRIPTION.ID = in_subscription_id;
11:21:08 1076  	   EXCEPTION
11:21:08 1077  	     WHEN NO_DATA_FOUND THEN
11:21:08 1078  	       RAISE BAD_SUBSCRIPTION_ID;
11:21:08 1079  	 END;
11:21:08 1080  
11:21:08 1081  	 BEGIN
11:21:08 1082  	   SELECT
11:21:08 1083  	     CHARGE.CHARGE_AMOUNT into var_recent_charge
11:21:08 1084  	   FROM
11:21:08 1085  	     LICENSE
11:21:08 1086  	     INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
11:21:08 1087  	     INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
11:21:08 1088  	     INNER JOIN CHARGE ON CHARGE.INVOICE_ID = INVOICE.ID
11:21:08 1089  	   WHERE
11:21:08 1090  	     -- TODO: Review
11:21:08 1091  	     -- LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V18.LICENSE_PROCESSED
11:21:08 1092  	     -- AND
11:21:08 1093  	     SUBSCRIPTION.ID = in_subscription_id
11:21:08 1094  	     AND CHARGE.CHARGE_AMOUNT >= 0
11:21:08 1095  	     AND ROWNUM <= 1
11:21:08 1096  	   ORDER BY
11:21:08 1097  	     LICENSE.ID ASC, CHARGE.ID DESC;
11:21:08 1098  	   EXCEPTION
11:21:08 1099  	     WHEN NO_DATA_FOUND THEN
11:21:08 1100  	       var_recent_charge := 0;
11:21:08 1101  	 END;
11:21:08 1102  
11:21:08 1103  	 RETURN var_recent_charge;
11:21:08 1104  
11:21:08 1105  EXCEPTION
11:21:08 1106  WHEN BAD_SUBSCRIPTION_ID THEN
11:21:08 1107  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:08 1108  	   SPROC_NAME, 'No such subscription');
11:21:08 1109  WHEN OTHERS THEN
11:21:08 1110  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:08 1111  	   SPROC_NAME, 'Unknown error', SQLERRM);
11:21:08 1112  END GET_RECENT_CHARGE;
11:21:08 1113  
11:21:08 1114  /******************************************************************************/
