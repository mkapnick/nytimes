11:21:09 623  	  AND in_invoice_status_id != GLOBAL_STATUSES_V18.INVOICE_CLOSED THEN
11:21:09 624  	  RAISE BAD_INVOICE_STATUS_ID;
11:21:09 625  	END IF;
11:21:09 626  
