11:21:09 328  
11:21:09 329  PROCEDURE REACTIVATE_ACCOUNT (
11:21:09 330  /*
11:21:09 331  Throws exceptions:
11:21:09 332  APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 333  APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:09 334  */
11:21:09 335  	in_group_id   IN NUMBER,
11:21:09 336  	in_updated_by IN VARCHAR2,
11:21:09 337  	in_note       IN VARCHAR2,
11:21:09 338  	in_agent_id   IN NUMBER
11:21:09 339  ) AS
11:21:09 340  -- VARIABLES
11:21:09 341  SPROC_NAME	      CONSTANT VARCHAR2(18) := 'REACTIVATE_ACCOUNT';
11:21:09 342  var_account_id	      NUMBER;
11:21:09 343  current_account_status  NUMBER;
11:21:09 344  
11:21:09 345  -- EXCEPTIONS
11:21:09 346  BAD_CURRENT_ACC_STATUS EXCEPTION;
11:21:09 347  CAN_NOT_CREATE_NOTE    EXCEPTION;
11:21:09 348  EXCEPTION_MESSAGE      VARCHAR2(1024);
11:21:09 349  BEGIN
11:21:09 350  	-- Get account id, status
11:21:09 351  	SELECT
11:21:09 352  	  ACCOUNT.ACCOUNT_STATUS_ID,
11:21:09 353  	  ACCOUNT.ID
11:21:09 354  	INTO
11:21:09 355  	  current_account_status,
11:21:09 356  	  var_account_id
11:21:09 357  	FROM ACCOUNT
11:21:09 358  	WHERE
11:21:09 359  	  ACCOUNT.GROUP_ID = in_group_id;
11:21:09 360  
11:21:09 361  	IF current_account_status != GLOBAL_STATUSES_V18.ACCOUNT_FROZEN THEN
11:21:09 362  	  RAISE BAD_CURRENT_ACC_STATUS;
11:21:09 363  	END IF;
11:21:09 364  
11:21:09 365  	-- Change account status
11:21:09 366  	PROCS_ACCOUNT_V18.UPDATE_ACCOUNT_STATUS(
11:21:09 367  	  in_account_id        => var_account_id,
11:21:09 368  	  in_updated_by        => in_updated_by,
11:21:09 369  	  in_account_status_id => GLOBAL_STATUSES_V18.ACCOUNT_ACTIVE
11:21:09 370  	);
11:21:09 371  
11:21:09 372  	-- Add note
11:21:09 373  	BEGIN
11:21:09 374  	  PROCS_ACCOUNT_V18.ANNOTATE_ACCOUNT(
11:21:09 375  	    in_group_id   => in_group_id,
11:21:09 376  	    in_agent_id   => in_agent_id,
11:21:09 377  	    in_note	  => in_note,
11:21:09 378  	    in_created_by => in_updated_by
11:21:09 379  	  );
11:21:09 380  	  EXCEPTION
11:21:09 381  	    WHEN OTHERS THEN
11:21:09 382  	      EXCEPTION_MESSAGE := SQLERRM;
11:21:09 383  	      RAISE CAN_NOT_CREATE_NOTE;
11:21:09 384  	END;
11:21:09 385  
11:21:09 386  EXCEPTION
11:21:09 387  WHEN BAD_CURRENT_ACC_STATUS THEN
11:21:09 388  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.STATE_ERROR,
11:21:09 389  	  SPROC_NAME, 'Current account status is not "frozen"');
11:21:09 390  WHEN NO_DATA_FOUND THEN
11:21:09 391  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 392  	  SPROC_NAME, 'Could not find account with given group ID');
11:21:09 393  WHEN CAN_NOT_CREATE_NOTE THEN
11:21:09 394  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.INTERNAL_ERROR,
11:21:09 395  	  SPROC_NAME, 'Could not annotate account');
11:21:09 396  WHEN OTHERS THEN
11:21:09 397  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:09 398  	  SPROC_NAME, 'Unknown error', SQLERRM);
11:21:09 399  END REACTIVATE_ACCOUNT;
