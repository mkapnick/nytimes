11:21:07  60  	  INNER JOIN LICENSE LIC ON LIC.SUBSCRIPTION_ID = SB.ID
11:21:07  61  	  INNER JOIN OFFER_PRODUCT_OFFERING OPO ON OPO.OFFER_ID = LIC.OFFER_ID
11:21:07  62  	  INNER JOIN PRODUCT_OFFERING PO ON PO.ID = OPO.PRODUCT_OFFERING_ID
11:21:07  63  	  INNER JOIN CAPABILITY C ON PO.CAPABILITY_ID = C.ID
11:21:07  64  	WHERE
11:21:07  65  	  LIC.ENTITLEMENT_END_DATE >= TRUNC(SYSDATE)
11:21:07  66  	  AND LIC.START_DATE <= SYSDATE
11:21:07  67  	  AND AC.GROUP_ID = in_group_id
