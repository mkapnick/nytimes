11:21:08 1121  temp_subscription_id NUMBER;
11:21:08 1122  var_offer_duration   VARCHAR2(30);
11:21:08 1123  
11:21:08 1124  -- EXCEPTIONS
11:21:08 1125  BAD_SUBSCRIPTION_ID EXCEPTION;
11:21:08 1126  BEGIN
11:21:08 1127  
11:21:08 1128  	 BEGIN
11:21:08 1129  	   SELECT
11:21:08 1130  	     SUBSCRIPTION.ID into temp_subscription_id
11:21:08 1131  	   FROM
