11:21:09  45  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_UNREDEEMED_GCS';
11:21:09  46  BEGIN
11:21:09  47  	OPEN out_result_set FOR
11:21:09  48  SELECT * FROM
11:21:09  49  (
11:21:09  50  	SELECT
11:21:09  51  	  gc.EXPIRATION_DATE,
11:21:09  52  	  ch.name,
11:21:09  53  	  ch.id offer_chain_id,
11:21:09  54  	  gc.sender_email,
11:21:09  55  	  gc.sender_name,
11:21:09  56  	  gc.recipient_email,
11:21:09  57  	  gc.recipient_name,
11:21:09  58  	  gc.purchase_date,
11:21:09  59  	  gc.redemption_date,
11:21:09  60  	  gc.purchaser_group_id,
11:21:09  61  	  gc.redeemer_group_id,
11:21:09  62  	  gc.code,
11:21:09  63  	  gc.gift_message,
11:21:09  64  	  gc.recipient_notify_date,
11:21:09  65  	  gc.id
11:21:09  66  	FROM
11:21:09  67  	  GIFT_CERTIFICATE gc,
11:21:09  68  	  OFFER_CHAIN ch
11:21:09  69  	WHERE
11:21:09  70  	  ch.id = gc.offer_chain_id
11:21:09  71  	  AND gc.RECIPIENT_NOTIFY_DATE is not null
11:21:09  72  	  AND gc.RECIPIENT_NOTIFY_DATE >= (sysdate - in_hours_number/24)
11:21:09  73  	  AND gc.RECIPIENT_NOTIFY_DATE < (sysdate - (in_hours_number-72)/24)
11:21:09  74  	  AND gc.redeemer_group_id is null
11:21:09  75  	  AND NOT EXISTS(
11:21:09  76  	    SELECT NULL
11:21:09  77  	    FROM PROCESS_RETRY_THROTTLE
11:21:09  78  	    WHERE PROCESS_NAME = in_process_name
11:21:09  79  	      AND GENERIC_ID = gc.id
11:21:09  80  	  ) AND EXISTS(
11:21:09  81  	    SELECT NULL
11:21:09  82  	    FROM
11:21:09  83  	      charge c,
11:21:09  84  	      transaction_attempt ta,
11:21:09  85  	      transaction t
11:21:09  86  	    WHERE
11:21:09  87  	      c.invoice_id = gc.purchase_invoice_id and
11:21:09  88  	      c.transaction_id = t.id and
