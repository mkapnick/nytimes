11:21:07 972  var_offer_duration VARCHAR2(30);
11:21:07 973  var_offer_years	 NUMBER;
11:21:07 974  var_offer_months	 NUMBER;
11:21:07 975  var_offer_days	 NUMBER;
11:21:07 976  var_offer_end_date DATE;
11:21:07 977  -- EXCEPTIONS
11:21:07 978  BAD_OFFER_ID EXCEPTION;
11:21:07 979  BEGIN
11:21:07 980  
11:21:07 981  	BEGIN
11:21:07 982  	  SELECT
11:21:07 983  	    OFFER.ENTITLEMENT_DURATION into var_offer_duration
11:21:07 984  	  FROM
11:21:07 985  	    OFFER
11:21:07 986  	  WHERE
11:21:07 987  	    OFFER.ID = in_offer_id;
11:21:07 988  	  EXCEPTION
11:21:07 989  	    WHEN NO_DATA_FOUND THEN
11:21:07 990  	      RAISE BAD_OFFER_ID;
11:21:07 991  	END;
11:21:07 992  
11:21:07 993  	PROCS_COMMON_V18.ISO8601DURATION_TO_INTERVALS (
11:21:07 994  	  var_offer_duration,
11:21:07 995  	  var_offer_years,
11:21:07 996  	  var_offer_months,
11:21:07 997  	  var_offer_days
11:21:07 998  	);
11:21:07 999  
11:21:07 1000  	 var_offer_end_date := ( ( in_start_date
11:21:07 1001  	   + GLOBAL_CONSTANTS_V18.ONE_DAY_INTERVAL * var_offer_days )
11:21:07 1002  	   + GLOBAL_CONSTANTS_V18.ONE_MONTH_INTERVAL * var_offer_months )
11:21:07 1003  	   + GLOBAL_CONSTANTS_V18.ONE_YEAR_INTERVAL * var_offer_years;
11:21:07 1004  
11:21:07 1005  	 out_days := var_offer_end_date - in_start_date;
11:21:07 1006  
11:21:07 1007  EXCEPTION
11:21:07 1008  WHEN BAD_OFFER_ID THEN
11:21:07 1009  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:07 1010  	   SPROC_NAME, 'No such offer');
11:21:07 1011  WHEN OTHERS THEN
11:21:07 1012  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:07 1013  	   SPROC_NAME, 'Unknown error', SQLERRM);
11:21:07 1014  END GET_OFFER_LENGTH_IN_DAYS;
