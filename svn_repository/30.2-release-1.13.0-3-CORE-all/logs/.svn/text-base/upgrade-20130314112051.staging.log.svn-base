11:21:09 1453  PROCEDURE GET_GROUPS_ID_BY_INVOICE_ID (
11:21:09 1454  /*
11:21:09 1455  This procedure is using for LOCKING only
11:21:09 1456  
11:21:09 1457  Throws exceptions:
11:21:09 1458  APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 1459  APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR
11:21:09 1460  */
11:21:09 1461  	 in_invoice_id IN NUMBER,
11:21:09 1462  	 out_group_ids OUT SYS_REFCURSOR
11:21:09 1463  ) AS
11:21:09 1464  SPROC_NAME CONSTANT VARCHAR2(27) := 'GET_GROUPS_ID_BY_INVOICE_ID';
11:21:09 1465  -- VARIABLES
11:21:09 1466  temp_invoice_id		 NUMBER;
11:21:09 1467  var_subscrib_group_id	 NUMBER;
11:21:09 1468  var_gc_purchaser_group_id NUMBER;
11:21:09 1469  var_gc_redeemer_group_id  NUMBER;
11:21:09 1470  -- EXCEPTIONS
11:21:09 1471  BAD_INVOICE_ID	    EXCEPTION;
11:21:09 1472  CAN_NOT_FIND_ACCOUNT EXCEPTION;
11:21:09 1473  BEGIN
11:21:09 1474  
11:21:09 1475  	 BEGIN
11:21:09 1476  	   SELECT
11:21:09 1477  	     INVOICE.ID into temp_invoice_id
11:21:09 1478  	   FROM
11:21:09 1479  	     INVOICE
11:21:09 1480  	   WHERE
11:21:09 1481  	     INVOICE.ID = in_invoice_id;
11:21:09 1482  	   EXCEPTION
11:21:09 1483  	     WHEN NO_DATA_FOUND THEN
11:21:09 1484  	       RAISE BAD_INVOICE_ID;
11:21:09 1485  	 END;
11:21:09 1486  
11:21:09 1487  	 BEGIN
11:21:09 1488  	   SELECT DISTINCT
11:21:09 1489  	     ACCOUNT.GROUP_ID into var_subscrib_group_id
11:21:09 1490  	   FROM
11:21:09 1491  	     ACCOUNT
11:21:09 1492  	     INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.ACCOUNT_ID = ACCOUNT.ID
11:21:09 1493  	     INNER JOIN LICENSE ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
11:21:09 1494  	   WHERE
11:21:09 1495  	     LICENSE.INVOICE_ID = in_invoice_id;
11:21:09 1496  	   EXCEPTION
11:21:09 1497  	     WHEN NO_DATA_FOUND THEN
11:21:09 1498  	       var_subscrib_group_id := NULL;
11:21:09 1499  	 END;
11:21:09 1500  
11:21:09 1501  	 IF var_subscrib_group_id IS NULL THEN
11:21:09 1502  	   BEGIN
11:21:09 1503  	     SELECT
11:21:09 1504  	       GIFT_CERTIFICATE.PURCHASER_GROUP_ID,
11:21:09 1505  	       GIFT_CERTIFICATE.REDEEMER_GROUP_ID
11:21:09 1506  	       into
11:21:09 1507  	       var_gc_purchaser_group_id,
11:21:09 1508  	       var_gc_redeemer_group_id
11:21:09 1509  	     FROM
11:21:09 1510  	       GIFT_CERTIFICATE
11:21:09 1511  	     WHERE
11:21:09 1512  	       GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = in_invoice_id
11:21:09 1513  	       OR GIFT_CERTIFICATE.FINALIZED_INVOICE_ID = in_invoice_id;
11:21:09 1514  	     EXCEPTION
11:21:09 1515  	       WHEN NO_DATA_FOUND THEN
11:21:09 1516  		 var_gc_purchaser_group_id := NULL;
11:21:09 1517  		 var_gc_redeemer_group_id  := NULL;
11:21:09 1518  	   END;
11:21:09 1519  	 END IF;
11:21:09 1520  
11:21:09 1521  	 IF var_subscrib_group_id IS NULL
11:21:09 1522  	   AND var_gc_purchaser_group_id IS NULL
11:21:09 1523  	   AND var_gc_redeemer_group_id IS NULL THEN
11:21:09 1524  	     RAISE CAN_NOT_FIND_ACCOUNT;
11:21:09 1525  	 END IF;
11:21:09 1526  
11:21:09 1527  	 OPEN out_group_ids FOR
11:21:09 1528  	 SELECT GROUP_ID FROM (
11:21:09 1529  	   SELECT
11:21:09 1530  	     var_subscrib_group_id as "GROUP_ID"
11:21:09 1531  	   FROM
11:21:09 1532  	     DUAL
11:21:09 1533  	   UNION
11:21:09 1534  	   SELECT
11:21:09 1535  	     var_gc_purchaser_group_id as "GROUP_ID"
11:21:09 1536  	   FROM
11:21:09 1537  	     DUAL
11:21:09 1538  	   UNION
11:21:09 1539  	   SELECT
11:21:09 1540  	     var_gc_redeemer_group_id as "GROUP_ID"
11:21:09 1541  	   FROM
11:21:09 1542  	     DUAL
11:21:09 1543  	 )
11:21:09 1544  	 WHERE
11:21:09 1545  	   GROUP_ID IS NOT NULL;
11:21:09 1546  
11:21:09 1547  EXCEPTION
11:21:09 1548  WHEN BAD_INVOICE_ID THEN
11:21:09 1549  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 1550  	   SPROC_NAME, 'No such invoice');
11:21:09 1551  WHEN CAN_NOT_FIND_ACCOUNT THEN
11:21:09 1552  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 1553  	   SPROC_NAME, 'Could not find account for given invoice');
11:21:09 1554  WHEN OTHERS THEN
11:21:09 1555  	 PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.UNKNOWN_ERROR,
11:21:09 1556  	   SPROC_NAME, 'Unknown error', SQLERRM);
11:21:09 1557  END GET_GROUPS_ID_BY_INVOICE_ID;
