11:21:09 637  WHEN BAD_INVOICE_ID THEN
11:21:09 638  	PROCS_COMMON_V18.THROW_EXCEPTION(APP_EXCEPTION_CODES_V18.NOT_FOUND,
11:21:09 639  	  SPROC_NAME, 'No such invoice');
