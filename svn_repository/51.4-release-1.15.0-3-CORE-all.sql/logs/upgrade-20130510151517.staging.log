15:15:36 160  BEGIN
15:15:36 161  
15:15:36 162  	BEGIN
15:15:36 163  	  SELECT
15:15:36 164  	    ID into temp_license_id
15:15:36 165  	  FROM
15:15:36 166  	    LICENSE
15:15:36 167  	  WHERE
15:15:36 168  	    LICENSE.ID = in_license_id;
15:15:36 169  	  EXCEPTION
15:15:36 170  	    WHEN NO_DATA_FOUND THEN
15:15:36 171  	      RAISE BAD_LICENSE_ID;
15:15:36 172  	END;
15:15:36 173  
15:15:36 174  	IF in_license_status != GLOBAL_STATUSES_V21.LICENSE_CLOSED
15:15:36 175  	   AND in_license_status != GLOBAL_STATUSES_V21.LICENSE_ACTIVE
15:15:36 176  	   AND in_license_status != GLOBAL_STATUSES_V21.LICENSE_IN_GRACE_PERIOD THEN
15:15:36 177  	  RAISE BAD_LICENSE_STATUS;
15:15:36 178  	END IF;
15:15:36 179  
15:15:36 180  	IF (in_ent_end is not null and in_ent_end = GLOBAL_CONSTANTS_V21.TRUE) then
15:15:36 181  	  PROCS_LICENSE_CRU_V21.UPDATE_LICENSE(
15:15:36 182  	    in_license_id	 => in_license_id,
15:15:36 183  	    in_updated_by	 => in_updated_by,
