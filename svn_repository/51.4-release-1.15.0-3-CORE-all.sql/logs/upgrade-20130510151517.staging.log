15:15:36 421  	  AND CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V21.INSTRUMENT_GIFT_CERTIFICATE;
15:15:36 422  
15:15:36 423  EXCEPTION
15:15:36 424  WHEN BAD_INVOICE_ID THEN
15:15:36 425  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
