15:15:37 1414  	 out_subscription_id		 OUT NUMBER,
15:15:37 1415  	 out_license_id 		 OUT NUMBER
15:15:37 1416  ) AS
15:15:37 1417  SPROC_NAME	       CONSTANT VARCHAR2(23) := 'REDEEM_GIFT_CERTIFICATE';
15:15:37 1418  -- VARIABLES
15:15:37 1419  var_gift_certificate_id NUMBER;
15:15:37 1420  -- norlov: #38151 var_offer_chain_id replaced by var_purchased_oc_id and var_oc_id_to_redeem
15:15:37 1421  var_purchased_oc_id     NUMBER;
15:15:37 1422  var_oc_id_to_redeem     NUMBER := in_redemption_offer_chain_id;
15:15:37 1423  var_offer_duration      VARCHAR2(30);
15:15:37 1424  var_invoice_id	       NUMBER;
15:15:37 1425  var_succ_purch_attempts_num NUMBER;
15:15:37 1426  var_subscription_id     NUMBER;
