15:15:36 516  /******************************************************************************/
15:15:36 517  
15:15:36 518  PROCEDURE SUSPEND_SUBSCRIPTION(
15:15:36 519  /*
15:15:36 520  Throws exceptions:
15:15:36 521  APP_EXCEPTION_CODES_V21.NOT_FOUND
15:15:36 522  APP_EXCEPTION_CODES_V21.INTERNAL_ERROR
15:15:36 523  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
15:15:36 524  */
15:15:36 525  	  in_subs_id	IN NUMBER,
15:15:36 526  	  in_updated_by IN VARCHAR2
15:15:36 527  ) AS
15:15:36 528  SPROC_NAME		  CONSTANT VARCHAR2(20) := 'SUSPEND_SUBSCRIPTION';
15:15:36 529  var_subscription_status_id  NUMBER;
15:15:36 530  var_license_id		  NUMBER;
15:15:36 531  var_offer_id		  NUMBER;
15:15:36 532  var_license_start_date	  DATE;
15:15:36 533  var_license_end_date	  DATE;
15:15:36 534  
15:15:36 535  var_entitlement_dupration   VARCHAR2(30);
15:15:36 536  var_d_entitlement_dupration NUMBER;
15:15:36 537  
15:15:36 538  var_ym_interval INTERVAL YEAR TO MONTH;
15:15:36 539  var_ds_interval INTERVAL DAY(3) TO SECOND;
15:15:36 540  
15:15:36 541  -- EXCEPTIONS
15:15:36 542  BAD_SUBSCRIPTION_ID     EXCEPTION;
15:15:36 543  BAD_SUBSCRIPTION_STATUS EXCEPTION;
15:15:36 544  NO_LICENSE_FOUND	      EXCEPTION;
15:15:36 545  NO_OFFER_FOUND	      EXCEPTION;
15:15:36 546  EXCEPTION_MESSAGE       VARCHAR2(1024);
15:15:36 547  BEGIN
15:15:36 548  	-- TODO: Finish this prcedure (in Phase II)
15:15:36 549  
15:15:36 550  	-- Get subscription by id. FAULT if no such subscription.
15:15:36 551  	-- begin TX
15:15:36 552  	--   Get for update associated license (subscription.license_id). FAULT if not found.
15:15:36 553  	--   Set status to PROCESSED.
15:15:36 554  	--   updated record.
15:15:36 555  	--   compute days remaining in the subscription: original end_date - today = days_remaining_adjustment
15:15:36 556  	--   new subscription status is SUSPENDED.
15:15:36 557  	--   suspend_date is now.
15:15:36 558  	--   update subscription record.
15:15:36 559  	-- end TX
15:15:36 560  
15:15:36 561  	BEGIN
15:15:36 562  	  SELECT
15:15:36 563  	    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID into var_subscription_status_id
15:15:36 564  	  FROM
15:15:36 565  	    SUBSCRIPTION
15:15:36 566  	  WHERE
15:15:36 567  	    SUBSCRIPTION.ID = in_subs_id;
15:15:36 568  	  EXCEPTION
15:15:36 569  	  WHEN NO_DATA_FOUND THEN
15:15:36 570  	    RAISE BAD_SUBSCRIPTION_ID;
15:15:36 571  	END;
15:15:36 572  
15:15:36 573  	IF var_subscription_status_id != GLOBAL_STATUSES_V21.SUBSCRIPTION_ACTIVE THEN
15:15:36 574  	  RAISE BAD_SUBSCRIPTION_STATUS;
15:15:36 575  	END IF;
15:15:36 576  
15:15:36 577  	BEGIN
15:15:36 578  	  SELECT
15:15:36 579  	    LICENSE.ID,
15:15:36 580  	    LICENSE.OFFER_ID,
15:15:36 581  	    LICENSE.START_DATE
15:15:36 582  	    into
15:15:36 583  	    var_license_id,
15:15:36 584  	    var_offer_id,
15:15:36 585  	    var_license_start_date
15:15:36 586  	  FROM
15:15:36 587  	    LICENSE
15:15:36 588  	  WHERE
15:15:36 589  	    LICENSE.SUBSCRIPTION_ID = in_subs_id
15:15:36 590  	      AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V21.LICENSE_ACTIVE;
15:15:36 591  	  EXCEPTION
15:15:36 592  	  WHEN NO_DATA_FOUND THEN
15:15:36 593  	    RAISE NO_LICENSE_FOUND;
15:15:36 594  	END;
15:15:36 595  
15:15:36 596  	PROCS_LICENSE_CRU_V21.UPDATE_LICENSE(
15:15:36 597  	  in_license_id        => var_license_id,
15:15:36 598  	  in_updated_by        => in_updated_by,
15:15:36 599  	  in_license_status_id => GLOBAL_STATUSES_V21.LICENSE_CLOSED
15:15:36 600  	);
15:15:36 601  
15:15:36 602  	BEGIN
15:15:36 603  	  SELECT
15:15:36 604  	    OFFER.ENTITLEMENT_DURATION into var_entitlement_dupration
15:15:36 605  	  FROM
15:15:36 606  	    OFFER
15:15:36 607  	  WHERE
15:15:36 608  	    OFFER.ID = var_offer_id;
15:15:36 609  	  EXCEPTION
15:15:36 610  	  WHEN NO_DATA_FOUND THEN
15:15:36 611  	    RAISE NO_OFFER_FOUND;
15:15:36 612  	END;
15:15:36 613  
15:15:36 614  	var_ym_interval := substr(var_entitlement_dupration, 0, 4);
15:15:36 615  	var_ds_interval := substr(var_entitlement_dupration, 4);
15:15:36 616  
15:15:36 617  	var_license_end_date := var_license_start_date + var_ym_interval + var_ds_interval;
15:15:36 618  
15:15:36 619  	var_d_entitlement_dupration := var_license_end_date - current_date;
15:15:36 620  
15:15:36 621  	PROCS_SUBSCRIPTION_CRU_V21.UPDATE_SUBSCRIPTION(
15:15:36 622  	  in_subscription_id	       => in_subs_id,
15:15:36 623  	  in_updated_by 	       => in_updated_by,
15:15:36 624  	  in_suspend_date	       => SYSDATE,
15:15:36 625  	  in_subscription_status_id    => GLOBAL_STATUSES_V21.SUBSCRIPTION_SUSPENDED,
15:15:36 626  	  in_days_remainning_ajustment => var_d_entitlement_dupration
15:15:36 627  	);
15:15:36 628  
15:15:36 629  EXCEPTION
15:15:36 630  WHEN BAD_SUBSCRIPTION_ID THEN
15:15:36 631  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 632  	  SPROC_NAME, 'No such subscription');
15:15:36 633  WHEN BAD_SUBSCRIPTION_STATUS THEN
15:15:36 634  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
15:15:36 635  	  SPROC_NAME, 'Subscription is not active');
15:15:36 636  WHEN NO_LICENSE_FOUND THEN
15:15:36 637  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 638  	  SPROC_NAME, 'Can not find license associated with given subscription ID');
15:15:36 639  WHEN NO_OFFER_FOUND THEN
15:15:36 640  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 641  	  SPROC_NAME, 'Can not find offer associated with given subscription ID');
15:15:36 642  WHEN OTHERS THEN
15:15:36 643  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 644  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:15:36 645  END SUSPEND_SUBSCRIPTION;
