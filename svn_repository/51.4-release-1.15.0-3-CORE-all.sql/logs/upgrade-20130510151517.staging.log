15:15:37 462  SPROC_NAME CONSTANT VARCHAR2(14) := 'DISABLE_PAYPAL';
15:15:37 463  -- VARIABLES
15:15:37 464  var_paypal_status_id NUMBER;
15:15:37 465  var_account_id	   NUMBER;
15:15:37 466  var_group_id	   NUMBER;
