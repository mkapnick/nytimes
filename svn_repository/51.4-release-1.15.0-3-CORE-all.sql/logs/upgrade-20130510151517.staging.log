15:15:36 1851  	       RAISE CAN_NOT_CREATE_TRANSACTION;
15:15:36 1852  	 END;
15:15:36 1853  
15:15:36 1854  	 BEGIN
15:15:36 1855  	   PROCS_CHARGE_V21.CREATE_CHARGE(
15:15:36 1856  	     in_invoice_id	   => in_invoice_id,
15:15:36 1857  	     in_transaction_id	   => var_new_transaction_id,
15:15:36 1858  	     in_instrument_type_id => var_instrument_type_id,
15:15:36 1859  	     in_instrument_id	   => var_instrument_id,
15:15:36 1860  	     in_charge_amount	   => -in_refund_amount,
15:15:36 1861  	     in_created_by	   => in_created_by,
15:15:36 1862  	     in_charge_status_id   => GLOBAL_STATUSES_V21.CHARGE_OPENED,
15:15:36 1863  	     out_charge_id	   => var_new_charge_id
15:15:36 1864  	   );
15:15:36 1865  	   EXCEPTION
15:15:36 1866  	     WHEN OTHERS THEN
15:15:36 1867  	       EXCEPTION_MESSAGE := SQLERRM;
15:15:36 1868  	       RAISE CAN_NOT_CREATE_CHARGE;
15:15:36 1869  	 END;
15:15:36 1870  
15:15:36 1871  	 out_charge_id := var_new_charge_id;
15:15:36 1872  
15:15:36 1873  	 IF in_note IS NOT NULL AND var_subscription_id IS NOT NULL THEN
15:15:36 1874  	   BEGIN
15:15:36 1875  	     PROCS_SUBSCRIPTION_V21.ANNOTATE_SUBSCRIPTION(
15:15:36 1876  	       in_subscription_id => var_subscription_id,
15:15:36 1877  	       in_agent_id	  => 0, -- AGENT_ID??
15:15:36 1878  	       in_note		  => in_note,
15:15:36 1879  	       in_created_by	  => in_created_by
15:15:36 1880  	     );
15:15:36 1881  	     EXCEPTION
15:15:36 1882  	       WHEN OTHERS THEN
15:15:36 1883  		 EXCEPTION_MESSAGE := SQLERRM;
15:15:36 1884  		 RAISE CAN_NOT_ANNOTATE_SUBSCRIPTION;
15:15:36 1885  	   END;
15:15:36 1886  	 END IF;
15:15:36 1887  
15:15:36 1888  EXCEPTION
