15:15:36 1770  	 END IF;
15:15:36 1771  
15:15:36 1772  	 IF var_invoice_status_id != GLOBAL_STATUSES_V21.INVOICE_CLOSED THEN
15:15:36 1773  	   RAISE INVOICE_IS_NOT_CLOSED;
15:15:36 1774  	 END IF;
15:15:36 1775  
15:15:36 1776  	 BEGIN
15:15:36 1777  	   PROCS_INVOICE_V21.CALCULATE_INVOICE_AMOUNT (
15:15:36 1778  	     in_invoice_id => in_invoice_id,
15:15:36 1779  	     out_amount    => var_invoice_amount
15:15:36 1780  	   );
15:15:36 1781  	   EXCEPTION
15:15:36 1782  	     WHEN OTHERS THEN
15:15:36 1783  	       EXCEPTION_MESSAGE := SQLERRM;
15:15:36 1784  	       RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
15:15:36 1785  	 END;
15:15:36 1786  
15:15:36 1787  	 IF ( in_refund_amount > var_invoice_amount ) THEN
15:15:36 1788  	   RAISE REFUND_IS_GREATER_THAN_ANOUNT;
15:15:36 1789  	 END IF;
15:15:36 1790  
15:15:36 1791  	 SELECT /*+ STAR_TRANSFORMATION */
