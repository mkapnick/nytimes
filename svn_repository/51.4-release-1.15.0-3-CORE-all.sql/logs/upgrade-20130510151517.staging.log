15:15:36 147  PROCEDURE UPDATE_LICENSE_STATUS(
15:15:36 148  	  in_license_id     IN NUMBER,
15:15:36 149  	  in_license_status IN NUMBER,
15:15:36 150  	  in_updated_by     IN VARCHAR2,
15:15:36 151  	  in_ent_end	    IN NUMBER DEFAULT GLOBAL_CONSTANTS_V21.FALSE
15:15:36 152  ) AS
15:15:36 153  SPROC_NAME CONSTANT VARCHAR2(21) := 'UPDATE_LICENSE_STATUS';
15:15:36 154  -- VARIABLES
15:15:36 155  temp_license_id NUMBER;
15:15:36 156  -- EXCEPTIONS
15:15:36 157  BAD_LICENSE_ID	     EXCEPTION;
15:15:36 158  BAD_LICENSE_STATUS     EXCEPTION;
