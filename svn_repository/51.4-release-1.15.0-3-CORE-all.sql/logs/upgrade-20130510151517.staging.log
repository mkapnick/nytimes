15:15:36 697  PROCEDURE GET_ENDED_GC_LICENSES (
15:15:36 698  	out_result_set		OUT SYS_REFCURSOR,
15:15:36 699  	in_hours_number 	IN NUMBER DEFAULT 14*24,
15:15:36 700  	in_num_rows		IN NUMBER DEFAULT 10000,
15:15:36 701  	in_process_name IN PROCESS_RETRY_THROTTLE.PROCESS_NAME%TYPE
15:15:36 702  ) AS
15:15:36 703  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_ENDED_GC_INVOICES';
15:15:36 704  var_days		NUMBER;
15:15:36 705  var_hours 	NUMBER;
15:15:36 706  var_time_interval INTERVAL DAY (3) TO SECOND;
15:15:36 707  BEGIN
15:15:36 708  	var_hours := mod(in_hours_number,24);
15:15:36 709  	var_days := (in_hours_number - var_hours) / 24;
15:15:36 710  	var_time_interval := var_days||' '||var_hours||':0:0';
15:15:36 711  	OPEN out_result_set FOR
15:15:36 712  SELECT * FROM
15:15:36 713  (
15:15:36 714  	SELECT
15:15:36 715  	  l.ID,
15:15:36 716  	  l.CREATE_DATE,
15:15:36 717  	  l.CREATED_BY,
15:15:36 718  	  l.CURRENT_OFFER_INDEX,
15:15:36 719  	  l.CURRENT_OFFER_RECURR_NUM,
15:15:36 720  	  l.END_DATE,
15:15:36 721  	  l.ENTITLEMENT_END_DATE,
15:15:36 722  	  l.INVOICE_ID,
15:15:36 723  	  l.IS_EXTENSION,
15:15:36 724  	  l.LICENSE_STATUS_ID,
15:15:36 725  	  l.OFFER_ID,
15:15:36 726  	  l.START_DATE,
15:15:36 727  	  l.SUBSCRIPTION_ID,
15:15:36 728  	  l.UPDATE_DATE,
15:15:36 729  	  l.UPDATED_BY
15:15:36 730  	FROM
15:15:36 731  	  GIFT_CERTIFICATE gc
15:15:36 732  	  INNER JOIN INVOICE i ON i.id = gc.PURCHASE_INVOICE_ID
15:15:36 733  	  INNER JOIN LICENSE l ON l.invoice_id = i.id
15:15:36 734  	  LEFT JOIN SUBSCRIPTION s ON s.id = l.subscription_id
15:15:36 735  	WHERE
15:15:36 736  	  l.LICENSE_STATUS_ID != GLOBAL_STATUSES_V21.LICENSE_ACTIVE
15:15:36 737  	  AND l.ENTITLEMENT_END_DATE <= (current_timestamp)
15:15:36 738  	  AND l.ENTITLEMENT_END_DATE > (current_timestamp - var_time_interval)
15:15:36 739  	  AND s.subscription_status_id = GLOBAL_STATUSES_V21.SUBSCRIPTION_CLOSED
15:15:36 740  	  AND NOT EXISTS (
15:15:36 741  	    SELECT NULL
15:15:36 742  	    FROM PROCESS_RETRY_THROTTLE
15:15:36 743  	    WHERE PROCESS_NAME = in_process_name
15:15:36 744  	      AND GENERIC_ID = l.ID
15:15:36 745  	  )
15:15:36 746  	  AND ROWNUM <= in_num_rows*10
15:15:36 747  	  ORDER BY dbms_random.value
15:15:36 748  ) WHERE
15:15:36 749  	  ROWNUM <= in_num_rows
15:15:36 750  	  GROUP BY SUBSCRIPTION_ID;
15:15:36 751  EXCEPTION
15:15:36 752  WHEN OTHERS THEN
15:15:36 753  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 754  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:15:36 755  END GET_ENDED_GC_LICENSES;
