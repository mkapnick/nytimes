15:15:36 1593  	     in_subscription_id => in_subscription_id,
15:15:36 1594  	     in_agent_id	=> 0, -- FIXME: What should to be here (agent id)?
15:15:36 1595  	     in_note		=> in_note,
15:15:36 1596  	     in_created_by	=> in_updated_by
15:15:36 1597  	   );
15:15:36 1598  	   EXCEPTION
15:15:36 1599  	     WHEN OTHERS THEN
