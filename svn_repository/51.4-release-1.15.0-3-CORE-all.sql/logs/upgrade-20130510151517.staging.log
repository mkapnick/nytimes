15:15:36 1988  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 1989  	   SPROC_NAME, 'No such invoice');
15:15:36 1990  WHEN NO_DATA_FOUND THEN
15:15:36 1991  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
