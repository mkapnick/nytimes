15:15:36 155  PROCEDURE CALCULATE_INVOICE_AMOUNT (
15:15:36 156  /*
15:15:36 157  Throws exceptions:
15:15:36 158  APP_EXCEPTION_CODES_V21.NOT_FOUND
15:15:36 159  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
15:15:36 160  */
15:15:36 161  	in_invoice_id IN  NUMBER,
15:15:36 162  	out_amount    OUT NUMBER
15:15:36 163  ) AS
15:15:36 164  -- VARIABLES
15:15:36 165  SPROC_NAME	     CONSTANT VARCHAR2(24) := 'CALCULATE_INVOICE_AMOUNT';
15:15:36 166  temp_invoice_id	     NUMBER;
15:15:36 167  var_total_amount	     NUMBER(10,6);
15:15:36 168  var_final_amount	     NUMBER(10,2);
15:15:36 169  var_line_item_amount   NUMBER(10,6);
15:15:36 170  
15:15:36 171  var_line_items_set     SYS_REFCURSOR;
15:15:36 172  var_line_item_id	     NUMBER;
15:15:36 173  var_line_item_quantity NUMBER;
15:15:36 174  var_line_item_price    NUMBER (10,2);
15:15:36 175  
15:15:36 176  var_discount_fixed_amount NUMBER (10,2);
15:15:36 177  var_discount_percent_amount NUMBER (10,2);
15:15:36 178  
15:15:36 179  -- EXCEPTIONS
15:15:36 180  BAD_INVOICE_ID		    EXCEPTION;
15:15:36 181  CAN_NOT_CALC_LINE_ITEM_AMOUNT EXCEPTION;
15:15:36 182  EXCEPTION_MESSAGE VARCHAR2(1024);
15:15:36 183  BEGIN
15:15:36 184  
15:15:36 185  	var_total_amount := 0;
15:15:36 186  
15:15:36 187  	-- Check that given invoice exists
15:15:36 188  	BEGIN
15:15:36 189  	  SELECT
15:15:36 190  	    INVOICE.ID into temp_invoice_id
15:15:36 191  	  FROM
15:15:36 192  	    INVOICE
15:15:36 193  	  WHERE
15:15:36 194  	    INVOICE.ID = in_invoice_id
15:15:36 195  	    AND ROWNUM <= 1;
15:15:36 196  	  EXCEPTION
15:15:36 197  	    WHEN NO_DATA_FOUND THEN
15:15:36 198  	      RAISE BAD_INVOICE_ID;
15:15:36 199  	END;
15:15:36 200  
15:15:36 201  	-- Calculate amount for each line item in invoice
15:15:36 202  	FOR f_line_item IN (
15:15:36 203  	  SELECT
15:15:36 204  	    LINE_ITEM.ID
15:15:36 205  	  FROM
15:15:36 206  	    LINE_ITEM
15:15:36 207  	  WHERE
15:15:36 208  	    LINE_ITEM.INVOICE_ID = in_invoice_id
15:15:36 209  	)
15:15:36 210  	LOOP
15:15:36 211  	  BEGIN
15:15:36 212  	    PROCS_LINE_ITEMS_V21.CALCULATE_LINE_ITEM_AMOUNT(
15:15:36 213  	      in_line_item_id => f_line_item.ID,
15:15:36 214  	      out_amount      => var_line_item_amount
15:15:36 215  	    );
15:15:36 216  	    var_total_amount := var_total_amount + var_line_item_amount;
15:15:36 217  	    EXCEPTION
15:15:36 218  	      WHEN OTHERS THEN
15:15:36 219  		EXCEPTION_MESSAGE := SQLERRM;
15:15:36 220  		RAISE CAN_NOT_CALC_LINE_ITEM_AMOUNT;
15:15:36 221  	  END;
15:15:36 222  	END LOOP;
15:15:36 223  	var_final_amount := var_total_amount;
15:15:36 224  	out_amount := var_final_amount;
15:15:36 225  
15:15:36 226  EXCEPTION
15:15:36 227  WHEN BAD_INVOICE_ID THEN
15:15:36 228  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 229  	  SPROC_NAME, 'No such invoice');
15:15:36 230  WHEN CAN_NOT_CALC_LINE_ITEM_AMOUNT THEN
15:15:36 231  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:36 232  	  SPROC_NAME, 'Could not calculate line item amount', EXCEPTION_MESSAGE);
15:15:36 233  WHEN OTHERS THEN
15:15:36 234  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 235  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:15:36 236  END;
