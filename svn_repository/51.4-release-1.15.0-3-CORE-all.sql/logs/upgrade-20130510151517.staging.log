15:15:36 189  	    in_license_id	 => in_license_id,
15:15:36 190  	    in_updated_by	 => in_updated_by,
15:15:36 191  	    in_license_status_id => in_license_status
15:15:36 192  	  );
15:15:36 193  	END IF;
15:15:36 194  
15:15:36 195  EXCEPTION
15:15:36 196  WHEN BAD_LICENSE_STATUS THEN
