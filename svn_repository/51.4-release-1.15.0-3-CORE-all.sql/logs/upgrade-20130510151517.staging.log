15:15:36 1335  
15:15:36 1336  	 BEGIN
15:15:36 1337  	   SELECT
15:15:36 1338  	     INVOICE.ID into temp_invoice_id
15:15:36 1339  	   FROM
15:15:36 1340  	     INVOICE
15:15:36 1341  	   WHERE
15:15:36 1342  	     INVOICE.ID = in_invoice_id;
15:15:36 1343  	   EXCEPTION
15:15:36 1344  	     WHEN NO_DATA_FOUND THEN
15:15:36 1345  	       RAISE BAD_INVOICE_ID;
15:15:36 1346  	 END;
15:15:36 1347  
15:15:36 1348  	 -- Needs to close charges. No refund.
15:15:36 1349  	 FOR f_charge_to_close IN (
15:15:36 1350  	   SELECT CHARGE.ID FROM CHARGE WHERE CHARGE.INVOICE_ID = in_invoice_id
15:15:36 1351  	 )
15:15:36 1352  	 LOOP
15:15:36 1353  	   PROCS_CHARGE_V21.UPDATE_CHARGE_STATUS(
15:15:36 1354  	     in_charge_id	 => f_charge_to_close.ID,
15:15:36 1355  	     in_updated_by	 => in_updated_by,
15:15:36 1356  	     in_charge_status_id => GLOBAL_STATUSES_V21.CHARGE_CANCELED
15:15:36 1357  	   );
15:15:36 1358  	 END LOOP;
15:15:36 1359  
15:15:36 1360  	 --FOR f_license_to_cancel IN (
15:15:36 1361  	 --  SELECT LICENSE.ID FROM LICENSE WHERE LICENSE.INVOICE_ID = in_invoice_id AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V21.LICENSE_ACTIVE
15:15:36 1362  	 --)
15:15:36 1363  	 --LOOP
15:15:36 1364  	 --  PROCS_LICENSE_CRU_V21.UPDATE_LICENSE(
15:15:36 1365  	 --    in_license_id	     => f_license_to_cancel.ID,
15:15:36 1366  	 --    in_license_status_id  => GLOBAL_STATUSES_V21.LICENSE_CLOSED,
15:15:36 1367  	 --    in_needs_entitlements => GLOBAL_CONSTANTS_V21.FALSE,
15:15:36 1368  	 --    in_updated_by	     => in_updated_by
15:15:36 1369  	 --  );
15:15:36 1370  	 --END LOOP;
15:15:36 1371  
15:15:36 1372  	 PROCS_INVOICE_V21.UPDATE_INVOICE_STATUS(
15:15:36 1373  	   in_invoice_id		  => in_invoice_id,
15:15:36 1374  	   in_updated_by		  => in_updated_by,
15:15:36 1375  	   in_invoice_status_id 	  => GLOBAL_STATUSES_V21.INVOICE_CLOSED
15:15:36 1376  	 );
15:15:36 1377  
15:15:36 1378  	 FOR f_transaction_to_close IN (
15:15:36 1379  	   SELECT DISTINCT CHARGE.TRANSACTION_ID AS "ID" FROM CHARGE WHERE CHARGE.INVOICE_ID = in_invoice_id
15:15:36 1380  	 )
15:15:36 1381  	 LOOP
