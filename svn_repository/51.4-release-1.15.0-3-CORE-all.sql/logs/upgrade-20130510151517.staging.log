15:15:36 1016  
15:15:36 1017  PROCEDURE GET_MAX_REFUND (
15:15:36 1018  	 in_invoice_id IN NUMBER,
15:15:36 1019  	 out_amount    OUT NUMBER
15:15:36 1020  ) AS
15:15:36 1021  SPROC_NAME CONSTANT VARCHAR2(14) := 'GET_MAX_REFUND';
15:15:36 1022  -- VARIABLES
15:15:36 1023  temp_invoice_id	       NUMBER;
15:15:36 1024  var_invoice_refunds_sum NUMBER(10,2);
15:15:36 1025  var_invoice_charges_sum NUMBER(10,2);
15:15:36 1026  -- EXCEPTIONS
15:15:36 1027  BAD_INVOICE_ID EXCEPTION;
15:15:36 1028  BEGIN
15:15:36 1029  
15:15:36 1030  	 BEGIN
15:15:36 1031  	   SELECT
15:15:36 1032  	     INVOICE.ID into temp_invoice_id
15:15:36 1033  	   FROM
15:15:36 1034  	     INVOICE
15:15:36 1035  	   WHERE
15:15:36 1036  	     INVOICE.ID = in_invoice_id;
15:15:36 1037  	   EXCEPTION
15:15:36 1038  	     WHEN NO_DATA_FOUND THEN
15:15:36 1039  	       RAISE BAD_INVOICE_ID;
15:15:36 1040  	 END;
15:15:36 1041  
15:15:36 1042  	 var_invoice_refunds_sum := 0;
15:15:36 1043  	 var_invoice_charges_sum := 0;
15:15:36 1044  
15:15:36 1045  	 FOR f_charge IN (
15:15:36 1046  	   SELECT
15:15:36 1047  	     CHARGE.ID,
15:15:36 1048  	     CHARGE.CHARGE_STATUS_ID,
15:15:36 1049  	     CHARGE.CHARGE_AMOUNT,
15:15:36 1050  	     CHARGE.TRANSACTION_ID
15:15:36 1051  	   FROM
15:15:36 1052  	     CHARGE
15:15:36 1053  	   WHERE
15:15:36 1054  	     CHARGE.INVOICE_ID = in_invoice_id
15:15:36 1055  	 )
15:15:36 1056  	 LOOP
15:15:36 1057  	   -- If charge.status = canceled then continue
15:15:36 1058  	   IF f_charge.CHARGE_STATUS_ID != GLOBAL_STATUSES_V21.CHARGE_CANCELED THEN
15:15:36 1059  
15:15:36 1060  	     IF f_charge.CHARGE_AMOUNT > 0 THEN
15:15:36 1061  	       IF f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V21.CHARGE_PROCESSED
15:15:36 1062  		  AND PROCS_TRANSACTION_V21.IS_TRANSACTION_COLLECTED(f_charge.TRANSACTION_ID) = GLOBAL_CONSTANTS_V21.TRUE THEN
15:15:36 1063  		 -- Transaction collected
15:15:36 1064  		 var_invoice_charges_sum := var_invoice_charges_sum + f_charge.CHARGE_AMOUNT;
15:15:36 1065  	       ELSE
15:15:36 1066  		 -- Transaction is not collected. Do nothing
15:15:36 1067  		 NULL;
15:15:36 1068  	       END IF;
15:15:36 1069  	     ELSE
15:15:36 1070  	       IF f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V21.CHARGE_PROCESSED
15:15:36 1071  		  AND PROCS_TRANSACTION_V21.IS_TRANSACTION_COLLECTED(f_charge.TRANSACTION_ID) = GLOBAL_CONSTANTS_V21.FALSE THEN
15:15:36 1072  		 -- If charge is processed transaction is not collected then do nothing
15:15:36 1073  		 NULL;
15:15:36 1074  	       ELSE
15:15:36 1075  		 var_invoice_refunds_sum := var_invoice_refunds_sum + f_charge.CHARGE_AMOUNT;
15:15:36 1076  	       END IF;
15:15:36 1077  	     END IF;
15:15:36 1078  
15:15:36 1079  	   END IF;
15:15:36 1080  	 END LOOP;
15:15:36 1081  
15:15:36 1082  	 -- Refunds are negative
15:15:36 1083  	 var_invoice_refunds_sum := 0 - var_invoice_refunds_sum;
15:15:36 1084  
15:15:36 1085  	 out_amount := var_invoice_charges_sum - var_invoice_refunds_sum;
15:15:36 1086  
15:15:36 1087  EXCEPTION
15:15:36 1088  WHEN BAD_INVOICE_ID THEN
15:15:36 1089  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 1090  	   SPROC_NAME, 'No such invoice');
15:15:36 1091  WHEN OTHERS THEN
15:15:36 1092  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 1093  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:15:36 1094  END GET_MAX_REFUND;
