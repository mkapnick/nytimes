15:15:36 416  
15:15:36 417  PROCEDURE FINALIZE_SUBSCRIPTION_CREATION (
15:15:36 418  	in_subscription_id    IN NUMBER,
15:15:36 419  	in_invoice_id	      IN NUMBER,
15:15:36 420  	in_instrument_type_id IN NUMBER,
15:15:36 421  	in_instrument_id      IN NUMBER,
15:15:36 422  	in_created_by	      IN VARCHAR2
15:15:36 423  ) AS
15:15:36 424  SPROC_NAME CONSTANT VARCHAR2(30) := 'FINALIZE_SUBSCRIPTION_CREATION';
15:15:36 425  -- VARIABLES
15:15:36 426  var_invoice_amount     NUMBER(10, 2);
15:15:36 427  var_new_transaction_id NUMBER;
15:15:36 428  var_new_charge_id      NUMBER;
15:15:36 429  -- EXCEPTIONS
15:15:36 430  CAN_NOT_USE_FCINSTR	  EXCEPTION;
15:15:36 431  CAN_NOT_CALC_INVOICE_AMOUNT EXCEPTION;
15:15:36 432  CAN_NOT_CREATE_TRANSACTION  EXCEPTION;
15:15:36 433  CAN_NOT_CREATE_CHARGE	  EXCEPTION;
15:15:36 434  
15:15:36 435  EXCEPTION_MESSAGE VARCHAR2(1024);
15:15:36 436  BEGIN
15:15:36 437  
15:15:36 438  	-- Calculate invoice amount ( + discounts, taxes)
15:15:36 439  	BEGIN
15:15:36 440  	  PROCS_INVOICE_V21.CALCULATE_INVOICE_AMOUNT(
15:15:36 441  	    in_invoice_id => in_invoice_id,
15:15:36 442  	    out_amount	  => var_invoice_amount
15:15:36 443  	  );
15:15:36 444  	  EXCEPTION
15:15:36 445  	   WHEN OTHERS THEN
15:15:36 446  	     EXCEPTION_MESSAGE := SQLERRM;
15:15:36 447  	     RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
15:15:36 448  	END;
15:15:36 449  
15:15:36 450  	IF in_instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_ZCI_INSTRUMENT
15:15:36 451  	  AND var_invoice_amount > 0 THEN
15:15:36 452  	  RAISE CAN_NOT_USE_FCINSTR;
15:15:36 453  	END IF;
15:15:36 454  
15:15:36 455  	IF var_invoice_amount = 0 THEN
15:15:36 456  	  -- UPDATE INVOICE. SET STATUS TO PROCESSED
15:15:36 457  	  PROCS_INVOICE_CRU_V21.UPDATE_INVOICE(
15:15:36 458  	    in_invoice_id		   => in_invoice_id,
15:15:36 459  	    in_updated_by		   => in_created_by,
15:15:36 460  	    in_invoice_status_id	   => GLOBAL_STATUSES_V21.INVOICE_CLOSED
15:15:36 461  	  );
15:15:36 462  	ELSE
15:15:36 463  	  -- Create transaction and charge
15:15:36 464  	  BEGIN
15:15:36 465  	    PROCS_TRANSACTION_V21.CREATE_TRANSACTION(
15:15:36 466  	      in_transaction_id 	=> NULL,
15:15:36 467  	      in_status_id		=> GLOBAL_STATUSES_V21.TRANSACTION_PENDING,
15:15:36 468  	      in_amount 		=> var_invoice_amount,
15:15:36 469  	      in_created_by		=> in_created_by,
15:15:36 470  	      in_order_id		=> NULL,
15:15:36 471  	      in_transaction_type_code	=> 'START_SUBSCRIPTION',
15:15:36 472  	      out_transaction_id	=> var_new_transaction_id
15:15:36 473  	    );
15:15:36 474  	    EXCEPTION
15:15:36 475  	      WHEN OTHERS THEN
15:15:36 476  		EXCEPTION_MESSAGE := SQLERRM;
15:15:36 477  		RAISE CAN_NOT_CREATE_TRANSACTION;
15:15:36 478  	  END;
15:15:36 479  
15:15:36 480  	  BEGIN
15:15:36 481  	    PROCS_CHARGE_V21.CREATE_CHARGE(
15:15:36 482  	      in_invoice_id	    => in_invoice_id,
15:15:36 483  	      in_transaction_id     => var_new_transaction_id,
15:15:36 484  	      in_instrument_type_id => in_instrument_type_id,
15:15:36 485  	      in_instrument_id	    => in_instrument_id,
15:15:36 486  	      in_charge_amount	    => var_invoice_amount,
15:15:36 487  	      in_created_by	    => in_created_by,
15:15:36 488  	      in_charge_status_id   => GLOBAL_STATUSES_V21.CHARGE_OPENED,
15:15:36 489  	      out_charge_id	    => var_new_charge_id
15:15:36 490  	    );
15:15:36 491  	    EXCEPTION
15:15:36 492  	      WHEN OTHERS THEN
15:15:36 493  		EXCEPTION_MESSAGE := SQLERRM;
15:15:36 494  		RAISE CAN_NOT_CREATE_CHARGE;
15:15:36 495  	  END;
15:15:36 496  	END IF;
15:15:36 497  
15:15:36 498  EXCEPTION
15:15:36 499  WHEN CAN_NOT_USE_FCINSTR THEN
15:15:36 500  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
15:15:36 501  	  SPROC_NAME, 'Could not use "free charge instrument" for non-zero invoice');
15:15:36 502  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
15:15:36 503  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:36 504  	  SPROC_NAME, 'Could not calculate invoice amount', EXCEPTION_MESSAGE);
15:15:36 505  WHEN CAN_NOT_CREATE_TRANSACTION THEN
15:15:36 506  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:36 507  	  SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
15:15:36 508  WHEN CAN_NOT_CREATE_CHARGE THEN
15:15:36 509  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:36 510  	  SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
15:15:36 511  WHEN OTHERS THEN
15:15:36 512  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 513  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:15:36 514  END FINALIZE_SUBSCRIPTION_CREATION;
