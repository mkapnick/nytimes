15:15:36 1546  	     in_status_id		 => GLOBAL_STATUSES_V21.LICENSE_ACTIVE,
15:15:36 1547  	     in_needs_entitlements	 => GLOBAL_CONSTANTS_V21.TRUE,
15:15:36 1548  	     in_start_date		 => in_effective_start_date,
15:15:36 1549  	     in_end_date		 => in_effective_end_date,
15:15:36 1550  	     in_offer_id		 => var_current_offer_id,
15:15:36 1551  	     in_subscription_id 	 => in_subscription_id,
15:15:36 1552  	     in_invoice_id		 => var_free_invoice_id,
15:15:36 1553  	     in_created_by		 => in_updated_by,
15:15:36 1554  	     in_is_extension		 => GLOBAL_CONSTANTS_V21.TRUE,
15:15:36 1555  	     in_current_offer_index	 => var_current_offer_index,
15:15:36 1556  	     in_current_offer_recurr_num => var_current_offer_recurr_num,
15:15:36 1557  	     out_license_id		 => var_free_license_id
15:15:36 1558  	   );
15:15:36 1559  	   EXCEPTION
