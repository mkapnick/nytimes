15:15:36 595  
15:15:36 596  FUNCTION IS_INVOICE_PAYING_STARTED (
15:15:36 597  /*
15:15:36 598  Throws exceptions:
15:15:36 599  APP_EXCEPTION_CODES_V21.NOT_FOUND
15:15:36 600  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
15:15:36 601  */
15:15:36 602  	in_invoice_id IN NUMBER
15:15:36 603  ) RETURN NUMBER AS
15:15:36 604  -- VARIABLES
15:15:36 605  SPROC_NAME		 CONSTANT VARCHAR2(30) := 'IS_INVOICE_PAYING_STARTED';
15:15:36 606  temp_invoice_id		 NUMBER;
15:15:36 607  var_processed_charges_num  NUMBER;
15:15:36 608  var_processed_transac_num  NUMBER;
15:15:36 609  var_success_attempts_num	 NUMBER;
15:15:36 610  var_is_gc 		 NUMBER;
15:15:36 611  -- EXCEPTIONS
15:15:36 612  BAD_INVOICE_ID EXCEPTION;
15:15:36 613  BEGIN
15:15:36 614  
15:15:36 615  	-- Check that invoice exists
15:15:36 616  	BEGIN
15:15:36 617  	  SELECT
15:15:36 618  	    INVOICE.ID into temp_invoice_id
15:15:36 619  	  FROM
15:15:36 620  	    INVOICE
15:15:36 621  	  WHERE
15:15:36 622  	    INVOICE.ID = in_invoice_id;
15:15:36 623  	  EXCEPTION
15:15:36 624  	   WHEN NO_DATA_FOUND THEN
15:15:36 625  	     RAISE BAD_INVOICE_ID;
15:15:36 626  	END;
15:15:36 627  
15:15:36 628  	-- Check that there are tansaction attempts with status success
15:15:36 629  	SELECT
15:15:36 630  	  COUNT(1) into var_success_attempts_num
15:15:36 631  	FROM
15:15:36 632  	  TRANSACTION_ATTEMPT ta,
15:15:36 633  	  TRANSACTION t,
15:15:36 634  	  CHARGE c
15:15:36 635  	WHERE
15:15:36 636  	  ta.TRANSACTION_ATTEMPT_STATUS_ID = GLOBAL_STATUSES_V21.TRANS_ATTEMPT_SUCCESS and
15:15:36 637  	  ta.transaction_id = t.id and
15:15:36 638  	  c.transaction_id = t.id and
15:15:36 639  	  t.is_refund = GLOBAL_CONSTANTS_V21.FALSE and
15:15:36 640  	  c.invoice_id = in_invoice_id
15:15:36 641  	;
15:15:36 642  
15:15:36 643  	IF var_success_attempts_num > 0 THEN
15:15:36 644  	  RETURN GLOBAL_CONSTANTS_V21.TRUE;
15:15:36 645  	END IF;
15:15:36 646  
15:15:36 647  	SELECT
15:15:36 648  	  COUNT(1) into var_success_attempts_num
15:15:36 649  	FROM
15:15:36 650  	  TRANSACTION t,
15:15:36 651  	  CHARGE c
15:15:36 652  	WHERE
15:15:36 653  	  c.transaction_id = t.id and
15:15:36 654  	  t.is_refund = GLOBAL_CONSTANTS_V21.FALSE and
15:15:36 655  	  t.is_settled = GLOBAL_CONSTANTS_V21.TRUE and
15:15:36 656  	  c.invoice_id = in_invoice_id
15:15:36 657  	;
15:15:36 658  
15:15:36 659  	IF var_success_attempts_num > 0 THEN
15:15:36 660  	  RETURN GLOBAL_CONSTANTS_V21.TRUE;
15:15:36 661  	END IF;
15:15:36 662  
15:15:36 663  	SELECT
15:15:36 664  	  COUNT(1) into var_is_gc
15:15:36 665  	FROM
15:15:36 666  	  gift_certificate gc
15:15:36 667  	WHERE
15:15:36 668  	  gc.finalized_invoice_id = in_invoice_id
15:15:36 669  	;
15:15:36 670  
15:15:36 671  	IF var_is_gc > 0 THEN
15:15:36 672  	  RETURN GLOBAL_CONSTANTS_V21.TRUE;
15:15:36 673  	END IF;
15:15:36 674  
15:15:36 675  	RETURN GLOBAL_CONSTANTS_V21.FALSE;
15:15:36 676  
15:15:36 677  EXCEPTION
15:15:36 678  WHEN BAD_INVOICE_ID THEN
15:15:36 679  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 680  	  SPROC_NAME, 'No such invoice');
15:15:36 681  WHEN OTHERS THEN
15:15:36 682  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 683  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:15:36 684  END IS_INVOICE_PAYING_STARTED;
