15:15:36 428  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 429  	  SPROC_NAME, 'Could not find account for given invoice id', EXCEPTION_MESSAGE);
15:15:36 430  WHEN OTHERS THEN
15:15:36 431  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 432  	  SPROC_NAME, 'Unknown error', SQLERRM);
