15:15:36 202  WHEN OTHERS THEN
15:15:36 203  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 204  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:15:36 205  END UPDATE_LICENSE_STATUS;
