15:15:35 1076  FUNCTION CHECK_FOR_SAME_PRODUCTS (
15:15:35 1077  	 in_offer_chain_1	  IN OFFER_CHAIN.ID%TYPE,
15:15:35 1078  	 in_offer_chain_2	  IN OFFER_CHAIN.ID%TYPE,
15:15:35 1079  	 in_use_eligibility_rules IN NUMBER DEFAULT GLOBAL_CONSTANTS_V21.FALSE
15:15:35 1080  ) RETURN NUMBER AS
15:15:35 1081  SPROC_NAME CONSTANT VARCHAR2(23) := 'CHECK_FOR_SAME_PRODUCTS';
15:15:35 1082  -- CONSTANTS
15:15:35 1083  PRODUCT_ELIGIBILITY_NAME CONSTANT VARCHAR2(19) := 'MAX_CONCURRENT_SUBS';
15:15:35 1084  -- VARIABLES
15:15:35 1085  temp_offer_chain_id	OFFER_CHAIN.ID%TYPE;
15:15:35 1086  var_same_products	SYS_REFCURSOR;
15:15:35 1087  var_same_product_id	NUMBER;
15:15:35 1088  same_product_count	NUMBER;
15:15:35 1089  var_product_eligibility_limit NUMBER;
15:15:35 1090  s_product_eligibility_limit   VARCHAR2(100);
15:15:35 1091  -- EXCEPTIONS
15:15:35 1092  BAD_FIRST_OFFER_CHAIN	      EXCEPTION;
15:15:35 1093  BAD_SECOND_OFFER_CHAIN	      EXCEPTION;
15:15:35 1094  BEGIN
15:15:35 1095  
15:15:35 1096  	 -- Check that first offer chain exists
15:15:35 1097  	 BEGIN
15:15:35 1098  	   SELECT
15:15:35 1099  	     OFFER_CHAIN.ID into temp_offer_chain_id
15:15:35 1100  	   FROM
15:15:35 1101  	     OFFER_CHAIN
15:15:35 1102  	   WHERE
15:15:35 1103  	     OFFER_CHAIN.ID = in_offer_chain_1;
15:15:35 1104  	   EXCEPTION
15:15:35 1105  	     WHEN NO_DATA_FOUND THEN
15:15:35 1106  	       RAISE BAD_FIRST_OFFER_CHAIN;
15:15:35 1107  	 END;
15:15:35 1108  
15:15:35 1109  	 -- Check that second offer chain exists
15:15:35 1110  	 BEGIN
15:15:35 1111  	   SELECT
15:15:35 1112  	     OFFER_CHAIN.ID into temp_offer_chain_id
15:15:35 1113  	   FROM
15:15:35 1114  	     OFFER_CHAIN
15:15:35 1115  	   WHERE
15:15:35 1116  	     OFFER_CHAIN.ID = in_offer_chain_2;
15:15:35 1117  	   EXCEPTION
15:15:35 1118  	     WHEN NO_DATA_FOUND THEN
15:15:35 1119  	       RAISE BAD_SECOND_OFFER_CHAIN;
15:15:35 1120  	 END;
15:15:35 1121  
15:15:35 1122  	 PROCS_OFFER_CHAIN_V21.GET_OFF_CHAINS_SAME_PRODUCTS(
15:15:35 1123  	   in_offer_chain_1 => in_offer_chain_1,
15:15:35 1124  	   in_offer_chain_2 => in_offer_chain_2,
15:15:35 1125  	   out_result_set   => var_same_products
15:15:35 1126  	 );
15:15:35 1127  
15:15:35 1128  	 LOOP
15:15:35 1129  	   FETCH var_same_products INTO var_same_product_id, same_product_count;
15:15:35 1130  	   EXIT WHEN var_same_products%NOTFOUND;
15:15:35 1131  
15:15:35 1132  	   IF in_use_eligibility_rules = GLOBAL_CONSTANTS_V21.FALSE THEN
15:15:35 1133  	     -- Return false because this offer chains having same products
15:15:35 1134  	     RETURN GLOBAL_CONSTANTS_V21.TRUE;
15:15:35 1135  	   ELSE
15:15:35 1136  
15:15:35 1137  	     -- Get eligibility rule for given product
15:15:35 1138  	     BEGIN
15:15:35 1139  	       SELECT
15:15:35 1140  		 PRODUCT_ELIGIBILITY.VALUE into s_product_eligibility_limit
15:15:35 1141  	       FROM
15:15:35 1142  		 PRODUCT_ELIGIBILITY
15:15:35 1143  	       WHERE
15:15:35 1144  		 PRODUCT_ELIGIBILITY.PRODUCT_ID = var_same_product_id
15:15:35 1145  		 AND PRODUCT_ELIGIBILITY.NAME = PRODUCT_ELIGIBILITY_NAME;
15:15:35 1146  
15:15:35 1147  	       -- REVU: What should to be here? 1?
15:15:35 1148  	       EXCEPTION
15:15:35 1149  		 WHEN NO_DATA_FOUND THEN
15:15:35 1150  		   s_product_eligibility_limit := '1';
15:15:35 1151  	     END;
15:15:35 1152  
15:15:35 1153  	     IF UPPER(s_product_eligibility_limit) = GLOBAL_CONSTANTS_V21.MAX_CONSURRENT_PRD_UNLIM THEN
15:15:35 1154  	       RETURN GLOBAL_CONSTANTS_V21.FALSE;
15:15:35 1155  	     END IF;
15:15:35 1156  
15:15:35 1157  	     var_product_eligibility_limit := TO_NUMBER(s_product_eligibility_limit);
15:15:35 1158  
15:15:35 1159  	     -- Check for limit
15:15:35 1160  	     IF var_product_eligibility_limit < same_product_count THEN
15:15:35 1161  	       RETURN GLOBAL_CONSTANTS_V21.TRUE;
15:15:35 1162  	     END IF;
15:15:35 1163  
15:15:35 1164  	   END IF;
15:15:35 1165  	 END LOOP;
15:15:35 1166  
15:15:35 1167  	 RETURN GLOBAL_CONSTANTS_V21.FALSE;
15:15:35 1168  
