15:15:37 1405  
15:15:37 1406  PROCEDURE REDEEM_GIFT_CERTIFICATE (
15:15:37 1407  	 in_group_id			 IN NUMBER,
15:15:37 1408  	 in_gift_certificate_code	 IN VARCHAR2,
15:15:37 1409  	 in_created_by			 IN VARCHAR2,
15:15:37 1410  	 in_redeemer_address_id 	 IN NUMBER,
15:15:37 1411  	 in_fin_instrument_id		 IN NUMBER,
