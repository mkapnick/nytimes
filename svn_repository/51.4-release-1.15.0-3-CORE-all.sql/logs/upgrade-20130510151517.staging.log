15:15:35  50  
15:15:35  51  	SELECT
15:15:35  52  	  C.CODE NAME,
15:15:35  53  	  C.DESCRIPTION,
15:15:35  54  	  0 INHERITED,
15:15:35  55  	  C.SHAREABLE,
15:15:35  56  	  MAX(LIC.ENTITLEMENT_END_DATE) EXPIRES
15:15:35  57  	FROM
15:15:35  58  	  SUBSCRIPTION SB
15:15:35  59  	  INNER JOIN ACCOUNT AC ON AC.ID = SB.ACCOUNT_ID
15:15:35  60  	  INNER JOIN LICENSE LIC ON LIC.SUBSCRIPTION_ID = SB.ID
15:15:35  61  	  INNER JOIN OFFER_PRODUCT_OFFERING OPO ON OPO.OFFER_ID = LIC.OFFER_ID
15:15:35  62  	  INNER JOIN PRODUCT_OFFERING PO ON PO.ID = OPO.PRODUCT_OFFERING_ID
15:15:35  63  	  INNER JOIN CAPABILITY C ON PO.CAPABILITY_ID = C.ID
15:15:35  64  	WHERE
15:15:35  65  	  LIC.ENTITLEMENT_END_DATE >= TRUNC(SYSDATE)
15:15:35  66  	  AND LIC.START_DATE <= SYSDATE
15:15:35  67  	  AND AC.GROUP_ID = in_group_id
