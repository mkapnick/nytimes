15:15:37  98  	      c.transaction_id = t.id and
15:15:37  99  	      t.is_refund = GLOBAL_CONSTANTS_V21.TRUE
15:15:37 100  	  )
15:15:37 101  	  AND ROWNUM <= in_num_rows*10
15:15:37 102  	  ORDER BY dbms_random.value
15:15:37 103  ) WHERE
15:15:37 104  	  ROWNUM <= in_num_rows;
15:15:37 105  EXCEPTION
15:15:37 106  WHEN OTHERS THEN
