15:15:36 2823  PROCEDURE ARE_REFUNDS_PENDING_FOR_SUBSCR (
15:15:36 2824  /*
15:15:36 2825  APP_EXCEPTION_CODES_V21.NOT_FOUND
15:15:36 2826  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
15:15:36 2827  */
15:15:36 2828  	 in_subscription_id IN NUMBER,
15:15:36 2829  	 out_result	    OUT NUMBER
15:15:36 2830  ) AS
15:15:36 2831  SPROC_NAME CONSTANT VARCHAR2(30) := 'ARE_REFUNDS_PENDING_FOR_SUBSCR';
15:15:36 2832  -- VARIABLES
15:15:36 2833  temp_subscription_id NUMBER;
15:15:36 2834  var_local_result     NUMBER;
15:15:36 2835  -- EXCEPTIONS
15:15:36 2836  BAD_SUBSCRIPTION_ID EXCEPTION;
15:15:36 2837  BEGIN
15:15:36 2838  
15:15:36 2839  	 -- Check that subscription exists
15:15:36 2840  	 BEGIN
15:15:36 2841  	   SELECT
15:15:36 2842  	     SUBSCRIPTION.ID into temp_subscription_id
15:15:36 2843  	   FROM
15:15:36 2844  	     SUBSCRIPTION
15:15:36 2845  	   WHERE
15:15:36 2846  	     SUBSCRIPTION.ID = in_subscription_id;
15:15:36 2847  	   EXCEPTION
15:15:36 2848  	     WHEN NO_DATA_FOUND THEN
15:15:36 2849  	       RAISE BAD_SUBSCRIPTION_ID;
15:15:36 2850  	 END;
15:15:36 2851  
15:15:36 2852  	 var_local_result := NULL;
15:15:36 2853  
15:15:36 2854  	 -- Check charges for each invoice associated with gived subscription
15:15:36 2855  	 FOR f_invoice IN (
15:15:36 2856  	   SELECT DISTINCT
15:15:36 2857  	     LICENSE.INVOICE_ID as "ID"
15:15:36 2858  	   FROM
15:15:36 2859  	     LICENSE
15:15:36 2860  	   WHERE
15:15:36 2861  	     LICENSE.SUBSCRIPTION_ID = in_subscription_id
15:15:36 2862  	     AND LICENSE.LICENSE_STATUS_ID IN ( SELECT GLOBAL_STATUSES_V21.LICENSE_ACTIVE FROM DUAL )
15:15:36 2863  	 )
15:15:36 2864  	 LOOP
15:15:36 2865  
15:15:36 2866  	   -- Check each charge in invoice
15:15:36 2867  	   FOR f_charge IN (
15:15:36 2868  	     SELECT
15:15:36 2869  	       CHARGE.ID,
15:15:36 2870  	       CHARGE.CHARGE_STATUS_ID,
15:15:36 2871  	       CHARGE.CHARGE_AMOUNT
15:15:36 2872  	     FROM
15:15:36 2873  	       CHARGE
15:15:36 2874  	     WHERE
15:15:36 2875  	       CHARGE.INVOICE_ID = f_invoice.ID
15:15:36 2876  	   )
15:15:36 2877  	   LOOP
15:15:36 2878  
15:15:36 2879  	     -- Charge amount < 0     => it is a refund
15:15:36 2880  	     -- Charge status is OPEN => means that it is not processed yet
15:15:36 2881  	     IF f_charge.CHARGE_AMOUNT < 0
15:15:36 2882  		AND f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V21.CHARGE_OPENED THEN
15:15:36 2883  	       var_local_result := GLOBAL_CONSTANTS_V21.TRUE;
15:15:36 2884  	     END IF;
15:15:36 2885  
15:15:36 2886  	   END LOOP;
15:15:36 2887  
15:15:36 2888  	 END LOOP;
15:15:36 2889  
15:15:36 2890  	 IF var_local_result IS NULL THEN
15:15:36 2891  	   out_result := GLOBAL_CONSTANTS_V21.FALSE;
15:15:36 2892  	 ELSE
15:15:36 2893  	   out_result := GLOBAL_CONSTANTS_V21.TRUE;
15:15:36 2894  	 END IF;
15:15:36 2895  
15:15:36 2896  EXCEPTION
15:15:36 2897  WHEN BAD_SUBSCRIPTION_ID THEN
15:15:36 2898  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 2899  	   SPROC_NAME, 'No such transaction id');
15:15:36 2900  WHEN OTHERS THEN
15:15:36 2901  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 2902  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:15:36 2903  END ARE_REFUNDS_PENDING_FOR_SUBSCR;
