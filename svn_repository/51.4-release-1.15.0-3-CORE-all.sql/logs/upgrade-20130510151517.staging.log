15:15:37 368  	in_end		    IN NUMBER,
15:15:37 369  	out_Result_Set	    OUT Sys_Refcursor
15:15:37 370  ) AS
15:15:37 371  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_SUBSCRIPTION_SHARES';
15:15:37 372  range_diff NUMBER := 0;
15:15:37 373  upper_bond_diff NUMBER := 0;
15:15:37 374  l_start NUMBER := 0;
15:15:37 375  l_end   NUMBER := 0;
15:15:37 376  BEGIN
15:15:37 377  	-- Normalize the end points [START]
15:15:37 378  	IF (in_start IS NULL OR in_start < 0) Then
15:15:37 379  	  l_start := 0;
15:15:37 380  	ELSE
15:15:37 381  	  l_start := in_start;
15:15:37 382  	END IF;
15:15:37 383  
15:15:37 384  	IF (in_end IS NULL) Then
15:15:37 385  	  l_end := 11;
15:15:37 386  	ELSE
15:15:37 387  	  l_end := in_end;
15:15:37 388  	END IF;
15:15:37 389  
15:15:37 390  	l_start := l_start + 1;
15:15:37 391  	l_end	:= l_end   + 1;
15:15:37 392  
15:15:37 393  	range_diff := l_end - l_start;
15:15:37 394  	upper_bond_diff :=  range_diff - GLOBAL_CONSTANTS_V21.MAX_RETURN_COUNT;
15:15:37 395  
15:15:37 396  	IF (upper_bond_diff > 0) Then
15:15:37 397  	  l_end := l_end - upper_bond_diff;
15:15:37 398  	END IF;
15:15:37 399  	-- Normalize the end points [END]
15:15:37 400  
15:15:37 401  	BEGIN
15:15:37 402  	   OPEN out_result_set FOR
15:15:37 403  	   SELECT *
15:15:37 404  	   FROM
15:15:37 405  	     (SELECT rownum rnum, Q.*
15:15:37 406  	      FROM
15:15:37 407  	       (SELECT
15:15:37 408  		  ss.ID,
15:15:37 409  		  ss.GROUP_ACCOUNT_ID,
15:15:37 410  		  ss.BORROWER_ACCOUNT_ID,
15:15:37 411  		  ss.IP_ADDRESS,
15:15:37 412  		  ss.START_DATE,
15:15:37 413  		  ss.END_DATE,
15:15:37 414  		  ss.CREATED_BY,
15:15:37 415  		  ss.CREATE_DATE,
15:15:37 416  		  ss.UPDATED_BY,
15:15:37 417  		  ss.UPDATE_DATE,
15:15:37 418  		  a.GROUP_ID AS BORROWER_GROUP_ID
15:15:37 419  		FROM
15:15:37 420  		  SUBSCRIPTION_SHARE ss,
15:15:37 421  		  ACCOUNT a,
15:15:37 422  		  LICENSE l,
15:15:37 423  		  GROUP_ACCOUNT ga
15:15:37 424  		WHERE
15:15:37 425  		  ss.GROUP_ACCOUNT_ID = in_group_account_id AND
15:15:37 426  		  ss.GROUP_ACCOUNT_ID = ga.ID AND
15:15:37 427  		  GA.SUBSCRIPTION_ID = l.SUBSCRIPTION_ID AND
15:15:37 428  		  SYSDATE BETWEEN l.START_DATE AND l.ENTITLEMENT_END_DATE AND
15:15:37 429  		  SYSDATE BETWEEN ss.START_DATE AND ss.END_DATE AND
15:15:37 430  		  ss.BORROWER_ACCOUNT_ID  = a.ID
15:15:37 431  	      ) Q
15:15:37 432  	    WHERE rownum <= l_end)
15:15:37 433  	  WHERE rnum >= l_start;
15:15:37 434  	END;
15:15:37 435  EXCEPTION
15:15:37 436  WHEN OTHERS THEN
15:15:37 437  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:37 438  	  SPROC_NAME, 'Unknown error while retrieving subscription shares', SQLERRM);
15:15:37 439  END GET_SUBSCRIPTION_SHARES;
