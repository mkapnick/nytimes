15:15:36 1842  
15:15:36 1843  PROCEDURE HAS_FUTURE_LICENSE (
15:15:36 1844  /*
15:15:36 1845  APP_EXCEPTION_CODES_V21.NOT_FOUND
15:15:36 1846  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
15:15:36 1847  --
15:15:36 1848  RETURNS:
15:15:36 1849  GLOBAL_CONSTANTS_V21.TRUE - if has,
15:15:36 1850  GLOBAL_CONSTANTS_V21.FALSE - else
15:15:36 1851  */
15:15:36 1852  	 in_license_id IN  NUMBER,
15:15:36 1853  	 out_result    OUT NUMBER
15:15:36 1854  ) AS
15:15:36 1855  -- VARIABLES
15:15:36 1856  SPROC_NAME		 CONSTANT VARCHAR2(18) := 'HAS_FUTURE_LICENSE';
15:15:36 1857  var_subscription_id	 NUMBER;
15:15:36 1858  var_future_licenses_count NUMBER;
15:15:36 1859  -- EXCEPTIONS
15:15:36 1860  BAD_LICENSE_ID	    EXCEPTION;
15:15:36 1861  BEGIN
15:15:36 1862  
15:15:36 1863  	 BEGIN
15:15:36 1864  	   SELECT
15:15:36 1865  	     LICENSE.SUBSCRIPTION_ID into var_subscription_id
15:15:36 1866  	   FROM
15:15:36 1867  	     LICENSE
15:15:36 1868  	   WHERE
15:15:36 1869  	     LICENSE.ID = in_license_id;
15:15:36 1870  	   EXCEPTION
15:15:36 1871  	     WHEN NO_DATA_FOUND THEN
15:15:36 1872  	       RAISE BAD_LICENSE_ID;
15:15:36 1873  	 END;
15:15:36 1874  
15:15:36 1875  	 SELECT
15:15:36 1876  	   COUNT(*) into var_future_licenses_count
15:15:36 1877  	 FROM
15:15:36 1878  	   LICENSE
15:15:36 1879  	 WHERE
15:15:36 1880  	   LICENSE.ID != in_license_id
15:15:36 1881  	   AND LICENSE.SUBSCRIPTION_ID = var_subscription_id
15:15:36 1882  	   AND LICENSE.END_DATE > sysdate;
15:15:36 1883  
15:15:36 1884  	 IF var_future_licenses_count > 0 THEN
15:15:36 1885  	   out_result := GLOBAL_CONSTANTS_V21.TRUE;
15:15:36 1886  	 ELSE
15:15:36 1887  	   out_result := GLOBAL_CONSTANTS_V21.FALSE;
15:15:36 1888  	 END IF;
15:15:36 1889  
15:15:36 1890  EXCEPTION
15:15:36 1891  WHEN BAD_LICENSE_ID THEN
15:15:36 1892  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 1893  	   SPROC_NAME, 'No such license');
15:15:36 1894  WHEN OTHERS THEN
15:15:36 1895  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 1896  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:15:36 1897  END HAS_FUTURE_LICENSE;
