15:15:37 925  	in_created_by IN GROUP_ACCOUNT_IP_RANGE.CREATED_BY%TYPE
15:15:37 926  ) AS
15:15:37 927  SPROC_NAME CONSTANT VARCHAR2(32) := 'ADD_IP_RANGE';
15:15:37 928  var_ga_type OFFER_CHAIN.GROUP_ACCOUNT_TYPE_ID%TYPE;
15:15:37 929  var_is_dupe NUMBER(1);
15:15:37 930  NOT_GL EXCEPTION;
15:15:37 931  DUPE EXCEPTION;
15:15:37 932  BEGIN
15:15:37 933  	  SELECT OFFER_CHAIN.GROUP_ACCOUNT_TYPE_ID into var_ga_type
15:15:37 934  	  FROM
15:15:37 935  	    GROUP_ACCOUNT,
15:15:37 936  	    SUBSCRIPTION,
15:15:37 937  	    OFFER_CHAIN
15:15:37 938  	  WHERE
