15:15:37 1802  PROCEDURE GET_GIFT_CERTIFICATE_BY_CODE (
15:15:37 1803  /*
15:15:37 1804  Throws exceptions:
15:15:37 1805  APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:37 1806  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
15:15:37 1807  */
15:15:37 1808  	 in_code	IN VARCHAR,
15:15:37 1809  	 out_result_set OUT SYS_REFCURSOR
15:15:37 1810  ) AS
15:15:37 1811  -- VARIABLES
15:15:37 1812  SPROC_NAME		CONSTANT VARCHAR2(28) := 'GET_GIFT_CERTIFICATE_BY_CODE';
15:15:37 1813  temp_gift_certificate_id NUMBER;
15:15:37 1814  -- EXCEPTIONS
15:15:37 1815  BAD_GIFT_CERTIFICATE_CODE EXCEPTION;
15:15:37 1816  BEGIN
15:15:37 1817  
15:15:37 1818  	 BEGIN
15:15:37 1819  	   SELECT
15:15:37 1820  	     GIFT_CERTIFICATE.ID into temp_gift_certificate_id
15:15:37 1821  	   FROM
15:15:37 1822  	     GIFT_CERTIFICATE
15:15:37 1823  	   WHERE
15:15:37 1824  	     GIFT_CERTIFICATE.CODE = in_code;
15:15:37 1825  	   EXCEPTION
15:15:37 1826  	     WHEN NO_DATA_FOUND THEN
15:15:37 1827  	       RAISE BAD_GIFT_CERTIFICATE_CODE;
15:15:37 1828  	 END;
15:15:37 1829  
15:15:37 1830  	 -- Select all gift certificates with given code
15:15:37 1831  	 OPEN out_result_set FOR
15:15:37 1832  	 SELECT
15:15:37 1833  	   gc.EXPIRATION_DATE,
15:15:37 1834  	   ch.name,
15:15:37 1835  	   ch.id,
15:15:37 1836  	   gc.sender_email,
15:15:37 1837  	   gc.sender_name,
15:15:37 1838  	   gc.recipient_email,
15:15:37 1839  	   gc.recipient_name,
15:15:37 1840  	   gc.purchase_date,
15:15:37 1841  	   gc.redemption_date,
15:15:37 1842  	   gc.purchaser_group_id,
15:15:37 1843  	   gc.redeemer_group_id,
15:15:37 1844  	   gc.gift_message,
15:15:37 1845  	   ocmd.value redemption_offer_chain_ids,
15:15:37 1846  	   s.offer_chain_id redeemed_offer_chain_id,
15:15:37 1847  	   gc.recipient_notify_date,
15:15:37 1848  	   gc.gift_certificate_status_id,
15:15:37 1849  	   gc.purchase_invoice_id,
15:15:37 1850  	   gc.finalized_invoice_id
15:15:37 1851  	 FROM
15:15:37 1852  	   GIFT_CERTIFICATE gc
15:15:37 1853  	 INNER JOIN OFFER_CHAIN ch ON ch.id = gc.offer_chain_id
15:15:37 1854  	 INNER JOIN OFFER_CHAIN_META_DATA ocmd ON gc.offer_chain_id = ocmd.offer_chain_id AND ocmd.name = 'redemption offer chain id'
15:15:37 1855  	 LEFT JOIN LICENSE l ON l.invoice_id = gc.finalized_invoice_id
15:15:37 1856  	 LEFT JOIN SUBSCRIPTION s ON l.subscription_id = s.id
15:15:37 1857  	 WHERE
15:15:37 1858  	   gc.code = in_code;
15:15:37 1859  
15:15:37 1860  EXCEPTION
15:15:37 1861  WHEN BAD_GIFT_CERTIFICATE_CODE THEN
15:15:37 1862  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:37 1863  	   SPROC_NAME, 'No such gift certificate');
15:15:37 1864  WHEN OTHERS THEN
15:15:37 1865  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:37 1866  	   SPROC_NAME, 'Unknown Error', SQLERRM);
15:15:37 1867  END;
