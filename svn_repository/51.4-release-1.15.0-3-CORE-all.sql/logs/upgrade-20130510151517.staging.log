15:15:34  37  			    LICENSE l INNER JOIN SUBSCRIPTION s ON L.SUBSCRIPTION_ID = s.ID
15:15:34  38  				      INNER JOIN CREDIT_CARD cc ON S.INSTRUMENT_ID   = cc.ID
15:15:34  39  			  WHERE
15:15:34  40  			    s.INSTRUMENT_TYPE_ID	 = 1
15:15:34  41  			    AND cc.CREDIT_CARD_STATUS_ID = 1
15:15:34  42  			    AND s.SUBSCRIPTION_STATUS_ID = 1
15:15:34  43  			    AND l.LICENSE_STATUS_ID	 = 2
15:15:34  44  			    AND cc.CREDIT_CARD_TYPE_ID IN (2,3)
15:15:34  45  			    AND l.END_DATE BETWEEN var_start_date AND var_end_date
15:15:34  46  			    AND l.ID NOT IN (SELECT LICENSE_ID FROM CC_UPDATE))
15:15:34  47  	   LOOP
15:15:34  48  	     var_license_count := 0;
15:15:34  49  	     SELECT COUNT(1) INTO  var_license_count FROM CC_UPDATE WHERE LICENSE_ID = record.LICENSE_ID;
15:15:34  50  
15:15:34  51  	     IF var_license_count = 0 THEN
15:15:34  52  		INSERT INTO CC_UPDATE (ID,
