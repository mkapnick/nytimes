15:15:36 1900  
15:15:36 1901  PROCEDURE CLOSE_SUBSCRIPTION (
15:15:36 1902  	 in_subscription_id IN NUMBER,
15:15:36 1903  	 in_updated_by	    IN VARCHAR2
15:15:36 1904  ) AS
15:15:36 1905  -- VARIABLES
15:15:36 1906  SPROC_NAME	    CONSTANT VARCHAR2(18) := 'CLOSE_SUBSCRIPTION';
15:15:36 1907  temp_subscription_id NUMBER;
15:15:36 1908  var_licenses_count   NUMBER;
15:15:36 1909  -- EXCEPTIONS
15:15:36 1910  BAD_SUBSCRIPTION_ID    EXCEPTION;
15:15:36 1911  ACTIVE_LICENSES_FOUND  EXCEPTION;
15:15:36 1912  EXCEPTION_MESSAGE      VARCHAR2(1024);
15:15:36 1913  BEGIN
15:15:36 1914  
15:15:36 1915  	 BEGIN
15:15:36 1916  	   SELECT
15:15:36 1917  	     SUBSCRIPTION.ID into temp_subscription_id
15:15:36 1918  	   FROM
15:15:36 1919  	     SUBSCRIPTION
15:15:36 1920  	   WHERE
15:15:36 1921  	     SUBSCRIPTION.ID = in_subscription_id;
15:15:36 1922  	   EXCEPTION
15:15:36 1923  	     WHEN NO_DATA_FOUND THEN
15:15:36 1924  	       RAISE BAD_SUBSCRIPTION_ID;
15:15:36 1925  	 END;
15:15:36 1926  
15:15:36 1927  	 SELECT
15:15:36 1928  	   COUNT(*) into var_licenses_count
15:15:36 1929  	 FROM
15:15:36 1930  	   LICENSE
15:15:36 1931  	 WHERE
15:15:36 1932  	   LICENSE.SUBSCRIPTION_ID = in_subscription_id
15:15:36 1933  	   AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V21.LICENSE_ACTIVE;
15:15:36 1934  
15:15:36 1935  	 IF var_licenses_count > 0 THEN
15:15:36 1936  	   RAISE ACTIVE_LICENSES_FOUND;
15:15:36 1937  	 END IF;
15:15:36 1938  
15:15:36 1939  	 PROCS_SUBSCRIPTION_V21.UPDATE_SUBSCRIPTION_STATUS(
15:15:36 1940  	   in_subscription_id	     => in_subscription_id,
15:15:36 1941  	   in_updated_by	     => in_updated_by,
15:15:36 1942  	   in_subscription_status_id => GLOBAL_STATUSES_V21.SUBSCRIPTION_CLOSED
15:15:36 1943  	 );
15:15:36 1944  
15:15:36 1945  EXCEPTION
15:15:36 1946  WHEN BAD_SUBSCRIPTION_ID THEN
15:15:36 1947  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 1948  	   SPROC_NAME, 'No such subscription');
15:15:36 1949  WHEN ACTIVE_LICENSES_FOUND THEN
15:15:36 1950  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
15:15:36 1951  	   SPROC_NAME, 'Active licenses found');
15:15:36 1952  WHEN OTHERS THEN
15:15:36 1953  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 1954  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:15:36 1955  END CLOSE_SUBSCRIPTION;
