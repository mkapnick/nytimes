15:15:37 1445  
15:15:37 1446  var_offers SYS_REFCURSOR;
15:15:37 1447  
15:15:37 1448  -- EXCEPTIONS
15:15:37 1449  BAD_GIFT_CERTIFICATE_CODE      EXCEPTION;
15:15:37 1450  BAD_GROUP_ID		      EXCEPTION;
15:15:37 1451  CAN_NOT_CREATE_LICENSE	      EXCEPTION;
15:15:37 1452  GIFT_CERT_IS_FINALIZED	      EXCEPTION;
15:15:37 1453  GIFT_CERT_IS_REFUNDED	      EXCEPTION;
15:15:37 1454  CAN_NOT_UPDATE_CERTIFICATE     EXCEPTION;
15:15:37 1455  GIFT_CERTIFICATE_EXPIRED       EXCEPTION;
15:15:37 1456  GIFT_CERTIFICATE_REDEEMED      EXCEPTION;
15:15:37 1457  USER_ALREADY_SUBSCRIBED_TO_PRD EXCEPTION;
15:15:37 1458  LIMIT_REACHED		      EXCEPTION;
15:15:37 1459  GC_PURCHASE_INVOICE_NOT_CLOSED EXCEPTION;
15:15:37 1460  PURCHASE_INVOICES_NOT_PAID     EXCEPTION;
15:15:37 1461  OC_TO_REDEEM_NOT_FOUND	      EXCEPTION;
15:15:37 1462  CAN_NOT_GET_FIRST_OFFER_CHAIN  EXCEPTION;
15:15:37 1463  EXCEPTION_MESSAGE	      VARCHAR2(1024);
15:15:37 1464  BEGIN
15:15:37 1465  
15:15:37 1466  	 -- Get account id
15:15:37 1467  	 BEGIN
15:15:37 1468  	   SELECT
15:15:37 1469  	     ACCOUNT.ID,
15:15:37 1470  	     ACCOUNT.TAX_EXEMPT_ID
15:15:37 1471  	     into
15:15:37 1472  	     var_account_id,
15:15:37 1473  	     var_account_tax_exempt_id
15:15:37 1474  	   FROM
15:15:37 1475  	     ACCOUNT
15:15:37 1476  	   WHERE
15:15:37 1477  	     ACCOUNT.GROUP_ID = in_group_id
15:15:37 1478  	     AND ROWNUM <= 1;
15:15:37 1479  
15:15:37 1480  	   EXCEPTION
15:15:37 1481  	   WHEN NO_DATA_FOUND THEN
15:15:37 1482  	     RAISE BAD_GROUP_ID;
15:15:37 1483  	 END;
15:15:37 1484  
15:15:37 1485  	 -- Get gift certificate data
15:15:37 1486  	 BEGIN
15:15:37 1487  	   SELECT
15:15:37 1488  	     GIFT_CERTIFICATE.ID,
15:15:37 1489  	     GIFT_CERTIFICATE.OFFER_CHAIN_ID,
15:15:37 1490  	     GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID,
15:15:37 1491  	     GIFT_CERTIFICATE.PURCHASER_GROUP_ID,
15:15:37 1492  	     GIFT_CERTIFICATE.EXPIRATION_DATE,
15:15:37 1493  	     GIFT_CERTIFICATE.REDEEMER_GROUP_ID,
15:15:37 1494  	     GIFT_CERTIFICATE.PURCHASE_INVOICE_ID
15:15:37 1495  	     into
15:15:37 1496  	     var_gift_certificate_id,
15:15:37 1497  	     var_purchased_oc_id,
15:15:37 1498  	     var_gc_status_id,
15:15:37 1499  	     var_purchaser_group_id,
15:15:37 1500  	     var_gc_expiration_date,
15:15:37 1501  	     var_gc_redeemer_group_id,
15:15:37 1502  	     var_gc_purchase_invoice_id
15:15:37 1503  	   FROM
15:15:37 1504  	     GIFT_CERTIFICATE
15:15:37 1505  	   WHERE
15:15:37 1506  	     GIFT_CERTIFICATE.CODE = in_gift_certificate_code
15:15:37 1507  	     AND ROWNUM <= 1;
15:15:37 1508  
15:15:37 1509  	   EXCEPTION
15:15:37 1510  	   WHEN NO_DATA_FOUND THEN
15:15:37 1511  	     RAISE BAD_GIFT_CERTIFICATE_CODE;
15:15:37 1512  	 END;
15:15:37 1513  
15:15:37 1514  	 -- get redemption oc id from meta data if it wasn't passed in, parsing will fail for gcs with multiple redemption offer chains,
15:15:37 1515  	 -- but in that case a redemption offer chain id should always be passed in
15:15:37 1516  	 IF var_oc_id_to_redeem IS NULL THEN
15:15:37 1517  	   BEGIN
15:15:37 1518  	     SELECT
15:15:37 1519  	       to_number(OFFER_CHAIN_META_DATA.VALUE)
15:15:37 1520  	       into
15:15:37 1521  	       var_oc_id_to_redeem
15:15:37 1522  	     FROM
15:15:37 1523  	       OFFER_CHAIN_META_DATA
15:15:37 1524  	     WHERE
15:15:37 1525  	       OFFER_CHAIN_META_DATA.OFFER_CHAIN_ID = var_purchased_oc_id
15:15:37 1526  	       AND OFFER_CHAIN_META_DATA.NAME = GLOBAL_CONSTANTS_V21.REDEMPTION_OC_ID
15:15:37 1527  	       AND ROWNUM = 1;
15:15:37 1528  -- requested by ticket so (but above is correct for the actual migrated data):
15:15:37 1529  --  SELECT
15:15:37 1530  --      OFFER_CHAIN.ID
15:15:37 1531  --      into
15:15:37 1532  --      var_oc_id_to_redeem
15:15:37 1533  --    FROM
15:15:37 1534  --      OFFER_CHAIN
15:15:37 1535  --	 INNER JOIN ELIGIBILITY ON OFFER_CHAIN.ID = ELIGIBILITY.OFFER_CHAIN_ID
15:15:37 1536  --	 INNER JOIN OFFER_CHAIN_META_DATA ON OFFER_CHAIN.ID = OFFER_CHAIN_META_DATA.OFFER_CHAIN_ID
15:15:37 1537  --    WHERE
15:15:37 1538  --      ELIGIBILITY.OFFER_CHAIN_ID = OFFER_CHAIN.ID
15:15:37 1539  --      AND ELIGIBILITY.NAME = GLOBAL_CONSTANTS_V21.GIFT_CERTIFICATE_REQUIRED
15:15:37 1540  --      AND ELIGIBILITY.VALUE = GLOBAL_CONSTANTS_V21.ELIGIBILITY_FLAG_SET
15:15:37 1541  --      AND OFFER_CHAIN_META_DATA.OFFER_CHAIN_ID = OFFER_CHAIN.ID
15:15:37 1542  --      AND OFFER_CHAIN_META_DATA.NAME = GLOBAL_CONSTANTS_V21.REDEMPTION_OC_ID
15:15:37 1543  --      AND to_number(OFFER_CHAIN_META_DATA.VALUE) = var_purchased_oc_id
15:15:37 1544  --      AND ROWNUM = 1;
15:15:37 1545  
15:15:37 1546  	     EXCEPTION
15:15:37 1547  	     WHEN NO_DATA_FOUND THEN
15:15:37 1548  	       RAISE OC_TO_REDEEM_NOT_FOUND;
15:15:37 1549  	   END;
15:15:37 1550  	 END IF;
15:15:37 1551  
15:15:37 1552  	 -- Check that purchase invoice for this GC was closed
15:15:37 1553  	 SELECT
15:15:37 1554  	   INVOICE.INVOICE_STATUS_ID into var_gc_purchase_inv_status_id
15:15:37 1555  	 FROM
15:15:37 1556  	   INVOICE
15:15:37 1557  	 WHERE
15:15:37 1558  	   INVOICE.ID = var_gc_purchase_invoice_id;
15:15:37 1559  
15:15:37 1560  	 IF var_gc_purchase_inv_status_id != GLOBAL_STATUSES_V21.INVOICE_CLOSED THEN
15:15:37 1561  	   RAISE GC_PURCHASE_INVOICE_NOT_CLOSED;
15:15:37 1562  	 END IF;
15:15:37 1563  
15:15:37 1564  	 -- Check that this invoice was successfully processed by billing
15:15:37 1565  	 SELECT
15:15:37 1566  	   COUNT(1) into var_succ_purch_attempts_num
15:15:37 1567  	 FROM
15:15:37 1568  	   TRANSACTION_ATTEMPT TA
15:15:37 1569  	   INNER JOIN TRANSACTION T ON T.ID = TA.TRANSACTION_ID
15:15:37 1570  	   INNER JOIN CHARGE CH ON CH.TRANSACTION_ID = T.ID
15:15:37 1571  	 WHERE
15:15:37 1572  	   CH.INVOICE_ID = var_gc_purchase_invoice_id
15:15:37 1573  	   AND TA.TRANSACTION_ATTEMPT_STATUS_ID = GLOBAL_STATUSES_V21.TRANS_ATTEMPT_SUCCESS;
15:15:37 1574  
15:15:37 1575  	 IF var_succ_purch_attempts_num = 0 THEN
15:15:37 1576  	   SELECT
15:15:37 1577  	     COUNT(1) into var_succ_purch_attempts_num
15:15:37 1578  	   FROM
15:15:37 1579  	     DUAL
15:15:37 1580  	   WHERE
15:15:37 1581  	     PROCS_INVOICE_V21.F_CALCULATE_INVOICE_AMOUNT(var_gc_purchase_invoice_id) = 0;
15:15:37 1582  	 END IF;
15:15:37 1583  
15:15:37 1584  	 IF var_succ_purch_attempts_num = 0 THEN
15:15:37 1585  	   RAISE PURCHASE_INVOICES_NOT_PAID;
15:15:37 1586  	 END IF;
15:15:37 1587  
15:15:37 1588  	 -- Check limit for gc's offer chain
15:15:37 1589  	 SELECT
15:15:37 1590  	   COUNT(*) into var_same_offer_chains_num
15:15:37 1591  	 FROM
15:15:37 1592  	   SUBSCRIPTION
15:15:37 1593  	 WHERE
15:15:37 1594  	   SUBSCRIPTION.ACCOUNT_ID = var_account_id
15:15:37 1595  	   AND SUBSCRIPTION.OFFER_CHAIN_ID = var_oc_id_to_redeem
15:15:37 1596  	   AND (
15:15:37 1597  	     SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V21.SUBSCRIPTION_ACTIVE
15:15:37 1598  	     OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V21.SUBSCRIPTION_IN_GRACE_PERIOD);
15:15:37 1599  IF var_same_offer_chains_num = 0 THEN
15:15:37 1600  	   -- if user does not have any active existing subscriptions to the offer chain
15:15:37 1601  	   -- and if product from the offer chain is already owned from another offer chain
15:15:37 1602  	   -- then deny purchase
15:15:37 1603  	   FOR f_account_offer_chains IN (
15:15:37 1604  	     SELECT DISTINCT
15:15:37 1605  	       OFFER_CHAIN_ID
