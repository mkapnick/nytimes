15:15:37 318  PROCEDURE DISABLE_CREDIT_CARD (
15:15:37 319  /*
15:15:37 320  Throws exceptions:
15:15:37 321  APP_EXCEPTION_CODES_V21.NOT_FOUND
15:15:37 322  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
15:15:37 323  APP_EXCEPTION_CODES_V21.INTERNAL_ERROR
15:15:37 324  */
15:15:37 325  	in_credit_card_id IN NUMBER,
15:15:37 326  	in_updated_by	  IN VARCHAR2
15:15:37 327  ) AS
15:15:37 328  SPROC_NAME		   CONSTANT VARCHAR2(19) := 'DISABLE_CREDIT_CARD';
15:15:37 329  -- VARIBLES
15:15:37 330  var_account_id		     NUMBER;
15:15:37 331  var_group_id		     NUMBER;
15:15:37 332  var_credit_card_status	     NUMBER;
15:15:37 333  var_pending_transactions_num   NUMBER;
15:15:37 334  var_pending_invoices_num	     NUMBER;
15:15:37 335  current_def_instrument_type_id NUMBER;
15:15:37 336  current_def_instrument_id      NUMBER;
15:15:37 337  -- EXCEPTIONS
15:15:37 338  BAD_CC_STATUS		   EXCEPTION;
15:15:37 339  PENDING_TRANSACTIONS_FOUNDED EXCEPTION;
15:15:37 340  CAN_NOT_GET_DEF_FINANCIAL    EXCEPTION;
15:15:37 341  CAN_NOT_DEL_DEF_FINANCIAL    EXCEPTION;
15:15:37 342  CAN_NOT_DISABLE_CREDIT_CARD  EXCEPTION;
15:15:37 343  EXCEPTION_MESSAGE 	   VARCHAR2(1024);
15:15:37 344  BEGIN
15:15:37 345  
15:15:37 346  	-- Get credit card status
15:15:37 347  	-- Get account id
15:15:37 348  	SELECT
15:15:37 349  	  CREDIT_CARD.CREDIT_CARD_STATUS_ID,
15:15:37 350  	  CREDIT_CARD.ACCOUNT_ID
15:15:37 351  	  into
15:15:37 352  	  var_credit_card_status,
15:15:37 353  	  var_account_id
15:15:37 354  	FROM
15:15:37 355  	  CREDIT_CARD
15:15:37 356  	WHERE
15:15:37 357  	  CREDIT_CARD.ID = in_credit_card_id;
15:15:37 358  
15:15:37 359  	-- Check that we can disable this credit card (STUB)
15:15:37 360  	IF F_CAN_DISABLE_CREDIT_CARD(in_credit_card_id) = GLOBAL_CONSTANTS_V21.FALSE THEN
15:15:37 361  	  RAISE CAN_NOT_DISABLE_CREDIT_CARD;
15:15:37 362  	END IF;
15:15:37 363  
15:15:37 364  	-- Get account id
15:15:37 365  	SELECT
15:15:37 366  	  ACCOUNT.GROUP_ID into var_group_id
15:15:37 367  	FROM
15:15:37 368  	  ACCOUNT
15:15:37 369  	WHERE
15:15:37 370  	  ACCOUNT.ID = var_account_id;
15:15:37 371  
15:15:37 372  	-- Card should to be active
15:15:37 373  	IF var_credit_card_status != GLOBAL_STATUSES_V21.CREDIT_CARD_ACTIVE THEN
15:15:37 374  	  RAISE BAD_CC_STATUS;
15:15:37 375  	END IF;
15:15:37 376  
15:15:37 377  	-- Looking for pending transactions associated with given credit card
15:15:37 378  	SELECT
15:15:37 379  	  COUNT(*) into var_pending_invoices_num
15:15:37 380  	FROM
15:15:37 381  	  CHARGE
15:15:37 382  	  INNER JOIN TRANSACTION ON CHARGE.TRANSACTION_ID = TRANSACTION.ID
15:15:37 383  	WHERE
15:15:37 384  	  CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V21.INSTRUMENT_CREDIT_CARD
15:15:37 385  	  AND CHARGE.INSTRUMENT_ID = in_credit_card_id
15:15:37 386  	  AND TRANSACTION.TRANSACTION_STATUS_ID = GLOBAL_STATUSES_V21.TRANSACTION_PENDING;
15:15:37 387  
15:15:37 388  	IF var_pending_invoices_num > 0 THEN
15:15:37 389  	  RAISE PENDING_TRANSACTIONS_FOUNDED;
15:15:37 390  	END IF;
15:15:37 391  
15:15:37 392  	-- Getting current default financial instrument
15:15:37 393  	BEGIN
15:15:37 394  	  GET_DEF_FINANCIAL_INSTRUMENT(
15:15:37 395  	    in_group_id 	   => var_group_id,
15:15:37 396  	    out_instrument_type_id => current_def_instrument_type_id,
15:15:37 397  	    out_instrument_id	   => current_def_instrument_id
15:15:37 398  	  );
15:15:37 399  	  EXCEPTION
15:15:37 400  	    WHEN OTHERS THEN
15:15:37 401  	      EXCEPTION_MESSAGE := SQLERRM;
15:15:37 402  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
15:15:37 403  	END;
15:15:37 404  
15:15:37 405  	-- Checking that credit card is not default
15:15:37 406  	IF current_def_instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_CREDIT_CARD
15:15:37 407  	  AND current_def_instrument_id = in_credit_card_id THEN
15:15:37 408  	  BEGIN
15:15:37 409  	    DEL_DEF_FINANCIAL_INSTRUMENT(
15:15:37 410  	      in_group_id => var_group_id
15:15:37 411  	    );
15:15:37 412  	  EXCEPTION
15:15:37 413  	    WHEN OTHERS THEN
15:15:37 414  	      EXCEPTION_MESSAGE := SQLERRM;
15:15:37 415  	      RAISE CAN_NOT_DEL_DEF_FINANCIAL;
15:15:37 416  	  END;
15:15:37 417  	END IF;
15:15:37 418  
15:15:37 419  	-- Update credit card status
15:15:37 420  	PROCS_FIN_INSTRUMENTS_V21.UPDATE_CREDIT_CARD_STATUS(
15:15:37 421  	  in_credit_card_id	   => in_credit_card_id,
15:15:37 422  	  in_updated_by 	   => in_updated_by,
15:15:37 423  	  in_credit_card_status_id => GLOBAL_STATUSES_V21.CREDIT_CARD_DISABLED
15:15:37 424  	);
15:15:37 425  
15:15:37 426  EXCEPTION
15:15:37 427  WHEN NO_DATA_FOUND THEN
15:15:37 428  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:37 429  	  SPROC_NAME, 'No such credit card');
15:15:37 430  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
15:15:37 431  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:37 432  	  SPROC_NAME, 'Could not get current default financial instrument', EXCEPTION_MESSAGE);
15:15:37 433  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
15:15:37 434  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:37 435  	  SPROC_NAME, 'Could not delete information about default financial instrument from account', EXCEPTION_MESSAGE);
15:15:37 436  WHEN BAD_CC_STATUS THEN
15:15:37 437  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
15:15:37 438  	  SPROC_NAME, 'Credit card is not active');
15:15:37 439  WHEN PENDING_TRANSACTIONS_FOUNDED THEN
15:15:37 440  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
15:15:37 441  	  SPROC_NAME, 'Account has pending charge which is using this card');
15:15:37 442  WHEN CAN_NOT_DISABLE_CREDIT_CARD THEN
15:15:37 443  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:37 444  	  SPROC_NAME, 'Could not disable this credit card', EXCEPTION_MESSAGE);
15:15:37 445  WHEN OTHERS THEN
15:15:37 446  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:37 447  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:15:37 448  END DISABLE_CREDIT_CARD;
15:15:37 449  
15:15:37 450  /******************************************************************************/
15:15:37 451  
15:15:37 452  PROCEDURE DISABLE_PAYPAL (
15:15:37 453  /*
15:15:37 454  Throws exceptions:
