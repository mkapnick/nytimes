15:15:35 915  var_offer_duration VARCHAR2(30);
15:15:35 916  var_offer_years	 NUMBER;
15:15:35 917  var_offer_months	 NUMBER;
15:15:35 918  var_offer_days	 NUMBER;
15:15:35 919  var_offer_end_date DATE;
15:15:35 920  -- EXCEPTIONS
15:15:35 921  BAD_OFFER_ID EXCEPTION;
15:15:35 922  BEGIN
15:15:35 923  
15:15:35 924  	BEGIN
15:15:35 925  	  SELECT
15:15:35 926  	    OFFER.ENTITLEMENT_DURATION into var_offer_duration
15:15:35 927  	  FROM
15:15:35 928  	    OFFER
15:15:35 929  	  WHERE
15:15:35 930  	    OFFER.ID = in_offer_id;
15:15:35 931  	  EXCEPTION
15:15:35 932  	    WHEN NO_DATA_FOUND THEN
15:15:35 933  	      RAISE BAD_OFFER_ID;
15:15:35 934  	END;
15:15:35 935  
15:15:35 936  	PROCS_COMMON_V21.ISO8601DURATION_TO_INTERVALS (
15:15:35 937  	  var_offer_duration,
15:15:35 938  	  var_offer_years,
15:15:35 939  	  var_offer_months,
15:15:35 940  	  var_offer_days
15:15:35 941  	);
15:15:35 942  
15:15:35 943  	var_offer_end_date := ( ( in_start_date
15:15:35 944  	  + GLOBAL_CONSTANTS_V21.ONE_DAY_INTERVAL * var_offer_days )
15:15:35 945  	  + GLOBAL_CONSTANTS_V21.ONE_MONTH_INTERVAL * var_offer_months )
15:15:35 946  	  + GLOBAL_CONSTANTS_V21.ONE_YEAR_INTERVAL * var_offer_years;
15:15:35 947  
15:15:35 948  	out_days := var_offer_end_date - in_start_date;
15:15:35 949  
15:15:35 950  EXCEPTION
15:15:35 951  WHEN BAD_OFFER_ID THEN
15:15:35 952  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:35 953  	  SPROC_NAME, 'No such offer');
15:15:35 954  WHEN OTHERS THEN
15:15:35 955  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:35 956  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:15:35 957  END GET_OFFER_LENGTH_IN_DAYS;
