15:15:36 1823  	     AND EXISTS (SELECT 1 FROM TRANSACTION_ATTEMPT ta where ta.transaction_id = CHARGE.TRANSACTION_ID and ta.transaction_attempt_status_id = GLOBAL_STATUSES_V21.TRANS_ATTEMPT_SUCCESS)
15:15:36 1824  	 )
15:15:36 1825  	 LOOP
15:15:36 1826  	   var_charges_amount := var_charges_amount + f_processed_charges.CHARGE_AMOUNT;
15:15:36 1827  	 END LOOP;
15:15:36 1828  
15:15:36 1829  	 IF (in_refund_amount + var_refunds_before > var_invoice_amount) THEN
15:15:36 1830  	   RAISE TOT_REF_IS_GREATER_THAN_ANOUNT;
15:15:36 1831  	 END IF;
15:15:36 1832  
15:15:36 1833  	 IF (in_refund_amount + var_refunds_before > var_charges_amount) THEN
15:15:36 1834  	   RAISE TOT_REF_IS_GRATER_THAN_CHARGES;
15:15:36 1835  	 END IF;
15:15:36 1836  
