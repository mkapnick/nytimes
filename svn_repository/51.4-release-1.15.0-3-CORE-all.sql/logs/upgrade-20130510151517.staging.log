15:15:36 697  /******************************************************************************/
15:15:36 698  
15:15:36 699  PROCEDURE CALCULATE_INVOICE_CHARGEBACK (
15:15:36 700  	in_invoice_id	      IN NUMBER,
15:15:36 701  	in_chargeback_date    IN DATE,
15:15:36 702  	out_chargeback_amount OUT NUMBER
15:15:36 703  ) AS
15:15:36 704  -- VARIABLES
15:15:36 705  SPROC_NAME	     CONSTANT VARCHAR2(28) := 'CALCULATE_INVOICE_CHARGEBACK';
15:15:36 706  var_chargeback_date    DATE;
15:15:36 707  temp_invoice_id	     NUMBER;
15:15:36 708  var_licenses_number    NUMBER;
15:15:36 709  var_invoice_start_date DATE;
15:15:36 710  var_invoice_end_date   DATE;
15:15:36 711  var_offer_id	     NUMBER;
15:15:36 712  var_offer_days_interval NUMBER;
15:15:36 713  var_license_days_used  NUMBER;
15:15:36 714  var_invoice_amount     NUMBER(10,2);
15:15:36 715  var_offer_chain_id     NUMBER;
15:15:36 716  var_offer_chain_meta_data_val VARCHAR2(1024);
15:15:36 717  var_offer_chain_full_refund   NUMBER;
15:15:36 718  var_offer_chain_prorated_ref  NUMBER;
15:15:36 719  var_chargeback_calculated NUMBER;
15:15:36 720  var_max_invoice_refund	NUMBER;
15:15:36 721  -- EXCEPTIONS
15:15:36 722  BAD_INVOICE_ID		  EXCEPTION;
15:15:36 723  NO_LICENSES_FOUND_EXCEPTION EXCEPTION;
15:15:36 724  OFFER_LENGTH_IS_ZERO	  EXCEPTION;
15:15:36 725  BEGIN
15:15:36 726  
15:15:36 727  	IF in_chargeback_date IS NULL THEN
15:15:36 728  	  var_chargeback_date := PROCS_COMMON_V21.NORMALIZE_DATE(current_date);
15:15:36 729  	ELSE
15:15:36 730  	  var_chargeback_date := PROCS_COMMON_V21.NORMALIZE_DATE(in_chargeback_date);
15:15:36 731  	END IF;
15:15:36 732  
15:15:36 733  	-- Check that invoice exists
15:15:36 734  	BEGIN
15:15:36 735  	  SELECT
15:15:36 736  	    INVOICE.ID into temp_invoice_id
15:15:36 737  	  FROM
15:15:36 738  	    INVOICE
15:15:36 739  	  WHERE
15:15:36 740  	    INVOICE.ID = in_invoice_id;
15:15:36 741  	  EXCEPTION
15:15:36 742  	    WHEN NO_DATA_FOUND THEN
15:15:36 743  	      RAISE BAD_INVOICE_ID;
15:15:36 744  	END;
15:15:36 745  
15:15:36 746  	-- Check that invoice has at least one license
15:15:36 747  	SELECT
15:15:36 748  	  COUNT(*) into var_licenses_number
15:15:36 749  	FROM
15:15:36 750  	  LICENSE
15:15:36 751  	WHERE
15:15:36 752  	  LICENSE.INVOICE_ID = in_invoice_id;
15:15:36 753  
15:15:36 754  	IF var_licenses_number = 0 THEN
15:15:36 755  	  RAISE NO_LICENSES_FOUND_EXCEPTION;
15:15:36 756  	END IF;
15:15:36 757  
15:15:36 758  	SELECT
15:15:36 759  	  MIN(LICENSE.START_DATE) into var_invoice_start_date
15:15:36 760  	FROM
15:15:36 761  	  LICENSE
15:15:36 762  	WHERE
15:15:36 763  	  LICENSE.INVOICE_ID = in_invoice_id;
15:15:36 764  
15:15:36 765  	var_invoice_start_date := PROCS_COMMON_V21.NORMALIZE_DATE(var_invoice_start_date);
15:15:36 766  
15:15:36 767  	SELECT DISTINCT
15:15:36 768  	  LICENSE.OFFER_ID into var_offer_id
15:15:36 769  	FROM
15:15:36 770  	  LICENSE
15:15:36 771  	WHERE
15:15:36 772  	  LICENSE.INVOICE_ID = in_invoice_id;
15:15:36 773  
15:15:36 774  	SELECT
15:15:36 775  	  MAX (LICENSE.END_DATE) into var_invoice_end_date
15:15:36 776  	FROM
15:15:36 777  	  LICENSE
15:15:36 778  	WHERE
15:15:36 779  	  LICENSE.INVOICE_ID = in_invoice_id;
15:15:36 780  
15:15:36 781  	-- All licenses for given invoice should point into the same offer
15:15:36 782  
15:15:36 783  	PROCS_OFFER_CHAIN_V21.GET_OFFER_LENGTH_IN_DAYS(
15:15:36 784  	  in_offer_id	=> var_offer_id,
15:15:36 785  	  in_start_date => var_invoice_start_date,
15:15:36 786  	  out_days	=> var_offer_days_interval
15:15:36 787  	);
15:15:36 788  
15:15:36 789  	IF var_offer_days_interval = 0 THEN
15:15:36 790  	  RAISE OFFER_LENGTH_IS_ZERO;
15:15:36 791  	END IF;
15:15:36 792  
15:15:36 793  	PROCS_INVOICE_V21.GET_INVOICE_DAYS_USED_NUMBER(
15:15:36 794  	  in_invoice_id      => in_invoice_id,
15:15:36 795  	  in_chargeback_date => var_chargeback_date,
15:15:36 796  	  out_days_num	     => var_license_days_used
15:15:36 797  	);
15:15:36 798  
15:15:36 799  	PROCS_INVOICE_V21.CALCULATE_INVOICE_AMOUNT(
15:15:36 800  	  in_invoice_id => in_invoice_id,
15:15:36 801  	  out_amount	=> var_invoice_amount
15:15:36 802  	);
15:15:36 803  
15:15:36 804  	-- 39437
15:15:36 805  	-- Get offer chain id by invoice id
15:15:36 806  	SELECT DISTINCT
15:15:36 807  	  SUBSCRIPTION.OFFER_CHAIN_ID into var_offer_chain_id
15:15:36 808  	FROM
15:15:36 809  	  SUBSCRIPTION
15:15:36 810  	  INNER JOIN LICENSE ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:15:36 811  	WHERE
15:15:36 812  	  LICENSE.INVOICE_ID = in_invoice_id;
15:15:36 813  
15:15:36 814  	-- Get offer chain's meta data full amount value
15:15:36 815  	PROCS_OFFER_CHAIN_V21.GET_OFFER_CHAIN_MD_VALUE(
15:15:36 816  	  in_offer_chain_id => var_offer_chain_id,
15:15:36 817  	  in_meta_data_name => GLOBAL_CONSTANTS_V21.OFFER_CHAIN_FULL_REFUND,
15:15:36 818  	  out_value	    => var_offer_chain_meta_data_val
15:15:36 819  	);
15:15:36 820  	IF var_offer_chain_meta_data_val IS NULL THEN
15:15:36 821  	  var_offer_chain_full_refund := NULL;
15:15:36 822  	ELSE
15:15:36 823  	  var_offer_chain_full_refund := TO_NUMBER(var_offer_chain_meta_data_val);
15:15:36 824  	END IF;
15:15:36 825  
15:15:36 826  	-- Get offer chain's meta data prorated amount value
15:15:36 827  	PROCS_OFFER_CHAIN_V21.GET_OFFER_CHAIN_MD_VALUE(
15:15:36 828  	  in_offer_chain_id => var_offer_chain_id,
15:15:36 829  	  in_meta_data_name => GLOBAL_CONSTANTS_V21.OFFER_CHAIN_PRORATED_REFUND,
15:15:36 830  	  out_value	    => var_offer_chain_meta_data_val
15:15:36 831  	);
15:15:36 832  	IF var_offer_chain_meta_data_val IS NULL THEN
15:15:36 833  	  var_offer_chain_prorated_ref := NULL;
15:15:36 834  	ELSE
15:15:36 835  	  var_offer_chain_prorated_ref := TO_NUMBER(var_offer_chain_meta_data_val);
15:15:36 836  	END IF;
15:15:36 837  
15:15:36 838  	var_chargeback_calculated := GLOBAL_CONSTANTS_V21.FALSE;
15:15:36 839  
15:15:36 840  	IF var_offer_chain_full_refund IS NOT NULL
15:15:36 841  	   AND var_chargeback_calculated = GLOBAL_CONSTANTS_V21.FALSE THEN
15:15:36 842  	  IF var_license_days_used < var_offer_chain_full_refund THEN
15:15:36 843  	    out_chargeback_amount := var_invoice_amount;
15:15:36 844  	    var_chargeback_calculated := GLOBAL_CONSTANTS_V21.TRUE;
15:15:36 845  	  END IF;
15:15:36 846  	END IF;
15:15:36 847  
15:15:36 848  	IF var_offer_chain_prorated_ref IS NOT NULL
15:15:36 849  	   AND var_chargeback_calculated = GLOBAL_CONSTANTS_V21.FALSE THEN
15:15:36 850  	  IF var_license_days_used < var_offer_chain_prorated_ref THEN
15:15:36 851  	    out_chargeback_amount := ( var_invoice_amount * (var_offer_days_interval - var_license_days_used) ) / var_offer_days_interval;
15:15:36 852  	    var_chargeback_calculated := GLOBAL_CONSTANTS_V21.TRUE;
15:15:36 853  	  END IF;
15:15:36 854  	END IF;
15:15:36 855  
15:15:36 856  	IF var_chargeback_calculated = GLOBAL_CONSTANTS_V21.TRUE THEN
15:15:36 857  	  PROCS_INVOICE_V21.GET_MAX_REFUND(
15:15:36 858  	    in_invoice_id => in_invoice_id,
15:15:36 859  	    out_amount	  => var_max_invoice_refund
15:15:36 860  	  );
15:15:36 861  	  IF var_max_invoice_refund < out_chargeback_amount THEN
15:15:36 862  	    out_chargeback_amount := var_max_invoice_refund;
15:15:36 863  	  END IF;
15:15:36 864  	END IF;
15:15:36 865  
15:15:36 866  	IF var_chargeback_calculated = GLOBAL_CONSTANTS_V21.FALSE THEN
15:15:36 867  	  out_chargeback_amount := 0;
15:15:36 868  	END IF;
15:15:36 869  
15:15:36 870  EXCEPTION
15:15:36 871  WHEN BAD_INVOICE_ID THEN
15:15:36 872  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 873  	  SPROC_NAME, 'No such invoice');
15:15:36 874  WHEN NO_LICENSES_FOUND_EXCEPTION THEN
15:15:36 875  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 876  	  SPROC_NAME, 'No licenses found');
15:15:36 877  WHEN OFFER_LENGTH_IS_ZERO THEN
15:15:36 878  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:36 879  	  SPROC_NAME, 'Offer length is zero');
15:15:36 880  WHEN OTHERS THEN
15:15:36 881  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 882  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:15:36 883  END CALCULATE_INVOICE_CHARGEBACK;
15:15:36 884  
15:15:36 885  /********************************************************************/
15:15:36 886  
15:15:36 887  PROCEDURE APPLY_REFUND (
15:15:36 888  	in_invoice_id	     IN NUMBER,
15:15:36 889  	in_chargeback_amount IN NUMBER,
15:15:36 890  	in_created_by	     IN VARCHAR2,
15:15:36 891  	out_charge_id	     OUT NUMBER
15:15:36 892  ) AS
15:15:36 893  -- VARIABLES
15:15:36 894  SPROC_NAME		 CONSTANT VARCHAR2(16) := 'APPLY_CHARGEBACK';
15:15:36 895  temp_invoice_id		 NUMBER;
15:15:36 896  var_total_charges_amount	 NUMBER(10,2);
15:15:36 897  var_charge_amount_to_apply NUMBER(10,2);
15:15:36 898  var_transaction_id	 NUMBER;
15:15:36 899  var_instrument_type_id	 NUMBER;
15:15:36 900  var_instrument_id 	 NUMBER;
15:15:36 901  var_charge_id		 NUMBER;
15:15:36 902  -- EXCEPTIONS
15:15:36 903  BAD_INVOICE_ID		 EXCEPTION;
15:15:36 904  CAN_NOT_CREATE_TRANSACTION EXCEPTION;
15:15:36 905  CAN_NOT_FIND_INSTRUMENT	 EXCEPTION;
15:15:36 906  CAN_NOT_CREATE_CHARGE	 EXCEPTION;
15:15:36 907  EXCEPTION_MESSAGE 	 VARCHAR2(1024);
15:15:36 908  BEGIN
15:15:36 909  
15:15:36 910  	BEGIN
15:15:36 911  	  SELECT
15:15:36 912  	    INVOICE.ID into temp_invoice_id
15:15:36 913  	  FROM
15:15:36 914  	    INVOICE
15:15:36 915  	  WHERE
15:15:36 916  	    INVOICE.ID = in_invoice_id;
15:15:36 917  	  EXCEPTION
15:15:36 918  	    WHEN NO_DATA_FOUND THEN
15:15:36 919  	      RAISE BAD_INVOICE_ID;
15:15:36 920  	END;
15:15:36 921  
15:15:36 922  	SELECT
15:15:36 923  	  SUM (CHARGE.CHARGE_AMOUNT) into var_total_charges_amount
15:15:36 924  	FROM
15:15:36 925  	  CHARGE
15:15:36 926  	WHERE
15:15:36 927  	  CHARGE.INVOICE_ID = in_invoice_id
15:15:36 928  	  AND CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V21.CHARGE_PROCESSED
15:15:36 929  	  AND CHARGE.INSTRUMENT_TYPE_ID != GLOBAL_ENUMS_V21.INSTRUMENT_GIFT_CERTIFICATE;
15:15:36 930  
15:15:36 931  	-- FIXME: Maybe whe should throw exception here?
15:15:36 932  	IF var_total_charges_amount < in_chargeback_amount THEN
15:15:36 933  	  var_charge_amount_to_apply := var_total_charges_amount;
15:15:36 934  	ELSE
15:15:36 935  	  var_charge_amount_to_apply := in_chargeback_amount;
15:15:36 936  	END IF;
15:15:36 937  
15:15:36 938  	BEGIN
15:15:36 939  	  PROCS_TRANSACTION_V21.CREATE_TRANSACTION(
15:15:36 940  	    in_transaction_id	      => NULL,
15:15:36 941  	    in_status_id	      => GLOBAL_STATUSES_V21.TRANSACTION_PREPARE,
15:15:36 942  	    in_amount		      => -var_charge_amount_to_apply,
15:15:36 943  	    in_created_by	      => in_created_by,
15:15:36 944  	    in_order_id 	      => NULL,
15:15:36 945  	    in_is_refund	      => GLOBAL_CONSTANTS_V21.TRUE,
15:15:36 946  	    in_transaction_type_code  => 'REFUND',
15:15:36 947  	    out_transaction_id	      => var_transaction_id
15:15:36 948  	  );
15:15:36 949  	  EXCEPTION
15:15:36 950  	    WHEN OTHERS THEN
15:15:36 951  	      EXCEPTION_MESSAGE := SQLERRM;
15:15:36 952  	      RAISE CAN_NOT_CREATE_TRANSACTION;
15:15:36 953  	END;
15:15:36 954  
15:15:36 955  	BEGIN
15:15:36 956  	  SELECT
15:15:36 957  	    C.INSTRUMENT_TYPE_ID,
15:15:36 958  	    C.INSTRUMENT_ID
15:15:36 959  	    into
15:15:36 960  	    var_instrument_type_id,
15:15:36 961  	    var_instrument_id
15:15:36 962  	  FROM
15:15:36 963  	    CHARGE C,
15:15:36 964  	    TRANSACTION_ATTEMPT TA,
15:15:36 965  	    TRANSACTION T
15:15:36 966  	  WHERE
15:15:36 967  	    C.INVOICE_ID = in_invoice_id and
15:15:36 968  	    C.TRANSACTION_ID = T.ID and
15:15:36 969  	    TA.TRANSACTION_ID = T.ID and
15:15:36 970  	    TA.TRANSACTION_ATTEMPT_STATUS_ID = GLOBAL_STATUSES_V21.TRANS_ATTEMPT_SUCCESS and
15:15:36 971  	    T.IS_REFUND = GLOBAL_CONSTANTS_V21.FALSE and
15:15:36 972  	    T.TRANSACTION_AMOUNT >= 0 and
15:15:36 973  	    rownum < 2;
15:15:36 974  	EXCEPTION
15:15:36 975  	    WHEN NO_DATA_FOUND THEN
15:15:36 976  	      RAISE CAN_NOT_FIND_INSTRUMENT;
15:15:36 977  	END;
15:15:36 978  	BEGIN
15:15:36 979  	  PROCS_CHARGE_V21.CREATE_CHARGE(
15:15:36 980  	    in_invoice_id	  => in_invoice_id,
15:15:36 981  	    in_transaction_id	  => var_transaction_id,
15:15:36 982  	    in_instrument_type_id => var_instrument_type_id,
15:15:36 983  	    in_instrument_id	  => var_instrument_id,
15:15:36 984  	    in_charge_amount	  => -var_charge_amount_to_apply,
15:15:36 985  	    in_created_by	  => in_created_by,
15:15:36 986  	    in_charge_status_id   => GLOBAL_STATUSES_V21.CHARGE_OPENED,
15:15:36 987  	    out_charge_id	  => var_charge_id
15:15:36 988  	  );
15:15:36 989  	  EXCEPTION
15:15:36 990  	    WHEN OTHERS THEN
15:15:36 991  	      EXCEPTION_MESSAGE := SQLERRM;
15:15:36 992  	      RAISE CAN_NOT_CREATE_CHARGE;
15:15:36 993  	END;
15:15:36 994  
15:15:36 995  	out_charge_id := var_charge_id;
15:15:36 996  
15:15:36 997  EXCEPTION
15:15:36 998  WHEN BAD_INVOICE_ID THEN
15:15:36 999  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 1000  	   SPROC_NAME, 'No such invoice');
15:15:36 1001  WHEN CAN_NOT_CREATE_TRANSACTION THEN
15:15:36 1002  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:36 1003  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
15:15:36 1004  WHEN CAN_NOT_FIND_INSTRUMENT THEN
15:15:36 1005  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 1006  	   SPROC_NAME, 'Could not find financial instrument');
15:15:36 1007  WHEN CAN_NOT_CREATE_CHARGE THEN
15:15:36 1008  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:36 1009  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
15:15:36 1010  WHEN OTHERS THEN
15:15:36 1011  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 1012  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:15:36 1013  END APPLY_REFUND;
