15:15:36 266  	      FROM (
15:15:36 267  		SELECT
15:15:36 268  		  ID
15:15:36 269  		FROM
15:15:36 270  		  SUBSCRIPTION
15:15:36 271  		WHERE
15:15:36 272  		  SUBSCRIPTION.ACCOUNT_ID = var_account_id
15:15:36 273  		  AND SUBSCRIPTION.OFFER_CHAIN_ID = f_account_offer_chains.OFFER_CHAIN_ID
15:15:36 274  		  AND (
