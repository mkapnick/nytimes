15:15:37 916  /******************************************************************************/
15:15:37 917  
15:15:37 918  PROCEDURE FINALIZE_GC_PURCHASING (
15:15:37 919  	in_invoice_id	      IN NUMBER,
15:15:37 920  	in_created_by	      IN VARCHAR2,
15:15:37 921  	in_instrument_id      IN NUMBER,
15:15:37 922  	in_instrument_type_id IN NUMBER,
15:15:37 923  	in_order_id	      IN VARCHAR2,
15:15:37 924  	in_transaction_id     IN NUMBER,
15:15:37 925  	out_charge_amount     OUT NUMBER
15:15:37 926  ) AS
15:15:37 927  SPROC_NAME CONSTANT VARCHAR2(22) := 'FINALIZE_GC_PURCHASING';
15:15:37 928  -- VARIABLES
15:15:37 929  temp_transaction_id_count NUMBER;
15:15:37 930  var_invoice_amount	NUMBER(10,2);
15:15:37 931  var_transaction_id	NUMBER;
15:15:37 932  var_new_charge_id 	NUMBER;
15:15:37 933  -- EXCEPTIONS
15:15:37 934  BAD_CREDIT_CARD_ID	  EXCEPTION;
15:15:37 935  BAD_PAYPAL_ID		  EXCEPTION;
15:15:37 936  BAD_INSTRUMENT_TYPE	  EXCEPTION;
15:15:37 937  TRANSACTION_EXISTS	  EXCEPTION;
15:15:37 938  CAN_NOT_CALC_INVOICE_AMOUNT EXCEPTION;
15:15:37 939  CAN_NOT_USE_FCINSTR	  EXCEPTION;
15:15:37 940  CAN_NOT_CREATE_TRANSACTION  EXCEPTION;
15:15:37 941  CAN_NOT_CREATE_CHARGE	  EXCEPTION;
15:15:37 942  EXCEPTION_MESSAGE   VARCHAR2(1024);
15:15:37 943  BEGIN
15:15:37 944  
15:15:37 945  	-- Check that instrument exists
15:15:37 946  	IF in_instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_CREDIT_CARD THEN
15:15:37 947  	  IF IS_CREDIT_CARD_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V21.FALSE THEN
15:15:37 948  	    RAISE BAD_CREDIT_CARD_ID;
15:15:37 949  	  END IF;
15:15:37 950  	ELSIF in_instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_PAYPAL THEN
15:15:37 951  	  IF IS_PAYPAL_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V21.FALSE THEN
15:15:37 952  	    RAISE BAD_PAYPAL_ID;
15:15:37 953  	  END IF;
15:15:37 954  	ELSIF in_instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_ZCI_INSTRUMENT THEN
15:15:37 955  	  NULL;
15:15:37 956  	ELSE
15:15:37 957  	  RAISE BAD_INSTRUMENT_TYPE;
15:15:37 958  	END IF;
15:15:37 959  
15:15:37 960  	-- Check that transaction with given id do not exists
15:15:37 961  	SELECT
15:15:37 962  	  COUNT(*) into temp_transaction_id_count
15:15:37 963  	FROM
15:15:37 964  	  TRANSACTION
15:15:37 965  	WHERE
15:15:37 966  	  TRANSACTION.ID = in_transaction_id;
15:15:37 967  
15:15:37 968  	IF temp_transaction_id_count > 0 THEN
15:15:37 969  	  RAISE TRANSACTION_EXISTS;
15:15:37 970  	END IF;
15:15:37 971  
15:15:37 972  	-- Calculate new invoice amount
15:15:37 973  	BEGIN
15:15:37 974  	  PROCS_INVOICE_V21.CALCULATE_INVOICE_AMOUNT(in_invoice_id, var_invoice_amount);
15:15:37 975  	  EXCEPTION
15:15:37 976  	    WHEN OTHERS THEN
15:15:37 977  	      EXCEPTION_MESSAGE := SQLERRM;
15:15:37 978  	      RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
15:15:37 979  	END;
15:15:37 980  
15:15:37 981  	IF var_invoice_amount > 0
15:15:37 982  	  AND in_instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_ZCI_INSTRUMENT THEN
15:15:37 983  	  RAISE CAN_NOT_USE_FCINSTR;
15:15:37 984  	END IF;
15:15:37 985  
15:15:37 986  	-- If invoice amount iz 0 then we need to set status for this invoice to PROCCESSED
15:15:37 987  	IF var_invoice_amount = 0 THEN
15:15:37 988  	  PROCS_INVOICE_CRU_V21.UPDATE_INVOICE(
15:15:37 989  	    in_invoice_id		   => in_invoice_id,
15:15:37 990  	    in_updated_by		   => in_created_by,
15:15:37 991  	    in_invoice_status_id	   => GLOBAL_STATUSES_V21.INVOICE_CLOSED
15:15:37 992  	  );
15:15:37 993  	END IF;
15:15:37 994  
15:15:37 995  	IF var_invoice_amount > 0 THEN
15:15:37 996  	  -- Create transaction
15:15:37 997  	  BEGIN
15:15:37 998  	    PROCS_TRANSACTION_V21.CREATE_TRANSACTION(
15:15:37 999  	      in_transaction_id        => in_transaction_id,
15:15:37 1000  	       in_status_id		=> GLOBAL_STATUSES_V21.TRANSACTION_PENDING,
15:15:37 1001  	       in_amount		=> var_invoice_amount,
15:15:37 1002  	       in_created_by		=> in_created_by,
15:15:37 1003  	       in_order_id		=> in_order_id,
15:15:37 1004  	       in_transaction_type_code => 'GIFT_CERTIFICATE_PURCHASE',
15:15:37 1005  	       out_transaction_id	=> var_transaction_id
15:15:37 1006  	     );
15:15:37 1007  	     EXCEPTION
15:15:37 1008  	       WHEN OTHERS THEN
15:15:37 1009  		 EXCEPTION_MESSAGE := SQLERRM;
15:15:37 1010  		 RAISE CAN_NOT_CREATE_TRANSACTION;
15:15:37 1011  	   END;
15:15:37 1012  
15:15:37 1013  	   -- Create charge
15:15:37 1014  	   BEGIN
15:15:37 1015  	     PROCS_CHARGE_V21.CREATE_CHARGE(
15:15:37 1016  	       in_invoice_id	     => in_invoice_id,
15:15:37 1017  	       in_transaction_id     => var_transaction_id,
15:15:37 1018  	       in_instrument_type_id => in_instrument_type_id,
15:15:37 1019  	       in_instrument_id      => in_instrument_id,
15:15:37 1020  	       in_charge_amount      => var_invoice_amount,
15:15:37 1021  	       in_created_by	     => in_created_by,
15:15:37 1022  	       in_charge_status_id   => GLOBAL_STATUSES_V21.CHARGE_OPENED,
15:15:37 1023  	       out_charge_id	     => var_new_charge_id
15:15:37 1024  	     );
15:15:37 1025  	     out_charge_amount := var_invoice_amount;
15:15:37 1026  	     EXCEPTION
15:15:37 1027  	       WHEN OTHERS THEN
15:15:37 1028  		 EXCEPTION_MESSAGE := SQLERRM;
15:15:37 1029  		 RAISE CAN_NOT_CREATE_CHARGE;
15:15:37 1030  	   END;
15:15:37 1031  	 ELSE
15:15:37 1032  	   out_charge_amount := 0;
15:15:37 1033  	 END IF;
15:15:37 1034  
15:15:37 1035  EXCEPTION
15:15:37 1036  WHEN CAN_NOT_USE_FCINSTR THEN
15:15:37 1037  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
15:15:37 1038  	   SPROC_NAME, 'Could not use "free charge instrument" for non-zero invoice');
15:15:37 1039  WHEN BAD_CREDIT_CARD_ID THEN
15:15:37 1040  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:37 1041  	   SPROC_NAME, 'Bad credit card id');
15:15:37 1042  WHEN BAD_PAYPAL_ID THEN
15:15:37 1043  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:37 1044  	   SPROC_NAME, 'Bad paypal id');
15:15:37 1045  WHEN BAD_INSTRUMENT_TYPE THEN
15:15:37 1046  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INVALID_PARAMETER,
15:15:37 1047  	   SPROC_NAME, 'Bad instrument type');
15:15:37 1048  WHEN TRANSACTION_EXISTS THEN
15:15:37 1049  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.DUPLICATE_ERROR,
15:15:37 1050  	   SPROC_NAME, 'Transaction with given id already exists');
15:15:37 1051  WHEN CAN_NOT_CREATE_TRANSACTION THEN
15:15:37 1052  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:37 1053  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
15:15:37 1054  WHEN CAN_NOT_CREATE_CHARGE THEN
15:15:37 1055  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:37 1056  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
15:15:37 1057  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
15:15:37 1058  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:37 1059  	   SPROC_NAME, 'Could not calculate amount for new invoice', EXCEPTION_MESSAGE);
15:15:37 1060  WHEN OTHERS THEN
15:15:37 1061  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:37 1062  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:15:37 1063  END FINALIZE_GC_PURCHASING;
15:15:37 1064  
15:15:37 1065  /******************************************************************************/
15:15:37 1066  
15:15:37 1067  PROCEDURE PURCHASE_GIFT_CERTIFICATE (
15:15:37 1068  	 in_group_id		   IN NUMBER,
15:15:37 1069  	 in_offer_chain_id	   IN VARCHAR2,
15:15:37 1070  	 in_gift_certificate_code  IN VARCHAR2,
15:15:37 1071  	 in_created_by		   IN VARCHAR2,
15:15:37 1072  	 in_recipient_name	   IN VARCHAR2,
15:15:37 1073  	 in_recipient_email	   IN VARCHAR2,
15:15:37 1074  	 in_sender_name 	   IN VARCHAR2,
15:15:37 1075  	 in_sender_email	   IN VARCHAR2,
15:15:37 1076  	 in_gift_message	   IN VARCHAR2,
15:15:37 1077  	 in_instrument_id	   IN NUMBER,
15:15:37 1078  	 in_instrument_type_id	   IN NUMBER,
15:15:37 1079  	 in_expiration_date	   IN DATE,
15:15:37 1080  	 in_order_id		   IN VARCHAR2,
15:15:37 1081  	 in_transaction_id	   IN NUMBER
15:15:37 1082  ) AS
15:15:37 1083  SPROC_NAME	  CONSTANT VARCHAR2(25) := 'PURCHASE_GIFT_CERTIFICATE';
15:15:37 1084  -- VARIABLES
15:15:37 1085  var_gift_cert_id   NUMBER;
15:15:37 1086  var_account_id	  NUMBER;
15:15:37 1087  var_invoice_amount NUMBER (10,2);
15:15:37 1088  var_new_invoice_id NUMBER;
15:15:37 1089  var_new_charge_id  NUMBER;
15:15:37 1090  var_och_is_gc	  NUMBER;
15:15:37 1091  var_offer_chain_status_id NUMBER;
15:15:37 1092  var_is_for_redemption	 NUMBER;
15:15:37 1093  var_account_tax_exempt_id VARCHAR2(255);
15:15:37 1094  
15:15:37 1095  temp_transaction_id_count NUMBER;
15:15:37 1096  var_transaction_id	 NUMBER;
15:15:37 1097  temp_gc_code VARCHAR2(255);
15:15:37 1098  
15:15:37 1099  var_invoice_status_id NUMBER;
15:15:37 1100  -- EXCEPTIONS
15:15:37 1101  CAN_NOT_CREATE_INVOICE		   EXCEPTION;
15:15:37 1102  CAN_NOT_CREATE_TRANSACTION	   EXCEPTION;
15:15:37 1103  CAN_NOT_CREATE_CHARGE		   EXCEPTION;
15:15:37 1104  CAN_NOT_CREATE_LINE_ITEMS	   EXCEPTION;
15:15:37 1105  BAD_GROUP_ID			   EXCEPTION;
15:15:37 1106  BAD_OFFER_CHAIN_ID		   EXCEPTION;
15:15:37 1107  OCH_IS_NOT_GIFT_CERTIFICATE	   EXCEPTION;
15:15:37 1108  TRANSACTION_EXISTS		   EXCEPTION;
15:15:37 1109  GC_CODE_ALREADY_EXISTS		   EXCEPTION;
15:15:37 1110  BAD_INSTRUMENT_TYPE		   EXCEPTION;
15:15:37 1111  BAD_CREDIT_CARD_ID		   EXCEPTION;
15:15:37 1112  BAD_PAYPAL_ID			   EXCEPTION;
15:15:37 1113  CAN_NOT_CALCULATE_OCH_AMOUNT	   EXCEPTION;
15:15:37 1114  BAD_OFFER_CHAIN_STATUS		   EXCEPTION;
15:15:37 1115  OFFER_REC_NUM_LESS_THAN_ONE	   EXCEPTION;
15:15:37 1116  CAN_NOT_CALC_INVOICE_AMOUNT	   EXCEPTION;
15:15:37 1117  CAN_NOT_USE_FCINSTR		   EXCEPTION;
15:15:37 1118  CAN_NOT_PURCHASE_GC_FOR_RDMPN	   EXCEPTION;
15:15:37 1119  EXCEPTION_MESSAGE		   VARCHAR2(1024);
15:15:37 1120  BEGIN
15:15:37 1121  
15:15:37 1122  	 -- Get account id
15:15:37 1123  	 BEGIN
15:15:37 1124  	   SELECT
15:15:37 1125  	     ACCOUNT.ID,
15:15:37 1126  	     ACCOUNT.TAX_EXEMPT_ID
15:15:37 1127  	     into
15:15:37 1128  	     var_account_id,
15:15:37 1129  	     var_account_tax_exempt_id
15:15:37 1130  	   FROM
15:15:37 1131  	     ACCOUNT
15:15:37 1132  	   WHERE
15:15:37 1133  	     ACCOUNT.GROUP_ID = in_group_id;
15:15:37 1134  	   EXCEPTION
15:15:37 1135  	   WHEN NO_DATA_FOUND THEN
15:15:37 1136  	     RAISE BAD_GROUP_ID;
15:15:37 1137  	 END;
15:15:37 1138  
15:15:37 1139  	 -- Check that instrument exists
15:15:37 1140  	 IF in_instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_CREDIT_CARD THEN
15:15:37 1141  	   IF IS_CREDIT_CARD_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V21.FALSE THEN
15:15:37 1142  	     RAISE BAD_CREDIT_CARD_ID;
15:15:37 1143  	   END IF;
15:15:37 1144  	 ELSIF in_instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_PAYPAL THEN
15:15:37 1145  	   IF IS_PAYPAL_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V21.FALSE THEN
15:15:37 1146  	     RAISE BAD_PAYPAL_ID;
15:15:37 1147  	   END IF;
15:15:37 1148  	 ELSIF in_instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_ZCI_INSTRUMENT THEN
15:15:37 1149  	   NULL;
15:15:37 1150  	 ELSE
15:15:37 1151  	   RAISE BAD_INSTRUMENT_TYPE;
15:15:37 1152  	 END IF;
15:15:37 1153  
15:15:37 1154  	 -- Check for the same code
15:15:37 1155  	 BEGIN
15:15:37 1156  	   SELECT
15:15:37 1157  	     GIFT_CERTIFICATE.CODE into temp_gc_code
15:15:37 1158  	   FROM
15:15:37 1159  	     GIFT_CERTIFICATE
15:15:37 1160  	   WHERE
15:15:37 1161  	     GIFT_CERTIFICATE.CODE = in_gift_certificate_code;
15:15:37 1162  
15:15:37 1163  	   RAISE GC_CODE_ALREADY_EXISTS;
15:15:37 1164  
15:15:37 1165  	   EXCEPTION
15:15:37 1166  	     WHEN NO_DATA_FOUND THEN
15:15:37 1167  	       NULL;
15:15:37 1168  	 END;
15:15:37 1169  
15:15:37 1170  	 -- Get offer chain flag "is_gift_certificate"
15:15:37 1171  	 BEGIN
15:15:37 1172  	   SELECT
15:15:37 1173  	     OFFER_CHAIN.IS_GIFT_CERTIFICATE,
15:15:37 1174  	     OFFER_CHAIN.OFFER_CHAIN_STATUS_ID
15:15:37 1175  	     into
15:15:37 1176  	     var_och_is_gc,
15:15:37 1177  	     var_offer_chain_status_id
15:15:37 1178  	   FROM
15:15:37 1179  	     OFFER_CHAIN
15:15:37 1180  	   WHERE
15:15:37 1181  	     OFFER_CHAIN.ID = in_offer_chain_id;
15:15:37 1182  	   EXCEPTION
15:15:37 1183  	   WHEN NO_DATA_FOUND THEN
15:15:37 1184  	     RAISE BAD_OFFER_CHAIN_ID;
15:15:37 1185  	 END;
15:15:37 1186  
15:15:37 1187  	 IF var_och_is_gc != GLOBAL_CONSTANTS_V21.TRUE
15:15:37 1188  	   OR var_och_is_gc IS NULL THEN
15:15:37 1189  	   RAISE OCH_IS_NOT_GIFT_CERTIFICATE;
15:15:37 1190  	 END IF;
15:15:37 1191  
15:15:37 1192  	 IF var_offer_chain_status_id != GLOBAL_STATUSES_V21.OFFER_CHAIN_ACTIVE THEN
15:15:37 1193  	   RAISE BAD_OFFER_CHAIN_STATUS;
15:15:37 1194  	 END IF;
15:15:37 1195  
15:15:37 1196  	 -- norlov: #38151 check if the OC is for Redemption:
15:15:37 1197  	 SELECT
15:15:37 1198  	   COUNT(*) into var_is_for_redemption
15:15:37 1199  	 FROM
15:15:37 1200  	   OFFER_CHAIN_ELIGIBILITY
15:15:37 1201  	 WHERE
15:15:37 1202  	   OFFER_CHAIN_ELIGIBILITY.OFFER_CHAIN_ID = in_offer_chain_id
15:15:37 1203  	   AND OFFER_CHAIN_ELIGIBILITY.NAME = GLOBAL_CONSTANTS_V21.GIFT_CERTIFICATE_REQUIRED
15:15:37 1204  	   AND OFFER_CHAIN_ELIGIBILITY.VALUE = GLOBAL_CONSTANTS_V21.ELIGIBILITY_FLAG_SET;
15:15:37 1205  
15:15:37 1206  	 IF var_is_for_redemption > 0 THEN
15:15:37 1207  	   RAISE CAN_NOT_PURCHASE_GC_FOR_RDMPN;
15:15:37 1208  	 END IF;
15:15:37 1209  
15:15:37 1210  	 -- Check that transaction with given id do not exists
15:15:37 1211  	 SELECT
15:15:37 1212  	   COUNT(*) into temp_transaction_id_count
15:15:37 1213  	 FROM
15:15:37 1214  	   TRANSACTION
15:15:37 1215  	 WHERE
15:15:37 1216  	   TRANSACTION.ID = in_transaction_id;
15:15:37 1217  
15:15:37 1218  	 IF temp_transaction_id_count > 0 THEN
15:15:37 1219  	   RAISE TRANSACTION_EXISTS;
15:15:37 1220  	 END IF;
15:15:37 1221  
15:15:37 1222  	 -- Create new invoice
15:15:37 1223  	 BEGIN
15:15:37 1224  	   PROCS_INVOICE_V21.CREATE_INVOICE(
15:15:37 1225  	     in_invoice_status => GLOBAL_STATUSES_V21.INVOICE_OPEN,
15:15:37 1226  	     in_created_by     => in_created_by,
15:15:37 1227  	     in_tax_exempt_id  => var_account_tax_exempt_id,
15:15:37 1228  	     out_invoice_id    => var_new_invoice_id
15:15:37 1229  	   );
15:15:37 1230  	   EXCEPTION
15:15:37 1231  	     WHEN OTHERS THEN
15:15:37 1232  	       EXCEPTION_MESSAGE := SQLERRM;
15:15:37 1233  	       RAISE CAN_NOT_CREATE_INVOICE;
15:15:37 1234  	 END;
15:15:37 1235  
15:15:37 1236  	 -- Add line items for new invoice
15:15:37 1237  	 BEGIN
15:15:37 1238  	   FOR f_offer_data IN (
15:15:37 1239  	     SELECT
15:15:37 1240  	       OFFER_ID,
15:15:37 1241  	       NUM_RECURRENCES
15:15:37 1242  	     FROM
15:15:37 1243  	       OFFER_OFFER_CHAIN
15:15:37 1244  	     WHERE
15:15:37 1245  	       OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
15:15:37 1246  	   )
15:15:37 1247  	   LOOP
15:15:37 1248  	     IF f_offer_data.NUM_RECURRENCES < 1 THEN
15:15:37 1249  	       RAISE OFFER_REC_NUM_LESS_THAN_ONE;
15:15:37 1250  	     END IF;
15:15:37 1251  	     FOR i_offer_recurrences_iterator IN 1..f_offer_data.NUM_RECURRENCES
15:15:37 1252  	     LOOP
15:15:37 1253  	       PROCS_LINE_ITEMS_V21.ADD_LINE_ITEMS(
15:15:37 1254  		 in_invoice_id => var_new_invoice_id,
15:15:37 1255  		 in_offer_id   => f_offer_data.OFFER_ID,
