15:15:36 2384  	       EXCEPTION_MESSAGE := SQLERRM;
15:15:36 2385  	       RAISE CAN_NOT_UPDATE_SUBSCRIPTION;
15:15:36 2386  	 END;
15:15:36 2387  
15:15:36 2388  	 -- Terminate grace period for licenses in grace [SAR-31]
15:15:36 2389  	 BEGIN
15:15:36 2390  	   SELECT
15:15:36 2391  	     LICENSE.ID into var_license_to_disgrace
15:15:36 2392  	   FROM
15:15:36 2393  	     LICENSE
15:15:36 2394  	   WHERE
15:15:36 2395  	     LICENSE.SUBSCRIPTION_ID = in_subscription_id
15:15:36 2396  	     AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V21.LICENSE_IN_GRACE_PERIOD
15:15:36 2397  	     AND ROWNUM <= 1
15:15:36 2398  	   ORDER BY
15:15:36 2399  	     CREATE_DATE DESC;
15:15:36 2400  	 EXCEPTION
