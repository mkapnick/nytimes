15:15:36 391  	  CHARGE.CHARGE_STATUS_ID
15:15:36 392  	FROM
15:15:36 393  	  CHARGE
15:15:36 394  	WHERE
15:15:36 395  	  CHARGE.INVOICE_ID = in_invoice_id
15:15:36 396  	  AND CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V21.INSTRUMENT_PAYPAL;
15:15:36 397  
15:15:36 398  	-- get all credir cards for given invoice
15:15:36 399  	OPEN out_cc_charges_set FOR
