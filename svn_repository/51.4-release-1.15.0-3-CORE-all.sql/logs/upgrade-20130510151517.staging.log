15:15:36 658  /******************************************************************************/
15:15:36 659  
15:15:36 660  PROCEDURE GET_SUBSCRIPTION_INFO (
15:15:36 661  /*
15:15:36 662  Throws exceptions:
15:15:36 663  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
15:15:36 664  */
15:15:36 665  	  in_subscription_id  IN  NUMBER,
15:15:36 666  	  out_result_set      OUT SYS_REFCURSOR
15:15:36 667  ) AS
15:15:36 668  SPROC_NAME CONSTANT VARCHAR2(21) := 'GET_SUBSCRIPTION_INFO';
15:15:36 669  -- VARIABLES
15:15:36 670  temp_subscription_id NUMBER;
15:15:36 671  -- EXCEPTIONS
15:15:36 672  BAD_SUBSCRIPTION_ID EXCEPTION;
15:15:36 673  BEGIN
15:15:36 674  	-- Find subscription by id
15:15:36 675  	-- Return its details
15:15:36 676  
15:15:36 677  	BEGIN
15:15:36 678  	  SELECT
15:15:36 679  	    SUBSCRIPTION.ID into temp_subscription_id
15:15:36 680  	  FROM
15:15:36 681  	    SUBSCRIPTION
15:15:36 682  	  WHERE
15:15:36 683  	    SUBSCRIPTION.ID = in_subscription_id;
15:15:36 684  	  EXCEPTION
15:15:36 685  	    WHEN NO_DATA_FOUND THEN
15:15:36 686  	      RAISE BAD_SUBSCRIPTION_ID;
15:15:36 687  	END;
15:15:36 688  
15:15:36 689  	OPEN out_result_set FOR
15:15:36 690  	SELECT
15:15:36 691  	  SUBSCRIPTION.ID AS "SUBSCRIPTION_ID",
15:15:36 692  	  SUBSCRIPTION.SUBSCRIPTION_STATUS_ID,
15:15:36 693  	  SUBSCRIPTION.PURCHASE_DATE,
15:15:36 694  	  SUBSCRIPTION.SUSPEND_DATE,
15:15:36 695  	  SUBSCRIPTION.REACTIVATION_DATE,
15:15:36 696  	  SUBSCRIPTION.CANCELLATION_DATE,
15:15:36 697  	  SUBSCRIPTION_CANCEL_REASON.VALUE as "CANCEL_TYPE",
15:15:36 698  	  OFFER_CHAIN.ID AS "OFFER_CHAIN_ID",
15:15:36 699  	  OFFER_CHAIN.NAME,
15:15:36 700  	  OFFER_CHAIN.DESCRIPTION,
15:15:36 701  	  OFFER_CHAIN.PRODUCT_URI,
15:15:36 702  	  SUBSCRIPTION.INSTRUMENT_ID,
15:15:36 703  	  SUBSCRIPTION.INSTRUMENT_TYPE_ID,
15:15:36 704  	  PROCS_SUBSCRIPTION_V21.CALC_SUBSCRIPTION_END_DATE(SUBSCRIPTION.ID) as "END_DATE",
15:15:36 705  	  PROCS_SUBSCRIPTION_V21.GET_RECENT_CHARGE(SUBSCRIPTION.ID) AS "RECENT_CHARGE",
15:15:36 706  	  PROCS_SUBSCRIPTION_V21.GET_RENEWAL_DATE(SUBSCRIPTION.ID) AS "RENEWAL_DATE",
15:15:36 707  	  PROCS_SUBSCRIPTION_V21.GET_BILLING_CYCLE(SUBSCRIPTION.ID) AS "BILLING_CYCLE",
15:15:36 708  	  (
15:15:36 709  	    SELECT
15:15:36 710  	      ACCOUNT.GROUP_ID
15:15:36 711  	      FROM ACCOUNT
15:15:36 712  	      WHERE ACCOUNT.ID = SUBSCRIPTION.ACCOUNT_ID
15:15:36 713  	  ) as "GROUP_ID",
15:15:36 714  	  (
15:15:36 715  	    SELECT
15:15:36 716  	      MAX(ENTITLEMENT_END_DATE)
15:15:36 717  	      FROM LICENSE
15:15:36 718  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:15:36 719  	  )
15:15:36 720  	  as "ENT_END_DATE",
15:15:36 721  	  (
15:15:36 722  	    SELECT
15:15:36 723  	      MIN(START_DATE)
15:15:36 724  	      FROM LICENSE
15:15:36 725  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:15:36 726  	  )
15:15:36 727  	  as "ENT_START_DATE",
15:15:36 728  	  PROCS_SUBSCRIPTION_V21.IS_SUBSCRIPTION_CANCELABLE(SUBSCRIPTION.ID) AS "IS_CANCELABLE",
15:15:36 729  	  ITUNES_RECEIPT.ID AS "ITUNES_RECEIPT_ID",
15:15:36 730  	  GROUP_ACCOUNT.ID GA_ID,
15:15:36 731  	  GROUP_ACCOUNT.SUBSCRIPTION_ID GA_SUBSCRIPTION_ID,
15:15:36 732  	  GROUP_ACCOUNT.GROUP_NAME GA_GROUP_NAME,
15:15:36 733  	  GROUP_ACCOUNT.FIRST_NAME GA_FIRST_NAME,
15:15:36 734  	  GROUP_ACCOUNT.LAST_NAME GA_LAST_NAME,
15:15:36 735  	  GROUP_ACCOUNT.EMAIL GA_EMAIL,
15:15:36 736  	  GROUP_ACCOUNT.PHONE GA_PHONE,
15:15:36 737  	  GROUP_ACCOUNT.ORGANIZATION_TYPE GA_ORGANIZATION_TYPE,
15:15:36 738  	  GROUP_ACCOUNT.SEATS GA_SEATS,
15:15:36 739  	  PROCS_GROUP_ACCOUNT_V21.F_GET_NUM_OCCUPIED_GROUP_SEATS(GROUP_ACCOUNT.ID) GA_SEATS_USED,
15:15:36 740  	  GROUP_ACCOUNT.IP GA_IP,
15:15:36 741  	  PROCS_SUBSCRIPTION_V21.GET_GIFT_CERT_CODE_BY_SUB_ID(SUBSCRIPTION.ID) gift_certificate_code,
15:15:36 742  	  PROCS_ACCOUNT_V21.GET_GRACE_START_DATE(SUBSCRIPTION.ID) GRACE_START_DATE,
15:15:36 743  	  PROCS_ACCOUNT_V21.GET_GRACE_END_DATE(SUBSCRIPTION.ID) GRACE_END_DATE
15:15:36 744  	FROM
15:15:36 745  	  SUBSCRIPTION
15:15:36 746  	  INNER JOIN OFFER_CHAIN ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
15:15:36 747  	  LEFT JOIN SUBSCRIPTION_CANCEL_REASON ON SUBSCRIPTION.SCT_ID = SUBSCRIPTION_CANCEL_REASON.ID
15:15:36 748  	  LEFT JOIN ITUNES_RECEIPT ON SUBSCRIPTION.ID = ITUNES_RECEIPT.SUBSCRIPTION_ID
15:15:36 749  	  LEFT JOIN GROUP_ACCOUNT ON SUBSCRIPTION.ID = GROUP_ACCOUNT.SUBSCRIPTION_ID
15:15:36 750  	WHERE
15:15:36 751  	  SUBSCRIPTION.ID = in_subscription_id;
15:15:36 752  
15:15:36 753  EXCEPTION
15:15:36 754  WHEN BAD_SUBSCRIPTION_ID THEN
15:15:36 755  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 756  	  SPROC_NAME, 'No such subscription');
15:15:36 757  WHEN OTHERS THEN
15:15:36 758  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 759  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:15:36 760  END GET_SUBSCRIPTION_INFO;
