15:15:36 1801  	       AND EXISTS (
15:15:36 1802  		 SELECT 1 FROM TRANSACTION_ATTEMPT ta where ta.transaction_id = CHARGE.TRANSACTION_ID and ta.transaction_attempt_status_id = GLOBAL_STATUSES_V21.TRANS_ATTEMPT_SUCCESS
15:15:36 1803  	       )
15:15:36 1804  	     )
15:15:36 1805  	   )
15:15:36 1806  	   AND CHARGE.CHARGE_AMOUNT < 0;
15:15:36 1807  
15:15:36 1808  	 -- Refunds are negative
15:15:36 1809  	 IF var_refunds_before IS NULL THEN var_refunds_before := 0; END IF;
15:15:36 1810  	 var_refunds_before := 0 - var_refunds_before;
15:15:36 1811  
15:15:36 1812  	 var_charges_amount := 0;
15:15:36 1813  
15:15:36 1814  	 FOR f_processed_charges IN (
