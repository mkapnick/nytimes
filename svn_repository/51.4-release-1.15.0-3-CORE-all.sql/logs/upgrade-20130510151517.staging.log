15:15:36 1929  PROCEDURE GET_PAYMENT_INFO_BY_INVOICE_ID (
15:15:36 1930  	 in_invoice_id		     IN NUMBER,
15:15:36 1931  	 out_order_id		     OUT VARCHAR2,
15:15:36 1932  	 out_external_transaction_id OUT VARCHAR2
15:15:36 1933  ) AS
15:15:36 1934  SPROC_NAME CONSTANT VARCHAR2(30) := 'GET_PEYMENT_INFO_BY_INVOICE_ID';
15:15:36 1935  -- VARIABLES
15:15:36 1936  temp_invoice_id number;
15:15:36 1937  cnt_matched_instr number := 0;
15:15:36 1938  -- EXCEPTIONS
15:15:36 1939  BAD_INVOICE_ID EXCEPTION;
15:15:36 1940  BEGIN
15:15:36 1941  
15:15:36 1942  	 BEGIN
15:15:36 1943  	   SELECT
15:15:36 1944  	     i.id into temp_invoice_id
15:15:36 1945  	   from
15:15:36 1946  	     invoice i
15:15:36 1947  	   where
15:15:36 1948  	     i.id = in_invoice_id;
15:15:36 1949  	   EXCEPTION
15:15:36 1950  	     WHEN NO_DATA_FOUND THEN
15:15:36 1951  	       RAISE BAD_INVOICE_ID;
15:15:36 1952  	 END;
15:15:36 1953  
15:15:36 1954  	 select
15:15:36 1955  	   count(1) into cnt_matched_instr
15:15:36 1956  	 from
15:15:36 1957  	   charge ch
15:15:36 1958  	 inner join
15:15:36 1959  	   subscription s
15:15:36 1960  	 on
15:15:36 1961  	   s.instrument_id = ch.instrument_id
15:15:36 1962  	 where
15:15:36 1963  	   ch.invoice_id = in_invoice_id;
15:15:36 1964  
15:15:36 1965  	 if cnt_matched_instr = 0 then
15:15:36 1966  	   out_external_transaction_id := null;
15:15:36 1967  	   out_order_id := null;
15:15:36 1968  	   return;
15:15:36 1969  	 end if;
15:15:36 1970  
15:15:36 1971  	 SELECT
15:15:36 1972  	   t.order_id,
15:15:36 1973  	   ta.external_transaction_id
15:15:36 1974  	   into
15:15:36 1975  	   out_order_id,
15:15:36 1976  	   out_external_transaction_id
15:15:36 1977  	 from
15:15:36 1978  	   charge ch
15:15:36 1979  	   inner join transaction t on ch.transaction_id = t.id
15:15:36 1980  	   inner join transaction_attempt ta on ta.transaction_id = t.id
15:15:36 1981  	 where
15:15:36 1982  	   ch.invoice_id = in_invoice_id
15:15:36 1983  	   and ta.transaction_attempt_status_id = GLOBAL_STATUSES_V21.TRANS_ATTEMPT_SUCCESS
15:15:36 1984  	   and ch.charge_amount > 0; -- We are not creating charges for the 0-amount invoices
15:15:36 1985  
15:15:36 1986  EXCEPTION
