15:15:36 1572  	       in_start_date		   => in_effective_end_date,
15:15:36 1573  	       in_end_date		   => var_current_license_end_date + (in_effective_end_date - in_effective_start_date),
15:15:36 1574  	       in_offer_id		   => var_current_offer_id,
15:15:36 1575  	       in_subscription_id	   => in_subscription_id,
15:15:36 1576  	       in_invoice_id		   => var_current_invoice_id,
15:15:36 1577  	       in_created_by		   => in_updated_by,
15:15:36 1578  	       in_is_extension		   => GLOBAL_CONSTANTS_V21.FALSE,
15:15:36 1579  	       in_current_offer_index	   => var_current_offer_index,
15:15:36 1580  	       in_current_offer_recurr_num => var_current_offer_recurr_num,
15:15:36 1581  	       out_license_id		   => var_ext_license_id
15:15:36 1582  	     );
15:15:36 1583  	     EXCEPTION
