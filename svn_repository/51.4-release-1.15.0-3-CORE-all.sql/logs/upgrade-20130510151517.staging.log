15:15:37 1431  var_gc_expiration_date  DATE;
15:15:37 1432  var_gc_redeemer_group_id NUMBER;
15:15:37 1433  var_gc_purchase_invoice_id NUMBER;
15:15:37 1434  var_gc_purchase_inv_status_id NUMBER;
