15:15:36 238  PROCEDURE GET_ACCOUNT_BY_INVOICE_ID (
15:15:36 239  /*
15:15:36 240  Throws exceptions:
15:15:36 241  APP_EXCEPTION_CODES_V21.NOT_FOUND
15:15:36 242  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
15:15:36 243  */
15:15:36 244  	in_invoice_id  IN  NUMBER,
15:15:36 245  	out_account_id OUT NUMBER
15:15:36 246  ) AS
15:15:36 247  -- VARIABLES
15:15:36 248  SPROC_NAME	 CONSTANT VARCHAR2(25) := 'GET_ACCOUNT_BY_INVOICE_ID';
15:15:36 249  temp_gc_account_id NUMBER;
15:15:36 250  temp_ss_account_id NUMBER;
15:15:36 251  temp_invoice_id	 NUMBER;
15:15:36 252  -- EXCEPTIONS
15:15:36 253  BAD_INVOICE_ID	   EXCEPTION;
15:15:36 254  CAN_NOT_FIND_ACCOUNT EXCEPTION;
15:15:36 255  BEGIN
15:15:36 256  
15:15:36 257  	-- Check that given invoice exists
15:15:36 258  	BEGIN
15:15:36 259  	  SELECT
15:15:36 260  	    INVOICE.ID into temp_invoice_id
15:15:36 261  	  FROM
15:15:36 262  	    INVOICE
15:15:36 263  	  WHERE
15:15:36 264  	    INVOICE.ID = in_invoice_id;
15:15:36 265  	  EXCEPTION
15:15:36 266  	    WHEN NO_DATA_FOUND THEN
15:15:36 267  	      RAISE BAD_INVOICE_ID;
15:15:36 268  	END;
15:15:36 269  
15:15:36 270  	-- Try to find gift certificate with given invoice
15:15:36 271  	BEGIN
15:15:36 272  	  SELECT
15:15:36 273  	    ACCOUNT.GROUP_ID into temp_gc_account_id
15:15:36 274  	  FROM
15:15:36 275  	    GIFT_CERTIFICATE
15:15:36 276  	    INNER JOIN ACCOUNT ON GIFT_CERTIFICATE.PURCHASER_GROUP_ID = ACCOUNT.GROUP_ID
15:15:36 277  	  WHERE
15:15:36 278  	    GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = in_invoice_id
15:15:36 279  	    AND ROWNUM <= 1;
15:15:36 280  	  EXCEPTION
15:15:36 281  	    WHEN NO_DATA_FOUND THEN
15:15:36 282  	      temp_gc_account_id := NULL;
15:15:36 283  	END;
15:15:36 284  
15:15:36 285  	-- check subscriptions for given invoice
15:15:36 286  	IF temp_gc_account_id IS NOT NULL THEN
15:15:36 287  	  out_account_id := temp_gc_account_id;
15:15:36 288  	ELSE
15:15:36 289  	  BEGIN
15:15:36 290  	    SELECT
15:15:36 291  	      ACCOUNT.GROUP_ID into temp_ss_account_id
15:15:36 292  	    FROM
15:15:36 293  	      LICENSE
15:15:36 294  	      INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:15:36 295  	      INNER JOIN ACCOUNT ON SUBSCRIPTION.ACCOUNT_ID = ACCOUNT.ID
15:15:36 296  	    WHERE
15:15:36 297  	      LICENSE.INVOICE_ID = in_invoice_id
15:15:36 298  	      AND ROWNUM <= 1;
15:15:36 299  	    EXCEPTION
15:15:36 300  	      WHEN NO_DATA_FOUND THEN
15:15:36 301  		temp_ss_account_id := NULL;
15:15:36 302  	  END;
15:15:36 303  	  IF temp_ss_account_id IS NULL THEN
15:15:36 304  	    RAISE CAN_NOT_FIND_ACCOUNT;
15:15:36 305  	  ELSE
15:15:36 306  	    out_account_id := temp_ss_account_id;
15:15:36 307  	  END IF;
15:15:36 308  	END IF;
15:15:36 309  
15:15:36 310  EXCEPTION
15:15:36 311  WHEN BAD_INVOICE_ID THEN
15:15:36 312  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 313  	  SPROC_NAME, 'No such invoice');
15:15:36 314  WHEN CAN_NOT_FIND_ACCOUNT THEN
15:15:36 315  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 316  	  SPROC_NAME, 'Could not find account by given invoice id');
15:15:36 317  WHEN OTHERS THEN
15:15:36 318  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 319  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:15:36 320  END GET_ACCOUNT_BY_INVOICE_ID;
