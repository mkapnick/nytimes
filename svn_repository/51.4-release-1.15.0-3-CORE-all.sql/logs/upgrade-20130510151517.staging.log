15:15:36 350  	    in_start_date		=> var_date,
15:15:36 351  	    in_end_date 		=> NULL, -- Will be calculated automatically
15:15:36 352  	    in_offer_id 		=> var_first_offer_id,
15:15:36 353  	    in_subscription_id		=> var_new_subscription_id,
15:15:36 354  	    in_invoice_id		=> var_new_invoice_id,
15:15:36 355  	    in_created_by		=> in_created_by,
15:15:36 356  	    in_is_extension		=> GLOBAL_CONSTANTS_V21.FALSE,
15:15:36 357  	    in_current_offer_index	=> PROCS_OFFER_CHAIN_V21.GET_FIRST_OFFER_INDEX(in_offer_chain_id),
15:15:36 358  	    in_current_offer_recurr_num => 1,
15:15:36 359  	    out_license_id		=> out_new_license_id
15:15:36 360  	  );
15:15:36 361  	  EXCEPTION
15:15:36 362  	    WHEN OTHERS THEN
15:15:36 363  	      EXCEPTION_MESSAGE := SQLERRM;
15:15:36 364  	      RAISE CAN_NOT_CREATE_LICENSE;
15:15:36 365  	END;
15:15:36 366  
15:15:36 367  	out_subscription_id := var_new_subscription_id;
15:15:36 368  	out_invoice_id := var_new_invoice_id;
15:15:36 369  
15:15:36 370  EXCEPTION
