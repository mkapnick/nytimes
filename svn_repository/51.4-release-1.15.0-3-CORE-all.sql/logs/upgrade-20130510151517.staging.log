15:15:36 208  
15:15:36 209  PROCEDURE GET_ENDING_LICENSES (
15:15:36 210  	in_hours_number IN NUMBER,
15:15:36 211  	out_result_set	OUT SYS_REFCURSOR
15:15:36 212  ) AS
15:15:36 213  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_ENDING_LICENSES';
15:15:36 214  -- VARIABLES
15:15:36 215  var_days		NUMBER;
15:15:36 216  var_hours 	NUMBER;
15:15:36 217  var_time_interval INTERVAL DAY (3) TO SECOND;
15:15:36 218  BEGIN
15:15:36 219  
15:15:36 220  	var_hours := mod(in_hours_number,24);
15:15:36 221  	var_days := (in_hours_number - var_hours) / 24;
15:15:36 222  	var_time_interval := var_days||' '||var_hours||':0:0';
15:15:36 223  
15:15:36 224  	OPEN out_result_set FOR
15:15:36 225  SELECT * FROM
15:15:36 226  (
15:15:36 227  	SELECT
15:15:36 228  	  LICENSE.ID,
15:15:36 229  	  LICENSE.CREATE_DATE,
15:15:36 230  	  LICENSE.CREATED_BY,
15:15:36 231  	  LICENSE.CURRENT_OFFER_INDEX,
15:15:36 232  	  LICENSE.CURRENT_OFFER_RECURR_NUM,
15:15:36 233  	  LICENSE.END_DATE,
15:15:36 234  	  LICENSE.ENTITLEMENT_END_DATE,
15:15:36 235  	  LICENSE.INVOICE_ID,
15:15:36 236  	  LICENSE.IS_EXTENSION,
15:15:36 237  	  LICENSE.LICENSE_STATUS_ID,
15:15:36 238  	  LICENSE.OFFER_ID,
15:15:36 239  	  LICENSE.START_DATE,
15:15:36 240  	  LICENSE.SUBSCRIPTION_ID,
15:15:36 241  	  LICENSE.UPDATE_DATE,
15:15:36 242  	  LICENSE.UPDATED_BY
15:15:36 243  	FROM
15:15:36 244  	  LICENSE
15:15:36 245  	WHERE
15:15:36 246  	  TO_DATE(LICENSE.END_DATE) <= (current_timestamp + var_time_interval)
15:15:36 247  	  AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V21.LICENSE_ACTIVE
15:15:36 248  	  AND NOT EXISTS
15:15:36 249  	  (
15:15:36 250  	    SELECT NULL
15:15:36 251  	    FROM PROCESS_RETRY_THROTTLE
15:15:36 252  	    WHERE PROCESS_NAME = SPROC_NAME
15:15:36 253  	      AND GENERIC_ID = LICENSE.ID
15:15:36 254  	  )
15:15:36 255  	  AND ROWNUM <= 10000
15:15:36 256  	  ORDER BY dbms_random.value
15:15:36 257  ) WHERE
15:15:36 258  	  ROWNUM <= 1000
15:15:36 259  	  ;
15:15:36 260  EXCEPTION
15:15:36 261  WHEN OTHERS THEN
15:15:36 262  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 263  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:15:36 264  END GET_ENDING_LICENSES;
