15:15:34  88  	PROCEDURE CHASE_PROFILE_BY_REQ_FILE_ID(
15:15:34  89  	  in_request_file_id IN CC_UPDATE.CC_REQUEST_FILE_ID%TYPE,
15:15:34  90  	  in_start	     IN NUMBER,
15:15:34  91  	  in_end	     IN NUMBER,
15:15:34  92  	  out_result_set     OUT SYS_REFCURSOR
15:15:34  93  	) AS
15:15:34  94  	SPROC_NAME CONSTANT VARCHAR2(32) := 'CHASE_PROFILE_BY_REQ_FILE_ID';
15:15:34  95  	var_range_diff	    NUMBER := 0;
15:15:34  96  	var_upper_bond_diff NUMBER := 0;
15:15:34  97  	var_l_start	    NUMBER := 0;
15:15:34  98  	var_l_end	    NUMBER := 0;
15:15:34  99  	BEGIN
15:15:34 100  	  --Normalize the end points [START]
15:15:34 101  	  IF (in_start IS NULL OR in_start < 0) Then
15:15:34 102  	    var_l_start := 0;
15:15:34 103  	  ELSE
15:15:34 104  	    var_l_start := in_start;
15:15:34 105  	  END IF;
15:15:34 106  
15:15:34 107  	  IF (in_end IS NULL) Then
15:15:34 108  	    var_l_end := 500;
15:15:34 109  	  ELSE
15:15:34 110  	    var_l_end := in_end;
15:15:34 111  	  END IF;
15:15:34 112  
15:15:34 113  	  var_l_start := var_l_start + 1;
15:15:34 114  	  var_l_end   := var_l_end   + 1;
15:15:34 115  
15:15:34 116  	  var_range_diff  := var_l_end - var_l_start;
15:15:34 117  	  var_upper_bond_diff :=  var_range_diff - 1000;
15:15:34 118  
15:15:34 119  	  IF (var_upper_bond_diff > 0) Then
15:15:34 120  	    var_l_end := var_l_end - var_upper_bond_diff;
15:15:34 121  	  END IF;
15:15:34 122  	  --Normalize the end points [END]
15:15:34 123  
15:15:34 124  	  OPEN out_result_set FOR
15:15:34 125  	    SELECT CHASE_PROFILE_ID FROM
15:15:34 126  	      (SELECT rownum rnum, q.* FROM
15:15:34 127  		 (SELECT
15:15:34 128  		    cc.CHASE_PROFILE_ID
15:15:34 129  		  FROM
15:15:34 130  		    CREDIT_CARD cc,
15:15:34 131  		    CC_UPDATE ccu
15:15:34 132  		  WHERE
15:15:34 133  		    ccu.CC_REQUEST_FILE_ID = in_request_file_id
15:15:34 134  		    AND ccu.CREDIT_CARD_ID = cc.id
15:15:34 135  		) Q
15:15:34 136  	      WHERE rownum <= var_l_end)
15:15:34 137  	    WHERE rnum >= var_l_Start;
15:15:34 138  	END CHASE_PROFILE_BY_REQ_FILE_ID;
