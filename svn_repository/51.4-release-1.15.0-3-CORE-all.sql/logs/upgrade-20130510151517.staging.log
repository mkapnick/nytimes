15:15:36 401  	  CHARGE.ID as "CHARGE_ID",
15:15:36 402  	  CHARGE.CHARGE_AMOUNT,
15:15:36 403  	  CHARGE.INSTRUMENT_ID,
15:15:36 404  	  CHARGE.CHARGE_STATUS_ID
15:15:36 405  	FROM
15:15:36 406  	  CHARGE
15:15:36 407  	WHERE
15:15:36 408  	  CHARGE.INVOICE_ID = in_invoice_id
15:15:36 409  	  AND CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V21.INSTRUMENT_CREDIT_CARD;
15:15:36 410  
15:15:36 411  	OPEN out_gc_charges_set FOR
