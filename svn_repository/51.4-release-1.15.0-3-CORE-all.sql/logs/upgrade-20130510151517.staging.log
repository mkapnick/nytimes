15:15:37 275  PROCEDURE REACTIVATE_ACCOUNT (
15:15:37 276  	in_group_id   IN NUMBER,
15:15:37 277  	in_updated_by IN VARCHAR2,
15:15:37 278  	in_note       IN VARCHAR2,
15:15:37 279  	in_agent_id   IN NUMBER
15:15:37 280  ) AS
15:15:37 281  -- VARIABLES
15:15:37 282  SPROC_NAME	      CONSTANT VARCHAR2(18) := 'REACTIVATE_ACCOUNT';
15:15:37 283  var_account_id	      NUMBER;
15:15:37 284  current_account_status  NUMBER;
15:15:37 285  
15:15:37 286  -- EXCEPTIONS
15:15:37 287  BAD_CURRENT_ACC_STATUS EXCEPTION;
15:15:37 288  CAN_NOT_CREATE_NOTE    EXCEPTION;
15:15:37 289  EXCEPTION_MESSAGE      VARCHAR2(1024);
15:15:37 290  BEGIN
15:15:37 291  	-- Get account id, status
15:15:37 292  	SELECT
15:15:37 293  	  ACCOUNT.ACCOUNT_STATUS_ID,
15:15:37 294  	  ACCOUNT.ID
15:15:37 295  	INTO
15:15:37 296  	  current_account_status,
15:15:37 297  	  var_account_id
15:15:37 298  	FROM ACCOUNT
15:15:37 299  	WHERE
15:15:37 300  	  ACCOUNT.GROUP_ID = in_group_id;
15:15:37 301  
15:15:37 302  	IF current_account_status != GLOBAL_STATUSES_V21.ACCOUNT_FROZEN THEN
15:15:37 303  	  RAISE BAD_CURRENT_ACC_STATUS;
15:15:37 304  	END IF;
15:15:37 305  
15:15:37 306  	-- Change account status
15:15:37 307  	PROCS_ACCOUNT_V21.UPDATE_ACCOUNT_STATUS(
15:15:37 308  	  in_account_id        => var_account_id,
15:15:37 309  	  in_updated_by        => in_updated_by,
15:15:37 310  	  in_account_status_id => GLOBAL_STATUSES_V21.ACCOUNT_ACTIVE
15:15:37 311  	);
15:15:37 312  
15:15:37 313  	-- Add note
15:15:37 314  	BEGIN
15:15:37 315  	  PROCS_ACCOUNT_V21.ANNOTATE_ACCOUNT(
15:15:37 316  	    in_group_id   => in_group_id,
15:15:37 317  	    in_agent_id   => in_agent_id,
15:15:37 318  	    in_note	  => in_note,
15:15:37 319  	    in_created_by => in_updated_by
15:15:37 320  	  );
15:15:37 321  	  EXCEPTION
15:15:37 322  	    WHEN OTHERS THEN
15:15:37 323  	      EXCEPTION_MESSAGE := SQLERRM;
15:15:37 324  	      RAISE CAN_NOT_CREATE_NOTE;
15:15:37 325  	END;
15:15:37 326  
15:15:37 327  EXCEPTION
15:15:37 328  WHEN BAD_CURRENT_ACC_STATUS THEN
15:15:37 329  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
15:15:37 330  	  SPROC_NAME, 'Current account status is not "frozen"');
15:15:37 331  WHEN NO_DATA_FOUND THEN
15:15:37 332  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:37 333  	  SPROC_NAME, 'Could not find account with given group ID');
15:15:37 334  WHEN CAN_NOT_CREATE_NOTE THEN
15:15:37 335  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:37 336  	  SPROC_NAME, 'Could not annotate account');
15:15:37 337  WHEN OTHERS THEN
15:15:37 338  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:37 339  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:15:37 340  END REACTIVATE_ACCOUNT;
