15:15:35 680  FUNCTION GET_NEXT_OFFER_INDEX (
15:15:35 681  	in_offer_chain_id	     IN NUMBER,
15:15:35 682  	in_offer_chain_current_index IN NUMBER
15:15:35 683  ) RETURN NUMBER AS
15:15:35 684  -- VARIABLES
15:15:35 685  SPROC_NAME		     CONSTANT VARCHAR2(20) := 'GET_NEXT_OFFER_INDEX';
15:15:35 686  temp_offer_chain_id	     NUMBER;
15:15:35 687  temp_offer_chain_current_index NUMBER;
15:15:35 688  var_result		     NUMBER;
15:15:35 689  -- EXCEPTIONS
15:15:35 690  BAD_OFFER_CHAIN_ID      EXCEPTION;
15:15:35 691  BAD_CURRENT_INDEX_VALUE EXCEPTION;
15:15:35 692  BEGIN
15:15:35 693  
15:15:35 694  	-- Check that offer chain exists
15:15:35 695  	BEGIN
15:15:35 696  	  SELECT
15:15:35 697  	    OFFER_CHAIN.ID into temp_offer_chain_id
15:15:35 698  	  FROM
15:15:35 699  	    OFFER_CHAIN
15:15:35 700  	  WHERE
15:15:35 701  	    OFFER_CHAIN.ID = in_offer_chain_id;
15:15:35 702  	  EXCEPTION
15:15:35 703  	    WHEN NO_DATA_FOUND THEN
15:15:35 704  	      RAISE BAD_OFFER_CHAIN_ID;
15:15:35 705  	END;
15:15:35 706  
15:15:35 707  	-- Check that current offer index exists
15:15:35 708  	BEGIN
15:15:35 709  	  SELECT
15:15:35 710  	    OFFER_OFFER_CHAIN.INDEX_VALUE into temp_offer_chain_current_index
15:15:35 711  	  FROM
15:15:35 712  	    OFFER_OFFER_CHAIN
15:15:35 713  	  WHERE
15:15:35 714  	    OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
15:15:35 715  	    AND OFFER_OFFER_CHAIN.INDEX_VALUE = in_offer_chain_current_index
15:15:35 716  	    -- TODO: delete next line
15:15:35 717  	    AND ROWNUM <= 1;
15:15:35 718  	  EXCEPTION
15:15:35 719  	    WHEN NO_DATA_FOUND THEN
15:15:35 720  	      RAISE BAD_CURRENT_INDEX_VALUE;
15:15:35 721  	END;
15:15:35 722  
15:15:35 723  	-- Get next offer index
15:15:35 724  	BEGIN
15:15:35 725  	  SELECT
15:15:35 726  	    INDEX_VALUE into var_result
15:15:35 727  	  FROM (
15:15:35 728  	    SELECT
15:15:35 729  	      OFFER_OFFER_CHAIN.INDEX_VALUE
15:15:35 730  	    FROM
15:15:35 731  	      OFFER_OFFER_CHAIN
15:15:35 732  	    WHERE
15:15:35 733  	      OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
15:15:35 734  	      AND OFFER_OFFER_CHAIN.INDEX_VALUE > in_offer_chain_current_index
15:15:35 735  	    ORDER BY
15:15:35 736  	      OFFER_OFFER_CHAIN.INDEX_VALUE ASC
15:15:35 737  	  )
15:15:35 738  	  WHERE
15:15:35 739  	    ROWNUM <= 1;
15:15:35 740  	  EXCEPTION
15:15:35 741  	    WHEN NO_DATA_FOUND THEN
15:15:35 742  	      var_result := NULL;
15:15:35 743  	END;
15:15:35 744  
15:15:35 745  	RETURN var_result;
15:15:35 746  
15:15:35 747  EXCEPTION
15:15:35 748  WHEN BAD_OFFER_CHAIN_ID THEN
15:15:35 749  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:35 750  	  SPROC_NAME, 'No such offer chain');
15:15:35 751  WHEN BAD_CURRENT_INDEX_VALUE THEN
15:15:35 752  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:35 753  	  SPROC_NAME, 'Bad current index value');
