15:15:37 682  PROCEDURE FREEZE_ACCOUNT (
15:15:37 683  	in_group_id   IN NUMBER,
15:15:37 684  	in_updated_by IN VARCHAR2,
15:15:37 685  	in_note       IN VARCHAR2,
15:15:37 686  	in_agent_id   IN NUMBER
15:15:37 687  ) AS
15:15:37 688  SPROC_NAME	    CONSTANT VARCHAR2(14) := 'FREEZE_ACCOUNT';
15:15:37 689  -- VARIABLES
15:15:37 690  var_account_id	    NUMBER;
15:15:37 691  var_account_status_id NUMBER;
15:15:37 692  -- EXCEPTIONS
15:15:37 693  BAD_GROUP_ID	    EXCEPTION;
15:15:37 694  BAD_ACCOUNT_STATUS_ID EXCEPTION;
15:15:37 695  CAN_NOT_CREATE_NOTE   EXCEPTION;
15:15:37 696  EXCEPTION_MESSAGE     VARCHAR2(1024);
15:15:37 697  BEGIN
15:15:37 698  
15:15:37 699  	-- Get account status, account id
15:15:37 700  	BEGIN
15:15:37 701  	  SELECT
15:15:37 702  	    ACCOUNT.ID,
15:15:37 703  	    ACCOUNT.ACCOUNT_STATUS_ID
15:15:37 704  	    into
15:15:37 705  	    var_account_id,
15:15:37 706  	    var_account_status_id
15:15:37 707  	  FROM
15:15:37 708  	    ACCOUNT
15:15:37 709  	  WHERE
15:15:37 710  	    ACCOUNT.GROUP_ID = in_group_id;
15:15:37 711  	  EXCEPTION
15:15:37 712  	    WHEN NO_DATA_FOUND THEN
15:15:37 713  	      RAISE BAD_GROUP_ID;
15:15:37 714  	END;
15:15:37 715  
15:15:37 716  	-- We can freeze only ACTIVE accounts
15:15:37 717  	IF var_account_status_id != GLOBAL_STATUSES_V21.ACCOUNT_ACTIVE
15:15:37 718  	  AND var_account_status_id != GLOBAL_STATUSES_V21.ACCOUNT_FROZEN THEN
15:15:37 719  	  RAISE BAD_ACCOUNT_STATUS_ID;
15:15:37 720  	END IF;
15:15:37 721  
15:15:37 722  	-- Set account status
15:15:37 723  	PROCS_ACCOUNT_V21.UPDATE_ACCOUNT_STATUS(
15:15:37 724  	  in_account_id        => var_account_id,
15:15:37 725  	  in_updated_by        => in_updated_by,
15:15:37 726  	  in_account_status_id => GLOBAL_STATUSES_V21.ACCOUNT_FROZEN
15:15:37 727  	);
15:15:37 728  
15:15:37 729  	-- Annotate account
15:15:37 730  	BEGIN
15:15:37 731  	  PROCS_ACCOUNT_V21.ANNOTATE_ACCOUNT(
15:15:37 732  	    in_group_id   => in_group_id,
15:15:37 733  	    in_agent_id   => in_agent_id,
15:15:37 734  	    in_note	  => in_note,
15:15:37 735  	    in_created_by => in_updated_by
15:15:37 736  	  );
15:15:37 737  	  EXCEPTION
15:15:37 738  	    WHEN OTHERS THEN
15:15:37 739  	      EXCEPTION_MESSAGE := SQLERRM;
15:15:37 740  	      RAISE CAN_NOT_CREATE_NOTE;
15:15:37 741  	END;
15:15:37 742  
15:15:37 743  EXCEPTION
15:15:37 744  WHEN BAD_GROUP_ID THEN
15:15:37 745  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:37 746  	  SPROC_NAME, 'No such group id');
15:15:37 747  WHEN BAD_ACCOUNT_STATUS_ID THEN
15:15:37 748  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
15:15:37 749  	  SPROC_NAME, 'Could not update this account. Status should to be active or frozen');
15:15:37 750  WHEN CAN_NOT_CREATE_NOTE THEN
15:15:37 751  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:37 752  	  SPROC_NAME, 'Could not annotate account', EXCEPTION_MESSAGE);
15:15:37 753  WHEN OTHERS THEN
15:15:37 754  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:37 755  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:15:37 756  END FREEZE_ACCOUNT;
