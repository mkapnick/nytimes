15:15:35 126  	in_reason      IN VARCHAR2
15:15:35 127  ) AS
15:15:35 128  SPROC_NAME CONSTANT VARCHAR2(12) := 'LOCK_ACCOUNT';
15:15:35 129  -- CONSTANTS
15:15:35 130  one_second_interval CONSTANT INTERVAL DAY TO SECOND := INTERVAL '0 00:00:01' DAY TO SECOND;
15:15:35 131  -- VARIABLES
15:15:35 132  var_account_id NUMBER;
15:15:35 133  -- arosolovskiy: using data variable instead of direct get data calls (#38860)
15:15:35 134  var_date		DATE := SYSDATE;
15:15:35 135  var_lock_end_date DATE;
15:15:35 136  -- EXCEPTIONS
15:15:35 137  BAD_GROUP_ID   EXCEPTION;
15:15:35 138  ALREADY_LOCKED EXCEPTION;
15:15:35 139  BEGIN
15:15:35 140  
15:15:35 141  	BEGIN
15:15:35 142  	  SELECT
15:15:35 143  	    ACCOUNT.ID into var_account_id
15:15:35 144  	  FROM
15:15:35 145  	    ACCOUNT
