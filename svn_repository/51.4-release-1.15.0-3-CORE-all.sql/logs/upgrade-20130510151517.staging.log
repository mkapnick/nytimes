15:15:36  24  BAD_INVOICE_ID	     EXCEPTION;
15:15:36  25  BAD_TRANSACTION_ID     EXCEPTION;
15:15:36  26  BAD_PAYPAL_ID	     EXCEPTION;
15:15:36  27  BAD_CREDIT_CARD_ID     EXCEPTION;
15:15:36  28  BAD_INSTRUMENT_TYPE    EXCEPTION;
15:15:36  29  BAD_CHARGE_STATUS_ID   EXCEPTION;
15:15:36  30  EXCEPTION_MESSAGE      VARCHAR2(1024);
15:15:36  31  BEGIN
15:15:36  32  	-- Check that incoming data is correct
15:15:36  33  	IF in_instrument_type_id != GLOBAL_ENUMS_V21.INSTRUMENT_CREDIT_CARD
15:15:36  34  	  AND in_instrument_type_id != GLOBAL_ENUMS_V21.INSTRUMENT_PAYPAL THEN
15:15:36  35  	  RAISE BAD_INSTRUMENT_TYPE;
15:15:36  36  	END IF;
15:15:36  37  
15:15:36  38  	-- Check that status is correct
15:15:36  39  	IF in_charge_status_id != GLOBAL_STATUSES_V21.CHARGE_OPENED
15:15:36  40  	  AND in_charge_status_id != GLOBAL_STATUSES_V21.CHARGE_PROCESSED
15:15:36  41  	  AND in_charge_status_id != GLOBAL_STATUSES_V21.CHARGE_CANCELED THEN
15:15:36  42  	  RAISE BAD_CHARGE_STATUS_ID;
15:15:36  43  	END IF;
15:15:36  44  
15:15:36  45  	-- Check that invoice exists
15:15:36  46  	BEGIN
15:15:36  47  	  SELECT
15:15:36  48  	    INVOICE.ID into temp_invoice_id
15:15:36  49  	  FROM
15:15:36  50  	    INVOICE
15:15:36  51  	  WHERE
15:15:36  52  	    INVOICE.ID = in_invoice_id;
15:15:36  53  	  EXCEPTION
15:15:36  54  	    WHEN NO_DATA_FOUND THEN
15:15:36  55  	      RAISE BAD_INVOICE_ID;
15:15:36  56  	END;
15:15:36  57  
15:15:36  58  	-- Check that transaction exists
15:15:36  59  	BEGIN
15:15:36  60  	  SELECT
15:15:36  61  	    TRANSACTION.ID into temp_transaction_id
15:15:36  62  	  FROM
15:15:36  63  	    TRANSACTION
15:15:36  64  	  WHERE
15:15:36  65  	    TRANSACTION.ID = in_transaction_id;
15:15:36  66  	  EXCEPTION
15:15:36  67  	    WHEN NO_DATA_FOUND THEN
15:15:36  68  	      RAISE BAD_TRANSACTION_ID;
15:15:36  69  	END;
15:15:36  70  
15:15:36  71  	-- Check that instrument exists
15:15:36  72  	IF in_instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_CREDIT_CARD THEN
15:15:36  73  	  IF PROCS_FIN_INSTRUMENTS_V21.IS_CREDIT_CARD_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V21.FALSE THEN
15:15:36  74  	    RAISE BAD_CREDIT_CARD_ID;
15:15:36  75  	  END IF;
15:15:36  76  	ELSIF in_instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_PAYPAL THEN
15:15:36  77  	  IF PROCS_FIN_INSTRUMENTS_V21.IS_PAYPAL_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V21.FALSE THEN
15:15:36  78  	    RAISE BAD_PAYPAL_ID;
15:15:36  79  	  END IF;
15:15:36  80  	END IF;
15:15:36  81  
15:15:36  82  	-- Create new charge
15:15:36  83  	PROCS_CHARGE_CRU_V21.CREATE_CHARGE(
15:15:36  84  	  out_charge_id 	=> var_new_charge_id,
15:15:36  85  	  in_invoice_id 	=> in_invoice_id,
15:15:36  86  	  in_transaction_id	=> in_transaction_id,
15:15:36  87  	  in_instrument_type_id => in_instrument_type_id,
15:15:36  88  	  in_instrument_id	=> in_instrument_id,
15:15:36  89  	  in_charge_amount	=> in_charge_amount,
15:15:36  90  	  in_charge_status_id	=> in_charge_status_id,
15:15:36  91  	  in_created_by 	=> in_created_by
15:15:36  92  	);
15:15:36  93  
15:15:36  94  	out_charge_id := var_new_charge_id;
15:15:36  95  
15:15:36  96  EXCEPTION
15:15:36  97  WHEN BAD_CHARGE_STATUS_ID THEN
15:15:36  98  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INVALID_PARAMETER,
15:15:36  99  	  PROCS_NAME, 'Bad charge status: '||in_charge_status_id);
15:15:36 100  WHEN BAD_INSTRUMENT_TYPE THEN
15:15:36 101  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INVALID_PARAMETER,
15:15:36 102  	  PROCS_NAME, 'Bad instrument type id');
15:15:36 103  WHEN BAD_INVOICE_ID THEN
15:15:36 104  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 105  	  PROCS_NAME, 'No such invoice');
15:15:36 106  WHEN BAD_TRANSACTION_ID THEN
15:15:36 107  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 108  	  PROCS_NAME, 'No such transaction');
15:15:36 109  WHEN BAD_PAYPAL_ID THEN
15:15:36 110  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 111  	  PROCS_NAME, 'No such paypal');
15:15:36 112  WHEN BAD_CREDIT_CARD_ID THEN
15:15:36 113  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 114  	  PROCS_NAME, 'No such credit card');
15:15:36 115  WHEN OTHERS THEN
15:15:36 116  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 117  	  PROCS_NAME, 'Unknown error', SQLERRM);
15:15:36 118  END CREATE_CHARGE;
