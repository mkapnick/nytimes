15:15:37 2930  /*
15:15:37 2931  	   Note, this returns paypal records which are in the given window of time
15:15:37 2932  	   based on their creation date, not their expiration date, because we've seen
15:15:37 2933  	   suspicious values in the data for expiration dates. The java layer should
15:15:37 2934  	   inspect the expiration dates and decide whether or not to act on the data.
15:15:37 2935  */
15:15:37 2936  
15:15:37 2937  PROCEDURE GET_EXPIRING_PAYPAL (
15:15:37 2938  	 in_expire_window_days	 IN NUMBER,
15:15:37 2939  	 in_creation_limit_days  IN NUMBER,
15:15:37 2940  	 in_retry_throttle_name  IN PROCESS_RETRY_THROTTLE.PROCESS_NAME%TYPE,
15:15:37 2941  	 out_result_set 	 OUT SYS_REFCURSOR
15:15:37 2942  ) AS
15:15:37 2943  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_EXPIRING_PAYPAL';
15:15:37 2944  -- If creation date is later than this, return the record
15:15:37 2945  var_end_creation DATE := SYSDATE - in_creation_limit_days + in_expire_window_days;
15:15:37 2946  var_rows_to_return CONSTANT NUMBER := 300;
15:15:37 2947  BEGIN
15:15:37 2948  	 OPEN out_result_set FOR
15:15:37 2949  	 SELECT * FROM (
15:15:37 2950  	   SELECT
15:15:37 2951  	       ac.group_id	   AS group_id,
15:15:37 2952  	       pp.id		   AS paypal_id,
15:15:37 2953  	       sb.id		   AS subscription_id,
15:15:37 2954  	       pp.create_date	   AS creation_date,
15:15:37 2955  	       pp.expiration_date  AS expiration_date
15:15:37 2956  	   FROM paypal pp
15:15:37 2957  	   INNER JOIN subscription sb
15:15:37 2958  	   ON pp.account_id = sb.account_id
15:15:37 2959  	   INNER JOIN account ac
15:15:37 2960  	   ON pp.account_id = ac.id
15:15:37 2961  	   WHERE pp.paypal_status_id = GLOBAL_STATUSES_V21.PAYPAL_ACTIVE
15:15:37 2962  	     AND sb.instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_PAYPAL
15:15:37 2963  	     AND sb.subscription_status_id IN (GLOBAL_STATUSES_V21.SUBSCRIPTION_ACTIVE,
15:15:37 2964  					       GLOBAL_STATUSES_V21.SUBSCRIPTION_IN_GRACE_PERIOD)
15:15:37 2965  	     AND sb.instrument_id = pp.id
15:15:37 2966  	     AND pp.create_date <= var_end_creation
15:15:37 2967  	     AND NOT EXISTS(
15:15:37 2968  	       SELECT NULL
15:15:37 2969  	       FROM process_retry_throttle rt
15:15:37 2970  	       WHERE rt.process_name = in_retry_throttle_name
15:15:37 2971  		 AND rt.generic_id = pp.id
15:15:37 2972  	     )
15:15:37 2973  	     AND ROWNUM <= 10 * var_rows_to_return
15:15:37 2974  	   ORDER BY dbms_random.value
15:15:37 2975  	 )
15:15:37 2976  	 WHERE ROWNUM <= var_rows_to_return;
15:15:37 2977  END GET_EXPIRING_PAYPAL;
