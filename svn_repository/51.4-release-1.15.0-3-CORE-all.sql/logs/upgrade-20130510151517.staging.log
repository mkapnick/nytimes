15:15:34  72  	     END IF;
15:15:34  73  	   END LOOP;
15:15:34  74  
15:15:34  75  	   SELECT COUNT(1) INTO var_cc_update_count
15:15:34  76  	   FROM CC_UPDATE
15:15:34  77  	   WHERE CC_REQUEST_FILE_ID = var_request_file_id;
15:15:34  78  	   IF var_cc_update_count <= 0 THEN
