15:15:37 634  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_GRP_ACCNT_EMAIL_DOMAINS';
15:15:37 635  range_diff NUMBER := 0;
15:15:37 636  upper_bond_diff NUMBER := 0;
15:15:37 637  l_start NUMBER := 0;
15:15:37 638  l_end   NUMBER := 0;
15:15:37 639  BEGIN
15:15:37 640  	--Normalize the end points [START]
15:15:37 641  	IF (in_start IS NULL OR in_start < 0) Then
15:15:37 642  	  l_start := 0;
15:15:37 643  	ELSE
15:15:37 644  	  l_start := in_start;
15:15:37 645  	END IF;
15:15:37 646  
15:15:37 647  	IF (in_end IS NULL) Then
15:15:37 648  	  l_end := 11;
15:15:37 649  	ELSE
15:15:37 650  	  l_end := in_end;
15:15:37 651  	END IF;
15:15:37 652  
15:15:37 653  	l_start := l_start + 1;
15:15:37 654  	l_end	:= l_end   + 1;
15:15:37 655  
15:15:37 656  	range_diff := l_end - l_start;
15:15:37 657  	upper_bond_diff :=  range_diff - GLOBAL_CONSTANTS_V21.MAX_RETURN_COUNT;
15:15:37 658  
15:15:37 659  	IF (upper_bond_diff > 0) Then
15:15:37 660  	  l_end := l_end - upper_bond_diff;
15:15:37 661  	END IF;
15:15:37 662  	--Normalize the end points [END]
15:15:37 663  
15:15:37 664  	--Total count of records [START]
15:15:37 665  	SELECT
15:15:37 666  	  COUNT(1) INTO out_record_count
15:15:37 667  	FROM
15:15:37 668  	  GROUP_ACCOUNT_EMAIL_DOMAIN
15:15:37 669  	WHERE
15:15:37 670  	  GROUP_ACCOUNT_ID = in_group_account_id AND
15:15:37 671  	  (IS_ACTIVE IS NULL OR IS_ACTIVE = in_status);
15:15:37 672  	--Total count of records [END]
15:15:37 673  
15:15:37 674  	OPEN out_result_set FOR
15:15:37 675  	SELECT
15:15:37 676  	  *
15:15:37 677  	FROM
15:15:37 678  	  (SELECT rownum rnum, q.*
15:15:37 679  	   FROM
15:15:37 680  	    (SELECT
15:15:37 681  	      ID,
15:15:37 682  	      GROUP_ACCOUNT_ID,
15:15:37 683  	      EMAIL_DOMAIN,
15:15:37 684  	      IS_ACTIVE
15:15:37 685  	     FROM
15:15:37 686  	       GROUP_ACCOUNT_EMAIL_DOMAIN
15:15:37 687  	     WHERE
15:15:37 688  	       GROUP_ACCOUNT_ID = in_group_account_id AND
15:15:37 689  	       (in_status IS NULL OR
15:15:37 690  		IS_ACTIVE = in_status)
15:15:37 691  	    ) Q
15:15:37 692  	  WHERE rownum <= l_end)
15:15:37 693  	WHERE rnum >= l_Start;
15:15:37 694  
15:15:37 695  EXCEPTION
15:15:37 696  WHEN OTHERS THEN
15:15:37 697  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:37 698  	  SPROC_NAME, 'Unknown error while retrieving Email Domains ', SQLERRM);
