15:15:36 449  
15:15:36 450  PROCEDURE GET_NEXT_OFFER (
15:15:36 451  /*
15:15:36 452  Throws exceptions:
15:15:36 453  APP_EXCEPTION_CODES_V21.NOT_FOUND
15:15:36 454  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
15:15:36 455  */
15:15:36 456  	in_license_id  IN NUMBER,
15:15:36 457  	out_result_set OUT SYS_REFCURSOR
15:15:36 458  ) AS
15:15:36 459  -- VARIABLES
15:15:36 460  SPROC_NAME		CONSTANT VARCHAR2(14) := 'GET_NEXT_OFFER';
15:15:36 461  var_offer_chain_id	NUMBER;
15:15:36 462  var_offer_id		NUMBER;
15:15:36 463  var_license_current_index NUMBER;
15:15:36 464  var_next_offer_index	NUMBER;
15:15:36 465  -- EXCEPTIONS
15:15:36 466  BAD_LICENSE_ID	      EXCEPTION;
15:15:36 467  CAN_NOT_FIND_NEXT_OFFER EXCEPTION;
15:15:36 468  EXCEPTION_MESSAGE       VARCHAR2(1024);
15:15:36 469  BEGIN
15:15:36 470  
15:15:36 471  	BEGIN
15:15:36 472  	  SELECT
15:15:36 473  	    SUBSCRIPTION.OFFER_CHAIN_ID,
15:15:36 474  	    LICENSE.OFFER_ID,
15:15:36 475  	    LICENSE.CURRENT_OFFER_INDEX
15:15:36 476  	    into
15:15:36 477  	    var_offer_chain_id,
15:15:36 478  	    var_offer_id,
15:15:36 479  	    var_license_current_index
15:15:36 480  	  FROM
15:15:36 481  	    LICENSE
15:15:36 482  	    INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:15:36 483  	  WHERE
15:15:36 484  	    LICENSE.ID = in_license_id;
15:15:36 485  	  EXCEPTION
15:15:36 486  	    WHEN NO_DATA_FOUND THEN
15:15:36 487  	      RAISE BAD_LICENSE_ID;
15:15:36 488  	END;
15:15:36 489  
15:15:36 490  	BEGIN
