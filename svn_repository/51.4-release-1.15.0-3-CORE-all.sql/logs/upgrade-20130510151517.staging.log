15:15:36 400  FUNCTION IS_CHARGE_COLLECTED (
15:15:36 401  /*
15:15:36 402  Throws:
15:15:36 403  APP_EXCEPTION_CODES_V21.NOT_FOUND
15:15:36 404  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
15:15:36 405  Returns:
15:15:36 406  GLOBAL_CONST.TRUE if transaction collected,
15:15:36 407  GLOBAL_CONST.FALSE else
15:15:36 408  */
15:15:36 409  	in_charge_id IN NUMBER
15:15:36 410  ) RETURN NUMBER AS
15:15:36 411  SPROC_NAME CONSTANT VARCHAR2(19) := 'IS_CHARGE_COLLECTED';
15:15:36 412  -- VARIABLES
15:15:36 413  var_transaction_id NUMBER;
15:15:36 414  is_transaction_collected NUMBER;
15:15:36 415  -- EXCEPTIONS
15:15:36 416  BAD_CHARGE_ID	       EXCEPTION;
15:15:36 417  CAN_NOT_CHECK_TRANSACTION EXCEPTION;
15:15:36 418  EXCEPTION_MESSAGE        VARCHAR2(1024);
15:15:36 419  BEGIN
15:15:36 420  
15:15:36 421  	BEGIN
15:15:36 422  	  SELECT
15:15:36 423  	    CHARGE.TRANSACTION_ID into var_transaction_id
15:15:36 424  	  FROM
15:15:36 425  	    CHARGE
15:15:36 426  	  WHERE
15:15:36 427  	    CHARGE.ID = in_charge_id;
15:15:36 428  	  EXCEPTION
15:15:36 429  	    WHEN NO_DATA_FOUND THEN
15:15:36 430  	      RAISE BAD_CHARGE_ID;
15:15:36 431  	END;
15:15:36 432  
15:15:36 433  	BEGIN
15:15:36 434  	  is_transaction_collected := PROCS_TRANSACTION_V21.IS_TRANSACTION_COLLECTED(var_transaction_id);
15:15:36 435  	  EXCEPTION
15:15:36 436  	    WHEN OTHERS THEN
15:15:36 437  	      EXCEPTION_MESSAGE := SQLERRM;
15:15:36 438  	      RAISE CAN_NOT_CHECK_TRANSACTION;
15:15:36 439  	END;
15:15:36 440  
15:15:36 441  	RETURN is_transaction_collected;
15:15:36 442  
15:15:36 443  EXCEPTION
15:15:36 444  WHEN BAD_CHARGE_ID THEN
