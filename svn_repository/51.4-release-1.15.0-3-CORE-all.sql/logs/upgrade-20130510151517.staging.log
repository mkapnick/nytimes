15:15:37 824  PROCEDURE SHOULD_MOVE_TO_GRACE(
15:15:37 825  	in_invoice_id  IN INVOICE.ID%TYPE,
15:15:37 826  	out_result     OUT NUMBER
15:15:37 827  ) AS
15:15:37 828  BEGIN
15:15:37 829  	-- if the invoice preceding the given invoice has no transaction attempts, then
15:15:37 830  	-- it is not billed out of Sartre. if so, then the associated subscription
15:15:37 831  	-- should be canceled after a final failed billing attempt--not moved to grace.
15:15:37 832  	SELECT DECODE(COUNT(1), 0, 0, 1) INTO out_result
15:15:37 833  	FROM CHARGE c
15:15:37 834  	INNER JOIN TRANSACTION t ON c.TRANSACTION_ID = t.ID
15:15:37 835  	INNER JOIN TRANSACTION_ATTEMPT ta ON ta.TRANSACTION_ID = t.ID
15:15:37 836  	WHERE c.INVOICE_ID = (
15:15:37 837  	  -- select previous invoice_id, or -1 if there is none
15:15:37 838  	  SELECT PREV_INVOICE_ID FROM (
15:15:37 839  	    SELECT i.ID, LAG(i.ID, 1, -1) OVER (ORDER BY i.CREATE_DATE) AS PREV_INVOICE_ID
15:15:37 840  	    FROM INVOICE i
15:15:37 841  	    INNER JOIN LICENSE l ON i.ID = l.INVOICE_ID
15:15:37 842  	    WHERE l.SUBSCRIPTION_ID = (
15:15:37 843  	      SELECT SUBSCRIPTION_ID FROM LICENSE WHERE INVOICE_ID = in_invoice_id
15:15:37 844  	    )
15:15:37 845  	  ) WHERE ID = in_invoice_id
15:15:37 846  	);
15:15:37 847  END SHOULD_MOVE_TO_GRACE;
15:15:37 848  
