15:15:36 1097  
15:15:36 1098  PROCEDURE GET_INVOICE_DAYS_USED_NUMBER (
15:15:36 1099  	 in_invoice_id	     IN NUMBER,
15:15:36 1100  	 in_chargeback_date  IN DATE DEFAULT SYSDATE,
15:15:36 1101  	 out_days_num	     OUT NUMBER
15:15:36 1102  ) AS
15:15:36 1103  SPROC_NAME CONSTANT VARCHAR2(28) := 'GET_INVOICE_DAYS_USED_NUMBER';
15:15:36 1104  -- VARIABLES
15:15:36 1105  temp_invoice_id	      NUMBER;
15:15:36 1106  var_license_start_date DATE;
15:15:36 1107  var_license_end_date   DATE;
15:15:36 1108  var_chargeback_date    DATE;
15:15:36 1109  var_invoice_days_used  NUMBER;
15:15:36 1110  -- EXCEPTIONS
15:15:36 1111  BAD_INVOICE_ID EXCEPTION;
15:15:36 1112  BEGIN
15:15:36 1113  
15:15:36 1114  	 var_chargeback_date := NVL(in_chargeback_date, SYSDATE);
15:15:36 1115  
15:15:36 1116  	 BEGIN
15:15:36 1117  	   SELECT
15:15:36 1118  	     INVOICE.ID into temp_invoice_id
15:15:36 1119  	   FROM
15:15:36 1120  	     INVOICE
15:15:36 1121  	   WHERE
15:15:36 1122  	     INVOICE.ID = in_invoice_id;
15:15:36 1123  	   EXCEPTION
15:15:36 1124  	     WHEN NO_DATA_FOUND THEN
15:15:36 1125  	       RAISE BAD_INVOICE_ID;
15:15:36 1126  	 END;
15:15:36 1127  
15:15:36 1128  	 var_invoice_days_used := 0;
15:15:36 1129  
15:15:36 1130  	 FOR f_license IN (
15:15:36 1131  	   SELECT
15:15:36 1132  	     LICENSE.START_DATE,
15:15:36 1133  	     LICENSE.END_DATE
15:15:36 1134  	   FROM
15:15:36 1135  	     LICENSE
15:15:36 1136  	   WHERE
15:15:36 1137  	     LICENSE.INVOICE_ID = in_invoice_id
15:15:36 1138  	     AND LICENSE.IS_EXTENSION = GLOBAL_CONSTANTS_V21.FALSE
15:15:36 1139  	 )
15:15:36 1140  	 LOOP
15:15:36 1141  	   var_license_start_date := PROCS_COMMON_V21.NORMALIZE_DATE(f_license.START_DATE);
15:15:36 1142  	   var_license_end_date := PROCS_COMMON_V21.NORMALIZE_DATE(f_license.END_DATE);
15:15:36 1143  
15:15:36 1144  	   IF var_license_start_date <= var_chargeback_date THEN
15:15:36 1145  	     IF var_license_end_date <= var_chargeback_date THEN
15:15:36 1146  	       -- License is passed
15:15:36 1147  	       var_invoice_days_used := var_invoice_days_used + (var_license_end_date - var_license_start_date);
15:15:36 1148  	     ELSE
15:15:36 1149  	       -- This is current license
15:15:36 1150  	       var_invoice_days_used := var_invoice_days_used + (var_chargeback_date - var_license_start_date);
15:15:36 1151  	     END IF;
15:15:36 1152  	   ELSE
15:15:36 1153  	     -- if var_license_start_date > in_chargeback_date then do nothing
15:15:36 1154  	     NULL;
15:15:36 1155  	   END IF;
15:15:36 1156  	 END LOOP;
15:15:36 1157  
15:15:36 1158  	 out_days_num := var_invoice_days_used;
15:15:36 1159  
15:15:36 1160  EXCEPTION
15:15:36 1161  WHEN BAD_INVOICE_ID THEN
15:15:36 1162  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 1163  	   SPROC_NAME, 'No such invoice');
15:15:36 1164  WHEN OTHERS THEN
15:15:36 1165  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 1166  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:15:36 1167  END GET_INVOICE_DAYS_USED_NUMBER;
