15:15:36 2572  
15:15:36 2573  FUNCTION IS_SUBSCRIPTION_CANCELABLE (
15:15:36 2574  	 in_subscription_id IN NUMBER
15:15:36 2575  ) RETURN NUMBER AS
15:15:36 2576  SPROC_NAME CONSTANT VARCHAR2(26) := 'IS_SUBSCRIPTION_CANCELABLE';
15:15:36 2577  -- VARIABLES
15:15:36 2578  var_is_offer_chain_cancelable NUMBER;
15:15:36 2579  --64603
15:15:36 2580  var_end_date date;
15:15:36 2581  today_date date := current_date;
15:15:36 2582  offer_id number;
15:15:36 2583  -- EXCEPTIONS
15:15:36 2584  COULD_NOT_CHECK	   EXCEPTION;
15:15:36 2585  BAD_SUBSCRIPTION_ID EXCEPTION;
15:15:36 2586  EXCEPTION_MESSAGE   VARCHAR2(1024);
15:15:36 2587  BEGIN
15:15:36 2588  
15:15:36 2589  	 BEGIN
15:15:36 2590  	   -- find offer_chain_id for given in_subscription_id
15:15:36 2591  	   SELECT OFFER_CHAIN_ID into offer_id
15:15:36 2592  	   FROM SUBSCRIPTION
15:15:36 2593  	   WHERE ID = in_subscription_id;
15:15:36 2594  	   EXCEPTION
15:15:36 2595  	     WHEN NO_DATA_FOUND THEN
15:15:36 2596  	       RAISE BAD_SUBSCRIPTION_ID;
15:15:36 2597  	     WHEN OTHERS THEN
15:15:36 2598  	       EXCEPTION_MESSAGE := SQLERRM;
15:15:36 2599  	       RAISE COULD_NOT_CHECK;
15:15:36 2600  	 END;
15:15:36 2601  
15:15:36 2602  	 -- find if it was redeemed from a gift certificate
15:15:36 2603  	 BEGIN
15:15:36 2604  	   SELECT l.end_date INTO var_end_date
15:15:36 2605  	   FROM LICENSE l, GIFT_CERTIFICATE g
15:15:36 2606  	   WHERE l.invoice_id = g.finalized_invoice_id
15:15:36 2607  	   AND l.subscription_id = in_subscription_id;
15:15:36 2608  
15:15:36 2609  	   -- if the license end_date is bigger than today, we are in the
15:15:36 2610  	   -- first period, so we cannot cancel; otherwise can cancel
15:15:36 2611  	   IF var_end_date > today_date THEN
15:15:36 2612  	       RETURN GLOBAL_CONSTANTS_V21.FALSE;
15:15:36 2613  	   ELSE
15:15:36 2614  	       RETURN GLOBAL_CONSTANTS_V21.TRUE;
15:15:36 2615  	   END IF;
15:15:36 2616  
15:15:36 2617  	   EXCEPTION
15:15:36 2618  	       -- not coming from a gift certificate,
15:15:36 2619  	       -- use old logic
15:15:36 2620  	       WHEN NO_DATA_FOUND THEN
15:15:36 2621  		   SELECT
15:15:36 2622  		       PROCS_OFFER_CHAIN_V21.IS_OFFER_CHAIN_CANCELABLE(offer_id)
15:15:36 2623  		       INTO var_is_offer_chain_cancelable
15:15:36 2624  		   FROM DUAL;
15:15:36 2625  		   RETURN var_is_offer_chain_cancelable;
15:15:36 2626  	 END;
15:15:36 2627  
15:15:36 2628  EXCEPTION
15:15:36 2629  WHEN BAD_SUBSCRIPTION_ID THEN
15:15:36 2630  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 2631  	   SPROC_NAME, 'No such subscription');
15:15:36 2632  WHEN COULD_NOT_CHECK THEN
15:15:36 2633  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:36 2634  	   SPROC_NAME, 'Could not check if offer chain calcelable', EXCEPTION_MESSAGE);
15:15:36 2635  WHEN OTHERS THEN
15:15:36 2636  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 2637  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:15:36 2638  
15:15:36 2639  END IS_SUBSCRIPTION_CANCELABLE;
15:15:36 2640  /******************************************************************************/
15:15:36 2641  
15:15:36 2642  PROCEDURE GET_SUBSCR_PROD_OFFERRINGS (
15:15:36 2643  /*
15:15:36 2644  APP_EXCEPTION_CODES_V21.NOT_FOUND
15:15:36 2645  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
15:15:36 2646  */
15:15:36 2647  	 in_subscription_id IN NUMBER,
15:15:36 2648  	 out_result_set     OUT SYS_REFCURSOR
15:15:36 2649  ) AS
15:15:36 2650  SPROC_NAME CONSTANT VARCHAR2(26) := 'GET_SUBSCR_PROD_OFFERRINGS';
15:15:36 2651  -- VARIABLES
15:15:36 2652  var_offer NUMBER;
15:15:36 2653  -- EXCEPTIONS
15:15:36 2654  BAD_SUBSCRIPTION_ID EXCEPTION;
15:15:36 2655  BEGIN
15:15:36 2656  
15:15:36 2657  	 OPEN out_result_set FOR
15:15:36 2658  	 SELECT DISTINCT
15:15:36 2659  	   PRODUCT_OFFERING.ID,
15:15:36 2660  	   PRODUCT_OFFERING.PRODUCT_ID,
15:15:36 2661  	   PRODUCT_OFFERING.TAX_CATEGORY_ID,
15:15:36 2662  	   PRODUCT_OFFERING.UNIT_PRICE,
15:15:36 2663  	   PRODUCT_OFFERING.QUANTITY,
15:15:36 2664  	   PRODUCT_OFFERING.CREATE_DATE,
15:15:36 2665  	   PRODUCT_OFFERING.CREATED_BY,
15:15:36 2666  	   CAPABILITY.ID CAP_ID,
15:15:36 2667  	   CAPABILITY.CODE CAP_CODE,
15:15:36 2668  	   CAPABILITY.DESCRIPTION CAP_DESCRIPTION,
15:15:36 2669  	   CAPABILITY.SHAREABLE CAP_SHAREABLE
15:15:36 2670  	 FROM
15:15:36 2671  	   OFFER_PRODUCT_OFFERING
15:15:36 2672  	   INNER JOIN PRODUCT_OFFERING ON PRODUCT_OFFERING.ID = OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID
15:15:36 2673  	   INNER JOIN CAPABILITY ON PRODUCT_OFFERING.CAPABILITY_ID = CAPABILITY.ID
15:15:36 2674  	 WHERE
15:15:36 2675  	   OFFER_PRODUCT_OFFERING.OFFER_ID IN (
15:15:36 2676  	     SELECT
15:15:36 2677  	       LICENSE.OFFER_ID
15:15:36 2678  	     FROM
15:15:36 2679  	       SUBSCRIPTION
15:15:36 2680  	       JOIN LICENSE ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID AND LICENSE.NEEDS_ENTITLEMENTS = GLOBAL_CONSTANTS_V21.TRUE
15:15:36 2681  	     WHERE
15:15:36 2682  	       SUBSCRIPTION.ID = in_subscription_id
15:15:36 2683  	   );
