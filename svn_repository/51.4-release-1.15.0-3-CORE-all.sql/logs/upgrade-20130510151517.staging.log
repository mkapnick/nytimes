15:15:36 122  PROCEDURE GET_PENDING_REFUND_CHARGES (
15:15:36 123  /*
15:15:36 124  Throws exceptions:
15:15:36 125  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
15:15:36 126  */
15:15:36 127  	out_result_set	    OUT SYS_REFCURSOR,
15:15:36 128  	in_row_number	    IN NUMBER DEFAULT NULL
15:15:36 129  ) AS
15:15:36 130  SPROC_NAME CONSTANT VARCHAR2(26) := 'GET_PENDING_REFUND_CHARGES';
15:15:36 131  -- COMSTANTS
15:15:36 132  DEFAULT_ROW_NUMBER CONSTANT NUMBER := 1;
15:15:36 133  -- VARIABLES
15:15:36 134  var_row_number NUMBER;
15:15:36 135  BEGIN
15:15:36 136  	IF in_row_number IS NULL THEN
15:15:36 137  	  var_row_number := DEFAULT_ROW_NUMBER;
15:15:36 138  	ELSE
15:15:36 139  	  var_row_number := in_row_number;
15:15:36 140  	END IF;
15:15:36 141  
15:15:36 142  	-- Select charges
15:15:36 143  	OPEN out_result_set FOR
15:15:36 144  SELECT * FROM
15:15:36 145  (
15:15:36 146  	SELECT
15:15:36 147  	  CHARGE.ID,
15:15:36 148  	  CHARGE.TRANSACTION_ID,
15:15:36 149  	  CHARGE.INSTRUMENT_ID,
15:15:36 150  	  CHARGE.INSTRUMENT_TYPE_ID,
15:15:36 151  	  CHARGE.CHARGE_AMOUNT,
15:15:36 152  	  CHARGE.CREATE_DATE,
15:15:36 153  	  CHARGE.CREATED_BY,
15:15:36 154  	  CHARGE.INVOICE_ID
15:15:36 155  	FROM
15:15:36 156  	  CHARGE
15:15:36 157  	  INNER JOIN TRANSACTION ON CHARGE.TRANSACTION_ID = TRANSACTION.ID
15:15:36 158  	WHERE
15:15:36 159  	  TRANSACTION.TRANSACTION_STATUS_ID = GLOBAL_STATUSES_V21.TRANSACTION_PENDING
15:15:36 160  	  AND TRANSACTION.IS_REFUND = GLOBAL_CONSTANTS_V21.TRUE
15:15:36 161  	  AND TRANSACTION.TRANSACTION_AMOUNT < 0
15:15:36 162  	  AND NOT EXISTS
15:15:36 163  	  (
15:15:36 164  	    SELECT NULL
15:15:36 165  	    FROM PROCESS_RETRY_THROTTLE
15:15:36 166  	    WHERE PROCESS_NAME = SPROC_NAME
15:15:36 167  	      AND GENERIC_ID = CHARGE.ID
15:15:36 168  	  )
15:15:36 169  	  AND ROWNUM <= var_row_number*10
15:15:36 170  	  ORDER BY dbms_random.value
15:15:36 171  ) WHERE
15:15:36 172  	  ROWNUM <= var_row_number;
15:15:36 173  
15:15:36 174  END GET_PENDING_REFUND_CHARGES;
