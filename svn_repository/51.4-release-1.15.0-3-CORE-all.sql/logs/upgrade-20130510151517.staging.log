15:15:37 473  PENDING_TRANSACTIONS_FOUND EXCEPTION;
15:15:37 474  CAN_NOT_GET_DEF_FINANCIAL EXCEPTION;
15:15:37 475  CAN_NOT_DEL_DEF_FINANCIAL EXCEPTION;
15:15:37 476  EXCEPTION_MESSAGE VARCHAR2(1024);
15:15:37 477  BEGIN
15:15:37 478  
15:15:37 479  	-- Get Paypal status
15:15:37 480  	-- Get account
15:15:37 481  	BEGIN
15:15:37 482  	  SELECT
15:15:37 483  	    PAYPAL.PAYPAL_STATUS_ID,
15:15:37 484  	    PAYPAL.ACCOUNT_ID
15:15:37 485  	    into
15:15:37 486  	    var_paypal_status_id,
