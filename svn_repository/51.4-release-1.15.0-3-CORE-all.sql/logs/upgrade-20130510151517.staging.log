15:15:35 414  FUNCTION CALCULATE_OFFER_CHAIN_END_DATE (
15:15:35 415  	in_offer_chain_id	  IN NUMBER,
15:15:35 416  	in_offer_chain_start_date IN DATE
15:15:35 417  ) RETURN DATE AS
15:15:35 418  -- VARIABLES
15:15:35 419  SPROC_NAME	     CONSTANT VARCHAR2(30) := 'CALCULATE_OFFER_CHAIN_END_DATE';
15:15:35 420  temp_offer_chain_id    NUMBER;
15:15:35 421  var_offer_chain_length NUMBER;
15:15:35 422  var_offer_duration     VARCHAR2(30);
15:15:35 423  var_offer_recurrences  NUMBER;
15:15:35 424  var_end_date	     DATE;
15:15:35 425  
15:15:35 426  var_offer_ym_interval INTERVAL YEAR TO MONTH;
15:15:35 427  var_offer_ds_interval INTERVAL DAY(3) TO SECOND;
15:15:35 428  var_offer_years	    NUMBER;
15:15:35 429  var_offer_months	    NUMBER;
15:15:35 430  var_offer_days	    NUMBER;
15:15:35 431  var_infinity_offers_count NUMBER;
15:15:35 432  
15:15:35 433  var_offers_set SYS_REFCURSOR;
15:15:35 434  
15:15:35 435  -- EXCEPTIONS
15:15:35 436  BAD_OFFER_CHAIN_ID EXCEPTION;
15:15:35 437  BEGIN
15:15:35 438  
15:15:35 439  	var_end_date := in_offer_chain_start_date;
15:15:35 440  
15:15:35 441  	-- Check that offer chain exists
15:15:35 442  	BEGIN
15:15:35 443  	  SELECT
15:15:35 444  	    OFFER_CHAIN.ID into temp_offer_chain_id
15:15:35 445  	  FROM
15:15:35 446  	    OFFER_CHAIN
15:15:35 447  	  WHERE
15:15:35 448  	    OFFER_CHAIN.ID = in_offer_chain_id;
15:15:35 449  	  EXCEPTION
15:15:35 450  	    WHEN NO_DATA_FOUND
15:15:35 451  	      THEN RAISE BAD_OFFER_CHAIN_ID;
15:15:35 452  	END;
15:15:35 453  
15:15:35 454  	SELECT
15:15:35 455  	  COUNT(*) into var_infinity_offers_count
15:15:35 456  	FROM
15:15:35 457  	  OFFER_OFFER_CHAIN
15:15:35 458  	WHERE
15:15:35 459  	  OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
15:15:35 460  	  AND OFFER_OFFER_CHAIN.NUM_RECURRENCES = GLOBAL_ENUMS_V21.OFFER_REC_INFINITY;
15:15:35 461  
15:15:35 462  	IF var_infinity_offers_count > 0 THEN
15:15:35 463  	  -- Offer chain contains offers with infinity num of recurrences
15:15:35 464  	  RETURN NULL;
15:15:35 465  	END IF;
15:15:35 466  
15:15:35 467  	-- Select offers durations
15:15:35 468  	OPEN var_offers_set FOR
15:15:35 469  	SELECT
15:15:35 470  	  OFFER.ENTITLEMENT_DURATION,
15:15:35 471  	  OFFER_OFFER_CHAIN.NUM_RECURRENCES
15:15:35 472  	FROM
15:15:35 473  	  OFFER_OFFER_CHAIN
15:15:35 474  	  INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
15:15:35 475  	WHERE
15:15:35 476  	  OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id;
15:15:35 477  
15:15:35 478  	-- Calculate sum of offers durations
15:15:35 479  	LOOP
15:15:35 480  	  FETCH var_offers_set INTO var_offer_duration, var_offer_recurrences;
15:15:35 481  	  EXIT WHEN var_offers_set%NOTFOUND;
15:15:35 482  	  PROCS_COMMON_V21.ISO8601DURATION_TO_INTERVALS(var_offer_duration, var_offer_years, var_offer_months, var_offer_days);
15:15:35 483  	  var_offer_ym_interval := var_offer_years||'-'||var_offer_months;
15:15:35 484  	  var_offer_ds_interval := var_offer_days||' 0:0:0';
15:15:35 485  	  var_end_date := var_end_date + ( var_offer_ym_interval * ( var_offer_recurrences + 1) ) + ( var_offer_ds_interval * ( var_offer_recurrences + 1) );
15:15:35 486  	END LOOP;
15:15:35 487  
15:15:35 488  	RETURN var_end_date;
15:15:35 489  
15:15:35 490  EXCEPTION
15:15:35 491  WHEN BAD_OFFER_CHAIN_ID THEN
15:15:35 492  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:35 493  	  SPROC_NAME, 'No such offer chain');
15:15:35 494  WHEN OTHERS THEN
15:15:35 495  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:35 496  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:15:35 497  END CALCULATE_OFFER_CHAIN_END_DATE;
