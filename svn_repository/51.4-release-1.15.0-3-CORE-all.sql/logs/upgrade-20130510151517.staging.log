15:15:36 437  PROCEDURE GET_TRANSACTION_INVOICE (
15:15:36 438  /*
15:15:36 439  Throws exceptions:
15:15:36 440  APP_EXCEPTION_CODES_V21.NOT_FOUND
15:15:36 441  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
15:15:36 442  */
15:15:36 443  	in_transaction_id  IN  NUMBER,
15:15:36 444  	out_result_set	      OUT SYS_REFCURSOR
15:15:36 445  ) AS
15:15:36 446  SPROC_NAME CONSTANT  VARCHAR2(23) := 'GET_TRANSACTION_INVOICE';
15:15:36 447  -- VARIABLES
15:15:36 448  temp_transaction_id  NUMBER;
15:15:36 449  var_invoice_id	   NUMBER;
15:15:36 450  var_subscription_id  NUMBER;
15:15:36 451  var_offer_chain_id   NUMBER;
15:15:36 452  var_offer_chain_name VARCHAR2(255);
15:15:36 453  -- EXCEPTIONS
15:15:36 454  BAD_TRANSACTION_ID     EXCEPTION;
15:15:36 455  EXCEPTION_MESSAGE      VARCHAR2(1024);
15:15:36 456  BEGIN
15:15:36 457   -- check if there is the transaction
15:15:36 458  	BEGIN
15:15:36 459  	  SELECT
15:15:36 460  	    TRANSACTION.ID into temp_transaction_id
15:15:36 461  	  FROM
15:15:36 462  	    TRANSACTION
15:15:36 463  	  WHERE
15:15:36 464  	    TRANSACTION.ID = in_transaction_id;
15:15:36 465  	  EXCEPTION
15:15:36 466  	    WHEN NO_DATA_FOUND THEN
15:15:36 467  	      RAISE BAD_TRANSACTION_ID;
15:15:36 468  	END;
15:15:36 469  
15:15:36 470  	-- Get invoice id
15:15:36 471  	SELECT DISTINCT
15:15:36 472  	  CHARGE.INVOICE_ID into var_invoice_id
15:15:36 473  	FROM
15:15:36 474  	  CHARGE
15:15:36 475  	WHERE
15:15:36 476  	  CHARGE.TRANSACTION_ID = in_transaction_id;
15:15:36 477  
15:15:36 478  	-- Get subscription id if exists
15:15:36 479  	BEGIN
15:15:36 480  	  SELECT DISTINCT
15:15:36 481  	    LICENSE.SUBSCRIPTION_ID into var_subscription_id
15:15:36 482  	  FROM
15:15:36 483  	    LICENSE
15:15:36 484  	  WHERE
15:15:36 485  	    LICENSE.INVOICE_ID = var_invoice_id;
15:15:36 486  	  EXCEPTION
15:15:36 487  	    WHEN NO_DATA_FOUND THEN
15:15:36 488  	      var_subscription_id := NULL;
15:15:36 489  	END;
15:15:36 490  
15:15:36 491  	IF var_subscription_id IS NOT NULL THEN
15:15:36 492  	  -- Fetch offer chain from subscription
15:15:36 493  	  SELECT
15:15:36 494  	    OFFER_CHAIN.ID,
15:15:36 495  	    OFFER_CHAIN.NAME
15:15:36 496  	    into
15:15:36 497  	    var_offer_chain_id,
15:15:36 498  	    var_offer_chain_name
15:15:36 499  	  FROM
15:15:36 500  	    OFFER_CHAIN
15:15:36 501  	    INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
15:15:36 502  	  WHERE
15:15:36 503  	    SUBSCRIPTION.ID = var_subscription_id;
15:15:36 504  	ELSE
15:15:36 505  	  -- Fetch offer chain from GC
15:15:36 506  	  SELECT
15:15:36 507  	    OFFER_CHAIN.ID,
15:15:36 508  	    OFFER_CHAIN.NAME
15:15:36 509  	    into
