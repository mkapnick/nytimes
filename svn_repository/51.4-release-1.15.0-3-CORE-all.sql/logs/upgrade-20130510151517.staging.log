15:15:36 328  
15:15:36 329  PROCEDURE GET_RECURRING_OFFER (
15:15:36 330  	in_license_id  IN NUMBER,
15:15:36 331  	out_result_set OUT SYS_REFCURSOR
15:15:36 332  ) AS
15:15:36 333  -- VARIABLES
15:15:36 334  SPROC_NAME		    CONSTANT VARCHAR2(19) := 'GET_RECURRING_OFFER';
15:15:36 335  var_offer_chain_id	    NUMBER;
15:15:36 336  var_offer_id		    NUMBER;
15:15:36 337  var_offer_num_recurrences     NUMBER;
15:15:36 338  var_license_cur_offer_rec_num NUMBER;
15:15:36 339  var_offer_index		    NUMBER;
15:15:36 340  -- EXCEPTIONS
15:15:36 341  BAD_LICENSE_ID	     EXCEPTION;
15:15:36 342  CAN_NOT_GET_OFFER_INFO EXCEPTION;
15:15:36 343  BEGIN
15:15:36 344  
15:15:36 345  	BEGIN
15:15:36 346  	  SELECT
15:15:36 347  	    SUBSCRIPTION.OFFER_CHAIN_ID,
15:15:36 348  	    LICENSE.OFFER_ID,
15:15:36 349  	    LICENSE.CURRENT_OFFER_RECURR_NUM
15:15:36 350  	    into
15:15:36 351  	    var_offer_chain_id,
15:15:36 352  	    var_offer_id,
15:15:36 353  	    var_license_cur_offer_rec_num
15:15:36 354  	  FROM
15:15:36 355  	    LICENSE
15:15:36 356  	    INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:15:36 357  	  WHERE
15:15:36 358  	    LICENSE.ID = in_license_id;
15:15:36 359  	  EXCEPTION
15:15:36 360  	    WHEN NO_DATA_FOUND THEN
15:15:36 361  	      RAISE BAD_LICENSE_ID;
15:15:36 362  	END;
15:15:36 363  
15:15:36 364  	BEGIN
15:15:36 365  	  SELECT
15:15:36 366  	    OFFER_OFFER_CHAIN.NUM_RECURRENCES,
15:15:36 367  	    OFFER_OFFER_CHAIN.INDEX_VALUE
15:15:36 368  	    into
15:15:36 369  	    var_offer_num_recurrences,
15:15:36 370  	    var_offer_index
15:15:36 371  	  FROM
15:15:36 372  	    OFFER_OFFER_CHAIN
15:15:36 373  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
15:15:36 374  	  WHERE
15:15:36 375  	    OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
15:15:36 376  	    AND OFFER_OFFER_CHAIN.OFFER_ID = var_offer_id;
15:15:36 377  	  EXCEPTION
15:15:36 378  	    WHEN NO_DATA_FOUND THEN
15:15:36 379  	      RAISE CAN_NOT_GET_OFFER_INFO;
15:15:36 380  	END;
15:15:36 381  
15:15:36 382  	IF var_offer_num_recurrences = 0 THEN
15:15:36 383  	  --out_result_set := NULL;
15:15:36 384  	  -- TODO: Remove this hardcode
15:15:36 385  	  OPEN out_result_set FOR
15:15:36 386  	  SELECT
15:15:36 387  	    OFFER.ID,
15:15:36 388  	    OFFER.OFFER_STATUS_ID,
15:15:36 389  	    OFFER.ENTITLEMENT_DURATION,
15:15:36 390  	    OFFER.CREATED_BY,
15:15:36 391  	    OFFER.CREATE_DATE,
15:15:36 392  	    OFFER.UPDATED_BY,
15:15:36 393  	    OFFER.UPDATE_DATE,
15:15:36 394  	    var_offer_num_recurrences as "RECURRENCE_NUMBER",
15:15:36 395  	    var_offer_index as "OFFER_INDEX"
15:15:36 396  	  FROM
15:15:36 397  	    OFFER_OFFER_CHAIN
15:15:36 398  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
15:15:36 399  	  WHERE ROWNUM = 0;
15:15:36 400  	ELSIF var_license_cur_offer_rec_num = var_offer_num_recurrences THEN
15:15:36 401  	  --out_result_set := NULL;
15:15:36 402  	  -- TODO: Remove this hardcode
15:15:36 403  	  OPEN out_result_set FOR
15:15:36 404  	  SELECT
15:15:36 405  	    OFFER.ID,
15:15:36 406  	    OFFER.OFFER_STATUS_ID,
15:15:36 407  	    OFFER.ENTITLEMENT_DURATION,
15:15:36 408  	    OFFER.CREATED_BY,
15:15:36 409  	    OFFER.CREATE_DATE,
15:15:36 410  	    OFFER.UPDATED_BY,
15:15:36 411  	    OFFER.UPDATE_DATE,
15:15:36 412  	    var_offer_num_recurrences as "RECURRENCE_NUMBER",
15:15:36 413  	    var_offer_index as "OFFER_INDEX"
15:15:36 414  	  FROM
15:15:36 415  	    OFFER_OFFER_CHAIN
15:15:36 416  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
15:15:36 417  	  WHERE ROWNUM = 0;
15:15:36 418  	ELSE
15:15:36 419  	  OPEN out_result_set FOR
15:15:36 420  	  SELECT
15:15:36 421  	    OFFER.ID,
15:15:36 422  	    OFFER.OFFER_STATUS_ID,
15:15:36 423  	    OFFER.ENTITLEMENT_DURATION,
15:15:36 424  	    OFFER.CREATED_BY,
15:15:36 425  	    OFFER.CREATE_DATE,
15:15:36 426  	    OFFER.UPDATED_BY,
15:15:36 427  	    OFFER.UPDATE_DATE,
15:15:36 428  	    var_offer_num_recurrences as "RECURRENCE_NUMBER",
15:15:36 429  	    var_offer_index as "OFFER_INDEX"
15:15:36 430  	  FROM
15:15:36 431  	    OFFER
15:15:36 432  	  WHERE
15:15:36 433  	    OFFER.ID = var_offer_id;
15:15:36 434  	END IF;
15:15:36 435  
15:15:36 436  EXCEPTION
15:15:36 437  WHEN BAD_LICENSE_ID THEN
15:15:36 438  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 439  	  SPROC_NAME, 'No such license');
15:15:36 440  WHEN CAN_NOT_GET_OFFER_INFO THEN
15:15:36 441  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 442  	  SPROC_NAME, 'Could not get offer information. Offer id = '||var_offer_id||', Offer chain id = '||var_offer_chain_id);
15:15:36 443  WHEN OTHERS THEN
15:15:36 444  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 445  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:15:36 446  END GET_RECURRING_OFFER;
