15:15:37 702  /******************************************************************************/
15:15:37 703  
15:15:37 704  PROCEDURE START_GC_PURCHASING (
15:15:37 705  	in_group_id		  IN NUMBER,
15:15:37 706  	in_offer_chain_id	  IN VARCHAR2,
15:15:37 707  	in_gift_certificate_code  IN  VARCHAR2,
15:15:37 708  	in_created_by		  IN  VARCHAR2,
15:15:37 709  	in_recipient_name	  IN  VARCHAR2,
15:15:37 710  	in_recipient_email	  IN  VARCHAR2,
15:15:37 711  	in_recipient_address_id   IN NUMBER,
15:15:37 712  	in_recipient_notify_date  IN DATE,
15:15:37 713  	in_sender_name		  IN VARCHAR2,
15:15:37 714  	in_sender_email 	  IN VARCHAR2,
15:15:37 715  	in_gift_message 	  IN  VARCHAR2,
15:15:37 716  	in_expiration_date	  IN DATE,
15:15:37 717  	out_gift_certificate_id   OUT NUMBER,
15:15:37 718  	out_invoice_id		  OUT NUMBER
15:15:37 719  ) AS
15:15:37 720  SPROC_NAME CONSTANT VARCHAR2(19) := 'START_GC_PURCHASING';
15:15:37 721  -- VARIABLES
15:15:37 722  var_account_id		NUMBER;
15:15:37 723  temp_gc_code		GIFT_CERTIFICATE.CODE%TYPE;
15:15:37 724  var_och_is_gc		NUMBER;
15:15:37 725  var_offer_chain_status_id NUMBER;
15:15:37 726  var_is_for_redemption	NUMBER;
15:15:37 727  var_new_invoice_id	NUMBER;
15:15:37 728  var_gift_cert_id		NUMBER;
15:15:37 729  var_account_tax_exempt_id VARCHAR2(255);
15:15:37 730  -- EXCEPTIONS
15:15:37 731  BAD_GROUP_ID		    EXCEPTION;
15:15:37 732  GC_CODE_ALREADY_EXISTS	    EXCEPTION;
15:15:37 733  BAD_OFFER_CHAIN_ID	    EXCEPTION;
15:15:37 734  OCH_IS_NOT_GIFT_CERTIFICATE   EXCEPTION;
15:15:37 735  BAD_OFFER_CHAIN_STATUS	    EXCEPTION;
15:15:37 736  CAN_NOT_PURCHASE_GC_FOR_RDMPN EXCEPTION;
15:15:37 737  CAN_NOT_CREATE_INVOICE	    EXCEPTION;
15:15:37 738  OFFER_REC_NUM_LESS_THAN_ONE   EXCEPTION;
15:15:37 739  CAN_NOT_CREATE_LINE_ITEMS     EXCEPTION;
15:15:37 740  
15:15:37 741  EXCEPTION_MESSAGE VARCHAR2(1024);
15:15:37 742  BEGIN
15:15:37 743  	-- Get account id
15:15:37 744  	BEGIN
15:15:37 745  	  SELECT
15:15:37 746  	    ACCOUNT.ID,
15:15:37 747  	    ACCOUNT.TAX_EXEMPT_ID
15:15:37 748  	    into
15:15:37 749  	    var_account_id,
15:15:37 750  	    var_account_tax_exempt_id
15:15:37 751  	  FROM
15:15:37 752  	    ACCOUNT
15:15:37 753  	  WHERE
15:15:37 754  	    ACCOUNT.GROUP_ID = in_group_id;
15:15:37 755  	  EXCEPTION
15:15:37 756  	  WHEN NO_DATA_FOUND THEN
15:15:37 757  	    RAISE BAD_GROUP_ID;
15:15:37 758  	END;
15:15:37 759  
15:15:37 760  	-- Check for the same code
15:15:37 761  	BEGIN
15:15:37 762  	  SELECT
15:15:37 763  	    GIFT_CERTIFICATE.CODE into temp_gc_code
15:15:37 764  	  FROM
15:15:37 765  	    GIFT_CERTIFICATE
15:15:37 766  	  WHERE
15:15:37 767  	    GIFT_CERTIFICATE.CODE = in_gift_certificate_code;
15:15:37 768  
15:15:37 769  	  RAISE GC_CODE_ALREADY_EXISTS;
15:15:37 770  
15:15:37 771  	  EXCEPTION
15:15:37 772  	    WHEN NO_DATA_FOUND THEN
15:15:37 773  	      NULL;
15:15:37 774  	END;
15:15:37 775  
15:15:37 776  	-- Get offer chain flag "is_gift_certificate"
15:15:37 777  	BEGIN
15:15:37 778  	  SELECT
15:15:37 779  	    OFFER_CHAIN.IS_GIFT_CERTIFICATE,
15:15:37 780  	    OFFER_CHAIN.OFFER_CHAIN_STATUS_ID
15:15:37 781  	    into
15:15:37 782  	    var_och_is_gc,
15:15:37 783  	    var_offer_chain_status_id
15:15:37 784  	  FROM
15:15:37 785  	    OFFER_CHAIN
15:15:37 786  	  WHERE
15:15:37 787  	    OFFER_CHAIN.ID = in_offer_chain_id;
15:15:37 788  	  EXCEPTION
15:15:37 789  	  WHEN NO_DATA_FOUND THEN
15:15:37 790  	    RAISE BAD_OFFER_CHAIN_ID;
15:15:37 791  	END;
15:15:37 792  
15:15:37 793  	IF var_och_is_gc != GLOBAL_CONSTANTS_V21.TRUE
15:15:37 794  	  OR var_och_is_gc IS NULL THEN
15:15:37 795  	  RAISE OCH_IS_NOT_GIFT_CERTIFICATE;
15:15:37 796  	END IF;
15:15:37 797  
15:15:37 798  	IF var_offer_chain_status_id != GLOBAL_STATUSES_V21.OFFER_CHAIN_ACTIVE THEN
15:15:37 799  	  RAISE BAD_OFFER_CHAIN_STATUS;
15:15:37 800  	END IF;
15:15:37 801  
15:15:37 802  	-- norlov: #38151 check if the OC is for Redemption:
15:15:37 803  	SELECT
15:15:37 804  	  COUNT(*) into var_is_for_redemption
15:15:37 805  	FROM
15:15:37 806  	  OFFER_CHAIN_ELIGIBILITY
15:15:37 807  	WHERE
15:15:37 808  	  OFFER_CHAIN_ELIGIBILITY.OFFER_CHAIN_ID = in_offer_chain_id
15:15:37 809  	  AND OFFER_CHAIN_ELIGIBILITY.NAME = GLOBAL_CONSTANTS_V21.GIFT_CERTIFICATE_REQUIRED
15:15:37 810  	  AND OFFER_CHAIN_ELIGIBILITY.VALUE = GLOBAL_CONSTANTS_V21.ELIGIBILITY_FLAG_SET;
15:15:37 811  
15:15:37 812  	IF var_is_for_redemption > 0 THEN
15:15:37 813  	  RAISE CAN_NOT_PURCHASE_GC_FOR_RDMPN;
15:15:37 814  	END IF;
15:15:37 815  
15:15:37 816  	-- Create new invoice
15:15:37 817  	BEGIN
15:15:37 818  	  PROCS_INVOICE_V21.CREATE_INVOICE(
15:15:37 819  	    in_invoice_status => GLOBAL_STATUSES_V21.INVOICE_OPEN,
15:15:37 820  	    in_created_by     => in_created_by,
15:15:37 821  	    in_tax_exempt_id  => var_account_tax_exempt_id,
15:15:37 822  	    out_invoice_id    => var_new_invoice_id
15:15:37 823  	  );
15:15:37 824  	  EXCEPTION
15:15:37 825  	    WHEN OTHERS THEN
15:15:37 826  	      EXCEPTION_MESSAGE := SQLERRM;
15:15:37 827  	      RAISE CAN_NOT_CREATE_INVOICE;
15:15:37 828  	END;
15:15:37 829  
15:15:37 830  	-- Add line items for new invoice
15:15:37 831  	BEGIN
15:15:37 832  	  FOR f_offer_data IN (
15:15:37 833  	    SELECT
15:15:37 834  	      OFFER_ID,
15:15:37 835  	      NUM_RECURRENCES
15:15:37 836  	    FROM
15:15:37 837  	      OFFER_OFFER_CHAIN
15:15:37 838  	    WHERE
15:15:37 839  	      OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
15:15:37 840  	  )
15:15:37 841  	  LOOP
15:15:37 842  	    IF f_offer_data.NUM_RECURRENCES < 1 THEN
15:15:37 843  	      RAISE OFFER_REC_NUM_LESS_THAN_ONE;
15:15:37 844  	    END IF;
15:15:37 845  	    FOR i_offer_recurrences_iterator IN 1..f_offer_data.NUM_RECURRENCES
15:15:37 846  	    LOOP
15:15:37 847  	      PROCS_LINE_ITEMS_V21.ADD_LINE_ITEMS(
15:15:37 848  		in_invoice_id => var_new_invoice_id,
15:15:37 849  		in_offer_id   => f_offer_data.OFFER_ID,
15:15:37 850  		in_created_by => in_created_by
15:15:37 851  	      );
15:15:37 852  	    END LOOP;
15:15:37 853  	  END LOOP;
15:15:37 854  
15:15:37 855  	  EXCEPTION
15:15:37 856  	    WHEN OTHERS THEN
15:15:37 857  	      EXCEPTION_MESSAGE := SQLERRM;
15:15:37 858  	      RAISE CAN_NOT_CREATE_LINE_ITEMS;
15:15:37 859  	END;
15:15:37 860  
15:15:37 861  	PROCS_FIN_INSTRUMENTS_CRU_V21.CREATE_GIFT_CERTIFICATE(
15:15:37 862  	  out_gift_certificate_id	=> var_gift_cert_id,
15:15:37 863  	  in_purchaser_group_id 	=> in_group_id,
15:15:37 864  	  in_purchaser_invoice_id	=> var_new_invoice_id,
15:15:37 865  	  in_offer_chain_id		=> in_offer_chain_id,
15:15:37 866  	  in_expiration_date		=> in_expiration_date,
15:15:37 867  	  in_purchase_date		=> SYSDATE,
15:15:37 868  	  in_gift_certificate_status_id => GLOBAL_STATUSES_V21.GIFT_CERTIFICATE_ACTIVE,
15:15:37 869  	  in_code			=> in_gift_certificate_code,
15:15:37 870  	  in_created_by 		=> in_created_by,
15:15:37 871  	  in_recipient_name		=> in_recipient_name,
15:15:37 872  	  in_gift_message		=> in_gift_message,
15:15:37 873  	  in_recipient_email		=> in_recipient_email,
15:15:37 874  	  in_sender_email		=> in_sender_email,
15:15:37 875  	  in_sender_name		=> in_sender_name,
15:15:37 876  	  in_recipient_address_id	=> in_recipient_address_id,
15:15:37 877  	  in_recipient_notify_date	=> in_recipient_notify_date
15:15:37 878  	);
15:15:37 879  
15:15:37 880  	out_gift_certificate_id := var_gift_cert_id;
15:15:37 881  	out_invoice_id := var_new_invoice_id;
15:15:37 882  
15:15:37 883  EXCEPTION
15:15:37 884  WHEN BAD_OFFER_CHAIN_STATUS THEN
15:15:37 885  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
15:15:37 886  	  SPROC_NAME, 'Offer chain is not active');
15:15:37 887  WHEN GC_CODE_ALREADY_EXISTS THEN
15:15:37 888  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.DUPLICATE_ERROR,
15:15:37 889  	  SPROC_NAME, 'Gift certificate with same code already exists');
15:15:37 890  WHEN OCH_IS_NOT_GIFT_CERTIFICATE THEN
15:15:37 891  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INVALID_PARAMETER,
15:15:37 892  	  SPROC_NAME, 'This offer chain can not be used for gift certificate');
15:15:37 893  WHEN CAN_NOT_PURCHASE_GC_FOR_RDMPN THEN
15:15:37 894  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INVALID_PARAMETER,
15:15:37 895  	  SPROC_NAME, 'This offer chain can not be purchased for gift certificate since it is for redemption');
15:15:37 896  WHEN CAN_NOT_CREATE_INVOICE THEN
15:15:37 897  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:37 898  	  SPROC_NAME, 'Could not create new invoice', EXCEPTION_MESSAGE);
15:15:37 899  WHEN CAN_NOT_CREATE_LINE_ITEMS THEN
15:15:37 900  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:37 901  	  SPROC_NAME, 'Could not create line items', EXCEPTION_MESSAGE);
15:15:37 902  WHEN BAD_GROUP_ID THEN
15:15:37 903  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:37 904  	  SPROC_NAME, 'No such group id');
15:15:37 905  WHEN BAD_OFFER_CHAIN_ID THEN
15:15:37 906  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:37 907  	  SPROC_NAME, 'No such offer chain');
15:15:37 908  WHEN OFFER_REC_NUM_LESS_THAN_ONE THEN
15:15:37 909  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:37 910  	  SPROC_NAME, 'Some offer has recurrences number less than 1');
15:15:37 911  WHEN OTHERS THEN
15:15:37 912  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:37 913  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:15:37 914  END START_GC_PURCHASING;
