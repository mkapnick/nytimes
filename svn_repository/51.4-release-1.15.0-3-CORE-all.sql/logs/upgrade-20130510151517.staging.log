15:15:36 338  -- VARIABLES
15:15:36 339  SPROC_NAME CONSTANT VARCHAR2(19) := 'GET_INVOICE_DETAILS';
15:15:36 340  
15:15:36 341  -- EXCEPTIONS
15:15:36 342  BAD_INVOICE_ID	     EXCEPTION;
15:15:36 343  CAN_NOT_FIND_ACCOUNT   EXCEPTION;
15:15:36 344  EXCEPTION_MESSAGE      VARCHAR2(1024);
15:15:36 345  BEGIN
15:15:36 346  
15:15:36 347  	-- get invoice status
15:15:36 348  	BEGIN
15:15:36 349  	  SELECT
15:15:36 350  	    INVOICE.INVOICE_STATUS_ID into out_status_id
15:15:36 351  	  FROM
15:15:36 352  	    INVOICE
15:15:36 353  	  WHERE
15:15:36 354  	    INVOICE.ID = in_invoice_id;
15:15:36 355  	  EXCEPTION
15:15:36 356  	    WHEN NO_DATA_FOUND THEN
15:15:36 357  	      RAISE BAD_INVOICE_ID;
15:15:36 358  	END;
15:15:36 359  
15:15:36 360  	-- get group id
15:15:36 361  	BEGIN
15:15:36 362  	  PROCS_INVOICE_V21.GET_ACCOUNT_BY_INVOICE_ID(in_invoice_id, out_group_id);
15:15:36 363  	  EXCEPTION
15:15:36 364  	    WHEN OTHERS THEN
15:15:36 365  	      EXCEPTION_MESSAGE := SQLERRM;
15:15:36 366  	      RAISE CAN_NOT_FIND_ACCOUNT;
15:15:36 367  	END;
15:15:36 368  
15:15:36 369  	-- get all line items for given invoice
15:15:36 370  	OPEN out_line_items_set FOR
15:15:36 371  	SELECT
15:15:36 372  	  LINE_ITEM.AMOUNT,
15:15:36 373  	  LINE_ITEM.ID,
15:15:36 374  	  LINE_ITEM.CREATED_BY,
15:15:36 375  	  LINE_ITEM.CREATE_DATE,
