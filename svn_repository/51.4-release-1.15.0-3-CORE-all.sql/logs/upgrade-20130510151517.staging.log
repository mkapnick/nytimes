15:15:35 106  FUNCTION CREATE_NEW_TRANSFER_SET
15:15:35 107  	  (
15:15:35 108  	      in_set_maximum NUMBER)
15:15:35 109  	  RETURN core_owner.polling_sync.set_id%type
15:15:35 110  IS
15:15:35 111  	  pragma autonomous_transaction;
15:15:35 112  	  v_set_id core_owner.polling_sync.set_id%type;
15:15:35 113  BEGIN
15:15:35 114  	  SELECT
15:15:35 115  	      pollsync_setid_seq.nextval
15:15:35 116  	  INTO
15:15:35 117  	      v_set_id
15:15:35 118  	  FROM
15:15:35 119  	      dual;
15:15:35 120  	  update
15:15:35 121  	    core_owner.polling_sync ps
15:15:35 122  	  set
15:15:35 123  	    ps.set_id = v_set_id
15:15:35 124  	  where
15:15:35 125  	    ps.set_id IS NULL and
15:15:35 126  	    rownum <= in_set_maximum
15:15:35 127  	  ;
15:15:35 128  	  COMMIT;
15:15:35 129  	  RETURN v_set_id;
15:15:35 130  END;
15:15:35 131  --------------------------------------------------------------------------------
15:15:35 132  FUNCTION RETREIVE_TRANSFER_SET
15:15:35 133  	  (
15:15:35 134  	      in_set_id core_owner.polling_sync.set_id%type)
15:15:35 135  	  RETURN sys_refcursor
15:15:35 136  IS
15:15:35 137  	  v_refcursor sys_refcursor;
15:15:35 138  BEGIN
15:15:35 139  	  UPDATE
15:15:35 140  	      core_owner.polling_sync ps
15:15:35 141  	  SET
15:15:35 142  	      ps.last_send_date = sysdate,
15:15:35 143  	      ps.num_calls = ps.num_calls + 1
15:15:35 144  	  WHERE
15:15:35 145  	      ps.set_id = in_set_id ;
15:15:35 146  	  OPEN v_refcursor FOR
15:15:35 147  	  SELECT
15:15:35 148  	      ps.set_id,
15:15:35 149  	      ps.group_id,
15:15:35 150  	      ps.event_type,
15:15:35 151  	      ps.event_date
15:15:35 152  	  FROM
15:15:35 153  	      core_owner.polling_sync ps
15:15:35 154  	  WHERE
15:15:35 155  	      ps.set_id = in_set_id
15:15:35 156  	  AND ps.group_id IS NOT NULL ;
15:15:35 157  	  RETURN v_refcursor;
15:15:35 158  END;
15:15:35 159  --------------------------------------------------------------------------------
15:15:35 160  PROCEDURE GET_TRANSFER_SET
15:15:35 161  	  (
15:15:35 162  	      in_set_maximum NUMBER,
15:15:35 163  	      in_max_retries NUMBER,
15:15:35 164  	      out_refcursor OUT sys_refcursor)
15:15:35 165  IS
15:15:35 166  	  v_set_id core_owner.polling_sync.set_id%type;
15:15:35 167  BEGIN
15:15:35 168  	  /* Look for previously sent but unconfirmed sets and
15:15:35 169  	  send again until max_retries calls */
15:15:35 170  	  FOR x IN
15:15:35 171  	  (
15:15:35 172  	      SELECT
15:15:35 173  		  ps.set_id,
15:15:35 174  		  COUNT( *) set_size
15:15:35 175  	      FROM
15:15:35 176  		  core_owner.polling_sync ps
15:15:35 177  	      WHERE
15:15:35 178  		  ps.event_date > sysdate - 14
15:15:35 179  	      AND ps.confirm_date IS NULL
15:15:35 180  	      AND ps.last_send_date IS NOT NULL
15:15:35 181  	      AND ps.num_calls < in_max_retries
15:15:35 182  	      GROUP BY
15:15:35 183  		  ps.set_id
15:15:35 184  	      ORDER BY
15:15:35 185  		  ps.set_id
15:15:35 186  	  )
15:15:35 187  	  LOOP
15:15:35 188  	      out_refcursor := Retreive_Transfer_Set(x.set_id) ;
15:15:35 189  	      RETURN;
15:15:35 190  	  END LOOP;
15:15:35 191  	  v_set_id := Create_New_Transfer_Set(in_set_maximum) ;
15:15:35 192  	  out_refcursor := Retreive_Transfer_Set(v_set_id) ;
15:15:35 193  	  RETURN;
15:15:35 194  END;
15:15:35 195  --------------------------------------------------------------------------------
15:15:35 196  PROCEDURE CONFIRM_TRANSFER_SET
15:15:35 197  	  (
15:15:35 198  	      in_set_id core_owner.polling_sync.set_id%type)
15:15:35 199  IS
15:15:35 200  	  v_unconfirmable EXCEPTION;
15:15:35 201  BEGIN
15:15:35 202  	  UPDATE
15:15:35 203  	      core_owner.polling_sync ps
15:15:35 204  	  SET
15:15:35 205  	      ps.confirm_date = systimestamp
15:15:35 206  	  WHERE
15:15:35 207  	      ps.set_id = in_set_id
15:15:35 208  	  AND ps.confirm_date IS NULL ;
15:15:35 209  	  IF(sql%rowcount < 1) THEN
15:15:35 210  	      raise v_unconfirmable;
15:15:35 211  	  END IF;
15:15:35 212  END;
15:15:35 213  PROCEDURE SET_LAST_RUN(ts in timestamp)
15:15:35 214  IS
15:15:35 215  BEGIN
15:15:35 216  	  UPDATE POLLING_SYNC_LASTRUN
15:15:35 217  	  SET last_run = current_timestamp;
15:15:35 218  	  IF ( sql%rowcount = 0 )
15:15:35 219  	  THEN
15:15:35 220  	    INSERT INTO POLLING_SYNC_LASTRUN VALUES (ts);
15:15:35 221  	  END if;
15:15:35 222  	  COMMIT;
15:15:35 223  END;
15:15:35 224  PROCEDURE GET_LAST_RUN(ts out timestamp)
15:15:35 225  IS
15:15:35 226  BEGIN
15:15:35 227  	  SELECT LAST_RUN INTO ts
15:15:35 228  	  FROM POLLING_SYNC_LASTRUN
15:15:35 229  	  WHERE ROWNUM < 2;
15:15:35 230  EXCEPTION
15:15:35 231  	WHEN NO_DATA_FOUND
15:15:35 232  	THEN
15:15:35 233  	  ts := current_timestamp;
15:15:35 234  END;
15:15:35 235  END PROCS_POLLING_SYNC;
