15:15:35 151  	END;
15:15:35 152  
15:15:35 153  	var_lock_end_date := var_date + ( in_seconds_num * one_second_interval );
15:15:35 154  
15:15:35 155  	BEGIN
15:15:35 156  
15:15:35 157  	  INSERT INTO ACCOUNT_LOCK (
15:15:35 158  	    ACCOUNT_ID,
15:15:35 159  	    LOCK_KEY,
15:15:35 160  	    END_DATE,
15:15:35 161  	    CREATED_BY,
15:15:35 162  	    REASON
15:15:35 163  	  ) VALUES (
