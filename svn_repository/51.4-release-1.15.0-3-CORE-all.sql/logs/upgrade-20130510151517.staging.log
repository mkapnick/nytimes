15:15:35 499  FUNCTION CALCULATE_OFFER_AMOUNT (
15:15:35 500  	in_offer_id IN NUMBER
15:15:35 501  ) RETURN NUMBER AS
15:15:35 502  -- VARIABLES
15:15:35 503  SPROC_NAME    CONSTANT VARCHAR2(22) := 'CALCULATE_OFFER_AMOUNT';
15:15:35 504  temp_offer_id NUMBER;
15:15:35 505  
15:15:35 506  var_product_offering_set	    SYS_REFCURSOR;
15:15:35 507  var_product_offering_id	    NUMBER;
15:15:35 508  var_product_offering_price    NUMBER(10,6);
15:15:35 509  var_product_offering_t_amount NUMBER(10,6);
15:15:35 510  var_product_offering_quantity NUMBER;
15:15:35 511  
15:15:35 512  var_total_amount NUMBER(10,6);
15:15:35 513  var_final_amount NUMBER(10,2);
15:15:35 514  
15:15:35 515  var_percent_discount NUMBER(10,2);
15:15:35 516  var_fixed_discount NUMBER(10,6);
15:15:35 517  
15:15:35 518  -- EXCEPTIONS
15:15:35 519  BAD_OFFER_ID EXCEPTION;
15:15:35 520  BEGIN
15:15:35 521  
15:15:35 522  	BEGIN
15:15:35 523  	  SELECT
15:15:35 524  	    OFFER.ID into temp_offer_id
15:15:35 525  	  FROM
15:15:35 526  	    OFFER
15:15:35 527  	  WHERE
15:15:35 528  	    OFFER.ID = in_offer_id;
15:15:35 529  	  EXCEPTION
15:15:35 530  	    WHEN NO_DATA_FOUND THEN
15:15:35 531  	      RAISE BAD_OFFER_ID;
15:15:35 532  	END;
15:15:35 533  
15:15:35 534  	OPEN var_product_offering_set FOR
15:15:35 535  	SELECT
15:15:35 536  	  OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID,
15:15:35 537  	  PRODUCT_OFFERING.UNIT_PRICE,
15:15:35 538  	  PRODUCT_OFFERING.QUANTITY
15:15:35 539  	FROM
15:15:35 540  	  OFFER_PRODUCT_OFFERING
15:15:35 541  	  INNER JOIN PRODUCT_OFFERING ON OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = PRODUCT_OFFERING.ID
15:15:35 542  	WHERE
15:15:35 543  	  OFFER_PRODUCT_OFFERING.OFFER_ID = in_offer_id;
15:15:35 544  
15:15:35 545  	var_total_amount := 0;
15:15:35 546  
15:15:35 547  	LOOP
15:15:35 548  	  FETCH var_product_offering_set into
15:15:35 549  	    var_product_offering_id,
15:15:35 550  	    var_product_offering_price,
15:15:35 551  	    var_product_offering_quantity;
15:15:35 552  	  EXIT WHEN var_product_offering_set%NOTFOUND;
15:15:35 553  
15:15:35 554  	  SELECT
15:15:35 555  	    SUM(DISCOUNT.FIXED_AMOUNT) into var_fixed_discount
15:15:35 556  	  FROM
15:15:35 557  	    DISCOUNT_PRODUCT_OFFERING
15:15:35 558  	    INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
15:15:35 559  	  WHERE
15:15:35 560  	    DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
15:15:35 561  	    AND DISCOUNT.FIXED_AMOUNT IS NOT NULL;
15:15:35 562  
15:15:35 563  	  SELECT
15:15:35 564  	    SUM(DISCOUNT.PERCENT_AMOUNT) into var_percent_discount
15:15:35 565  	  FROM
15:15:35 566  	    DISCOUNT_PRODUCT_OFFERING
15:15:35 567  	    INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
15:15:35 568  	  WHERE
15:15:35 569  	    DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
15:15:35 570  	    AND DISCOUNT.PERCENT_AMOUNT IS NOT NULL;
15:15:35 571  
15:15:35 572  	  var_product_offering_t_amount := var_product_offering_price * var_product_offering_quantity;
15:15:35 573  
15:15:35 574  	  IF var_percent_discount IS NOT NULL THEN
15:15:35 575  	    var_product_offering_t_amount := var_product_offering_t_amount - ( var_product_offering_t_amount * var_percent_discount / 100 );
15:15:35 576  	  END IF;
15:15:35 577  
15:15:35 578  	  IF var_fixed_discount IS NOT NULL THEN
15:15:35 579  	    var_product_offering_t_amount := var_product_offering_t_amount - var_fixed_discount;
15:15:35 580  	  END IF;
15:15:35 581  
15:15:35 582  	  var_total_amount := var_total_amount + var_product_offering_t_amount;
15:15:35 583  	END LOOP;
15:15:35 584  	var_final_amount := var_total_amount;
15:15:35 585  	RETURN var_final_amount;
15:15:35 586  
15:15:35 587  EXCEPTION
15:15:35 588  WHEN BAD_OFFER_ID THEN
15:15:35 589  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:35 590  	  SPROC_NAME, 'No such offer');
15:15:35 591  WHEN OTHERS THEN
15:15:35 592  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:35 593  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:15:35 594  END CALCULATE_OFFER_AMOUNT;
