15:15:36  84  PROCEDURE GET_PENDING_INVOICES (
15:15:36  85  /*
15:15:36  86  Throws exceptions:
15:15:36  87  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
15:15:36  88  */
15:15:36  89  	out_result_set	     OUT SYS_REFCURSOR,
15:15:36  90  	in_row_number	     IN NUMBER DEFAULT NULL
15:15:36  91  ) AS
15:15:36  92  SPROC_NAME CONSTANT VARCHAR2(20) := 'GET_PENDING_INVOICES';
15:15:36  93  -- COMSTANTS
15:15:36  94  DEFAULT_ROW_NUMBER CONSTANT NUMBER := 1;
15:15:36  95  -- VARIABLES
15:15:36  96  var_row_number NUMBER;
15:15:36  97  BEGIN
15:15:36  98  
15:15:36  99  	IF in_row_number IS NULL THEN
15:15:36 100  	  var_row_number := DEFAULT_ROW_NUMBER;
15:15:36 101  	ELSE
15:15:36 102  	  var_row_number := in_row_number;
15:15:36 103  	END IF;
15:15:36 104  
15:15:36 105  	-- Invoices with one or more payments(charges) with transaction status PENDING
15:15:36 106  	OPEN out_result_set FOR
15:15:36 107  SELECT * FROM
15:15:36 108  (
15:15:36 109  	SELECT
15:15:36 110  	  INVOICE.ID
15:15:36 111  	FROM
15:15:36 112  	  CHARGE
15:15:36 113  	  INNER JOIN INVOICE ON CHARGE.INVOICE_ID = INVOICE.ID
15:15:36 114  	WHERE
15:15:36 115  	  EXISTS(
15:15:36 116  	    SELECT NULL
15:15:36 117  	    FROM TRANSACTION
15:15:36 118  	    WHERE
15:15:36 119  	      TRANSACTION.TRANSACTION_STATUS_ID = GLOBAL_STATUSES_V21.TRANSACTION_PENDING
15:15:36 120  	      AND TRANSACTION.ID = CHARGE.TRANSACTION_ID
15:15:36 121  	      AND TRANSACTION.IS_REFUND != GLOBAL_CONSTANTS_V21.TRUE
15:15:36 122  	      AND TRANSACTION.TRANSACTION_AMOUNT >= 0
15:15:36 123  	  )
15:15:36 124  	  AND
15:15:36 125  	  NOT EXISTS(
15:15:36 126  	    SELECT NULL
15:15:36 127  	    FROM PROCESS_RETRY_THROTTLE
15:15:36 128  	    WHERE PROCESS_NAME = SPROC_NAME
15:15:36 129  	      AND GENERIC_ID = INVOICE.ID
15:15:36 130  	  )
15:15:36 131  	  AND ROWNUM <= var_row_number*10
15:15:36 132  	  ORDER BY dbms_random.value
15:15:36 133  ) WHERE
15:15:36 134  	  ROWNUM <= var_row_number;
15:15:36 135  
15:15:36 136  EXCEPTION
15:15:36 137  WHEN OTHERS THEN
15:15:36 138  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 139  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:15:36 140  END GET_PENDING_INVOICES;
