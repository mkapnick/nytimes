15:15:36 1383  	     in_transaction_id	      => f_transaction_to_close.ID,
15:15:36 1384  	     in_updated_by	      => in_updated_by,
15:15:36 1385  	     in_transaction_status_id => GLOBAL_STATUSES_V21.TRANSACTION_CLOSED
15:15:36 1386  	   );
15:15:36 1387  	 END LOOP;
15:15:36 1388  
15:15:36 1389  EXCEPTION
15:15:36 1390  WHEN BAD_INVOICE_ID THEN
15:15:36 1391  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 1392  	   SPROC_NAME, 'No such invoice');
