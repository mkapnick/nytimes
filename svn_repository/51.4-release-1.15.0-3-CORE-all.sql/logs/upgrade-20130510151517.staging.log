15:15:36 845  PROCEDURE GET_GRACE_LICE_FOR_FINAL_TRANS (
15:15:36 846  	in_days_before_close	 IN NUMBER,
15:15:36 847  	in_num_licenses_to_fetch IN NUMBER,
15:15:36 848  	out_result_set		 OUT SYS_REFCURSOR
15:15:36 849  ) AS
15:15:36 850  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_GRACE_LICE_FOR_FINAL_TRANS';
15:15:36 851  BEGIN
15:15:36 852  	OPEN out_result_set FOR
15:15:36 853  	SELECT
15:15:36 854  	    *
15:15:36 855  	FROM
15:15:36 856  	    (
15:15:36 857  		SELECT
15:15:36 858  		    l.id
15:15:36 859  		FROM
15:15:36 860  		    license l
15:15:36 861  		JOIN
15:15:36 862  		    invoice i
15:15:36 863  		ON
15:15:36 864  		    l.invoice_id = i.id
15:15:36 865  		WHERE
15:15:36 866  		    i.invoice_status_id = GLOBAL_STATUSES_V21.INVOICE_OPEN
15:15:36 867  		AND l.license_status_id = GLOBAL_STATUSES_V21.LICENSE_IN_GRACE_PERIOD
15:15:36 868  		AND SYSDATE + in_days_before_close >= l.grace_end_date
15:15:36 869  		AND NOT EXISTS
15:15:36 870  		    (
15:15:36 871  			SELECT
15:15:36 872  			    1
15:15:36 873  			FROM
15:15:36 874  			    charge c
15:15:36 875  			WHERE
15:15:36 876  			    c.invoice_id = i.id
15:15:36 877  			AND c.charge_status_id = GLOBAL_STATUSES_V21.CHARGE_OPENED)
15:15:36 878  		AND rownum <= in_num_licenses_to_fetch * 10
15:15:36 879  		ORDER BY
15:15:36 880  		    dbms_random.value)
15:15:36 881  	WHERE
15:15:36 882  	    rownum <= in_num_licenses_to_fetch;
15:15:36 883  END GET_GRACE_LICE_FOR_FINAL_TRANS;
