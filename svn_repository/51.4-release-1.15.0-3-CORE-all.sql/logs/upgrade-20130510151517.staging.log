15:15:36  61  	  WHERE
15:15:36  62  	    OFFER.ID = in_offer_id;
15:15:36  63  	  EXCEPTION
15:15:36  64  	  WHEN NO_DATA_FOUND THEN
15:15:36  65  	    RAISE BAD_OFFER_ID;
15:15:36  66  	END;
15:15:36  67  
15:15:36  68  	-- Check that subscription exists
15:15:36  69  	BEGIN
15:15:36  70  	  SELECT
15:15:36  71  	    SUBSCRIPTION.ID into temp_subscription_id
15:15:36  72  	  FROM
15:15:36  73  	    SUBSCRIPTION
15:15:36  74  	  WHERE
15:15:36  75  	    SUBSCRIPTION.ID = in_subscription_id;
15:15:36  76  	  EXCEPTION
15:15:36  77  	  WHEN NO_DATA_FOUND THEN
15:15:36  78  	    RAISE BAD_SUBSCRIPTION_ID;
15:15:36  79  	END;
15:15:36  80  
15:15:36  81  	-- Check that invoice exists
15:15:36  82  	BEGIN
15:15:36  83  	  SELECT
15:15:36  84  	    INVOICE.ID into temp_invoice_id
15:15:36  85  	  FROM
15:15:36  86  	    INVOICE
15:15:36  87  	  WHERE
15:15:36  88  	    INVOICE.ID = in_invoice_id;
15:15:36  89  	  EXCEPTION
15:15:36  90  	  WHEN NO_DATA_FOUND THEN
15:15:36  91  	    RAISE BAD_INVOICE_ID;
15:15:36  92  	END;
15:15:36  93  
15:15:36  94  	-- convert offer duration into intervals
15:15:36  95  	BEGIN
15:15:36  96  	  PROCS_COMMON_V21.ISO8601DURATION_TO_INTERVALS(
15:15:36  97  	    var_offer_duration,
15:15:36  98  	    var_offer_years,
15:15:36  99  	    var_offer_months,
15:15:36 100  	    var_offer_days);
15:15:36 101  	  var_offer_ym_interval := var_offer_years||'-'||var_offer_months;
15:15:36 102  	  var_offer_ds_interval := var_offer_days||' 0:0:0';
15:15:36 103  	  EXCEPTION
15:15:36 104  	    WHEN OTHERS THEN
15:15:36 105  	      EXCEPTION_MESSAGE := SQLERRM;
15:15:36 106  	      RAISE BAD_OFFER_DURATION;
15:15:36 107  	END;
15:15:36 108  
15:15:36 109  	-- insert new row into license table
15:15:36 110  	PROCS_LICENSE_CRU_V21.CREATE_LICENSE(
15:15:36 111  	  out_license_id	      => var_new_license_id,
15:15:36 112  	  in_license_status_id	      => in_status_id,
15:15:36 113  	  in_needs_entitlements       => in_needs_entitlements,
15:15:36 114  	  in_start_date 	      => in_start_date,
15:15:36 115  	  in_offer_id		      => in_offer_id,
15:15:36 116  	  in_subscription_id	      => in_subscription_id,
15:15:36 117  	  in_invoice_id 	      => in_invoice_id,
15:15:36 118  	  in_end_date		      => NVL(in_end_date, in_start_date + var_offer_ym_interval + var_offer_ds_interval),
15:15:36 119  	  in_created_by 	      => in_created_by,
15:15:36 120  	  in_is_extension	      => in_is_extension,
15:15:36 121  	  in_current_offer_index      => in_current_offer_index,
15:15:36 122  	  in_current_offer_recurr_num => in_current_offer_recurr_num
15:15:36 123  	);
15:15:36 124  
15:15:36 125  	out_license_id := var_new_license_id;
15:15:36 126  
15:15:36 127  EXCEPTION
15:15:36 128  WHEN BAD_OFFER_DURATION THEN
15:15:36 129  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:36 130  	  SPROC_NAME, 'Bad offer duration format', EXCEPTION_MESSAGE);
15:15:36 131  WHEN BAD_OFFER_ID THEN
15:15:36 132  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 133  	  SPROC_NAME, 'No such offer');
15:15:36 134  WHEN BAD_SUBSCRIPTION_ID THEN
15:15:36 135  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 136  	  SPROC_NAME, 'No such subscription');
15:15:36 137  WHEN BAD_INVOICE_ID THEN
15:15:36 138  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 139  	  SPROC_NAME, 'No such invoice');
15:15:36 140  WHEN OTHERS THEN
15:15:36 141  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 142  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:15:36 143  END;
