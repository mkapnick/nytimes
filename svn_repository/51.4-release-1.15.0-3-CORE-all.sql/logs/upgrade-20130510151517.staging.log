15:15:37 495  	END;
15:15:37 496  
15:15:37 497  	-- Get group id
15:15:37 498  	SELECT
15:15:37 499  	  ACCOUNT.GROUP_ID into var_group_id
15:15:37 500  	FROM
15:15:37 501  	  ACCOUNT
15:15:37 502  	WHERE
15:15:37 503  	  ACCOUNT.ID = var_account_id;
15:15:37 504  
15:15:37 505  	-- Card should not be disabled
15:15:37 506  	IF var_paypal_status_id = GLOBAL_STATUSES_V21.PAYPAL_INACTIVE THEN
15:15:37 507  	  RAISE PAYPAL_ALREADY_INACTIVE;
15:15:37 508  	END IF;
15:15:37 509  
15:15:37 510  	-- Looking for pending transactions associated with given credit card
15:15:37 511  	SELECT
15:15:37 512  	  COUNT(*) into var_pending_invoices_num
15:15:37 513  	FROM
15:15:37 514  	  CHARGE
15:15:37 515  	  INNER JOIN TRANSACTION ON CHARGE.TRANSACTION_ID = TRANSACTION.ID
15:15:37 516  	WHERE
15:15:37 517  	  CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V21.INSTRUMENT_PAYPAL
15:15:37 518  	  AND CHARGE.INSTRUMENT_ID = in_paypal_id
15:15:37 519  	  AND TRANSACTION.TRANSACTION_STATUS_ID = GLOBAL_STATUSES_V21.TRANSACTION_PENDING;
15:15:37 520  
15:15:37 521  	IF var_pending_invoices_num > 0 THEN
15:15:37 522  	  RAISE PENDING_TRANSACTIONS_FOUND;
15:15:37 523  	END IF;
15:15:37 524  
15:15:37 525  	-- Getting current default financial instrument
15:15:37 526  	BEGIN
15:15:37 527  	  GET_DEF_FINANCIAL_INSTRUMENT(
15:15:37 528  	    in_group_id 	   => var_group_id,
15:15:37 529  	    out_instrument_type_id => current_def_instrument_type_id,
15:15:37 530  	    out_instrument_id	   => current_def_instrument_id
15:15:37 531  	  );
15:15:37 532  	  EXCEPTION
15:15:37 533  	    WHEN OTHERS THEN
15:15:37 534  	      EXCEPTION_MESSAGE := SQLERRM;
15:15:37 535  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
15:15:37 536  	END;
15:15:37 537  
15:15:37 538  	-- Checking that credit card is not default
15:15:37 539  	IF current_def_instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_PAYPAL
15:15:37 540  	  AND current_def_instrument_id = in_paypal_id THEN
15:15:37 541  	  BEGIN
15:15:37 542  	    DEL_DEF_FINANCIAL_INSTRUMENT(
15:15:37 543  	      in_group_id => var_group_id
15:15:37 544  	    );
15:15:37 545  	  EXCEPTION
15:15:37 546  	    WHEN OTHERS THEN
15:15:37 547  	      EXCEPTION_MESSAGE := SQLERRM;
15:15:37 548  	      RAISE CAN_NOT_DEL_DEF_FINANCIAL;
15:15:37 549  	  END;
15:15:37 550  	END IF;
15:15:37 551  
15:15:37 552  	PROCS_FIN_INSTRUMENTS_V21.UPDATE_PAYPAL_STATUS(
15:15:37 553  	  in_paypal_id	      => in_paypal_id,
15:15:37 554  	  in_updated_by       => in_updated_by,
15:15:37 555  	  in_paypal_status_id => GLOBAL_STATUSES_V21.PAYPAL_INACTIVE
15:15:37 556  	);
15:15:37 557  
15:15:37 558  EXCEPTION
15:15:37 559  WHEN BAD_PAYPAL_ID THEN
15:15:37 560  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:37 561  	  SPROC_NAME, 'No such paypal');
15:15:37 562  WHEN PAYPAL_ALREADY_INACTIVE THEN
15:15:37 563  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
15:15:37 564  	  SPROC_NAME, 'Paypal already inactive');
15:15:37 565  WHEN PENDING_TRANSACTIONS_FOUND THEN
15:15:37 566  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
15:15:37 567  	  SPROC_NAME, 'Account has pending charge which are using this paypal');
15:15:37 568  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
15:15:37 569  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:37 570  	  SPROC_NAME, 'Could not get current default financial instrument', EXCEPTION_MESSAGE);
15:15:37 571  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
15:15:37 572  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:37 573  	  SPROC_NAME, 'Could not delete information about default financial instrument from account', EXCEPTION_MESSAGE);
15:15:37 574  WHEN OTHERS THEN
15:15:37 575  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:37 576  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:15:37 577  END DISABLE_PAYPAL;
15:15:37 578  
15:15:37 579  /******************************************************************************/
15:15:37 580  
15:15:37 581  PROCEDURE UPDATE_CREDIT_CARD (
15:15:37 582  /*
15:15:37 583  Throws exceptions:
15:15:37 584  APP_EXCEPTION_CODES_V21.NOT_FOUND
15:15:37 585  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
15:15:37 586  */
15:15:37 587  	in_credit_card_id	  IN NUMBER,
15:15:37 588  	in_updated_by		  IN VARCHAR2,
15:15:37 589  	in_instrument_name	  IN VARCHAR2,
15:15:37 590  	in_is_default		  IN NUMBER
15:15:37 591  ) AS
15:15:37 592  SPROC_NAME CONSTANT VARCHAR2(18) := 'UPDATE_CREDIT_CARD';
15:15:37 593  -- VARIABLES
15:15:37 594  var_account_id NUMBER;
15:15:37 595  var_group_id   NUMBER;
15:15:37 596  temp_cc_rownum NUMBER;
15:15:37 597  current_def_instrument_type_id NUMBER;
15:15:37 598  current_def_instrument_id      NUMBER;
15:15:37 599  -- EXCEPTION
15:15:37 600  CAN_NOT_SET_DEF_FINANCIAL  EXCEPTION;
15:15:37 601  BAD_IS_DEFAULT_VALUE	 EXCEPTION;
15:15:37 602  CAN_NOT_GET_DEF_FINANCIAL  EXCEPTION;
15:15:37 603  CAN_NOT_DEL_DEF_FINANCIAL  EXCEPTION;
15:15:37 604  EXCEPTION_MESSAGE 	 VARCHAR2(1024);
15:15:37 605  BEGIN
15:15:37 606  
15:15:37 607  	-- Get account id
15:15:37 608  	SELECT
15:15:37 609  	  CREDIT_CARD.ACCOUNT_ID
15:15:37 610  	  into
15:15:37 611  	  var_account_id
15:15:37 612  	FROM
15:15:37 613  	  CREDIT_CARD
15:15:37 614  	WHERE
15:15:37 615  	  CREDIT_CARD.ID = in_credit_card_id;
15:15:37 616  
15:15:37 617  	-- Get group id
15:15:37 618  	SELECT
15:15:37 619  	  ACCOUNT.GROUP_ID into var_group_id
15:15:37 620  	FROM
15:15:37 621  	  ACCOUNT
15:15:37 622  	WHERE
15:15:37 623  	  ACCOUNT.ID = var_account_id;
15:15:37 624  
15:15:37 625  	-- Check that passed data is correct
15:15:37 626  	IF in_is_default != GLOBAL_CONSTANTS_V21.TRUE
15:15:37 627  	  AND in_is_default != GLOBAL_CONSTANTS_V21.FALSE
15:15:37 628  	  AND in_is_default IS NOT NULL THEN
15:15:37 629  	  RAISE BAD_IS_DEFAULT_VALUE;
15:15:37 630  	END IF;
15:15:37 631  
15:15:37 632  	-- Update credit card
15:15:37 633  	IF in_instrument_name IS NOT NULL THEN
15:15:37 634  	  PROCS_FIN_INSTRUMENTS_CRU_V21.UPDATE_CREDIT_CARD(
15:15:37 635  	    in_credit_card_id  => in_credit_card_id,
15:15:37 636  	    in_updated_by      => in_updated_by,
15:15:37 637  	    in_instrument_name => in_instrument_name
15:15:37 638  	  );
15:15:37 639  	END IF;
15:15:37 640  
15:15:37 641  	-- Set default financial instrument
15:15:37 642  	IF in_is_default = GLOBAL_CONSTANTS_V21.TRUE THEN
15:15:37 643  	  BEGIN
15:15:37 644  	    PROCS_FIN_INSTRUMENTS_V21.SET_DEF_FINANCIAL_INSTRUMENT(
15:15:37 645  	      in_group_id	    => var_group_id,
15:15:37 646  	      in_instrument_type_id => GLOBAL_ENUMS_V21.INSTRUMENT_CREDIT_CARD,
15:15:37 647  	      in_instrument_id	    => in_credit_card_id,
15:15:37 648  	      in_updated_by	    => in_updated_by
15:15:37 649  	    );
15:15:37 650  	    EXCEPTION
15:15:37 651  	      WHEN OTHERS THEN
15:15:37 652  		EXCEPTION_MESSAGE := SQLERRM;
15:15:37 653  		RAISE CAN_NOT_SET_DEF_FINANCIAL;
15:15:37 654  	  END;
15:15:37 655  	END IF;
15:15:37 656  
15:15:37 657  	-- Set default financial instrument
15:15:37 658  	IF in_is_default = GLOBAL_CONSTANTS_V21.FALSE THEN
15:15:37 659  	  BEGIN
15:15:37 660  	    GET_DEF_FINANCIAL_INSTRUMENT(
15:15:37 661  	      in_group_id	     => var_group_id,
15:15:37 662  	      out_instrument_type_id => current_def_instrument_type_id,
15:15:37 663  	      out_instrument_id      => current_def_instrument_id
15:15:37 664  	    );
15:15:37 665  	  EXCEPTION
15:15:37 666  	    WHEN OTHERS THEN
15:15:37 667  	      EXCEPTION_MESSAGE := SQLERRM;
15:15:37 668  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
15:15:37 669  	  END;
15:15:37 670  	  IF current_def_instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_CREDIT_CARD
15:15:37 671  	    AND current_def_instrument_id = in_credit_card_id THEN
15:15:37 672  	    BEGIN
15:15:37 673  	      DEL_DEF_FINANCIAL_INSTRUMENT(
15:15:37 674  		in_group_id => var_group_id
15:15:37 675  	      );
15:15:37 676  	      EXCEPTION
15:15:37 677  		WHEN OTHERS THEN
15:15:37 678  		  EXCEPTION_MESSAGE := SQLERRM;
15:15:37 679  		  RAISE CAN_NOT_DEL_DEF_FINANCIAL;
15:15:37 680  	    END;
15:15:37 681  	  END IF;
15:15:37 682  	END IF;
15:15:37 683  
15:15:37 684  EXCEPTION
15:15:37 685  WHEN NO_DATA_FOUND THEN
15:15:37 686  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:37 687  	  SPROC_NAME, 'No such credit card');
15:15:37 688  WHEN CAN_NOT_SET_DEF_FINANCIAL THEN
15:15:37 689  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:37 690  	  SPROC_NAME, 'Could not set default financial instrument for account', EXCEPTION_MESSAGE);
15:15:37 691  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
15:15:37 692  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:37 693  	  SPROC_NAME, 'Could not get default financial instrument for account', EXCEPTION_MESSAGE);
15:15:37 694  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
15:15:37 695  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:37 696  	  SPROC_NAME, 'Could not delete information about default financial instrument', EXCEPTION_MESSAGE);
15:15:37 697  WHEN OTHERS THEN
15:15:37 698  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:37 699  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:15:37 700  END UPDATE_CREDIT_CARD;
