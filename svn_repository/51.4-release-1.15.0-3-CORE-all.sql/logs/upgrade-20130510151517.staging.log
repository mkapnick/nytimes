15:15:37 2702  
15:15:37 2703  PROCEDURE SWITCH_FINANCIAL_INSTRUMENT (
15:15:37 2704  	 in_old_fin_instrument_id   IN NUMBER,
15:15:37 2705  	 in_old_fin_instrument_type IN NUMBER,
15:15:37 2706  	 in_new_fin_instrument_id   IN NUMBER,
15:15:37 2707  	 in_new_fin_instrument_type IN NUMBER,
15:15:37 2708  	 in_updated_by		    IN VARCHAR2
15:15:37 2709  ) AS
15:15:37 2710  SPROC_NAME CONSTANT VARCHAR2(27) := 'SWITCH_FINANCIAL_INSTRUMENT';
15:15:37 2711  -- variables
15:15:37 2712  temp_out_transaction_id NUMBER;
15:15:37 2713  temp_out_charge_id      NUMBER;
15:15:37 2714  var_accounts_count      NUMBER;
15:15:37 2715  var_transaction_type_old  "TRANSACTION".TRANSACTION_TYPE_CODE%TYPE;
15:15:37 2716  var_transaction_type	 "TRANSACTION".TRANSACTION_TYPE_CODE%TYPE;
15:15:37 2717  -- EXCEPTIONS
15:15:37 2718  BAD_OLD_CC   EXCEPTION;
15:15:37 2719  BAD_OLD_PP   EXCEPTION;
15:15:37 2720  BAD_OLD_TYPE EXCEPTION;
15:15:37 2721  BAD_NEW_CC   EXCEPTION;
15:15:37 2722  BAD_NEW_PP   EXCEPTION;
15:15:37 2723  BAD_NEW_TYPE EXCEPTION;
15:15:37 2724  DIFFERENT_OWNERS EXCEPTION;
15:15:37 2725  BEGIN
15:15:37 2726  
15:15:37 2727  	 IF in_old_fin_instrument_type = GLOBAL_ENUMS_V21.INSTRUMENT_CREDIT_CARD THEN
15:15:37 2728  	   IF PROCS_FIN_INSTRUMENTS_V21.IS_CREDIT_CARD_EXISTS(in_old_fin_instrument_id) = GLOBAL_CONSTANTS_V21.FALSE THEN
15:15:37 2729  	     -- throw exception: bad old credit card
15:15:37 2730  	     RAISE BAD_OLD_CC;
15:15:37 2731  	   END IF;
15:15:37 2732  	 ELSIF in_old_fin_instrument_type = GLOBAL_ENUMS_V21.INSTRUMENT_PAYPAL THEN
15:15:37 2733  	   IF PROCS_FIN_INSTRUMENTS_V21.IS_PAYPAL_EXISTS(in_old_fin_instrument_id) = GLOBAL_CONSTANTS_V21.FALSE THEN
15:15:37 2734  	     -- throw exception: bad old paypal
15:15:37 2735  	     RAISE BAD_OLD_PP;
15:15:37 2736  	   END IF;
15:15:37 2737  	 ELSE
15:15:37 2738  	   -- throw exception: bad instrument type
15:15:37 2739  	   RAISE BAD_OLD_TYPE;
15:15:37 2740  	 END IF;
15:15:37 2741  
15:15:37 2742  	 IF in_new_fin_instrument_type = GLOBAL_ENUMS_V21.INSTRUMENT_CREDIT_CARD THEN
15:15:37 2743  	   IF PROCS_FIN_INSTRUMENTS_V21.IS_CREDIT_CARD_EXISTS(in_new_fin_instrument_id) = GLOBAL_CONSTANTS_V21.FALSE THEN
15:15:37 2744  	     -- throw exception: bad new credit card
15:15:37 2745  	     RAISE BAD_NEW_CC;
15:15:37 2746  	   END IF;
15:15:37 2747  	 ELSIF in_new_fin_instrument_type = GLOBAL_ENUMS_V21.INSTRUMENT_PAYPAL THEN
15:15:37 2748  	   IF PROCS_FIN_INSTRUMENTS_V21.IS_PAYPAL_EXISTS(in_new_fin_instrument_id) = GLOBAL_CONSTANTS_V21.FALSE THEN
15:15:37 2749  	     -- throw exception: bad new paypal
15:15:37 2750  	     RAISE BAD_NEW_PP;
15:15:37 2751  	   END IF;
15:15:37 2752  	 ELSE
15:15:37 2753  	   -- throw exception: bad new instrument type
15:15:37 2754  	   RAISE BAD_NEW_TYPE;
15:15:37 2755  	 END IF;
15:15:37 2756  
15:15:37 2757  	 -- Check that owner of both instruments - same man
15:15:37 2758  
15:15:37 2759  	 SELECT count(1) into var_accounts_count FROM (
15:15:37 2760  	   SELECT
15:15:37 2761  	     CC.ACCOUNT_ID
15:15:37 2762  	   FROM
15:15:37 2763  	     CREDIT_CARD CC
15:15:37 2764  	   WHERE
15:15:37 2765  	     (
15:15:37 2766  	       CC.ID = in_old_fin_instrument_id
15:15:37 2767  	       AND in_old_fin_instrument_type = GLOBAL_ENUMS_V21.INSTRUMENT_CREDIT_CARD
15:15:37 2768  	     )
15:15:37 2769  	     OR
15:15:37 2770  	     (
15:15:37 2771  	       CC.ID = in_new_fin_instrument_id
15:15:37 2772  	       AND in_new_fin_instrument_type = GLOBAL_ENUMS_V21.INSTRUMENT_CREDIT_CARD
15:15:37 2773  	     )
15:15:37 2774  	   UNION
15:15:37 2775  	   SELECT
