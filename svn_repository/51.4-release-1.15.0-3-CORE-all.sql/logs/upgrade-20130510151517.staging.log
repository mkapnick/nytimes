15:15:36 1756  		     RAISE CAN_NOT_FIND_SUBSCR_OR_GC;
15:15:36 1757  	       END;
15:15:36 1758  	 END;
15:15:36 1759  
15:15:36 1760  	 -- Check account status. It should not to be frozen
15:15:36 1761  	 SELECT
15:15:36 1762  	   ACCOUNT.ACCOUNT_STATUS_ID into var_account_status_id
15:15:36 1763  	 FROM
15:15:36 1764  	   ACCOUNT
