15:15:35  73  	  C.DESCRIPTION,
15:15:35  74  	  1 INHERITED,
15:15:35  75  	  C.SHAREABLE,
15:15:35  76  	  MAX(LEAST(SS.END_DATE, LIC.ENTITLEMENT_END_DATE)) EXPIRES
15:15:35  77  	FROM
15:15:35  78  	  ACCOUNT BORROWER,
15:15:35  79  	  SUBSCRIPTION S,
15:15:35  80  	  LICENSE LIC,
15:15:35  81  	  OFFER_PRODUCT_OFFERING OPO,
