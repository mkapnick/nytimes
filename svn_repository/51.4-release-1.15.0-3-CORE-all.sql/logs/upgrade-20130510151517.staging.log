15:15:37 383  	in_instr_status   IN NUMBER DEFAULT GLOBAL_CONSTANTS_V21.TRUE
15:15:37 384  ) AS
15:15:37 385  SPROC_NAME     CONSTANT VARCHAR2(29) := 'GET_ACCOUNT_GIFT_CERTIFICATES';
15:15:37 386  var_account_id NUMBER;
15:15:37 387  
15:15:37 388  -- Exceptions
15:15:37 389  WRONG_INSTR_EXCEPTION	 EXCEPTION;
15:15:37 390  BEGIN
15:15:37 391  
15:15:37 392  	-- Get account id
15:15:37 393  	SELECT
15:15:37 394  	  ACCOUNT.ID INTO var_account_id
15:15:37 395  	FROM
15:15:37 396  	  ACCOUNT
15:15:37 397  	WHERE
15:15:37 398  	  ACCOUNT.GROUP_ID = in_group_id;
15:15:37 399  
15:15:37 400  	-- Check that incoming data is correct
15:15:37 401  	IF in_instr_status != GLOBAL_CONSTANTS_V21.TRUE AND in_instr_status != GLOBAL_CONSTANTS_V21.FALSE THEN
15:15:37 402  	  RAISE WRONG_INSTR_EXCEPTION;
15:15:37 403  	END IF;
15:15:37 404  
15:15:37 405  	OPEN out_result_gc_set FOR
15:15:37 406  	SELECT
15:15:37 407  	  GIFT_CERTIFICATE.ID,
15:15:37 408  	  GIFT_CERTIFICATE.PURCHASER_GROUP_ID,
15:15:37 409  	  GIFT_CERTIFICATE.PURCHASE_INVOICE_ID,
15:15:37 410  	  GIFT_CERTIFICATE.PURCHASE_DATE,
15:15:37 411  	  GIFT_CERTIFICATE.OFFER_CHAIN_ID,
15:15:37 412  	  GIFT_CERTIFICATE.EXPIRATION_DATE,
15:15:37 413  	  GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID,
15:15:37 414  	  GIFT_CERTIFICATE.CODE,
15:15:37 415  	  GIFT_CERTIFICATE.CREATE_DATE,
15:15:37 416  	  GIFT_CERTIFICATE.CREATED_BY,
15:15:37 417  	  GIFT_CERTIFICATE.UPDATE_DATE,
15:15:37 418  	  GIFT_CERTIFICATE.UPDATED_BY,
15:15:37 419  	  GIFT_CERTIFICATE.RECIPIENT_NAME,
15:15:37 420  	  GIFT_CERTIFICATE.RECIPIENT_EMAIL,
15:15:37 421  	  GIFT_CERTIFICATE.SENDER_NAME,
15:15:37 422  	  GIFT_CERTIFICATE.SENDER_EMAIL,
15:15:37 423  	  GIFT_CERTIFICATE.REDEEMER_GROUP_ID,
15:15:37 424  	  GIFT_CERTIFICATE.REDEMPTION_DATE,
15:15:37 425  	  GIFT_CERTIFICATE.FINALIZED_INVOICE_ID,
15:15:37 426  	  GIFT_CERTIFICATE.GIFT_MESSAGE
15:15:37 427  	FROM
15:15:37 428  	  GIFT_CERTIFICATE
15:15:37 429  	WHERE
15:15:37 430  	  GIFT_CERTIFICATE.PURCHASER_GROUP_ID = in_group_id
15:15:37 431  	  AND (
15:15:37 432  		( in_instr_status = GLOBAL_CONSTANTS_V21.TRUE AND
15:15:37 433  		  (
15:15:37 434  		    GIFT_CERTIFICATE.EXPIRATION_DATE >= current_date
15:15:37 435  		    AND GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID = GLOBAL_STATUSES_V21.GIFT_CERTIFICATE_ACTIVE
15:15:37 436  		  )
15:15:37 437  		)
15:15:37 438  		OR
15:15:37 439  		(
15:15:37 440  		  in_instr_status = GLOBAL_CONSTANTS_V21.FALSE AND
15:15:37 441  		  (
15:15:37 442  		    GIFT_CERTIFICATE.EXPIRATION_DATE < current_date
15:15:37 443  		    OR GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID != GLOBAL_STATUSES_V21.GIFT_CERTIFICATE_ACTIVE
15:15:37 444  		  )
15:15:37 445  		)
15:15:37 446  	      );
15:15:37 447  
15:15:37 448  EXCEPTION
15:15:37 449  WHEN NO_DATA_FOUND THEN
15:15:37 450  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:37 451  	  SPROC_NAME, 'Can not find account with given group id');
15:15:37 452  WHEN WRONG_INSTR_EXCEPTION THEN
15:15:37 453  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INVALID_PARAMETER,
15:15:37 454  	  SPROC_NAME, 'Wrong gift certificate type');
15:15:37 455  WHEN OTHERS THEN
15:15:37 456  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:37 457  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:15:37 458  END GET_ACCOUNT_GIFT_CERTIFICATES;
