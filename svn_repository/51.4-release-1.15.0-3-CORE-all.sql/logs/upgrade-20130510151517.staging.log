15:15:36 1640  /******************************************************************************/
15:15:36 1641  
15:15:36 1642  FUNCTION CALC_SUBSCRIPTION_END_DATE (
15:15:36 1643  /*
15:15:36 1644  APP_EXCEPTION_CODES_V21.NOT_FOUND
15:15:36 1645  APP_EXCEPTION_CODES_V21.INTERNAL_ERROR
15:15:36 1646  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
15:15:36 1647  Returns:
15:15:36 1648  NULL if it is impossible to calculate end date (for example,
15:15:36 1649  	 offer chain includes offer with infinity recurrences number)
15:15:36 1650  DATE else
15:15:36 1651  */
15:15:36 1652  	 in_subscription_id IN NUMBER
15:15:36 1653  ) RETURN DATE AS
15:15:36 1654  -- VARIABLES
15:15:36 1655  SPROC_NAME		     CONSTANT VARCHAR2(26) := 'CALC_SUBSCRIPTION_END_DATE';
15:15:36 1656  last_license_id		     NUMBER;
15:15:36 1657  last_license_end_date	     DATE;
15:15:36 1658  last_license_offer_id	     NUMBER;
15:15:36 1659  last_license_offer_index      NUMBER;
15:15:36 1660  last_license_offer_recurr_num NUMBER;
15:15:36 1661  var_last_license_id	     NUMBER;
15:15:36 1662  var_offer_chain_id	     NUMBER;
15:15:36 1663  var_current_offer_rec_number  NUMBER;
15:15:36 1664  var_next_offers_set	     SYS_REFCURSOR;
15:15:36 1665  var_next_offer_duration	     VARCHAR2(30);
15:15:36 1666  var_next_offer_recur	     NUMBER;
15:15:36 1667  var_infinity_offers_number    NUMBER;
15:15:36 1668  
15:15:36 1669  var_result_date DATE;
15:15:36 1670  
15:15:36 1671  var_ym_interval	INTERVAL YEAR TO MONTH;
15:15:36 1672  var_ds_interval	INTERVAL DAY(3) TO SECOND;
15:15:36 1673  var_offer_years	NUMBER;
15:15:36 1674  var_offer_months NUMBER;
15:15:36 1675  var_offer_days	NUMBER;
15:15:36 1676  
15:15:36 1677  -- EXCEPTIONS
15:15:36 1678  BAD_SUBSCRIPTION_ID	  EXCEPTION;
15:15:36 1679  CAN_NOT_FIND_LAST_LICENSE  EXCEPTION;
15:15:36 1680  CAN_NOT_CALC_OFFER_LENGTH  EXCEPTION;
15:15:36 1681  CAN_NOT_CALC_OFFER_LENGTH2 EXCEPTION;
15:15:36 1682  EXCEPTION_MESSAGE	  VARCHAR2(1024);
15:15:36 1683  BEGIN
15:15:36 1684  
15:15:36 1685  	 BEGIN
15:15:36 1686  	   SELECT
15:15:36 1687  	     SUBSCRIPTION.OFFER_CHAIN_ID into var_offer_chain_id
15:15:36 1688  	   FROM
15:15:36 1689  	     SUBSCRIPTION
15:15:36 1690  	   WHERE
15:15:36 1691  	     SUBSCRIPTION.ID = in_subscription_id;
15:15:36 1692  	   EXCEPTION
15:15:36 1693  	     WHEN NO_DATA_FOUND THEN
15:15:36 1694  	       RAISE BAD_SUBSCRIPTION_ID;
15:15:36 1695  	 END;
15:15:36 1696  
15:15:36 1697  	 BEGIN
15:15:36 1698  	   SELECT
15:15:36 1699  	     ID into var_last_license_id
15:15:36 1700  	   FROM
15:15:36 1701  	     (
15:15:36 1702  	       SELECT
15:15:36 1703  		 LICENSE.ID
15:15:36 1704  	       FROM
15:15:36 1705  		 LICENSE
15:15:36 1706  	       WHERE
15:15:36 1707  		 LICENSE.SUBSCRIPTION_ID = in_subscription_id
15:15:36 1708  	       ORDER BY
15:15:36 1709  		 LICENSE.END_DATE DESC
15:15:36 1710  	     )
15:15:36 1711  	   WHERE
15:15:36 1712  	     ROWNUM <= 1;
15:15:36 1713  	   EXCEPTION
15:15:36 1714  	     WHEN NO_DATA_FOUND THEN
15:15:36 1715  	       RAISE CAN_NOT_FIND_LAST_LICENSE;
15:15:36 1716  	 END;
15:15:36 1717  
15:15:36 1718  	 SELECT
15:15:36 1719  	   COUNT(*) into var_infinity_offers_number
15:15:36 1720  	 FROM
15:15:36 1721  	   OFFER_OFFER_CHAIN
15:15:36 1722  	 WHERE
15:15:36 1723  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
15:15:36 1724  	   AND OFFER_OFFER_CHAIN.NUM_RECURRENCES = GLOBAL_ENUMS_V21.OFFER_REC_INFINITY;
15:15:36 1725  
15:15:36 1726  	 IF var_infinity_offers_number > 0 THEN
15:15:36 1727  	   RETURN NULL;
15:15:36 1728  	 END IF;
15:15:36 1729  
15:15:36 1730  	 BEGIN
15:15:36 1731  	   SELECT
15:15:36 1732  	     LICENSE.ID,
15:15:36 1733  	     LICENSE.END_DATE,
15:15:36 1734  	     LICENSE.CURRENT_OFFER_INDEX,
15:15:36 1735  	     LICENSE.CURRENT_OFFER_RECURR_NUM,
15:15:36 1736  	     LICENSE.OFFER_ID
15:15:36 1737  	     into
15:15:36 1738  	     last_license_id,
15:15:36 1739  	     last_license_end_date,
15:15:36 1740  	     last_license_offer_index,
15:15:36 1741  	     last_license_offer_recurr_num,
15:15:36 1742  	     last_license_offer_id
15:15:36 1743  	   FROM
15:15:36 1744  	     LICENSE
15:15:36 1745  	   WHERE
15:15:36 1746  	     LICENSE.ID = var_last_license_id;
15:15:36 1747  	   EXCEPTION
15:15:36 1748  	     WHEN NO_DATA_FOUND THEN
15:15:36 1749  	       RAISE CAN_NOT_FIND_LAST_LICENSE;
15:15:36 1750  	 END;
15:15:36 1751  
15:15:36 1752  	 var_result_date := last_license_end_date;
15:15:36 1753  
15:15:36 1754  	 -- Find current recurrence number
15:15:36 1755  	 SELECT
15:15:36 1756  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES into var_current_offer_rec_number
15:15:36 1757  	 FROM
15:15:36 1758  	   OFFER_OFFER_CHAIN
15:15:36 1759  	 WHERE
15:15:36 1760  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
15:15:36 1761  	   AND OFFER_OFFER_CHAIN.OFFER_ID = last_license_offer_id
15:15:36 1762  	   AND OFFER_OFFER_CHAIN.INDEX_VALUE = last_license_offer_index;
15:15:36 1763  
15:15:36 1764  	 IF var_current_offer_rec_number > last_license_offer_recurr_num THEN
15:15:36 1765  	   BEGIN
15:15:36 1766  	     PROCS_OFFER_CHAIN_V21.GET_OFFER_LENGTH(
15:15:36 1767  	       last_license_offer_id,
15:15:36 1768  	       var_offer_years,
15:15:36 1769  	       var_offer_months,
15:15:36 1770  	       var_offer_days
15:15:36 1771  	     );
15:15:36 1772  
15:15:36 1773  	     var_ym_interval := var_offer_years||'-'||var_offer_months;
15:15:36 1774  	     var_ds_interval := var_offer_days||' 0:0:0';
15:15:36 1775  
15:15:36 1776  	     var_result_date := var_result_date
15:15:36 1777  	       + ( var_ym_interval * ( var_current_offer_rec_number - last_license_offer_recurr_num ) )
15:15:36 1778  	       + ( var_ds_interval * ( var_current_offer_rec_number - last_license_offer_recurr_num ) );
15:15:36 1779  	     EXCEPTION
15:15:36 1780  	       WHEN OTHERS THEN
15:15:36 1781  		 EXCEPTION_MESSAGE := SQLERRM;
15:15:36 1782  		 RAISE CAN_NOT_CALC_OFFER_LENGTH;
15:15:36 1783  	   END;
15:15:36 1784  	 END IF;
15:15:36 1785  
15:15:36 1786  	 OPEN var_next_offers_set FOR
15:15:36 1787  	 SELECT
15:15:36 1788  	   OFFER.ENTITLEMENT_DURATION,
15:15:36 1789  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES
15:15:36 1790  	 FROM
15:15:36 1791  	   OFFER_OFFER_CHAIN
15:15:36 1792  	   INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
15:15:36 1793  	 WHERE
15:15:36 1794  	   OFFER_OFFER_CHAIN.INDEX_VALUE > last_license_offer_index
15:15:36 1795  	   AND OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id;
15:15:36 1796  
15:15:36 1797  	 LOOP
15:15:36 1798  	   FETCH var_next_offers_set into var_next_offer_duration, var_next_offer_recur;
15:15:36 1799  	   EXIT WHEN var_next_offers_set%NOTFOUND;
15:15:36 1800  	   BEGIN
15:15:36 1801  	     PROCS_COMMON_V21.ISO8601DURATION_TO_INTERVALS(
15:15:36 1802  	       var_next_offer_duration,
15:15:36 1803  	       var_offer_years,
15:15:36 1804  	       var_offer_months,
15:15:36 1805  	       var_offer_days
15:15:36 1806  	     );
15:15:36 1807  
15:15:36 1808  	     var_ym_interval := var_offer_years||'-'||var_offer_months;
15:15:36 1809  	     var_ds_interval := var_offer_days||' 0:0:0';
15:15:36 1810  
15:15:36 1811  	     var_result_date := var_result_date
15:15:36 1812  	       + ( var_ym_interval * var_next_offer_recur )
15:15:36 1813  	       + ( var_ds_interval * var_next_offer_recur );
15:15:36 1814  	     EXCEPTION
15:15:36 1815  	       WHEN OTHERS THEN
15:15:36 1816  		 EXCEPTION_MESSAGE := SQLERRM;
15:15:36 1817  		 RAISE CAN_NOT_CALC_OFFER_LENGTH2;
15:15:36 1818  	   END;
15:15:36 1819  	 END LOOP;
15:15:36 1820  
15:15:36 1821  	 RETURN var_result_date;
15:15:36 1822  
15:15:36 1823  EXCEPTION
15:15:36 1824  WHEN BAD_SUBSCRIPTION_ID THEN
15:15:36 1825  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 1826  	   SPROC_NAME, 'No such subscription');
15:15:36 1827  WHEN CAN_NOT_FIND_LAST_LICENSE THEN
15:15:36 1828  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 1829  	   SPROC_NAME, 'Could not find last license for given subscription');
15:15:36 1830  WHEN CAN_NOT_CALC_OFFER_LENGTH THEN
15:15:36 1831  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:36 1832  	   SPROC_NAME, 'Could not calculate offer length', EXCEPTION_MESSAGE);
15:15:36 1833  WHEN CAN_NOT_CALC_OFFER_LENGTH2 THEN
15:15:36 1834  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:36 1835  	   SPROC_NAME, 'Could not calculate last offer length', EXCEPTION_MESSAGE);
15:15:36 1836  WHEN OTHERS THEN
15:15:36 1837  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 1838  	   SPROC_NAME, 'Unkown error', SQLERRM);
15:15:36 1839  END CALC_SUBSCRIPTION_END_DATE;
