15:15:37 1276  PROCEDURE GET_GROUPS_ID_BY_INVOICE_ID (
15:15:37 1277  	 in_invoice_id IN NUMBER,
15:15:37 1278  	 out_group_ids OUT SYS_REFCURSOR
15:15:37 1279  ) AS
15:15:37 1280  SPROC_NAME CONSTANT VARCHAR2(27) := 'GET_GROUPS_ID_BY_INVOICE_ID';
15:15:37 1281  -- VARIABLES
15:15:37 1282  temp_invoice_id		 NUMBER;
15:15:37 1283  var_subscrib_group_id	 NUMBER;
15:15:37 1284  var_gc_purchaser_group_id NUMBER;
15:15:37 1285  var_gc_redeemer_group_id  NUMBER;
15:15:37 1286  -- EXCEPTIONS
15:15:37 1287  BAD_INVOICE_ID	    EXCEPTION;
15:15:37 1288  CAN_NOT_FIND_ACCOUNT EXCEPTION;
15:15:37 1289  BEGIN
15:15:37 1290  
15:15:37 1291  	 BEGIN
15:15:37 1292  	   SELECT
15:15:37 1293  	     INVOICE.ID into temp_invoice_id
15:15:37 1294  	   FROM
15:15:37 1295  	     INVOICE
15:15:37 1296  	   WHERE
15:15:37 1297  	     INVOICE.ID = in_invoice_id;
15:15:37 1298  	   EXCEPTION
15:15:37 1299  	     WHEN NO_DATA_FOUND THEN
15:15:37 1300  	       RAISE BAD_INVOICE_ID;
15:15:37 1301  	 END;
15:15:37 1302  
15:15:37 1303  	 BEGIN
15:15:37 1304  	   SELECT DISTINCT
15:15:37 1305  	     ACCOUNT.GROUP_ID into var_subscrib_group_id
15:15:37 1306  	   FROM
15:15:37 1307  	     ACCOUNT
15:15:37 1308  	     INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.ACCOUNT_ID = ACCOUNT.ID
15:15:37 1309  	     INNER JOIN LICENSE ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:15:37 1310  	   WHERE
15:15:37 1311  	     LICENSE.INVOICE_ID = in_invoice_id;
15:15:37 1312  	   EXCEPTION
15:15:37 1313  	     WHEN NO_DATA_FOUND THEN
15:15:37 1314  	       var_subscrib_group_id := NULL;
15:15:37 1315  	 END;
15:15:37 1316  
15:15:37 1317  	 IF var_subscrib_group_id IS NULL THEN
15:15:37 1318  	   BEGIN
15:15:37 1319  	     SELECT
15:15:37 1320  	       GIFT_CERTIFICATE.PURCHASER_GROUP_ID,
15:15:37 1321  	       GIFT_CERTIFICATE.REDEEMER_GROUP_ID
15:15:37 1322  	       into
15:15:37 1323  	       var_gc_purchaser_group_id,
15:15:37 1324  	       var_gc_redeemer_group_id
15:15:37 1325  	     FROM
15:15:37 1326  	       GIFT_CERTIFICATE
15:15:37 1327  	     WHERE
15:15:37 1328  	       GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = in_invoice_id
15:15:37 1329  	       OR GIFT_CERTIFICATE.FINALIZED_INVOICE_ID = in_invoice_id;
15:15:37 1330  	     EXCEPTION
15:15:37 1331  	       WHEN NO_DATA_FOUND THEN
15:15:37 1332  		 var_gc_purchaser_group_id := NULL;
15:15:37 1333  		 var_gc_redeemer_group_id  := NULL;
15:15:37 1334  	   END;
15:15:37 1335  	 END IF;
15:15:37 1336  
15:15:37 1337  	 IF var_subscrib_group_id IS NULL
15:15:37 1338  	   AND var_gc_purchaser_group_id IS NULL
15:15:37 1339  	   AND var_gc_redeemer_group_id IS NULL THEN
15:15:37 1340  	     RAISE CAN_NOT_FIND_ACCOUNT;
15:15:37 1341  	 END IF;
15:15:37 1342  
15:15:37 1343  	 OPEN out_group_ids FOR
15:15:37 1344  	 SELECT GROUP_ID FROM (
15:15:37 1345  	   SELECT
15:15:37 1346  	     var_subscrib_group_id as "GROUP_ID"
15:15:37 1347  	   FROM
15:15:37 1348  	     DUAL
15:15:37 1349  	   UNION
15:15:37 1350  	   SELECT
15:15:37 1351  	     var_gc_purchaser_group_id as "GROUP_ID"
15:15:37 1352  	   FROM
15:15:37 1353  	     DUAL
15:15:37 1354  	   UNION
15:15:37 1355  	   SELECT
15:15:37 1356  	     var_gc_redeemer_group_id as "GROUP_ID"
15:15:37 1357  	   FROM
15:15:37 1358  	     DUAL
15:15:37 1359  	 )
15:15:37 1360  	 WHERE
15:15:37 1361  	   GROUP_ID IS NOT NULL;
15:15:37 1362  
15:15:37 1363  EXCEPTION
15:15:37 1364  WHEN BAD_INVOICE_ID THEN
15:15:37 1365  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:37 1366  	   SPROC_NAME, 'No such invoice');
15:15:37 1367  WHEN CAN_NOT_FIND_ACCOUNT THEN
15:15:37 1368  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:37 1369  	   SPROC_NAME, 'Could not find account for given invoice');
15:15:37 1370  WHEN OTHERS THEN
15:15:37 1371  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:37 1372  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:15:37 1373  END GET_GROUPS_ID_BY_INVOICE_ID;
