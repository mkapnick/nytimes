15:15:35 1758  PROCEDURE CHECK_PRODUCT_ELIGIBILITY (
15:15:35 1759  	 in_group_id	   IN NUMBER,
15:15:35 1760  	 in_offer_chain_id IN NUMBER,
15:15:35 1761  	 out_is_eligible   OUT NUMBER,
15:15:35 1762  	 out_concurrent_subscription_id OUT NUMBER
15:15:35 1763  ) AS
15:15:35 1764  SPROC_NAME CONSTANT VARCHAR2(25) := 'CHECK_PRODUCT_ELIGIBILITY';
15:15:35 1765  -- Variables
15:15:35 1766  var_account_id  NUMBER;
15:15:35 1767  var_is_eligible NUMBER;
15:15:35 1768  var_is_gc       NUMBER;
15:15:35 1769  -- Exceptions
15:15:35 1770  BAD_GROUP_ID		   EXCEPTION;
15:15:35 1771  CAN_NOT_CHECK_SAME_PRODUCTS EXCEPTION;
15:15:35 1772  BAD_OC_ID		   EXCEPTION;
15:15:35 1773  EXCEPTION_MESSAGE	   VARCHAR(1024);
15:15:35 1774  BEGIN
15:15:35 1775  
15:15:35 1776  	 var_is_eligible := GLOBAL_CONSTANTS_V21.TRUE;
15:15:35 1777  
15:15:35 1778  	 out_concurrent_subscription_id := NULL;
15:15:35 1779  
15:15:35 1780  	 BEGIN
15:15:35 1781  	   SELECT
15:15:35 1782  	     OC.IS_GIFT_CERTIFICATE into var_is_gc
15:15:35 1783  	   FROM
15:15:35 1784  	     OFFER_CHAIN OC
15:15:35 1785  	   WHERE
15:15:35 1786  	     OC.ID = in_offer_chain_id;
15:15:35 1787  	   EXCEPTION
15:15:35 1788  	     WHEN NO_DATA_FOUND THEN
15:15:35 1789  	       RAISE BAD_OC_ID;
15:15:35 1790  	 END;
15:15:35 1791  
15:15:35 1792  	 -- only check eligibility if this is not a gift certificate
15:15:35 1793  	 IF (var_is_gc IS NULL OR var_is_gc != 1) THEN
15:15:35 1794  	   BEGIN
15:15:35 1795  	     SELECT
15:15:35 1796  	       A.ID into var_account_id
15:15:35 1797  	     FROM
15:15:35 1798  	       ACCOUNT A
15:15:35 1799  	     WHERE
15:15:35 1800  	       A.GROUP_ID = in_group_id;
15:15:35 1801  	     EXCEPTION
15:15:35 1802  	       WHEN NO_DATA_FOUND THEN
15:15:35 1803  		 RAISE BAD_GROUP_ID;
15:15:35 1804  	   END;
15:15:35 1805  
15:15:35 1806  
15:15:35 1807  	   FOR f_offer_chain IN (
15:15:35 1808  	       SELECT
15:15:35 1809  		 S.ID as SUBSCRIPTION_ID,
15:15:35 1810  		 S.OFFER_CHAIN_ID
15:15:35 1811  	       FROM
15:15:35 1812  		 SUBSCRIPTION S
15:15:35 1813  	       WHERE
15:15:35 1814  		 S.ACCOUNT_ID = var_account_id
15:15:35 1815  		 AND (S.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V21.SUBSCRIPTION_ACTIVE
15:15:35 1816  		      OR S.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V21.SUBSCRIPTION_IN_GRACE_PERIOD)
15:15:35 1817  	   )
15:15:35 1818  	   LOOP
15:15:35 1819  	     BEGIN
15:15:35 1820  	       IF (
15:15:35 1821  		 PROCS_OFFER_CHAIN_V21.CHECK_FOR_SAME_PRODUCTS(
15:15:35 1822  		   in_offer_chain_id,
15:15:35 1823  		   f_offer_chain.offer_chain_id,
15:15:35 1824  		   GLOBAL_CONSTANTS_V21.TRUE
15:15:35 1825  		 ) = GLOBAL_CONSTANTS_V21.TRUE
15:15:35 1826  	       ) THEN
15:15:35 1827  		 var_is_eligible := GLOBAL_CONSTANTS_V21.FALSE;
15:15:35 1828  		 out_concurrent_subscription_id := f_offer_chain.SUBSCRIPTION_ID;
15:15:35 1829  	       END IF;
15:15:35 1830  	       EXCEPTION
15:15:35 1831  		 WHEN OTHERS THEN
15:15:35 1832  		   EXCEPTION_MESSAGE := SQLERRM;
15:15:35 1833  		   RAISE CAN_NOT_CHECK_SAME_PRODUCTS;
15:15:35 1834  	     END;
15:15:35 1835  	   END LOOP;
15:15:35 1836  	 END IF;
15:15:35 1837  	 out_is_eligible := var_is_eligible;
15:15:35 1838  
15:15:35 1839  EXCEPTION
15:15:35 1840  WHEN BAD_GROUP_ID THEN
15:15:35 1841  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:35 1842  	   SPROC_NAME, 'No such offer chain', SQLERRM);
15:15:35 1843  WHEN BAD_OC_ID THEN
15:15:35 1844  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:35 1845  	   SPROC_NAME, 'No such offer chain', SQLERRM);
15:15:35 1846  WHEN CAN_NOT_CHECK_SAME_PRODUCTS THEN
15:15:35 1847  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:35 1848  	   SPROC_NAME, 'Could not check offers for same products', EXCEPTION_MESSAGE);
15:15:35 1849  WHEN OTHERS THEN
15:15:35 1850  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:35 1851  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:15:35 1852  END CHECK_PRODUCT_ELIGIBILITY;
