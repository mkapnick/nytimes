15:15:36 1659  PROCEDURE REFUND_INVOICE (
15:15:36 1660  	 in_invoice_id	    IN NUMBER,
15:15:36 1661  	 in_refund_amount   IN NUMBER,
15:15:36 1662  	 in_note	    IN VARCHAR2,
15:15:36 1663  	 in_created_by	    IN VARCHAR2,
15:15:36 1664  	 out_charge_id	    OUT NUMBER
15:15:36 1665  ) AS
15:15:36 1666  SPROC_NAME CONSTANT VARCHAR2(14) := 'REFUND_INVOICE';
15:15:36 1667  -- VARIABLES
15:15:36 1668  var_invoice_status_id  NUMBER;
15:15:36 1669  var_subscription_id    NUMBER;
15:15:36 1670  var_account_id	      NUMBER;
15:15:36 1671  var_group_id	      NUMBER;
15:15:36 1672  var_account_status_id  NUMBER;
15:15:36 1673  var_new_transaction_id NUMBER;
15:15:36 1674  var_instrument_type_id NUMBER;
15:15:36 1675  var_instrument_id      NUMBER;
15:15:36 1676  var_new_charge_id      NUMBER;
15:15:36 1677  var_invoice_amount     NUMBER(10,2);
15:15:36 1678  var_refunds_before     NUMBER(10,2);
15:15:36 1679  var_charges_amount     NUMBER(10,2);
15:15:36 1680  -- EXCEPTIONS
15:15:36 1681  CAN_NOT_FIND_SUBSCR_OR_GC     EXCEPTION;
15:15:36 1682  ACCOUNT_IS_FROZEN	     EXCEPTION;
15:15:36 1683  BAD_INVOICE_ID		     EXCEPTION;
15:15:36 1684  CAN_NOT_CREATE_TRANSACTION    EXCEPTION;
15:15:36 1685  CAN_NOT_CREATE_CHARGE	     EXCEPTION;
15:15:36 1686  CAN_NOT_CALC_INVOICE_AMOUNT   EXCEPTION;
15:15:36 1687  REFUND_IS_GREATER_THAN_ANOUNT EXCEPTION;
15:15:36 1688  CAN_NOT_ANNOTATE_SUBSCRIPTION EXCEPTION;
15:15:36 1689  TOT_REF_IS_GREATER_THAN_ANOUNT EXCEPTION;
15:15:36 1690  INVOICE_IS_NOT_CLOSED	     EXCEPTION;
15:15:36 1691  TOT_REF_IS_GRATER_THAN_CHARGES EXCEPTION;
15:15:36 1692  EXCEPTION_MESSAGE	      VARCHAR2(1024);
15:15:36 1693  BEGIN
15:15:36 1694  
15:15:36 1695  	 BEGIN
15:15:36 1696  	   SELECT
15:15:36 1697  	     INVOICE.INVOICE_STATUS_ID into var_invoice_status_id
15:15:36 1698  	   FROM
15:15:36 1699  	     INVOICE
15:15:36 1700  	   WHERE
15:15:36 1701  	     INVOICE.ID = in_invoice_id;
15:15:36 1702  	   EXCEPTION
15:15:36 1703  	     WHEN NO_DATA_FOUND THEN
15:15:36 1704  	       RAISE BAD_INVOICE_ID;
15:15:36 1705  	 END;
15:15:36 1706  
15:15:36 1707  	 -- Get instrument and subscription id if exists
15:15:36 1708  	 BEGIN
15:15:36 1709  	   SELECT
15:15:36 1710  	     SUBSCRIPTION.INSTRUMENT_ID,
15:15:36 1711  	     SUBSCRIPTION.INSTRUMENT_TYPE_ID,
15:15:36 1712  	     SUBSCRIPTION.ACCOUNT_ID,
15:15:36 1713  	     SUBSCRIPTION.ID
15:15:36 1714  	     into
15:15:36 1715  	     var_instrument_id,
15:15:36 1716  	     var_instrument_type_id,
15:15:36 1717  	     var_account_id,
15:15:36 1718  	     var_subscription_id
15:15:36 1719  	   FROM
15:15:36 1720  	     SUBSCRIPTION
15:15:36 1721  	     INNER JOIN LICENSE ON SUBSCRIPTION.ID = LICENSE.SUBSCRIPTION_ID
15:15:36 1722  	   WHERE
15:15:36 1723  	     LICENSE.INVOICE_ID = in_invoice_id
15:15:36 1724  	     AND ROWNUM <= 1;
15:15:36 1725  	   EXCEPTION
15:15:36 1726  	     WHEN NO_DATA_FOUND THEN
15:15:36 1727  	       BEGIN
15:15:36 1728  
15:15:36 1729  		 var_subscription_id := NULL;
15:15:36 1730  
15:15:36 1731  		 SELECT
