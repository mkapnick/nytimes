15:15:36 936  
15:15:36 937  FUNCTION GET_RENEWAL_DATE (
15:15:36 938  /*
15:15:36 939  Throws exceptions:
15:15:36 940  APP_EXCEPTION_CODES_V21.NOT_FOUND
15:15:36 941  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
15:15:36 942  */
15:15:36 943  	in_subscription_id in NUMBER
15:15:36 944  ) RETURN DATE AS
15:15:36 945  -- VARIABLES
15:15:36 946  SPROC_NAME	      CONSTANT VARCHAR2(16) := 'GET_RENEWAL_DATE';
15:15:36 947  var_subscription_status NUMBER;
15:15:36 948  var_licenses_count      NUMBER;
15:15:36 949  var_license_end_date    DATE;
15:15:36 950  var_last_offer_id       NUMBER;
15:15:36 951  var_offer_chain_id      NUMBER;
15:15:36 952  var_last_license_id     NUMBER;
15:15:36 953  var_current_offer_index NUMBER;
15:15:36 954  var_current_offer_recurr_num NUMBER;
15:15:36 955  var_offer_recurr_num    NUMBER;
15:15:36 956  -- EXCEPTIONS
15:15:36 957  BAD_SUBSCRIPTION_ID EXCEPTION;
15:15:36 958  NO_LICENSES_FOUND EXCEPTION;
15:15:36 959  BEGIN
15:15:36 960  
15:15:36 961  	-- Get subscription id and offer chain id
15:15:36 962  	BEGIN
15:15:36 963  	  SELECT
15:15:36 964  	    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID,
15:15:36 965  	    SUBSCRIPTION.OFFER_CHAIN_ID
15:15:36 966  	    into
15:15:36 967  	    var_subscription_status,
15:15:36 968  	    var_offer_chain_id
15:15:36 969  	  FROM
15:15:36 970  	    SUBSCRIPTION
15:15:36 971  	  WHERE
15:15:36 972  	    SUBSCRIPTION.ID = in_subscription_id;
15:15:36 973  	  EXCEPTION
15:15:36 974  	    WHEN NO_DATA_FOUND THEN
15:15:36 975  	      RAISE BAD_SUBSCRIPTION_ID;
15:15:36 976  	END;
15:15:36 977  
15:15:36 978  	IF var_subscription_status != GLOBAL_STATUSES_V21.SUBSCRIPTION_ACTIVE THEN
15:15:36 979  	  -- TODO: Is suspended subscription has renewal date? (For the phase II)
15:15:36 980  	  -- AND var_subscription_status != GLOBAL_STATUSES_V21.SUBSCRIPTION_SUSPENDED THEN
15:15:36 981  	  RETURN NULL;
15:15:36 982  	END IF;
15:15:36 983  
15:15:36 984  	BEGIN
15:15:36 985  	  SELECT
15:15:36 986  	    LICENSE_ID,
15:15:36 987  	    END_DATE,
15:15:36 988  	    OFFER_ID,
15:15:36 989  	    CURRENT_OFFER_INDEX,
15:15:36 990  	    CURRENT_OFFER_RECURR_NUM
15:15:36 991  	    into
15:15:36 992  	    var_last_license_id,
15:15:36 993  	    var_license_end_date,
15:15:36 994  	    var_last_offer_id,
15:15:36 995  	    var_current_offer_index,
15:15:36 996  	    var_current_offer_recurr_num
15:15:36 997  	  FROM
15:15:36 998  	    (
15:15:36 999  	      SELECT
15:15:36 1000  		 LICENSE.ID as "LICENSE_ID",
15:15:36 1001  		 LICENSE.END_DATE,
15:15:36 1002  		 LICENSE.OFFER_ID,
15:15:36 1003  		 LICENSE.CURRENT_OFFER_INDEX,
15:15:36 1004  		 LICENSE.CURRENT_OFFER_RECURR_NUM
15:15:36 1005  	       FROM
15:15:36 1006  		 LICENSE
15:15:36 1007  	       WHERE
15:15:36 1008  		 LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V21.LICENSE_ACTIVE
15:15:36 1009  		 AND LICENSE.SUBSCRIPTION_ID = in_subscription_id
15:15:36 1010  	       ORDER BY END_DATE DESC
15:15:36 1011  	     )
15:15:36 1012  	     INNER JOIN OFFER ON OFFER_ID = OFFER.ID
15:15:36 1013  	   WHERE
15:15:36 1014  	     ROWNUM <= 1;
15:15:36 1015  
15:15:36 1016  	   EXCEPTION
15:15:36 1017  	     WHEN NO_DATA_FOUND THEN
15:15:36 1018  	       -- RAISE NO_LICENSES_FOUND;
15:15:36 1019  	       RETURN NULL;
15:15:36 1020  	 END;
15:15:36 1021  
15:15:36 1022  	 SELECT
15:15:36 1023  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES into var_offer_recurr_num
15:15:36 1024  	 FROM
15:15:36 1025  	   OFFER_OFFER_CHAIN
15:15:36 1026  	 WHERE
15:15:36 1027  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
15:15:36 1028  	   AND OFFER_OFFER_CHAIN.OFFER_ID = var_last_offer_id;
15:15:36 1029  
15:15:36 1030  	 IF PROCS_OFFER_CHAIN_V21.GET_NEXT_OFFER_INDEX(var_offer_chain_id, var_current_offer_index) IS NULL
15:15:36 1031  	   AND var_offer_recurr_num = var_current_offer_recurr_num THEN
15:15:36 1032  	   -- There is no next offer for this subscription
15:15:36 1033  	   RETURN NULL;
15:15:36 1034  	 END IF;
15:15:36 1035  
15:15:36 1036  	 RETURN var_license_end_date;
15:15:36 1037  
15:15:36 1038  EXCEPTION
15:15:36 1039  WHEN BAD_SUBSCRIPTION_ID THEN
15:15:36 1040  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 1041  	   SPROC_NAME, 'No such subscription');
15:15:36 1042  WHEN NO_LICENSES_FOUND THEN
15:15:36 1043  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 1044  	   SPROC_NAME, 'No licenses found');
15:15:36 1045  WHEN OTHERS THEN
15:15:36 1046  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 1047  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:15:36 1048  END GET_RENEWAL_DATE;
15:15:36 1049  
15:15:36 1050  /******************************************************************************/
15:15:36 1051  
15:15:36 1052  FUNCTION GET_RECENT_CHARGE (
15:15:36 1053  /*
15:15:36 1054  Throws exceptions:
15:15:36 1055  APP_EXCEPTION_CODES_V21.NOT_FOUND
15:15:36 1056  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
15:15:36 1057  */
15:15:36 1058  	 in_subscription_id IN NUMBER
15:15:36 1059  ) RETURN NUMBER AS
15:15:36 1060  -- VARIABLES
15:15:36 1061  SPROC_NAME	    CONSTANT VARCHAR2(17) := 'GET_RECENT_CHARGE';
15:15:36 1062  temp_subscription_id NUMBER;
15:15:36 1063  var_recent_charge    NUMBER(10,2);
15:15:36 1064  
15:15:36 1065  -- EXCEPTIONS
15:15:36 1066  BAD_SUBSCRIPTION_ID EXCEPTION;
15:15:36 1067  BEGIN
15:15:36 1068  
15:15:36 1069  	 BEGIN
15:15:36 1070  	   SELECT
15:15:36 1071  	     SUBSCRIPTION.ID into temp_subscription_id
15:15:36 1072  	   FROM
15:15:36 1073  	     SUBSCRIPTION
15:15:36 1074  	   WHERE
15:15:36 1075  	     SUBSCRIPTION.ID = in_subscription_id;
15:15:36 1076  	   EXCEPTION
15:15:36 1077  	     WHEN NO_DATA_FOUND THEN
15:15:36 1078  	       RAISE BAD_SUBSCRIPTION_ID;
15:15:36 1079  	 END;
15:15:36 1080  
15:15:36 1081  	 BEGIN
15:15:36 1082  	   SELECT
15:15:36 1083  	     CHARGE.CHARGE_AMOUNT into var_recent_charge
15:15:36 1084  	   FROM
15:15:36 1085  	     LICENSE
15:15:36 1086  	     INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:15:36 1087  	     INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
15:15:36 1088  	     INNER JOIN CHARGE ON CHARGE.INVOICE_ID = INVOICE.ID
15:15:36 1089  	   WHERE
15:15:36 1090  	     -- TODO: Review
15:15:36 1091  	     -- LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V21.LICENSE_PROCESSED
15:15:36 1092  	     -- AND
15:15:36 1093  	     SUBSCRIPTION.ID = in_subscription_id
15:15:36 1094  	     AND CHARGE.CHARGE_AMOUNT >= 0
15:15:36 1095  	     AND ROWNUM <= 1
15:15:36 1096  	   ORDER BY
15:15:36 1097  	     LICENSE.ID ASC, CHARGE.ID DESC;
15:15:36 1098  	   EXCEPTION
15:15:36 1099  	     WHEN NO_DATA_FOUND THEN
15:15:36 1100  	       var_recent_charge := 0;
15:15:36 1101  	 END;
15:15:36 1102  
15:15:36 1103  	 RETURN var_recent_charge;
15:15:36 1104  
15:15:36 1105  EXCEPTION
15:15:36 1106  WHEN BAD_SUBSCRIPTION_ID THEN
15:15:36 1107  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 1108  	   SPROC_NAME, 'No such subscription');
15:15:36 1109  WHEN OTHERS THEN
15:15:36 1110  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 1111  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:15:36 1112  END GET_RECENT_CHARGE;
15:15:36 1113  
15:15:36 1114  /******************************************************************************/
15:15:36 1115  
15:15:36 1116  FUNCTION GET_BILLING_CYCLE (
15:15:36 1117  	 in_subscription_id IN NUMBER
15:15:36 1118  ) RETURN VARCHAR2 AS
15:15:36 1119  -- VARIABLES
15:15:36 1120  SPROC_NAME	    CONSTANT VARCHAR2(17) := 'GET_BILLING_CYCLE';
15:15:36 1121  temp_subscription_id NUMBER;
15:15:36 1122  var_offer_duration   VARCHAR2(30);
15:15:36 1123  
15:15:36 1124  -- EXCEPTIONS
15:15:36 1125  BAD_SUBSCRIPTION_ID EXCEPTION;
15:15:36 1126  BEGIN
15:15:36 1127  
15:15:36 1128  	 BEGIN
15:15:36 1129  	   SELECT
15:15:36 1130  	     SUBSCRIPTION.ID into temp_subscription_id
15:15:36 1131  	   FROM
15:15:36 1132  	     SUBSCRIPTION
15:15:36 1133  	   WHERE
15:15:36 1134  	     SUBSCRIPTION.ID = in_subscription_id;
15:15:36 1135  	   EXCEPTION
15:15:36 1136  	     WHEN NO_DATA_FOUND THEN
15:15:36 1137  	       RAISE BAD_SUBSCRIPTION_ID;
15:15:36 1138  	 END;
15:15:36 1139  
15:15:36 1140  	 SELECT
15:15:36 1141  	   OFFER.ENTITLEMENT_DURATION into var_offer_duration
15:15:36 1142  	 FROM
15:15:36 1143  	   LICENSE
15:15:36 1144  	   INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:15:36 1145  	   INNER JOIN OFFER ON LICENSE.OFFER_ID = OFFER.ID
15:15:36 1146  	 WHERE
15:15:36 1147  	   --LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V21.LICENSE_PROCESSED
15:15:36 1148  	   --AND
15:15:36 1149  	   SUBSCRIPTION.ID = in_subscription_id
15:15:36 1150  	   AND ROWNUM <= 1
15:15:36 1151  	 ORDER BY
15:15:36 1152  	   LICENSE.ID ASC;
15:15:36 1153  
15:15:36 1154  	 RETURN var_offer_duration;
15:15:36 1155  
15:15:36 1156  EXCEPTION
15:15:36 1157  WHEN BAD_SUBSCRIPTION_ID THEN
15:15:36 1158  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 1159  	   SPROC_NAME, 'No such subscription');
15:15:36 1160  WHEN OTHERS THEN
15:15:36 1161  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 1162  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:15:36 1163  END GET_BILLING_CYCLE;
15:15:36 1164  
15:15:36 1165  /******************************************************************************/
15:15:36 1166  
15:15:36 1167  PROCEDURE REFUND_SUBSCRIPTION (
15:15:36 1168  /*
15:15:36 1169  Throws exceptions:
15:15:36 1170  APP_EXCEPTION_CODES_V21.NOT_FOUND
15:15:36 1171  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
15:15:36 1172  */
15:15:36 1173  	 in_subscription_id IN NUMBER,
15:15:36 1174  	 in_invoice_id	    IN NUMBER,
15:15:36 1175  	 in_refund_amount   IN NUMBER,
15:15:36 1176  	 in_note	    IN VARCHAR2,
15:15:36 1177  	 in_created_by	    IN VARCHAR2,
15:15:36 1178  	 out_charge_id	    OUT NUMBER
15:15:36 1179  ) AS
15:15:36 1180  -- VARIABLES
15:15:36 1181  SPROC_NAME	      CONSTANT VARCHAR2(19) := 'REFUND_SUBSCRIPTION';
15:15:36 1182  var_invoice_status_id  NUMBER;
15:15:36 1183  var_account_id	      NUMBER;
15:15:36 1184  var_account_status_id  NUMBER;
15:15:36 1185  var_new_transaction_id NUMBER;
15:15:36 1186  var_instrument_type_id NUMBER;
15:15:36 1187  var_instrument_id      NUMBER;
15:15:36 1188  var_new_charge_id      NUMBER;
15:15:36 1189  var_invoice_amount     NUMBER(10,2);
15:15:36 1190  var_refunds_before     NUMBER(10,2);
15:15:36 1191  var_charges_amount     NUMBER(10,2);
15:15:36 1192  -- EXCEPTIONS
15:15:36 1193  BAD_SUBSCRIPTION_ID	     EXCEPTION;
15:15:36 1194  ACCOUNT_IS_FROZEN	     EXCEPTION;
15:15:36 1195  BAD_INVOICE_ID		     EXCEPTION;
15:15:36 1196  CAN_NOT_CREATE_TRANSACTION    EXCEPTION;
15:15:36 1197  CAN_NOT_CREATE_CHARGE	     EXCEPTION;
15:15:36 1198  CAN_NOT_CALC_INVOICE_AMOUNT   EXCEPTION;
15:15:36 1199  REFUND_IS_GREATER_THAN_ANOUNT EXCEPTION;
15:15:36 1200  CAN_NOT_ANNOTATE_SUBSCRIPTION EXCEPTION;
15:15:36 1201  TOT_REF_IS_GREATER_THAN_ANOUNT EXCEPTION;
15:15:36 1202  INVOICE_IS_NOT_CLOSED	     EXCEPTION;
15:15:36 1203  TOT_REF_IS_GRATER_THAN_CHARGES EXCEPTION;
15:15:36 1204  EXCEPTION_MESSAGE	      VARCHAR2(1024);
15:15:36 1205  BEGIN
15:15:36 1206  
15:15:36 1207  	 BEGIN
15:15:36 1208  	   SELECT
15:15:36 1209  	     SUBSCRIPTION.INSTRUMENT_ID,
15:15:36 1210  	     SUBSCRIPTION.INSTRUMENT_TYPE_ID,
15:15:36 1211  	     SUBSCRIPTION.ACCOUNT_ID
15:15:36 1212  	     into
15:15:36 1213  	     var_instrument_id,
15:15:36 1214  	     var_instrument_type_id,
15:15:36 1215  	     var_account_id
15:15:36 1216  	   FROM
15:15:36 1217  	     SUBSCRIPTION
15:15:36 1218  	   WHERE
15:15:36 1219  	     SUBSCRIPTION.ID = in_subscription_id;
15:15:36 1220  	   EXCEPTION
15:15:36 1221  	     WHEN NO_DATA_FOUND THEN
15:15:36 1222  	       RAISE BAD_SUBSCRIPTION_ID;
15:15:36 1223  	 END;
15:15:36 1224  
15:15:36 1225  	 -- Check account status. It should not to be frozen
15:15:36 1226  	 SELECT
15:15:36 1227  	   ACCOUNT.ACCOUNT_STATUS_ID into var_account_status_id
15:15:36 1228  	 FROM
15:15:36 1229  	   ACCOUNT
15:15:36 1230  	 WHERE
15:15:36 1231  	   ACCOUNT.ID = var_account_id;
15:15:36 1232  
15:15:36 1233  	 IF var_account_status_id = GLOBAL_STATUSES_V21.ACCOUNT_FROZEN THEN
15:15:36 1234  	   RAISE ACCOUNT_IS_FROZEN;
15:15:36 1235  	 END IF;
15:15:36 1236  
15:15:36 1237  	 BEGIN
15:15:36 1238  	   SELECT
15:15:36 1239  	     INVOICE.INVOICE_STATUS_ID into var_invoice_status_id
15:15:36 1240  	   FROM
15:15:36 1241  	     INVOICE
15:15:36 1242  	   WHERE
15:15:36 1243  	     INVOICE.ID = in_invoice_id;
15:15:36 1244  	   EXCEPTION
15:15:36 1245  	     WHEN NO_DATA_FOUND THEN
15:15:36 1246  	       RAISE BAD_INVOICE_ID;
15:15:36 1247  	 END;
15:15:36 1248  
15:15:36 1249  	 IF var_invoice_status_id != GLOBAL_STATUSES_V21.INVOICE_CLOSED THEN
15:15:36 1250  	   RAISE INVOICE_IS_NOT_CLOSED;
15:15:36 1251  	 END IF;
15:15:36 1252  
15:15:36 1253  	 BEGIN
15:15:36 1254  	   PROCS_INVOICE_V21.CALCULATE_INVOICE_AMOUNT (
15:15:36 1255  	     in_invoice_id => in_invoice_id,
15:15:36 1256  	     out_amount    => var_invoice_amount
15:15:36 1257  	   );
15:15:36 1258  	   EXCEPTION
15:15:36 1259  	     WHEN OTHERS THEN
15:15:36 1260  	       EXCEPTION_MESSAGE := SQLERRM;
15:15:36 1261  	       RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
15:15:36 1262  	 END;
15:15:36 1263  
15:15:36 1264  	 IF ( in_refund_amount > var_invoice_amount ) THEN
15:15:36 1265  	   RAISE REFUND_IS_GREATER_THAN_ANOUNT;
15:15:36 1266  	 END IF;
15:15:36 1267  
15:15:36 1268  	 SELECT /*+ STAR_TRANSFORMATION */
15:15:36 1269  	   SUM(CHARGE.CHARGE_AMOUNT) into var_refunds_before
15:15:36 1270  	 FROM
15:15:36 1271  	   CHARGE
15:15:36 1272  	 WHERE
15:15:36 1273  	   CHARGE.INVOICE_ID = in_invoice_id
15:15:36 1274  	   AND (
15:15:36 1275  	     CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V21.CHARGE_OPENED
15:15:36 1276  	     OR CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V21.CHARGE_PROCESSED
15:15:36 1277  	   )
15:15:36 1278  	   AND CHARGE.CHARGE_AMOUNT < 0;
15:15:36 1279  
15:15:36 1280  	 -- Refunds are negative
15:15:36 1281  	 var_refunds_before := -var_refunds_before;
15:15:36 1282  
15:15:36 1283  	 var_charges_amount := 0;
15:15:36 1284  
15:15:36 1285  	 FOR f_processed_charges IN (
15:15:36 1286  	   SELECT
15:15:36 1287  	     CHARGE.CHARGE_AMOUNT
15:15:36 1288  	   FROM
15:15:36 1289  	     CHARGE
15:15:36 1290  	   WHERE
15:15:36 1291  	     CHARGE.INVOICE_ID = in_invoice_id
15:15:36 1292  	     AND CHARGE.CHARGE_STATUS_ID IN (SELECT GLOBAL_STATUSES_V21.CHARGE_PROCESSED FROM DUAL)
15:15:36 1293  	 )
15:15:36 1294  	 LOOP
15:15:36 1295  	   IF f_processed_charges.CHARGE_AMOUNT > 0 THEN
15:15:36 1296  	     var_charges_amount := var_charges_amount + f_processed_charges.CHARGE_AMOUNT;
15:15:36 1297  	   END IF;
15:15:36 1298  	 END LOOP;
15:15:36 1299  
15:15:36 1300  	 IF (in_refund_amount + var_refunds_before > var_invoice_amount) THEN
15:15:36 1301  	   RAISE TOT_REF_IS_GREATER_THAN_ANOUNT;
15:15:36 1302  	 END IF;
15:15:36 1303  
15:15:36 1304  	 IF (in_refund_amount + var_refunds_before > var_charges_amount) THEN
15:15:36 1305  	   RAISE TOT_REF_IS_GRATER_THAN_CHARGES;
15:15:36 1306  	 END IF;
15:15:36 1307  
15:15:36 1308  	 BEGIN
15:15:36 1309  	   PROCS_TRANSACTION_V21.CREATE_TRANSACTION(
15:15:36 1310  	     in_transaction_id	       => NULL,
15:15:36 1311  	     in_status_id	       => GLOBAL_STATUSES_V21.TRANSACTION_PREPARE,
15:15:36 1312  	     in_amount		       => -in_refund_amount,
15:15:36 1313  	     in_created_by	       => in_created_by,
15:15:36 1314  	     in_order_id	       => NULL,
15:15:36 1315  	     in_is_refund	       => GLOBAL_CONSTANTS_V21.TRUE,
15:15:36 1316  	     in_transaction_type_code  => 'REFUND',
15:15:36 1317  	     out_transaction_id        => var_new_transaction_id
15:15:36 1318  	   );
15:15:36 1319  	   EXCEPTION
15:15:36 1320  	     WHEN OTHERS THEN
15:15:36 1321  	       EXCEPTION_MESSAGE := SQLERRM;
15:15:36 1322  	       RAISE CAN_NOT_CREATE_TRANSACTION;
15:15:36 1323  	 END;
15:15:36 1324  
15:15:36 1325  	 BEGIN
15:15:36 1326  	   PROCS_CHARGE_V21.CREATE_CHARGE(
15:15:36 1327  	     in_invoice_id	   => in_invoice_id,
15:15:36 1328  	     in_transaction_id	   => var_new_transaction_id,
15:15:36 1329  	     in_instrument_type_id => var_instrument_type_id,
15:15:36 1330  	     in_instrument_id	   => var_instrument_id,
15:15:36 1331  	     in_charge_amount	   => -in_refund_amount,
15:15:36 1332  	     in_created_by	   => in_created_by,
15:15:36 1333  	     in_charge_status_id   => GLOBAL_STATUSES_V21.CHARGE_OPENED,
15:15:36 1334  	     out_charge_id	   => var_new_charge_id
15:15:36 1335  	   );
15:15:36 1336  	   EXCEPTION
15:15:36 1337  	     WHEN OTHERS THEN
15:15:36 1338  	       EXCEPTION_MESSAGE := SQLERRM;
15:15:36 1339  	       RAISE CAN_NOT_CREATE_CHARGE;
15:15:36 1340  	 END;
15:15:36 1341  
15:15:36 1342  	 IF in_note IS NOT NULL THEN
15:15:36 1343  	   BEGIN
15:15:36 1344  	     PROCS_SUBSCRIPTION_V21.ANNOTATE_SUBSCRIPTION(
15:15:36 1345  	       in_subscription_id => in_subscription_id,
15:15:36 1346  	       in_agent_id	  => 0, -- AGENT_ID??
15:15:36 1347  	       in_note		  => in_note,
15:15:36 1348  	       in_created_by	  => in_created_by
15:15:36 1349  	     );
15:15:36 1350  	     EXCEPTION
15:15:36 1351  	       WHEN OTHERS THEN
15:15:36 1352  		 EXCEPTION_MESSAGE := SQLERRM;
15:15:36 1353  		 RAISE CAN_NOT_ANNOTATE_SUBSCRIPTION;
15:15:36 1354  	   END;
15:15:36 1355  	 END IF;
15:15:36 1356  
15:15:36 1357  	 out_charge_id := var_new_charge_id;
15:15:36 1358  
15:15:36 1359  EXCEPTION
15:15:36 1360  WHEN INVOICE_IS_NOT_CLOSED THEN
15:15:36 1361  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
15:15:36 1362  	   SPROC_NAME, 'Invoice is not closed');
15:15:36 1363  WHEN BAD_SUBSCRIPTION_ID THEN
15:15:36 1364  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 1365  	   SPROC_NAME, 'No such subscription');
15:15:36 1366  WHEN ACCOUNT_IS_FROZEN THEN
15:15:36 1367  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
15:15:36 1368  	   SPROC_NAME, 'Could not refund subscription for frozen account');
15:15:36 1369  WHEN BAD_INVOICE_ID THEN
15:15:36 1370  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 1371  	   SPROC_NAME, 'No such invoice');
15:15:36 1372  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
15:15:36 1373  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:36 1374  	   SPROC_NAME, 'Could not calculate invoice amount', EXCEPTION_MESSAGE);
15:15:36 1375  WHEN REFUND_IS_GREATER_THAN_ANOUNT THEN
15:15:36 1376  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INVALID_PARAMETER,
15:15:36 1377  	   SPROC_NAME, 'Refund is greater than amount');
15:15:36 1378  WHEN TOT_REF_IS_GREATER_THAN_ANOUNT THEN
15:15:36 1379  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INVALID_PARAMETER,
15:15:36 1380  	   SPROC_NAME, 'There were refunds before and sum of all refunds and new refund more than invoice amount');
15:15:36 1381  WHEN TOT_REF_IS_GRATER_THAN_CHARGES THEN
15:15:36 1382  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INVALID_PARAMETER,
15:15:36 1383  	   SPROC_NAME, 'Total refund amount is greater than sum of processed charges');
15:15:36 1384  WHEN CAN_NOT_CREATE_TRANSACTION THEN
15:15:36 1385  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:36 1386  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
15:15:36 1387  WHEN CAN_NOT_CREATE_CHARGE THEN
15:15:36 1388  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:36 1389  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
15:15:36 1390  WHEN CAN_NOT_ANNOTATE_SUBSCRIPTION THEN
15:15:36 1391  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:36 1392  	   SPROC_NAME, 'Could not annotate subscription', EXCEPTION_MESSAGE);
15:15:36 1393  WHEN OTHERS THEN
15:15:36 1394  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 1395  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:15:36 1396  END REFUND_SUBSCRIPTION;
15:15:36 1397  
15:15:36 1398  /******************************************************************************/
15:15:36 1399  
15:15:36 1400  PROCEDURE ADD_SUBSCRIPTION_EXTENSION (
15:15:36 1401  /*
15:15:36 1402  APP_EXCEPTION_CODES_V21.NOT_FOUND
15:15:36 1403  APP_EXCEPTION_CODES_V21.INTERNAL_ERROR
15:15:36 1404  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
15:15:36 1405  */
15:15:36 1406  	 in_subscription_id	 IN NUMBER,
15:15:36 1407  	 in_effective_start_date IN DATE,
15:15:36 1408  	 in_effective_end_date	 IN DATE,
15:15:36 1409  	 in_note		 IN VARCHAR2,
15:15:36 1410  	 in_updated_by		 IN VARCHAR2
15:15:36 1411  ) AS
15:15:36 1412  -- VARIABLES
15:15:36 1413  SPROC_NAME		    CONSTANT VARCHAR2(26) := 'ADD_SUBSCRIPTION_EXTENSION';
15:15:36 1414  temp_subscription_id	    NUMBER;
15:15:36 1415  var_current_license_id	    NUMBER;
15:15:36 1416  var_current_license_start_date DATE;
15:15:36 1417  var_current_license_end_date DATE;
15:15:36 1418  var_current_offer_id	    NUMBER;
15:15:36 1419  var_current_invoice_id	    NUMBER;
15:15:36 1420  var_current_date 	    DATE;
15:15:36 1421  var_current_offer_index	    NUMBER;
15:15:36 1422  var_current_offer_recurr_num NUMBER;
15:15:36 1423  var_account_tax_exempt_id    VARCHAR2(255);
15:15:36 1424  
15:15:36 1425  var_free_invoice_id NUMBER;
15:15:36 1426  var_free_license_id NUMBER;
15:15:36 1427  var_new_license_id  NUMBER;
15:15:36 1428  var_ext_license_id  NUMBER;
15:15:36 1429  -- EXCEPTIONS
15:15:36 1430  BAD_SUBSCRIPTION_ID	     EXCEPTION;
15:15:36 1431  CAN_NOT_FIND_OFFER_OR_LICENSE EXCEPTION;
15:15:36 1432  CAN_NOT_CHANGE_LICENSE_STATUS EXCEPTION;
15:15:36 1433  CAN_NOT_CREATE_INVOICE	     EXCEPTION;
15:15:36 1434  CAN_NOT_CREATE_NEW_LICENSE    EXCEPTION;
15:15:36 1435  CAN_NOT_CREATE_END_LICENSE    EXCEPTION;
15:15:36 1436  CAN_NOT_ANNOTATE_SUBSCRIPTION EXCEPTION;
15:15:36 1437  EXTENS_START_DATE_IS_TOO_FAR  EXCEPTION;
15:15:36 1438  EXT_START_DATE_LATER_THEN_END EXCEPTION;
15:15:36 1439  EXTENS_START_DATE_IS_TOO_SMALL EXCEPTION;
15:15:36 1440  EXCEPTION_MESSAGE	     VARCHAR2(1024);
15:15:36 1441  BEGIN
15:15:36 1442  
15:15:36 1443  	 var_current_date := PROCS_COMMON_V21.NORMALIZE_DATE(SYSDATE);
15:15:36 1444  
15:15:36 1445  	 -- Check that subscription exists
15:15:36 1446  	 BEGIN
15:15:36 1447  	   SELECT
15:15:36 1448  	     SUBSCRIPTION.ID into temp_subscription_id
15:15:36 1449  	   FROM
15:15:36 1450  	     SUBSCRIPTION
15:15:36 1451  	   WHERE
15:15:36 1452  	     SUBSCRIPTION.ID = in_subscription_id;
15:15:36 1453  	   EXCEPTION
15:15:36 1454  	     WHEN NO_DATA_FOUND THEN
15:15:36 1455  	       RAISE BAD_SUBSCRIPTION_ID;
15:15:36 1456  	 END;
15:15:36 1457  
15:15:36 1458  	 -- Get account tax exempt id
15:15:36 1459  	 SELECT DISTINCT
15:15:36 1460  	   ACCOUNT.TAX_EXEMPT_ID into var_account_tax_exempt_id
15:15:36 1461  	 FROM
15:15:36 1462  	   ACCOUNT
15:15:36 1463  	   INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.ACCOUNT_ID = ACCOUNT.ID
15:15:36 1464  	 WHERE
15:15:36 1465  	   SUBSCRIPTION.ID = in_subscription_id;
15:15:36 1466  
15:15:36 1467  	 -- Select current data
15:15:36 1468  	 BEGIN
15:15:36 1469  	   SELECT
15:15:36 1470  	     LICENSE.ID,
15:15:36 1471  	     LICENSE.START_DATE,
15:15:36 1472  	     LICENSE.END_DATE,
15:15:36 1473  	     LICENSE.CURRENT_OFFER_INDEX,
15:15:36 1474  	     LICENSE.CURRENT_OFFER_RECURR_NUM,
15:15:36 1475  	     OFFER.ID,
15:15:36 1476  	     INVOICE.ID
15:15:36 1477  	   INTO
15:15:36 1478  	     var_current_license_id,
15:15:36 1479  	     var_current_license_start_date,
15:15:36 1480  	     var_current_license_end_date,
15:15:36 1481  	     var_current_offer_index,
15:15:36 1482  	     var_current_offer_recurr_num,
15:15:36 1483  	     var_current_offer_id,
15:15:36 1484  	     var_current_invoice_id
15:15:36 1485  	   FROM
15:15:36 1486  	     LICENSE
15:15:36 1487  	     INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:15:36 1488  	     INNER JOIN OFFER ON LICENSE.OFFER_ID = OFFER.ID
15:15:36 1489  	     INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
15:15:36 1490  	   WHERE
15:15:36 1491  	     SUBSCRIPTION.ID = in_subscription_id
15:15:36 1492  	     AND PROCS_COMMON_V21.NORMALIZE_DATE(LICENSE.END_DATE) > var_current_date
15:15:36 1493  	     AND PROCS_COMMON_V21.NORMALIZE_DATE(LICENSE.START_DATE) <= var_current_date
15:15:36 1494  	     AND ROWNUM <= 1
15:15:36 1495  	   ORDER BY
15:15:36 1496  	     LICENSE.ID DESC;
15:15:36 1497  	   EXCEPTION
15:15:36 1498  	     WHEN NO_DATA_FOUND THEN
15:15:36 1499  	       RAISE CAN_NOT_FIND_OFFER_OR_LICENSE;
15:15:36 1500  	 END;
15:15:36 1501  
15:15:36 1502  	 IF var_current_license_end_date < in_effective_start_date THEN
15:15:36 1503  	   RAISE EXTENS_START_DATE_IS_TOO_FAR;
15:15:36 1504  	 END IF;
15:15:36 1505  
15:15:36 1506  	 IF var_current_license_start_date > in_effective_start_date THEN
15:15:36 1507  	   RAISE EXTENS_START_DATE_IS_TOO_SMALL;
15:15:36 1508  	 END IF;
15:15:36 1509  
15:15:36 1510  	 IF in_effective_start_date > in_effective_end_date THEN
15:15:36 1511  	   RAISE EXT_START_DATE_LATER_THEN_END;
15:15:36 1512  	 END IF;
15:15:36 1513  
15:15:36 1514  	 -- Closing curent license
15:15:36 1515  	 BEGIN
15:15:36 1516  	   PROCS_LICENSE_CRU_V21.UPDATE_LICENSE(
15:15:36 1517  	     in_license_id	   => var_current_license_id,
