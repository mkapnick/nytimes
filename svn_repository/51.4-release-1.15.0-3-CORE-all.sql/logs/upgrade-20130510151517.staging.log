15:15:36 2402  	     var_license_to_disgrace := NULL;
15:15:36 2403  	 END;
15:15:36 2404  
15:15:36 2405  	 IF var_license_to_disgrace IS NOT NULL THEN
15:15:36 2406  	   PROCS_LICENSE_CRU_V21.UPDATE_LICENSE(
15:15:36 2407  	     in_license_id	     => var_license_to_disgrace,
15:15:36 2408  	     in_license_status_id    => GLOBAL_STATUSES_V21.LICENSE_CLOSED,
15:15:36 2409  	     in_updated_by	     => in_updated_by
15:15:36 2410  	   );
15:15:36 2411  	 END IF;
15:15:36 2412  
15:15:36 2413  	 -- Annotate subscription
15:15:36 2414  	 IF in_note IS NOT NULL THEN
15:15:36 2415  	   BEGIN
15:15:36 2416  	     PROCS_SUBSCRIPTION_V21.ANNOTATE_SUBSCRIPTION(
15:15:36 2417  	       in_subscription_id,
15:15:36 2418  	       in_agent_id,
15:15:36 2419  	       in_note,
15:15:36 2420  	       in_updated_by
15:15:36 2421  	     );
15:15:36 2422  	     EXCEPTION
15:15:36 2423  	      WHEN OTHERS THEN
15:15:36 2424  		EXCEPTION_MESSAGE := SQLERRM;
15:15:36 2425  		RAISE CAN_NOT_CREATE_NOTE;
15:15:36 2426  	   END;
15:15:36 2427  	 END IF;
15:15:36 2428  
15:15:36 2429  EXCEPTION
15:15:36 2430  WHEN BAD_SUBSCRIPTION_ID THEN
15:15:36 2431  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 2432  	   SPROC_NAME, 'No such subscription');
15:15:36 2433  WHEN BAD_SUBSCRIPTION_STATUS THEN
15:15:36 2434  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INVALID_PARAMETER,
15:15:36 2435  	   SPROC_NAME, 'Bad current subscription status');
15:15:36 2436  WHEN BAD_CANCELATION_REASON THEN
15:15:36 2437  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INVALID_PARAMETER,
15:15:36 2438  	   SPROC_NAME, 'Bad cancellation reason');
15:15:36 2439  WHEN CAN_NOT_UPDATE_SUBSCRIPTION THEN
15:15:36 2440  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:36 2441  	   SPROC_NAME, 'Could not update subscription data', EXCEPTION_MESSAGE);
15:15:36 2442  WHEN ACTIVE_INVOICES_FOUND THEN
15:15:36 2443  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
15:15:36 2444  	   SPROC_NAME, 'Invoices with active licenses found');
15:15:36 2445  WHEN CAN_NOT_CREATE_NOTE THEN
15:15:36 2446  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:36 2447  	   SPROC_NAME, 'Could not annotate subscription', EXCEPTION_MESSAGE);
15:15:36 2448  --WHEN OTHERS THEN
15:15:36 2449  --  PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 2450  --    SPROC_NAME, 'Unknown error', SQLERRM);
15:15:36 2451  END FINALIZE_CANCELATION;
