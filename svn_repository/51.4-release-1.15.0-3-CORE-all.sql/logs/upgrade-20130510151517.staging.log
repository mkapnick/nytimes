15:15:35   3  PROCEDURE ADD_LINE_ITEMS(
15:15:35   4  /*
15:15:35   5  Throws exceptions:
15:15:35   6  APP_EXCEPTION_CODES_V21.NOT_FOUND
15:15:35   7  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
15:15:35   8  */
15:15:35   9  	in_invoice_id IN NUMBER,
15:15:35  10  	in_offer_id   IN NUMBER,
15:15:35  11  	in_created_by IN VARCHAR2
15:15:35  12  ) AS
15:15:35  13  -- VARIABLES
15:15:35  14  SPROC_NAME      CONSTANT VARCHAR2(14) := 'ADD_LINE_ITEMS';
15:15:35  15  temp_invoice_id NUMBER;
15:15:35  16  temp_offer_id   NUMBER;
15:15:35  17  
15:15:35  18  var_line_item_data SYS_REFCURSOR;
15:15:35  19  var_new_line_item_id NUMBER;
15:15:35  20  var_product_unit_price NUMBER (10,6);
15:15:35  21  var_product_offering_price NUMBER(10,6);
15:15:35  22  var_product_offering_oprice NUMBER(10,6);
15:15:35  23  var_product_quantity NUMBER;
15:15:35  24  var_product_offering_id NUMBER;
15:15:35  25  
15:15:35  26  var_line_item_price	  NUMBER(10,2);
15:15:35  27  var_discount_fixed_amount   NUMBER(10,6);
15:15:35  28  var_discount_percent_amount NUMBER(10,2);
15:15:35  29  
15:15:35  30  
15:15:35  31  -- EXCEPTIONS
15:15:35  32  BAD_INVOICE_ID EXCEPTION;
15:15:35  33  BAD_OFFER_ID EXCEPTION;
15:15:35  34  BAD_DISCOUNT EXCEPTION;
15:15:35  35  BEGIN
15:15:35  36  
15:15:35  37  	-- Check that given invoice exists
15:15:35  38  	BEGIN
15:15:35  39  	  SELECT
15:15:35  40  	    INVOICE.ID into temp_invoice_id
15:15:35  41  	  FROM
15:15:35  42  	    INVOICE
15:15:35  43  	  WHERE
15:15:35  44  	    INVOICE.ID = in_invoice_id;
15:15:35  45  
15:15:35  46  	  EXCEPTION
15:15:35  47  	    WHEN NO_DATA_FOUND THEN
15:15:35  48  	      RAISE BAD_INVOICE_ID;
15:15:35  49  	END;
15:15:35  50  
15:15:35  51  	-- Check that given offer exists
15:15:35  52  	BEGIN
15:15:35  53  	  SELECT
15:15:35  54  	    OFFER.ID into temp_offer_id
15:15:35  55  	  FROM
15:15:35  56  	    OFFER
15:15:35  57  	  WHERE
15:15:35  58  	    OFFER.ID = in_offer_id;
15:15:35  59  
15:15:35  60  	  EXCEPTION
15:15:35  61  	    WHEN NO_DATA_FOUND THEN
15:15:35  62  	      RAISE BAD_OFFER_ID;
15:15:35  63  	END;
15:15:35  64  
15:15:35  65  	-- Get product_offering data
15:15:35  66  	OPEN var_line_item_data FOR
15:15:35  67  	SELECT
15:15:35  68  	  PRODUCT_OFFERING.ID,
15:15:35  69  	  PRODUCT_OFFERING.UNIT_PRICE,
15:15:35  70  	  PRODUCT_OFFERING.QUANTITY
15:15:35  71  	FROM
15:15:35  72  	  OFFER_PRODUCT_OFFERING
15:15:35  73  	  INNER JOIN PRODUCT_OFFERING ON OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = PRODUCT_OFFERING.ID
15:15:35  74  	WHERE
15:15:35  75  	  OFFER_PRODUCT_OFFERING.OFFER_ID = in_offer_id;
15:15:35  76  
15:15:35  77  	-- insert line items
15:15:35  78  	-- add discounts to line items
15:15:35  79  	LOOP
15:15:35  80  	  FETCH var_line_item_data INTO
15:15:35  81  	    var_product_offering_id,
15:15:35  82  	    var_product_unit_price,
15:15:35  83  	    var_product_quantity;
15:15:35  84  	  EXIT WHEN var_line_item_data%NOTFOUND;
15:15:35  85  
15:15:35  86  	  var_product_offering_oprice := var_product_unit_price * var_product_quantity;
15:15:35  87  	  var_product_offering_price := var_product_offering_oprice;
15:15:35  88  
15:15:35  89  	  -- Apply discounts to line_item
15:15:35  90  	  BEGIN
15:15:35  91  	    SELECT
15:15:35  92  	      SUM (DISCOUNT.FIXED_AMOUNT) into var_discount_fixed_amount
15:15:35  93  	    FROM
15:15:35  94  	      DISCOUNT_PRODUCT_OFFERING
15:15:35  95  	      INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
15:15:35  96  	    WHERE
15:15:35  97  	      DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
15:15:35  98  	      AND DISCOUNT.FIXED_AMOUNT IS NOT NULL;
15:15:35  99  	    EXCEPTION
15:15:35 100  	    WHEN NO_DATA_FOUND THEN
15:15:35 101  	      var_discount_fixed_amount := NULL;
15:15:35 102  	  END;
15:15:35 103  
15:15:35 104  	  BEGIN
15:15:35 105  	    SELECT
15:15:35 106  	      SUM (DISCOUNT.PERCENT_AMOUNT) into var_discount_percent_amount
15:15:35 107  	    FROM
15:15:35 108  	      DISCOUNT_PRODUCT_OFFERING
15:15:35 109  	      INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
15:15:35 110  	    WHERE
15:15:35 111  	      DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
15:15:35 112  	      AND DISCOUNT.PERCENT_AMOUNT IS NOT NULL;
15:15:35 113  	    EXCEPTION
15:15:35 114  	    WHEN NO_DATA_FOUND THEN
15:15:35 115  	      var_discount_percent_amount := NULL;
15:15:35 116  	  END;
15:15:35 117  
15:15:35 118  	  IF (var_discount_percent_amount IS NOT NULL) THEN
15:15:35 119  	    var_product_offering_price := var_product_offering_price * ( 1 - var_discount_percent_amount / 100 );
15:15:35 120  	  END IF;
15:15:35 121  
15:15:35 122  	  IF (var_discount_fixed_amount IS NOT NULL) THEN
15:15:35 123  	    var_product_offering_price := var_product_offering_price - var_discount_fixed_amount;
15:15:35 124  	  END IF;
15:15:35 125  
15:15:35 126  	  var_line_item_price := PROCS_COMMON_V21.ROUND_10_6_TO_10_2(var_product_offering_price);
15:15:35 127  
15:15:35 128  	  IF (var_line_item_price < 0) THEN
15:15:35 129  	      RAISE BAD_DISCOUNT;
15:15:35 130  	  END IF;
15:15:35 131  
15:15:35 132  	  var_new_line_item_id := NULL;
15:15:35 133  	  PROCS_LINE_ITEMS_CRU_V21.CREATE_LINE_ITEM(
15:15:35 134  	    inout_line_item_id	=> var_new_line_item_id,
15:15:35 135  	    in_product_offer_id => var_product_offering_id,
15:15:35 136  	    in_invoice_id	=> in_invoice_id,
15:15:35 137  	    in_amount		=> var_line_item_price,
15:15:35 138  	    in_created_by	=> in_created_by,
15:15:35 139  	    in_discount_amount	=> var_product_offering_oprice - var_line_item_price,
15:15:35 140  	    in_taxes_amount	=> NULL
15:15:35 141  	  );
15:15:35 142  
15:15:35 143  	  FOR f_discount IN (
15:15:35 144  	    SELECT
15:15:35 145  	      DISCOUNT.ID
15:15:35 146  	    FROM
15:15:35 147  	      DISCOUNT_PRODUCT_OFFERING
15:15:35 148  	      INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
15:15:35 149  	    WHERE
15:15:35 150  	      DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
15:15:35 151  	  )
15:15:35 152  	  LOOP
15:15:35 153  	    PROCS_LINE_ITEMS_CRU_V21.CREATE_DISCOUNT_LINE_ITEM(
15:15:35 154  	      in_discount_id =>  f_discount.ID,
15:15:35 155  	      in_line_item_id => var_new_line_item_id
15:15:35 156  	    );
15:15:35 157  	  END LOOP;
15:15:35 158  	END LOOP;
15:15:35 159  
15:15:35 160  EXCEPTION
15:15:35 161  WHEN BAD_INVOICE_ID THEN
15:15:35 162  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:35 163  	  SPROC_NAME, 'No such license');
15:15:35 164  WHEN BAD_OFFER_ID THEN
15:15:35 165  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:35 166  	  SPROC_NAME, 'No such offer');
15:15:35 167  WHEN BAD_DISCOUNT THEN
15:15:35 168  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:35 169  	  SPROC_NAME, 'Bad Discount');
15:15:35 170  WHEN OTHERS THEN
15:15:35 171  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:35 172  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:15:35 173  END ADD_LINE_ITEMS;
