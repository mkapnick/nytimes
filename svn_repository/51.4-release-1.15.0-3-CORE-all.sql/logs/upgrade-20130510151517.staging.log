15:15:36 2113  
15:15:36 2114  PROCEDURE CANCEL_SUBSCRIPTION_INVOICE (
15:15:36 2115  /*
15:15:36 2116  APP_EXCEPTION_CODES_V21.NOT_FOUND
15:15:36 2117  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
15:15:36 2118  APP_EXCEPTION_CODES_V21.INTERNAL_ERROR
15:15:36 2119  */
15:15:36 2120  	 in_invoice_id	      IN NUMBER,
15:15:36 2121  	 in_updated_by	      IN VARCHAR2,
15:15:36 2122  	 in_refundable	      IN NUMBER DEFAULT GLOBAL_CONSTANTS_V21.FALSE
15:15:36 2123  ) AS
15:15:36 2124  SPROC_NAME CONSTANT VARCHAR2(27) := 'CANCEL_SUBSCRIPTION_INVOICE';
15:15:36 2125  -- VARIABLES
15:15:36 2126  temp_invoice_id	      INVOICE.ID%TYPE;
15:15:36 2127  var_chargeback_amount  NUMBER(10,2);
15:15:36 2128  ver_refund_charge_id   NUMBER;
15:15:36 2129  -- EXCEPTIONS
15:15:36 2130  BAD_INVOICE_ID		    EXCEPTION;
15:15:36 2131  CAN_NOT_CALCULATE_CHARGEBACK EXCEPTION;
15:15:36 2132  CAN_NOT_APPLY_CHARGEBACK     EXCEPTION;
15:15:36 2133  EXCEPTION_MESSAGE	    VARCHAR2(1024);
15:15:36 2134  -- STUB
15:15:36 2135  var_now DATE;
15:15:36 2136  var_revoke NUMBER;
15:15:36 2137  var_refund NUMBER;
15:15:36 2138  var_billed NUMBER;
15:15:36 2139  var_subscription_in_grace NUMBER;
15:15:36 2140  BEGIN
15:15:36 2141  
15:15:36 2142  	 var_now := sysdate;
15:15:36 2143  
15:15:36 2144  	 -- Check that invoice exists
15:15:36 2145  	 BEGIN
15:15:36 2146  	   SELECT
15:15:36 2147  	     INVOICE.ID into temp_invoice_id
15:15:36 2148  	   FROM
15:15:36 2149  	     INVOICE
15:15:36 2150  	   WHERE
15:15:36 2151  	     INVOICE.ID = in_invoice_id;
15:15:36 2152  	   EXCEPTION
15:15:36 2153  	     WHEN NO_DATA_FOUND THEN
15:15:36 2154  	       RAISE BAD_INVOICE_ID;
15:15:36 2155  	 END;
15:15:36 2156  
15:15:36 2157  	 select decode(count(1), 0, GLOBAL_CONSTANTS_V21.FALSE, GLOBAL_CONSTANTS_V21.TRUE) into var_revoke
15:15:36 2158  	 from license l, subscription s, offer_chain oc
15:15:36 2159  	 where
15:15:36 2160  	   l.subscription_id = s.id and
15:15:36 2161  	   s.offer_chain_id = oc.id and
15:15:36 2162  	   l.invoice_id = in_invoice_id and
15:15:36 2163  	   oc.revoke_entitlements = GLOBAL_CONSTANTS_V21.TRUE and
15:15:36 2164  	   rownum < 2;
15:15:36 2165  
15:15:36 2166  	 select
15:15:36 2167  	   decode(s.subscription_status_id, GLOBAL_STATUSES_V21.SUBSCRIPTION_IN_GRACE_PERIOD, 1, 0)
15:15:36 2168  	 into
15:15:36 2169  	   var_subscription_in_grace
15:15:36 2170  	 from license l, subscription s
15:15:36 2171  	 where
15:15:36 2172  	   l.subscription_id = s.id and
15:15:36 2173  	   l.invoice_id = in_invoice_id and
15:15:36 2174  	   rownum < 2;
15:15:36 2175  
15:15:36 2176  	 var_billed := PROCS_INVOICE_V21.IS_INVOICE_PAYING_STARTED(in_invoice_id);
15:15:36 2177  	 var_refund := GLOBAL_CONSTANTS_V21.FALSE;
15:15:36 2178  
15:15:36 2179  	 -- Check that transaction for given invoice not started
15:15:36 2180  	 -- if refund enabled calculate and apply chargeback
15:15:36 2181  	 IF (
15:15:36 2182  	     var_billed = GLOBAL_CONSTANTS_V21.TRUE
15:15:36 2183  	   )THEN
15:15:36 2184  	   if (in_refundable = GLOBAL_CONSTANTS_V21.TRUE) then
15:15:36 2185  	     -- If started then we need to calculate refund
15:15:36 2186  	     BEGIN
15:15:36 2187  	       PROCS_INVOICE_V21.CALCULATE_INVOICE_CHARGEBACK(
15:15:36 2188  		 in_invoice_id,
15:15:36 2189  		 var_now,
15:15:36 2190  		 var_chargeback_amount
15:15:36 2191  	       );
15:15:36 2192  	       EXCEPTION
15:15:36 2193  		 WHEN OTHERS THEN
15:15:36 2194  		   EXCEPTION_MESSAGE := SQLERRM;
15:15:36 2195  		   RAISE CAN_NOT_CALCULATE_CHARGEBACK;
15:15:36 2196  	     END;
15:15:36 2197  	     IF var_chargeback_amount > 0 THEN
15:15:36 2198  	       BEGIN
15:15:36 2199  		 PROCS_INVOICE_V21.APPLY_REFUND(
15:15:36 2200  		   in_invoice_id,
15:15:36 2201  		   var_chargeback_amount,
15:15:36 2202  		   in_updated_by,
15:15:36 2203  		   ver_refund_charge_id
15:15:36 2204  		 );
15:15:36 2205  		 EXCEPTION
15:15:36 2206  		   WHEN OTHERS THEN
15:15:36 2207  		     EXCEPTION_MESSAGE := SQLERRM;
15:15:36 2208  		     RAISE CAN_NOT_APPLY_CHARGEBACK;
15:15:36 2209  	       END;
15:15:36 2210  	       var_refund := GLOBAL_CONSTANTS_V21.TRUE;
15:15:36 2211  	     END IF;
15:15:36 2212  	   end if;
15:15:36 2213  	 ELSE
15:15:36 2214  
15:15:36 2215  	   FOR f_transaction_to_close IN (
15:15:36 2216  	     SELECT DISTINCT CHARGE.TRANSACTION_ID AS "ID" FROM CHARGE WHERE CHARGE.INVOICE_ID = in_invoice_id and CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V21.CHARGE_OPENED
15:15:36 2217  	   )
15:15:36 2218  	   LOOP
15:15:36 2219  	     PROCS_TRANSACTION_V21.UPDATE_TRANSACTION_STATUS(
15:15:36 2220  	       in_transaction_id	=> f_transaction_to_close.ID,
15:15:36 2221  	       in_updated_by		=> in_updated_by,
15:15:36 2222  	       in_transaction_status_id => GLOBAL_STATUSES_V21.TRANSACTION_CLOSED
15:15:36 2223  	     );
15:15:36 2224  	   END LOOP;
15:15:36 2225  	   -- Needs to close charges. No refund.
15:15:36 2226  	   FOR f_charge_to_close IN (
15:15:36 2227  	     SELECT CHARGE.ID FROM CHARGE WHERE CHARGE.INVOICE_ID = in_invoice_id and CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V21.CHARGE_OPENED
15:15:36 2228  	   )
15:15:36 2229  	   LOOP
15:15:36 2230  	     PROCS_CHARGE_V21.UPDATE_CHARGE_STATUS(
15:15:36 2231  	       in_charge_id	   => f_charge_to_close.ID,
15:15:36 2232  	       in_updated_by	   => in_updated_by,
15:15:36 2233  	       in_charge_status_id => GLOBAL_STATUSES_V21.CHARGE_CANCELED
15:15:36 2234  	     );
15:15:36 2235  	   END LOOP;
15:15:36 2236  
15:15:36 2237  	   PROCS_INVOICE_V21.UPDATE_INVOICE_STATUS(
15:15:36 2238  	     in_invoice_id		    => in_invoice_id,
15:15:36 2239  	     in_updated_by		    => in_updated_by,
15:15:36 2240  	     in_invoice_status_id	    => GLOBAL_STATUSES_V21.INVOICE_CLOSED
15:15:36 2241  	   );
15:15:36 2242  
15:15:36 2243  	 END IF;
15:15:36 2244  	 -- update licenses
15:15:36 2245  	 IF(var_revoke = GLOBAL_CONSTANTS_V21.TRUE OR var_chargeback_amount > 0 OR (var_subscription_in_grace = GLOBAL_CONSTANTS_V21.FALSE AND var_billed = GLOBAL_CONSTANTS_V21.FALSE)) THEN
15:15:36 2246  	   FOR f_license_to_cancel IN (
15:15:36 2247  	     SELECT LICENSE.ID FROM LICENSE WHERE LICENSE.INVOICE_ID = in_invoice_id AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V21.LICENSE_ACTIVE
15:15:36 2248  	   )
15:15:36 2249  	   LOOP
15:15:36 2250  	     PROCS_LICENSE_CRU_V21.UPDATE_LICENSE(
15:15:36 2251  	       in_license_id	     => f_license_to_cancel.ID,
15:15:36 2252  	       in_license_status_id  => GLOBAL_STATUSES_V21.LICENSE_CLOSED,
15:15:36 2253  	       in_needs_entitlements => GLOBAL_CONSTANTS_V21.TRUE,
15:15:36 2254  	       in_updated_by	     => in_updated_by,
15:15:36 2255  	       in_entitlement_end_date => var_now
15:15:36 2256  	     );
15:15:36 2257  	   END LOOP;
15:15:36 2258  	 ELSE
15:15:36 2259  	   FOR f_license_to_cancel IN (
15:15:36 2260  	     SELECT LICENSE.ID FROM LICENSE WHERE LICENSE.INVOICE_ID = in_invoice_id AND LICENSE.LICENSE_STATUS_ID in (GLOBAL_STATUSES_V21.LICENSE_ACTIVE, GLOBAL_STATUSES_V21.LICENSE_IN_GRACE_PERIOD)
15:15:36 2261  	   )
15:15:36 2262  	   LOOP
15:15:36 2263  	     PROCS_LICENSE_CRU_V21.UPDATE_LICENSE(
15:15:36 2264  	       in_license_id	     => f_license_to_cancel.ID,
15:15:36 2265  	       in_license_status_id  => GLOBAL_STATUSES_V21.LICENSE_CLOSED,
15:15:36 2266  	       in_updated_by	     => in_updated_by
15:15:36 2267  	     );
15:15:36 2268  	   END LOOP;
15:15:36 2269  	 END IF;
15:15:36 2270  
15:15:36 2271  
15:15:36 2272  EXCEPTION
15:15:36 2273  WHEN BAD_INVOICE_ID THEN
15:15:36 2274  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
15:15:36 2275  	   SPROC_NAME, 'No such invoice');
15:15:36 2276  WHEN CAN_NOT_CALCULATE_CHARGEBACK THEN
15:15:36 2277  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:36 2278  	   SPROC_NAME, 'Could not calculate invoice refund', EXCEPTION_MESSAGE);
15:15:36 2279  WHEN CAN_NOT_APPLY_CHARGEBACK THEN
15:15:36 2280  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
15:15:36 2281  	   SPROC_NAME, 'Could not apply chargeback', EXCEPTION_MESSAGE);
15:15:36 2282  WHEN OTHERS THEN
15:15:36 2283  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
15:15:36 2284  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:15:36 2285  END CANCEL_SUBSCRIPTION_INVOICE;
15:15:36 2286  
15:15:36 2287  /******************************************************************************/
15:15:36 2288  
15:15:36 2289  PROCEDURE FINALIZE_CANCELATION (
15:15:36 2290  /*
15:15:36 2291  APP_EXCEPTION_CODES_V21.NOT_FOUND
15:15:36 2292  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
15:15:36 2293  APP_EXCEPTION_CODES_V21.INTERNAL_ERROR
15:15:36 2294  */
15:15:36 2295  --  in_subscription_id	 IN SUBSCRIPTION.ID%TYPE,
15:15:36 2296  --  in_cancelation_reason IN SUBSCRIPTION_CANCEL_REASON.VALUE%TYPE,
15:15:36 2297  --  in_cancelation_date	 IN SUBSCRIPTION.CANCELLATION_DATE%TYPE,
15:15:36 2298  --  in_note		 IN SUBSCRIPTION_NOTE.NOTE%TYPE,
15:15:36 2299  --  in_agent_id		 IN SUBSCRIPTION_NOTE.AGENT_ID%TYPE,
15:15:36 2300  --  in_updated_by	 IN SUBSCRIPTION.UPDATED_BY%TYPE
15:15:36 2301  	 in_subscription_id    IN NUMBER,
15:15:36 2302  	 in_cancelation_reason IN VARCHAR2,
15:15:36 2303  	 in_cancelation_date   IN DATE,
15:15:36 2304  	 in_note	       IN VARCHAR2,
15:15:36 2305  	 in_agent_id	       IN NUMBER,
15:15:36 2306  	 in_updated_by	       IN VARCHAR2
15:15:36 2307  ) AS
15:15:36 2308  SPROC_NAME CONSTANT VARCHAR2(20) := 'FINALIZE_CANCELATION';
15:15:36 2309  -- VARIABLES
15:15:36 2310  var_current_subscr_status SUBSCRIPTION.SUBSCRIPTION_STATUS_ID%TYPE;
15:15:36 2311  var_sct_id		 SUBSCRIPTION.SCT_ID%TYPE;
15:15:36 2312  var_active_invoices_count NUMBER;
15:15:36 2313  var_license_to_disgrace	 LICENSE.ID%TYPE;
15:15:36 2314  var_now			 DATE := SYSDATE;
15:15:36 2315  -- EXCEPTIONS
15:15:36 2316  BAD_SUBSCRIPTION_ID	   EXCEPTION;
15:15:36 2317  BAD_SUBSCRIPTION_STATUS	   EXCEPTION;
15:15:36 2318  BAD_CANCELATION_REASON	   EXCEPTION;
15:15:36 2319  CAN_NOT_UPDATE_SUBSCRIPTION EXCEPTION;
15:15:36 2320  ACTIVE_INVOICES_FOUND	   EXCEPTION;
15:15:36 2321  CAN_NOT_CREATE_NOTE	   EXCEPTION;
15:15:36 2322  EXCEPTION_MESSAGE	   VARCHAR2(1024);
15:15:36 2323  BEGIN
15:15:36 2324  
15:15:36 2325  	 -- Get current subscription status
15:15:36 2326  	 BEGIN
15:15:36 2327  	   SELECT
15:15:36 2328  	     SUBSCRIPTION.SUBSCRIPTION_STATUS_ID into var_current_subscr_status
15:15:36 2329  	   FROM
15:15:36 2330  	     SUBSCRIPTION
15:15:36 2331  	   WHERE
15:15:36 2332  	     SUBSCRIPTION.ID = in_subscription_id;
15:15:36 2333  	   EXCEPTION
15:15:36 2334  	     WHEN NO_DATA_FOUND THEN
15:15:36 2335  	       RAISE BAD_SUBSCRIPTION_ID;
15:15:36 2336  	 END;
15:15:36 2337  
15:15:36 2338  	 -- Check that subscription reason is correct
15:15:36 2339  	 BEGIN
15:15:36 2340  	   SELECT
15:15:36 2341  	     SUBSCRIPTION_CANCEL_REASON.ID into var_sct_id
15:15:36 2342  	   FROM
15:15:36 2343  	     SUBSCRIPTION_CANCEL_REASON
15:15:36 2344  	   WHERE
15:15:36 2345  	     SUBSCRIPTION_CANCEL_REASON.VALUE LIKE in_cancelation_reason
15:15:36 2346  	     AND ROWNUM <= 1;
15:15:36 2347  	   EXCEPTION
15:15:36 2348  	     WHEN NO_DATA_FOUND THEN
15:15:36 2349  	       RAISE BAD_CANCELATION_REASON;
15:15:36 2350  	 END;
15:15:36 2351  
15:15:36 2352  	 -- Check for invoices with active licenses
15:15:36 2353  	 SELECT
15:15:36 2354  	   COUNT(*) into var_active_invoices_count
15:15:36 2355  	 FROM
15:15:36 2356  	   LICENSE
15:15:36 2357  	   INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
15:15:36 2358  	 WHERE
15:15:36 2359  	   LICENSE.LICENSE_STATUS_ID in (GLOBAL_STATUSES_V21.LICENSE_ACTIVE, GLOBAL_STATUSES_V21.LICENSE_IN_GRACE_PERIOD)
15:15:36 2360  	   AND LICENSE.SUBSCRIPTION_ID = in_subscription_id;
15:15:36 2361  
15:15:36 2362  	 IF var_active_invoices_count > 0 THEN
15:15:36 2363  	   RAISE ACTIVE_INVOICES_FOUND;
15:15:36 2364  	 END IF;
15:15:36 2365  
15:15:36 2366  	 -- Check that subscription is active
15:15:36 2367  	 IF var_current_subscr_status != GLOBAL_STATUSES_V21.SUBSCRIPTION_ACTIVE AND
15:15:36 2368  	    var_current_subscr_status != GLOBAL_STATUSES_V21.SUBSCRIPTION_SUSPENDED AND
15:15:36 2369  	    var_current_subscr_status != GLOBAL_STATUSES_V21.SUBSCRIPTION_IN_GRACE_PERIOD THEN
15:15:36 2370  	   RAISE BAD_SUBSCRIPTION_STATUS;
15:15:36 2371  	 END IF;
15:15:36 2372  
15:15:36 2373  	 -- Update subscription data
15:15:36 2374  	 BEGIN
15:15:36 2375  	   PROCS_SUBSCRIPTION_CRU_V21.UPDATE_SUBSCRIPTION(
15:15:36 2376  	     in_subscription_id        => in_subscription_id,
15:15:36 2377  	     in_subscription_status_id => GLOBAL_STATUSES_V21.SUBSCRIPTION_CANCELED,
15:15:36 2378  	     in_cancelation_date       => in_cancelation_date,
15:15:36 2379  	     in_updated_by	       => in_updated_by,
15:15:36 2380  	     in_sct_id		       => var_sct_id
15:15:36 2381  	   );
15:15:36 2382  	   EXCEPTION
