16:23:20 2350  	 END;
16:23:20 2351  
16:23:20 2352  	 -- Check for invoices with active licenses
16:23:20 2353  	 SELECT
16:23:20 2354  	   COUNT(*) into var_active_invoices_count
16:23:20 2355  	 FROM
