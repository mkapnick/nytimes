16:23:20 2403  	 END;
16:23:20 2404  
16:23:20 2405  	 IF var_license_to_disgrace IS NOT NULL THEN
16:23:20 2406  	   PROCS_LICENSE_CRU_V19.UPDATE_LICENSE(
16:23:20 2407  	     in_license_id	     => var_license_to_disgrace,
16:23:20 2408  	     in_license_status_id    => GLOBAL_STATUSES_V19.LICENSE_CLOSED,
16:23:20 2409  	     in_updated_by	     => in_updated_by
16:23:20 2410  	   );
16:23:20 2411  	 END IF;
16:23:20 2412  
16:23:20 2413  	 -- Annotate subscription
16:23:20 2414  	 IF in_note IS NOT NULL THEN
16:23:20 2415  	   BEGIN
16:23:20 2416  	     PROCS_SUBSCRIPTION_V19.ANNOTATE_SUBSCRIPTION(
16:23:20 2417  	       in_subscription_id,
16:23:20 2418  	       in_agent_id,
16:23:20 2419  	       in_note,
16:23:20 2420  	       in_updated_by
16:23:20 2421  	     );
16:23:20 2422  	     EXCEPTION
16:23:20 2423  	      WHEN OTHERS THEN
16:23:20 2424  		EXCEPTION_MESSAGE := SQLERRM;
16:23:20 2425  		RAISE CAN_NOT_CREATE_NOTE;
16:23:20 2426  	   END;
16:23:20 2427  	 END IF;
16:23:20 2428  
16:23:20 2429  EXCEPTION
