16:23:20 2747  	 ELSIF in_new_fin_instrument_type = GLOBAL_ENUMS_V19.INSTRUMENT_PAYPAL THEN
16:23:20 2748  	   IF PROCS_FIN_INSTRUMENTS_V19.IS_PAYPAL_EXISTS(in_new_fin_instrument_id) = GLOBAL_CONSTANTS_V19.FALSE THEN
16:23:20 2749  	     -- throw exception: bad new paypal
16:23:20 2750  	     RAISE BAD_NEW_PP;
16:23:20 2751  	   END IF;
16:23:20 2752  	 ELSE
16:23:20 2753  	   -- throw exception: bad new instrument type
