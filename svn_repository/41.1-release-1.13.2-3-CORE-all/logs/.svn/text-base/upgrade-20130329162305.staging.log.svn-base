16:23:20 388  	  SPROC_NAME, 'No such offer chain');
16:23:20 389  WHEN PRODUCT_ALREADY_PURCHASED THEN
16:23:20 390  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.CONFLICT_ERROR,
16:23:20 391  	  SPROC_NAME, 'User already subscribed to some product in given offer chain. Concurrent subscription id: ' || var_concur_subscription_id);
16:23:20 392  WHEN CAN_NOT_GET_FIRST_OFFER_CHAIN THEN
16:23:20 393  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 394  	  SPROC_NAME, 'Could not find first offer in offer chain', EXCEPTION_MESSAGE);
16:23:20 395  WHEN CAN_NOT_CREATE_INVOICE THEN
16:23:20 396  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 397  	  SPROC_NAME, 'Could not create invoice', EXCEPTION_MESSAGE);
16:23:20 398  WHEN CAN_NOT_CREATE_LINE_ITEMS THEN
