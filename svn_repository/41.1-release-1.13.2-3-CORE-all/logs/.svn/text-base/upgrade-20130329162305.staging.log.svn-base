16:23:20 2914  SPROC_NAME CONSTANT VARCHAR2(28) := 'GET_EXISTING_SUBSCR_NUMBER';
16:23:20 2915  -- VARIABLES
16:23:20 2916  temp_acct_id	    NUMBER;
16:23:20 2917  temp_oc_id	    NUMBER;
16:23:20 2918  -- EXCEPTIONS
