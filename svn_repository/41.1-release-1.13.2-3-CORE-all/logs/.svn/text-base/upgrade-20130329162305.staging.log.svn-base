16:23:20 777  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 778  	  SPROC_NAME, 'Unknown error', SQLERRM);
