16:23:20 2295  --  in_subscription_id	 IN SUBSCRIPTION.ID%TYPE,
16:23:20 2296  --  in_cancelation_reason IN SUBSCRIPTION_CANCEL_REASON.VALUE%TYPE,
16:23:20 2297  --  in_cancelation_date	 IN SUBSCRIPTION.CANCELLATION_DATE%TYPE,
16:23:20 2298  --  in_note		 IN SUBSCRIPTION_NOTE.NOTE%TYPE,
16:23:20 2299  --  in_agent_id		 IN SUBSCRIPTION_NOTE.AGENT_ID%TYPE,
16:23:20 2300  --  in_updated_by	 IN SUBSCRIPTION.UPDATED_BY%TYPE
16:23:20 2301  	 in_subscription_id    IN NUMBER,
16:23:20 2302  	 in_cancelation_reason IN VARCHAR2,
16:23:20 2303  	 in_cancelation_date   IN DATE,
16:23:20 2304  	 in_note	       IN VARCHAR2,
16:23:20 2305  	 in_agent_id	       IN NUMBER,
16:23:20 2306  	 in_updated_by	       IN VARCHAR2
16:23:20 2307  ) AS
16:23:20 2308  SPROC_NAME CONSTANT VARCHAR2(20) := 'FINALIZE_CANCELATION';
16:23:20 2309  -- VARIABLES
16:23:20 2310  var_current_subscr_status SUBSCRIPTION.SUBSCRIPTION_STATUS_ID%TYPE;
16:23:20 2311  var_sct_id		 SUBSCRIPTION.SCT_ID%TYPE;
16:23:20 2312  var_active_invoices_count NUMBER;
16:23:20 2313  var_license_to_disgrace	 LICENSE.ID%TYPE;
16:23:20 2314  var_now			 DATE := SYSDATE;
16:23:20 2315  -- EXCEPTIONS
16:23:20 2316  BAD_SUBSCRIPTION_ID	   EXCEPTION;
16:23:20 2317  BAD_SUBSCRIPTION_STATUS	   EXCEPTION;
16:23:20 2318  BAD_CANCELATION_REASON	   EXCEPTION;
16:23:20 2319  CAN_NOT_UPDATE_SUBSCRIPTION EXCEPTION;
16:23:20 2320  ACTIVE_INVOICES_FOUND	   EXCEPTION;
16:23:20 2321  CAN_NOT_CREATE_NOTE	   EXCEPTION;
16:23:20 2322  EXCEPTION_MESSAGE	   VARCHAR2(1024);
16:23:20 2323  BEGIN
16:23:20 2324  
16:23:20 2325  	 -- Get current subscription status
16:23:20 2326  	 BEGIN
16:23:20 2327  	   SELECT
16:23:20 2328  	     SUBSCRIPTION.SUBSCRIPTION_STATUS_ID into var_current_subscr_status
16:23:20 2329  	   FROM
16:23:20 2330  	     SUBSCRIPTION
16:23:20 2331  	   WHERE
16:23:20 2332  	     SUBSCRIPTION.ID = in_subscription_id;
