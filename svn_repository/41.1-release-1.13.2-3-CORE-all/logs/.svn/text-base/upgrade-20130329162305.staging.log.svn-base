16:23:20 1648  	   SPROC_NAME, 'No such invoice');
16:23:20 1649  WHEN CAN_NOT_UPDATE_INVOCIE THEN
16:23:20 1650  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 1651  	   SPROC_NAME, 'Could not update invoice', EXCEPTION_MESSAGE);
16:23:20 1652  WHEN OTHERS THEN
16:23:20 1653  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 1654  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 1655  END SET_IS_TAX_CALCULATION_NEEDED;
