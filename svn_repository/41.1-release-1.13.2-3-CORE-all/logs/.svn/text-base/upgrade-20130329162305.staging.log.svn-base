16:23:20 2956  	     OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V19.SUBSCRIPTION_IN_GRACE_PERIOD);
16:23:20 2957  
16:23:20 2958  EXCEPTION
16:23:20 2959  WHEN BAD_GROUP_ID THEN
16:23:20 2960  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 2961  	   SPROC_NAME, 'No such transaction id');
16:23:20 2962  WHEN BAD_OFFER_CHAIN_ID THEN
16:23:20 2963  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
