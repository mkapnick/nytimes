16:23:20 2843  	   ) LOOP
16:23:20 2844  
16:23:20 2845  	     SELECT
16:23:20 2846  	       DECODE(TRANSACTION_TYPE_CODE, 'RECURRING_BILLING', 'RECURRING_BILLING_USER_UPDATE',
16:23:20 2847  					     'GRACE_PERIOD_FINAL', 'GRACE_PERIOD_USER_UPDATE',
16:23:20 2848  					     TRANSACTION_TYPE_CODE)
16:23:20 2849  	     INTO var_transaction_type
16:23:20 2850  	     FROM
16:23:20 2851  	       Transaction
16:23:20 2852  	     WHERE
16:23:20 2853  	       id = f_pending_transaction.id
16:23:20 2854  	       AND ROWNUM <= 1;
16:23:20 2855  
16:23:20 2856  	     PROCS_TRANSACTION_V19.CREATE_TRANSACTION (
16:23:20 2857  	       in_transaction_id => NULL,
16:23:20 2858  	       in_status_id  => GLOBAL_STATUSES_V19.TRANSACTION_PENDING,
16:23:20 2859  	       in_amount     => f_pending_transaction.transaction_amount,
16:23:20 2860  	       in_created_by => in_updated_by,
16:23:20 2861  	       in_order_id   => null,
16:23:20 2862  	       in_is_refund  => f_pending_transaction.is_refund,
16:23:20 2863  	       in_transaction_type_code => var_transaction_type,
16:23:20 2864  	       out_transaction_id => temp_out_transaction_id
16:23:20 2865  	     );
16:23:20 2866  
16:23:20 2867  	     PROCS_TRANSACTION_V19.UPDATE_TRANSACTION_STATUS(
