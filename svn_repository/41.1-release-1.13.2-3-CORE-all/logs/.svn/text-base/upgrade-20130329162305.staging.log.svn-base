16:23:20 1333  BAD_INVOICE_ID EXCEPTION;
16:23:20 1334  BEGIN
16:23:20 1335  
16:23:20 1336  	 BEGIN
16:23:20 1337  	   SELECT
16:23:20 1338  	     INVOICE.ID into temp_invoice_id
16:23:20 1339  	   FROM
16:23:20 1340  	     INVOICE
16:23:20 1341  	   WHERE
16:23:20 1342  	     INVOICE.ID = in_invoice_id;
16:23:20 1343  	   EXCEPTION
16:23:20 1344  	     WHEN NO_DATA_FOUND THEN
16:23:20 1345  	       RAISE BAD_INVOICE_ID;
16:23:20 1346  	 END;
16:23:20 1347  
16:23:20 1348  	 -- Needs to close charges. No refund.
