16:23:19   5  	PROCEDURE POPULATE_REQUEST_INFO(
16:23:19   6  	  in_hours_prior    IN	NUMBER,
16:23:19   7  	  in_filename	    IN	CC_REQUEST_FILE.FILE_NAME%TYPE,
16:23:19   8  	  in_creator	    IN	CC_REQUEST_FILE.UPDATED_BY%TYPE
16:23:19   9  	) AS
16:23:19  10  	SPROC_NAME CONSTANT VARCHAR2(32) := 'POPULATE_REQUEST_INFO';
16:23:19  11  	var_start_date	    DATE := SYSDATE;
16:23:19  12  	var_end_date	    DATE := var_start_date + (in_hours_prior/24);
16:23:19  13  	var_request_file_id NUMBER := 0;
16:23:19  14  	var_license_count   NUMBER := 0;
16:23:19  15  	var_cc_update_count NUMBER := 0;
16:23:19  16  	BEGIN
16:23:19  17  	    SELECT CC_REQUEST_FILE_ID_SEQ.NEXTVAL INTO var_request_file_id  FROM DUAL;
16:23:19  18  	    INSERT INTO CC_REQUEST_FILE (ID,
16:23:19  19  					 FILE_NAME,
16:23:19  20  					 CC_REQUEST_FILE_STATUS,
16:23:19  21  					 CREATE_DATE,
16:23:19  22  					 CREATED_BY,
16:23:19  23  					 UPDATE_DATE,
16:23:19  24  					 UPDATED_BY)
16:23:19  25  					 VALUES (
16:23:19  26  					 var_request_file_id,
16:23:19  27  					 in_filename,
16:23:19  28  					 'NOT_CREATED',
16:23:19  29  					 var_start_date,
16:23:19  30  					 in_creator,
16:23:19  31  					 var_start_date,
16:23:19  32  					 in_creator);
16:23:19  33  
16:23:19  34  	   FOR record IN (SELECT
16:23:19  35  			    l.ID LICENSE_ID, cc.ID CREDIT_CARD_ID
16:23:19  36  			  FROM
16:23:19  37  			    LICENSE l INNER JOIN SUBSCRIPTION s ON L.SUBSCRIPTION_ID = s.ID
16:23:19  38  				      INNER JOIN CREDIT_CARD cc ON S.INSTRUMENT_ID   = cc.ID
16:23:19  39  			  WHERE
16:23:19  40  			    s.INSTRUMENT_TYPE_ID	 = 1
16:23:19  41  			    AND cc.CREDIT_CARD_STATUS_ID = 1
16:23:19  42  			    AND s.SUBSCRIPTION_STATUS_ID = 1
16:23:19  43  			    AND l.LICENSE_STATUS_ID	 = 2
16:23:19  44  			    AND cc.CREDIT_CARD_TYPE_ID IN (2,3)
16:23:19  45  			    AND l.END_DATE BETWEEN var_start_date AND var_end_date
16:23:19  46  			    AND l.ID NOT IN (SELECT LICENSE_ID FROM CC_UPDATE))
16:23:19  47  	   LOOP
16:23:19  48  	     var_license_count := 0;
16:23:19  49  	     SELECT COUNT(1) INTO  var_license_count FROM CC_UPDATE WHERE LICENSE_ID = record.LICENSE_ID;
16:23:19  50  
16:23:19  51  	     IF var_license_count = 0 THEN
16:23:19  52  		INSERT INTO CC_UPDATE (ID,
16:23:19  53  				       CREDIT_CARD_ID,
16:23:19  54  				       LICENSE_ID,
16:23:19  55  				       CC_UPDATE_STATUS,
16:23:19  56  				       CC_REQUEST_FILE_ID,
16:23:19  57  				       CREATE_DATE,
16:23:19  58  				       UPDATE_DATE,
16:23:19  59  				       CREATED_BY,
16:23:19  60  				       UPDATED_BY
16:23:19  61  				       ) VALUES (
16:23:19  62  				       CC_UPDATE_SEQ.NEXTVAL,
16:23:19  63  				       record.CREDIT_CARD_ID,
16:23:19  64  				       record.LICENSE_ID,
16:23:19  65  				       'NOT_ADDED_TO_FILE',
16:23:19  66  				       var_request_file_id,
16:23:19  67  				       var_start_date,
16:23:19  68  				       var_start_date,
16:23:19  69  				       in_creator,
16:23:19  70  				       in_creator
16:23:19  71  				       );
16:23:19  72  	     END IF;
16:23:19  73  	   END LOOP;
16:23:19  74  
16:23:19  75  	   SELECT COUNT(1) INTO var_cc_update_count
16:23:19  76  	   FROM CC_UPDATE
16:23:19  77  	   WHERE CC_REQUEST_FILE_ID = var_request_file_id;
16:23:19  78  	   IF var_cc_update_count <= 0 THEN
16:23:19  79  	     UPDATE CC_REQUEST_FILE
16:23:19  80  	     SET CC_REQUEST_FILE_STATUS = 'EMPTY'
16:23:19  81  	     WHERE ID = var_request_file_id;
16:23:19  82  	   END IF;
16:23:19  83  
16:23:19  84  	END POPULATE_REQUEST_INFO;
