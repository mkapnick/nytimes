16:23:20 737  SPROC_NAME     CONSTANT VARCHAR2(29) := 'GET_CLOSED_REFUNDS_BY_INVOICE';
16:23:20 738  temp_invoice_id NUMBER;
16:23:20 739  -- EXCEPTIONS
16:23:20 740  BAD_INVOICE_ID EXCEPTION;
16:23:20 741  BEGIN
16:23:20 742  
16:23:20 743  	BEGIN
16:23:20 744  	  SELECT
16:23:20 745  	    INVOICE.ID into temp_invoice_id
16:23:20 746  	  FROM
16:23:20 747  	    INVOICE
16:23:20 748  	  WHERE
16:23:20 749  	    INVOICE.ID = in_invoice_id;
16:23:20 750  	  EXCEPTION
16:23:20 751  	    WHEN NO_DATA_FOUND THEN
16:23:20 752  	      RAISE BAD_INVOICE_ID;
16:23:20 753  	END;
16:23:20 754  
16:23:20 755  	OPEN out_result_set FOR
16:23:20 756  	SELECT DISTINCT
16:23:20 757  	  TRANSACTION.ID,
16:23:20 758  	  TRANSACTION.TRANSACTION_STATUS_ID,
16:23:20 759  	  TRANSACTION.CREATE_DATE,
16:23:20 760  	  TRANSACTION.UPDATE_DATE,
16:23:20 761  	  TRANSACTION.ORDER_ID,
16:23:20 762  	  TRANSACTION.TRANSACTION_AMOUNT
16:23:20 763  	FROM
