16:23:19 1012  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:19 1013  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:23:19 1014  END GET_OFFER_LENGTH_IN_DAYS;
