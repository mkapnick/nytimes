16:23:19 708  
16:23:19 709  EXCEPTION
16:23:19 710  WHEN NO_DATA_FOUND THEN
16:23:19 711  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:19 712  	  SPROC_NAME, 'No such offer chain');
16:23:19 713  WHEN OTHERS THEN
16:23:19 714  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:19 715  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:23:19 716  END GET_FIRST_OFFER_INDEX;
16:23:19 717  
16:23:19 718  /******************************************************************************/
16:23:19 719  
16:23:19 720  FUNCTION GET_NEXT_OFFER_INDEX (
16:23:19 721  /*
16:23:19 722  NULL, if not exists
16:23:19 723  */
16:23:19 724  	in_offer_chain_id	     IN NUMBER,
16:23:19 725  	in_offer_chain_current_index IN NUMBER
16:23:19 726  ) RETURN NUMBER AS
16:23:19 727  -- VARIABLES
16:23:19 728  SPROC_NAME		     CONSTANT VARCHAR2(20) := 'GET_NEXT_OFFER_INDEX';
16:23:19 729  temp_offer_chain_id	     NUMBER;
16:23:19 730  temp_offer_chain_current_index NUMBER;
16:23:19 731  var_result		     NUMBER;
16:23:19 732  -- EXCEPTIONS
16:23:19 733  BAD_OFFER_CHAIN_ID      EXCEPTION;
16:23:19 734  BAD_CURRENT_INDEX_VALUE EXCEPTION;
16:23:19 735  BEGIN
16:23:19 736  
16:23:19 737  	-- Check that offer chain exists
16:23:19 738  	BEGIN
16:23:19 739  	  SELECT
16:23:19 740  	    OFFER_CHAIN.ID into temp_offer_chain_id
16:23:19 741  	  FROM
16:23:19 742  	    OFFER_CHAIN
16:23:19 743  	  WHERE
16:23:19 744  	    OFFER_CHAIN.ID = in_offer_chain_id;
16:23:19 745  	  EXCEPTION
16:23:19 746  	    WHEN NO_DATA_FOUND THEN
16:23:19 747  	      RAISE BAD_OFFER_CHAIN_ID;
16:23:19 748  	END;
16:23:19 749  
16:23:19 750  	-- Check that current offer index exists
16:23:19 751  	BEGIN
16:23:19 752  	  SELECT
16:23:19 753  	    OFFER_OFFER_CHAIN.INDEX_VALUE into temp_offer_chain_current_index
16:23:19 754  	  FROM
16:23:19 755  	    OFFER_OFFER_CHAIN
16:23:19 756  	  WHERE
16:23:19 757  	    OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
16:23:19 758  	    AND OFFER_OFFER_CHAIN.INDEX_VALUE = in_offer_chain_current_index
16:23:19 759  	    -- TODO: delete next line
16:23:19 760  	    AND ROWNUM <= 1;
16:23:19 761  	  EXCEPTION
16:23:19 762  	    WHEN NO_DATA_FOUND THEN
16:23:19 763  	      RAISE BAD_CURRENT_INDEX_VALUE;
16:23:19 764  	END;
16:23:19 765  
16:23:19 766  	-- Get next offer index
16:23:19 767  	BEGIN
16:23:19 768  	  SELECT
16:23:19 769  	    INDEX_VALUE into var_result
16:23:19 770  	  FROM (
16:23:19 771  	    SELECT
16:23:19 772  	      OFFER_OFFER_CHAIN.INDEX_VALUE
16:23:19 773  	    FROM
16:23:19 774  	      OFFER_OFFER_CHAIN
16:23:19 775  	    WHERE
16:23:19 776  	      OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
16:23:19 777  	      AND OFFER_OFFER_CHAIN.INDEX_VALUE > in_offer_chain_current_index
16:23:19 778  	    ORDER BY
16:23:19 779  	      OFFER_OFFER_CHAIN.INDEX_VALUE ASC
16:23:19 780  	  )
16:23:19 781  	  WHERE
16:23:19 782  	    ROWNUM <= 1;
16:23:19 783  	  EXCEPTION
16:23:19 784  	    WHEN NO_DATA_FOUND THEN
16:23:19 785  	      var_result := NULL;
16:23:19 786  	END;
16:23:19 787  
16:23:19 788  	RETURN var_result;
16:23:19 789  
16:23:19 790  EXCEPTION
16:23:19 791  WHEN BAD_OFFER_CHAIN_ID THEN
16:23:19 792  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:19 793  	  SPROC_NAME, 'No such offer chain');
16:23:19 794  WHEN BAD_CURRENT_INDEX_VALUE THEN
16:23:19 795  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:19 796  	  SPROC_NAME, 'Bad current index value');
16:23:19 797  WHEN OTHERS THEN
16:23:19 798  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:19 799  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:23:19 800  END GET_NEXT_OFFER_INDEX;
