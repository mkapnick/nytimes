16:23:20 2334  	     WHEN NO_DATA_FOUND THEN
16:23:20 2335  	       RAISE BAD_SUBSCRIPTION_ID;
16:23:20 2336  	 END;
16:23:20 2337  
16:23:20 2338  	 -- Check that subscription reason is correct
16:23:20 2339  	 BEGIN
