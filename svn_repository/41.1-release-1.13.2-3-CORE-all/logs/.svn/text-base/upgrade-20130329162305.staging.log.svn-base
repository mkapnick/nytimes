16:23:19 973  var_offer_years	 NUMBER;
16:23:19 974  var_offer_months	 NUMBER;
16:23:19 975  var_offer_days	 NUMBER;
16:23:19 976  var_offer_end_date DATE;
16:23:19 977  -- EXCEPTIONS
16:23:19 978  BAD_OFFER_ID EXCEPTION;
16:23:19 979  BEGIN
16:23:19 980  
16:23:19 981  	BEGIN
16:23:19 982  	  SELECT
16:23:19 983  	    OFFER.ENTITLEMENT_DURATION into var_offer_duration
16:23:19 984  	  FROM
16:23:19 985  	    OFFER
16:23:19 986  	  WHERE
16:23:19 987  	    OFFER.ID = in_offer_id;
16:23:19 988  	  EXCEPTION
16:23:19 989  	    WHEN NO_DATA_FOUND THEN
16:23:19 990  	      RAISE BAD_OFFER_ID;
16:23:19 991  	END;
16:23:19 992  
16:23:19 993  	PROCS_COMMON_V19.ISO8601DURATION_TO_INTERVALS (
16:23:19 994  	  var_offer_duration,
16:23:19 995  	  var_offer_years,
16:23:19 996  	  var_offer_months,
16:23:19 997  	  var_offer_days
16:23:19 998  	);
16:23:19 999  
16:23:19 1000  	 var_offer_end_date := ( ( in_start_date
16:23:19 1001  	   + GLOBAL_CONSTANTS_V19.ONE_DAY_INTERVAL * var_offer_days )
16:23:19 1002  	   + GLOBAL_CONSTANTS_V19.ONE_MONTH_INTERVAL * var_offer_months )
16:23:19 1003  	   + GLOBAL_CONSTANTS_V19.ONE_YEAR_INTERVAL * var_offer_years;
16:23:19 1004  
16:23:19 1005  	 out_days := var_offer_end_date - in_start_date;
16:23:19 1006  
16:23:19 1007  EXCEPTION
16:23:19 1008  WHEN BAD_OFFER_ID THEN
16:23:19 1009  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:19 1010  	   SPROC_NAME, 'No such offer');
