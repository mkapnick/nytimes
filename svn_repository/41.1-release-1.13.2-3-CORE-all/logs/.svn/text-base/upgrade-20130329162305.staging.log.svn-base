16:23:20 1366  	 --    in_license_status_id  => GLOBAL_STATUSES_V19.LICENSE_CLOSED,
16:23:20 1367  	 --    in_needs_entitlements => GLOBAL_CONSTANTS_V19.FALSE,
16:23:20 1368  	 --    in_updated_by	     => in_updated_by
16:23:20 1369  	 --  );
16:23:20 1370  	 --END LOOP;
16:23:20 1371  
16:23:20 1372  	 PROCS_INVOICE_V19.UPDATE_INVOICE_STATUS(
