16:23:20 2113  
16:23:20 2114  PROCEDURE CANCEL_SUBSCRIPTION_INVOICE (
16:23:20 2115  /*
16:23:20 2116  APP_EXCEPTION_CODES_V19.NOT_FOUND
16:23:20 2117  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
16:23:20 2118  APP_EXCEPTION_CODES_V19.INTERNAL_ERROR
16:23:20 2119  */
16:23:20 2120  	 in_invoice_id	      IN NUMBER,
16:23:20 2121  	 in_updated_by	      IN VARCHAR2,
16:23:20 2122  	 in_refundable	      IN NUMBER DEFAULT GLOBAL_CONSTANTS_V19.FALSE
16:23:20 2123  ) AS
16:23:20 2124  SPROC_NAME CONSTANT VARCHAR2(27) := 'CANCEL_SUBSCRIPTION_INVOICE';
16:23:20 2125  -- VARIABLES
16:23:20 2126  temp_invoice_id	      INVOICE.ID%TYPE;
16:23:20 2127  var_chargeback_amount  NUMBER(10,2);
16:23:20 2128  ver_refund_charge_id   NUMBER;
16:23:20 2129  -- EXCEPTIONS
16:23:20 2130  BAD_INVOICE_ID		    EXCEPTION;
16:23:20 2131  CAN_NOT_CALCULATE_CHARGEBACK EXCEPTION;
16:23:20 2132  CAN_NOT_APPLY_CHARGEBACK     EXCEPTION;
16:23:20 2133  EXCEPTION_MESSAGE	    VARCHAR2(1024);
16:23:20 2134  -- STUB
16:23:20 2135  var_now DATE;
16:23:20 2136  var_revoke NUMBER;
16:23:20 2137  var_refund NUMBER;
16:23:20 2138  var_billed NUMBER;
16:23:20 2139  var_subscription_in_grace NUMBER;
16:23:20 2140  BEGIN
16:23:20 2141  
16:23:20 2142  	 var_now := sysdate;
16:23:20 2143  
16:23:20 2144  	 -- Check that invoice exists
16:23:20 2145  	 BEGIN
16:23:20 2146  	   SELECT
16:23:20 2147  	     INVOICE.ID into temp_invoice_id
16:23:20 2148  	   FROM
16:23:20 2149  	     INVOICE
16:23:20 2150  	   WHERE
16:23:20 2151  	     INVOICE.ID = in_invoice_id;
16:23:20 2152  	   EXCEPTION
16:23:20 2153  	     WHEN NO_DATA_FOUND THEN
16:23:20 2154  	       RAISE BAD_INVOICE_ID;
16:23:20 2155  	 END;
16:23:20 2156  
16:23:20 2157  	 select decode(count(1), 0, GLOBAL_CONSTANTS_V19.FALSE, GLOBAL_CONSTANTS_V19.TRUE) into var_revoke
16:23:20 2158  	 from license l, subscription s, offer_chain oc
16:23:20 2159  	 where
16:23:20 2160  	   l.subscription_id = s.id and
16:23:20 2161  	   s.offer_chain_id = oc.id and
16:23:20 2162  	   l.invoice_id = in_invoice_id and
16:23:20 2163  	   oc.revoke_entitlements = GLOBAL_CONSTANTS_V19.TRUE and
16:23:20 2164  	   rownum < 2;
16:23:20 2165  
16:23:20 2166  	 select
16:23:20 2167  	   decode(s.subscription_status_id, GLOBAL_STATUSES_V19.SUBSCRIPTION_IN_GRACE_PERIOD, 1, 0)
16:23:20 2168  	 into
16:23:20 2169  	   var_subscription_in_grace
16:23:20 2170  	 from license l, subscription s
16:23:20 2171  	 where
16:23:20 2172  	   l.subscription_id = s.id and
16:23:20 2173  	   l.invoice_id = in_invoice_id and
16:23:20 2174  	   rownum < 2;
16:23:20 2175  
16:23:20 2176  	 var_billed := PROCS_INVOICE_V19.IS_INVOICE_PAYING_STARTED(in_invoice_id);
16:23:20 2177  	 var_refund := GLOBAL_CONSTANTS_V19.FALSE;
16:23:20 2178  
16:23:20 2179  	 -- Check that transaction for given invoice not started
16:23:20 2180  	 -- if refund enabled calculate and apply chargeback
16:23:20 2181  	 IF (
16:23:20 2182  	     var_billed = GLOBAL_CONSTANTS_V19.TRUE
16:23:20 2183  	   )THEN
16:23:20 2184  	   if (in_refundable = GLOBAL_CONSTANTS_V19.TRUE) then
16:23:20 2185  	     -- If started then we need to calculate refund
16:23:20 2186  	     BEGIN
16:23:20 2187  	       PROCS_INVOICE_V19.CALCULATE_INVOICE_CHARGEBACK(
16:23:20 2188  		 in_invoice_id,
16:23:20 2189  		 var_now,
16:23:20 2190  		 var_chargeback_amount
16:23:20 2191  	       );
16:23:20 2192  	       EXCEPTION
16:23:20 2193  		 WHEN OTHERS THEN
16:23:20 2194  		   EXCEPTION_MESSAGE := SQLERRM;
