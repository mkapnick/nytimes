16:23:20 514  	  FROM
16:23:20 515  	    OFFER_OFFER_CHAIN
16:23:20 516  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
16:23:20 517  	  WHERE
16:23:20 518  	    1=2;
16:23:20 519  	ELSE
16:23:20 520  	  OPEN out_result_set FOR
16:23:20 521  	  SELECT
