16:23:20 368  	  ACCOUNT
16:23:20 369  	WHERE
16:23:20 370  	  ACCOUNT.ID = var_account_id;
16:23:20 371  
16:23:20 372  	-- Card should to be active
16:23:20 373  	IF var_credit_card_status != GLOBAL_STATUSES_V19.CREDIT_CARD_ACTIVE THEN
16:23:20 374  	  RAISE BAD_CC_STATUS;
16:23:20 375  	END IF;
16:23:20 376  
16:23:20 377  	-- Looking for pending transactions associated with given credit card
16:23:20 378  	SELECT
16:23:20 379  	  COUNT(*) into var_pending_invoices_num
16:23:20 380  	FROM
16:23:20 381  	  CHARGE
16:23:20 382  	  INNER JOIN TRANSACTION ON CHARGE.TRANSACTION_ID = TRANSACTION.ID
16:23:20 383  	WHERE
16:23:20 384  	  CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V19.INSTRUMENT_CREDIT_CARD
16:23:20 385  	  AND CHARGE.INSTRUMENT_ID = in_credit_card_id
16:23:20 386  	  AND TRANSACTION.TRANSACTION_STATUS_ID = GLOBAL_STATUSES_V19.TRANSACTION_PENDING;
16:23:20 387  
16:23:20 388  	IF var_pending_invoices_num > 0 THEN
16:23:20 389  	  RAISE PENDING_TRANSACTIONS_FOUNDED;
16:23:20 390  	END IF;
16:23:20 391  
16:23:20 392  	-- Getting current default financial instrument
16:23:20 393  	BEGIN
16:23:20 394  	  GET_DEF_FINANCIAL_INSTRUMENT(
16:23:20 395  	    in_group_id 	   => var_group_id,
16:23:20 396  	    out_instrument_type_id => current_def_instrument_type_id,
16:23:20 397  	    out_instrument_id	   => current_def_instrument_id
16:23:20 398  	  );
16:23:20 399  	  EXCEPTION
16:23:20 400  	    WHEN OTHERS THEN
16:23:20 401  	      EXCEPTION_MESSAGE := SQLERRM;
16:23:20 402  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
16:23:20 403  	END;
16:23:20 404  
16:23:20 405  	-- Checking that credit card is not default
16:23:20 406  	IF current_def_instrument_type_id = GLOBAL_ENUMS_V19.INSTRUMENT_CREDIT_CARD
16:23:20 407  	  AND current_def_instrument_id = in_credit_card_id THEN
16:23:20 408  	  BEGIN
16:23:20 409  	    DEL_DEF_FINANCIAL_INSTRUMENT(
16:23:20 410  	      in_group_id => var_group_id
16:23:20 411  	    );
16:23:20 412  	  EXCEPTION
16:23:20 413  	    WHEN OTHERS THEN
16:23:20 414  	      EXCEPTION_MESSAGE := SQLERRM;
16:23:20 415  	      RAISE CAN_NOT_DEL_DEF_FINANCIAL;
16:23:20 416  	  END;
16:23:20 417  	END IF;
16:23:20 418  
16:23:20 419  	-- Update credit card status
16:23:20 420  	PROCS_FIN_INSTRUMENTS_V19.UPDATE_CREDIT_CARD_STATUS(
16:23:20 421  	  in_credit_card_id	   => in_credit_card_id,
16:23:20 422  	  in_updated_by 	   => in_updated_by,
16:23:20 423  	  in_credit_card_status_id => GLOBAL_STATUSES_V19.CREDIT_CARD_DISABLED
16:23:20 424  	);
16:23:20 425  
16:23:20 426  EXCEPTION
16:23:20 427  WHEN NO_DATA_FOUND THEN
16:23:20 428  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 429  	  SPROC_NAME, 'No such credit card');
16:23:20 430  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
16:23:20 431  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 432  	  SPROC_NAME, 'Could not get current default financial instrument', EXCEPTION_MESSAGE);
16:23:20 433  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
16:23:20 434  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 435  	  SPROC_NAME, 'Could not delete information about default financial instrument from account', EXCEPTION_MESSAGE);
16:23:20 436  WHEN BAD_CC_STATUS THEN
16:23:20 437  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
16:23:20 438  	  SPROC_NAME, 'Credit card is not active');
16:23:20 439  WHEN PENDING_TRANSACTIONS_FOUNDED THEN
16:23:20 440  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
16:23:20 441  	  SPROC_NAME, 'Account has pending charge which is using this card');
16:23:20 442  WHEN CAN_NOT_DISABLE_CREDIT_CARD THEN
16:23:20 443  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 444  	  SPROC_NAME, 'Could not disable this credit card', EXCEPTION_MESSAGE);
16:23:20 445  WHEN OTHERS THEN
16:23:20 446  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 447  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 448  END DISABLE_CREDIT_CARD;
