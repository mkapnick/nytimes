16:23:20 2358  	 WHERE
16:23:20 2359  	   LICENSE.LICENSE_STATUS_ID in (GLOBAL_STATUSES_V19.LICENSE_ACTIVE, GLOBAL_STATUSES_V19.LICENSE_IN_GRACE_PERIOD)
16:23:20 2360  	   AND LICENSE.SUBSCRIPTION_ID = in_subscription_id;
16:23:20 2361  
16:23:20 2362  	 IF var_active_invoices_count > 0 THEN
16:23:20 2363  	   RAISE ACTIVE_INVOICES_FOUND;
16:23:20 2364  	 END IF;
16:23:20 2365  
16:23:20 2366  	 -- Check that subscription is active
16:23:20 2367  	 IF var_current_subscr_status != GLOBAL_STATUSES_V19.SUBSCRIPTION_ACTIVE AND
16:23:20 2368  	    var_current_subscr_status != GLOBAL_STATUSES_V19.SUBSCRIPTION_SUSPENDED AND
