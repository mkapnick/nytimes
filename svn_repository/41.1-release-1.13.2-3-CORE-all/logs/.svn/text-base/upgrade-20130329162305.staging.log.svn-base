16:23:20  30  	  function getRefundAmount(in_line_item_id line_item.id%type)
16:23:20  31  	      return line_item.amount%type
16:23:20  32  	  is
16:23:20  33  	      v_li_total  line_item.amount%type;
16:23:20  34  	      v_inv_total line_item.amount%type;
16:23:20  35  	      v_ref_total line_item.amount%type;
16:23:20  36  	  begin
16:23:20  37  
16:23:20  38  	      for li in (
16:23:20  39  		  select li.invoice_id, po.*
16:23:20  40  		  from line_item li
16:23:20  41  			  join core_owner.product_offering po on li.product_offer_id = po.id
16:23:20  42  		  where li.id = in_line_item_id
16:23:20  43  	      )
16:23:20  44  	      loop
16:23:20  45  		  v_li_total := (li.quantity * li.unit_price) - getDiscountAmount(in_line_item_id);
16:23:20  46  
16:23:20  47  		  v_inv_total := 0;
16:23:20  48  		  v_ref_total := 0;
16:23:20  49  		  for x in (
