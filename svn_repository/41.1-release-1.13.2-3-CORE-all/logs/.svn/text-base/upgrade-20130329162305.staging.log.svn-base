16:23:20 671  
16:23:20 672  PROCEDURE GET_ACCOUNT_SUBSCRIPTIONS (
16:23:20 673  	  in_group_id	 IN  NUMBER,
16:23:20 674  	  in_start_date  IN DATE,
16:23:20 675  	  in_end_date	 IN DATE,
16:23:20 676  	  in_status	 IN NUMBER,
16:23:20 677  	  in_group_account_type IN VARCHAR2,
16:23:20 678  	  out_result_set OUT SYS_REFCURSOR
16:23:20 679  ) AS
16:23:20 680  -- VARIABLES
16:23:20 681  SPROC_NAME     CONSTANT VARCHAR2(25) := 'GET_ACCOUNT_SUBSCRIPTIONS';
16:23:20 682  var_account_id NUMBER;
16:23:20 683  -- EXCEPTIONS
16:23:20 684  BAD_GROUP_ID	      EXCEPTION;
16:23:20 685  BEGIN
16:23:20 686  	-- Get account id
16:23:20 687  	BEGIN
16:23:20 688  	  SELECT
16:23:20 689  	    ACCOUNT.ID INTO var_account_id
16:23:20 690  	  FROM
16:23:20 691  	    ACCOUNT
16:23:20 692  	  WHERE
16:23:20 693  	    ACCOUNT.GROUP_ID = in_group_id;
16:23:20 694  	  EXCEPTION
16:23:20 695  	    WHEN NO_DATA_FOUND THEN
16:23:20 696  	      RAISE BAD_GROUP_ID;
16:23:20 697  	END;
16:23:20 698  
16:23:20 699  	-- Get information about account subscriptions
16:23:20 700  	OPEN out_result_set FOR
16:23:20 701  	SELECT
16:23:20 702  	  in_group_id AS "GROUP_ID",
16:23:20 703  	  SUBSCRIPTION.ID AS "SUBSCRIPTION_ID",
16:23:20 704  	  SUBSCRIPTION.SUBSCRIPTION_STATUS_ID,
16:23:20 705  	  SUBSCRIPTION.PURCHASE_DATE,
16:23:20 706  	  SUBSCRIPTION.SUSPEND_DATE,
16:23:20 707  	  SUBSCRIPTION.REACTIVATION_DATE,
16:23:20 708  	  SUBSCRIPTION.CANCELLATION_DATE,
16:23:20 709  	  SUBSCRIPTION_CANCEL_REASON.VALUE as "CANCEL_TYPE",
16:23:20 710  	  SUBSCRIPTION.INSTRUMENT_ID,
16:23:20 711  	  SUBSCRIPTION.INSTRUMENT_TYPE_ID,
16:23:20 712  	  OFFER_CHAIN.ID AS "OFFER_CHAIN_ID",
16:23:20 713  	  OFFER_CHAIN.NAME,
16:23:20 714  	  OFFER_CHAIN.DESCRIPTION,
16:23:20 715  	  OFFER_CHAIN.PRODUCT_URI,
16:23:20 716  	  PROCS_SUBSCRIPTION_V19.CALC_SUBSCRIPTION_END_DATE(SUBSCRIPTION.ID) as "END_DATE",
16:23:20 717  	  PROCS_SUBSCRIPTION_V19.GET_RECENT_CHARGE(SUBSCRIPTION.ID) AS "RECENT_CHARGE",
16:23:20 718  	  PROCS_SUBSCRIPTION_V19.GET_RENEWAL_DATE(SUBSCRIPTION.ID) AS "RENEWAL_DATE",
16:23:20 719  	  PROCS_SUBSCRIPTION_V19.GET_BILLING_CYCLE(SUBSCRIPTION.ID) AS "BILLING_CYCLE",
16:23:20 720  	  PROCS_SUBSCRIPTION_V19.IS_SUBSCRIPTION_CANCELABLE(SUBSCRIPTION.ID) AS "IS_CANCELABLE",
16:23:20 721  	  ITUNES_RECEIPT.ID AS "ITUNES_RECEIPT_ID",
16:23:20 722  	  (
16:23:20 723  	    SELECT
16:23:20 724  	      MAX(ENTITLEMENT_END_DATE)
16:23:20 725  	      FROM LICENSE
16:23:20 726  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
16:23:20 727  	  )
16:23:20 728  	  as "ENT_END_DATE",
16:23:20 729  	  (
16:23:20 730  	    SELECT
16:23:20 731  	      MIN(START_DATE)
16:23:20 732  	      FROM LICENSE
16:23:20 733  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
16:23:20 734  	  )
16:23:20 735  	  as "ENT_START_DATE",
16:23:20 736  	  GROUP_ACCOUNT.SUBSCRIPTION_ID GA_SUBSCRIPTION_ID,
16:23:20 737  	  GROUP_ACCOUNT.ID GA_ID,
16:23:20 738  	  GROUP_ACCOUNT.GROUP_NAME GA_GROUP_NAME,
16:23:20 739  	  GROUP_ACCOUNT.FIRST_NAME GA_FIRST_NAME,
16:23:20 740  	  GROUP_ACCOUNT.LAST_NAME GA_LAST_NAME,
16:23:20 741  	  GROUP_ACCOUNT.EMAIL GA_EMAIL,
16:23:20 742  	  GROUP_ACCOUNT.PHONE GA_PHONE,
16:23:20 743  	  GROUP_ACCOUNT.ORGANIZATION_TYPE GA_ORGANIZATION_TYPE,
16:23:20 744  	  GROUP_ACCOUNT.SEATS GA_SEATS,
16:23:20 745  	  PROCS_GROUP_ACCOUNT_V19.F_GET_NUM_OCCUPIED_GROUP_SEATS(GROUP_ACCOUNT.ID) GA_SEATS_USED,
16:23:20 746  	  GROUP_ACCOUNT.IP GA_IP,
16:23:20 747  	  PROCS_SUBSCRIPTION_V19.GET_GIFT_CERT_CODE_BY_SUB_ID(SUBSCRIPTION.ID) GIFT_CERTIFICATE_CODE,
16:23:20 748  	  PROCS_ACCOUNT_V19.GET_GRACE_START_DATE(SUBSCRIPTION.ID) GRACE_START_DATE,
16:23:20 749  	  PROCS_ACCOUNT_V19.GET_GRACE_END_DATE(SUBSCRIPTION.ID) GRACE_END_DATE
16:23:20 750  	FROM
16:23:20 751  	  SUBSCRIPTION
16:23:20 752  	  INNER JOIN OFFER_CHAIN ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
16:23:20 753  	  LEFT JOIN SUBSCRIPTION_CANCEL_REASON ON SUBSCRIPTION.SCT_ID = SUBSCRIPTION_CANCEL_REASON.ID
16:23:20 754  	  LEFT JOIN ITUNES_RECEIPT ON SUBSCRIPTION.ID = ITUNES_RECEIPT.SUBSCRIPTION_ID
16:23:20 755  	  LEFT JOIN GROUP_ACCOUNT ON SUBSCRIPTION.ID = GROUP_ACCOUNT.SUBSCRIPTION_ID
16:23:20 756  	WHERE
16:23:20 757  	  SUBSCRIPTION.ACCOUNT_ID = var_account_id
16:23:20 758  	  AND (SUBSCRIPTION.SCT_ID IS NULL OR SUBSCRIPTION.SCT_ID != GLOBAL_STATUSES_V19.REAL_TIME_CANCEL_REASON)
16:23:20 759  	  AND SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = NVL(in_status, SUBSCRIPTION.SUBSCRIPTION_STATUS_ID)
16:23:20 760  	  AND PROCS_COMMON_V19.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE) >= NVL(in_start_date, PROCS_COMMON_V19.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE))
16:23:20 761  	  AND PROCS_COMMON_V19.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE) <= NVL(in_end_date, PROCS_COMMON_V19.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE))
16:23:20 762  	  AND (in_group_account_type IS NULL OR OFFER_CHAIN.GROUP_ACCOUNT_TYPE_ID = in_group_account_type);
16:23:20 763  
16:23:20 764  EXCEPTION
16:23:20 765  WHEN BAD_GROUP_ID THEN
16:23:20 766  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 767  	  SPROC_NAME, 'Can not find account with given group id');
16:23:20 768  WHEN OTHERS THEN
16:23:20 769  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 770  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 771  END GET_ACCOUNT_SUBSCRIPTIONS;
16:23:20 772  
16:23:20 773  /******************************************************************************/
16:23:20 774  
16:23:20 775  PROCEDURE FREEZE_ACCOUNT (
16:23:20 776  /*
16:23:20 777  Throws exceptions:
16:23:20 778  APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 779  APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 780  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
16:23:20 781  */
16:23:20 782  	in_group_id   IN NUMBER,
16:23:20 783  	in_updated_by IN VARCHAR2,
16:23:20 784  	in_note       IN VARCHAR2,
16:23:20 785  	in_agent_id   IN NUMBER
16:23:20 786  ) AS
16:23:20 787  SPROC_NAME	    CONSTANT VARCHAR2(14) := 'FREEZE_ACCOUNT';
16:23:20 788  -- VARIABLES
16:23:20 789  var_account_id	    NUMBER;
16:23:20 790  var_account_status_id NUMBER;
16:23:20 791  -- EXCEPTIONS
16:23:20 792  BAD_GROUP_ID	    EXCEPTION;
16:23:20 793  BAD_ACCOUNT_STATUS_ID EXCEPTION;
16:23:20 794  CAN_NOT_CREATE_NOTE   EXCEPTION;
16:23:20 795  EXCEPTION_MESSAGE     VARCHAR2(1024);
16:23:20 796  BEGIN
16:23:20 797  
16:23:20 798  	-- Get account status, account id
16:23:20 799  	BEGIN
16:23:20 800  	  SELECT
16:23:20 801  	    ACCOUNT.ID,
16:23:20 802  	    ACCOUNT.ACCOUNT_STATUS_ID
16:23:20 803  	    into
16:23:20 804  	    var_account_id,
16:23:20 805  	    var_account_status_id
16:23:20 806  	  FROM
16:23:20 807  	    ACCOUNT
16:23:20 808  	  WHERE
16:23:20 809  	    ACCOUNT.GROUP_ID = in_group_id;
16:23:20 810  	  EXCEPTION
16:23:20 811  	    WHEN NO_DATA_FOUND THEN
16:23:20 812  	      RAISE BAD_GROUP_ID;
16:23:20 813  	END;
16:23:20 814  
16:23:20 815  	-- We can freeze only ACTIVE accounts
16:23:20 816  	IF var_account_status_id != GLOBAL_STATUSES_V19.ACCOUNT_ACTIVE
16:23:20 817  	  AND var_account_status_id != GLOBAL_STATUSES_V19.ACCOUNT_FROZEN THEN
16:23:20 818  	  RAISE BAD_ACCOUNT_STATUS_ID;
16:23:20 819  	END IF;
16:23:20 820  
16:23:20 821  	-- Set account status
16:23:20 822  	PROCS_ACCOUNT_V19.UPDATE_ACCOUNT_STATUS(
16:23:20 823  	  in_account_id        => var_account_id,
16:23:20 824  	  in_updated_by        => in_updated_by,
16:23:20 825  	  in_account_status_id => GLOBAL_STATUSES_V19.ACCOUNT_FROZEN
16:23:20 826  	);
16:23:20 827  
16:23:20 828  	-- Annotate account
16:23:20 829  	BEGIN
16:23:20 830  	  PROCS_ACCOUNT_V19.ANNOTATE_ACCOUNT(
16:23:20 831  	    in_group_id   => in_group_id,
16:23:20 832  	    in_agent_id   => in_agent_id,
16:23:20 833  	    in_note	  => in_note,
16:23:20 834  	    in_created_by => in_updated_by
16:23:20 835  	  );
16:23:20 836  	  EXCEPTION
16:23:20 837  	    WHEN OTHERS THEN
16:23:20 838  	      EXCEPTION_MESSAGE := SQLERRM;
16:23:20 839  	      RAISE CAN_NOT_CREATE_NOTE;
16:23:20 840  	END;
16:23:20 841  
16:23:20 842  EXCEPTION
16:23:20 843  WHEN BAD_GROUP_ID THEN
16:23:20 844  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 845  	  SPROC_NAME, 'No such group id');
16:23:20 846  WHEN BAD_ACCOUNT_STATUS_ID THEN
16:23:20 847  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
16:23:20 848  	  SPROC_NAME, 'Could not update this account. Status should to be active or frozen');
16:23:20 849  WHEN CAN_NOT_CREATE_NOTE THEN
16:23:20 850  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 851  	  SPROC_NAME, 'Could not annotate account', EXCEPTION_MESSAGE);
16:23:20 852  WHEN OTHERS THEN
16:23:20 853  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 854  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 855  END FREEZE_ACCOUNT;
16:23:20 856  
16:23:20 857  /******************************************************************************/
16:23:20 858  
16:23:20 859  PROCEDURE GET_ACCOUNT_SUBSCR_INVOICES (
16:23:20 860  /*
16:23:20 861  Throws exceptions:
16:23:20 862  APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 863  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
16:23:20 864  */
16:23:20 865  	in_group_id	   IN  NUMBER,
16:23:20 866  	in_subscription_id IN NUMBER,
16:23:20 867  	out_result_set	   OUT SYS_REFCURSOR
16:23:20 868  ) AS
16:23:20 869  SPROC_NAME	   CONSTANT VARCHAR2(27) := 'GET_ACCOUNT_SUBSCR_INVOICES';
16:23:20 870  -- VARIABLES
16:23:20 871  var_account_id	   NUMBER;
16:23:20 872  temp_subscription_id NUMBER;
16:23:20 873  -- EXCEPTIONS
16:23:20 874  BAD_GROUP_ID	  EXCEPTION;
16:23:20 875  BAD_SUBSCRIPTION_ID EXCEPTION;
16:23:20 876  BEGIN
16:23:20 877  	-- Get account id
16:23:20 878  	BEGIN
16:23:20 879  	  SELECT
16:23:20 880  	    ACCOUNT.ID into var_account_id
16:23:20 881  	  FROM
16:23:20 882  	    ACCOUNT
16:23:20 883  	  WHERE
16:23:20 884  	    ACCOUNT.GROUP_ID = in_group_id;
16:23:20 885  	  EXCEPTION
16:23:20 886  	    WHEN NO_DATA_FOUND THEN
16:23:20 887  	      RAISE BAD_GROUP_ID;
16:23:20 888  	END;
16:23:20 889  
16:23:20 890  	-- Check that subscription exists
16:23:20 891  	BEGIN
16:23:20 892  	  IF in_subscription_id IS NOT NULL THEN
16:23:20 893  	    SELECT
16:23:20 894  	      SUBSCRIPTION.ID into temp_subscription_id
16:23:20 895  	    FROM
16:23:20 896  	      SUBSCRIPTION
16:23:20 897  	    WHERE
16:23:20 898  	      SUBSCRIPTION.ID = in_subscription_id;
16:23:20 899  	  END IF;
16:23:20 900  	  EXCEPTION
16:23:20 901  	    WHEN NO_DATA_FOUND THEN
16:23:20 902  	      RAISE BAD_SUBSCRIPTION_ID;
16:23:20 903  	END;
16:23:20 904  
16:23:20 905  	OPEN out_result_set FOR
16:23:20 906  	SELECT DISTINCT
16:23:20 907  	  INVOICE.ID as "INVOICE_ID",
16:23:20 908  	  INVOICE.CREATE_DATE,
16:23:20 909  	  INVOICE.INVOICE_STATUS_ID,
16:23:20 910  	  PROCS_INVOICE_V19.F_CALCULATE_INVOICE_AMOUNT(INVOICE.ID) as "AMOUNT",
16:23:20 911  	  OFFER_CHAIN.ID as "OFFER_CHAIN_ID",
16:23:20 912  	  OFFER_CHAIN.NAME as "OFFER_CHAIN_NAME",
16:23:20 913  	  SUBSCRIPTION.ID as "SUBSCRIPTION_ID",
16:23:20 914  	  NULL as "GC_CODE",
16:23:20 915  	  NULL as "GC_ID"
16:23:20 916  	FROM
16:23:20 917  	  LICENSE
16:23:20 918  	  INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
16:23:20 919  	  INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
16:23:20 920  	  INNER JOIN OFFER_CHAIN ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
16:23:20 921  	WHERE
16:23:20 922  	  SUBSCRIPTION.ACCOUNT_ID = var_account_id
16:23:20 923  	  AND (SUBSCRIPTION.SCT_ID IS NULL OR SUBSCRIPTION.SCT_ID != GLOBAL_STATUSES_V19.REAL_TIME_CANCEL_REASON)
16:23:20 924  	  AND SUBSCRIPTION.ID = NVL(in_subscription_id, SUBSCRIPTION.ID);
16:23:20 925  
16:23:20 926  EXCEPTION
16:23:20 927  WHEN BAD_GROUP_ID THEN
16:23:20 928  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 929  	  SPROC_NAME, 'No such account');
16:23:20 930  WHEN BAD_SUBSCRIPTION_ID THEN
16:23:20 931  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 932  	  SPROC_NAME, 'No such subscription');
16:23:20 933  WHEN OTHERS THEN
16:23:20 934  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 935  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 936  END GET_ACCOUNT_SUBSCR_INVOICES;
16:23:20 937  
16:23:20 938  /******************************************************************************/
16:23:20 939  
16:23:20 940  PROCEDURE GET_ACCOUNT_GC_INVOICES (
16:23:20 941  /*
16:23:20 942  Throws exceptions:
16:23:20 943  APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 944  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
16:23:20 945  */
16:23:20 946  	in_group_id    IN  NUMBER,
16:23:20 947  	out_result_set OUT SYS_REFCURSOR
16:23:20 948  ) AS
16:23:20 949  SPROC_NAME     CONSTANT VARCHAR2(23) := 'GET_ACCOUNT_GC_INVOICES';
16:23:20 950  -- VARIABLES
16:23:20 951  var_account_id NUMBER;
16:23:20 952  -- EXCEPTIONS
16:23:20 953  BAD_GROUP_ID EXCEPTION;
16:23:20 954  BEGIN
16:23:20 955  	-- Get account id
16:23:20 956  	BEGIN
16:23:20 957  	  SELECT
16:23:20 958  	    ACCOUNT.ID into var_account_id
16:23:20 959  	  FROM
16:23:20 960  	    ACCOUNT
16:23:20 961  	  WHERE
16:23:20 962  	    ACCOUNT.GROUP_ID = in_group_id;
16:23:20 963  	  EXCEPTION
16:23:20 964  	    WHEN NO_DATA_FOUND THEN
16:23:20 965  	      RAISE BAD_GROUP_ID;
16:23:20 966  	END;
16:23:20 967  
16:23:20 968  	-- Get invoices
16:23:20 969  	OPEN out_result_set FOR
16:23:20 970  	SELECT DISTINCT
16:23:20 971  	  INVOICE.ID as "INVOICE_ID",
16:23:20 972  	  INVOICE.CREATE_DATE,
16:23:20 973  	  INVOICE.INVOICE_STATUS_ID,
16:23:20 974  	  PROCS_INVOICE_V19.F_CALCULATE_INVOICE_AMOUNT(INVOICE.ID) as "AMOUNT",
16:23:20 975  	  OFFER_CHAIN.ID as "OFFER_CHAIN_ID",
16:23:20 976  	  OFFER_CHAIN.NAME as "OFFER_CHAIN_NAME",
16:23:20 977  	  NULL as "SUBSCRIPTION_ID",
16:23:20 978  	  GIFT_CERTIFICATE.CODE as "GC_CODE",
16:23:20 979  	  GIFT_CERTIFICATE.ID as "GC_ID"
16:23:20 980  	FROM
16:23:20 981  	  GIFT_CERTIFICATE
16:23:20 982  	  INNER JOIN INVOICE ON GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = INVOICE.ID
16:23:20 983  	  INNER JOIN OFFER_CHAIN ON GIFT_CERTIFICATE.OFFER_CHAIN_ID = OFFER_CHAIN.ID
