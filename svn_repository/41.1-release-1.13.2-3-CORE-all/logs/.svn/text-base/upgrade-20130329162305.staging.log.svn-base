16:23:19   3  PROCEDURE ADD_LINE_ITEMS(
16:23:19   4  /*
16:23:19   5  Throws exceptions:
16:23:19   6  APP_EXCEPTION_CODES_V19.NOT_FOUND
16:23:19   7  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
16:23:19   8  */
16:23:19   9  	in_invoice_id IN NUMBER,
16:23:19  10  	in_offer_id   IN NUMBER,
16:23:19  11  	in_created_by IN VARCHAR2
16:23:19  12  ) AS
16:23:19  13  -- VARIABLES
16:23:19  14  SPROC_NAME      CONSTANT VARCHAR2(14) := 'ADD_LINE_ITEMS';
16:23:19  15  temp_invoice_id NUMBER;
16:23:19  16  temp_offer_id   NUMBER;
16:23:19  17  
16:23:19  18  var_line_item_data SYS_REFCURSOR;
16:23:19  19  var_new_line_item_id NUMBER;
16:23:19  20  var_product_unit_price NUMBER (10,6);
16:23:19  21  var_product_offering_price NUMBER(10,6);
16:23:19  22  var_product_offering_oprice NUMBER(10,6);
16:23:19  23  var_product_quantity NUMBER;
16:23:19  24  var_product_offering_id NUMBER;
16:23:19  25  
16:23:19  26  var_line_item_price	  NUMBER(10,2);
16:23:19  27  var_discount_fixed_amount   NUMBER(10,6);
16:23:19  28  var_discount_percent_amount NUMBER(10,2);
16:23:19  29  
16:23:19  30  
16:23:19  31  -- EXCEPTIONS
16:23:19  32  BAD_INVOICE_ID EXCEPTION;
16:23:19  33  BAD_OFFER_ID EXCEPTION;
16:23:19  34  BAD_DISCOUNT EXCEPTION;
16:23:19  35  BEGIN
16:23:19  36  
16:23:19  37  	-- Check that given invoice exists
16:23:19  38  	BEGIN
16:23:19  39  	  SELECT
16:23:19  40  	    INVOICE.ID into temp_invoice_id
16:23:19  41  	  FROM
16:23:19  42  	    INVOICE
16:23:19  43  	  WHERE
16:23:19  44  	    INVOICE.ID = in_invoice_id;
16:23:19  45  
16:23:19  46  	  EXCEPTION
16:23:19  47  	    WHEN NO_DATA_FOUND THEN
16:23:19  48  	      RAISE BAD_INVOICE_ID;
16:23:19  49  	END;
16:23:19  50  
16:23:19  51  	-- Check that given offer exists
16:23:19  52  	BEGIN
16:23:19  53  	  SELECT
16:23:19  54  	    OFFER.ID into temp_offer_id
16:23:19  55  	  FROM
16:23:19  56  	    OFFER
16:23:19  57  	  WHERE
16:23:19  58  	    OFFER.ID = in_offer_id;
16:23:19  59  
16:23:19  60  	  EXCEPTION
16:23:19  61  	    WHEN NO_DATA_FOUND THEN
16:23:19  62  	      RAISE BAD_OFFER_ID;
16:23:19  63  	END;
16:23:19  64  
16:23:19  65  	-- Get product_offering data
16:23:19  66  	OPEN var_line_item_data FOR
16:23:19  67  	SELECT
16:23:19  68  	  PRODUCT_OFFERING.ID,
16:23:19  69  	  PRODUCT_OFFERING.UNIT_PRICE,
16:23:19  70  	  PRODUCT_OFFERING.QUANTITY
16:23:19  71  	FROM
16:23:19  72  	  OFFER_PRODUCT_OFFERING
16:23:19  73  	  INNER JOIN PRODUCT_OFFERING ON OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = PRODUCT_OFFERING.ID
16:23:19  74  	WHERE
16:23:19  75  	  OFFER_PRODUCT_OFFERING.OFFER_ID = in_offer_id;
16:23:19  76  
16:23:19  77  	-- insert line items
16:23:19  78  	-- add discounts to line items
16:23:19  79  	LOOP
16:23:19  80  	  FETCH var_line_item_data INTO
16:23:19  81  	    var_product_offering_id,
16:23:19  82  	    var_product_unit_price,
16:23:19  83  	    var_product_quantity;
16:23:19  84  	  EXIT WHEN var_line_item_data%NOTFOUND;
16:23:19  85  
16:23:19  86  	  var_product_offering_oprice := var_product_unit_price * var_product_quantity;
16:23:19  87  	  var_product_offering_price := var_product_offering_oprice;
16:23:19  88  
16:23:19  89  	  -- Apply discounts to line_item
16:23:19  90  	  BEGIN
16:23:19  91  	    SELECT
16:23:19  92  	      SUM (DISCOUNT.FIXED_AMOUNT) into var_discount_fixed_amount
16:23:19  93  	    FROM
16:23:19  94  	      DISCOUNT_PRODUCT_OFFERING
16:23:19  95  	      INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
16:23:19  96  	    WHERE
16:23:19  97  	      DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
16:23:19  98  	      AND DISCOUNT.FIXED_AMOUNT IS NOT NULL;
16:23:19  99  	    EXCEPTION
16:23:19 100  	    WHEN NO_DATA_FOUND THEN
16:23:19 101  	      var_discount_fixed_amount := NULL;
16:23:19 102  	  END;
16:23:19 103  
16:23:19 104  	  BEGIN
16:23:19 105  	    SELECT
16:23:19 106  	      SUM (DISCOUNT.PERCENT_AMOUNT) into var_discount_percent_amount
16:23:19 107  	    FROM
16:23:19 108  	      DISCOUNT_PRODUCT_OFFERING
16:23:19 109  	      INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
16:23:19 110  	    WHERE
16:23:19 111  	      DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
16:23:19 112  	      AND DISCOUNT.PERCENT_AMOUNT IS NOT NULL;
16:23:19 113  	    EXCEPTION
16:23:19 114  	    WHEN NO_DATA_FOUND THEN
16:23:19 115  	      var_discount_percent_amount := NULL;
16:23:19 116  	  END;
16:23:19 117  
16:23:19 118  	  IF (var_discount_percent_amount IS NOT NULL) THEN
16:23:19 119  	    var_product_offering_price := var_product_offering_price * ( 1 - var_discount_percent_amount / 100 );
16:23:19 120  	  END IF;
16:23:19 121  
16:23:19 122  	  IF (var_discount_fixed_amount IS NOT NULL) THEN
16:23:19 123  	    var_product_offering_price := var_product_offering_price - var_discount_fixed_amount;
16:23:19 124  	  END IF;
16:23:19 125  
16:23:19 126  	  var_line_item_price := PROCS_COMMON_V19.ROUND_10_6_TO_10_2(var_product_offering_price);
16:23:19 127  
16:23:19 128  	  IF (var_line_item_price < 0) THEN
16:23:19 129  	      RAISE BAD_DISCOUNT;
16:23:19 130  	  END IF;
16:23:19 131  
16:23:19 132  	  var_new_line_item_id := NULL;
16:23:19 133  	  PROCS_LINE_ITEMS_CRU_V19.CREATE_LINE_ITEM(
16:23:19 134  	    inout_line_item_id	=> var_new_line_item_id,
16:23:19 135  	    in_product_offer_id => var_product_offering_id,
16:23:19 136  	    in_invoice_id	=> in_invoice_id,
16:23:19 137  	    in_amount		=> var_line_item_price,
16:23:19 138  	    in_created_by	=> in_created_by,
16:23:19 139  	    in_discount_amount	=> var_product_offering_oprice - var_line_item_price,
16:23:19 140  	    in_taxes_amount	=> NULL
16:23:19 141  	  );
16:23:19 142  
16:23:19 143  	  FOR f_discount IN (
16:23:19 144  	    SELECT
16:23:19 145  	      DISCOUNT.ID
