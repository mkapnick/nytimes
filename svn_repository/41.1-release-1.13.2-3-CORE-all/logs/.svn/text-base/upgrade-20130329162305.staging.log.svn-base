16:23:20 369  	-- get all line items for given invoice
16:23:20 370  	OPEN out_line_items_set FOR
16:23:20 371  	SELECT
16:23:20 372  	  LINE_ITEM.AMOUNT,
16:23:20 373  	  LINE_ITEM.ID,
16:23:20 374  	  LINE_ITEM.CREATED_BY,
16:23:20 375  	  LINE_ITEM.CREATE_DATE,
16:23:20 376  	  LINE_ITEM.DISCOUNT_AMOUNT,
16:23:20 377  	  LINE_ITEM.TAXES_AMOUNT,
16:23:20 378  	  LINE_ITEM.PRODUCT_OFFER_ID,
16:23:20 379  	  LINE_ITEM.INVOICE_ID
16:23:20 380  	FROM
16:23:20 381  	  LINE_ITEM
16:23:20 382  	WHERE
16:23:20 383  	  LINE_ITEM.INVOICE_ID = in_invoice_id;
16:23:20 384  
16:23:20 385  	-- get all pp charges for given invoice
16:23:20 386  	OPEN out_pp_charges_set FOR
16:23:20 387  	SELECT
16:23:20 388  	  CHARGE.ID as "CHARGE_ID",
16:23:20 389  	  CHARGE_AMOUNT,
16:23:20 390  	  CHARGE.INSTRUMENT_ID,
16:23:20 391  	  CHARGE.CHARGE_STATUS_ID
16:23:20 392  	FROM
16:23:20 393  	  CHARGE
16:23:20 394  	WHERE
16:23:20 395  	  CHARGE.INVOICE_ID = in_invoice_id
16:23:20 396  	  AND CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V19.INSTRUMENT_PAYPAL;
16:23:20 397  
16:23:20 398  	-- get all credir cards for given invoice
16:23:20 399  	OPEN out_cc_charges_set FOR
16:23:20 400  	SELECT
16:23:20 401  	  CHARGE.ID as "CHARGE_ID",
16:23:20 402  	  CHARGE.CHARGE_AMOUNT,
16:23:20 403  	  CHARGE.INSTRUMENT_ID,
16:23:20 404  	  CHARGE.CHARGE_STATUS_ID
16:23:20 405  	FROM
16:23:20 406  	  CHARGE
16:23:20 407  	WHERE
16:23:20 408  	  CHARGE.INVOICE_ID = in_invoice_id
16:23:20 409  	  AND CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V19.INSTRUMENT_CREDIT_CARD;
16:23:20 410  
16:23:20 411  	OPEN out_gc_charges_set FOR
16:23:20 412  	SELECT
16:23:20 413  	  CHARGE.ID as "CHARGE_ID",
16:23:20 414  	  CHARGE.CHARGE_AMOUNT,
16:23:20 415  	  CHARGE.INSTRUMENT_ID,
16:23:20 416  	  CHARGE.CHARGE_STATUS_ID
16:23:20 417  	FROM
16:23:20 418  	  CHARGE
16:23:20 419  	WHERE
16:23:20 420  	  CHARGE.INVOICE_ID = in_invoice_id
16:23:20 421  	  AND CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V19.INSTRUMENT_GIFT_CERTIFICATE;
16:23:20 422  
16:23:20 423  EXCEPTION
16:23:20 424  WHEN BAD_INVOICE_ID THEN
16:23:20 425  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 426  	  SPROC_NAME, 'No such invoice');
16:23:20 427  WHEN CAN_NOT_FIND_ACCOUNT THEN
16:23:20 428  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 429  	  SPROC_NAME, 'Could not find account for given invoice id', EXCEPTION_MESSAGE);
16:23:20 430  WHEN OTHERS THEN
16:23:20 431  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 432  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 433  END GET_INVOICE_DETAILS;
