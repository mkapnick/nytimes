16:23:20 695  /******************************************************************************/
16:23:20 696  
16:23:20 697  PROCEDURE GET_ENDED_GC_LICENSES (
16:23:20 698  	out_result_set		OUT SYS_REFCURSOR,
16:23:20 699  	in_hours_number 	IN NUMBER DEFAULT 14*24,
16:23:20 700  	in_num_rows		IN NUMBER DEFAULT 10000,
16:23:20 701  	in_process_name IN PROCESS_RETRY_THROTTLE.PROCESS_NAME%TYPE
16:23:20 702  ) AS
16:23:20 703  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_ENDED_GC_INVOICES';
16:23:20 704  var_days		NUMBER;
16:23:20 705  var_hours 	NUMBER;
16:23:20 706  var_time_interval INTERVAL DAY (3) TO SECOND;
16:23:20 707  BEGIN
16:23:20 708  	var_hours := mod(in_hours_number,24);
16:23:20 709  	var_days := (in_hours_number - var_hours) / 24;
16:23:20 710  	var_time_interval := var_days||' '||var_hours||':0:0';
16:23:20 711  	OPEN out_result_set FOR
16:23:20 712  SELECT * FROM
16:23:20 713  (
16:23:20 714  	SELECT
16:23:20 715  	  l.ID,
16:23:20 716  	  l.CREATE_DATE,
16:23:20 717  	  l.CREATED_BY,
16:23:20 718  	  l.CURRENT_OFFER_INDEX,
16:23:20 719  	  l.CURRENT_OFFER_RECURR_NUM,
16:23:20 720  	  l.END_DATE,
16:23:20 721  	  l.ENTITLEMENT_END_DATE,
16:23:20 722  	  l.INVOICE_ID,
16:23:20 723  	  l.IS_EXTENSION,
16:23:20 724  	  l.LICENSE_STATUS_ID,
16:23:20 725  	  l.OFFER_ID,
16:23:20 726  	  l.START_DATE,
16:23:20 727  	  l.SUBSCRIPTION_ID,
16:23:20 728  	  l.UPDATE_DATE,
16:23:20 729  	  l.UPDATED_BY
16:23:20 730  	FROM
16:23:20 731  	  GIFT_CERTIFICATE gc
16:23:20 732  	  INNER JOIN INVOICE i ON i.id = gc.PURCHASE_INVOICE_ID
16:23:20 733  	  INNER JOIN LICENSE l ON l.invoice_id = i.id
16:23:20 734  	  LEFT JOIN SUBSCRIPTION s ON s.id = l.subscription_id
16:23:20 735  	WHERE
16:23:20 736  	  l.LICENSE_STATUS_ID != GLOBAL_STATUSES_V19.LICENSE_ACTIVE
16:23:20 737  	  AND l.ENTITLEMENT_END_DATE <= (current_timestamp)
16:23:20 738  	  AND l.ENTITLEMENT_END_DATE > (current_timestamp - var_time_interval)
16:23:20 739  	  AND s.subscription_status_id = GLOBAL_STATUSES_V19.SUBSCRIPTION_CLOSED
16:23:20 740  	  AND NOT EXISTS (
16:23:20 741  	    SELECT NULL
16:23:20 742  	    FROM PROCESS_RETRY_THROTTLE
16:23:20 743  	    WHERE PROCESS_NAME = in_process_name
16:23:20 744  	      AND GENERIC_ID = l.ID
16:23:20 745  	  )
16:23:20 746  	  AND ROWNUM <= in_num_rows*10
16:23:20 747  	  ORDER BY dbms_random.value
16:23:20 748  ) WHERE
16:23:20 749  	  ROWNUM <= in_num_rows
16:23:20 750  	  GROUP BY SUBSCRIPTION_ID;
16:23:20 751  EXCEPTION
16:23:20 752  WHEN OTHERS THEN
16:23:20 753  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 754  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 755  END GET_ENDED_GC_LICENSES;
16:23:20 756  
16:23:20 757  PROCEDURE GET_LICENSE_BY_ID (
