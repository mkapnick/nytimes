16:23:20 935  /******************************************************************************/
16:23:20 936  
16:23:20 937  FUNCTION GET_RENEWAL_DATE (
16:23:20 938  /*
16:23:20 939  Throws exceptions:
16:23:20 940  APP_EXCEPTION_CODES_V19.NOT_FOUND
16:23:20 941  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
16:23:20 942  */
16:23:20 943  	in_subscription_id in NUMBER
16:23:20 944  ) RETURN DATE AS
16:23:20 945  -- VARIABLES
16:23:20 946  SPROC_NAME	      CONSTANT VARCHAR2(16) := 'GET_RENEWAL_DATE';
16:23:20 947  var_subscription_status NUMBER;
16:23:20 948  var_licenses_count      NUMBER;
16:23:20 949  var_license_end_date    DATE;
16:23:20 950  var_last_offer_id       NUMBER;
16:23:20 951  var_offer_chain_id      NUMBER;
16:23:20 952  var_last_license_id     NUMBER;
16:23:20 953  var_current_offer_index NUMBER;
16:23:20 954  var_current_offer_recurr_num NUMBER;
16:23:20 955  var_offer_recurr_num    NUMBER;
16:23:20 956  -- EXCEPTIONS
16:23:20 957  BAD_SUBSCRIPTION_ID EXCEPTION;
16:23:20 958  NO_LICENSES_FOUND EXCEPTION;
16:23:20 959  BEGIN
16:23:20 960  
16:23:20 961  	-- Get subscription id and offer chain id
16:23:20 962  	BEGIN
16:23:20 963  	  SELECT
16:23:20 964  	    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID,
16:23:20 965  	    SUBSCRIPTION.OFFER_CHAIN_ID
16:23:20 966  	    into
16:23:20 967  	    var_subscription_status,
16:23:20 968  	    var_offer_chain_id
16:23:20 969  	  FROM
16:23:20 970  	    SUBSCRIPTION
16:23:20 971  	  WHERE
16:23:20 972  	    SUBSCRIPTION.ID = in_subscription_id;
16:23:20 973  	  EXCEPTION
16:23:20 974  	    WHEN NO_DATA_FOUND THEN
16:23:20 975  	      RAISE BAD_SUBSCRIPTION_ID;
16:23:20 976  	END;
16:23:20 977  
16:23:20 978  	IF var_subscription_status != GLOBAL_STATUSES_V19.SUBSCRIPTION_ACTIVE THEN
16:23:20 979  	  -- TODO: Is suspended subscription has renewal date? (For the phase II)
16:23:20 980  	  -- AND var_subscription_status != GLOBAL_STATUSES_V19.SUBSCRIPTION_SUSPENDED THEN
16:23:20 981  	  RETURN NULL;
16:23:20 982  	END IF;
16:23:20 983  
16:23:20 984  	BEGIN
16:23:20 985  	  SELECT
16:23:20 986  	    LICENSE_ID,
16:23:20 987  	    END_DATE,
16:23:20 988  	    OFFER_ID,
16:23:20 989  	    CURRENT_OFFER_INDEX,
16:23:20 990  	    CURRENT_OFFER_RECURR_NUM
16:23:20 991  	    into
16:23:20 992  	    var_last_license_id,
16:23:20 993  	    var_license_end_date,
16:23:20 994  	    var_last_offer_id,
16:23:20 995  	    var_current_offer_index,
16:23:20 996  	    var_current_offer_recurr_num
16:23:20 997  	  FROM
16:23:20 998  	    (
16:23:20 999  	      SELECT
16:23:20 1000  		 LICENSE.ID as "LICENSE_ID",
16:23:20 1001  		 LICENSE.END_DATE,
16:23:20 1002  		 LICENSE.OFFER_ID,
16:23:20 1003  		 LICENSE.CURRENT_OFFER_INDEX,
16:23:20 1004  		 LICENSE.CURRENT_OFFER_RECURR_NUM
16:23:20 1005  	       FROM
16:23:20 1006  		 LICENSE
16:23:20 1007  	       WHERE
16:23:20 1008  		 LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V19.LICENSE_ACTIVE
16:23:20 1009  		 AND LICENSE.SUBSCRIPTION_ID = in_subscription_id
16:23:20 1010  	       ORDER BY END_DATE DESC
16:23:20 1011  	     )
16:23:20 1012  	     INNER JOIN OFFER ON OFFER_ID = OFFER.ID
16:23:20 1013  	   WHERE
16:23:20 1014  	     ROWNUM <= 1;
16:23:20 1015  
16:23:20 1016  	   EXCEPTION
16:23:20 1017  	     WHEN NO_DATA_FOUND THEN
16:23:20 1018  	       -- RAISE NO_LICENSES_FOUND;
16:23:20 1019  	       RETURN NULL;
16:23:20 1020  	 END;
16:23:20 1021  
16:23:20 1022  	 SELECT
16:23:20 1023  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES into var_offer_recurr_num
16:23:20 1024  	 FROM
16:23:20 1025  	   OFFER_OFFER_CHAIN
16:23:20 1026  	 WHERE
16:23:20 1027  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
16:23:20 1028  	   AND OFFER_OFFER_CHAIN.OFFER_ID = var_last_offer_id;
16:23:20 1029  
16:23:20 1030  	 IF PROCS_OFFER_CHAIN_V19.GET_NEXT_OFFER_INDEX(var_offer_chain_id, var_current_offer_index) IS NULL
16:23:20 1031  	   AND var_offer_recurr_num = var_current_offer_recurr_num THEN
16:23:20 1032  	   -- There is no next offer for this subscription
16:23:20 1033  	   RETURN NULL;
16:23:20 1034  	 END IF;
16:23:20 1035  
16:23:20 1036  	 RETURN var_license_end_date;
16:23:20 1037  
16:23:20 1038  EXCEPTION
16:23:20 1039  WHEN BAD_SUBSCRIPTION_ID THEN
16:23:20 1040  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 1041  	   SPROC_NAME, 'No such subscription');
16:23:20 1042  WHEN NO_LICENSES_FOUND THEN
16:23:20 1043  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 1044  	   SPROC_NAME, 'No licenses found');
16:23:20 1045  WHEN OTHERS THEN
16:23:20 1046  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 1047  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 1048  END GET_RENEWAL_DATE;
16:23:20 1049  
16:23:20 1050  /******************************************************************************/
16:23:20 1051  
16:23:20 1052  FUNCTION GET_RECENT_CHARGE (
16:23:20 1053  /*
16:23:20 1054  Throws exceptions:
16:23:20 1055  APP_EXCEPTION_CODES_V19.NOT_FOUND
16:23:20 1056  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
16:23:20 1057  */
16:23:20 1058  	 in_subscription_id IN NUMBER
16:23:20 1059  ) RETURN NUMBER AS
16:23:20 1060  -- VARIABLES
16:23:20 1061  SPROC_NAME	    CONSTANT VARCHAR2(17) := 'GET_RECENT_CHARGE';
16:23:20 1062  temp_subscription_id NUMBER;
16:23:20 1063  var_recent_charge    NUMBER(10,2);
16:23:20 1064  
16:23:20 1065  -- EXCEPTIONS
16:23:20 1066  BAD_SUBSCRIPTION_ID EXCEPTION;
16:23:20 1067  BEGIN
16:23:20 1068  
16:23:20 1069  	 BEGIN
16:23:20 1070  	   SELECT
16:23:20 1071  	     SUBSCRIPTION.ID into temp_subscription_id
16:23:20 1072  	   FROM
16:23:20 1073  	     SUBSCRIPTION
16:23:20 1074  	   WHERE
16:23:20 1075  	     SUBSCRIPTION.ID = in_subscription_id;
16:23:20 1076  	   EXCEPTION
16:23:20 1077  	     WHEN NO_DATA_FOUND THEN
16:23:20 1078  	       RAISE BAD_SUBSCRIPTION_ID;
16:23:20 1079  	 END;
16:23:20 1080  
16:23:20 1081  	 BEGIN
16:23:20 1082  	   SELECT
16:23:20 1083  	     CHARGE.CHARGE_AMOUNT into var_recent_charge
16:23:20 1084  	   FROM
16:23:20 1085  	     LICENSE
16:23:20 1086  	     INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
16:23:20 1087  	     INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
16:23:20 1088  	     INNER JOIN CHARGE ON CHARGE.INVOICE_ID = INVOICE.ID
16:23:20 1089  	   WHERE
16:23:20 1090  	     -- TODO: Review
16:23:20 1091  	     -- LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V19.LICENSE_PROCESSED
16:23:20 1092  	     -- AND
16:23:20 1093  	     SUBSCRIPTION.ID = in_subscription_id
16:23:20 1094  	     AND CHARGE.CHARGE_AMOUNT >= 0
16:23:20 1095  	     AND ROWNUM <= 1
16:23:20 1096  	   ORDER BY
16:23:20 1097  	     LICENSE.ID ASC, CHARGE.ID DESC;
16:23:20 1098  	   EXCEPTION
16:23:20 1099  	     WHEN NO_DATA_FOUND THEN
16:23:20 1100  	       var_recent_charge := 0;
16:23:20 1101  	 END;
16:23:20 1102  
16:23:20 1103  	 RETURN var_recent_charge;
16:23:20 1104  
16:23:20 1105  EXCEPTION
16:23:20 1106  WHEN BAD_SUBSCRIPTION_ID THEN
16:23:20 1107  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 1108  	   SPROC_NAME, 'No such subscription');
16:23:20 1109  WHEN OTHERS THEN
16:23:20 1110  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 1111  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 1112  END GET_RECENT_CHARGE;
16:23:20 1113  
16:23:20 1114  /******************************************************************************/
