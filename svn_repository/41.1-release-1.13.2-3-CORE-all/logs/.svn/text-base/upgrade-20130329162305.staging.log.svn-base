16:23:20 335  	    in_subscription_id => var_new_subscription_id,
16:23:20 336  	    in_agent_id        => in_agent_id,
16:23:20 337  	    in_note	       => in_note,
16:23:20 338  	    in_created_by      => in_created_by
16:23:20 339  	  );
16:23:20 340  	  EXCEPTION
16:23:20 341  	   WHEN OTHERS THEN
16:23:20 342  	     EXCEPTION_MESSAGE := SQLERRM;
16:23:20 343  	     RAISE CAN_NOT_CREATE_NOTE;
16:23:20 344  	END;
16:23:20 345  
16:23:20 346  	BEGIN
16:23:20 347  	  PROCS_LICENSE_V19.CREATE_LICENSE(
16:23:20 348  	    in_status_id		=> GLOBAL_STATUSES_V19.LICENSE_ACTIVE,
16:23:20 349  	    in_needs_entitlements	=> GLOBAL_CONSTANTS_V19.TRUE,
16:23:20 350  	    in_start_date		=> var_date,
16:23:20 351  	    in_end_date 		=> NULL, -- Will be calculated automatically
16:23:20 352  	    in_offer_id 		=> var_first_offer_id,
16:23:20 353  	    in_subscription_id		=> var_new_subscription_id,
16:23:20 354  	    in_invoice_id		=> var_new_invoice_id,
16:23:20 355  	    in_created_by		=> in_created_by,
16:23:20 356  	    in_is_extension		=> GLOBAL_CONSTANTS_V19.FALSE,
16:23:20 357  	    in_current_offer_index	=> PROCS_OFFER_CHAIN_V19.GET_FIRST_OFFER_INDEX(in_offer_chain_id),
16:23:20 358  	    in_current_offer_recurr_num => 1,
16:23:20 359  	    out_license_id		=> out_new_license_id
16:23:20 360  	  );
16:23:20 361  	  EXCEPTION
16:23:20 362  	    WHEN OTHERS THEN
16:23:20 363  	      EXCEPTION_MESSAGE := SQLERRM;
16:23:20 364  	      RAISE CAN_NOT_CREATE_LICENSE;
16:23:20 365  	END;
16:23:20 366  
16:23:20 367  	out_subscription_id := var_new_subscription_id;
16:23:20 368  	out_invoice_id := var_new_invoice_id;
16:23:20 369  
16:23:20 370  EXCEPTION
16:23:20 371  WHEN BAD_OFFER_CHAIN_STATUS THEN
16:23:20 372  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
16:23:20 373  	  SPROC_NAME, 'Offer chain is not active');
16:23:20 374  WHEN LIMIT_REACHED THEN
16:23:20 375  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.CONFLICT_ERROR,
16:23:20 376  	  SPROC_NAME, 'Limit reached for given offer chain. Concurrent subscription id: ' || var_concur_subscription_id);
16:23:20 377  WHEN CAN_NOT_CREATE_FOR_DISABLE THEN
16:23:20 378  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
16:23:20 379  	  SPROC_NAME, 'Can not create subsscription for disabled account');
16:23:20 380  WHEN CAN_NOT_SUBSCRIBE_TO_GC THEN
16:23:20 381  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
16:23:20 382  	  SPROC_NAME, 'Can not subscribe to Offer Chain for Gift Certificate');
16:23:20 383  WHEN CAN_NOT_SUBSCRIBE_TO_RGC THEN
16:23:20 384  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
16:23:20 385  	  SPROC_NAME, 'Can not subscribe to Offer Chain that is for redemption');
16:23:20 386  WHEN BAD_OFFER_CHAIN THEN
