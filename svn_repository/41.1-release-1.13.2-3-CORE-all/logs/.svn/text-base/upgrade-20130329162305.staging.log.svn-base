16:23:20 659  
16:23:20 660  PROCEDURE GET_SUBSCRIPTION_INFO (
16:23:20 661  /*
16:23:20 662  Throws exceptions:
16:23:20 663  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
16:23:20 664  */
16:23:20 665  	  in_subscription_id  IN  NUMBER,
16:23:20 666  	  out_result_set      OUT SYS_REFCURSOR
16:23:20 667  ) AS
16:23:20 668  SPROC_NAME CONSTANT VARCHAR2(21) := 'GET_SUBSCRIPTION_INFO';
16:23:20 669  -- VARIABLES
16:23:20 670  temp_subscription_id NUMBER;
16:23:20 671  -- EXCEPTIONS
16:23:20 672  BAD_SUBSCRIPTION_ID EXCEPTION;
16:23:20 673  BEGIN
16:23:20 674  	-- Find subscription by id
16:23:20 675  	-- Return its details
16:23:20 676  
16:23:20 677  	BEGIN
16:23:20 678  	  SELECT
16:23:20 679  	    SUBSCRIPTION.ID into temp_subscription_id
16:23:20 680  	  FROM
16:23:20 681  	    SUBSCRIPTION
16:23:20 682  	  WHERE
16:23:20 683  	    SUBSCRIPTION.ID = in_subscription_id;
16:23:20 684  	  EXCEPTION
16:23:20 685  	    WHEN NO_DATA_FOUND THEN
16:23:20 686  	      RAISE BAD_SUBSCRIPTION_ID;
16:23:20 687  	END;
16:23:20 688  
16:23:20 689  	OPEN out_result_set FOR
16:23:20 690  	SELECT
16:23:20 691  	  SUBSCRIPTION.ID AS "SUBSCRIPTION_ID",
16:23:20 692  	  SUBSCRIPTION.SUBSCRIPTION_STATUS_ID,
16:23:20 693  	  SUBSCRIPTION.PURCHASE_DATE,
16:23:20 694  	  SUBSCRIPTION.SUSPEND_DATE,
16:23:20 695  	  SUBSCRIPTION.REACTIVATION_DATE,
16:23:20 696  	  SUBSCRIPTION.CANCELLATION_DATE,
16:23:20 697  	  SUBSCRIPTION_CANCEL_REASON.VALUE as "CANCEL_TYPE",
16:23:20 698  	  OFFER_CHAIN.ID AS "OFFER_CHAIN_ID",
16:23:20 699  	  OFFER_CHAIN.NAME,
16:23:20 700  	  OFFER_CHAIN.DESCRIPTION,
16:23:20 701  	  OFFER_CHAIN.PRODUCT_URI,
16:23:20 702  	  SUBSCRIPTION.INSTRUMENT_ID,
16:23:20 703  	  SUBSCRIPTION.INSTRUMENT_TYPE_ID,
16:23:20 704  	  PROCS_SUBSCRIPTION_V19.CALC_SUBSCRIPTION_END_DATE(SUBSCRIPTION.ID) as "END_DATE",
16:23:20 705  	  PROCS_SUBSCRIPTION_V19.GET_RECENT_CHARGE(SUBSCRIPTION.ID) AS "RECENT_CHARGE",
16:23:20 706  	  PROCS_SUBSCRIPTION_V19.GET_RENEWAL_DATE(SUBSCRIPTION.ID) AS "RENEWAL_DATE",
16:23:20 707  	  PROCS_SUBSCRIPTION_V19.GET_BILLING_CYCLE(SUBSCRIPTION.ID) AS "BILLING_CYCLE",
16:23:20 708  	  (
16:23:20 709  	    SELECT
16:23:20 710  	      ACCOUNT.GROUP_ID
16:23:20 711  	      FROM ACCOUNT
16:23:20 712  	      WHERE ACCOUNT.ID = SUBSCRIPTION.ACCOUNT_ID
16:23:20 713  	  ) as "GROUP_ID",
16:23:20 714  	  (
16:23:20 715  	    SELECT
16:23:20 716  	      MAX(ENTITLEMENT_END_DATE)
16:23:20 717  	      FROM LICENSE
16:23:20 718  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
16:23:20 719  	  )
16:23:20 720  	  as "ENT_END_DATE",
16:23:20 721  	  (
16:23:20 722  	    SELECT
16:23:20 723  	      MIN(START_DATE)
16:23:20 724  	      FROM LICENSE
16:23:20 725  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
16:23:20 726  	  )
16:23:20 727  	  as "ENT_START_DATE",
16:23:20 728  	  PROCS_SUBSCRIPTION_V19.IS_SUBSCRIPTION_CANCELABLE(SUBSCRIPTION.ID) AS "IS_CANCELABLE",
16:23:20 729  	  ITUNES_RECEIPT.ID AS "ITUNES_RECEIPT_ID",
16:23:20 730  	  GROUP_ACCOUNT.ID GA_ID,
16:23:20 731  	  GROUP_ACCOUNT.SUBSCRIPTION_ID GA_SUBSCRIPTION_ID,
16:23:20 732  	  GROUP_ACCOUNT.GROUP_NAME GA_GROUP_NAME,
16:23:20 733  	  GROUP_ACCOUNT.FIRST_NAME GA_FIRST_NAME,
16:23:20 734  	  GROUP_ACCOUNT.LAST_NAME GA_LAST_NAME,
16:23:20 735  	  GROUP_ACCOUNT.EMAIL GA_EMAIL,
16:23:20 736  	  GROUP_ACCOUNT.PHONE GA_PHONE,
16:23:20 737  	  GROUP_ACCOUNT.ORGANIZATION_TYPE GA_ORGANIZATION_TYPE,
16:23:20 738  	  GROUP_ACCOUNT.SEATS GA_SEATS,
16:23:20 739  	  PROCS_GROUP_ACCOUNT_V19.F_GET_NUM_OCCUPIED_GROUP_SEATS(GROUP_ACCOUNT.ID) GA_SEATS_USED,
16:23:20 740  	  GROUP_ACCOUNT.IP GA_IP,
16:23:20 741  	  PROCS_SUBSCRIPTION_V19.GET_GIFT_CERT_CODE_BY_SUB_ID(SUBSCRIPTION.ID) gift_certificate_code,
16:23:20 742  	  PROCS_ACCOUNT_V19.GET_GRACE_START_DATE(SUBSCRIPTION.ID) GRACE_START_DATE,
16:23:20 743  	  PROCS_ACCOUNT_V19.GET_GRACE_END_DATE(SUBSCRIPTION.ID) GRACE_END_DATE
16:23:20 744  	FROM
16:23:20 745  	  SUBSCRIPTION
16:23:20 746  	  INNER JOIN OFFER_CHAIN ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
16:23:20 747  	  LEFT JOIN SUBSCRIPTION_CANCEL_REASON ON SUBSCRIPTION.SCT_ID = SUBSCRIPTION_CANCEL_REASON.ID
16:23:20 748  	  LEFT JOIN ITUNES_RECEIPT ON SUBSCRIPTION.ID = ITUNES_RECEIPT.SUBSCRIPTION_ID
16:23:20 749  	  LEFT JOIN GROUP_ACCOUNT ON SUBSCRIPTION.ID = GROUP_ACCOUNT.SUBSCRIPTION_ID
16:23:20 750  	WHERE
16:23:20 751  	  SUBSCRIPTION.ID = in_subscription_id;
16:23:20 752  
16:23:20 753  EXCEPTION
16:23:20 754  WHEN BAD_SUBSCRIPTION_ID THEN
16:23:20 755  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 756  	  SPROC_NAME, 'No such subscription');
16:23:20 757  WHEN OTHERS THEN
16:23:20 758  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 759  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 760  END GET_SUBSCRIPTION_INFO;
16:23:20 761  
