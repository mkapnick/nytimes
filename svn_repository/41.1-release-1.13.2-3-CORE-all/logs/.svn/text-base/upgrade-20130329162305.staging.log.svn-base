16:23:20 451  
16:23:20 452  PROCEDURE DISABLE_PAYPAL (
16:23:20 453  /*
16:23:20 454  Throws exceptions:
16:23:20 455  APP_EXCEPTION_CODES_V19.NOT_FOUND
16:23:20 456  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
16:23:20 457  APP_EXCEPTION_CODES_V19.INTERNAL_ERROR
16:23:20 458  */
16:23:20 459  	in_paypal_id  IN NUMBER,
16:23:20 460  	in_updated_by IN VARCHAR2
16:23:20 461  ) AS
16:23:20 462  SPROC_NAME CONSTANT VARCHAR2(14) := 'DISABLE_PAYPAL';
16:23:20 463  -- VARIABLES
16:23:20 464  var_paypal_status_id NUMBER;
16:23:20 465  var_account_id	   NUMBER;
16:23:20 466  var_group_id	   NUMBER;
16:23:20 467  var_pending_invoices_num NUMBER;
16:23:20 468  current_def_instrument_type_id NUMBER;
16:23:20 469  current_def_instrument_id NUMBER;
16:23:20 470  -- EXCEPTIONS
16:23:20 471  BAD_PAYPAL_ID EXCEPTION;
16:23:20 472  PAYPAL_ALREADY_INACTIVE EXCEPTION;
16:23:20 473  PENDING_TRANSACTIONS_FOUND EXCEPTION;
16:23:20 474  CAN_NOT_GET_DEF_FINANCIAL EXCEPTION;
16:23:20 475  CAN_NOT_DEL_DEF_FINANCIAL EXCEPTION;
16:23:20 476  EXCEPTION_MESSAGE VARCHAR2(1024);
16:23:20 477  BEGIN
16:23:20 478  
16:23:20 479  	-- Get Paypal status
16:23:20 480  	-- Get account
16:23:20 481  	BEGIN
16:23:20 482  	  SELECT
16:23:20 483  	    PAYPAL.PAYPAL_STATUS_ID,
16:23:20 484  	    PAYPAL.ACCOUNT_ID
16:23:20 485  	    into
16:23:20 486  	    var_paypal_status_id,
16:23:20 487  	    var_account_id
16:23:20 488  	  FROM
16:23:20 489  	    PAYPAL
16:23:20 490  	  WHERE
16:23:20 491  	    PAYPAL.ID = in_paypal_id;
16:23:20 492  	  EXCEPTION
16:23:20 493  	    WHEN NO_DATA_FOUND THEN
16:23:20 494  	      RAISE BAD_PAYPAL_ID;
16:23:20 495  	END;
16:23:20 496  
16:23:20 497  	-- Get group id
16:23:20 498  	SELECT
16:23:20 499  	  ACCOUNT.GROUP_ID into var_group_id
16:23:20 500  	FROM
16:23:20 501  	  ACCOUNT
16:23:20 502  	WHERE
16:23:20 503  	  ACCOUNT.ID = var_account_id;
16:23:20 504  
16:23:20 505  	-- Card should not be disabled
16:23:20 506  	IF var_paypal_status_id = GLOBAL_STATUSES_V19.PAYPAL_INACTIVE THEN
16:23:20 507  	  RAISE PAYPAL_ALREADY_INACTIVE;
16:23:20 508  	END IF;
16:23:20 509  
16:23:20 510  	-- Looking for pending transactions associated with given credit card
16:23:20 511  	SELECT
16:23:20 512  	  COUNT(*) into var_pending_invoices_num
16:23:20 513  	FROM
16:23:20 514  	  CHARGE
16:23:20 515  	  INNER JOIN TRANSACTION ON CHARGE.TRANSACTION_ID = TRANSACTION.ID
16:23:20 516  	WHERE
16:23:20 517  	  CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V19.INSTRUMENT_PAYPAL
16:23:20 518  	  AND CHARGE.INSTRUMENT_ID = in_paypal_id
16:23:20 519  	  AND TRANSACTION.TRANSACTION_STATUS_ID = GLOBAL_STATUSES_V19.TRANSACTION_PENDING;
16:23:20 520  
16:23:20 521  	IF var_pending_invoices_num > 0 THEN
16:23:20 522  	  RAISE PENDING_TRANSACTIONS_FOUND;
16:23:20 523  	END IF;
16:23:20 524  
16:23:20 525  	-- Getting current default financial instrument
16:23:20 526  	BEGIN
16:23:20 527  	  GET_DEF_FINANCIAL_INSTRUMENT(
16:23:20 528  	    in_group_id 	   => var_group_id,
16:23:20 529  	    out_instrument_type_id => current_def_instrument_type_id,
16:23:20 530  	    out_instrument_id	   => current_def_instrument_id
16:23:20 531  	  );
16:23:20 532  	  EXCEPTION
16:23:20 533  	    WHEN OTHERS THEN
16:23:20 534  	      EXCEPTION_MESSAGE := SQLERRM;
16:23:20 535  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
16:23:20 536  	END;
16:23:20 537  
16:23:20 538  	-- Checking that credit card is not default
16:23:20 539  	IF current_def_instrument_type_id = GLOBAL_ENUMS_V19.INSTRUMENT_PAYPAL
16:23:20 540  	  AND current_def_instrument_id = in_paypal_id THEN
16:23:20 541  	  BEGIN
16:23:20 542  	    DEL_DEF_FINANCIAL_INSTRUMENT(
16:23:20 543  	      in_group_id => var_group_id
16:23:20 544  	    );
16:23:20 545  	  EXCEPTION
16:23:20 546  	    WHEN OTHERS THEN
16:23:20 547  	      EXCEPTION_MESSAGE := SQLERRM;
16:23:20 548  	      RAISE CAN_NOT_DEL_DEF_FINANCIAL;
16:23:20 549  	  END;
16:23:20 550  	END IF;
16:23:20 551  
16:23:20 552  	PROCS_FIN_INSTRUMENTS_V19.UPDATE_PAYPAL_STATUS(
16:23:20 553  	  in_paypal_id	      => in_paypal_id,
16:23:20 554  	  in_updated_by       => in_updated_by,
16:23:20 555  	  in_paypal_status_id => GLOBAL_STATUSES_V19.PAYPAL_INACTIVE
16:23:20 556  	);
16:23:20 557  
16:23:20 558  EXCEPTION
16:23:20 559  WHEN BAD_PAYPAL_ID THEN
16:23:20 560  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 561  	  SPROC_NAME, 'No such paypal');
16:23:20 562  WHEN PAYPAL_ALREADY_INACTIVE THEN
16:23:20 563  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
16:23:20 564  	  SPROC_NAME, 'Paypal already inactive');
16:23:20 565  WHEN PENDING_TRANSACTIONS_FOUND THEN
16:23:20 566  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
16:23:20 567  	  SPROC_NAME, 'Account has pending charge which are using this paypal');
16:23:20 568  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
16:23:20 569  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 570  	  SPROC_NAME, 'Could not get current default financial instrument', EXCEPTION_MESSAGE);
16:23:20 571  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
16:23:20 572  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 573  	  SPROC_NAME, 'Could not delete information about default financial instrument from account', EXCEPTION_MESSAGE);
16:23:20 574  WHEN OTHERS THEN
16:23:20 575  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 576  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 577  END DISABLE_PAYPAL;
