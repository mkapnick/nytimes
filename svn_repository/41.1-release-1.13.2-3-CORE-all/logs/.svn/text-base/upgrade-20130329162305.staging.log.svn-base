16:23:20 1065  /******************************************************************************/
16:23:20 1066  
16:23:20 1067  PROCEDURE PURCHASE_GIFT_CERTIFICATE (
16:23:20 1068  	 in_group_id		   IN NUMBER,
16:23:20 1069  	 in_offer_chain_id	   IN VARCHAR2,
16:23:20 1070  	 in_gift_certificate_code  IN VARCHAR2,
16:23:20 1071  	 in_created_by		   IN VARCHAR2,
16:23:20 1072  	 in_recipient_name	   IN VARCHAR2,
16:23:20 1073  	 in_recipient_email	   IN VARCHAR2,
16:23:20 1074  	 in_sender_name 	   IN VARCHAR2,
16:23:20 1075  	 in_sender_email	   IN VARCHAR2,
16:23:20 1076  	 in_gift_message	   IN VARCHAR2,
16:23:20 1077  	 in_instrument_id	   IN NUMBER,
16:23:20 1078  	 in_instrument_type_id	   IN NUMBER,
16:23:20 1079  	 in_expiration_date	   IN DATE,
16:23:20 1080  	 in_order_id		   IN VARCHAR2,
16:23:20 1081  	 in_transaction_id	   IN NUMBER
16:23:20 1082  ) AS
16:23:20 1083  SPROC_NAME	  CONSTANT VARCHAR2(25) := 'PURCHASE_GIFT_CERTIFICATE';
16:23:20 1084  -- VARIABLES
16:23:20 1085  var_gift_cert_id   NUMBER;
16:23:20 1086  var_account_id	  NUMBER;
16:23:20 1087  var_invoice_amount NUMBER (10,2);
16:23:20 1088  var_new_invoice_id NUMBER;
16:23:20 1089  var_new_charge_id  NUMBER;
16:23:20 1090  var_och_is_gc	  NUMBER;
16:23:20 1091  var_offer_chain_status_id NUMBER;
16:23:20 1092  var_is_for_redemption	 NUMBER;
16:23:20 1093  var_account_tax_exempt_id VARCHAR2(255);
16:23:20 1094  
16:23:20 1095  temp_transaction_id_count NUMBER;
16:23:20 1096  var_transaction_id	 NUMBER;
16:23:20 1097  temp_gc_code VARCHAR2(255);
16:23:20 1098  
16:23:20 1099  var_invoice_status_id NUMBER;
16:23:20 1100  -- EXCEPTIONS
16:23:20 1101  CAN_NOT_CREATE_INVOICE		   EXCEPTION;
16:23:20 1102  CAN_NOT_CREATE_TRANSACTION	   EXCEPTION;
16:23:20 1103  CAN_NOT_CREATE_CHARGE		   EXCEPTION;
16:23:20 1104  CAN_NOT_CREATE_LINE_ITEMS	   EXCEPTION;
16:23:20 1105  BAD_GROUP_ID			   EXCEPTION;
16:23:20 1106  BAD_OFFER_CHAIN_ID		   EXCEPTION;
16:23:20 1107  OCH_IS_NOT_GIFT_CERTIFICATE	   EXCEPTION;
16:23:20 1108  TRANSACTION_EXISTS		   EXCEPTION;
16:23:20 1109  GC_CODE_ALREADY_EXISTS		   EXCEPTION;
16:23:20 1110  BAD_INSTRUMENT_TYPE		   EXCEPTION;
16:23:20 1111  BAD_CREDIT_CARD_ID		   EXCEPTION;
16:23:20 1112  BAD_PAYPAL_ID			   EXCEPTION;
16:23:20 1113  CAN_NOT_CALCULATE_OCH_AMOUNT	   EXCEPTION;
16:23:20 1114  BAD_OFFER_CHAIN_STATUS		   EXCEPTION;
16:23:20 1115  OFFER_REC_NUM_LESS_THAN_ONE	   EXCEPTION;
16:23:20 1116  CAN_NOT_CALC_INVOICE_AMOUNT	   EXCEPTION;
16:23:20 1117  CAN_NOT_USE_FCINSTR		   EXCEPTION;
16:23:20 1118  CAN_NOT_PURCHASE_GC_FOR_RDMPN	   EXCEPTION;
16:23:20 1119  EXCEPTION_MESSAGE		   VARCHAR2(1024);
16:23:20 1120  BEGIN
16:23:20 1121  
16:23:20 1122  	 -- Get account id
16:23:20 1123  	 BEGIN
16:23:20 1124  	   SELECT
16:23:20 1125  	     ACCOUNT.ID,
16:23:20 1126  	     ACCOUNT.TAX_EXEMPT_ID
