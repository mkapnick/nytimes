16:23:20 2807  	       s.subscription_status_id = GLOBAL_STATUSES_V19.SUBSCRIPTION_IN_GRACE_PERIOD
16:23:20 2808  	     )
16:23:20 2809  	     AND
16:23:20 2810  	     s.instrument_type_id = in_old_fin_instrument_type AND
16:23:20 2811  	     s.instrument_id = in_old_fin_instrument_id
16:23:20 2812  	 ) LOOP
16:23:20 2813  	   PROCS_SUBSCRIPTION_CRU_V19.UPDATE_SUBSCRIPTION(
16:23:20 2814  	     in_subscription_id => f_sub.id,
16:23:20 2815  	     in_instrument_type_id => in_new_fin_instrument_type,
16:23:20 2816  	     in_instrument_id => in_new_fin_instrument_id,
