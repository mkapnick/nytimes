16:23:20 697  /******************************************************************************/
16:23:20 698  
16:23:20 699  PROCEDURE CALCULATE_INVOICE_CHARGEBACK (
16:23:20 700  	in_invoice_id	      IN NUMBER,
16:23:20 701  	in_chargeback_date    IN DATE,
16:23:20 702  	out_chargeback_amount OUT NUMBER
16:23:20 703  ) AS
16:23:20 704  -- VARIABLES
16:23:20 705  SPROC_NAME	     CONSTANT VARCHAR2(28) := 'CALCULATE_INVOICE_CHARGEBACK';
16:23:20 706  var_chargeback_date    DATE;
16:23:20 707  temp_invoice_id	     NUMBER;
16:23:20 708  var_licenses_number    NUMBER;
16:23:20 709  var_invoice_start_date DATE;
16:23:20 710  var_invoice_end_date   DATE;
16:23:20 711  var_offer_id	     NUMBER;
16:23:20 712  var_offer_days_interval NUMBER;
16:23:20 713  var_license_days_used  NUMBER;
16:23:20 714  var_invoice_amount     NUMBER(10,2);
16:23:20 715  var_offer_chain_id     NUMBER;
16:23:20 716  var_offer_chain_meta_data_val VARCHAR2(1024);
16:23:20 717  var_offer_chain_full_refund   NUMBER;
16:23:20 718  var_offer_chain_prorated_ref  NUMBER;
16:23:20 719  var_chargeback_calculated NUMBER;
16:23:20 720  var_max_invoice_refund	NUMBER;
16:23:20 721  -- EXCEPTIONS
16:23:20 722  BAD_INVOICE_ID		  EXCEPTION;
16:23:20 723  NO_LICENSES_FOUND_EXCEPTION EXCEPTION;
16:23:20 724  OFFER_LENGTH_IS_ZERO	  EXCEPTION;
16:23:20 725  BEGIN
16:23:20 726  
16:23:20 727  	IF in_chargeback_date IS NULL THEN
16:23:20 728  	  var_chargeback_date := PROCS_COMMON_V19.NORMALIZE_DATE(current_date);
16:23:20 729  	ELSE
16:23:20 730  	  var_chargeback_date := PROCS_COMMON_V19.NORMALIZE_DATE(in_chargeback_date);
16:23:20 731  	END IF;
16:23:20 732  
16:23:20 733  	-- Check that invoice exists
16:23:20 734  	BEGIN
16:23:20 735  	  SELECT
16:23:20 736  	    INVOICE.ID into temp_invoice_id
16:23:20 737  	  FROM
16:23:20 738  	    INVOICE
16:23:20 739  	  WHERE
16:23:20 740  	    INVOICE.ID = in_invoice_id;
16:23:20 741  	  EXCEPTION
16:23:20 742  	    WHEN NO_DATA_FOUND THEN
16:23:20 743  	      RAISE BAD_INVOICE_ID;
16:23:20 744  	END;
16:23:20 745  
16:23:20 746  	-- Check that invoice has at least one license
16:23:20 747  	SELECT
16:23:20 748  	  COUNT(*) into var_licenses_number
16:23:20 749  	FROM
16:23:20 750  	  LICENSE
16:23:20 751  	WHERE
16:23:20 752  	  LICENSE.INVOICE_ID = in_invoice_id;
16:23:20 753  
16:23:20 754  	IF var_licenses_number = 0 THEN
16:23:20 755  	  RAISE NO_LICENSES_FOUND_EXCEPTION;
16:23:20 756  	END IF;
16:23:20 757  
16:23:20 758  	SELECT
16:23:20 759  	  MIN(LICENSE.START_DATE) into var_invoice_start_date
16:23:20 760  	FROM
16:23:20 761  	  LICENSE
16:23:20 762  	WHERE
16:23:20 763  	  LICENSE.INVOICE_ID = in_invoice_id;
16:23:20 764  
16:23:20 765  	var_invoice_start_date := PROCS_COMMON_V19.NORMALIZE_DATE(var_invoice_start_date);
16:23:20 766  
16:23:20 767  	SELECT DISTINCT
16:23:20 768  	  LICENSE.OFFER_ID into var_offer_id
16:23:20 769  	FROM
16:23:20 770  	  LICENSE
16:23:20 771  	WHERE
16:23:20 772  	  LICENSE.INVOICE_ID = in_invoice_id;
16:23:20 773  
16:23:20 774  	SELECT
16:23:20 775  	  MAX (LICENSE.END_DATE) into var_invoice_end_date
16:23:20 776  	FROM
16:23:20 777  	  LICENSE
16:23:20 778  	WHERE
16:23:20 779  	  LICENSE.INVOICE_ID = in_invoice_id;
16:23:20 780  
16:23:20 781  	-- All licenses for given invoice should point into the same offer
16:23:20 782  
16:23:20 783  	PROCS_OFFER_CHAIN_V19.GET_OFFER_LENGTH_IN_DAYS(
16:23:20 784  	  in_offer_id	=> var_offer_id,
16:23:20 785  	  in_start_date => var_invoice_start_date,
16:23:20 786  	  out_days	=> var_offer_days_interval
16:23:20 787  	);
16:23:20 788  
16:23:20 789  	IF var_offer_days_interval = 0 THEN
16:23:20 790  	  RAISE OFFER_LENGTH_IS_ZERO;
16:23:20 791  	END IF;
16:23:20 792  
16:23:20 793  	PROCS_INVOICE_V19.GET_INVOICE_DAYS_USED_NUMBER(
16:23:20 794  	  in_invoice_id      => in_invoice_id,
16:23:20 795  	  in_chargeback_date => var_chargeback_date,
16:23:20 796  	  out_days_num	     => var_license_days_used
16:23:20 797  	);
16:23:20 798  
16:23:20 799  	PROCS_INVOICE_V19.CALCULATE_INVOICE_AMOUNT(
16:23:20 800  	  in_invoice_id => in_invoice_id,
16:23:20 801  	  out_amount	=> var_invoice_amount
16:23:20 802  	);
16:23:20 803  
16:23:20 804  	-- 39437
16:23:20 805  	-- Get offer chain id by invoice id
16:23:20 806  	SELECT DISTINCT
16:23:20 807  	  SUBSCRIPTION.OFFER_CHAIN_ID into var_offer_chain_id
16:23:20 808  	FROM
16:23:20 809  	  SUBSCRIPTION
16:23:20 810  	  INNER JOIN LICENSE ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
16:23:20 811  	WHERE
16:23:20 812  	  LICENSE.INVOICE_ID = in_invoice_id;
16:23:20 813  
16:23:20 814  	-- Get offer chain's meta data full amount value
16:23:20 815  	PROCS_OFFER_CHAIN_V19.GET_OFFER_CHAIN_MD_VALUE(
16:23:20 816  	  in_offer_chain_id => var_offer_chain_id,
16:23:20 817  	  in_meta_data_name => GLOBAL_CONSTANTS_V19.OFFER_CHAIN_FULL_REFUND,
16:23:20 818  	  out_value	    => var_offer_chain_meta_data_val
16:23:20 819  	);
16:23:20 820  	IF var_offer_chain_meta_data_val IS NULL THEN
16:23:20 821  	  var_offer_chain_full_refund := NULL;
16:23:20 822  	ELSE
16:23:20 823  	  var_offer_chain_full_refund := TO_NUMBER(var_offer_chain_meta_data_val);
16:23:20 824  	END IF;
16:23:20 825  
16:23:20 826  	-- Get offer chain's meta data prorated amount value
16:23:20 827  	PROCS_OFFER_CHAIN_V19.GET_OFFER_CHAIN_MD_VALUE(
16:23:20 828  	  in_offer_chain_id => var_offer_chain_id,
16:23:20 829  	  in_meta_data_name => GLOBAL_CONSTANTS_V19.OFFER_CHAIN_PRORATED_REFUND,
16:23:20 830  	  out_value	    => var_offer_chain_meta_data_val
16:23:20 831  	);
16:23:20 832  	IF var_offer_chain_meta_data_val IS NULL THEN
16:23:20 833  	  var_offer_chain_prorated_ref := NULL;
16:23:20 834  	ELSE
16:23:20 835  	  var_offer_chain_prorated_ref := TO_NUMBER(var_offer_chain_meta_data_val);
16:23:20 836  	END IF;
16:23:20 837  
16:23:20 838  	var_chargeback_calculated := GLOBAL_CONSTANTS_V19.FALSE;
16:23:20 839  
16:23:20 840  	IF var_offer_chain_full_refund IS NOT NULL
16:23:20 841  	   AND var_chargeback_calculated = GLOBAL_CONSTANTS_V19.FALSE THEN
16:23:20 842  	  IF var_license_days_used < var_offer_chain_full_refund THEN
16:23:20 843  	    out_chargeback_amount := var_invoice_amount;
16:23:20 844  	    var_chargeback_calculated := GLOBAL_CONSTANTS_V19.TRUE;
16:23:20 845  	  END IF;
16:23:20 846  	END IF;
16:23:20 847  
16:23:20 848  	IF var_offer_chain_prorated_ref IS NOT NULL
16:23:20 849  	   AND var_chargeback_calculated = GLOBAL_CONSTANTS_V19.FALSE THEN
16:23:20 850  	  IF var_license_days_used < var_offer_chain_prorated_ref THEN
16:23:20 851  	    out_chargeback_amount := ( var_invoice_amount * (var_offer_days_interval - var_license_days_used) ) / var_offer_days_interval;
16:23:20 852  	    var_chargeback_calculated := GLOBAL_CONSTANTS_V19.TRUE;
16:23:20 853  	  END IF;
16:23:20 854  	END IF;
16:23:20 855  
16:23:20 856  	IF var_chargeback_calculated = GLOBAL_CONSTANTS_V19.TRUE THEN
16:23:20 857  	  PROCS_INVOICE_V19.GET_MAX_REFUND(
16:23:20 858  	    in_invoice_id => in_invoice_id,
16:23:20 859  	    out_amount	  => var_max_invoice_refund
16:23:20 860  	  );
16:23:20 861  	  IF var_max_invoice_refund < out_chargeback_amount THEN
16:23:20 862  	    out_chargeback_amount := var_max_invoice_refund;
16:23:20 863  	  END IF;
16:23:20 864  	END IF;
16:23:20 865  
16:23:20 866  	IF var_chargeback_calculated = GLOBAL_CONSTANTS_V19.FALSE THEN
16:23:20 867  	  out_chargeback_amount := 0;
16:23:20 868  	END IF;
16:23:20 869  
16:23:20 870  EXCEPTION
16:23:20 871  WHEN BAD_INVOICE_ID THEN
16:23:20 872  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 873  	  SPROC_NAME, 'No such invoice');
16:23:20 874  WHEN NO_LICENSES_FOUND_EXCEPTION THEN
16:23:20 875  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 876  	  SPROC_NAME, 'No licenses found');
16:23:20 877  WHEN OFFER_LENGTH_IS_ZERO THEN
16:23:20 878  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 879  	  SPROC_NAME, 'Offer length is zero');
16:23:20 880  WHEN OTHERS THEN
16:23:20 881  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 882  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 883  END CALCULATE_INVOICE_CHARGEBACK;
