16:23:20 573  /******************************************************************************/
16:23:20 574  
16:23:20 575  PROCEDURE GET_ACCOUNT_NOTES (
16:23:20 576  /*
16:23:20 577  Throws exceptions:
16:23:20 578  APP_EXCEPTION_CODES_V19.NOT_FOUND
16:23:20 579  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
16:23:20 580  */
16:23:20 581  	  in_group_id	 IN  NUMBER,
16:23:20 582  	  out_result_set OUT SYS_REFCURSOR
16:23:20 583  ) AS
16:23:20 584  -- VARIABLES
16:23:20 585  SPROC_NAME      CONSTANT VARCHAR2(17) := 'GET_ACCOUNT_NOTES';
16:23:20 586  var_account_id NUMBER;
16:23:20 587  -- EXCEPTIONS
16:23:20 588  BAD_ACCOUNT_ID EXCEPTION;
16:23:20 589  BEGIN
16:23:20 590  
16:23:20 591  	-- Check that account is exists
16:23:20 592  	BEGIN
16:23:20 593  	  SELECT
16:23:20 594  	    ACCOUNT.ID into var_account_id
16:23:20 595  	  FROM
16:23:20 596  	    ACCOUNT
16:23:20 597  	  WHERE
16:23:20 598  	    ACCOUNT.GROUP_ID = in_group_id;
16:23:20 599  	  EXCEPTION
16:23:20 600  	    WHEN NO_DATA_FOUND THEN
16:23:20 601  	      RAISE BAD_ACCOUNT_ID;
16:23:20 602  	END;
16:23:20 603  
16:23:20 604  	-- Get account notes
16:23:20 605  	OPEN out_result_set FOR
