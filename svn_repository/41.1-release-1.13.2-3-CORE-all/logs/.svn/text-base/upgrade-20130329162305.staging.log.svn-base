16:23:20   6  	  function getDiscountAmount(in_line_item_id line_item.id%type)
16:23:20   7  	      return line_item.amount%type
16:23:20   8  	  is
16:23:20   9  	      v_discount  line_item.amount%type := 0;
16:23:20  10  	  begin
16:23:20  11  	      for x in (
16:23:20  12  		  select d.id discount_id
16:23:20  13  			,nvl(d.fixed_amount, d.percent_amount * (po.quantity * po.unit_price)) discount_amount
16:23:20  14  		  from discount d
16:23:20  15  		      join discount_line_item dli	      on dli.discount_id = d.id
16:23:20  16  		      join discount_product_offering dop      on dop.discount_id = d.id
16:23:20  17  		      join product_offering po		      on po.id = dop.product_offering_id
16:23:20  18  		      join line_item li 		      on li.id = dli.line_item_id and li.product_offer_id = po.id
16:23:20  19  		  where dli.line_item_id = in_line_item_id
16:23:20  20  	      )
16:23:20  21  	      loop
16:23:20  22  		  v_discount := v_discount + x.discount_amount;
16:23:20  23  	      end loop;
16:23:20  24  
16:23:20  25  	      return v_discount;
16:23:20  26  	  end;
