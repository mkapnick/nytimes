16:23:20 916  /******************************************************************************/
16:23:20 917  
16:23:20 918  PROCEDURE FINALIZE_GC_PURCHASING (
16:23:20 919  	in_invoice_id	      IN NUMBER,
16:23:20 920  	in_created_by	      IN VARCHAR2,
16:23:20 921  	in_instrument_id      IN NUMBER,
16:23:20 922  	in_instrument_type_id IN NUMBER,
16:23:20 923  	in_order_id	      IN VARCHAR2,
16:23:20 924  	in_transaction_id     IN NUMBER,
16:23:20 925  	out_charge_amount     OUT NUMBER
16:23:20 926  ) AS
16:23:20 927  SPROC_NAME CONSTANT VARCHAR2(22) := 'FINALIZE_GC_PURCHASING';
16:23:20 928  -- VARIABLES
16:23:20 929  temp_transaction_id_count NUMBER;
16:23:20 930  var_invoice_amount	NUMBER(10,2);
16:23:20 931  var_transaction_id	NUMBER;
16:23:20 932  var_new_charge_id 	NUMBER;
16:23:20 933  -- EXCEPTIONS
16:23:20 934  BAD_CREDIT_CARD_ID	  EXCEPTION;
16:23:20 935  BAD_PAYPAL_ID		  EXCEPTION;
16:23:20 936  BAD_INSTRUMENT_TYPE	  EXCEPTION;
16:23:20 937  TRANSACTION_EXISTS	  EXCEPTION;
16:23:20 938  CAN_NOT_CALC_INVOICE_AMOUNT EXCEPTION;
16:23:20 939  CAN_NOT_USE_FCINSTR	  EXCEPTION;
16:23:20 940  CAN_NOT_CREATE_TRANSACTION  EXCEPTION;
16:23:20 941  CAN_NOT_CREATE_CHARGE	  EXCEPTION;
16:23:20 942  EXCEPTION_MESSAGE   VARCHAR2(1024);
16:23:20 943  BEGIN
16:23:20 944  
16:23:20 945  	-- Check that instrument exists
16:23:20 946  	IF in_instrument_type_id = GLOBAL_ENUMS_V19.INSTRUMENT_CREDIT_CARD THEN
16:23:20 947  	  IF IS_CREDIT_CARD_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V19.FALSE THEN
16:23:20 948  	    RAISE BAD_CREDIT_CARD_ID;
16:23:20 949  	  END IF;
16:23:20 950  	ELSIF in_instrument_type_id = GLOBAL_ENUMS_V19.INSTRUMENT_PAYPAL THEN
16:23:20 951  	  IF IS_PAYPAL_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V19.FALSE THEN
16:23:20 952  	    RAISE BAD_PAYPAL_ID;
16:23:20 953  	  END IF;
16:23:20 954  	ELSIF in_instrument_type_id = GLOBAL_ENUMS_V19.INSTRUMENT_ZCI_INSTRUMENT THEN
16:23:20 955  	  NULL;
16:23:20 956  	ELSE
16:23:20 957  	  RAISE BAD_INSTRUMENT_TYPE;
16:23:20 958  	END IF;
16:23:20 959  
16:23:20 960  	-- Check that transaction with given id do not exists
16:23:20 961  	SELECT
16:23:20 962  	  COUNT(*) into temp_transaction_id_count
16:23:20 963  	FROM
16:23:20 964  	  TRANSACTION
16:23:20 965  	WHERE
16:23:20 966  	  TRANSACTION.ID = in_transaction_id;
16:23:20 967  
16:23:20 968  	IF temp_transaction_id_count > 0 THEN
16:23:20 969  	  RAISE TRANSACTION_EXISTS;
16:23:20 970  	END IF;
16:23:20 971  
16:23:20 972  	-- Calculate new invoice amount
16:23:20 973  	BEGIN
16:23:20 974  	  PROCS_INVOICE_V19.CALCULATE_INVOICE_AMOUNT(in_invoice_id, var_invoice_amount);
16:23:20 975  	  EXCEPTION
16:23:20 976  	    WHEN OTHERS THEN
16:23:20 977  	      EXCEPTION_MESSAGE := SQLERRM;
16:23:20 978  	      RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
16:23:20 979  	END;
16:23:20 980  
16:23:20 981  	IF var_invoice_amount > 0
16:23:20 982  	  AND in_instrument_type_id = GLOBAL_ENUMS_V19.INSTRUMENT_ZCI_INSTRUMENT THEN
16:23:20 983  	  RAISE CAN_NOT_USE_FCINSTR;
16:23:20 984  	END IF;
16:23:20 985  
16:23:20 986  	-- If invoice amount iz 0 then we need to set status for this invoice to PROCCESSED
16:23:20 987  	IF var_invoice_amount = 0 THEN
16:23:20 988  	  PROCS_INVOICE_CRU_V19.UPDATE_INVOICE(
16:23:20 989  	    in_invoice_id		   => in_invoice_id,
16:23:20 990  	    in_updated_by		   => in_created_by,
16:23:20 991  	    in_invoice_status_id	   => GLOBAL_STATUSES_V19.INVOICE_CLOSED
16:23:20 992  	  );
16:23:20 993  	END IF;
16:23:20 994  
16:23:20 995  	IF var_invoice_amount > 0 THEN
16:23:20 996  	  -- Create transaction
16:23:20 997  	  BEGIN
16:23:20 998  	    PROCS_TRANSACTION_V19.CREATE_TRANSACTION(
16:23:20 999  	      in_transaction_id        => in_transaction_id,
16:23:20 1000  	       in_status_id		=> GLOBAL_STATUSES_V19.TRANSACTION_PENDING,
16:23:20 1001  	       in_amount		=> var_invoice_amount,
16:23:20 1002  	       in_created_by		=> in_created_by,
16:23:20 1003  	       in_order_id		=> in_order_id,
16:23:20 1004  	       in_transaction_type_code => 'GIFT_CERTIFICATE_PURCHASE',
16:23:20 1005  	       out_transaction_id	=> var_transaction_id
16:23:20 1006  	     );
16:23:20 1007  	     EXCEPTION
16:23:20 1008  	       WHEN OTHERS THEN
16:23:20 1009  		 EXCEPTION_MESSAGE := SQLERRM;
16:23:20 1010  		 RAISE CAN_NOT_CREATE_TRANSACTION;
16:23:20 1011  	   END;
16:23:20 1012  
16:23:20 1013  	   -- Create charge
16:23:20 1014  	   BEGIN
16:23:20 1015  	     PROCS_CHARGE_V19.CREATE_CHARGE(
16:23:20 1016  	       in_invoice_id	     => in_invoice_id,
16:23:20 1017  	       in_transaction_id     => var_transaction_id,
16:23:20 1018  	       in_instrument_type_id => in_instrument_type_id,
16:23:20 1019  	       in_instrument_id      => in_instrument_id,
16:23:20 1020  	       in_charge_amount      => var_invoice_amount,
16:23:20 1021  	       in_created_by	     => in_created_by,
16:23:20 1022  	       in_charge_status_id   => GLOBAL_STATUSES_V19.CHARGE_OPENED,
16:23:20 1023  	       out_charge_id	     => var_new_charge_id
16:23:20 1024  	     );
16:23:20 1025  	     out_charge_amount := var_invoice_amount;
16:23:20 1026  	     EXCEPTION
16:23:20 1027  	       WHEN OTHERS THEN
16:23:20 1028  		 EXCEPTION_MESSAGE := SQLERRM;
16:23:20 1029  		 RAISE CAN_NOT_CREATE_CHARGE;
16:23:20 1030  	   END;
16:23:20 1031  	 ELSE
16:23:20 1032  	   out_charge_amount := 0;
16:23:20 1033  	 END IF;
16:23:20 1034  
16:23:20 1035  EXCEPTION
16:23:20 1036  WHEN CAN_NOT_USE_FCINSTR THEN
16:23:20 1037  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
16:23:20 1038  	   SPROC_NAME, 'Could not use "free charge instrument" for non-zero invoice');
16:23:20 1039  WHEN BAD_CREDIT_CARD_ID THEN
16:23:20 1040  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 1041  	   SPROC_NAME, 'Bad credit card id');
16:23:20 1042  WHEN BAD_PAYPAL_ID THEN
16:23:20 1043  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 1044  	   SPROC_NAME, 'Bad paypal id');
16:23:20 1045  WHEN BAD_INSTRUMENT_TYPE THEN
16:23:20 1046  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
16:23:20 1047  	   SPROC_NAME, 'Bad instrument type');
16:23:20 1048  WHEN TRANSACTION_EXISTS THEN
16:23:20 1049  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.DUPLICATE_ERROR,
16:23:20 1050  	   SPROC_NAME, 'Transaction with given id already exists');
16:23:20 1051  WHEN CAN_NOT_CREATE_TRANSACTION THEN
16:23:20 1052  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 1053  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
16:23:20 1054  WHEN CAN_NOT_CREATE_CHARGE THEN
16:23:20 1055  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 1056  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
16:23:20 1057  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
16:23:20 1058  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 1059  	   SPROC_NAME, 'Could not calculate amount for new invoice', EXCEPTION_MESSAGE);
16:23:20 1060  WHEN OTHERS THEN
16:23:20 1061  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 1062  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 1063  END FINALIZE_GC_PURCHASING;
