16:23:20 885  /********************************************************************/
16:23:20 886  
16:23:20 887  PROCEDURE APPLY_REFUND (
16:23:20 888  	in_invoice_id	     IN NUMBER,
16:23:20 889  	in_chargeback_amount IN NUMBER,
16:23:20 890  	in_created_by	     IN VARCHAR2,
16:23:20 891  	out_charge_id	     OUT NUMBER
16:23:20 892  ) AS
16:23:20 893  -- VARIABLES
16:23:20 894  SPROC_NAME		 CONSTANT VARCHAR2(16) := 'APPLY_CHARGEBACK';
16:23:20 895  temp_invoice_id		 NUMBER;
16:23:20 896  var_total_charges_amount	 NUMBER(10,2);
16:23:20 897  var_charge_amount_to_apply NUMBER(10,2);
16:23:20 898  var_transaction_id	 NUMBER;
16:23:20 899  var_instrument_type_id	 NUMBER;
16:23:20 900  var_instrument_id 	 NUMBER;
16:23:20 901  var_charge_id		 NUMBER;
16:23:20 902  -- EXCEPTIONS
16:23:20 903  BAD_INVOICE_ID		 EXCEPTION;
16:23:20 904  CAN_NOT_CREATE_TRANSACTION EXCEPTION;
16:23:20 905  CAN_NOT_FIND_INSTRUMENT	 EXCEPTION;
16:23:20 906  CAN_NOT_CREATE_CHARGE	 EXCEPTION;
16:23:20 907  EXCEPTION_MESSAGE 	 VARCHAR2(1024);
16:23:20 908  BEGIN
16:23:20 909  
16:23:20 910  	BEGIN
16:23:20 911  	  SELECT
16:23:20 912  	    INVOICE.ID into temp_invoice_id
16:23:20 913  	  FROM
16:23:20 914  	    INVOICE
16:23:20 915  	  WHERE
16:23:20 916  	    INVOICE.ID = in_invoice_id;
16:23:20 917  	  EXCEPTION
16:23:20 918  	    WHEN NO_DATA_FOUND THEN
16:23:20 919  	      RAISE BAD_INVOICE_ID;
16:23:20 920  	END;
16:23:20 921  
16:23:20 922  	SELECT
16:23:20 923  	  SUM (CHARGE.CHARGE_AMOUNT) into var_total_charges_amount
16:23:20 924  	FROM
16:23:20 925  	  CHARGE
16:23:20 926  	WHERE
16:23:20 927  	  CHARGE.INVOICE_ID = in_invoice_id
16:23:20 928  	  AND CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V19.CHARGE_PROCESSED
16:23:20 929  	  AND CHARGE.INSTRUMENT_TYPE_ID != GLOBAL_ENUMS_V19.INSTRUMENT_GIFT_CERTIFICATE;
16:23:20 930  
16:23:20 931  	-- FIXME: Maybe whe should throw exception here?
16:23:20 932  	IF var_total_charges_amount < in_chargeback_amount THEN
16:23:20 933  	  var_charge_amount_to_apply := var_total_charges_amount;
16:23:20 934  	ELSE
16:23:20 935  	  var_charge_amount_to_apply := in_chargeback_amount;
16:23:20 936  	END IF;
16:23:20 937  
16:23:20 938  	BEGIN
16:23:20 939  	  PROCS_TRANSACTION_V19.CREATE_TRANSACTION(
16:23:20 940  	    in_transaction_id	      => NULL,
16:23:20 941  	    in_status_id	      => GLOBAL_STATUSES_V19.TRANSACTION_PREPARE,
16:23:20 942  	    in_amount		      => -var_charge_amount_to_apply,
16:23:20 943  	    in_created_by	      => in_created_by,
16:23:20 944  	    in_order_id 	      => NULL,
16:23:20 945  	    in_is_refund	      => GLOBAL_CONSTANTS_V19.TRUE,
16:23:20 946  	    in_transaction_type_code  => 'REFUND',
16:23:20 947  	    out_transaction_id	      => var_transaction_id
16:23:20 948  	  );
16:23:20 949  	  EXCEPTION
16:23:20 950  	    WHEN OTHERS THEN
16:23:20 951  	      EXCEPTION_MESSAGE := SQLERRM;
16:23:20 952  	      RAISE CAN_NOT_CREATE_TRANSACTION;
16:23:20 953  	END;
16:23:20 954  
16:23:20 955  	BEGIN
16:23:20 956  	  SELECT
16:23:20 957  	    C.INSTRUMENT_TYPE_ID,
16:23:20 958  	    C.INSTRUMENT_ID
16:23:20 959  	    into
16:23:20 960  	    var_instrument_type_id,
16:23:20 961  	    var_instrument_id
16:23:20 962  	  FROM
16:23:20 963  	    CHARGE C,
16:23:20 964  	    TRANSACTION_ATTEMPT TA,
16:23:20 965  	    TRANSACTION T
16:23:20 966  	  WHERE
16:23:20 967  	    C.INVOICE_ID = in_invoice_id and
16:23:20 968  	    C.TRANSACTION_ID = T.ID and
16:23:20 969  	    TA.TRANSACTION_ID = T.ID and
16:23:20 970  	    TA.TRANSACTION_ATTEMPT_STATUS_ID = GLOBAL_STATUSES_V19.TRANS_ATTEMPT_SUCCESS and
16:23:20 971  	    T.IS_REFUND = GLOBAL_CONSTANTS_V19.FALSE and
16:23:20 972  	    T.TRANSACTION_AMOUNT >= 0 and
16:23:20 973  	    rownum < 2;
16:23:20 974  	EXCEPTION
16:23:20 975  	    WHEN NO_DATA_FOUND THEN
16:23:20 976  	      RAISE CAN_NOT_FIND_INSTRUMENT;
16:23:20 977  	END;
16:23:20 978  	BEGIN
16:23:20 979  	  PROCS_CHARGE_V19.CREATE_CHARGE(
16:23:20 980  	    in_invoice_id	  => in_invoice_id,
16:23:20 981  	    in_transaction_id	  => var_transaction_id,
16:23:20 982  	    in_instrument_type_id => var_instrument_type_id,
16:23:20 983  	    in_instrument_id	  => var_instrument_id,
16:23:20 984  	    in_charge_amount	  => -var_charge_amount_to_apply,
16:23:20 985  	    in_created_by	  => in_created_by,
16:23:20 986  	    in_charge_status_id   => GLOBAL_STATUSES_V19.CHARGE_OPENED,
16:23:20 987  	    out_charge_id	  => var_charge_id
16:23:20 988  	  );
16:23:20 989  	  EXCEPTION
16:23:20 990  	    WHEN OTHERS THEN
16:23:20 991  	      EXCEPTION_MESSAGE := SQLERRM;
16:23:20 992  	      RAISE CAN_NOT_CREATE_CHARGE;
16:23:20 993  	END;
16:23:20 994  
16:23:20 995  	out_charge_id := var_charge_id;
16:23:20 996  
16:23:20 997  EXCEPTION
16:23:20 998  WHEN BAD_INVOICE_ID THEN
16:23:20 999  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 1000  	   SPROC_NAME, 'No such invoice');
16:23:20 1001  WHEN CAN_NOT_CREATE_TRANSACTION THEN
16:23:20 1002  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 1003  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
16:23:20 1004  WHEN CAN_NOT_FIND_INSTRUMENT THEN
16:23:20 1005  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 1006  	   SPROC_NAME, 'Could not find financial instrument');
16:23:20 1007  WHEN CAN_NOT_CREATE_CHARGE THEN
16:23:20 1008  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 1009  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
16:23:20 1010  WHEN OTHERS THEN
16:23:20 1011  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 1012  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 1013  END APPLY_REFUND;
16:23:20 1014  
16:23:20 1015  /******************************************************************************/
16:23:20 1016  
16:23:20 1017  PROCEDURE GET_MAX_REFUND (
16:23:20 1018  	 in_invoice_id IN NUMBER,
16:23:20 1019  	 out_amount    OUT NUMBER
16:23:20 1020  ) AS
16:23:20 1021  SPROC_NAME CONSTANT VARCHAR2(14) := 'GET_MAX_REFUND';
16:23:20 1022  -- VARIABLES
16:23:20 1023  temp_invoice_id	       NUMBER;
16:23:20 1024  var_invoice_refunds_sum NUMBER(10,2);
16:23:20 1025  var_invoice_charges_sum NUMBER(10,2);
16:23:20 1026  -- EXCEPTIONS
16:23:20 1027  BAD_INVOICE_ID EXCEPTION;
16:23:20 1028  BEGIN
16:23:20 1029  
16:23:20 1030  	 BEGIN
16:23:20 1031  	   SELECT
16:23:20 1032  	     INVOICE.ID into temp_invoice_id
16:23:20 1033  	   FROM
16:23:20 1034  	     INVOICE
16:23:20 1035  	   WHERE
16:23:20 1036  	     INVOICE.ID = in_invoice_id;
16:23:20 1037  	   EXCEPTION
16:23:20 1038  	     WHEN NO_DATA_FOUND THEN
16:23:20 1039  	       RAISE BAD_INVOICE_ID;
16:23:20 1040  	 END;
16:23:20 1041  
16:23:20 1042  	 var_invoice_refunds_sum := 0;
16:23:20 1043  	 var_invoice_charges_sum := 0;
16:23:20 1044  
16:23:20 1045  	 FOR f_charge IN (
16:23:20 1046  	   SELECT
16:23:20 1047  	     CHARGE.ID,
16:23:20 1048  	     CHARGE.CHARGE_STATUS_ID,
16:23:20 1049  	     CHARGE.CHARGE_AMOUNT,
16:23:20 1050  	     CHARGE.TRANSACTION_ID
16:23:20 1051  	   FROM
16:23:20 1052  	     CHARGE
16:23:20 1053  	   WHERE
16:23:20 1054  	     CHARGE.INVOICE_ID = in_invoice_id
16:23:20 1055  	 )
16:23:20 1056  	 LOOP
16:23:20 1057  	   -- If charge.status = canceled then continue
16:23:20 1058  	   IF f_charge.CHARGE_STATUS_ID != GLOBAL_STATUSES_V19.CHARGE_CANCELED THEN
16:23:20 1059  
16:23:20 1060  	     IF f_charge.CHARGE_AMOUNT > 0 THEN
16:23:20 1061  	       IF f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V19.CHARGE_PROCESSED
16:23:20 1062  		  AND PROCS_TRANSACTION_V19.IS_TRANSACTION_COLLECTED(f_charge.TRANSACTION_ID) = GLOBAL_CONSTANTS_V19.TRUE THEN
16:23:20 1063  		 -- Transaction collected
16:23:20 1064  		 var_invoice_charges_sum := var_invoice_charges_sum + f_charge.CHARGE_AMOUNT;
16:23:20 1065  	       ELSE
16:23:20 1066  		 -- Transaction is not collected. Do nothing
16:23:20 1067  		 NULL;
16:23:20 1068  	       END IF;
16:23:20 1069  	     ELSE
16:23:20 1070  	       IF f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V19.CHARGE_PROCESSED
16:23:20 1071  		  AND PROCS_TRANSACTION_V19.IS_TRANSACTION_COLLECTED(f_charge.TRANSACTION_ID) = GLOBAL_CONSTANTS_V19.FALSE THEN
16:23:20 1072  		 -- If charge is processed transaction is not collected then do nothing
16:23:20 1073  		 NULL;
16:23:20 1074  	       ELSE
16:23:20 1075  		 var_invoice_refunds_sum := var_invoice_refunds_sum + f_charge.CHARGE_AMOUNT;
16:23:20 1076  	       END IF;
16:23:20 1077  	     END IF;
16:23:20 1078  
16:23:20 1079  	   END IF;
16:23:20 1080  	 END LOOP;
16:23:20 1081  
16:23:20 1082  	 -- Refunds are negative
16:23:20 1083  	 var_invoice_refunds_sum := 0 - var_invoice_refunds_sum;
16:23:20 1084  
16:23:20 1085  	 out_amount := var_invoice_charges_sum - var_invoice_refunds_sum;
16:23:20 1086  
16:23:20 1087  EXCEPTION
16:23:20 1088  WHEN BAD_INVOICE_ID THEN
16:23:20 1089  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 1090  	   SPROC_NAME, 'No such invoice');
16:23:20 1091  WHEN OTHERS THEN
16:23:20 1092  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 1093  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 1094  END GET_MAX_REFUND;
16:23:20 1095  
16:23:20 1096  /******************************************************************************/
16:23:20 1097  
16:23:20 1098  PROCEDURE GET_INVOICE_DAYS_USED_NUMBER (
16:23:20 1099  	 in_invoice_id	     IN NUMBER,
16:23:20 1100  	 in_chargeback_date  IN DATE DEFAULT SYSDATE,
16:23:20 1101  	 out_days_num	     OUT NUMBER
16:23:20 1102  ) AS
16:23:20 1103  SPROC_NAME CONSTANT VARCHAR2(28) := 'GET_INVOICE_DAYS_USED_NUMBER';
16:23:20 1104  -- VARIABLES
16:23:20 1105  temp_invoice_id	      NUMBER;
16:23:20 1106  var_license_start_date DATE;
16:23:20 1107  var_license_end_date   DATE;
16:23:20 1108  var_chargeback_date    DATE;
16:23:20 1109  var_invoice_days_used  NUMBER;
16:23:20 1110  -- EXCEPTIONS
16:23:20 1111  BAD_INVOICE_ID EXCEPTION;
16:23:20 1112  BEGIN
16:23:20 1113  
16:23:20 1114  	 var_chargeback_date := NVL(in_chargeback_date, SYSDATE);
16:23:20 1115  
16:23:20 1116  	 BEGIN
16:23:20 1117  	   SELECT
16:23:20 1118  	     INVOICE.ID into temp_invoice_id
16:23:20 1119  	   FROM
16:23:20 1120  	     INVOICE
16:23:20 1121  	   WHERE
16:23:20 1122  	     INVOICE.ID = in_invoice_id;
16:23:20 1123  	   EXCEPTION
16:23:20 1124  	     WHEN NO_DATA_FOUND THEN
16:23:20 1125  	       RAISE BAD_INVOICE_ID;
16:23:20 1126  	 END;
16:23:20 1127  
16:23:20 1128  	 var_invoice_days_used := 0;
16:23:20 1129  
16:23:20 1130  	 FOR f_license IN (
16:23:20 1131  	   SELECT
16:23:20 1132  	     LICENSE.START_DATE,
16:23:20 1133  	     LICENSE.END_DATE
16:23:20 1134  	   FROM
16:23:20 1135  	     LICENSE
16:23:20 1136  	   WHERE
16:23:20 1137  	     LICENSE.INVOICE_ID = in_invoice_id
16:23:20 1138  	     AND LICENSE.IS_EXTENSION = GLOBAL_CONSTANTS_V19.FALSE
16:23:20 1139  	 )
16:23:20 1140  	 LOOP
16:23:20 1141  	   var_license_start_date := PROCS_COMMON_V19.NORMALIZE_DATE(f_license.START_DATE);
16:23:20 1142  	   var_license_end_date := PROCS_COMMON_V19.NORMALIZE_DATE(f_license.END_DATE);
16:23:20 1143  
16:23:20 1144  	   IF var_license_start_date <= var_chargeback_date THEN
16:23:20 1145  	     IF var_license_end_date <= var_chargeback_date THEN
16:23:20 1146  	       -- License is passed
16:23:20 1147  	       var_invoice_days_used := var_invoice_days_used + (var_license_end_date - var_license_start_date);
16:23:20 1148  	     ELSE
16:23:20 1149  	       -- This is current license
16:23:20 1150  	       var_invoice_days_used := var_invoice_days_used + (var_chargeback_date - var_license_start_date);
16:23:20 1151  	     END IF;
16:23:20 1152  	   ELSE
16:23:20 1153  	     -- if var_license_start_date > in_chargeback_date then do nothing
16:23:20 1154  	     NULL;
16:23:20 1155  	   END IF;
16:23:20 1156  	 END LOOP;
16:23:20 1157  
16:23:20 1158  	 out_days_num := var_invoice_days_used;
16:23:20 1159  
16:23:20 1160  EXCEPTION
16:23:20 1161  WHEN BAD_INVOICE_ID THEN
16:23:20 1162  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 1163  	   SPROC_NAME, 'No such invoice');
16:23:20 1164  WHEN OTHERS THEN
16:23:20 1165  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 1166  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 1167  END GET_INVOICE_DAYS_USED_NUMBER;
16:23:20 1168  
16:23:20 1169  /******************************************************************************/
16:23:20 1170  
16:23:20 1171  PROCEDURE GET_INVOICE_LINE_ITEMS (
16:23:20 1172  	 in_invoice_id	IN NUMBER,
16:23:20 1173  	 out_result_set OUT SYS_REFCURSOR
16:23:20 1174  ) AS
16:23:20 1175  SPROC_NAME CONSTANT VARCHAR2(22) := 'GET_INVOICE_LINE_ITEMS';
16:23:20 1176  -- VARIABLES
16:23:20 1177  temp_invoice_id NUMBER;
16:23:20 1178  -- EXCEPTIONS
16:23:20 1179  BAD_INVOICE_ID EXCEPTION;
16:23:20 1180  BEGIN
16:23:20 1181  
16:23:20 1182  	 BEGIN
16:23:20 1183  	   SELECT
16:23:20 1184  	     INVOICE.ID into temp_invoice_id
16:23:20 1185  	   FROM
16:23:20 1186  	     INVOICE
16:23:20 1187  	   WHERE
16:23:20 1188  	     INVOICE.ID = in_invoice_id;
16:23:20 1189  	   EXCEPTION
16:23:20 1190  	     WHEN NO_DATA_FOUND THEN
