16:23:20 1016  -- norlov: #38796
16:23:20 1017  PROCEDURE GET_TRANSACTIONS (
16:23:20 1018  /*
16:23:20 1019  Throws exceptions:
16:23:20 1020  APP_EXCEPTION_CODES_V19.NOT_FOUND
16:23:20 1021  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
16:23:20 1022  */
16:23:20 1023  	 in_group_id	       IN  NUMBER,
16:23:20 1024  	 in_invoice_id	       IN NUMBER DEFAULT NULL,
16:23:20 1025  	 in_subscription_id    IN NUMBER DEFAULT NULL,
16:23:20 1026  	 in_start_date	       IN TRANSACTION.CREATE_DATE%TYPE DEFAULT NULL,
16:23:20 1027  	 in_end_date	       IN TRANSACTION.CREATE_DATE%TYPE DEFAULT NULL,
16:23:20 1028  	 in_transaction_status IN NUMBER DEFAULT NULL,
16:23:20 1029  	 out_result_set        OUT SYS_REFCURSOR
16:23:20 1030  ) AS
16:23:20 1031  SPROC_NAME CONSTANT VARCHAR2(16) := 'GET_TRANSACTIONS';
16:23:20 1032  -- VARIABLES
16:23:20 1033  var_account_id ACCOUNT.ID%TYPE;
16:23:20 1034  statement VARCHAR2(2000);
16:23:20 1035  -- EXCEPTIONS
16:23:20 1036  BAD_GROUP_ID   EXCEPTION;
16:23:20 1037  BEGIN
16:23:20 1038  	-- check group id
16:23:20 1039  	BEGIN
16:23:20 1040  	   SELECT
16:23:20 1041  	     ACCOUNT.ID into var_account_id
16:23:20 1042  	   FROM
16:23:20 1043  	     ACCOUNT
16:23:20 1044  	   WHERE
16:23:20 1045  	     ACCOUNT.GROUP_ID = in_group_id;
16:23:20 1046  	   EXCEPTION
16:23:20 1047  	     WHEN NO_DATA_FOUND THEN
16:23:20 1048  	       RAISE BAD_GROUP_ID;
16:23:20 1049  	 END;
16:23:20 1050  
16:23:20 1051  	 OPEN out_result_set FOR
16:23:20 1052  	 SELECT
16:23:20 1053  	   TRANSACTION.ID,
16:23:20 1054  	   TRANSACTION.TRANSACTION_STATUS_ID,
16:23:20 1055  	   TRANSACTION.TRANSACTION_AMOUNT,
16:23:20 1056  	   TRANSACTION.CREATE_DATE,
16:23:20 1057  	   TRANSACTION.CREATED_BY,
16:23:20 1058  	   TRANSACTION.IS_REFUND,
16:23:20 1059  	   GET_TRANSACTION_TAX_AMOUNT(TRANSACTION.ID) as TRANSACTION_TAX_AMOUNT,
16:23:20 1060  	   GET_TRANSACTION_INTRL_TAXES(TRANSACTION.ID) as INTERNATIONAL_TOTAL
16:23:20 1061  	 FROM
16:23:20 1062  	   TRANSACTION
16:23:20 1063  	   INNER JOIN CHARGE ON TRANSACTION.ID = CHARGE.TRANSACTION_ID
16:23:20 1064  	   INNER JOIN INVOICE ON INVOICE.ID = CHARGE.INVOICE_ID
16:23:20 1065  	 WHERE
16:23:20 1066  	   -- Filter by invoice ID
16:23:20 1067  	   (
16:23:20 1068  	     INVOICE.ID IN (
16:23:20 1069  	       -- Gift certificate invoices
16:23:20 1070  	       SELECT
16:23:20 1071  		 GIFT_CERTIFICATE.PURCHASE_INVOICE_ID
16:23:20 1072  	       FROM
16:23:20 1073  		 GIFT_CERTIFICATE
16:23:20 1074  	       WHERE
16:23:20 1075  		 GIFT_CERTIFICATE.PURCHASER_GROUP_ID = in_group_id
16:23:20 1076  		 -- If subscription_id is set then return nothing
16:23:20 1077  		 AND EXISTS (SELECT 1 FROM DUAL WHERE in_subscription_id IS NULL)
16:23:20 1078  	     )
16:23:20 1079  	     OR
16:23:20 1080  	     INVOICE.ID IN (
16:23:20 1081  	       SELECT
16:23:20 1082  		 LICENSE.INVOICE_ID
16:23:20 1083  	       FROM
16:23:20 1084  		 LICENSE
16:23:20 1085  		 INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
16:23:20 1086  	       WHERE
16:23:20 1087  		 SUBSCRIPTION.ACCOUNT_ID = var_account_id
16:23:20 1088  		 -- Filter by subscription id
16:23:20 1089  		 AND SUBSCRIPTION.ID = NVL(in_subscription_id, SUBSCRIPTION.ID)
16:23:20 1090  	     )
16:23:20 1091  	   )
16:23:20 1092  	   -- Filter by invoice id
16:23:20 1093  	   AND INVOICE.ID = NVL(in_invoice_id, INVOICE.ID)
16:23:20 1094  	   -- Filter by start date
16:23:20 1095  	   AND TRANSACTION.CREATE_DATE >= NVL(in_start_date, TRANSACTION.CREATE_DATE)
16:23:20 1096  	   -- Filter by end date
16:23:20 1097  	   AND TRANSACTION.CREATE_DATE <= NVL(in_end_date, TRANSACTION.CREATE_DATE)
16:23:20 1098  	   -- Filter by transaction status
16:23:20 1099  	   AND TRANSACTION.TRANSACTION_STATUS_ID IN ( SELECT NVL(in_transaction_status, TRANSACTION.TRANSACTION_STATUS_ID) FROM DUAL);
16:23:20 1100  
16:23:20 1101  /*
16:23:20 1102  
16:23:20 1103  statement :=  'select distinct '||CHR(10)
16:23:20 1104  || ' TRANSACTION.ID,'||CHR(10)
16:23:20 1105  || ' TRANSACTION.TRANSACTION_STATUS_ID, '||CHR(10)
16:23:20 1106  || ' TRANSACTION.TRANSACTION_AMOUNT, '||CHR(10)
16:23:20 1107  || ' TRANSACTION.CREATE_DATE, '||CHR(10)
16:23:20 1108  || ' TRANSACTION.CREATED_BY, '||CHR(10)
16:23:20 1109  || ' TRANSACTION.IS_REFUND from TRANSACTION '||CHR(10)
16:23:20 1110  || ' inner join CHARGE on (CHARGE.TRANSACTION_ID = TRANSACTION.ID)'||CHR(10)
16:23:20 1111  || ' inner join INVOICE on (INVOICE.ID = CHARGE.INVOICE_ID)'||CHR(10)
16:23:20 1112  || ' inner join LICENSE on (LICENSE.INVOICE_ID = INVOICE.ID)'||CHR(10)
16:23:20 1113  || ' inner join SUBSCRIPTION on (SUBSCRIPTION.ID = LICENSE.SUBSCRIPTION_ID)'||CHR(10)
16:23:20 1114  || ' inner join ACCOUNT on (ACCOUNT.ID = SUBSCRIPTION.ACCOUNT_ID)'||CHR(10)
16:23:20 1115  || ' where ACCOUNT.GROUP_ID = '||in_group_id;
16:23:20 1116  
16:23:20 1117  IF (in_transaction_status IS NOT NULL) THEN
16:23:20 1118  	 statement := statement || CHR(10) || 'AND TRANSACTION.TRANSACTION_STATUS_ID=' || in_transaction_status;
16:23:20 1119  END IF;
16:23:20 1120  
16:23:20 1121  IF (in_invoice_id IS NOT NULL) THEN
16:23:20 1122  	 statement := statement || CHR(10) || 'AND INVOICE.ID=' || in_invoice_id;
16:23:20 1123  END IF;
16:23:20 1124  
16:23:20 1125  IF (in_subscription_id IS NOT NULL) THEN
16:23:20 1126  	 statement := statement || CHR(10) || 'AND SUBSCRIPTION.ID=' || in_subscription_id;
16:23:20 1127  END IF;
16:23:20 1128  
16:23:20 1129  IF (in_start_date IS NOT NULL) THEN
16:23:20 1130  	 statement := statement || CHR(10) || 'AND TRANSACTION.CREATE_DATE>= TO_DATE(''' || TO_CHAR(in_start_date,'yyyy/mm/dd:hh:mi:ss') || ''',''yyyy/mm/dd:hh:mi:ss'')';  -- norlov: ??
16:23:20 1131  END IF;
16:23:20 1132  
16:23:20 1133  IF (in_end_date IS NOT NULL) THEN
16:23:20 1134  	 statement := statement || CHR(10) || 'AND TRANSACTION.CREATE_DATE<= TO_DATE(''' || TO_CHAR(in_end_date,'yyyy/mm/dd:hh:mi:ss') || ''',''yyyy/mm/dd:hh:mi:ss'')'; -- norlov: ??
16:23:20 1135  END IF;
16:23:20 1136  dbms_output.put_line(statement);
16:23:20 1137  OPEN out_result_set FOR statement;
16:23:20 1138  
16:23:20 1139  */
16:23:20 1140  
16:23:20 1141  EXCEPTION
16:23:20 1142  WHEN BAD_GROUP_ID THEN
16:23:20 1143  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 1144  	   SPROC_NAME, 'No such group');
16:23:20 1145  WHEN OTHERS THEN
16:23:20 1146  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 1147  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 1148  END GET_TRANSACTIONS;
