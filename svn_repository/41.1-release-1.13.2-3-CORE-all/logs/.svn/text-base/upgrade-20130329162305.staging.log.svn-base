16:23:20 2756  
16:23:20 2757  	 -- Check that owner of both instruments - same man
16:23:20 2758  
16:23:20 2759  	 SELECT count(1) into var_accounts_count FROM (
16:23:20 2760  	   SELECT
16:23:20 2761  	     CC.ACCOUNT_ID
16:23:20 2762  	   FROM
16:23:20 2763  	     CREDIT_CARD CC
