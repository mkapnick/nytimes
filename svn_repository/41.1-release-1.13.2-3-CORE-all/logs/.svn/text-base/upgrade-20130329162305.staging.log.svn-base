16:23:20 763  
16:23:20 764  PROCEDURE GET_SUBSCRIPTION_INVOICES (
16:23:20 765  /*
16:23:20 766  Throws exceptions:
16:23:20 767  APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 768  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
16:23:20 769  */
16:23:20 770  	in_subscription_id  IN	NUMBER,
16:23:20 771  	out_result_set	    OUT SYS_REFCURSOR
16:23:20 772  ) AS
16:23:20 773  SPROC_NAME	   CONSTANT VARCHAR2(25) := 'GET_SUBSCRIPTION_INVOICES';
16:23:20 774  temp_subscription_id NUMBER;
16:23:20 775  BEGIN
16:23:20 776  	-- Note: A subscription has one or more associated licenses, each of which has an associated invoice.
16:23:20 777  	-- Find associated LICENSES for the subscription by "LICENSE.subscription_id"
16:23:20 778  	--   for each license
16:23:20 779  	--     get associated invoice
16:23:20 780  	--     add to results list
16:23:20 781  	--   end loop
16:23:20 782  	-- end
16:23:20 783  
16:23:20 784  	SELECT
16:23:20 785  	  SUBSCRIPTION.ID into temp_subscription_id
16:23:20 786  	FROM
16:23:20 787  	  SUBSCRIPTION
16:23:20 788  	WHERE
16:23:20 789  	  SUBSCRIPTION.ID = in_subscription_id;
16:23:20 790  
16:23:20 791  	OPEN out_result_set FOR
16:23:20 792  	SELECT
16:23:20 793  	  INVOICE.ID,
16:23:20 794  	  INVOICE.INVOICE_STATUS_ID,
16:23:20 795  	  INVOICE.CREATE_DATE,
16:23:20 796  	  INVOICE.CREATED_BY,
16:23:20 797  	  INVOICE.UPDATE_DATE,
16:23:20 798  	  INVOICE.UPDATED_BY,
16:23:20 799  	  INVOICE.TAX_EXEMPT_ID
16:23:20 800  	FROM
16:23:20 801  	  LICENSE
16:23:20 802  	    INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
16:23:20 803  	WHERE
16:23:20 804  	  LICENSE.SUBSCRIPTION_ID = in_subscription_id;
16:23:20 805  
16:23:20 806  EXCEPTION
16:23:20 807  WHEN NO_DATA_FOUND THEN
16:23:20 808  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 809  	  SPROC_NAME, 'Could not find subscription with given ID');
16:23:20 810  WHEN OTHERS THEN
16:23:20 811  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 812  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 813  END GET_SUBSCRIPTION_INVOICES;
