16:23:20 1356  	     in_charge_status_id => GLOBAL_STATUSES_V19.CHARGE_CANCELED
16:23:20 1357  	   );
16:23:20 1358  	 END LOOP;
16:23:20 1359  
16:23:20 1360  	 --FOR f_license_to_cancel IN (
16:23:20 1361  	 --  SELECT LICENSE.ID FROM LICENSE WHERE LICENSE.INVOICE_ID = in_invoice_id AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V19.LICENSE_ACTIVE
16:23:20 1362  	 --)
16:23:20 1363  	 --LOOP
