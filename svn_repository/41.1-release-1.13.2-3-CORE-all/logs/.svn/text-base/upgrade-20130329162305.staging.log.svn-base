16:23:20 380  	END;
16:23:20 381  
16:23:20 382  	IF var_offer_num_recurrences = 0 THEN
16:23:20 383  	  --out_result_set := NULL;
16:23:20 384  	  -- TODO: Remove this hardcode
16:23:20 385  	  OPEN out_result_set FOR
16:23:20 386  	  SELECT
16:23:20 387  	    OFFER.ID,
16:23:20 388  	    OFFER.OFFER_STATUS_ID,
16:23:20 389  	    OFFER.ENTITLEMENT_DURATION,
16:23:20 390  	    OFFER.CREATED_BY,
16:23:20 391  	    OFFER.CREATE_DATE,
16:23:20 392  	    OFFER.UPDATED_BY,
16:23:20 393  	    OFFER.UPDATE_DATE,
16:23:20 394  	    var_offer_num_recurrences as "RECURRENCE_NUMBER",
16:23:20 395  	    var_offer_index as "OFFER_INDEX"
16:23:20 396  	  FROM
16:23:20 397  	    OFFER_OFFER_CHAIN
16:23:20 398  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
16:23:20 399  	  WHERE ROWNUM = 0;
16:23:20 400  	ELSIF var_license_cur_offer_rec_num = var_offer_num_recurrences THEN
16:23:20 401  	  --out_result_set := NULL;
16:23:20 402  	  -- TODO: Remove this hardcode
16:23:20 403  	  OPEN out_result_set FOR
16:23:20 404  	  SELECT
16:23:20 405  	    OFFER.ID,
