16:23:19 1680  	       )
16:23:19 1681  	       INNER JOIN OFFER_PRODUCT_OFFERING ON OFFER_OFFER_CHAIN_OFFER_ID = OFFER_PRODUCT_OFFERING.OFFER_ID
16:23:19 1682  	       INNER JOIN PRODUCT_OFFERING ON OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = PRODUCT_OFFERING.ID
16:23:19 1683  	     GROUP BY
16:23:19 1684  	       PRODUCT_OFFERING.PRODUCT_ID
16:23:19 1685  	   )
16:23:19 1686  	   INNER JOIN
16:23:19 1687  	   (
16:23:19 1688  	     SELECT
16:23:19 1689  	       PRODUCT_OFFERING.PRODUCT_ID as "PRODUCT_ID_IN_OFFER_CH_2",
16:23:19 1690  	       COUNT(*) 		   as "COUNT_2"
16:23:19 1691  	     FROM
16:23:19 1692  	       (
16:23:19 1693  		 SELECT OFFER_ID as "OFFER_OFFER_CHAIN_OFFER_ID" FROM OFFER_OFFER_CHAIN WHERE OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_2
16:23:19 1694  	       )
16:23:19 1695  	       INNER JOIN OFFER_PRODUCT_OFFERING ON OFFER_OFFER_CHAIN_OFFER_ID = OFFER_PRODUCT_OFFERING.OFFER_ID
16:23:19 1696  	       INNER JOIN PRODUCT_OFFERING ON OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = PRODUCT_OFFERING.ID
16:23:19 1697  	     GROUP BY
16:23:19 1698  	       PRODUCT_OFFERING.PRODUCT_ID
16:23:19 1699  	   ) ON PRODUCT_ID_IN_OFFER_CH_1 = PRODUCT_ID_IN_OFFER_CH_2;
16:23:19 1700  
16:23:19 1701  END GET_OFF_CHAINS_SAME_PRODUCTS;
