16:23:20 2945  	 END;
16:23:20 2946  	 SELECT
16:23:20 2947  	   COUNT(*) into out_result
16:23:20 2948  	 FROM
16:23:20 2949  	   SUBSCRIPTION
16:23:20 2950  	     INNER JOIN ACCOUNT ON SUBSCRIPTION.ACCOUNT_ID = ACCOUNT.ID
16:23:20 2951  	 WHERE
