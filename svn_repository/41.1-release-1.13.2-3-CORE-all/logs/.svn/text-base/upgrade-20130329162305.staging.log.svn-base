16:23:20 192  	-- For now, we can disable account whenever
16:23:20 193  	IF current_account_status = GLOBAL_STATUSES_V19.ACCOUNT_DISABLED THEN
16:23:20 194  	  RAISE BAD_CURRENT_ACC_STATUS;
16:23:20 195  	END IF;
16:23:20 196  
16:23:20 197  	-- Checks for out outstanding balances
16:23:20 198  	-- CHECK: No outstanding balances. If monies are due, then we can not cancel account. Return ERROR.
16:23:20 199  	SELECT
16:23:20 200  	  COUNT(*) into var_pending_invoices_num
16:23:20 201  	FROM
16:23:20 202  	  LICENSE
16:23:20 203  	    INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
16:23:20 204  	    INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
16:23:20 205  	WHERE
16:23:20 206  	  SUBSCRIPTION.ACCOUNT_ID = var_account_id
16:23:20 207  	  AND INVOICE.INVOICE_STATUS_ID IN ( SELECT GLOBAL_STATUSES_V19.INVOICE_OPEN FROM DUAL );
16:23:20 208  
16:23:20 209  	IF var_pending_invoices_num > 0 THEN
16:23:20 210  	  RAISE PENDING_INVOICES_FOUND;
16:23:20 211  	END IF;
16:23:20 212  
16:23:20 213  	SELECT
16:23:20 214  	  COUNT(*) into var_active_subscriptions_num
16:23:20 215  	FROM
16:23:20 216  	  SUBSCRIPTION
16:23:20 217  	WHERE
16:23:20 218  	  SUBSCRIPTION.ACCOUNT_ID = var_account_id
16:23:20 219  	  AND (
16:23:20 220  	    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V19.SUBSCRIPTION_ACTIVE
16:23:20 221  	    OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V19.SUBSCRIPTION_SUSPENDED
16:23:20 222  	    OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V19.SUBSCRIPTION_IN_GRACE_PERIOD);
16:23:20 223  
16:23:20 224  	IF var_active_subscriptions_num > 0 THEN
16:23:20 225  	  RAISE ACCOUNT_HAS_ACIVE_SUBSCRS;
16:23:20 226  	END IF;
16:23:20 227  
16:23:20 228  	PROCS_ACCOUNT_V19.UPDATE_ACCOUNT_STATUS(
16:23:20 229  	  in_account_id        => var_account_id,
16:23:20 230  	  in_account_status_id => GLOBAL_STATUSES_V19.ACCOUNT_DISABLED,
16:23:20 231  	  in_updated_by        => in_updated_by
16:23:20 232  	);
16:23:20 233  
16:23:20 234  	-- Annotate account
16:23:20 235  	IF in_note IS NOT NULL THEN
16:23:20 236  	  BEGIN
16:23:20 237  	    PROCS_ACCOUNT_V19.ANNOTATE_ACCOUNT(
16:23:20 238  	      in_group_id   => in_group_id,
16:23:20 239  	      in_agent_id   => in_agent_id,
16:23:20 240  	      in_note	    => in_note,
16:23:20 241  	      in_created_by => in_updated_by
16:23:20 242  	    );
16:23:20 243  	    EXCEPTION
16:23:20 244  	      WHEN OTHERS THEN
16:23:20 245  		EXCEPTION_MESSAGE := SQLERRM;
16:23:20 246  		RAISE CAN_NOT_ANNOTATE_ACCOUNT;
16:23:20 247  	  END;
16:23:20 248  	END IF;
16:23:20 249  
16:23:20 250  EXCEPTION
16:23:20 251  WHEN ACCOUNT_HAS_ACIVE_SUBSCRS THEN
16:23:20 252  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 253  	  SPROC_NAME, 'Account has active or suspended subsciptions');
16:23:20 254  WHEN BAD_CURRENT_ACC_STATUS THEN
16:23:20 255  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
16:23:20 256  	  SPROC_NAME, 'Current account status is "disabled". Can not disable it one more time.');
16:23:20 257  WHEN PENDING_INVOICES_FOUND THEN
16:23:20 258  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 259  	  SPROC_NAME, 'Opened/Pending invoices founded');
16:23:20 260  WHEN BAD_ACOUNT_ID THEN
16:23:20 261  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 262  	  SPROC_NAME, 'No such account');
16:23:20 263  WHEN CAN_NOT_ANNOTATE_ACCOUNT THEN
16:23:20 264  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 265  	  SPROC_NAME, 'Could not annotate account', EXCEPTION_MESSAGE);
16:23:20 266  WHEN OTHERS THEN
16:23:20 267  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 268  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 269  END DISABLE_ACCOUNT;
16:23:20 270  
16:23:20 271  /******************************************************************************/
16:23:20 272  
16:23:20 273  PROCEDURE CREATE_ACTIVE_ACCOUNT(
16:23:20 274  /*
16:23:20 275  Throws exceptions:
16:23:20 276  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 277  APP_EXCEPTION_CODES_V19.DUPLICATE_ERROR
16:23:20 278  */
16:23:20 279  	  in_created_by IN VARCHAR2,
16:23:20 280  	  in_group_id	IN NUMBER
16:23:20 281  ) AS
16:23:20 282  -- VARIABLES
16:23:20 283  SPROC_NAME      CONSTANT VARCHAR2(21) := 'CREATE_ACTIVE_ACCOUNT';
16:23:20 284  new_account_id  NUMBER;
16:23:20 285  temp_group_id   NUMBER;
16:23:20 286  -- EXCEPTIONS
16:23:20 287  GROUP_EXISTS_EXCEPTION EXCEPTION;
16:23:20 288  EXCEPTION_MESSAGE      VARCHAR2(1024);
16:23:20 289  BEGIN
16:23:20 290  	-- Check if account already exists
16:23:20 291  	BEGIN
16:23:20 292  	  SELECT
16:23:20 293  	    ACCOUNT.GROUP_ID into temp_group_id
16:23:20 294  	  FROM
16:23:20 295  	    ACCOUNT
16:23:20 296  	  WHERE
16:23:20 297  	    ACCOUNT.GROUP_ID = in_group_id
16:23:20 298  	    AND ROWNUM <= 1;
16:23:20 299  
16:23:20 300  	  IF SQL%ROWCOUNT = 1 THEN
16:23:20 301  	    RAISE GROUP_EXISTS_EXCEPTION;
16:23:20 302  	  END IF;
16:23:20 303  
16:23:20 304  	  EXCEPTION
16:23:20 305  	    WHEN NO_DATA_FOUND THEN
16:23:20 306  	      NULL;
16:23:20 307  	END;
16:23:20 308  
16:23:20 309  	-- Insert new row into ACCOUNT table
16:23:20 310  	PROCS_ACCOUNT_CRU_V19.CREATE_ACCOUNT(
16:23:20 311  	  out_account_id	=> new_account_id,
16:23:20 312  	  in_account_status_id	=> GLOBAL_STATUSES_V19.ACCOUNT_ACTIVE,
16:23:20 313  	  in_group_id		=> in_group_id,
16:23:20 314  	  in_created_by 	=> in_created_by,
16:23:20 315  	  in_system_category_id => GLOBAL_ENUMS_V19.SYSTEM_CATEGORY_LIVE
16:23:20 316  	);
16:23:20 317  
16:23:20 318  EXCEPTION
16:23:20 319  WHEN GROUP_EXISTS_EXCEPTION THEN
16:23:20 320  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.DUPLICATE_ERROR,
16:23:20 321  	  SPROC_NAME, 'Group already exists');
16:23:20 322  WHEN OTHERS THEN
16:23:20 323  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 324  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 325  END CREATE_ACTIVE_ACCOUNT;
16:23:20 326  
16:23:20 327  /******************************************************************************/
16:23:20 328  
16:23:20 329  PROCEDURE REACTIVATE_ACCOUNT (
16:23:20 330  /*
16:23:20 331  Throws exceptions:
16:23:20 332  APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 333  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 334  */
16:23:20 335  	in_group_id   IN NUMBER,
16:23:20 336  	in_updated_by IN VARCHAR2,
16:23:20 337  	in_note       IN VARCHAR2,
16:23:20 338  	in_agent_id   IN NUMBER
16:23:20 339  ) AS
16:23:20 340  -- VARIABLES
16:23:20 341  SPROC_NAME	      CONSTANT VARCHAR2(18) := 'REACTIVATE_ACCOUNT';
16:23:20 342  var_account_id	      NUMBER;
16:23:20 343  current_account_status  NUMBER;
16:23:20 344  
16:23:20 345  -- EXCEPTIONS
16:23:20 346  BAD_CURRENT_ACC_STATUS EXCEPTION;
16:23:20 347  CAN_NOT_CREATE_NOTE    EXCEPTION;
16:23:20 348  EXCEPTION_MESSAGE      VARCHAR2(1024);
16:23:20 349  BEGIN
16:23:20 350  	-- Get account id, status
16:23:20 351  	SELECT
16:23:20 352  	  ACCOUNT.ACCOUNT_STATUS_ID,
16:23:20 353  	  ACCOUNT.ID
16:23:20 354  	INTO
16:23:20 355  	  current_account_status,
16:23:20 356  	  var_account_id
16:23:20 357  	FROM ACCOUNT
16:23:20 358  	WHERE
16:23:20 359  	  ACCOUNT.GROUP_ID = in_group_id;
16:23:20 360  
16:23:20 361  	IF current_account_status != GLOBAL_STATUSES_V19.ACCOUNT_FROZEN THEN
16:23:20 362  	  RAISE BAD_CURRENT_ACC_STATUS;
16:23:20 363  	END IF;
16:23:20 364  
16:23:20 365  	-- Change account status
16:23:20 366  	PROCS_ACCOUNT_V19.UPDATE_ACCOUNT_STATUS(
16:23:20 367  	  in_account_id        => var_account_id,
16:23:20 368  	  in_updated_by        => in_updated_by,
16:23:20 369  	  in_account_status_id => GLOBAL_STATUSES_V19.ACCOUNT_ACTIVE
16:23:20 370  	);
16:23:20 371  
16:23:20 372  	-- Add note
16:23:20 373  	BEGIN
16:23:20 374  	  PROCS_ACCOUNT_V19.ANNOTATE_ACCOUNT(
16:23:20 375  	    in_group_id   => in_group_id,
16:23:20 376  	    in_agent_id   => in_agent_id,
16:23:20 377  	    in_note	  => in_note,
16:23:20 378  	    in_created_by => in_updated_by
16:23:20 379  	  );
16:23:20 380  	  EXCEPTION
16:23:20 381  	    WHEN OTHERS THEN
16:23:20 382  	      EXCEPTION_MESSAGE := SQLERRM;
16:23:20 383  	      RAISE CAN_NOT_CREATE_NOTE;
16:23:20 384  	END;
16:23:20 385  
16:23:20 386  EXCEPTION
16:23:20 387  WHEN BAD_CURRENT_ACC_STATUS THEN
16:23:20 388  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
16:23:20 389  	  SPROC_NAME, 'Current account status is not "frozen"');
16:23:20 390  WHEN NO_DATA_FOUND THEN
16:23:20 391  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 392  	  SPROC_NAME, 'Could not find account with given group ID');
16:23:20 393  WHEN CAN_NOT_CREATE_NOTE THEN
16:23:20 394  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 395  	  SPROC_NAME, 'Could not annotate account');
16:23:20 396  WHEN OTHERS THEN
16:23:20 397  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 398  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 399  END REACTIVATE_ACCOUNT;
