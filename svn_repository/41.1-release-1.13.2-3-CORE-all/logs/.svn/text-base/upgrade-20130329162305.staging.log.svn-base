16:23:20 296   CURSOR c (v_from NUMBER, v_to NUMBER) IS SELECT ACCOUNT.GROUP_ID FROM ACCOUNT WHERE GROUP_ID BETWEEN v_from AND v_to;
16:23:20 297  BEGIN
16:23:20 298  -- arosolovskiy refactoring: call delete_user_account only "COUNT(group_id) WHERE ...." times instead of "in_end_group_id - in_start_group_id" times;
16:23:20 299  	/*
16:23:20 300  	FOR var_group_id IN in_start_group_id..in_end_group_id
16:23:20 301  	LOOP
16:23:20 302  	  TEST_DELETE_USER_ACCOUNT(var_group_id);
16:23:20 303  	END LOOP;*/
16:23:20 304  	OPEN c(in_start_group_id, in_end_group_id);
16:23:20 305  	WHILE c%ISOPEN LOOP
16:23:20 306  	  FETCH c INTO gid;
16:23:20 307  	  IF c%NOTFOUND THEN
16:23:20 308  	   CLOSE c;
16:23:20 309  	  END IF;
