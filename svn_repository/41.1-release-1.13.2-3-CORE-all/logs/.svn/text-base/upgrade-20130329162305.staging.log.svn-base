16:23:20  51  		      from charge c
16:23:20  52  		      where c.invoice_id = li.invoice_id
16:23:20  53  		      group by case when c.charge_amount < 0 then -1 else 1 end
16:23:20  54  		  )
16:23:20  55  		  loop
