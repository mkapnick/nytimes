16:23:20 595  
16:23:20 596  FUNCTION IS_INVOICE_PAYING_STARTED (
16:23:20 597  /*
16:23:20 598  Throws exceptions:
16:23:20 599  APP_EXCEPTION_CODES_V19.NOT_FOUND
16:23:20 600  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
16:23:20 601  */
16:23:20 602  	in_invoice_id IN NUMBER
16:23:20 603  ) RETURN NUMBER AS
16:23:20 604  -- VARIABLES
16:23:20 605  SPROC_NAME		 CONSTANT VARCHAR2(30) := 'IS_INVOICE_PAYING_STARTED';
16:23:20 606  temp_invoice_id		 NUMBER;
16:23:20 607  var_processed_charges_num  NUMBER;
16:23:20 608  var_processed_transac_num  NUMBER;
16:23:20 609  var_success_attempts_num	 NUMBER;
16:23:20 610  var_is_gc 		 NUMBER;
16:23:20 611  -- EXCEPTIONS
16:23:20 612  BAD_INVOICE_ID EXCEPTION;
16:23:20 613  BEGIN
16:23:20 614  
16:23:20 615  	-- Check that invoice exists
16:23:20 616  	BEGIN
16:23:20 617  	  SELECT
16:23:20 618  	    INVOICE.ID into temp_invoice_id
16:23:20 619  	  FROM
16:23:20 620  	    INVOICE
16:23:20 621  	  WHERE
16:23:20 622  	    INVOICE.ID = in_invoice_id;
16:23:20 623  	  EXCEPTION
16:23:20 624  	   WHEN NO_DATA_FOUND THEN
16:23:20 625  	     RAISE BAD_INVOICE_ID;
16:23:20 626  	END;
16:23:20 627  
16:23:20 628  	-- Check that there are tansaction attempts with status success
16:23:20 629  	SELECT
16:23:20 630  	  COUNT(1) into var_success_attempts_num
16:23:20 631  	FROM
16:23:20 632  	  TRANSACTION_ATTEMPT ta,
16:23:20 633  	  TRANSACTION t,
16:23:20 634  	  CHARGE c
16:23:20 635  	WHERE
16:23:20 636  	  ta.TRANSACTION_ATTEMPT_STATUS_ID = GLOBAL_STATUSES_V19.TRANS_ATTEMPT_SUCCESS and
16:23:20 637  	  ta.transaction_id = t.id and
16:23:20 638  	  c.transaction_id = t.id and
16:23:20 639  	  t.is_refund = GLOBAL_CONSTANTS_V19.FALSE and
16:23:20 640  	  c.invoice_id = in_invoice_id
16:23:20 641  	;
16:23:20 642  
16:23:20 643  	IF var_success_attempts_num > 0 THEN
16:23:20 644  	  RETURN GLOBAL_CONSTANTS_V19.TRUE;
16:23:20 645  	END IF;
16:23:20 646  
16:23:20 647  	SELECT
16:23:20 648  	  COUNT(1) into var_success_attempts_num
16:23:20 649  	FROM
16:23:20 650  	  TRANSACTION t,
16:23:20 651  	  CHARGE c
16:23:20 652  	WHERE
16:23:20 653  	  c.transaction_id = t.id and
16:23:20 654  	  t.is_refund = GLOBAL_CONSTANTS_V19.FALSE and
16:23:20 655  	  t.is_settled = GLOBAL_CONSTANTS_V19.TRUE and
16:23:20 656  	  c.invoice_id = in_invoice_id
16:23:20 657  	;
16:23:20 658  
16:23:20 659  	IF var_success_attempts_num > 0 THEN
16:23:20 660  	  RETURN GLOBAL_CONSTANTS_V19.TRUE;
16:23:20 661  	END IF;
16:23:20 662  
16:23:20 663  	SELECT
16:23:20 664  	  COUNT(1) into var_is_gc
16:23:20 665  	FROM
16:23:20 666  	  gift_certificate gc
16:23:20 667  	WHERE
16:23:20 668  	  gc.finalized_invoice_id = in_invoice_id
16:23:20 669  	;
16:23:20 670  
16:23:20 671  	IF var_is_gc > 0 THEN
16:23:20 672  	  RETURN GLOBAL_CONSTANTS_V19.TRUE;
16:23:20 673  	END IF;
16:23:20 674  
16:23:20 675  	RETURN GLOBAL_CONSTANTS_V19.FALSE;
16:23:20 676  
16:23:20 677  EXCEPTION
16:23:20 678  WHEN BAD_INVOICE_ID THEN
16:23:20 679  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 680  	  SPROC_NAME, 'No such invoice');
16:23:20 681  WHEN OTHERS THEN
16:23:20 682  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 683  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 684  END IS_INVOICE_PAYING_STARTED;
