16:23:20 2440  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 2441  	   SPROC_NAME, 'Could not update subscription data', EXCEPTION_MESSAGE);
16:23:20 2442  WHEN ACTIVE_INVOICES_FOUND THEN
16:23:20 2443  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
16:23:20 2444  	   SPROC_NAME, 'Invoices with active licenses found');
16:23:20 2445  WHEN CAN_NOT_CREATE_NOTE THEN
16:23:20 2446  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 2447  	   SPROC_NAME, 'Could not annotate subscription', EXCEPTION_MESSAGE);
16:23:20 2448  --WHEN OTHERS THEN
16:23:20 2449  --  PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 2450  --    SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 2451  END FINALIZE_CANCELATION;
