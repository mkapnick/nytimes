16:23:19 964  
16:23:19 965  PROCEDURE GET_OFFER_LENGTH_IN_DAYS (
16:23:19 966  	in_offer_id   IN NUMBER,
16:23:19 967  	in_start_date IN DATE DEFAULT SYSDATE,
16:23:19 968  	out_days      OUT NUMBER
16:23:19 969  ) AS
