16:23:20 2825  	     ch.transaction_id,
16:23:20 2826  	     ch.charge_amount
16:23:20 2827  	   FROM
16:23:20 2828  	     charge ch
16:23:20 2829  	   WHERE
16:23:20 2830  	     ch.instrument_type_id = in_old_fin_instrument_type
16:23:20 2831  	     AND ch.instrument_id = in_old_fin_instrument_id
16:23:20 2832  	     AND ch.charge_status_id = GLOBAL_STATUSES_V19.CHARGE_OPENED
16:23:20 2833  	 ) LOOP
16:23:20 2834  
16:23:20 2835  	   FOR f_pending_transaction IN (
16:23:20 2836  	     select
