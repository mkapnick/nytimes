16:23:20 824  PROCEDURE SHOULD_MOVE_TO_GRACE(
16:23:20 825  	in_invoice_id  IN INVOICE.ID%TYPE,
16:23:20 826  	out_result     OUT NUMBER
16:23:20 827  ) AS
16:23:20 828  BEGIN
16:23:20 829  	-- if the invoice preceding the given invoice has no transaction attempts, then
16:23:20 830  	-- it is not billed out of Sartre. if so, then the associated subscription
16:23:20 831  	-- should be canceled after a final failed billing attempt--not moved to grace.
16:23:20 832  	SELECT DECODE(COUNT(1), 0, 0, 1) INTO out_result
16:23:20 833  	FROM CHARGE c
16:23:20 834  	INNER JOIN TRANSACTION t ON c.TRANSACTION_ID = t.ID
16:23:20 835  	INNER JOIN TRANSACTION_ATTEMPT ta ON ta.TRANSACTION_ID = t.ID
16:23:20 836  	WHERE c.INVOICE_ID = (
16:23:20 837  	  -- select previous invoice_id, or -1 if there is none
16:23:20 838  	  SELECT PREV_INVOICE_ID FROM (
16:23:20 839  	    SELECT i.ID, LAG(i.ID, 1, -1) OVER (ORDER BY i.CREATE_DATE) AS PREV_INVOICE_ID
16:23:20 840  	    FROM INVOICE i
16:23:20 841  	    INNER JOIN LICENSE l ON i.ID = l.INVOICE_ID
16:23:20 842  	    WHERE l.SUBSCRIPTION_ID = (
16:23:20 843  	      SELECT SUBSCRIPTION_ID FROM LICENSE WHERE INVOICE_ID = in_invoice_id
16:23:20 844  	    )
16:23:20 845  	  ) WHERE ID = in_invoice_id
16:23:20 846  	);
16:23:20 847  END SHOULD_MOVE_TO_GRACE;
