16:23:20 210  	-- END FOR
16:23:20 211  	-- allow purchase;
16:23:20 212  
16:23:20 213  	-- if user have any active existing subscriptions to the offer chain
16:23:20 214  	-- and if MAX_CONCURRENT_SUBS <= [user's subscription count for the offer chain]
16:23:20 215  	-- then deny purchase
16:23:20 216  	var_max_concurrent_subscrs := PROCS_OFFER_CHAIN_V19.GET_OFFER_CHAIN_MAX_CONC_SUBSC(in_offer_chain_id);
16:23:20 217  	IF var_max_concurrent_subscrs != GLOBAL_CONSTANTS_V19.INFINITY
16:23:20 218  	  AND var_max_concurrent_subscrs <= var_same_offer_chains_num THEN
16:23:20 219  	  -- Find first concurrent subscription id:
16:23:20 220  	  SELECT
16:23:20 221  	    ID into var_concur_subscription_id
16:23:20 222  	  FROM (
16:23:20 223  	    SELECT
16:23:20 224  	      ID
16:23:20 225  	    FROM
16:23:20 226  	      SUBSCRIPTION
16:23:20 227  	    WHERE
16:23:20 228  	      SUBSCRIPTION.ACCOUNT_ID = var_account_id
16:23:20 229  	      AND SUBSCRIPTION.OFFER_CHAIN_ID = in_offer_chain_id
16:23:20 230  	      AND (SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V19.SUBSCRIPTION_ACTIVE
16:23:20 231  		   OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V19.SUBSCRIPTION_IN_GRACE_PERIOD)
16:23:20 232  	    ORDER BY
16:23:20 233  	      ID
16:23:20 234  	  )
16:23:20 235  	  WHERE
16:23:20 236  	    ROWNUM <= 1;
16:23:20 237  	  RAISE LIMIT_REACHED;
16:23:20 238  	END IF;
16:23:20 239  
16:23:20 240  	-- if user does not have any active existing subscriptions to the offer chain
16:23:20 241  	-- and if product from the offer chain is already owned from another offer chain
16:23:20 242  	-- then deny purchase
16:23:20 243  	IF (in_check_dupe_products != GLOBAL_CONSTANTS_V19.FALSE) THEN
16:23:20 244  	  FOR f_account_offer_chains IN (
16:23:20 245  	    SELECT DISTINCT
16:23:20 246  	      OFFER_CHAIN_ID
16:23:20 247  	    FROM
16:23:20 248  	      SUBSCRIPTION
16:23:20 249  	    WHERE
16:23:20 250  	      ACCOUNT_ID = var_account_id
16:23:20 251  	      AND (
16:23:20 252  		SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V19.SUBSCRIPTION_ACTIVE
16:23:20 253  		OR SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V19.SUBSCRIPTION_SUSPENDED
16:23:20 254  		OR SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V19.SUBSCRIPTION_IN_GRACE_PERIOD)
16:23:20 255  	  )
16:23:20 256  	  LOOP
16:23:20 257  	    IF PROCS_OFFER_CHAIN_V19.CHECK_FOR_SAME_PRODUCTS(
16:23:20 258  	      in_offer_chain_1	       => in_offer_chain_id,
16:23:20 259  	      in_offer_chain_2	       => f_account_offer_chains.OFFER_CHAIN_ID,
16:23:20 260  	      in_use_eligibility_rules => GLOBAL_CONSTANTS_V19.TRUE
16:23:20 261  	    ) = GLOBAL_CONSTANTS_V19.TRUE THEN
16:23:20 262  
16:23:20 263  	      -- Find first concurrent subscription id:
16:23:20 264  	      SELECT
16:23:20 265  		ID into var_concur_subscription_id
16:23:20 266  	      FROM (
16:23:20 267  		SELECT
16:23:20 268  		  ID
16:23:20 269  		FROM
16:23:20 270  		  SUBSCRIPTION
16:23:20 271  		WHERE
16:23:20 272  		  SUBSCRIPTION.ACCOUNT_ID = var_account_id
16:23:20 273  		  AND SUBSCRIPTION.OFFER_CHAIN_ID = f_account_offer_chains.OFFER_CHAIN_ID
16:23:20 274  		  AND (
16:23:20 275  		    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V19.SUBSCRIPTION_ACTIVE
16:23:20 276  		    OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V19.SUBSCRIPTION_SUSPENDED
16:23:20 277  		    OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V19.SUBSCRIPTION_IN_GRACE_PERIOD)
16:23:20 278  		ORDER BY
16:23:20 279  		  ID
16:23:20 280  	      )
16:23:20 281  	      WHERE
16:23:20 282  		ROWNUM <= 1;
16:23:20 283  
16:23:20 284  	      RAISE PRODUCT_ALREADY_PURCHASED;
16:23:20 285  	    END IF;
16:23:20 286  	  END LOOP;
16:23:20 287  	END IF;
16:23:20 288  
16:23:20 289  	BEGIN
16:23:20 290  	  PROCS_OFFER_CHAIN_V19.GET_FIRST_OFFER(in_offer_chain_id, var_first_offer_id);
16:23:20 291  	  EXCEPTION
16:23:20 292  	    WHEN OTHERS THEN
16:23:20 293  	      EXCEPTION_MESSAGE := SQLERRM;
