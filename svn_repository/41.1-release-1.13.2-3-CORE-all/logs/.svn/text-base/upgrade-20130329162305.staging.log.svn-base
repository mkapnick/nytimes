16:23:20 703  
16:23:20 704  PROCEDURE START_GC_PURCHASING (
16:23:20 705  	in_group_id		  IN NUMBER,
16:23:20 706  	in_offer_chain_id	  IN VARCHAR2,
16:23:20 707  	in_gift_certificate_code  IN  VARCHAR2,
16:23:20 708  	in_created_by		  IN  VARCHAR2,
16:23:20 709  	in_recipient_name	  IN  VARCHAR2,
16:23:20 710  	in_recipient_email	  IN  VARCHAR2,
16:23:20 711  	in_recipient_address_id   IN NUMBER,
16:23:20 712  	in_recipient_notify_date  IN DATE,
16:23:20 713  	in_sender_name		  IN VARCHAR2,
16:23:20 714  	in_sender_email 	  IN VARCHAR2,
16:23:20 715  	in_gift_message 	  IN  VARCHAR2,
16:23:20 716  	in_expiration_date	  IN DATE,
16:23:20 717  	out_gift_certificate_id   OUT NUMBER,
16:23:20 718  	out_invoice_id		  OUT NUMBER
16:23:20 719  ) AS
16:23:20 720  SPROC_NAME CONSTANT VARCHAR2(19) := 'START_GC_PURCHASING';
16:23:20 721  -- VARIABLES
16:23:20 722  var_account_id		NUMBER;
16:23:20 723  temp_gc_code		GIFT_CERTIFICATE.CODE%TYPE;
16:23:20 724  var_och_is_gc		NUMBER;
16:23:20 725  var_offer_chain_status_id NUMBER;
16:23:20 726  var_is_for_redemption	NUMBER;
16:23:20 727  var_new_invoice_id	NUMBER;
16:23:20 728  var_gift_cert_id		NUMBER;
16:23:20 729  var_account_tax_exempt_id VARCHAR2(255);
16:23:20 730  -- EXCEPTIONS
16:23:20 731  BAD_GROUP_ID		    EXCEPTION;
16:23:20 732  GC_CODE_ALREADY_EXISTS	    EXCEPTION;
16:23:20 733  BAD_OFFER_CHAIN_ID	    EXCEPTION;
16:23:20 734  OCH_IS_NOT_GIFT_CERTIFICATE   EXCEPTION;
16:23:20 735  BAD_OFFER_CHAIN_STATUS	    EXCEPTION;
16:23:20 736  CAN_NOT_PURCHASE_GC_FOR_RDMPN EXCEPTION;
16:23:20 737  CAN_NOT_CREATE_INVOICE	    EXCEPTION;
16:23:20 738  OFFER_REC_NUM_LESS_THAN_ONE   EXCEPTION;
16:23:20 739  CAN_NOT_CREATE_LINE_ITEMS     EXCEPTION;
16:23:20 740  
16:23:20 741  EXCEPTION_MESSAGE VARCHAR2(1024);
16:23:20 742  BEGIN
16:23:20 743  	-- Get account id
16:23:20 744  	BEGIN
16:23:20 745  	  SELECT
16:23:20 746  	    ACCOUNT.ID,
16:23:20 747  	    ACCOUNT.TAX_EXEMPT_ID
16:23:20 748  	    into
16:23:20 749  	    var_account_id,
16:23:20 750  	    var_account_tax_exempt_id
16:23:20 751  	  FROM
16:23:20 752  	    ACCOUNT
16:23:20 753  	  WHERE
16:23:20 754  	    ACCOUNT.GROUP_ID = in_group_id;
16:23:20 755  	  EXCEPTION
16:23:20 756  	  WHEN NO_DATA_FOUND THEN
16:23:20 757  	    RAISE BAD_GROUP_ID;
16:23:20 758  	END;
16:23:20 759  
16:23:20 760  	-- Check for the same code
16:23:20 761  	BEGIN
16:23:20 762  	  SELECT
16:23:20 763  	    GIFT_CERTIFICATE.CODE into temp_gc_code
16:23:20 764  	  FROM
16:23:20 765  	    GIFT_CERTIFICATE
16:23:20 766  	  WHERE
16:23:20 767  	    GIFT_CERTIFICATE.CODE = in_gift_certificate_code;
16:23:20 768  
16:23:20 769  	  RAISE GC_CODE_ALREADY_EXISTS;
16:23:20 770  
16:23:20 771  	  EXCEPTION
16:23:20 772  	    WHEN NO_DATA_FOUND THEN
16:23:20 773  	      NULL;
16:23:20 774  	END;
16:23:20 775  
16:23:20 776  	-- Get offer chain flag "is_gift_certificate"
16:23:20 777  	BEGIN
16:23:20 778  	  SELECT
16:23:20 779  	    OFFER_CHAIN.IS_GIFT_CERTIFICATE,
16:23:20 780  	    OFFER_CHAIN.OFFER_CHAIN_STATUS_ID
16:23:20 781  	    into
16:23:20 782  	    var_och_is_gc,
16:23:20 783  	    var_offer_chain_status_id
16:23:20 784  	  FROM
16:23:20 785  	    OFFER_CHAIN
16:23:20 786  	  WHERE
16:23:20 787  	    OFFER_CHAIN.ID = in_offer_chain_id;
16:23:20 788  	  EXCEPTION
16:23:20 789  	  WHEN NO_DATA_FOUND THEN
16:23:20 790  	    RAISE BAD_OFFER_CHAIN_ID;
16:23:20 791  	END;
16:23:20 792  
16:23:20 793  	IF var_och_is_gc != GLOBAL_CONSTANTS_V19.TRUE
16:23:20 794  	  OR var_och_is_gc IS NULL THEN
16:23:20 795  	  RAISE OCH_IS_NOT_GIFT_CERTIFICATE;
16:23:20 796  	END IF;
16:23:20 797  
16:23:20 798  	IF var_offer_chain_status_id != GLOBAL_STATUSES_V19.OFFER_CHAIN_ACTIVE THEN
16:23:20 799  	  RAISE BAD_OFFER_CHAIN_STATUS;
16:23:20 800  	END IF;
16:23:20 801  
16:23:20 802  	-- norlov: #38151 check if the OC is for Redemption:
16:23:20 803  	SELECT
16:23:20 804  	  COUNT(*) into var_is_for_redemption
16:23:20 805  	FROM
16:23:20 806  	  OFFER_CHAIN_ELIGIBILITY
16:23:20 807  	WHERE
16:23:20 808  	  OFFER_CHAIN_ELIGIBILITY.OFFER_CHAIN_ID = in_offer_chain_id
16:23:20 809  	  AND OFFER_CHAIN_ELIGIBILITY.NAME = GLOBAL_CONSTANTS_V19.GIFT_CERTIFICATE_REQUIRED
16:23:20 810  	  AND OFFER_CHAIN_ELIGIBILITY.VALUE = GLOBAL_CONSTANTS_V19.ELIGIBILITY_FLAG_SET;
16:23:20 811  
16:23:20 812  	IF var_is_for_redemption > 0 THEN
16:23:20 813  	  RAISE CAN_NOT_PURCHASE_GC_FOR_RDMPN;
16:23:20 814  	END IF;
16:23:20 815  
16:23:20 816  	-- Create new invoice
16:23:20 817  	BEGIN
16:23:20 818  	  PROCS_INVOICE_V19.CREATE_INVOICE(
16:23:20 819  	    in_invoice_status => GLOBAL_STATUSES_V19.INVOICE_OPEN,
16:23:20 820  	    in_created_by     => in_created_by,
16:23:20 821  	    in_tax_exempt_id  => var_account_tax_exempt_id,
16:23:20 822  	    out_invoice_id    => var_new_invoice_id
16:23:20 823  	  );
16:23:20 824  	  EXCEPTION
16:23:20 825  	    WHEN OTHERS THEN
16:23:20 826  	      EXCEPTION_MESSAGE := SQLERRM;
16:23:20 827  	      RAISE CAN_NOT_CREATE_INVOICE;
16:23:20 828  	END;
16:23:20 829  
16:23:20 830  	-- Add line items for new invoice
16:23:20 831  	BEGIN
16:23:20 832  	  FOR f_offer_data IN (
16:23:20 833  	    SELECT
16:23:20 834  	      OFFER_ID,
16:23:20 835  	      NUM_RECURRENCES
16:23:20 836  	    FROM
16:23:20 837  	      OFFER_OFFER_CHAIN
16:23:20 838  	    WHERE
16:23:20 839  	      OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
16:23:20 840  	  )
16:23:20 841  	  LOOP
16:23:20 842  	    IF f_offer_data.NUM_RECURRENCES < 1 THEN
16:23:20 843  	      RAISE OFFER_REC_NUM_LESS_THAN_ONE;
16:23:20 844  	    END IF;
16:23:20 845  	    FOR i_offer_recurrences_iterator IN 1..f_offer_data.NUM_RECURRENCES
16:23:20 846  	    LOOP
16:23:20 847  	      PROCS_LINE_ITEMS_V19.ADD_LINE_ITEMS(
16:23:20 848  		in_invoice_id => var_new_invoice_id,
16:23:20 849  		in_offer_id   => f_offer_data.OFFER_ID,
16:23:20 850  		in_created_by => in_created_by
16:23:20 851  	      );
16:23:20 852  	    END LOOP;
16:23:20 853  	  END LOOP;
16:23:20 854  
16:23:20 855  	  EXCEPTION
16:23:20 856  	    WHEN OTHERS THEN
16:23:20 857  	      EXCEPTION_MESSAGE := SQLERRM;
16:23:20 858  	      RAISE CAN_NOT_CREATE_LINE_ITEMS;
16:23:20 859  	END;
16:23:20 860  
16:23:20 861  	PROCS_FIN_INSTRUMENTS_CRU_V19.CREATE_GIFT_CERTIFICATE(
16:23:20 862  	  out_gift_certificate_id	=> var_gift_cert_id,
16:23:20 863  	  in_purchaser_group_id 	=> in_group_id,
16:23:20 864  	  in_purchaser_invoice_id	=> var_new_invoice_id,
16:23:20 865  	  in_offer_chain_id		=> in_offer_chain_id,
16:23:20 866  	  in_expiration_date		=> in_expiration_date,
16:23:20 867  	  in_purchase_date		=> SYSDATE,
16:23:20 868  	  in_gift_certificate_status_id => GLOBAL_STATUSES_V19.GIFT_CERTIFICATE_ACTIVE,
16:23:20 869  	  in_code			=> in_gift_certificate_code,
16:23:20 870  	  in_created_by 		=> in_created_by,
16:23:20 871  	  in_recipient_name		=> in_recipient_name,
16:23:20 872  	  in_gift_message		=> in_gift_message,
16:23:20 873  	  in_recipient_email		=> in_recipient_email,
16:23:20 874  	  in_sender_email		=> in_sender_email,
16:23:20 875  	  in_sender_name		=> in_sender_name,
16:23:20 876  	  in_recipient_address_id	=> in_recipient_address_id,
16:23:20 877  	  in_recipient_notify_date	=> in_recipient_notify_date
16:23:20 878  	);
16:23:20 879  
16:23:20 880  	out_gift_certificate_id := var_gift_cert_id;
16:23:20 881  	out_invoice_id := var_new_invoice_id;
16:23:20 882  
16:23:20 883  EXCEPTION
16:23:20 884  WHEN BAD_OFFER_CHAIN_STATUS THEN
16:23:20 885  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
16:23:20 886  	  SPROC_NAME, 'Offer chain is not active');
16:23:20 887  WHEN GC_CODE_ALREADY_EXISTS THEN
16:23:20 888  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.DUPLICATE_ERROR,
16:23:20 889  	  SPROC_NAME, 'Gift certificate with same code already exists');
16:23:20 890  WHEN OCH_IS_NOT_GIFT_CERTIFICATE THEN
16:23:20 891  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
16:23:20 892  	  SPROC_NAME, 'This offer chain can not be used for gift certificate');
16:23:20 893  WHEN CAN_NOT_PURCHASE_GC_FOR_RDMPN THEN
16:23:20 894  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
16:23:20 895  	  SPROC_NAME, 'This offer chain can not be purchased for gift certificate since it is for redemption');
16:23:20 896  WHEN CAN_NOT_CREATE_INVOICE THEN
16:23:20 897  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 898  	  SPROC_NAME, 'Could not create new invoice', EXCEPTION_MESSAGE);
16:23:20 899  WHEN CAN_NOT_CREATE_LINE_ITEMS THEN
16:23:20 900  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 901  	  SPROC_NAME, 'Could not create line items', EXCEPTION_MESSAGE);
16:23:20 902  WHEN BAD_GROUP_ID THEN
16:23:20 903  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 904  	  SPROC_NAME, 'No such group id');
16:23:20 905  WHEN BAD_OFFER_CHAIN_ID THEN
16:23:20 906  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 907  	  SPROC_NAME, 'No such offer chain');
16:23:20 908  WHEN OFFER_REC_NUM_LESS_THAN_ONE THEN
16:23:20 909  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 910  	  SPROC_NAME, 'Some offer has recurrences number less than 1');
16:23:20 911  WHEN OTHERS THEN
16:23:20 912  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 913  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 914  END START_GC_PURCHASING;
