16:23:20 2254  	       in_updated_by	     => in_updated_by,
16:23:20 2255  	       in_entitlement_end_date => var_now
16:23:20 2256  	     );
16:23:20 2257  	   END LOOP;
16:23:20 2258  	 ELSE
