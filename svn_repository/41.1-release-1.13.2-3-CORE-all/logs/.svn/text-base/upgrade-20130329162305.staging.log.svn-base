16:23:19  48  	BEGIN
16:23:19  49  	  OPEN out_result_set FOR
16:23:19  50  
16:23:19  51  	SELECT
16:23:19  52  	  C.CODE NAME,
16:23:19  53  	  C.DESCRIPTION,
16:23:19  54  	  0 INHERITED,
16:23:19  55  	  C.SHAREABLE,
16:23:19  56  	  MAX(LIC.ENTITLEMENT_END_DATE) EXPIRES
16:23:19  57  	FROM
16:23:19  58  	  SUBSCRIPTION SB
16:23:19  59  	  INNER JOIN ACCOUNT AC ON AC.ID = SB.ACCOUNT_ID
16:23:19  60  	  INNER JOIN LICENSE LIC ON LIC.SUBSCRIPTION_ID = SB.ID
16:23:19  61  	  INNER JOIN OFFER_PRODUCT_OFFERING OPO ON OPO.OFFER_ID = LIC.OFFER_ID
16:23:19  62  	  INNER JOIN PRODUCT_OFFERING PO ON PO.ID = OPO.PRODUCT_OFFERING_ID
16:23:19  63  	  INNER JOIN CAPABILITY C ON PO.CAPABILITY_ID = C.ID
16:23:19  64  	WHERE
16:23:19  65  	  LIC.ENTITLEMENT_END_DATE >= TRUNC(SYSDATE)
16:23:19  66  	  AND LIC.START_DATE <= SYSDATE
16:23:19  67  	  AND AC.GROUP_ID = in_group_id
16:23:19  68  	GROUP BY
16:23:19  69  	  C.CODE, 0, C.SHAREABLE, C.DESCRIPTION
16:23:19  70  UNION ALL
16:23:19  71  	SELECT
16:23:19  72  	  C.CODE NAME,
16:23:19  73  	  C.DESCRIPTION,
16:23:19  74  	  1 INHERITED,
16:23:19  75  	  C.SHAREABLE,
16:23:19  76  	  MAX(LEAST(SS.END_DATE, LIC.ENTITLEMENT_END_DATE)) EXPIRES
16:23:19  77  	FROM
16:23:19  78  	  ACCOUNT BORROWER,
16:23:19  79  	  SUBSCRIPTION S,
16:23:19  80  	  LICENSE LIC,
16:23:19  81  	  OFFER_PRODUCT_OFFERING OPO,
16:23:19  82  	  PRODUCT_OFFERING PO,
16:23:19  83  	  CAPABILITY C,
16:23:19  84  	  GROUP_ACCOUNT GA,
16:23:19  85  	  SUBSCRIPTION_SHARE SS
16:23:19  86  	WHERE
16:23:19  87  	  BORROWER.GROUP_ID = in_group_id
16:23:19  88  	  AND LIC.SUBSCRIPTION_ID = S.ID
16:23:19  89  	  AND OPO.OFFER_ID = LIC.OFFER_ID
16:23:19  90  	  AND PO.ID = OPO.PRODUCT_OFFERING_ID
16:23:19  91  	  AND PO.CAPABILITY_ID = C.ID
16:23:19  92  	  AND GA.SUBSCRIPTION_ID = S.ID
16:23:19  93  	  AND SS.BORROWER_ACCOUNT_ID = BORROWER.ID
16:23:19  94  	  AND SS.GROUP_ACCOUNT_ID = GA.ID
16:23:19  95  	  AND SYSDATE BETWEEN SS.START_DATE AND SS.END_DATE
16:23:19  96  	  AND SYSDATE BETWEEN LIC.START_DATE AND LIC.ENTITLEMENT_END_DATE
16:23:19  97  	  AND C.SHAREABLE = 1
16:23:19  98  	GROUP BY
16:23:19  99  	  C.CODE, 0, C.SHAREABLE, C.DESCRIPTION;
16:23:19 100  
16:23:19 101  	EXCEPTION
16:23:19 102  	  WHEN OTHERS THEN
