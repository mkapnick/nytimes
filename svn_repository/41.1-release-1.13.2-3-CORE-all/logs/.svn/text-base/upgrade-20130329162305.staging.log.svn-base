16:23:20 2241  	   );
16:23:20 2242  
16:23:20 2243  	 END IF;
16:23:20 2244  	 -- update licenses
16:23:20 2245  	 IF(var_revoke = GLOBAL_CONSTANTS_V19.TRUE OR var_chargeback_amount > 0 OR (var_subscription_in_grace = GLOBAL_CONSTANTS_V19.FALSE AND var_billed = GLOBAL_CONSTANTS_V19.FALSE)) THEN
