16:23:20 517  
16:23:20 518  PROCEDURE SUSPEND_SUBSCRIPTION(
16:23:20 519  /*
16:23:20 520  Throws exceptions:
16:23:20 521  APP_EXCEPTION_CODES_V19.NOT_FOUND
16:23:20 522  APP_EXCEPTION_CODES_V19.INTERNAL_ERROR
16:23:20 523  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
16:23:20 524  */
16:23:20 525  	  in_subs_id	IN NUMBER,
16:23:20 526  	  in_updated_by IN VARCHAR2
16:23:20 527  ) AS
16:23:20 528  SPROC_NAME		  CONSTANT VARCHAR2(20) := 'SUSPEND_SUBSCRIPTION';
16:23:20 529  var_subscription_status_id  NUMBER;
16:23:20 530  var_license_id		  NUMBER;
16:23:20 531  var_offer_id		  NUMBER;
16:23:20 532  var_license_start_date	  DATE;
16:23:20 533  var_license_end_date	  DATE;
16:23:20 534  
16:23:20 535  var_entitlement_dupration   VARCHAR2(30);
16:23:20 536  var_d_entitlement_dupration NUMBER;
16:23:20 537  
16:23:20 538  var_ym_interval INTERVAL YEAR TO MONTH;
16:23:20 539  var_ds_interval INTERVAL DAY(3) TO SECOND;
16:23:20 540  
16:23:20 541  -- EXCEPTIONS
16:23:20 542  BAD_SUBSCRIPTION_ID     EXCEPTION;
16:23:20 543  BAD_SUBSCRIPTION_STATUS EXCEPTION;
16:23:20 544  NO_LICENSE_FOUND	      EXCEPTION;
16:23:20 545  NO_OFFER_FOUND	      EXCEPTION;
16:23:20 546  EXCEPTION_MESSAGE       VARCHAR2(1024);
16:23:20 547  BEGIN
16:23:20 548  	-- TODO: Finish this prcedure (in Phase II)
16:23:20 549  
16:23:20 550  	-- Get subscription by id. FAULT if no such subscription.
16:23:20 551  	-- begin TX
16:23:20 552  	--   Get for update associated license (subscription.license_id). FAULT if not found.
16:23:20 553  	--   Set status to PROCESSED.
16:23:20 554  	--   updated record.
16:23:20 555  	--   compute days remaining in the subscription: original end_date - today = days_remaining_adjustment
16:23:20 556  	--   new subscription status is SUSPENDED.
16:23:20 557  	--   suspend_date is now.
16:23:20 558  	--   update subscription record.
16:23:20 559  	-- end TX
16:23:20 560  
16:23:20 561  	BEGIN
16:23:20 562  	  SELECT
16:23:20 563  	    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID into var_subscription_status_id
16:23:20 564  	  FROM
16:23:20 565  	    SUBSCRIPTION
16:23:20 566  	  WHERE
16:23:20 567  	    SUBSCRIPTION.ID = in_subs_id;
16:23:20 568  	  EXCEPTION
16:23:20 569  	  WHEN NO_DATA_FOUND THEN
16:23:20 570  	    RAISE BAD_SUBSCRIPTION_ID;
16:23:20 571  	END;
16:23:20 572  
16:23:20 573  	IF var_subscription_status_id != GLOBAL_STATUSES_V19.SUBSCRIPTION_ACTIVE THEN
16:23:20 574  	  RAISE BAD_SUBSCRIPTION_STATUS;
16:23:20 575  	END IF;
16:23:20 576  
16:23:20 577  	BEGIN
16:23:20 578  	  SELECT
16:23:20 579  	    LICENSE.ID,
16:23:20 580  	    LICENSE.OFFER_ID,
16:23:20 581  	    LICENSE.START_DATE
16:23:20 582  	    into
16:23:20 583  	    var_license_id,
16:23:20 584  	    var_offer_id,
16:23:20 585  	    var_license_start_date
16:23:20 586  	  FROM
16:23:20 587  	    LICENSE
16:23:20 588  	  WHERE
16:23:20 589  	    LICENSE.SUBSCRIPTION_ID = in_subs_id
16:23:20 590  	      AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V19.LICENSE_ACTIVE;
16:23:20 591  	  EXCEPTION
16:23:20 592  	  WHEN NO_DATA_FOUND THEN
16:23:20 593  	    RAISE NO_LICENSE_FOUND;
16:23:20 594  	END;
16:23:20 595  
16:23:20 596  	PROCS_LICENSE_CRU_V19.UPDATE_LICENSE(
16:23:20 597  	  in_license_id        => var_license_id,
16:23:20 598  	  in_updated_by        => in_updated_by,
16:23:20 599  	  in_license_status_id => GLOBAL_STATUSES_V19.LICENSE_CLOSED
16:23:20 600  	);
16:23:20 601  
16:23:20 602  	BEGIN
16:23:20 603  	  SELECT
16:23:20 604  	    OFFER.ENTITLEMENT_DURATION into var_entitlement_dupration
16:23:20 605  	  FROM
16:23:20 606  	    OFFER
16:23:20 607  	  WHERE
16:23:20 608  	    OFFER.ID = var_offer_id;
16:23:20 609  	  EXCEPTION
16:23:20 610  	  WHEN NO_DATA_FOUND THEN
16:23:20 611  	    RAISE NO_OFFER_FOUND;
16:23:20 612  	END;
16:23:20 613  
16:23:20 614  	var_ym_interval := substr(var_entitlement_dupration, 0, 4);
16:23:20 615  	var_ds_interval := substr(var_entitlement_dupration, 4);
16:23:20 616  
16:23:20 617  	var_license_end_date := var_license_start_date + var_ym_interval + var_ds_interval;
16:23:20 618  
16:23:20 619  	var_d_entitlement_dupration := var_license_end_date - current_date;
16:23:20 620  
16:23:20 621  	PROCS_SUBSCRIPTION_CRU_V19.UPDATE_SUBSCRIPTION(
16:23:20 622  	  in_subscription_id	       => in_subs_id,
16:23:20 623  	  in_updated_by 	       => in_updated_by,
16:23:20 624  	  in_suspend_date	       => SYSDATE,
16:23:20 625  	  in_subscription_status_id    => GLOBAL_STATUSES_V19.SUBSCRIPTION_SUSPENDED,
16:23:20 626  	  in_days_remainning_ajustment => var_d_entitlement_dupration
16:23:20 627  	);
16:23:20 628  
16:23:20 629  EXCEPTION
16:23:20 630  WHEN BAD_SUBSCRIPTION_ID THEN
16:23:20 631  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 632  	  SPROC_NAME, 'No such subscription');
16:23:20 633  WHEN BAD_SUBSCRIPTION_STATUS THEN
16:23:20 634  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
16:23:20 635  	  SPROC_NAME, 'Subscription is not active');
16:23:20 636  WHEN NO_LICENSE_FOUND THEN
16:23:20 637  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 638  	  SPROC_NAME, 'Can not find license associated with given subscription ID');
16:23:20 639  WHEN NO_OFFER_FOUND THEN
16:23:20 640  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 641  	  SPROC_NAME, 'Can not find offer associated with given subscription ID');
16:23:20 642  WHEN OTHERS THEN
16:23:20 643  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 644  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 645  END SUSPEND_SUBSCRIPTION;
