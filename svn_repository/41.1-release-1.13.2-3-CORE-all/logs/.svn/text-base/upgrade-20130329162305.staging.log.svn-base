16:23:20 1753  PROCEDURE GET_GROUP_IDS_BY_CC_INFO (
16:23:20 1754  	 in_last_four_cc IN CREDIT_CARD.LAST_FOUR_CC%TYPE,
16:23:20 1755  	 in_expiration_date IN DATE,
16:23:20 1756  	 in_country IN CREDIT_CARD.COUNTRY%TYPE DEFAULT NULL,
16:23:20 1757  	 in_postal_code IN CREDIT_CARD.POSTAL_CODE%TYPE DEFAULT NULL,
16:23:20 1758  	 in_city IN CREDIT_CARD.CITY%TYPE DEFAULT NULL,
16:23:20 1759  	 in_state IN CREDIT_CARD.STATE%TYPE DEFAULT NULL,
16:23:20 1760  	 in_credit_card_type_id IN CREDIT_CARD.CREDIT_CARD_TYPE_ID%TYPE DEFAULT NULL,
16:23:20 1761  	 in_credit_card_status_id IN CREDIT_CARD.CREDIT_CARD_STATUS_ID%TYPE DEFAULT NULL,
16:23:20 1762  	 in_lower_bound IN NUMBER DEFAULT 1,
16:23:20 1763  	 in_upper_bound IN NUMBER DEFAULT 11,
16:23:20 1764  	 out_result_set OUT SYS_REFCURSOR
16:23:20 1765  ) AS
16:23:20 1766  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_GROUP_IDS_BY_CC_INFO';
16:23:20 1767  BEGIN
16:23:20 1768  
16:23:20 1769  	 OPEN out_result_set FOR
16:23:20 1770  	     SELECT
16:23:20 1771  	       distinct /*+ first_rows(in_upper_bound-in_lower_bound) */ a.GROUP_ID GROUP_ID
16:23:20 1772  	     FROM
16:23:20 1773  	       account a,
16:23:20 1774  	       credit_card cc
16:23:20 1775  	     WHERE
16:23:20 1776  	       cc.expiration_date = in_expiration_date and
16:23:20 1777  	       cc.last_four_cc = in_last_four_cc and
16:23:20 1778  	       upper(cc.postal_code) = upper(nvl(in_postal_code, cc.postal_code)) and
16:23:20 1779  	       upper(cc.city) = upper(nvl(in_city, cc.city)) and
16:23:20 1780  	       upper(cc.state) = upper(nvl(in_state, cc.state)) and
16:23:20 1781  	       upper(cc.country) = upper(nvl(in_country, cc.country)) and
16:23:20 1782  	       cc.credit_card_status_id = nvl(in_credit_card_status_id, cc.credit_card_status_id) and
16:23:20 1783  	       cc.credit_card_type_id = nvl(in_credit_card_type_id, cc.credit_card_type_id) and
16:23:20 1784  	       a.id = cc.account_id and
16:23:20 1785  	       rownum >= in_lower_bound and
16:23:20 1786  	       rownum <= in_upper_bound
16:23:20 1787  	   ;
16:23:20 1788  
16:23:20 1789  EXCEPTION
16:23:20 1790  WHEN OTHERS THEN
16:23:20 1791  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 1792  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 1793  END GET_GROUP_IDS_BY_CC_INFO;
