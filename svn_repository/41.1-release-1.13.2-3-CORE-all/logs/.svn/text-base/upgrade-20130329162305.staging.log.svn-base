16:23:20 1657  /******************************************************************************/
16:23:20 1658  
16:23:20 1659  PROCEDURE REFUND_INVOICE (
16:23:20 1660  	 in_invoice_id	    IN NUMBER,
16:23:20 1661  	 in_refund_amount   IN NUMBER,
16:23:20 1662  	 in_note	    IN VARCHAR2,
16:23:20 1663  	 in_created_by	    IN VARCHAR2,
16:23:20 1664  	 out_charge_id	    OUT NUMBER
16:23:20 1665  ) AS
16:23:20 1666  SPROC_NAME CONSTANT VARCHAR2(14) := 'REFUND_INVOICE';
16:23:20 1667  -- VARIABLES
16:23:20 1668  var_invoice_status_id  NUMBER;
16:23:20 1669  var_subscription_id    NUMBER;
16:23:20 1670  var_account_id	      NUMBER;
16:23:20 1671  var_group_id	      NUMBER;
16:23:20 1672  var_account_status_id  NUMBER;
16:23:20 1673  var_new_transaction_id NUMBER;
16:23:20 1674  var_instrument_type_id NUMBER;
16:23:20 1675  var_instrument_id      NUMBER;
16:23:20 1676  var_new_charge_id      NUMBER;
16:23:20 1677  var_invoice_amount     NUMBER(10,2);
16:23:20 1678  var_refunds_before     NUMBER(10,2);
16:23:20 1679  var_charges_amount     NUMBER(10,2);
16:23:20 1680  -- EXCEPTIONS
16:23:20 1681  CAN_NOT_FIND_SUBSCR_OR_GC     EXCEPTION;
16:23:20 1682  ACCOUNT_IS_FROZEN	     EXCEPTION;
16:23:20 1683  BAD_INVOICE_ID		     EXCEPTION;
16:23:20 1684  CAN_NOT_CREATE_TRANSACTION    EXCEPTION;
16:23:20 1685  CAN_NOT_CREATE_CHARGE	     EXCEPTION;
16:23:20 1686  CAN_NOT_CALC_INVOICE_AMOUNT   EXCEPTION;
16:23:20 1687  REFUND_IS_GREATER_THAN_ANOUNT EXCEPTION;
16:23:20 1688  CAN_NOT_ANNOTATE_SUBSCRIPTION EXCEPTION;
16:23:20 1689  TOT_REF_IS_GREATER_THAN_ANOUNT EXCEPTION;
16:23:20 1690  INVOICE_IS_NOT_CLOSED	     EXCEPTION;
16:23:20 1691  TOT_REF_IS_GRATER_THAN_CHARGES EXCEPTION;
16:23:20 1692  EXCEPTION_MESSAGE	      VARCHAR2(1024);
16:23:20 1693  BEGIN
16:23:20 1694  
16:23:20 1695  	 BEGIN
16:23:20 1696  	   SELECT
16:23:20 1697  	     INVOICE.INVOICE_STATUS_ID into var_invoice_status_id
16:23:20 1698  	   FROM
16:23:20 1699  	     INVOICE
16:23:20 1700  	   WHERE
16:23:20 1701  	     INVOICE.ID = in_invoice_id;
16:23:20 1702  	   EXCEPTION
16:23:20 1703  	     WHEN NO_DATA_FOUND THEN
16:23:20 1704  	       RAISE BAD_INVOICE_ID;
16:23:20 1705  	 END;
16:23:20 1706  
16:23:20 1707  	 -- Get instrument and subscription id if exists
16:23:20 1708  	 BEGIN
16:23:20 1709  	   SELECT
16:23:20 1710  	     SUBSCRIPTION.INSTRUMENT_ID,
16:23:20 1711  	     SUBSCRIPTION.INSTRUMENT_TYPE_ID,
16:23:20 1712  	     SUBSCRIPTION.ACCOUNT_ID,
16:23:20 1713  	     SUBSCRIPTION.ID
16:23:20 1714  	     into
16:23:20 1715  	     var_instrument_id,
16:23:20 1716  	     var_instrument_type_id,
16:23:20 1717  	     var_account_id,
16:23:20 1718  	     var_subscription_id
16:23:20 1719  	   FROM
16:23:20 1720  	     SUBSCRIPTION
16:23:20 1721  	     INNER JOIN LICENSE ON SUBSCRIPTION.ID = LICENSE.SUBSCRIPTION_ID
16:23:20 1722  	   WHERE
16:23:20 1723  	     LICENSE.INVOICE_ID = in_invoice_id
16:23:20 1724  	     AND ROWNUM <= 1;
16:23:20 1725  	   EXCEPTION
16:23:20 1726  	     WHEN NO_DATA_FOUND THEN
16:23:20 1727  	       BEGIN
16:23:20 1728  
16:23:20 1729  		 var_subscription_id := NULL;
16:23:20 1730  
16:23:20 1731  		 SELECT
16:23:20 1732  		   CHARGE.INSTRUMENT_ID,
16:23:20 1733  		   CHARGE.INSTRUMENT_TYPE_ID,
16:23:20 1734  		   GIFT_CERTIFICATE.PURCHASER_GROUP_ID
16:23:20 1735  		   into
16:23:20 1736  		   var_instrument_id,
16:23:20 1737  		   var_instrument_type_id,
16:23:20 1738  		   var_group_id
16:23:20 1739  		 FROM
16:23:20 1740  		   INVOICE
16:23:20 1741  		   INNER JOIN CHARGE ON INVOICE.ID = CHARGE.INVOICE_ID
16:23:20 1742  		   INNER JOIN GIFT_CERTIFICATE ON GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = INVOICE.ID
16:23:20 1743  		 WHERE
16:23:20 1744  		   INVOICE.ID = in_invoice_id
16:23:20 1745  		   AND ROWNUM <= 1;
16:23:20 1746  
16:23:20 1747  		 SELECT
16:23:20 1748  		   ACCOUNT.ID into var_account_id
16:23:20 1749  		 FROM
16:23:20 1750  		   ACCOUNT
16:23:20 1751  		 WHERE
16:23:20 1752  		   ACCOUNT.GROUP_ID = var_group_id;
16:23:20 1753  
16:23:20 1754  		 EXCEPTION
16:23:20 1755  		   WHEN NO_DATA_FOUND THEN
16:23:20 1756  		     RAISE CAN_NOT_FIND_SUBSCR_OR_GC;
16:23:20 1757  	       END;
16:23:20 1758  	 END;
16:23:20 1759  
16:23:20 1760  	 -- Check account status. It should not to be frozen
16:23:20 1761  	 SELECT
16:23:20 1762  	   ACCOUNT.ACCOUNT_STATUS_ID into var_account_status_id
16:23:20 1763  	 FROM
16:23:20 1764  	   ACCOUNT
16:23:20 1765  	 WHERE
16:23:20 1766  	   ACCOUNT.ID = var_account_id;
16:23:20 1767  
16:23:20 1768  	 IF var_account_status_id = GLOBAL_STATUSES_V19.ACCOUNT_FROZEN THEN
16:23:20 1769  	   RAISE ACCOUNT_IS_FROZEN;
16:23:20 1770  	 END IF;
16:23:20 1771  
16:23:20 1772  	 IF var_invoice_status_id != GLOBAL_STATUSES_V19.INVOICE_CLOSED THEN
16:23:20 1773  	   RAISE INVOICE_IS_NOT_CLOSED;
16:23:20 1774  	 END IF;
16:23:20 1775  
16:23:20 1776  	 BEGIN
16:23:20 1777  	   PROCS_INVOICE_V19.CALCULATE_INVOICE_AMOUNT (
16:23:20 1778  	     in_invoice_id => in_invoice_id,
16:23:20 1779  	     out_amount    => var_invoice_amount
16:23:20 1780  	   );
16:23:20 1781  	   EXCEPTION
16:23:20 1782  	     WHEN OTHERS THEN
16:23:20 1783  	       EXCEPTION_MESSAGE := SQLERRM;
16:23:20 1784  	       RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
16:23:20 1785  	 END;
16:23:20 1786  
16:23:20 1787  	 IF ( in_refund_amount > var_invoice_amount ) THEN
16:23:20 1788  	   RAISE REFUND_IS_GREATER_THAN_ANOUNT;
16:23:20 1789  	 END IF;
16:23:20 1790  
16:23:20 1791  	 SELECT /*+ STAR_TRANSFORMATION */
16:23:20 1792  	   SUM(CHARGE.CHARGE_AMOUNT) into var_refunds_before
16:23:20 1793  	 FROM
16:23:20 1794  	   CHARGE
16:23:20 1795  	 WHERE
16:23:20 1796  	   CHARGE.INVOICE_ID = in_invoice_id
16:23:20 1797  	   AND (
16:23:20 1798  	     CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V19.CHARGE_OPENED
16:23:20 1799  	     OR (
16:23:20 1800  	       CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V19.CHARGE_PROCESSED
16:23:20 1801  	       AND EXISTS (
16:23:20 1802  		 SELECT 1 FROM TRANSACTION_ATTEMPT ta where ta.transaction_id = CHARGE.TRANSACTION_ID and ta.transaction_attempt_status_id = GLOBAL_STATUSES_V19.TRANS_ATTEMPT_SUCCESS
16:23:20 1803  	       )
16:23:20 1804  	     )
16:23:20 1805  	   )
16:23:20 1806  	   AND CHARGE.CHARGE_AMOUNT < 0;
16:23:20 1807  
16:23:20 1808  	 -- Refunds are negative
16:23:20 1809  	 IF var_refunds_before IS NULL THEN var_refunds_before := 0; END IF;
16:23:20 1810  	 var_refunds_before := 0 - var_refunds_before;
16:23:20 1811  
16:23:20 1812  	 var_charges_amount := 0;
16:23:20 1813  
16:23:20 1814  	 FOR f_processed_charges IN (
16:23:20 1815  	   SELECT
16:23:20 1816  	     CHARGE.CHARGE_AMOUNT
16:23:20 1817  	   FROM
16:23:20 1818  	     CHARGE
16:23:20 1819  	   WHERE
16:23:20 1820  	     CHARGE.INVOICE_ID = in_invoice_id
16:23:20 1821  	     AND CHARGE.CHARGE_AMOUNT > 0
16:23:20 1822  	     AND CHARGE.CHARGE_STATUS_ID IN (SELECT GLOBAL_STATUSES_V19.CHARGE_PROCESSED FROM DUAL)
16:23:20 1823  	     AND EXISTS (SELECT 1 FROM TRANSACTION_ATTEMPT ta where ta.transaction_id = CHARGE.TRANSACTION_ID and ta.transaction_attempt_status_id = GLOBAL_STATUSES_V19.TRANS_ATTEMPT_SUCCESS)
16:23:20 1824  	 )
16:23:20 1825  	 LOOP
16:23:20 1826  	   var_charges_amount := var_charges_amount + f_processed_charges.CHARGE_AMOUNT;
16:23:20 1827  	 END LOOP;
16:23:20 1828  
16:23:20 1829  	 IF (in_refund_amount + var_refunds_before > var_invoice_amount) THEN
16:23:20 1830  	   RAISE TOT_REF_IS_GREATER_THAN_ANOUNT;
16:23:20 1831  	 END IF;
16:23:20 1832  
16:23:20 1833  	 IF (in_refund_amount + var_refunds_before > var_charges_amount) THEN
16:23:20 1834  	   RAISE TOT_REF_IS_GRATER_THAN_CHARGES;
16:23:20 1835  	 END IF;
16:23:20 1836  
16:23:20 1837  	 BEGIN
16:23:20 1838  	   PROCS_TRANSACTION_V19.CREATE_TRANSACTION(
16:23:20 1839  	     in_transaction_id	       => NULL,
16:23:20 1840  	     in_status_id	       => GLOBAL_STATUSES_V19.TRANSACTION_PREPARE,
16:23:20 1841  	     in_amount		       => -in_refund_amount,
16:23:20 1842  	     in_created_by	       => in_created_by,
16:23:20 1843  	     in_order_id	       => NULL,
16:23:20 1844  	     in_is_refund	       => GLOBAL_CONSTANTS_V19.TRUE,
16:23:20 1845  	     in_transaction_type_code  => 'REFUND',
16:23:20 1846  	     out_transaction_id        => var_new_transaction_id
16:23:20 1847  	   );
16:23:20 1848  	   EXCEPTION
16:23:20 1849  	     WHEN OTHERS THEN
16:23:20 1850  	       EXCEPTION_MESSAGE := SQLERRM;
16:23:20 1851  	       RAISE CAN_NOT_CREATE_TRANSACTION;
16:23:20 1852  	 END;
16:23:20 1853  
16:23:20 1854  	 BEGIN
16:23:20 1855  	   PROCS_CHARGE_V19.CREATE_CHARGE(
16:23:20 1856  	     in_invoice_id	   => in_invoice_id,
16:23:20 1857  	     in_transaction_id	   => var_new_transaction_id,
16:23:20 1858  	     in_instrument_type_id => var_instrument_type_id,
16:23:20 1859  	     in_instrument_id	   => var_instrument_id,
16:23:20 1860  	     in_charge_amount	   => -in_refund_amount,
16:23:20 1861  	     in_created_by	   => in_created_by,
16:23:20 1862  	     in_charge_status_id   => GLOBAL_STATUSES_V19.CHARGE_OPENED,
16:23:20 1863  	     out_charge_id	   => var_new_charge_id
16:23:20 1864  	   );
16:23:20 1865  	   EXCEPTION
16:23:20 1866  	     WHEN OTHERS THEN
16:23:20 1867  	       EXCEPTION_MESSAGE := SQLERRM;
16:23:20 1868  	       RAISE CAN_NOT_CREATE_CHARGE;
16:23:20 1869  	 END;
16:23:20 1870  
16:23:20 1871  	 out_charge_id := var_new_charge_id;
16:23:20 1872  
16:23:20 1873  	 IF in_note IS NOT NULL AND var_subscription_id IS NOT NULL THEN
16:23:20 1874  	   BEGIN
16:23:20 1875  	     PROCS_SUBSCRIPTION_V19.ANNOTATE_SUBSCRIPTION(
16:23:20 1876  	       in_subscription_id => var_subscription_id,
16:23:20 1877  	       in_agent_id	  => 0, -- AGENT_ID??
16:23:20 1878  	       in_note		  => in_note,
16:23:20 1879  	       in_created_by	  => in_created_by
16:23:20 1880  	     );
16:23:20 1881  	     EXCEPTION
16:23:20 1882  	       WHEN OTHERS THEN
16:23:20 1883  		 EXCEPTION_MESSAGE := SQLERRM;
16:23:20 1884  		 RAISE CAN_NOT_ANNOTATE_SUBSCRIPTION;
16:23:20 1885  	   END;
16:23:20 1886  	 END IF;
16:23:20 1887  
16:23:20 1888  EXCEPTION
16:23:20 1889  WHEN CAN_NOT_FIND_SUBSCR_OR_GC THEN
16:23:20 1890  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 1891  	   SPROC_NAME, 'Could not find subscription or GC for the inovice');
16:23:20 1892  WHEN INVOICE_IS_NOT_CLOSED THEN
16:23:20 1893  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
16:23:20 1894  	   SPROC_NAME, 'Invoice is not closed');
16:23:20 1895  WHEN ACCOUNT_IS_FROZEN THEN
16:23:20 1896  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
16:23:20 1897  	   SPROC_NAME, 'Could not refund subscription for frozen account');
16:23:20 1898  WHEN BAD_INVOICE_ID THEN
16:23:20 1899  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 1900  	   SPROC_NAME, 'No such invoice');
16:23:20 1901  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
16:23:20 1902  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 1903  	   SPROC_NAME, 'Could not calculate invoice amount', EXCEPTION_MESSAGE);
16:23:20 1904  WHEN REFUND_IS_GREATER_THAN_ANOUNT THEN
16:23:20 1905  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
16:23:20 1906  	   SPROC_NAME, 'Refund is greater than amount');
16:23:20 1907  WHEN TOT_REF_IS_GREATER_THAN_ANOUNT THEN
16:23:20 1908  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
16:23:20 1909  	   SPROC_NAME, 'There were refunds before and sum of all refunds and new refund more than invoice amount');
16:23:20 1910  WHEN TOT_REF_IS_GRATER_THAN_CHARGES THEN
16:23:20 1911  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
16:23:20 1912  	   SPROC_NAME, 'Total refund amount is greater than sum of processed charges');
16:23:20 1913  WHEN CAN_NOT_CREATE_TRANSACTION THEN
16:23:20 1914  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 1915  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
16:23:20 1916  WHEN CAN_NOT_CREATE_CHARGE THEN
16:23:20 1917  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 1918  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
16:23:20 1919  WHEN CAN_NOT_ANNOTATE_SUBSCRIPTION THEN
16:23:20 1920  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 1921  	   SPROC_NAME, 'Could not annotate subscription', EXCEPTION_MESSAGE);
16:23:20 1922  WHEN OTHERS THEN
16:23:20 1923  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 1924  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 1925  END REFUND_INVOICE;
16:23:20 1926  
16:23:20 1927  /******************************************************************************/
16:23:20 1928  
16:23:20 1929  PROCEDURE GET_PAYMENT_INFO_BY_INVOICE_ID (
16:23:20 1930  	 in_invoice_id		     IN NUMBER,
16:23:20 1931  	 out_order_id		     OUT VARCHAR2,
16:23:20 1932  	 out_external_transaction_id OUT VARCHAR2
16:23:20 1933  ) AS
16:23:20 1934  SPROC_NAME CONSTANT VARCHAR2(30) := 'GET_PEYMENT_INFO_BY_INVOICE_ID';
16:23:20 1935  -- VARIABLES
16:23:20 1936  temp_invoice_id number;
16:23:20 1937  cnt_matched_instr number := 0;
16:23:20 1938  -- EXCEPTIONS
16:23:20 1939  BAD_INVOICE_ID EXCEPTION;
16:23:20 1940  BEGIN
16:23:20 1941  
16:23:20 1942  	 BEGIN
16:23:20 1943  	   SELECT
16:23:20 1944  	     i.id into temp_invoice_id
16:23:20 1945  	   from
16:23:20 1946  	     invoice i
16:23:20 1947  	   where
16:23:20 1948  	     i.id = in_invoice_id;
16:23:20 1949  	   EXCEPTION
16:23:20 1950  	     WHEN NO_DATA_FOUND THEN
16:23:20 1951  	       RAISE BAD_INVOICE_ID;
16:23:20 1952  	 END;
16:23:20 1953  
16:23:20 1954  	 select
16:23:20 1955  	   count(1) into cnt_matched_instr
16:23:20 1956  	 from
16:23:20 1957  	   charge ch
16:23:20 1958  	 inner join
16:23:20 1959  	   subscription s
16:23:20 1960  	 on
16:23:20 1961  	   s.instrument_id = ch.instrument_id
16:23:20 1962  	 where
16:23:20 1963  	   ch.invoice_id = in_invoice_id;
16:23:20 1964  
16:23:20 1965  	 if cnt_matched_instr = 0 then
16:23:20 1966  	   out_external_transaction_id := null;
16:23:20 1967  	   out_order_id := null;
16:23:20 1968  	   return;
16:23:20 1969  	 end if;
16:23:20 1970  
16:23:20 1971  	 SELECT
16:23:20 1972  	   t.order_id,
16:23:20 1973  	   ta.external_transaction_id
16:23:20 1974  	   into
16:23:20 1975  	   out_order_id,
16:23:20 1976  	   out_external_transaction_id
16:23:20 1977  	 from
16:23:20 1978  	   charge ch
16:23:20 1979  	   inner join transaction t on ch.transaction_id = t.id
16:23:20 1980  	   inner join transaction_attempt ta on ta.transaction_id = t.id
16:23:20 1981  	 where
16:23:20 1982  	   ch.invoice_id = in_invoice_id
16:23:20 1983  	   and ta.transaction_attempt_status_id = GLOBAL_STATUSES_V19.TRANS_ATTEMPT_SUCCESS
16:23:20 1984  	   and ch.charge_amount > 0; -- We are not creating charges for the 0-amount invoices
16:23:20 1985  
16:23:20 1986  EXCEPTION
16:23:20 1987  WHEN BAD_INVOICE_ID THEN
16:23:20 1988  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 1989  	   SPROC_NAME, 'No such invoice');
16:23:20 1990  WHEN NO_DATA_FOUND THEN
16:23:20 1991  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 1992  	   SPROC_NAME, 'No payment data found');
16:23:20 1993  WHEN OTHERS THEN
