16:23:20 727  PROCEDURE GET_CLOSED_REFUNDS_BY_INVOICE (
16:23:20 728  /*
16:23:20 729  Throws exceptions:
16:23:20 730  APP_EXCEPTION_CODES_V19.NOT_FOUND
16:23:20 731  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
16:23:20 732  */
16:23:20 733  	in_invoice_id	IN  NUMBER,
16:23:20 734  	out_result_set OUT SYS_REFCURSOR
16:23:20 735  ) AS
