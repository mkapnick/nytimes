16:23:19 1164  PRODUCT_ELIGIBILITY_NAME CONSTANT VARCHAR2(19) := 'MAX_CONCURRENT_SUBS';
16:23:19 1165  -- VARIABLES
16:23:19 1166  temp_offer_chain_id	OFFER_CHAIN.ID%TYPE;
16:23:19 1167  var_same_products	SYS_REFCURSOR;
16:23:19 1168  var_same_product_id	NUMBER;
16:23:19 1169  same_product_count	NUMBER;
16:23:19 1170  var_product_eligibility_limit NUMBER;
16:23:19 1171  s_product_eligibility_limit   VARCHAR2(100);
16:23:19 1172  -- EXCEPTIONS
16:23:19 1173  BAD_FIRST_OFFER_CHAIN	      EXCEPTION;
16:23:19 1174  BAD_SECOND_OFFER_CHAIN	      EXCEPTION;
16:23:19 1175  BEGIN
16:23:19 1176  
16:23:19 1177  	 -- Check that first offer chain exists
16:23:19 1178  	 BEGIN
16:23:19 1179  	   SELECT
16:23:19 1180  	     OFFER_CHAIN.ID into temp_offer_chain_id
16:23:19 1181  	   FROM
16:23:19 1182  	     OFFER_CHAIN
16:23:19 1183  	   WHERE
16:23:19 1184  	     OFFER_CHAIN.ID = in_offer_chain_1;
16:23:19 1185  	   EXCEPTION
16:23:19 1186  	     WHEN NO_DATA_FOUND THEN
16:23:19 1187  	       RAISE BAD_FIRST_OFFER_CHAIN;
16:23:19 1188  	 END;
16:23:19 1189  
16:23:19 1190  	 -- Check that second offer chain exists
16:23:19 1191  	 BEGIN
16:23:19 1192  	   SELECT
16:23:19 1193  	     OFFER_CHAIN.ID into temp_offer_chain_id
16:23:19 1194  	   FROM
16:23:19 1195  	     OFFER_CHAIN
16:23:19 1196  	   WHERE
16:23:19 1197  	     OFFER_CHAIN.ID = in_offer_chain_2;
16:23:19 1198  	   EXCEPTION
16:23:19 1199  	     WHEN NO_DATA_FOUND THEN
16:23:19 1200  	       RAISE BAD_SECOND_OFFER_CHAIN;
16:23:19 1201  	 END;
16:23:19 1202  
16:23:19 1203  	 PROCS_OFFER_CHAIN_V19.GET_OFF_CHAINS_SAME_PRODUCTS(
16:23:19 1204  	   in_offer_chain_1 => in_offer_chain_1,
16:23:19 1205  	   in_offer_chain_2 => in_offer_chain_2,
16:23:19 1206  	   out_result_set   => var_same_products
16:23:19 1207  	 );
16:23:19 1208  
16:23:19 1209  	 LOOP
16:23:19 1210  	   FETCH var_same_products INTO var_same_product_id, same_product_count;
16:23:19 1211  	   EXIT WHEN var_same_products%NOTFOUND;
16:23:19 1212  
16:23:19 1213  	   IF in_use_eligibility_rules = GLOBAL_CONSTANTS_V19.FALSE THEN
16:23:19 1214  	     -- Return false because this offer chains having same products
16:23:19 1215  	     RETURN GLOBAL_CONSTANTS_V19.TRUE;
16:23:19 1216  	   ELSE
16:23:19 1217  
16:23:19 1218  	     -- Get eligibility rule for given product
16:23:19 1219  	     BEGIN
16:23:19 1220  	       SELECT
16:23:19 1221  		 PRODUCT_ELIGIBILITY.VALUE into s_product_eligibility_limit
16:23:19 1222  	       FROM
16:23:19 1223  		 PRODUCT_ELIGIBILITY
16:23:19 1224  	       WHERE
16:23:19 1225  		 PRODUCT_ELIGIBILITY.PRODUCT_ID = var_same_product_id
16:23:19 1226  		 AND PRODUCT_ELIGIBILITY.NAME = PRODUCT_ELIGIBILITY_NAME;
16:23:19 1227  
16:23:19 1228  	       -- REVU: What should to be here? 1?
16:23:19 1229  	       EXCEPTION
16:23:19 1230  		 WHEN NO_DATA_FOUND THEN
16:23:19 1231  		   s_product_eligibility_limit := '1';
16:23:19 1232  	     END;
16:23:19 1233  
16:23:19 1234  	     IF UPPER(s_product_eligibility_limit) = GLOBAL_CONSTANTS_V19.MAX_CONSURRENT_PRD_UNLIM THEN
16:23:19 1235  	       RETURN GLOBAL_CONSTANTS_V19.FALSE;
16:23:19 1236  	     END IF;
16:23:19 1237  
16:23:19 1238  	     var_product_eligibility_limit := TO_NUMBER(s_product_eligibility_limit);
16:23:19 1239  
16:23:19 1240  	     -- Check for limit
16:23:19 1241  	     IF var_product_eligibility_limit < same_product_count THEN
16:23:19 1242  	       RETURN GLOBAL_CONSTANTS_V19.TRUE;
16:23:19 1243  	     END IF;
16:23:19 1244  
16:23:19 1245  	   END IF;
16:23:19 1246  	 END LOOP;
16:23:19 1247  
16:23:19 1248  	 RETURN GLOBAL_CONSTANTS_V19.FALSE;
16:23:19 1249  
16:23:19 1250  EXCEPTION
16:23:19 1251  WHEN BAD_FIRST_OFFER_CHAIN THEN
16:23:19 1252  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:19 1253  	   SPROC_NAME, 'First offer chain not found');
16:23:19 1254  WHEN BAD_SECOND_OFFER_CHAIN THEN
16:23:19 1255  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:19 1256  	   SPROC_NAME, 'Second offer chain not found');
16:23:19 1257  WHEN OTHERS THEN
16:23:19 1258  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:19 1259  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:23:19 1260  END CHECK_FOR_SAME_PRODUCTS;
