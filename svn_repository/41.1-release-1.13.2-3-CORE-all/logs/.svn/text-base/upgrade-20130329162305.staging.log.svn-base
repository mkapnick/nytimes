16:23:20 416  
16:23:20 417  PROCEDURE FINALIZE_SUBSCRIPTION_CREATION (
16:23:20 418  	in_subscription_id    IN NUMBER,
16:23:20 419  	in_invoice_id	      IN NUMBER,
16:23:20 420  	in_instrument_type_id IN NUMBER,
16:23:20 421  	in_instrument_id      IN NUMBER,
16:23:20 422  	in_created_by	      IN VARCHAR2
16:23:20 423  ) AS
16:23:20 424  SPROC_NAME CONSTANT VARCHAR2(30) := 'FINALIZE_SUBSCRIPTION_CREATION';
16:23:20 425  -- VARIABLES
16:23:20 426  var_invoice_amount     NUMBER(10, 2);
16:23:20 427  var_new_transaction_id NUMBER;
16:23:20 428  var_new_charge_id      NUMBER;
16:23:20 429  -- EXCEPTIONS
16:23:20 430  CAN_NOT_USE_FCINSTR	  EXCEPTION;
16:23:20 431  CAN_NOT_CALC_INVOICE_AMOUNT EXCEPTION;
16:23:20 432  CAN_NOT_CREATE_TRANSACTION  EXCEPTION;
16:23:20 433  CAN_NOT_CREATE_CHARGE	  EXCEPTION;
16:23:20 434  
16:23:20 435  EXCEPTION_MESSAGE VARCHAR2(1024);
16:23:20 436  BEGIN
16:23:20 437  
16:23:20 438  	-- Calculate invoice amount ( + discounts, taxes)
16:23:20 439  	BEGIN
16:23:20 440  	  PROCS_INVOICE_V19.CALCULATE_INVOICE_AMOUNT(
16:23:20 441  	    in_invoice_id => in_invoice_id,
16:23:20 442  	    out_amount	  => var_invoice_amount
16:23:20 443  	  );
16:23:20 444  	  EXCEPTION
16:23:20 445  	   WHEN OTHERS THEN
16:23:20 446  	     EXCEPTION_MESSAGE := SQLERRM;
16:23:20 447  	     RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
16:23:20 448  	END;
16:23:20 449  
16:23:20 450  	IF in_instrument_type_id = GLOBAL_ENUMS_V19.INSTRUMENT_ZCI_INSTRUMENT
16:23:20 451  	  AND var_invoice_amount > 0 THEN
16:23:20 452  	  RAISE CAN_NOT_USE_FCINSTR;
16:23:20 453  	END IF;
16:23:20 454  
16:23:20 455  	IF var_invoice_amount = 0 THEN
16:23:20 456  	  -- UPDATE INVOICE. SET STATUS TO PROCESSED
16:23:20 457  	  PROCS_INVOICE_CRU_V19.UPDATE_INVOICE(
16:23:20 458  	    in_invoice_id		   => in_invoice_id,
16:23:20 459  	    in_updated_by		   => in_created_by,
16:23:20 460  	    in_invoice_status_id	   => GLOBAL_STATUSES_V19.INVOICE_CLOSED
16:23:20 461  	  );
16:23:20 462  	ELSE
16:23:20 463  	  -- Create transaction and charge
16:23:20 464  	  BEGIN
16:23:20 465  	    PROCS_TRANSACTION_V19.CREATE_TRANSACTION(
16:23:20 466  	      in_transaction_id 	=> NULL,
16:23:20 467  	      in_status_id		=> GLOBAL_STATUSES_V19.TRANSACTION_PENDING,
16:23:20 468  	      in_amount 		=> var_invoice_amount,
16:23:20 469  	      in_created_by		=> in_created_by,
16:23:20 470  	      in_order_id		=> NULL,
16:23:20 471  	      in_transaction_type_code	=> 'START_SUBSCRIPTION',
16:23:20 472  	      out_transaction_id	=> var_new_transaction_id
16:23:20 473  	    );
16:23:20 474  	    EXCEPTION
16:23:20 475  	      WHEN OTHERS THEN
16:23:20 476  		EXCEPTION_MESSAGE := SQLERRM;
16:23:20 477  		RAISE CAN_NOT_CREATE_TRANSACTION;
16:23:20 478  	  END;
16:23:20 479  
16:23:20 480  	  BEGIN
16:23:20 481  	    PROCS_CHARGE_V19.CREATE_CHARGE(
16:23:20 482  	      in_invoice_id	    => in_invoice_id,
16:23:20 483  	      in_transaction_id     => var_new_transaction_id,
16:23:20 484  	      in_instrument_type_id => in_instrument_type_id,
16:23:20 485  	      in_instrument_id	    => in_instrument_id,
16:23:20 486  	      in_charge_amount	    => var_invoice_amount,
16:23:20 487  	      in_created_by	    => in_created_by,
16:23:20 488  	      in_charge_status_id   => GLOBAL_STATUSES_V19.CHARGE_OPENED,
16:23:20 489  	      out_charge_id	    => var_new_charge_id
16:23:20 490  	    );
16:23:20 491  	    EXCEPTION
16:23:20 492  	      WHEN OTHERS THEN
16:23:20 493  		EXCEPTION_MESSAGE := SQLERRM;
16:23:20 494  		RAISE CAN_NOT_CREATE_CHARGE;
16:23:20 495  	  END;
16:23:20 496  	END IF;
16:23:20 497  
16:23:20 498  EXCEPTION
16:23:20 499  WHEN CAN_NOT_USE_FCINSTR THEN
16:23:20 500  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
16:23:20 501  	  SPROC_NAME, 'Could not use "free charge instrument" for non-zero invoice');
16:23:20 502  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
16:23:20 503  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 504  	  SPROC_NAME, 'Could not calculate invoice amount', EXCEPTION_MESSAGE);
16:23:20 505  WHEN CAN_NOT_CREATE_TRANSACTION THEN
16:23:20 506  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 507  	  SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
16:23:20 508  WHEN CAN_NOT_CREATE_CHARGE THEN
16:23:20 509  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 510  	  SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
16:23:20 511  WHEN OTHERS THEN
16:23:20 512  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 513  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 514  END FINALIZE_SUBSCRIPTION_CREATION;
