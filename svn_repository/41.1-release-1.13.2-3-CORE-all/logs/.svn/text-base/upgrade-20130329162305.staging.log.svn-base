16:23:20 2376  	     in_subscription_id        => in_subscription_id,
16:23:20 2377  	     in_subscription_status_id => GLOBAL_STATUSES_V19.SUBSCRIPTION_CANCELED,
16:23:20 2378  	     in_cancelation_date       => in_cancelation_date,
16:23:20 2379  	     in_updated_by	       => in_updated_by,
16:23:20 2380  	     in_sct_id		       => var_sct_id
16:23:20 2381  	   );
16:23:20 2382  	   EXCEPTION
