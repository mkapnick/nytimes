16:23:20 436  -- norlov: #38796
16:23:20 437  PROCEDURE GET_TRANSACTION_INVOICE (
16:23:20 438  /*
16:23:20 439  Throws exceptions:
16:23:20 440  APP_EXCEPTION_CODES_V19.NOT_FOUND
16:23:20 441  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
16:23:20 442  */
16:23:20 443  	in_transaction_id  IN  NUMBER,
16:23:20 444  	out_result_set	      OUT SYS_REFCURSOR
16:23:20 445  ) AS
16:23:20 446  SPROC_NAME CONSTANT  VARCHAR2(23) := 'GET_TRANSACTION_INVOICE';
16:23:20 447  -- VARIABLES
16:23:20 448  temp_transaction_id  NUMBER;
16:23:20 449  var_invoice_id	   NUMBER;
16:23:20 450  var_subscription_id  NUMBER;
16:23:20 451  var_offer_chain_id   NUMBER;
16:23:20 452  var_offer_chain_name VARCHAR2(255);
16:23:20 453  -- EXCEPTIONS
16:23:20 454  BAD_TRANSACTION_ID     EXCEPTION;
16:23:20 455  EXCEPTION_MESSAGE      VARCHAR2(1024);
16:23:20 456  BEGIN
16:23:20 457   -- check if there is the transaction
16:23:20 458  	BEGIN
16:23:20 459  	  SELECT
16:23:20 460  	    TRANSACTION.ID into temp_transaction_id
16:23:20 461  	  FROM
16:23:20 462  	    TRANSACTION
16:23:20 463  	  WHERE
16:23:20 464  	    TRANSACTION.ID = in_transaction_id;
16:23:20 465  	  EXCEPTION
16:23:20 466  	    WHEN NO_DATA_FOUND THEN
16:23:20 467  	      RAISE BAD_TRANSACTION_ID;
16:23:20 468  	END;
16:23:20 469  
16:23:20 470  	-- Get invoice id
16:23:20 471  	SELECT DISTINCT
16:23:20 472  	  CHARGE.INVOICE_ID into var_invoice_id
16:23:20 473  	FROM
16:23:20 474  	  CHARGE
16:23:20 475  	WHERE
16:23:20 476  	  CHARGE.TRANSACTION_ID = in_transaction_id;
16:23:20 477  
16:23:20 478  	-- Get subscription id if exists
16:23:20 479  	BEGIN
16:23:20 480  	  SELECT DISTINCT
16:23:20 481  	    LICENSE.SUBSCRIPTION_ID into var_subscription_id
16:23:20 482  	  FROM
16:23:20 483  	    LICENSE
16:23:20 484  	  WHERE
16:23:20 485  	    LICENSE.INVOICE_ID = var_invoice_id;
16:23:20 486  	  EXCEPTION
16:23:20 487  	    WHEN NO_DATA_FOUND THEN
16:23:20 488  	      var_subscription_id := NULL;
16:23:20 489  	END;
16:23:20 490  
16:23:20 491  	IF var_subscription_id IS NOT NULL THEN
16:23:20 492  	  -- Fetch offer chain from subscription
16:23:20 493  	  SELECT
16:23:20 494  	    OFFER_CHAIN.ID,
16:23:20 495  	    OFFER_CHAIN.NAME
16:23:20 496  	    into
16:23:20 497  	    var_offer_chain_id,
16:23:20 498  	    var_offer_chain_name
16:23:20 499  	  FROM
16:23:20 500  	    OFFER_CHAIN
16:23:20 501  	    INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
16:23:20 502  	  WHERE
16:23:20 503  	    SUBSCRIPTION.ID = var_subscription_id;
16:23:20 504  	ELSE
16:23:20 505  	  -- Fetch offer chain from GC
16:23:20 506  	  SELECT
16:23:20 507  	    OFFER_CHAIN.ID,
16:23:20 508  	    OFFER_CHAIN.NAME
16:23:20 509  	    into
16:23:20 510  	    var_offer_chain_id,
16:23:20 511  	    var_offer_chain_name
16:23:20 512  	  FROM
16:23:20 513  	    OFFER_CHAIN
16:23:20 514  	    INNER JOIN GIFT_CERTIFICATE ON GIFT_CERTIFICATE.OFFER_CHAIN_ID = OFFER_CHAIN.ID
16:23:20 515  	  WHERE
16:23:20 516  	    GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = var_invoice_id;
16:23:20 517  	END IF;
16:23:20 518  
16:23:20 519  	OPEN out_result_set FOR
16:23:20 520  	SELECT DISTINCT
16:23:20 521  	  var_invoice_id       AS "INVOICE_ID",
16:23:20 522  	  var_subscription_id  AS "SUBSCRIPTION_ID",
16:23:20 523  	  var_offer_chain_id   AS "OFFER_CHAIN_ID",
16:23:20 524  	  var_offer_chain_name AS "OFFER_CHAIN_NAME"
16:23:20 525  	FROM
16:23:20 526  	  DUAL;
16:23:20 527  
16:23:20 528  EXCEPTION
16:23:20 529  WHEN BAD_TRANSACTION_ID THEN
16:23:20 530  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 531  	  SPROC_NAME, 'No such transaction');
16:23:20 532  WHEN OTHERS THEN
16:23:20 533  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 534  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 535  END GET_TRANSACTION_INVOICE;
16:23:20 536  
