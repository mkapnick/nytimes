16:23:20 608  	  ACCOUNT_NOTE.AGENT_ID,
16:23:20 609  	  ACCOUNT_NOTE.CREATE_DATE,
16:23:20 610  	  ACCOUNT_NOTE.CREATED_BY,
16:23:20 611  	  ACCOUNT_NOTE.ID,
16:23:20 612  	  ACCOUNT_NOTE.NOTE
