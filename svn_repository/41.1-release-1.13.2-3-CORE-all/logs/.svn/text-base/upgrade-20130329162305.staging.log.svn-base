16:23:20 2767  	       AND in_old_fin_instrument_type = GLOBAL_ENUMS_V19.INSTRUMENT_CREDIT_CARD
16:23:20 2768  	     )
16:23:20 2769  	     OR
16:23:20 2770  	     (
16:23:20 2771  	       CC.ID = in_new_fin_instrument_id
16:23:20 2772  	       AND in_new_fin_instrument_type = GLOBAL_ENUMS_V19.INSTRUMENT_CREDIT_CARD
