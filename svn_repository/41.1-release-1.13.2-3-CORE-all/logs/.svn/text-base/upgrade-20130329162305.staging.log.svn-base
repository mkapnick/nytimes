16:23:20 580  
16:23:20 581  PROCEDURE UPDATE_CREDIT_CARD (
16:23:20 582  /*
16:23:20 583  Throws exceptions:
16:23:20 584  APP_EXCEPTION_CODES_V19.NOT_FOUND
16:23:20 585  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
16:23:20 586  */
16:23:20 587  	in_credit_card_id	  IN NUMBER,
16:23:20 588  	in_updated_by		  IN VARCHAR2,
16:23:20 589  	in_instrument_name	  IN VARCHAR2,
16:23:20 590  	in_is_default		  IN NUMBER
16:23:20 591  ) AS
16:23:20 592  SPROC_NAME CONSTANT VARCHAR2(18) := 'UPDATE_CREDIT_CARD';
16:23:20 593  -- VARIABLES
16:23:20 594  var_account_id NUMBER;
16:23:20 595  var_group_id   NUMBER;
16:23:20 596  temp_cc_rownum NUMBER;
16:23:20 597  current_def_instrument_type_id NUMBER;
16:23:20 598  current_def_instrument_id      NUMBER;
16:23:20 599  -- EXCEPTION
16:23:20 600  CAN_NOT_SET_DEF_FINANCIAL  EXCEPTION;
16:23:20 601  BAD_IS_DEFAULT_VALUE	 EXCEPTION;
16:23:20 602  CAN_NOT_GET_DEF_FINANCIAL  EXCEPTION;
16:23:20 603  CAN_NOT_DEL_DEF_FINANCIAL  EXCEPTION;
16:23:20 604  EXCEPTION_MESSAGE 	 VARCHAR2(1024);
16:23:20 605  BEGIN
16:23:20 606  
16:23:20 607  	-- Get account id
16:23:20 608  	SELECT
16:23:20 609  	  CREDIT_CARD.ACCOUNT_ID
16:23:20 610  	  into
16:23:20 611  	  var_account_id
16:23:20 612  	FROM
16:23:20 613  	  CREDIT_CARD
16:23:20 614  	WHERE
16:23:20 615  	  CREDIT_CARD.ID = in_credit_card_id;
16:23:20 616  
16:23:20 617  	-- Get group id
16:23:20 618  	SELECT
16:23:20 619  	  ACCOUNT.GROUP_ID into var_group_id
16:23:20 620  	FROM
16:23:20 621  	  ACCOUNT
16:23:20 622  	WHERE
16:23:20 623  	  ACCOUNT.ID = var_account_id;
16:23:20 624  
16:23:20 625  	-- Check that passed data is correct
16:23:20 626  	IF in_is_default != GLOBAL_CONSTANTS_V19.TRUE
16:23:20 627  	  AND in_is_default != GLOBAL_CONSTANTS_V19.FALSE
16:23:20 628  	  AND in_is_default IS NOT NULL THEN
16:23:20 629  	  RAISE BAD_IS_DEFAULT_VALUE;
16:23:20 630  	END IF;
16:23:20 631  
16:23:20 632  	-- Update credit card
16:23:20 633  	IF in_instrument_name IS NOT NULL THEN
16:23:20 634  	  PROCS_FIN_INSTRUMENTS_CRU_V19.UPDATE_CREDIT_CARD(
16:23:20 635  	    in_credit_card_id  => in_credit_card_id,
16:23:20 636  	    in_updated_by      => in_updated_by,
16:23:20 637  	    in_instrument_name => in_instrument_name
16:23:20 638  	  );
16:23:20 639  	END IF;
16:23:20 640  
16:23:20 641  	-- Set default financial instrument
16:23:20 642  	IF in_is_default = GLOBAL_CONSTANTS_V19.TRUE THEN
16:23:20 643  	  BEGIN
16:23:20 644  	    PROCS_FIN_INSTRUMENTS_V19.SET_DEF_FINANCIAL_INSTRUMENT(
16:23:20 645  	      in_group_id	    => var_group_id,
16:23:20 646  	      in_instrument_type_id => GLOBAL_ENUMS_V19.INSTRUMENT_CREDIT_CARD,
16:23:20 647  	      in_instrument_id	    => in_credit_card_id,
16:23:20 648  	      in_updated_by	    => in_updated_by
16:23:20 649  	    );
16:23:20 650  	    EXCEPTION
16:23:20 651  	      WHEN OTHERS THEN
16:23:20 652  		EXCEPTION_MESSAGE := SQLERRM;
16:23:20 653  		RAISE CAN_NOT_SET_DEF_FINANCIAL;
16:23:20 654  	  END;
16:23:20 655  	END IF;
16:23:20 656  
16:23:20 657  	-- Set default financial instrument
16:23:20 658  	IF in_is_default = GLOBAL_CONSTANTS_V19.FALSE THEN
16:23:20 659  	  BEGIN
16:23:20 660  	    GET_DEF_FINANCIAL_INSTRUMENT(
16:23:20 661  	      in_group_id	     => var_group_id,
16:23:20 662  	      out_instrument_type_id => current_def_instrument_type_id,
16:23:20 663  	      out_instrument_id      => current_def_instrument_id
16:23:20 664  	    );
16:23:20 665  	  EXCEPTION
16:23:20 666  	    WHEN OTHERS THEN
16:23:20 667  	      EXCEPTION_MESSAGE := SQLERRM;
16:23:20 668  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
16:23:20 669  	  END;
16:23:20 670  	  IF current_def_instrument_type_id = GLOBAL_ENUMS_V19.INSTRUMENT_CREDIT_CARD
16:23:20 671  	    AND current_def_instrument_id = in_credit_card_id THEN
16:23:20 672  	    BEGIN
16:23:20 673  	      DEL_DEF_FINANCIAL_INSTRUMENT(
16:23:20 674  		in_group_id => var_group_id
16:23:20 675  	      );
16:23:20 676  	      EXCEPTION
16:23:20 677  		WHEN OTHERS THEN
16:23:20 678  		  EXCEPTION_MESSAGE := SQLERRM;
16:23:20 679  		  RAISE CAN_NOT_DEL_DEF_FINANCIAL;
16:23:20 680  	    END;
16:23:20 681  	  END IF;
16:23:20 682  	END IF;
16:23:20 683  
16:23:20 684  EXCEPTION
16:23:20 685  WHEN NO_DATA_FOUND THEN
16:23:20 686  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 687  	  SPROC_NAME, 'No such credit card');
16:23:20 688  WHEN CAN_NOT_SET_DEF_FINANCIAL THEN
16:23:20 689  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 690  	  SPROC_NAME, 'Could not set default financial instrument for account', EXCEPTION_MESSAGE);
16:23:20 691  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
16:23:20 692  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 693  	  SPROC_NAME, 'Could not get default financial instrument for account', EXCEPTION_MESSAGE);
16:23:20 694  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
16:23:20 695  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 696  	  SPROC_NAME, 'Could not delete information about default financial instrument', EXCEPTION_MESSAGE);
16:23:20 697  WHEN OTHERS THEN
16:23:20 698  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 699  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 700  END UPDATE_CREDIT_CARD;
