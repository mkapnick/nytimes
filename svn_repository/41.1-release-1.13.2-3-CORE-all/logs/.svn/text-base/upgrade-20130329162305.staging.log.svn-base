16:23:19 1657  PROCEDURE GET_OFF_CHAINS_SAME_PRODUCTS (
16:23:19 1658  /*
16:23:19 1659  Throws exceptions (codes):
16:23:19 1660  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
16:23:19 1661  */
16:23:19 1662  	 in_offer_chain_1 IN NUMBER,
16:23:19 1663  	 in_offer_chain_2 IN NUMBER,
16:23:19 1664  	 out_result_set   OUT SYS_REFCURSOR
16:23:19 1665  ) AS
16:23:19 1666  BEGIN
16:23:19 1667  
16:23:19 1668  	 OPEN out_result_set FOR
16:23:19 1669  	 SELECT
16:23:19 1670  	   PRODUCT_ID_IN_OFFER_CH_1 AS "PRODUCT_ID",
16:23:19 1671  	   COUNT_1 + COUNT_2	    AS "COUNT"
16:23:19 1672  	 FROM
16:23:19 1673  	   (
16:23:19 1674  	     SELECT
16:23:19 1675  	       PRODUCT_OFFERING.PRODUCT_ID as "PRODUCT_ID_IN_OFFER_CH_1",
16:23:19 1676  	       COUNT(*) 		   as "COUNT_1"
