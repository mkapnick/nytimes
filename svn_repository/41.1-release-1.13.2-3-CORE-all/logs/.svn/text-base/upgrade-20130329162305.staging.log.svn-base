16:23:20 162  		      ,subscription.sct_id, license.start_date, license.end_date
16:23:20 163  		      ,account.group_id account_regi_id
16:23:20 164  		from	 license
16:23:20 165  		    join subscription ON subscription.id = license.subscription_id
16:23:20 166  		    join account      ON account.id	 = subscription.account_id
16:23:20 167  		union all
16:23:20 168  		select gc.purchase_invoice_id invoice_id, gc.offer_chain_id, gc.purchase_date
16:23:20 169  		      ,null sct_id, gc.purchase_date start_date, gc.expiration_date end_date
16:23:20 170  		      ,gc.purchaser_group_id account_regi_id
16:23:20 171  		from	 gift_certificate gc
16:23:20 172  		where
16:23:20 173  		      TRUNC(gc.create_date, 'HH') between TRUNC(in_lower_date_bound,'HH') AND TRUNC(in_upper_date_bound,'HH')
16:23:20 174  
16:23:20 175  	  ) gclicense
16:23:20 176  						on gclicense.invoice_id = invoice.id
16:23:20 177  	  JOIN offer_chain			ON offer_chain.id      = gclicense.offer_chain_id
16:23:20 178  	  JOIN charge				ON invoice.id	       = charge.invoice_id and charge.charge_status_id = 2
16:23:20 179  	  JOIN transaction			ON transaction.id      = charge.transaction_id and transaction.transaction_status_id = 2
16:23:20 180  	  JOIN transaction_attempt		ON transaction.id	= transaction_attempt.transaction_id AND transaction_attempt.transaction_attempt_status_id = 2
16:23:20 181  	  LEFT OUTER JOIN credit_card		ON charge.instrument_id = credit_card.id AND charge.instrument_type_id = 1
16:23:20 182  	  LEFT OUTER JOIN gift_certificate	ON charge.instrument_id = gift_certificate.id AND charge.instrument_type_id = 3
16:23:20 183  	  LEFT OUTER JOIN offer_chain_meta_data ON offer_chain.id	= offer_chain_meta_data.offer_chain_id
16:23:20 184  	  join liq				on line_item.id 	= liq.id
16:23:20 185  	    ;
