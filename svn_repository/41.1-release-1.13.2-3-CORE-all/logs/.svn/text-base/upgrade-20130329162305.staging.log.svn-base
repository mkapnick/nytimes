16:23:20 2776  	     PP.ACCOUNT_ID
16:23:20 2777  	   FROM
16:23:20 2778  	     PAYPAL PP
16:23:20 2779  	   WHERE
16:23:20 2780  	     (
16:23:20 2781  	       PP.ID = in_old_fin_instrument_id
16:23:20 2782  	       AND in_old_fin_instrument_type = GLOBAL_ENUMS_V19.INSTRUMENT_PAYPAL
16:23:20 2783  	     )
16:23:20 2784  	     OR
16:23:20 2785  	     (
16:23:20 2786  	       PP.ID = in_new_fin_instrument_id
16:23:20 2787  	       AND in_new_fin_instrument_type = GLOBAL_ENUMS_V19.INSTRUMENT_PAYPAL
16:23:20 2788  	     )
16:23:20 2789  	 )
16:23:20 2790  	 WHERE
16:23:20 2791  	   account_id IS NOT NULL;
16:23:20 2792  
16:23:20 2793  	 IF (var_accounts_count > 1) THEN
16:23:20 2794  	   -- Throw exception: different owners of instruments
16:23:20 2795  	   RAISE DIFFERENT_OWNERS;
16:23:20 2796  	 END IF;
16:23:20 2797  
16:23:20 2798  	 FOR f_sub IN (
16:23:20 2799  	   select
16:23:20 2800  	     s.id
16:23:20 2801  	   FROM
16:23:20 2802  	     subscription s
