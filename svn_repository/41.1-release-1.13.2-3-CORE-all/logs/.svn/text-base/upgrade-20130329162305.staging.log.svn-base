16:23:20 845  PROCEDURE GET_GRACE_LICE_FOR_FINAL_TRANS (
16:23:20 846  	in_days_before_close	 IN NUMBER,
16:23:20 847  	in_num_licenses_to_fetch IN NUMBER,
16:23:20 848  	out_result_set		 OUT SYS_REFCURSOR
16:23:20 849  ) AS
16:23:20 850  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_GRACE_LICE_FOR_FINAL_TRANS';
16:23:20 851  BEGIN
16:23:20 852  	OPEN out_result_set FOR
16:23:20 853  	SELECT
16:23:20 854  	    *
16:23:20 855  	FROM
16:23:20 856  	    (
16:23:20 857  		SELECT
16:23:20 858  		    l.id
16:23:20 859  		FROM
16:23:20 860  		    license l
16:23:20 861  		JOIN
16:23:20 862  		    invoice i
16:23:20 863  		ON
16:23:20 864  		    l.invoice_id = i.id
16:23:20 865  		WHERE
16:23:20 866  		    i.invoice_status_id = GLOBAL_STATUSES_V19.INVOICE_OPEN
16:23:20 867  		AND l.license_status_id = GLOBAL_STATUSES_V19.LICENSE_IN_GRACE_PERIOD
16:23:20 868  		AND SYSDATE + in_days_before_close >= l.grace_end_date
16:23:20 869  		AND NOT EXISTS
16:23:20 870  		    (
16:23:20 871  			SELECT
16:23:20 872  			    1
16:23:20 873  			FROM
16:23:20 874  			    charge c
16:23:20 875  			WHERE
16:23:20 876  			    c.invoice_id = i.id
16:23:20 877  			AND c.charge_status_id = GLOBAL_STATUSES_V19.CHARGE_OPENED)
16:23:20 878  		AND rownum <= in_num_licenses_to_fetch * 10
16:23:20 879  		ORDER BY
16:23:20 880  		    dbms_random.value)
16:23:20 881  	WHERE
16:23:20 882  	    rownum <= in_num_licenses_to_fetch;
16:23:20 883  END GET_GRACE_LICE_FOR_FINAL_TRANS;
