16:23:20 1606  
16:23:20 1607  PROCEDURE SET_IS_TAX_CALCULATION_NEEDED (
16:23:20 1608  	 in_invoice_id		      IN NUMBER,
16:23:20 1609  	 in_updated_by		      IN VARCHAR2,
16:23:20 1610  	 in_is_tax_calculation_needed IN NUMBER
16:23:20 1611  ) AS
16:23:20 1612  SPROC_NAME CONSTANT VARCHAR2(29) := 'SET_IS_TAX_CALCULATION_NEEDED';
16:23:20 1613  -- VARIABLES
16:23:20 1614  temp_invoice_id	      NUMBER;
16:23:20 1615  -- EXCEPTIONS
16:23:20 1616  BAD_INVOICE_ID	      EXCEPTION;
16:23:20 1617  CAN_NOT_UPDATE_INVOCIE EXCEPTION;
16:23:20 1618  EXCEPTION_MESSAGE      VARCHAR2(1024);
16:23:20 1619  BEGIN
16:23:20 1620  
16:23:20 1621  	 BEGIN
