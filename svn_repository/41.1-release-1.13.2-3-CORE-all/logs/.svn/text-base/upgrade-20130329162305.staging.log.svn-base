16:23:20 442  
16:23:20 443  PROCEDURE GET_ACCOUNT_GIFT_CERTIFICATES (
16:23:20 444  /*
16:23:20 445  IN:
16:23:20 446  instr_status:
16:23:20 447  GLOBAL_CONSTANTS_V19.TRUE - get active instruments only (default)
16:23:20 448  GLOBAL_CONSTANTS_V19.FALSE - get inactive instruments only
16:23:20 449  
16:23:20 450  Throws exceptions:
16:23:20 451  APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 452  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
16:23:20 453  */
16:23:20 454  	in_group_id	  IN NUMBER,
16:23:20 455  	out_result_gc_set OUT SYS_REFCURSOR,
16:23:20 456  	in_instr_status   IN NUMBER DEFAULT GLOBAL_CONSTANTS_V19.TRUE
16:23:20 457  ) AS
16:23:20 458  SPROC_NAME     CONSTANT VARCHAR2(29) := 'GET_ACCOUNT_GIFT_CERTIFICATES';
16:23:20 459  var_account_id NUMBER;
16:23:20 460  
16:23:20 461  -- Exceptions
16:23:20 462  WRONG_INSTR_EXCEPTION	 EXCEPTION;
16:23:20 463  BEGIN
16:23:20 464  
16:23:20 465  	-- Get account id
16:23:20 466  	SELECT
16:23:20 467  	  ACCOUNT.ID INTO var_account_id
16:23:20 468  	FROM
16:23:20 469  	  ACCOUNT
16:23:20 470  	WHERE
16:23:20 471  	  ACCOUNT.GROUP_ID = in_group_id;
16:23:20 472  
16:23:20 473  	-- Check that incoming data is correct
16:23:20 474  	IF in_instr_status != GLOBAL_CONSTANTS_V19.TRUE AND in_instr_status != GLOBAL_CONSTANTS_V19.FALSE THEN
16:23:20 475  	  RAISE WRONG_INSTR_EXCEPTION;
16:23:20 476  	END IF;
16:23:20 477  
16:23:20 478  	OPEN out_result_gc_set FOR
16:23:20 479  	SELECT
16:23:20 480  	  GIFT_CERTIFICATE.ID,
16:23:20 481  	  GIFT_CERTIFICATE.PURCHASER_GROUP_ID,
16:23:20 482  	  GIFT_CERTIFICATE.PURCHASE_INVOICE_ID,
16:23:20 483  	  GIFT_CERTIFICATE.PURCHASE_DATE,
16:23:20 484  	  GIFT_CERTIFICATE.OFFER_CHAIN_ID,
16:23:20 485  	  GIFT_CERTIFICATE.EXPIRATION_DATE,
16:23:20 486  	  GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID,
16:23:20 487  	  GIFT_CERTIFICATE.CODE,
16:23:20 488  	  GIFT_CERTIFICATE.CREATE_DATE,
16:23:20 489  	  GIFT_CERTIFICATE.CREATED_BY,
16:23:20 490  	  GIFT_CERTIFICATE.UPDATE_DATE,
16:23:20 491  	  GIFT_CERTIFICATE.UPDATED_BY,
16:23:20 492  	  GIFT_CERTIFICATE.RECIPIENT_NAME,
16:23:20 493  	  GIFT_CERTIFICATE.RECIPIENT_EMAIL,
16:23:20 494  	  GIFT_CERTIFICATE.SENDER_NAME,
16:23:20 495  	  GIFT_CERTIFICATE.SENDER_EMAIL,
16:23:20 496  	  GIFT_CERTIFICATE.REDEEMER_GROUP_ID,
16:23:20 497  	  GIFT_CERTIFICATE.REDEMPTION_DATE,
16:23:20 498  	  GIFT_CERTIFICATE.FINALIZED_INVOICE_ID,
16:23:20 499  	  GIFT_CERTIFICATE.GIFT_MESSAGE
16:23:20 500  	FROM
16:23:20 501  	  GIFT_CERTIFICATE
16:23:20 502  	WHERE
16:23:20 503  	  GIFT_CERTIFICATE.PURCHASER_GROUP_ID = in_group_id
16:23:20 504  	  AND (
16:23:20 505  		( in_instr_status = GLOBAL_CONSTANTS_V19.TRUE AND
16:23:20 506  		  (
16:23:20 507  		    GIFT_CERTIFICATE.EXPIRATION_DATE >= current_date
16:23:20 508  		    AND GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID = GLOBAL_STATUSES_V19.GIFT_CERTIFICATE_ACTIVE
16:23:20 509  		  )
16:23:20 510  		)
16:23:20 511  		OR
16:23:20 512  		(
16:23:20 513  		  in_instr_status = GLOBAL_CONSTANTS_V19.FALSE AND
16:23:20 514  		  (
16:23:20 515  		    GIFT_CERTIFICATE.EXPIRATION_DATE < current_date
16:23:20 516  		    OR GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID != GLOBAL_STATUSES_V19.GIFT_CERTIFICATE_ACTIVE
16:23:20 517  		  )
16:23:20 518  		)
16:23:20 519  	      );
16:23:20 520  
16:23:20 521  EXCEPTION
16:23:20 522  WHEN NO_DATA_FOUND THEN
16:23:20 523  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 524  	  SPROC_NAME, 'Can not find account with given group id');
16:23:20 525  WHEN WRONG_INSTR_EXCEPTION THEN
16:23:20 526  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
16:23:20 527  	  SPROC_NAME, 'Wrong gift certificate type');
16:23:20 528  WHEN OTHERS THEN
16:23:20 529  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 530  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 531  END GET_ACCOUNT_GIFT_CERTIFICATES;
