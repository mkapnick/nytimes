16:23:20 409  	    OFFER.CREATE_DATE,
16:23:20 410  	    OFFER.UPDATED_BY,
16:23:20 411  	    OFFER.UPDATE_DATE,
16:23:20 412  	    var_offer_num_recurrences as "RECURRENCE_NUMBER",
16:23:20 413  	    var_offer_index as "OFFER_INDEX"
16:23:20 414  	  FROM
16:23:20 415  	    OFFER_OFFER_CHAIN
16:23:20 416  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
16:23:20 417  	  WHERE ROWNUM = 0;
16:23:20 418  	ELSE
