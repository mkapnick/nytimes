16:23:20 1623  	     i.id into temp_invoice_id
16:23:20 1624  	   FROM
16:23:20 1625  	     invoice i
16:23:20 1626  	   WHERE
16:23:20 1627  	     i.id = in_invoice_id;
16:23:20 1628  	   EXCEPTION
16:23:20 1629  	     WHEN NO_DATA_FOUND THEN
16:23:20 1630  	       RAISE BAD_INVOICE_ID;
16:23:20 1631  	 END;
16:23:20 1632  
16:23:20 1633  	 BEGIN
16:23:20 1634  	   PROCS_INVOICE_CRU_V19.UPDATE_INVOICE(
16:23:20 1635  	     in_invoice_id => in_invoice_id,
16:23:20 1636  	     in_updated_by => in_updated_by,
16:23:20 1637  	     in_is_tax_calculation_needed => in_is_tax_calculation_needed
16:23:20 1638  	   );
16:23:20 1639  	   EXCEPTION
16:23:20 1640  	     WHEN OTHERS THEN
16:23:20 1641  	       EXCEPTION_MESSAGE := SQLERRM;
16:23:20 1642  	       RAISE CAN_NOT_UPDATE_INVOCIE;
16:23:20 1643  	 END;
16:23:20 1644  
16:23:20 1645  EXCEPTION
16:23:20 1646  WHEN BAD_INVOICE_ID THEN
