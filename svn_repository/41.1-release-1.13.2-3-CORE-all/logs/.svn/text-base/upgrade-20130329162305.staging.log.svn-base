16:23:20 1453  PROCEDURE GET_GROUPS_ID_BY_INVOICE_ID (
16:23:20 1454  /*
16:23:20 1455  This procedure is using for LOCKING only
16:23:20 1456  
16:23:20 1457  Throws exceptions:
16:23:20 1458  APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 1459  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
16:23:20 1460  */
16:23:20 1461  	 in_invoice_id IN NUMBER,
16:23:20 1462  	 out_group_ids OUT SYS_REFCURSOR
16:23:20 1463  ) AS
16:23:20 1464  SPROC_NAME CONSTANT VARCHAR2(27) := 'GET_GROUPS_ID_BY_INVOICE_ID';
16:23:20 1465  -- VARIABLES
16:23:20 1466  temp_invoice_id		 NUMBER;
16:23:20 1467  var_subscrib_group_id	 NUMBER;
16:23:20 1468  var_gc_purchaser_group_id NUMBER;
16:23:20 1469  var_gc_redeemer_group_id  NUMBER;
16:23:20 1470  -- EXCEPTIONS
16:23:20 1471  BAD_INVOICE_ID	    EXCEPTION;
16:23:20 1472  CAN_NOT_FIND_ACCOUNT EXCEPTION;
16:23:20 1473  BEGIN
16:23:20 1474  
16:23:20 1475  	 BEGIN
16:23:20 1476  	   SELECT
16:23:20 1477  	     INVOICE.ID into temp_invoice_id
16:23:20 1478  	   FROM
16:23:20 1479  	     INVOICE
16:23:20 1480  	   WHERE
16:23:20 1481  	     INVOICE.ID = in_invoice_id;
16:23:20 1482  	   EXCEPTION
16:23:20 1483  	     WHEN NO_DATA_FOUND THEN
16:23:20 1484  	       RAISE BAD_INVOICE_ID;
16:23:20 1485  	 END;
16:23:20 1486  
16:23:20 1487  	 BEGIN
16:23:20 1488  	   SELECT DISTINCT
16:23:20 1489  	     ACCOUNT.GROUP_ID into var_subscrib_group_id
16:23:20 1490  	   FROM
16:23:20 1491  	     ACCOUNT
16:23:20 1492  	     INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.ACCOUNT_ID = ACCOUNT.ID
16:23:20 1493  	     INNER JOIN LICENSE ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
16:23:20 1494  	   WHERE
16:23:20 1495  	     LICENSE.INVOICE_ID = in_invoice_id;
16:23:20 1496  	   EXCEPTION
16:23:20 1497  	     WHEN NO_DATA_FOUND THEN
16:23:20 1498  	       var_subscrib_group_id := NULL;
16:23:20 1499  	 END;
16:23:20 1500  
16:23:20 1501  	 IF var_subscrib_group_id IS NULL THEN
16:23:20 1502  	   BEGIN
16:23:20 1503  	     SELECT
16:23:20 1504  	       GIFT_CERTIFICATE.PURCHASER_GROUP_ID,
16:23:20 1505  	       GIFT_CERTIFICATE.REDEEMER_GROUP_ID
16:23:20 1506  	       into
16:23:20 1507  	       var_gc_purchaser_group_id,
16:23:20 1508  	       var_gc_redeemer_group_id
16:23:20 1509  	     FROM
16:23:20 1510  	       GIFT_CERTIFICATE
16:23:20 1511  	     WHERE
16:23:20 1512  	       GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = in_invoice_id
16:23:20 1513  	       OR GIFT_CERTIFICATE.FINALIZED_INVOICE_ID = in_invoice_id;
16:23:20 1514  	     EXCEPTION
16:23:20 1515  	       WHEN NO_DATA_FOUND THEN
16:23:20 1516  		 var_gc_purchaser_group_id := NULL;
16:23:20 1517  		 var_gc_redeemer_group_id  := NULL;
16:23:20 1518  	   END;
16:23:20 1519  	 END IF;
16:23:20 1520  
16:23:20 1521  	 IF var_subscrib_group_id IS NULL
16:23:20 1522  	   AND var_gc_purchaser_group_id IS NULL
16:23:20 1523  	   AND var_gc_redeemer_group_id IS NULL THEN
16:23:20 1524  	     RAISE CAN_NOT_FIND_ACCOUNT;
16:23:20 1525  	 END IF;
16:23:20 1526  
16:23:20 1527  	 OPEN out_group_ids FOR
16:23:20 1528  	 SELECT GROUP_ID FROM (
16:23:20 1529  	   SELECT
16:23:20 1530  	     var_subscrib_group_id as "GROUP_ID"
16:23:20 1531  	   FROM
16:23:20 1532  	     DUAL
16:23:20 1533  	   UNION
16:23:20 1534  	   SELECT
16:23:20 1535  	     var_gc_purchaser_group_id as "GROUP_ID"
16:23:20 1536  	   FROM
16:23:20 1537  	     DUAL
16:23:20 1538  	   UNION
16:23:20 1539  	   SELECT
16:23:20 1540  	     var_gc_redeemer_group_id as "GROUP_ID"
16:23:20 1541  	   FROM
16:23:20 1542  	     DUAL
16:23:20 1543  	 )
16:23:20 1544  	 WHERE
16:23:20 1545  	   GROUP_ID IS NOT NULL;
16:23:20 1546  
16:23:20 1547  EXCEPTION
16:23:20 1548  WHEN BAD_INVOICE_ID THEN
16:23:20 1549  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 1550  	   SPROC_NAME, 'No such invoice');
16:23:20 1551  WHEN CAN_NOT_FIND_ACCOUNT THEN
16:23:20 1552  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 1553  	   SPROC_NAME, 'Could not find account for given invoice');
16:23:20 1554  WHEN OTHERS THEN
16:23:20 1555  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 1556  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 1557  END GET_GROUPS_ID_BY_INVOICE_ID;
