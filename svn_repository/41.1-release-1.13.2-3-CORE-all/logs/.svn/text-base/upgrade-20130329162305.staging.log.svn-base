16:23:20 1121  temp_subscription_id NUMBER;
16:23:20 1122  var_offer_duration   VARCHAR2(30);
16:23:20 1123  
16:23:20 1124  -- EXCEPTIONS
16:23:20 1125  BAD_SUBSCRIPTION_ID EXCEPTION;
16:23:20 1126  BEGIN
16:23:20 1127  
16:23:20 1128  	 BEGIN
