16:23:20 2731  	   END IF;
16:23:20 2732  	 ELSIF in_old_fin_instrument_type = GLOBAL_ENUMS_V19.INSTRUMENT_PAYPAL THEN
16:23:20 2733  	   IF PROCS_FIN_INSTRUMENTS_V19.IS_PAYPAL_EXISTS(in_old_fin_instrument_id) = GLOBAL_CONSTANTS_V19.FALSE THEN
16:23:20 2734  	     -- throw exception: bad old paypal
16:23:20 2735  	     RAISE BAD_OLD_PP;
16:23:20 2736  	   END IF;
16:23:20 2737  	 ELSE
