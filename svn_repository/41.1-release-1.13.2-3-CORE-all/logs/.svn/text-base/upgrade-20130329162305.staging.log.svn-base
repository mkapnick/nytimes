16:23:19 439  /******************************************************************************/
16:23:19 440  
16:23:19 441  FUNCTION CALCULATE_OFFER_CHAIN_END_DATE (
16:23:19 442  /*
16:23:19 443  Throws exceptions (codes):
16:23:19 444  APP_EXCEPTION_CODES_V19.NOT_FOUND
16:23:19 445  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
16:23:19 446  */
16:23:19 447  	in_offer_chain_id	  IN NUMBER,
16:23:19 448  	in_offer_chain_start_date IN DATE
16:23:19 449  ) RETURN DATE AS
16:23:19 450  -- VARIABLES
16:23:19 451  SPROC_NAME	     CONSTANT VARCHAR2(30) := 'CALCULATE_OFFER_CHAIN_END_DATE';
16:23:19 452  temp_offer_chain_id    NUMBER;
16:23:19 453  var_offer_chain_length NUMBER;
16:23:19 454  var_offer_duration     VARCHAR2(30);
16:23:19 455  var_offer_recurrences  NUMBER;
16:23:19 456  var_end_date	     DATE;
16:23:19 457  
16:23:19 458  var_offer_ym_interval INTERVAL YEAR TO MONTH;
16:23:19 459  var_offer_ds_interval INTERVAL DAY(3) TO SECOND;
16:23:19 460  var_offer_years	    NUMBER;
16:23:19 461  var_offer_months	    NUMBER;
16:23:19 462  var_offer_days	    NUMBER;
16:23:19 463  var_infinity_offers_count NUMBER;
16:23:19 464  
16:23:19 465  var_offers_set SYS_REFCURSOR;
16:23:19 466  
16:23:19 467  -- EXCEPTIONS
16:23:19 468  BAD_OFFER_CHAIN_ID EXCEPTION;
16:23:19 469  BEGIN
16:23:19 470  
16:23:19 471  	var_end_date := in_offer_chain_start_date;
16:23:19 472  
16:23:19 473  	-- Check that offer chain exists
16:23:19 474  	BEGIN
16:23:19 475  	  SELECT
16:23:19 476  	    OFFER_CHAIN.ID into temp_offer_chain_id
16:23:19 477  	  FROM
16:23:19 478  	    OFFER_CHAIN
16:23:19 479  	  WHERE
16:23:19 480  	    OFFER_CHAIN.ID = in_offer_chain_id;
16:23:19 481  	  EXCEPTION
16:23:19 482  	    WHEN NO_DATA_FOUND
16:23:19 483  	      THEN RAISE BAD_OFFER_CHAIN_ID;
16:23:19 484  	END;
16:23:19 485  
16:23:19 486  	SELECT
16:23:19 487  	  COUNT(*) into var_infinity_offers_count
16:23:19 488  	FROM
16:23:19 489  	  OFFER_OFFER_CHAIN
16:23:19 490  	WHERE
16:23:19 491  	  OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
16:23:19 492  	  AND OFFER_OFFER_CHAIN.NUM_RECURRENCES = GLOBAL_ENUMS_V19.OFFER_REC_INFINITY;
16:23:19 493  
16:23:19 494  	IF var_infinity_offers_count > 0 THEN
16:23:19 495  	  -- Offer chain contains offers with infinity num of recurrences
16:23:19 496  	  RETURN NULL;
16:23:19 497  	END IF;
16:23:19 498  
16:23:19 499  	-- Select offers durations
16:23:19 500  	OPEN var_offers_set FOR
16:23:19 501  	SELECT
16:23:19 502  	  OFFER.ENTITLEMENT_DURATION,
16:23:19 503  	  OFFER_OFFER_CHAIN.NUM_RECURRENCES
16:23:19 504  	FROM
16:23:19 505  	  OFFER_OFFER_CHAIN
16:23:19 506  	  INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
16:23:19 507  	WHERE
16:23:19 508  	  OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id;
16:23:19 509  
16:23:19 510  	-- Calculate sum of offers durations
16:23:19 511  	LOOP
16:23:19 512  	  FETCH var_offers_set INTO var_offer_duration, var_offer_recurrences;
16:23:19 513  	  EXIT WHEN var_offers_set%NOTFOUND;
16:23:19 514  	  PROCS_COMMON_V19.ISO8601DURATION_TO_INTERVALS(var_offer_duration, var_offer_years, var_offer_months, var_offer_days);
16:23:19 515  	  var_offer_ym_interval := var_offer_years||'-'||var_offer_months;
16:23:19 516  	  var_offer_ds_interval := var_offer_days||' 0:0:0';
16:23:19 517  	  var_end_date := var_end_date + ( var_offer_ym_interval * ( var_offer_recurrences + 1) ) + ( var_offer_ds_interval * ( var_offer_recurrences + 1) );
16:23:19 518  	END LOOP;
16:23:19 519  
16:23:19 520  	RETURN var_end_date;
16:23:19 521  
16:23:19 522  EXCEPTION
16:23:19 523  WHEN BAD_OFFER_CHAIN_ID THEN
16:23:19 524  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:19 525  	  SPROC_NAME, 'No such offer chain');
16:23:19 526  WHEN OTHERS THEN
16:23:19 527  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:19 528  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:23:19 529  END CALCULATE_OFFER_CHAIN_END_DATE;
16:23:19 530  
16:23:19 531  /******************************************************************************/
16:23:19 532  
16:23:19 533  FUNCTION CALCULATE_OFFER_AMOUNT (
16:23:19 534  	in_offer_id IN NUMBER
16:23:19 535  ) RETURN NUMBER AS
16:23:19 536  -- VARIABLES
16:23:19 537  SPROC_NAME    CONSTANT VARCHAR2(22) := 'CALCULATE_OFFER_AMOUNT';
16:23:19 538  temp_offer_id NUMBER;
16:23:19 539  
16:23:19 540  var_product_offering_set	    SYS_REFCURSOR;
16:23:19 541  var_product_offering_id	    NUMBER;
16:23:19 542  var_product_offering_price    NUMBER(10,6);
16:23:19 543  var_product_offering_t_amount NUMBER(10,6);
16:23:19 544  var_product_offering_quantity NUMBER;
16:23:19 545  
16:23:19 546  var_total_amount NUMBER(10,6);
16:23:19 547  var_final_amount NUMBER(10,2);
16:23:19 548  
16:23:19 549  var_percent_discount NUMBER(10,2);
16:23:19 550  var_fixed_discount NUMBER(10,6);
16:23:19 551  
16:23:19 552  -- EXCEPTIONS
16:23:19 553  BAD_OFFER_ID EXCEPTION;
16:23:19 554  BEGIN
16:23:19 555  
16:23:19 556  	BEGIN
16:23:19 557  	  SELECT
16:23:19 558  	    OFFER.ID into temp_offer_id
16:23:19 559  	  FROM
16:23:19 560  	    OFFER
16:23:19 561  	  WHERE
16:23:19 562  	    OFFER.ID = in_offer_id;
16:23:19 563  	  EXCEPTION
16:23:19 564  	    WHEN NO_DATA_FOUND THEN
16:23:19 565  	      RAISE BAD_OFFER_ID;
16:23:19 566  	END;
16:23:19 567  
16:23:19 568  	OPEN var_product_offering_set FOR
16:23:19 569  	SELECT
16:23:19 570  	  OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID,
16:23:19 571  	  PRODUCT_OFFERING.UNIT_PRICE,
16:23:19 572  	  PRODUCT_OFFERING.QUANTITY
16:23:19 573  	FROM
16:23:19 574  	  OFFER_PRODUCT_OFFERING
16:23:19 575  	  INNER JOIN PRODUCT_OFFERING ON OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = PRODUCT_OFFERING.ID
16:23:19 576  	WHERE
16:23:19 577  	  OFFER_PRODUCT_OFFERING.OFFER_ID = in_offer_id;
16:23:19 578  
16:23:19 579  	var_total_amount := 0;
16:23:19 580  
16:23:19 581  	LOOP
16:23:19 582  	  FETCH var_product_offering_set into
16:23:19 583  	    var_product_offering_id,
16:23:19 584  	    var_product_offering_price,
16:23:19 585  	    var_product_offering_quantity;
16:23:19 586  	  EXIT WHEN var_product_offering_set%NOTFOUND;
16:23:19 587  
16:23:19 588  	  SELECT
16:23:19 589  	    SUM(DISCOUNT.FIXED_AMOUNT) into var_fixed_discount
16:23:19 590  	  FROM
16:23:19 591  	    DISCOUNT_PRODUCT_OFFERING
16:23:19 592  	    INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
16:23:19 593  	  WHERE
16:23:19 594  	    DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
16:23:19 595  	    AND DISCOUNT.FIXED_AMOUNT IS NOT NULL;
16:23:19 596  
16:23:19 597  	  SELECT
16:23:19 598  	    SUM(DISCOUNT.PERCENT_AMOUNT) into var_percent_discount
16:23:19 599  	  FROM
16:23:19 600  	    DISCOUNT_PRODUCT_OFFERING
16:23:19 601  	    INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
16:23:19 602  	  WHERE
16:23:19 603  	    DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
16:23:19 604  	    AND DISCOUNT.PERCENT_AMOUNT IS NOT NULL;
16:23:19 605  
16:23:19 606  	  var_product_offering_t_amount := var_product_offering_price * var_product_offering_quantity;
16:23:19 607  
16:23:19 608  	  IF var_percent_discount IS NOT NULL THEN
16:23:19 609  	    var_product_offering_t_amount := var_product_offering_t_amount - ( var_product_offering_t_amount * var_percent_discount / 100 );
16:23:19 610  	  END IF;
16:23:19 611  
16:23:19 612  	  IF var_fixed_discount IS NOT NULL THEN
16:23:19 613  	    var_product_offering_t_amount := var_product_offering_t_amount - var_fixed_discount;
16:23:19 614  	  END IF;
16:23:19 615  
16:23:19 616  	  var_total_amount := var_total_amount + var_product_offering_t_amount;
16:23:19 617  	END LOOP;
16:23:19 618  	var_final_amount := var_total_amount;
16:23:19 619  	RETURN var_final_amount;
16:23:19 620  
16:23:19 621  EXCEPTION
16:23:19 622  WHEN BAD_OFFER_ID THEN
16:23:19 623  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:19 624  	  SPROC_NAME, 'No such offer');
16:23:19 625  WHEN OTHERS THEN
16:23:19 626  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:19 627  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:23:19 628  END CALCULATE_OFFER_AMOUNT;
16:23:19 629  
16:23:19 630  /******************************************************************************/
16:23:19 631  
16:23:19 632  FUNCTION CALCULATE_OFFER_CHAIN_AMOUNT (
16:23:19 633  	in_offer_chain_id IN NUMBER
16:23:19 634  ) RETURN NUMBER AS
16:23:19 635  -- VARIABLES
16:23:19 636  SPROC_NAME	     CONSTANT VARCHAR2(28) := 'CALCULATE_OFFER_CHAIN_AMOUNT';
16:23:19 637  temp_offer_chain_id    NUMBER;
16:23:19 638  var_first_offer_id     NUMBER;
16:23:19 639  -- EXCEPTIONS
16:23:19 640  BAD_OFFER_CHAIN_ID      EXCEPTION;
16:23:19 641  CAN_NOT_GET_FIRST_OFFER EXCEPTION;
16:23:19 642  EXCEPTION_MESSAGE       VARCHAR2(1024);
16:23:19 643  BEGIN
16:23:19 644  
16:23:19 645  	BEGIN
16:23:19 646  	  SELECT
16:23:19 647  	    OFFER_CHAIN.ID into temp_offer_chain_id
16:23:19 648  	  FROM
16:23:19 649  	    OFFER_CHAIN
16:23:19 650  	  WHERE
16:23:19 651  	    OFFER_CHAIN.ID = in_offer_chain_id;
16:23:19 652  	  EXCEPTION
16:23:19 653  	    WHEN NO_DATA_FOUND THEN
16:23:19 654  	      RAISE BAD_OFFER_CHAIN_ID;
16:23:19 655  	END;
16:23:19 656  
16:23:19 657  	BEGIN
16:23:19 658  	  PROCS_OFFER_CHAIN_V19.GET_FIRST_OFFER(
16:23:19 659  	    in_offer_chain_id => in_offer_chain_id,
16:23:19 660  	    out_offer_id      => var_first_offer_id
16:23:19 661  	  );
16:23:19 662  	  EXCEPTION
16:23:19 663  	    WHEN OTHERS THEN
16:23:19 664  	      EXCEPTION_MESSAGE := SQLERRM;
16:23:19 665  	      RAISE CAN_NOT_GET_FIRST_OFFER;
16:23:19 666  	END;
16:23:19 667  
16:23:19 668  	RETURN CALCULATE_OFFER_AMOUNT(var_first_offer_id);
16:23:19 669  
16:23:19 670  EXCEPTION
16:23:19 671  WHEN BAD_OFFER_CHAIN_ID THEN
16:23:19 672  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:19 673  	  SPROC_NAME, 'No such offer chain');
16:23:19 674  WHEN CAN_NOT_GET_FIRST_OFFER THEN
16:23:19 675  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:19 676  	  SPROC_NAME, 'Could not find first offer', EXCEPTION_MESSAGE);
16:23:19 677  WHEN OTHERS THEN
16:23:19 678  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:19 679  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:23:19 680  END CALCULATE_OFFER_CHAIN_AMOUNT;
16:23:19 681  
16:23:19 682  /******************************************************************************/
16:23:19 683  
16:23:19 684  FUNCTION GET_FIRST_OFFER_INDEX (
16:23:19 685  	in_offer_chain_id IN NUMBER
16:23:19 686  ) RETURN NUMBER AS
16:23:19 687  -- VARIABLES
16:23:19 688  SPROC_NAME	    CONSTANT VARCHAR2(21) := 'GET_FIRST_OFFER_INDEX';
16:23:19 689  var_first_offer_index NUMBER;
16:23:19 690  BEGIN
16:23:19 691  
16:23:19 692  	SELECT
16:23:19 693  	  INDEX_VALUE into var_first_offer_index
16:23:19 694  	FROM (
16:23:19 695  	  SELECT
16:23:19 696  	    OFFER_OFFER_CHAIN.INDEX_VALUE
16:23:19 697  	  FROM
16:23:19 698  	    OFFER_OFFER_CHAIN
16:23:19 699  	  WHERE
16:23:19 700  	    OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
16:23:19 701  	  ORDER BY
16:23:19 702  	    OFFER_OFFER_CHAIN.INDEX_VALUE ASC
16:23:19 703  	)
16:23:19 704  	WHERE
16:23:19 705  	  ROWNUM <= 1;
16:23:19 706  
