16:23:20 760  ) AS
16:23:20 761  SPROC_NAME CONSTANT VARCHAR2(17) := 'GET_LICENSE_BY_ID';
16:23:20 762  -- VARIABLES
16:23:20 763  temp_license_id NUMBER;
16:23:20 764  -- EXCEPTIONS
16:23:20 765  BAD_LICENSE_ID EXCEPTION;
16:23:20 766  BEGIN
