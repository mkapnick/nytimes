16:23:19 1898  PROCEDURE CHECK_PRODUCT_ELIGIBILITY (
16:23:19 1899  	 in_group_id	   IN NUMBER,
16:23:19 1900  	 in_offer_chain_id IN NUMBER,
16:23:19 1901  	 out_is_eligible   OUT NUMBER,
16:23:19 1902  	 out_concurrent_subscription_id OUT NUMBER
16:23:19 1903  ) AS
16:23:19 1904  SPROC_NAME CONSTANT VARCHAR2(25) := 'CHECK_PRODUCT_ELIGIBILITY';
16:23:19 1905  -- Variables
16:23:19 1906  var_account_id  NUMBER;
16:23:19 1907  var_is_eligible NUMBER;
16:23:19 1908  var_is_gc       NUMBER;
16:23:19 1909  -- Exceptions
16:23:19 1910  BAD_GROUP_ID		   EXCEPTION;
16:23:19 1911  CAN_NOT_CHECK_SAME_PRODUCTS EXCEPTION;
16:23:19 1912  BAD_OC_ID		   EXCEPTION;
16:23:19 1913  EXCEPTION_MESSAGE	   VARCHAR(1024);
16:23:19 1914  BEGIN
16:23:19 1915  
16:23:19 1916  	 var_is_eligible := GLOBAL_CONSTANTS_V19.TRUE;
16:23:19 1917  
16:23:19 1918  	 out_concurrent_subscription_id := NULL;
16:23:19 1919  
16:23:19 1920  	 BEGIN
16:23:19 1921  	   SELECT
16:23:19 1922  	     OC.IS_GIFT_CERTIFICATE into var_is_gc
16:23:19 1923  	   FROM
16:23:19 1924  	     OFFER_CHAIN OC
16:23:19 1925  	   WHERE
16:23:19 1926  	     OC.ID = in_offer_chain_id;
16:23:19 1927  	   EXCEPTION
16:23:19 1928  	     WHEN NO_DATA_FOUND THEN
16:23:19 1929  	       RAISE BAD_OC_ID;
16:23:19 1930  	 END;
16:23:19 1931  
16:23:19 1932  	 -- only check eligibility if this is not a gift certificate
16:23:19 1933  	 IF (var_is_gc IS NULL OR var_is_gc != 1) THEN
16:23:19 1934  	   BEGIN
16:23:19 1935  	     SELECT
16:23:19 1936  	       A.ID into var_account_id
16:23:19 1937  	     FROM
16:23:19 1938  	       ACCOUNT A
16:23:19 1939  	     WHERE
16:23:19 1940  	       A.GROUP_ID = in_group_id;
16:23:19 1941  	     EXCEPTION
16:23:19 1942  	       WHEN NO_DATA_FOUND THEN
16:23:19 1943  		 RAISE BAD_GROUP_ID;
16:23:19 1944  	   END;
16:23:19 1945  
16:23:19 1946  
16:23:19 1947  	   FOR f_offer_chain IN (
16:23:19 1948  	       SELECT
16:23:19 1949  		 S.ID as SUBSCRIPTION_ID,
16:23:19 1950  		 S.OFFER_CHAIN_ID
16:23:19 1951  	       FROM
16:23:19 1952  		 SUBSCRIPTION S
16:23:19 1953  	       WHERE
16:23:19 1954  		 S.ACCOUNT_ID = var_account_id
16:23:19 1955  		 AND (S.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V19.SUBSCRIPTION_ACTIVE
16:23:19 1956  		      OR S.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V19.SUBSCRIPTION_IN_GRACE_PERIOD)
16:23:19 1957  	   )
16:23:19 1958  	   LOOP
16:23:19 1959  	     BEGIN
16:23:19 1960  	       IF (
16:23:19 1961  		 PROCS_OFFER_CHAIN_V19.CHECK_FOR_SAME_PRODUCTS(
16:23:19 1962  		   in_offer_chain_id,
16:23:19 1963  		   f_offer_chain.offer_chain_id,
16:23:19 1964  		   GLOBAL_CONSTANTS_V19.TRUE
16:23:19 1965  		 ) = GLOBAL_CONSTANTS_V19.TRUE
16:23:19 1966  	       ) THEN
16:23:19 1967  		 var_is_eligible := GLOBAL_CONSTANTS_V19.FALSE;
16:23:19 1968  		 out_concurrent_subscription_id := f_offer_chain.SUBSCRIPTION_ID;
16:23:19 1969  	       END IF;
16:23:19 1970  	       EXCEPTION
16:23:19 1971  		 WHEN OTHERS THEN
16:23:19 1972  		   EXCEPTION_MESSAGE := SQLERRM;
16:23:19 1973  		   RAISE CAN_NOT_CHECK_SAME_PRODUCTS;
16:23:19 1974  	     END;
16:23:19 1975  	   END LOOP;
16:23:19 1976  	 END IF;
16:23:19 1977  	 out_is_eligible := var_is_eligible;
16:23:19 1978  
16:23:19 1979  EXCEPTION
