16:23:20  39  PROCEDURE GET_UNREDEEMED_GCS (
16:23:20  40  	out_result_set		OUT SYS_REFCURSOR,
16:23:20  41  	in_hours_number 	IN NUMBER DEFAULT 14*24,
16:23:20  42  	in_num_rows		IN NUMBER DEFAULT 10000,
16:23:20  43  	in_process_name IN PROCESS_RETRY_THROTTLE.PROCESS_NAME%TYPE
16:23:20  44  ) AS
16:23:20  45  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_UNREDEEMED_GCS';
16:23:20  46  BEGIN
16:23:20  47  	OPEN out_result_set FOR
16:23:20  48  SELECT * FROM
16:23:20  49  (
16:23:20  50  	SELECT
16:23:20  51  	  gc.EXPIRATION_DATE,
16:23:20  52  	  ch.name,
16:23:20  53  	  ch.id offer_chain_id,
16:23:20  54  	  gc.sender_email,
16:23:20  55  	  gc.sender_name,
16:23:20  56  	  gc.recipient_email,
16:23:20  57  	  gc.recipient_name,
16:23:20  58  	  gc.purchase_date,
16:23:20  59  	  gc.redemption_date,
16:23:20  60  	  gc.purchaser_group_id,
16:23:20  61  	  gc.redeemer_group_id,
16:23:20  62  	  gc.code,
16:23:20  63  	  gc.gift_message,
16:23:20  64  	  gc.recipient_notify_date,
16:23:20  65  	  gc.id
16:23:20  66  	FROM
16:23:20  67  	  GIFT_CERTIFICATE gc,
16:23:20  68  	  OFFER_CHAIN ch
16:23:20  69  	WHERE
16:23:20  70  	  ch.id = gc.offer_chain_id
16:23:20  71  	  AND gc.RECIPIENT_NOTIFY_DATE is not null
16:23:20  72  	  AND gc.RECIPIENT_NOTIFY_DATE >= (sysdate - in_hours_number/24)
16:23:20  73  	  AND gc.RECIPIENT_NOTIFY_DATE < (sysdate - (in_hours_number-72)/24)
16:23:20  74  	  AND gc.redeemer_group_id is null
16:23:20  75  	  AND NOT EXISTS(
16:23:20  76  	    SELECT NULL
16:23:20  77  	    FROM PROCESS_RETRY_THROTTLE
16:23:20  78  	    WHERE PROCESS_NAME = in_process_name
16:23:20  79  	      AND GENERIC_ID = gc.id
16:23:20  80  	  ) AND EXISTS(
16:23:20  81  	    SELECT NULL
16:23:20  82  	    FROM
16:23:20  83  	      charge c,
16:23:20  84  	      transaction_attempt ta,
16:23:20  85  	      transaction t
16:23:20  86  	    WHERE
16:23:20  87  	      c.invoice_id = gc.purchase_invoice_id and
16:23:20  88  	      c.transaction_id = t.id and
16:23:20  89  	      t.id = ta.transaction_id and
16:23:20  90  	      ta.transaction_attempt_status_id = GLOBAL_STATUSES_V19.TRANS_ATTEMPT_SUCCESS
16:23:20  91  	  ) AND NOT EXISTS (
16:23:20  92  	    SELECT NULL
16:23:20  93  	    FROM
16:23:20  94  	      charge c,
16:23:20  95  	      transaction t
16:23:20  96  	    WHERE
16:23:20  97  	      c.invoice_id = gc.purchase_invoice_id and
16:23:20  98  	      c.transaction_id = t.id and
16:23:20  99  	      t.is_refund = GLOBAL_CONSTANTS_V19.TRUE
16:23:20 100  	  )
16:23:20 101  	  AND ROWNUM <= in_num_rows*10
16:23:20 102  	  ORDER BY dbms_random.value
16:23:20 103  ) WHERE
16:23:20 104  	  ROWNUM <= in_num_rows;
16:23:20 105  EXCEPTION
16:23:20 106  WHEN OTHERS THEN
16:23:20 107  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 108  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 109  END GET_UNREDEEMED_GCS;
