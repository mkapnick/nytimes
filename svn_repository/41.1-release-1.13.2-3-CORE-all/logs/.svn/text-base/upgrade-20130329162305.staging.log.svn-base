16:23:20 530  	  FROM
16:23:20 531  	    OFFER_OFFER_CHAIN
16:23:20 532  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
16:23:20 533  	  WHERE
16:23:20 534  	    OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
16:23:20 535  	    AND OFFER_OFFER_CHAIN.INDEX_VALUE = var_next_offer_index;
16:23:20 536  	END IF;
16:23:20 537  
16:23:20 538  EXCEPTION
