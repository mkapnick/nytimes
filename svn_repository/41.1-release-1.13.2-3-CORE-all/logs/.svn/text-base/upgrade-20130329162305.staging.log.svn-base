16:23:20 1145  	   INNER JOIN OFFER ON LICENSE.OFFER_ID = OFFER.ID
16:23:20 1146  	 WHERE
16:23:20 1147  	   --LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V19.LICENSE_PROCESSED
16:23:20 1148  	   --AND
