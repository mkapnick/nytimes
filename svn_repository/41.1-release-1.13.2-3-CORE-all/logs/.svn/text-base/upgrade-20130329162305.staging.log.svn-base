16:23:19 106  FUNCTION CREATE_NEW_TRANSFER_SET
16:23:19 107  	  (
16:23:19 108  	      in_set_maximum NUMBER)
16:23:19 109  	  RETURN core_owner.polling_sync.set_id%type
16:23:19 110  IS
16:23:19 111  	  pragma autonomous_transaction;
16:23:19 112  	  v_set_id core_owner.polling_sync.set_id%type;
16:23:19 113  BEGIN
16:23:19 114  	  SELECT
16:23:19 115  	      pollsync_setid_seq.nextval
16:23:19 116  	  INTO
16:23:19 117  	      v_set_id
16:23:19 118  	  FROM
16:23:19 119  	      dual;
16:23:19 120  	  update
16:23:19 121  	    core_owner.polling_sync ps
16:23:19 122  	  set
16:23:19 123  	    ps.set_id = v_set_id
16:23:19 124  	  where
16:23:19 125  	    ps.set_id IS NULL and
16:23:19 126  	    rownum <= in_set_maximum
16:23:19 127  	  ;
16:23:19 128  	  COMMIT;
16:23:19 129  	  RETURN v_set_id;
16:23:19 130  END;
16:23:19 131  --------------------------------------------------------------------------------
16:23:19 132  FUNCTION RETREIVE_TRANSFER_SET
16:23:19 133  	  (
16:23:19 134  	      in_set_id core_owner.polling_sync.set_id%type)
16:23:19 135  	  RETURN sys_refcursor
16:23:19 136  IS
16:23:19 137  	  v_refcursor sys_refcursor;
16:23:19 138  BEGIN
16:23:19 139  	  UPDATE
16:23:19 140  	      core_owner.polling_sync ps
16:23:19 141  	  SET
16:23:19 142  	      ps.last_send_date = sysdate,
16:23:19 143  	      ps.num_calls = ps.num_calls + 1
16:23:19 144  	  WHERE
16:23:19 145  	      ps.set_id = in_set_id ;
16:23:19 146  	  OPEN v_refcursor FOR
16:23:19 147  	  SELECT
16:23:19 148  	      ps.set_id,
16:23:19 149  	      ps.group_id,
16:23:19 150  	      ps.event_type,
16:23:19 151  	      ps.event_date
16:23:19 152  	  FROM
16:23:19 153  	      core_owner.polling_sync ps
16:23:19 154  	  WHERE
16:23:19 155  	      ps.set_id = in_set_id
16:23:19 156  	  AND ps.group_id IS NOT NULL ;
16:23:19 157  	  RETURN v_refcursor;
16:23:19 158  END;
16:23:19 159  --------------------------------------------------------------------------------
16:23:19 160  PROCEDURE GET_TRANSFER_SET
16:23:19 161  	  (
16:23:19 162  	      in_set_maximum NUMBER,
16:23:19 163  	      in_max_retries NUMBER,
16:23:19 164  	      out_refcursor OUT sys_refcursor)
16:23:19 165  IS
16:23:19 166  	  v_set_id core_owner.polling_sync.set_id%type;
16:23:19 167  BEGIN
16:23:19 168  	  /* Look for previously sent but unconfirmed sets and
16:23:19 169  	  send again until max_retries calls */
16:23:19 170  	  FOR x IN
16:23:19 171  	  (
16:23:19 172  	      SELECT
16:23:19 173  		  ps.set_id,
16:23:19 174  		  COUNT( *) set_size
16:23:19 175  	      FROM
16:23:19 176  		  core_owner.polling_sync ps
16:23:19 177  	      WHERE
16:23:19 178  		  ps.event_date > sysdate - 14
16:23:19 179  	      AND ps.confirm_date IS NULL
16:23:19 180  	      AND ps.last_send_date IS NOT NULL
16:23:19 181  	      AND ps.num_calls < in_max_retries
16:23:19 182  	      GROUP BY
16:23:19 183  		  ps.set_id
16:23:19 184  	      ORDER BY
16:23:19 185  		  ps.set_id
16:23:19 186  	  )
16:23:19 187  	  LOOP
16:23:19 188  	      out_refcursor := Retreive_Transfer_Set(x.set_id) ;
16:23:19 189  	      RETURN;
16:23:19 190  	  END LOOP;
16:23:19 191  	  v_set_id := Create_New_Transfer_Set(in_set_maximum) ;
16:23:19 192  	  out_refcursor := Retreive_Transfer_Set(v_set_id) ;
16:23:19 193  	  RETURN;
16:23:19 194  END;
16:23:19 195  --------------------------------------------------------------------------------
16:23:19 196  PROCEDURE CONFIRM_TRANSFER_SET
16:23:19 197  	  (
16:23:19 198  	      in_set_id core_owner.polling_sync.set_id%type)
16:23:19 199  IS
16:23:19 200  	  v_unconfirmable EXCEPTION;
16:23:19 201  BEGIN
16:23:19 202  	  UPDATE
16:23:19 203  	      core_owner.polling_sync ps
16:23:19 204  	  SET
16:23:19 205  	      ps.confirm_date = systimestamp
16:23:19 206  	  WHERE
16:23:19 207  	      ps.set_id = in_set_id
16:23:19 208  	  AND ps.confirm_date IS NULL ;
16:23:19 209  	  IF(sql%rowcount < 1) THEN
16:23:19 210  	      raise v_unconfirmable;
16:23:19 211  	  END IF;
16:23:19 212  END;
16:23:19 213  PROCEDURE SET_LAST_RUN(ts in timestamp)
16:23:19 214  IS
16:23:19 215  BEGIN
16:23:19 216  	  UPDATE POLLING_SYNC_LASTRUN
16:23:19 217  	  SET last_run = current_timestamp;
16:23:19 218  	  IF ( sql%rowcount = 0 )
16:23:19 219  	  THEN
16:23:19 220  	    INSERT INTO POLLING_SYNC_LASTRUN VALUES (ts);
16:23:19 221  	  END if;
16:23:19 222  	  COMMIT;
16:23:19 223  END;
16:23:19 224  PROCEDURE GET_LAST_RUN(ts out timestamp)
16:23:19 225  IS
16:23:19 226  BEGIN
16:23:19 227  	  SELECT LAST_RUN INTO ts
16:23:19 228  	  FROM POLLING_SYNC_LASTRUN
16:23:19 229  	  WHERE ROWNUM < 2;
16:23:19 230  EXCEPTION
16:23:19 231  	WHEN NO_DATA_FOUND
16:23:19 232  	THEN
16:23:19 233  	  ts := current_timestamp;
16:23:19 234  END;
16:23:19 235  END PROCS_POLLING_SYNC;
