16:23:20 143  	  , gclicense.account_regi_id
16:23:20 144  	  , product_offering.quantity			 purchase_quantity
16:23:20 145  	  , case when charge.charge_amount > 0 then charge.charge_amount else 0 end purchase_amount
16:23:20 146  	  , PROCS_REPORTING_1A.getDiscountAmount(line_item.id) discount_amount
16:23:20 147  	  , PROCS_REPORTING_1A.getRefundAmount(line_item.id) refund_amount
16:23:20 148  	  , decode(gclicense.sct_id,null,0, 1)		 is_refund_cancel
16:23:20 149  	  , 0						 purchase_tax_amount
16:23:20 150  	  , transaction_attempt.external_transaction_id  external_transaction_id
16:23:20 151  	  , invoice.id					 invoice_number
16:23:20 152  	  , NVL2(transaction.id, 1, 0)			 has_transaction
16:23:20 153  	  , NVL2(credit_card.id, 1, 0)			 is_cc_transaction
16:23:20 154  	  , NVL2(gift_certificate.id, 1, 0)		 is_gc_transaction
16:23:20 155  	  FROM
16:23:20 156  	       line_item
16:23:20 157  	  JOIN invoice				ON invoice.id	       = line_item.invoice_id
16:23:20 158  	  JOIN product_offering 		ON product_offering.id = line_item.product_offer_id
16:23:20 159  	  JOIN product				ON product.id	       = product_offering.product_id
16:23:20 160  	  join (
