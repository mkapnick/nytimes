16:23:20 2384  	       EXCEPTION_MESSAGE := SQLERRM;
16:23:20 2385  	       RAISE CAN_NOT_UPDATE_SUBSCRIPTION;
16:23:20 2386  	 END;
16:23:20 2387  
16:23:20 2388  	 -- Terminate grace period for licenses in grace [SAR-31]
16:23:20 2389  	 BEGIN
