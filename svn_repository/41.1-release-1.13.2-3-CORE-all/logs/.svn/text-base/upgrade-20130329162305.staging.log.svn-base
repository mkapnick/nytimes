16:23:20 2741  
16:23:20 2742  	 IF in_new_fin_instrument_type = GLOBAL_ENUMS_V19.INSTRUMENT_CREDIT_CARD THEN
16:23:20 2743  	   IF PROCS_FIN_INSTRUMENTS_V19.IS_CREDIT_CARD_EXISTS(in_new_fin_instrument_id) = GLOBAL_CONSTANTS_V19.FALSE THEN
16:23:20 2744  	     -- throw exception: bad new credit card
