16:23:20 1829  
16:23:20 1830  	 -- Select all gift certificates with given code
16:23:20 1831  	 OPEN out_result_set FOR
16:23:20 1832  	 SELECT
16:23:20 1833  	   gc.EXPIRATION_DATE,
16:23:20 1834  	   ch.name,
16:23:20 1835  	   ch.id,
16:23:20 1836  	   gc.sender_email,
16:23:20 1837  	   gc.sender_name,
