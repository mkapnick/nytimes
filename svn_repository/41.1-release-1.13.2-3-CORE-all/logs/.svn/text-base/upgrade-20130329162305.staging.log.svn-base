16:23:20 355  	out_result_set		      OUT SYS_REFCURSOR
16:23:20 356  ) AS
16:23:20 357  -- VARIABLES
16:23:20 358  SPROC_NAME	  CONSTANT VARCHAR2(30) := 'GET_TRNSCTN_ATTEMPTS_BY_STATUS';
16:23:20 359  temp_transaction_id NUMBER;
16:23:20 360  -- EXCEPTIONS
16:23:20 361  BAD_TRANSACTION_ID       EXCEPTION;
16:23:20 362  BAD_TRANS_ATTEMPT_STATUS EXCEPTION;
16:23:20 363  BEGIN
16:23:20 364  
16:23:20 365  	-- Check that transaction exists
16:23:20 366  	BEGIN
16:23:20 367  	  SELECT
16:23:20 368  	    TRANSACTION.ID into temp_transaction_id
16:23:20 369  	  FROM
16:23:20 370  	    TRANSACTION
16:23:20 371  	  WHERE
16:23:20 372  	    TRANSACTION.ID = in_transaction_id;
16:23:20 373  	  EXCEPTION
16:23:20 374  	    WHEN OTHERS THEN
16:23:20 375  	      RAISE BAD_TRANSACTION_ID;
16:23:20 376  	END;
16:23:20 377  
16:23:20 378  	-- Check that transaction attempt status is correct
16:23:20 379  	IF in_transaction_attempt_status != GLOBAL_STATUSES_V19.TRANS_ATTEMPT_IN_PROGRESS
16:23:20 380  	  AND in_transaction_attempt_status != GLOBAL_STATUSES_V19.TRANS_ATTEMPT_SUCCESS
16:23:20 381  	  AND in_transaction_attempt_status != GLOBAL_STATUSES_V19.TRANS_ATTEMPT_FAILED THEN
16:23:20 382  	  RAISE BAD_TRANS_ATTEMPT_STATUS;
16:23:20 383  	END IF;
16:23:20 384  
16:23:20 385  	OPEN out_result_set FOR
16:23:20 386  	SELECT
16:23:20 387  	  TRANSACTION_ATTEMPT.ID,
16:23:20 388  	  TRANSACTION_ATTEMPT.EXTERNAL_STATUS_CODE,
16:23:20 389  	  TRANSACTION_ATTEMPT.EXTERNAL_STATUS_MESSAGE,
16:23:20 390  	  TRANSACTION_ATTEMPT.CREATE_DATE,
16:23:20 391  	  TRANSACTION_ATTEMPT.CREATED_BY,
16:23:20 392  	  TRANSACTION_ATTEMPT.EXTERNAL_TRANSACTION_ID,
16:23:20 393  	  TRANSACTION_ATTEMPT.TRANSACTION_START_TIME,
16:23:20 394  	  TRANSACTION.TRANSACTION_AMOUNT,
16:23:20 395  	  TRANSACTION.ID as "TRANSACTION_ID",
16:23:20 396  	  TRANSACTION.UPDATE_DATE as "TRANSACTION_UPDATE_TIME"
16:23:20 397  	FROM
16:23:20 398  	  TRANSACTION_ATTEMPT
16:23:20 399  	  INNER JOIN TRANSACTION ON TRANSACTION_ATTEMPT.TRANSACTION_ID = TRANSACTION.ID
16:23:20 400  	WHERE
16:23:20 401  	  TRANSACTION_ATTEMPT.TRANSACTION_ID = in_transaction_id
16:23:20 402  	  AND TRANSACTION_ATTEMPT.TRANSACTION_ATTEMPT_STATUS_ID = in_transaction_attempt_status;
16:23:20 403  
16:23:20 404  EXCEPTION
16:23:20 405  WHEN BAD_TRANSACTION_ID THEN
16:23:20 406  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 407  	  SPROC_NAME, 'No such transaction');
16:23:20 408  WHEN BAD_TRANS_ATTEMPT_STATUS THEN
16:23:20 409  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
16:23:20 410  	  SPROC_NAME, 'Bad transaction attempt status');
16:23:20 411  WHEN OTHERS THEN
16:23:20 412  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 413  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 414  END GET_TRNSCTN_ATTEMPTS_BY_STATUS;
