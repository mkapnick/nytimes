16:23:20 1163  END GET_BILLING_CYCLE;
16:23:20 1164  
16:23:20 1165  /******************************************************************************/
16:23:20 1166  
16:23:20 1167  PROCEDURE REFUND_SUBSCRIPTION (
16:23:20 1168  /*
16:23:20 1169  Throws exceptions:
16:23:20 1170  APP_EXCEPTION_CODES_V19.NOT_FOUND
16:23:20 1171  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
16:23:20 1172  */
16:23:20 1173  	 in_subscription_id IN NUMBER,
16:23:20 1174  	 in_invoice_id	    IN NUMBER,
16:23:20 1175  	 in_refund_amount   IN NUMBER,
16:23:20 1176  	 in_note	    IN VARCHAR2,
16:23:20 1177  	 in_created_by	    IN VARCHAR2,
16:23:20 1178  	 out_charge_id	    OUT NUMBER
16:23:20 1179  ) AS
16:23:20 1180  -- VARIABLES
16:23:20 1181  SPROC_NAME	      CONSTANT VARCHAR2(19) := 'REFUND_SUBSCRIPTION';
16:23:20 1182  var_invoice_status_id  NUMBER;
16:23:20 1183  var_account_id	      NUMBER;
16:23:20 1184  var_account_status_id  NUMBER;
16:23:20 1185  var_new_transaction_id NUMBER;
16:23:20 1186  var_instrument_type_id NUMBER;
16:23:20 1187  var_instrument_id      NUMBER;
16:23:20 1188  var_new_charge_id      NUMBER;
16:23:20 1189  var_invoice_amount     NUMBER(10,2);
16:23:20 1190  var_refunds_before     NUMBER(10,2);
16:23:20 1191  var_charges_amount     NUMBER(10,2);
16:23:20 1192  -- EXCEPTIONS
16:23:20 1193  BAD_SUBSCRIPTION_ID	     EXCEPTION;
16:23:20 1194  ACCOUNT_IS_FROZEN	     EXCEPTION;
16:23:20 1195  BAD_INVOICE_ID		     EXCEPTION;
16:23:20 1196  CAN_NOT_CREATE_TRANSACTION    EXCEPTION;
16:23:20 1197  CAN_NOT_CREATE_CHARGE	     EXCEPTION;
16:23:20 1198  CAN_NOT_CALC_INVOICE_AMOUNT   EXCEPTION;
16:23:20 1199  REFUND_IS_GREATER_THAN_ANOUNT EXCEPTION;
16:23:20 1200  CAN_NOT_ANNOTATE_SUBSCRIPTION EXCEPTION;
16:23:20 1201  TOT_REF_IS_GREATER_THAN_ANOUNT EXCEPTION;
16:23:20 1202  INVOICE_IS_NOT_CLOSED	     EXCEPTION;
16:23:20 1203  TOT_REF_IS_GRATER_THAN_CHARGES EXCEPTION;
16:23:20 1204  EXCEPTION_MESSAGE	      VARCHAR2(1024);
16:23:20 1205  BEGIN
16:23:20 1206  
16:23:20 1207  	 BEGIN
16:23:20 1208  	   SELECT
16:23:20 1209  	     SUBSCRIPTION.INSTRUMENT_ID,
16:23:20 1210  	     SUBSCRIPTION.INSTRUMENT_TYPE_ID,
16:23:20 1211  	     SUBSCRIPTION.ACCOUNT_ID
16:23:20 1212  	     into
16:23:20 1213  	     var_instrument_id,
16:23:20 1214  	     var_instrument_type_id,
16:23:20 1215  	     var_account_id
16:23:20 1216  	   FROM
16:23:20 1217  	     SUBSCRIPTION
16:23:20 1218  	   WHERE
16:23:20 1219  	     SUBSCRIPTION.ID = in_subscription_id;
16:23:20 1220  	   EXCEPTION
16:23:20 1221  	     WHEN NO_DATA_FOUND THEN
16:23:20 1222  	       RAISE BAD_SUBSCRIPTION_ID;
16:23:20 1223  	 END;
16:23:20 1224  
16:23:20 1225  	 -- Check account status. It should not to be frozen
16:23:20 1226  	 SELECT
16:23:20 1227  	   ACCOUNT.ACCOUNT_STATUS_ID into var_account_status_id
16:23:20 1228  	 FROM
16:23:20 1229  	   ACCOUNT
16:23:20 1230  	 WHERE
16:23:20 1231  	   ACCOUNT.ID = var_account_id;
16:23:20 1232  
16:23:20 1233  	 IF var_account_status_id = GLOBAL_STATUSES_V19.ACCOUNT_FROZEN THEN
16:23:20 1234  	   RAISE ACCOUNT_IS_FROZEN;
16:23:20 1235  	 END IF;
16:23:20 1236  
16:23:20 1237  	 BEGIN
16:23:20 1238  	   SELECT
16:23:20 1239  	     INVOICE.INVOICE_STATUS_ID into var_invoice_status_id
16:23:20 1240  	   FROM
16:23:20 1241  	     INVOICE
16:23:20 1242  	   WHERE
16:23:20 1243  	     INVOICE.ID = in_invoice_id;
16:23:20 1244  	   EXCEPTION
16:23:20 1245  	     WHEN NO_DATA_FOUND THEN
16:23:20 1246  	       RAISE BAD_INVOICE_ID;
16:23:20 1247  	 END;
16:23:20 1248  
16:23:20 1249  	 IF var_invoice_status_id != GLOBAL_STATUSES_V19.INVOICE_CLOSED THEN
16:23:20 1250  	   RAISE INVOICE_IS_NOT_CLOSED;
16:23:20 1251  	 END IF;
16:23:20 1252  
16:23:20 1253  	 BEGIN
16:23:20 1254  	   PROCS_INVOICE_V19.CALCULATE_INVOICE_AMOUNT (
16:23:20 1255  	     in_invoice_id => in_invoice_id,
16:23:20 1256  	     out_amount    => var_invoice_amount
16:23:20 1257  	   );
16:23:20 1258  	   EXCEPTION
16:23:20 1259  	     WHEN OTHERS THEN
16:23:20 1260  	       EXCEPTION_MESSAGE := SQLERRM;
16:23:20 1261  	       RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
16:23:20 1262  	 END;
16:23:20 1263  
16:23:20 1264  	 IF ( in_refund_amount > var_invoice_amount ) THEN
16:23:20 1265  	   RAISE REFUND_IS_GREATER_THAN_ANOUNT;
16:23:20 1266  	 END IF;
16:23:20 1267  
16:23:20 1268  	 SELECT /*+ STAR_TRANSFORMATION */
16:23:20 1269  	   SUM(CHARGE.CHARGE_AMOUNT) into var_refunds_before
16:23:20 1270  	 FROM
16:23:20 1271  	   CHARGE
16:23:20 1272  	 WHERE
16:23:20 1273  	   CHARGE.INVOICE_ID = in_invoice_id
16:23:20 1274  	   AND (
16:23:20 1275  	     CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V19.CHARGE_OPENED
16:23:20 1276  	     OR CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V19.CHARGE_PROCESSED
16:23:20 1277  	   )
16:23:20 1278  	   AND CHARGE.CHARGE_AMOUNT < 0;
16:23:20 1279  
16:23:20 1280  	 -- Refunds are negative
16:23:20 1281  	 var_refunds_before := -var_refunds_before;
16:23:20 1282  
16:23:20 1283  	 var_charges_amount := 0;
16:23:20 1284  
16:23:20 1285  	 FOR f_processed_charges IN (
16:23:20 1286  	   SELECT
16:23:20 1287  	     CHARGE.CHARGE_AMOUNT
16:23:20 1288  	   FROM
16:23:20 1289  	     CHARGE
16:23:20 1290  	   WHERE
16:23:20 1291  	     CHARGE.INVOICE_ID = in_invoice_id
16:23:20 1292  	     AND CHARGE.CHARGE_STATUS_ID IN (SELECT GLOBAL_STATUSES_V19.CHARGE_PROCESSED FROM DUAL)
16:23:20 1293  	 )
16:23:20 1294  	 LOOP
16:23:20 1295  	   IF f_processed_charges.CHARGE_AMOUNT > 0 THEN
16:23:20 1296  	     var_charges_amount := var_charges_amount + f_processed_charges.CHARGE_AMOUNT;
16:23:20 1297  	   END IF;
16:23:20 1298  	 END LOOP;
16:23:20 1299  
16:23:20 1300  	 IF (in_refund_amount + var_refunds_before > var_invoice_amount) THEN
16:23:20 1301  	   RAISE TOT_REF_IS_GREATER_THAN_ANOUNT;
16:23:20 1302  	 END IF;
16:23:20 1303  
16:23:20 1304  	 IF (in_refund_amount + var_refunds_before > var_charges_amount) THEN
16:23:20 1305  	   RAISE TOT_REF_IS_GRATER_THAN_CHARGES;
16:23:20 1306  	 END IF;
16:23:20 1307  
16:23:20 1308  	 BEGIN
16:23:20 1309  	   PROCS_TRANSACTION_V19.CREATE_TRANSACTION(
16:23:20 1310  	     in_transaction_id	       => NULL,
16:23:20 1311  	     in_status_id	       => GLOBAL_STATUSES_V19.TRANSACTION_PREPARE,
16:23:20 1312  	     in_amount		       => -in_refund_amount,
16:23:20 1313  	     in_created_by	       => in_created_by,
16:23:20 1314  	     in_order_id	       => NULL,
16:23:20 1315  	     in_is_refund	       => GLOBAL_CONSTANTS_V19.TRUE,
16:23:20 1316  	     in_transaction_type_code  => 'REFUND',
16:23:20 1317  	     out_transaction_id        => var_new_transaction_id
16:23:20 1318  	   );
16:23:20 1319  	   EXCEPTION
16:23:20 1320  	     WHEN OTHERS THEN
16:23:20 1321  	       EXCEPTION_MESSAGE := SQLERRM;
16:23:20 1322  	       RAISE CAN_NOT_CREATE_TRANSACTION;
16:23:20 1323  	 END;
16:23:20 1324  
16:23:20 1325  	 BEGIN
16:23:20 1326  	   PROCS_CHARGE_V19.CREATE_CHARGE(
16:23:20 1327  	     in_invoice_id	   => in_invoice_id,
16:23:20 1328  	     in_transaction_id	   => var_new_transaction_id,
16:23:20 1329  	     in_instrument_type_id => var_instrument_type_id,
16:23:20 1330  	     in_instrument_id	   => var_instrument_id,
16:23:20 1331  	     in_charge_amount	   => -in_refund_amount,
16:23:20 1332  	     in_created_by	   => in_created_by,
16:23:20 1333  	     in_charge_status_id   => GLOBAL_STATUSES_V19.CHARGE_OPENED,
16:23:20 1334  	     out_charge_id	   => var_new_charge_id
16:23:20 1335  	   );
16:23:20 1336  	   EXCEPTION
16:23:20 1337  	     WHEN OTHERS THEN
16:23:20 1338  	       EXCEPTION_MESSAGE := SQLERRM;
16:23:20 1339  	       RAISE CAN_NOT_CREATE_CHARGE;
16:23:20 1340  	 END;
16:23:20 1341  
16:23:20 1342  	 IF in_note IS NOT NULL THEN
16:23:20 1343  	   BEGIN
16:23:20 1344  	     PROCS_SUBSCRIPTION_V19.ANNOTATE_SUBSCRIPTION(
16:23:20 1345  	       in_subscription_id => in_subscription_id,
16:23:20 1346  	       in_agent_id	  => 0, -- AGENT_ID??
16:23:20 1347  	       in_note		  => in_note,
16:23:20 1348  	       in_created_by	  => in_created_by
16:23:20 1349  	     );
16:23:20 1350  	     EXCEPTION
16:23:20 1351  	       WHEN OTHERS THEN
16:23:20 1352  		 EXCEPTION_MESSAGE := SQLERRM;
16:23:20 1353  		 RAISE CAN_NOT_ANNOTATE_SUBSCRIPTION;
16:23:20 1354  	   END;
16:23:20 1355  	 END IF;
16:23:20 1356  
16:23:20 1357  	 out_charge_id := var_new_charge_id;
16:23:20 1358  
16:23:20 1359  EXCEPTION
16:23:20 1360  WHEN INVOICE_IS_NOT_CLOSED THEN
16:23:20 1361  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
16:23:20 1362  	   SPROC_NAME, 'Invoice is not closed');
16:23:20 1363  WHEN BAD_SUBSCRIPTION_ID THEN
16:23:20 1364  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 1365  	   SPROC_NAME, 'No such subscription');
16:23:20 1366  WHEN ACCOUNT_IS_FROZEN THEN
16:23:20 1367  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
16:23:20 1368  	   SPROC_NAME, 'Could not refund subscription for frozen account');
16:23:20 1369  WHEN BAD_INVOICE_ID THEN
16:23:20 1370  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 1371  	   SPROC_NAME, 'No such invoice');
16:23:20 1372  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
16:23:20 1373  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 1374  	   SPROC_NAME, 'Could not calculate invoice amount', EXCEPTION_MESSAGE);
16:23:20 1375  WHEN REFUND_IS_GREATER_THAN_ANOUNT THEN
16:23:20 1376  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
16:23:20 1377  	   SPROC_NAME, 'Refund is greater than amount');
16:23:20 1378  WHEN TOT_REF_IS_GREATER_THAN_ANOUNT THEN
16:23:20 1379  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
16:23:20 1380  	   SPROC_NAME, 'There were refunds before and sum of all refunds and new refund more than invoice amount');
16:23:20 1381  WHEN TOT_REF_IS_GRATER_THAN_CHARGES THEN
16:23:20 1382  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
16:23:20 1383  	   SPROC_NAME, 'Total refund amount is greater than sum of processed charges');
16:23:20 1384  WHEN CAN_NOT_CREATE_TRANSACTION THEN
16:23:20 1385  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 1386  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
16:23:20 1387  WHEN CAN_NOT_CREATE_CHARGE THEN
16:23:20 1388  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 1389  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
16:23:20 1390  WHEN CAN_NOT_ANNOTATE_SUBSCRIPTION THEN
16:23:20 1391  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 1392  	   SPROC_NAME, 'Could not annotate subscription', EXCEPTION_MESSAGE);
16:23:20 1393  WHEN OTHERS THEN
16:23:20 1394  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 1395  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 1396  END REFUND_SUBSCRIPTION;
16:23:20 1397  
16:23:20 1398  /******************************************************************************/
16:23:20 1399  
16:23:20 1400  PROCEDURE ADD_SUBSCRIPTION_EXTENSION (
16:23:20 1401  /*
16:23:20 1402  APP_EXCEPTION_CODES_V19.NOT_FOUND
16:23:20 1403  APP_EXCEPTION_CODES_V19.INTERNAL_ERROR
16:23:20 1404  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
16:23:20 1405  */
16:23:20 1406  	 in_subscription_id	 IN NUMBER,
16:23:20 1407  	 in_effective_start_date IN DATE,
16:23:20 1408  	 in_effective_end_date	 IN DATE,
16:23:20 1409  	 in_note		 IN VARCHAR2,
16:23:20 1410  	 in_updated_by		 IN VARCHAR2
16:23:20 1411  ) AS
16:23:20 1412  -- VARIABLES
16:23:20 1413  SPROC_NAME		    CONSTANT VARCHAR2(26) := 'ADD_SUBSCRIPTION_EXTENSION';
16:23:20 1414  temp_subscription_id	    NUMBER;
16:23:20 1415  var_current_license_id	    NUMBER;
16:23:20 1416  var_current_license_start_date DATE;
16:23:20 1417  var_current_license_end_date DATE;
16:23:20 1418  var_current_offer_id	    NUMBER;
16:23:20 1419  var_current_invoice_id	    NUMBER;
16:23:20 1420  var_current_date 	    DATE;
16:23:20 1421  var_current_offer_index	    NUMBER;
16:23:20 1422  var_current_offer_recurr_num NUMBER;
16:23:20 1423  var_account_tax_exempt_id    VARCHAR2(255);
16:23:20 1424  
16:23:20 1425  var_free_invoice_id NUMBER;
16:23:20 1426  var_free_license_id NUMBER;
16:23:20 1427  var_new_license_id  NUMBER;
16:23:20 1428  var_ext_license_id  NUMBER;
16:23:20 1429  -- EXCEPTIONS
16:23:20 1430  BAD_SUBSCRIPTION_ID	     EXCEPTION;
16:23:20 1431  CAN_NOT_FIND_OFFER_OR_LICENSE EXCEPTION;
16:23:20 1432  CAN_NOT_CHANGE_LICENSE_STATUS EXCEPTION;
16:23:20 1433  CAN_NOT_CREATE_INVOICE	     EXCEPTION;
16:23:20 1434  CAN_NOT_CREATE_NEW_LICENSE    EXCEPTION;
16:23:20 1435  CAN_NOT_CREATE_END_LICENSE    EXCEPTION;
16:23:20 1436  CAN_NOT_ANNOTATE_SUBSCRIPTION EXCEPTION;
16:23:20 1437  EXTENS_START_DATE_IS_TOO_FAR  EXCEPTION;
16:23:20 1438  EXT_START_DATE_LATER_THEN_END EXCEPTION;
16:23:20 1439  EXTENS_START_DATE_IS_TOO_SMALL EXCEPTION;
16:23:20 1440  EXCEPTION_MESSAGE	     VARCHAR2(1024);
16:23:20 1441  BEGIN
16:23:20 1442  
16:23:20 1443  	 var_current_date := PROCS_COMMON_V19.NORMALIZE_DATE(SYSDATE);
16:23:20 1444  
16:23:20 1445  	 -- Check that subscription exists
16:23:20 1446  	 BEGIN
16:23:20 1447  	   SELECT
16:23:20 1448  	     SUBSCRIPTION.ID into temp_subscription_id
16:23:20 1449  	   FROM
16:23:20 1450  	     SUBSCRIPTION
16:23:20 1451  	   WHERE
16:23:20 1452  	     SUBSCRIPTION.ID = in_subscription_id;
16:23:20 1453  	   EXCEPTION
16:23:20 1454  	     WHEN NO_DATA_FOUND THEN
16:23:20 1455  	       RAISE BAD_SUBSCRIPTION_ID;
16:23:20 1456  	 END;
16:23:20 1457  
16:23:20 1458  	 -- Get account tax exempt id
16:23:20 1459  	 SELECT DISTINCT
16:23:20 1460  	   ACCOUNT.TAX_EXEMPT_ID into var_account_tax_exempt_id
16:23:20 1461  	 FROM
16:23:20 1462  	   ACCOUNT
16:23:20 1463  	   INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.ACCOUNT_ID = ACCOUNT.ID
16:23:20 1464  	 WHERE
16:23:20 1465  	   SUBSCRIPTION.ID = in_subscription_id;
16:23:20 1466  
16:23:20 1467  	 -- Select current data
16:23:20 1468  	 BEGIN
16:23:20 1469  	   SELECT
16:23:20 1470  	     LICENSE.ID,
16:23:20 1471  	     LICENSE.START_DATE,
16:23:20 1472  	     LICENSE.END_DATE,
16:23:20 1473  	     LICENSE.CURRENT_OFFER_INDEX,
16:23:20 1474  	     LICENSE.CURRENT_OFFER_RECURR_NUM,
16:23:20 1475  	     OFFER.ID,
16:23:20 1476  	     INVOICE.ID
16:23:20 1477  	   INTO
16:23:20 1478  	     var_current_license_id,
16:23:20 1479  	     var_current_license_start_date,
16:23:20 1480  	     var_current_license_end_date,
16:23:20 1481  	     var_current_offer_index,
16:23:20 1482  	     var_current_offer_recurr_num,
16:23:20 1483  	     var_current_offer_id,
16:23:20 1484  	     var_current_invoice_id
16:23:20 1485  	   FROM
16:23:20 1486  	     LICENSE
16:23:20 1487  	     INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
16:23:20 1488  	     INNER JOIN OFFER ON LICENSE.OFFER_ID = OFFER.ID
16:23:20 1489  	     INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
16:23:20 1490  	   WHERE
16:23:20 1491  	     SUBSCRIPTION.ID = in_subscription_id
16:23:20 1492  	     AND PROCS_COMMON_V19.NORMALIZE_DATE(LICENSE.END_DATE) > var_current_date
16:23:20 1493  	     AND PROCS_COMMON_V19.NORMALIZE_DATE(LICENSE.START_DATE) <= var_current_date
16:23:20 1494  	     AND ROWNUM <= 1
16:23:20 1495  	   ORDER BY
16:23:20 1496  	     LICENSE.ID DESC;
16:23:20 1497  	   EXCEPTION
16:23:20 1498  	     WHEN NO_DATA_FOUND THEN
16:23:20 1499  	       RAISE CAN_NOT_FIND_OFFER_OR_LICENSE;
16:23:20 1500  	 END;
16:23:20 1501  
16:23:20 1502  	 IF var_current_license_end_date < in_effective_start_date THEN
16:23:20 1503  	   RAISE EXTENS_START_DATE_IS_TOO_FAR;
16:23:20 1504  	 END IF;
16:23:20 1505  
16:23:20 1506  	 IF var_current_license_start_date > in_effective_start_date THEN
16:23:20 1507  	   RAISE EXTENS_START_DATE_IS_TOO_SMALL;
16:23:20 1508  	 END IF;
16:23:20 1509  
16:23:20 1510  	 IF in_effective_start_date > in_effective_end_date THEN
16:23:20 1511  	   RAISE EXT_START_DATE_LATER_THEN_END;
16:23:20 1512  	 END IF;
16:23:20 1513  
16:23:20 1514  	 -- Closing curent license
16:23:20 1515  	 BEGIN
16:23:20 1516  	   PROCS_LICENSE_CRU_V19.UPDATE_LICENSE(
16:23:20 1517  	     in_license_id	   => var_current_license_id,
16:23:20 1518  	     in_updated_by	   => in_updated_by,
16:23:20 1519  	     in_license_status_id  => GLOBAL_STATUSES_V19.LICENSE_CLOSED,
16:23:20 1520  	     in_end_date	   => in_effective_start_date,
16:23:20 1521  	     in_needs_entitlements => GLOBAL_CONSTANTS_V19.TRUE
16:23:20 1522  	   );
16:23:20 1523  	   EXCEPTION
16:23:20 1524  	     WHEN OTHERS THEN
16:23:20 1525  	       EXCEPTION_MESSAGE := SQLERRM;
16:23:20 1526  	       RAISE CAN_NOT_CHANGE_LICENSE_STATUS;
16:23:20 1527  	 END;
16:23:20 1528  
16:23:20 1529  	 -- Creating new "free" invoice
16:23:20 1530  	 BEGIN
16:23:20 1531  	   PROCS_INVOICE_V19.CREATE_INVOICE(
16:23:20 1532  	     in_invoice_status => GLOBAL_STATUSES_V19.INVOICE_CLOSED,
16:23:20 1533  	     in_created_by     => in_updated_by,
16:23:20 1534  	     in_tax_exempt_id  => var_account_tax_exempt_id,
16:23:20 1535  	     out_invoice_id    => var_free_invoice_id
16:23:20 1536  	   );
16:23:20 1537  	   EXCEPTION
16:23:20 1538  	     WHEN OTHERS THEN
16:23:20 1539  	       EXCEPTION_MESSAGE := SQLERRM;
16:23:20 1540  	       RAISE CAN_NOT_CREATE_INVOICE;
16:23:20 1541  	 END;
16:23:20 1542  
16:23:20 1543  	 -- Creating new "free" license
16:23:20 1544  	 BEGIN
16:23:20 1545  	   PROCS_LICENSE_V19.CREATE_LICENSE (
16:23:20 1546  	     in_status_id		 => GLOBAL_STATUSES_V19.LICENSE_ACTIVE,
16:23:20 1547  	     in_needs_entitlements	 => GLOBAL_CONSTANTS_V19.TRUE,
16:23:20 1548  	     in_start_date		 => in_effective_start_date,
16:23:20 1549  	     in_end_date		 => in_effective_end_date,
16:23:20 1550  	     in_offer_id		 => var_current_offer_id,
16:23:20 1551  	     in_subscription_id 	 => in_subscription_id,
16:23:20 1552  	     in_invoice_id		 => var_free_invoice_id,
16:23:20 1553  	     in_created_by		 => in_updated_by,
16:23:20 1554  	     in_is_extension		 => GLOBAL_CONSTANTS_V19.TRUE,
16:23:20 1555  	     in_current_offer_index	 => var_current_offer_index,
16:23:20 1556  	     in_current_offer_recurr_num => var_current_offer_recurr_num,
16:23:20 1557  	     out_license_id		 => var_free_license_id
16:23:20 1558  	   );
16:23:20 1559  	   EXCEPTION
16:23:20 1560  	     WHEN OTHERS THEN
16:23:20 1561  	       EXCEPTION_MESSAGE := SQLERRM;
16:23:20 1562  	       RAISE CAN_NOT_CREATE_NEW_LICENSE;
16:23:20 1563  	 END;
16:23:20 1564  
16:23:20 1565  	 -- Creating new license
16:23:20 1566  	 IF PROCS_COMMON_V19.NORMALIZE_DATE(var_current_license_end_date) >
16:23:20 1567  	    PROCS_COMMON_V19.NORMALIZE_DATE(in_effective_start_date) THEN
16:23:20 1568  	   BEGIN
16:23:20 1569  	     PROCS_LICENSE_V19.CREATE_LICENSE (
16:23:20 1570  	       in_status_id		   => GLOBAL_STATUSES_V19.LICENSE_ACTIVE,
16:23:20 1571  	       in_needs_entitlements	   => GLOBAL_CONSTANTS_V19.TRUE,
16:23:20 1572  	       in_start_date		   => in_effective_end_date,
16:23:20 1573  	       in_end_date		   => var_current_license_end_date + (in_effective_end_date - in_effective_start_date),
16:23:20 1574  	       in_offer_id		   => var_current_offer_id,
16:23:20 1575  	       in_subscription_id	   => in_subscription_id,
16:23:20 1576  	       in_invoice_id		   => var_current_invoice_id,
16:23:20 1577  	       in_created_by		   => in_updated_by,
16:23:20 1578  	       in_is_extension		   => GLOBAL_CONSTANTS_V19.FALSE,
16:23:20 1579  	       in_current_offer_index	   => var_current_offer_index,
16:23:20 1580  	       in_current_offer_recurr_num => var_current_offer_recurr_num,
16:23:20 1581  	       out_license_id		   => var_ext_license_id
16:23:20 1582  	     );
16:23:20 1583  	     EXCEPTION
16:23:20 1584  	       WHEN OTHERS THEN
16:23:20 1585  		 EXCEPTION_MESSAGE := SQLERRM;
16:23:20 1586  		 RAISE CAN_NOT_CREATE_END_LICENSE;
16:23:20 1587  	   END;
16:23:20 1588  	 END IF;
16:23:20 1589  
16:23:20 1590  	 -- Create new note for subscription
16:23:20 1591  	 BEGIN
16:23:20 1592  	   PROCS_SUBSCRIPTION_V19.ANNOTATE_SUBSCRIPTION (
16:23:20 1593  	     in_subscription_id => in_subscription_id,
16:23:20 1594  	     in_agent_id	=> 0, -- FIXME: What should to be here (agent id)?
16:23:20 1595  	     in_note		=> in_note,
16:23:20 1596  	     in_created_by	=> in_updated_by
16:23:20 1597  	   );
16:23:20 1598  	   EXCEPTION
16:23:20 1599  	     WHEN OTHERS THEN
16:23:20 1600  	       EXCEPTION_MESSAGE := SQLERRM;
16:23:20 1601  	       RAISE CAN_NOT_ANNOTATE_SUBSCRIPTION;
16:23:20 1602  	 END;
16:23:20 1603  
16:23:20 1604  EXCEPTION
16:23:20 1605  WHEN EXT_START_DATE_LATER_THEN_END THEN
16:23:20 1606  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
16:23:20 1607  	   SPROC_NAME, 'Extension start date is bigger then end date');
16:23:20 1608  WHEN EXTENS_START_DATE_IS_TOO_FAR THEN
16:23:20 1609  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
16:23:20 1610  	   SPROC_NAME, 'Extension start date is too far');
16:23:20 1611  WHEN EXTENS_START_DATE_IS_TOO_SMALL THEN
16:23:20 1612  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
16:23:20 1613  	   SPROC_NAME, 'Extension start date is too small');
16:23:20 1614  WHEN BAD_SUBSCRIPTION_ID THEN
16:23:20 1615  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 1616  	   SPROC_NAME, 'No such subscription');
16:23:20 1617  WHEN CAN_NOT_FIND_OFFER_OR_LICENSE THEN
16:23:20 1618  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 1619  	   SPROC_NAME, 'Could not find license and/or offer for given subscription');
16:23:20 1620  WHEN CAN_NOT_CHANGE_LICENSE_STATUS THEN
16:23:20 1621  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 1622  	   SPROC_NAME, 'Could not change license status', EXCEPTION_MESSAGE);
16:23:20 1623  WHEN CAN_NOT_CREATE_INVOICE THEN
16:23:20 1624  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 1625  	   SPROC_NAME, 'Could not create new invoice', EXCEPTION_MESSAGE);
16:23:20 1626  WHEN CAN_NOT_CREATE_NEW_LICENSE THEN
16:23:20 1627  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 1628  	   SPROC_NAME, 'Could not create new license', EXCEPTION_MESSAGE);
16:23:20 1629  WHEN CAN_NOT_CREATE_END_LICENSE THEN
16:23:20 1630  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 1631  	   SPROC_NAME, 'Could not create last license', EXCEPTION_MESSAGE);
16:23:20 1632  WHEN CAN_NOT_ANNOTATE_SUBSCRIPTION THEN
16:23:20 1633  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 1634  	   SPROC_NAME, 'Could not create new note for subscription', EXCEPTION_MESSAGE);
16:23:20 1635  WHEN OTHERS THEN
16:23:20 1636  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 1637  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:23:20 1638  END ADD_SUBSCRIPTION_EXTENSION;
16:23:20 1639  
16:23:20 1640  /******************************************************************************/
16:23:20 1641  
16:23:20 1642  FUNCTION CALC_SUBSCRIPTION_END_DATE (
16:23:20 1643  /*
16:23:20 1644  APP_EXCEPTION_CODES_V19.NOT_FOUND
16:23:20 1645  APP_EXCEPTION_CODES_V19.INTERNAL_ERROR
16:23:20 1646  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
16:23:20 1647  Returns:
16:23:20 1648  NULL if it is impossible to calculate end date (for example,
16:23:20 1649  	 offer chain includes offer with infinity recurrences number)
16:23:20 1650  DATE else
16:23:20 1651  */
16:23:20 1652  	 in_subscription_id IN NUMBER
16:23:20 1653  ) RETURN DATE AS
16:23:20 1654  -- VARIABLES
16:23:20 1655  SPROC_NAME		     CONSTANT VARCHAR2(26) := 'CALC_SUBSCRIPTION_END_DATE';
16:23:20 1656  last_license_id		     NUMBER;
16:23:20 1657  last_license_end_date	     DATE;
16:23:20 1658  last_license_offer_id	     NUMBER;
16:23:20 1659  last_license_offer_index      NUMBER;
16:23:20 1660  last_license_offer_recurr_num NUMBER;
16:23:20 1661  var_last_license_id	     NUMBER;
16:23:20 1662  var_offer_chain_id	     NUMBER;
16:23:20 1663  var_current_offer_rec_number  NUMBER;
16:23:20 1664  var_next_offers_set	     SYS_REFCURSOR;
16:23:20 1665  var_next_offer_duration	     VARCHAR2(30);
16:23:20 1666  var_next_offer_recur	     NUMBER;
16:23:20 1667  var_infinity_offers_number    NUMBER;
16:23:20 1668  
16:23:20 1669  var_result_date DATE;
16:23:20 1670  
16:23:20 1671  var_ym_interval	INTERVAL YEAR TO MONTH;
16:23:20 1672  var_ds_interval	INTERVAL DAY(3) TO SECOND;
16:23:20 1673  var_offer_years	NUMBER;
16:23:20 1674  var_offer_months NUMBER;
16:23:20 1675  var_offer_days	NUMBER;
16:23:20 1676  
16:23:20 1677  -- EXCEPTIONS
16:23:20 1678  BAD_SUBSCRIPTION_ID	  EXCEPTION;
16:23:20 1679  CAN_NOT_FIND_LAST_LICENSE  EXCEPTION;
16:23:20 1680  CAN_NOT_CALC_OFFER_LENGTH  EXCEPTION;
16:23:20 1681  CAN_NOT_CALC_OFFER_LENGTH2 EXCEPTION;
16:23:20 1682  EXCEPTION_MESSAGE	  VARCHAR2(1024);
16:23:20 1683  BEGIN
16:23:20 1684  
16:23:20 1685  	 BEGIN
16:23:20 1686  	   SELECT
16:23:20 1687  	     SUBSCRIPTION.OFFER_CHAIN_ID into var_offer_chain_id
16:23:20 1688  	   FROM
16:23:20 1689  	     SUBSCRIPTION
16:23:20 1690  	   WHERE
16:23:20 1691  	     SUBSCRIPTION.ID = in_subscription_id;
16:23:20 1692  	   EXCEPTION
16:23:20 1693  	     WHEN NO_DATA_FOUND THEN
16:23:20 1694  	       RAISE BAD_SUBSCRIPTION_ID;
16:23:20 1695  	 END;
16:23:20 1696  
16:23:20 1697  	 BEGIN
16:23:20 1698  	   SELECT
16:23:20 1699  	     ID into var_last_license_id
16:23:20 1700  	   FROM
16:23:20 1701  	     (
16:23:20 1702  	       SELECT
16:23:20 1703  		 LICENSE.ID
16:23:20 1704  	       FROM
16:23:20 1705  		 LICENSE
16:23:20 1706  	       WHERE
16:23:20 1707  		 LICENSE.SUBSCRIPTION_ID = in_subscription_id
16:23:20 1708  	       ORDER BY
16:23:20 1709  		 LICENSE.END_DATE DESC
16:23:20 1710  	     )
16:23:20 1711  	   WHERE
16:23:20 1712  	     ROWNUM <= 1;
16:23:20 1713  	   EXCEPTION
16:23:20 1714  	     WHEN NO_DATA_FOUND THEN
16:23:20 1715  	       RAISE CAN_NOT_FIND_LAST_LICENSE;
16:23:20 1716  	 END;
16:23:20 1717  
16:23:20 1718  	 SELECT
16:23:20 1719  	   COUNT(*) into var_infinity_offers_number
16:23:20 1720  	 FROM
16:23:20 1721  	   OFFER_OFFER_CHAIN
16:23:20 1722  	 WHERE
16:23:20 1723  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
16:23:20 1724  	   AND OFFER_OFFER_CHAIN.NUM_RECURRENCES = GLOBAL_ENUMS_V19.OFFER_REC_INFINITY;
16:23:20 1725  
16:23:20 1726  	 IF var_infinity_offers_number > 0 THEN
16:23:20 1727  	   RETURN NULL;
16:23:20 1728  	 END IF;
16:23:20 1729  
16:23:20 1730  	 BEGIN
16:23:20 1731  	   SELECT
16:23:20 1732  	     LICENSE.ID,
16:23:20 1733  	     LICENSE.END_DATE,
16:23:20 1734  	     LICENSE.CURRENT_OFFER_INDEX,
16:23:20 1735  	     LICENSE.CURRENT_OFFER_RECURR_NUM,
16:23:20 1736  	     LICENSE.OFFER_ID
16:23:20 1737  	     into
16:23:20 1738  	     last_license_id,
16:23:20 1739  	     last_license_end_date,
16:23:20 1740  	     last_license_offer_index,
16:23:20 1741  	     last_license_offer_recurr_num,
16:23:20 1742  	     last_license_offer_id
16:23:20 1743  	   FROM
16:23:20 1744  	     LICENSE
16:23:20 1745  	   WHERE
16:23:20 1746  	     LICENSE.ID = var_last_license_id;
16:23:20 1747  	   EXCEPTION
16:23:20 1748  	     WHEN NO_DATA_FOUND THEN
16:23:20 1749  	       RAISE CAN_NOT_FIND_LAST_LICENSE;
16:23:20 1750  	 END;
16:23:20 1751  
16:23:20 1752  	 var_result_date := last_license_end_date;
16:23:20 1753  
16:23:20 1754  	 -- Find current recurrence number
16:23:20 1755  	 SELECT
16:23:20 1756  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES into var_current_offer_rec_number
16:23:20 1757  	 FROM
16:23:20 1758  	   OFFER_OFFER_CHAIN
16:23:20 1759  	 WHERE
16:23:20 1760  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
16:23:20 1761  	   AND OFFER_OFFER_CHAIN.OFFER_ID = last_license_offer_id
16:23:20 1762  	   AND OFFER_OFFER_CHAIN.INDEX_VALUE = last_license_offer_index;
16:23:20 1763  
16:23:20 1764  	 IF var_current_offer_rec_number > last_license_offer_recurr_num THEN
16:23:20 1765  	   BEGIN
16:23:20 1766  	     PROCS_OFFER_CHAIN_V19.GET_OFFER_LENGTH(
16:23:20 1767  	       last_license_offer_id,
16:23:20 1768  	       var_offer_years,
16:23:20 1769  	       var_offer_months,
16:23:20 1770  	       var_offer_days
16:23:20 1771  	     );
16:23:20 1772  
16:23:20 1773  	     var_ym_interval := var_offer_years||'-'||var_offer_months;
16:23:20 1774  	     var_ds_interval := var_offer_days||' 0:0:0';
16:23:20 1775  
16:23:20 1776  	     var_result_date := var_result_date
16:23:20 1777  	       + ( var_ym_interval * ( var_current_offer_rec_number - last_license_offer_recurr_num ) )
16:23:20 1778  	       + ( var_ds_interval * ( var_current_offer_rec_number - last_license_offer_recurr_num ) );
16:23:20 1779  	     EXCEPTION
16:23:20 1780  	       WHEN OTHERS THEN
16:23:20 1781  		 EXCEPTION_MESSAGE := SQLERRM;
16:23:20 1782  		 RAISE CAN_NOT_CALC_OFFER_LENGTH;
16:23:20 1783  	   END;
16:23:20 1784  	 END IF;
16:23:20 1785  
16:23:20 1786  	 OPEN var_next_offers_set FOR
16:23:20 1787  	 SELECT
16:23:20 1788  	   OFFER.ENTITLEMENT_DURATION,
16:23:20 1789  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES
16:23:20 1790  	 FROM
16:23:20 1791  	   OFFER_OFFER_CHAIN
16:23:20 1792  	   INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
16:23:20 1793  	 WHERE
16:23:20 1794  	   OFFER_OFFER_CHAIN.INDEX_VALUE > last_license_offer_index
16:23:20 1795  	   AND OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id;
16:23:20 1796  
16:23:20 1797  	 LOOP
16:23:20 1798  	   FETCH var_next_offers_set into var_next_offer_duration, var_next_offer_recur;
16:23:20 1799  	   EXIT WHEN var_next_offers_set%NOTFOUND;
16:23:20 1800  	   BEGIN
16:23:20 1801  	     PROCS_COMMON_V19.ISO8601DURATION_TO_INTERVALS(
16:23:20 1802  	       var_next_offer_duration,
16:23:20 1803  	       var_offer_years,
16:23:20 1804  	       var_offer_months,
16:23:20 1805  	       var_offer_days
16:23:20 1806  	     );
16:23:20 1807  
16:23:20 1808  	     var_ym_interval := var_offer_years||'-'||var_offer_months;
16:23:20 1809  	     var_ds_interval := var_offer_days||' 0:0:0';
16:23:20 1810  
16:23:20 1811  	     var_result_date := var_result_date
16:23:20 1812  	       + ( var_ym_interval * var_next_offer_recur )
16:23:20 1813  	       + ( var_ds_interval * var_next_offer_recur );
16:23:20 1814  	     EXCEPTION
16:23:20 1815  	       WHEN OTHERS THEN
16:23:20 1816  		 EXCEPTION_MESSAGE := SQLERRM;
16:23:20 1817  		 RAISE CAN_NOT_CALC_OFFER_LENGTH2;
16:23:20 1818  	   END;
16:23:20 1819  	 END LOOP;
16:23:20 1820  
16:23:20 1821  	 RETURN var_result_date;
16:23:20 1822  
16:23:20 1823  EXCEPTION
16:23:20 1824  WHEN BAD_SUBSCRIPTION_ID THEN
16:23:20 1825  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 1826  	   SPROC_NAME, 'No such subscription');
16:23:20 1827  WHEN CAN_NOT_FIND_LAST_LICENSE THEN
16:23:20 1828  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
16:23:20 1829  	   SPROC_NAME, 'Could not find last license for given subscription');
16:23:20 1830  WHEN CAN_NOT_CALC_OFFER_LENGTH THEN
16:23:20 1831  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 1832  	   SPROC_NAME, 'Could not calculate offer length', EXCEPTION_MESSAGE);
16:23:20 1833  WHEN CAN_NOT_CALC_OFFER_LENGTH2 THEN
16:23:20 1834  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
16:23:20 1835  	   SPROC_NAME, 'Could not calculate last offer length', EXCEPTION_MESSAGE);
16:23:20 1836  WHEN OTHERS THEN
16:23:20 1837  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
16:23:20 1838  	   SPROC_NAME, 'Unkown error', SQLERRM);
16:23:20 1839  END CALC_SUBSCRIPTION_END_DATE;
