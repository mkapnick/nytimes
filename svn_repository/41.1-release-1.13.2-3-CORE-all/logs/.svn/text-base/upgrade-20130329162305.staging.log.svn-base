16:23:19  88  	PROCEDURE CHASE_PROFILE_BY_REQ_FILE_ID(
16:23:19  89  	  in_request_file_id IN CC_UPDATE.CC_REQUEST_FILE_ID%TYPE,
16:23:19  90  	  in_start	     IN NUMBER,
16:23:19  91  	  in_end	     IN NUMBER,
16:23:19  92  	  out_result_set     OUT SYS_REFCURSOR
16:23:19  93  	) AS
16:23:19  94  	SPROC_NAME CONSTANT VARCHAR2(32) := 'CHASE_PROFILE_BY_REQ_FILE_ID';
16:23:19  95  	var_range_diff	    NUMBER := 0;
16:23:19  96  	var_upper_bond_diff NUMBER := 0;
16:23:19  97  	var_l_start	    NUMBER := 0;
16:23:19  98  	var_l_end	    NUMBER := 0;
16:23:19  99  	BEGIN
16:23:19 100  	  --Normalize the end points [START]
16:23:19 101  	  IF (in_start IS NULL OR in_start < 0) Then
16:23:19 102  	    var_l_start := 0;
16:23:19 103  	  ELSE
16:23:19 104  	    var_l_start := in_start;
16:23:19 105  	  END IF;
16:23:19 106  
16:23:19 107  	  IF (in_end IS NULL) Then
16:23:19 108  	    var_l_end := 500;
16:23:19 109  	  ELSE
16:23:19 110  	    var_l_end := in_end;
16:23:19 111  	  END IF;
16:23:19 112  
16:23:19 113  	  var_l_start := var_l_start + 1;
16:23:19 114  	  var_l_end   := var_l_end   + 1;
16:23:19 115  
16:23:19 116  	  var_range_diff  := var_l_end - var_l_start;
16:23:19 117  	  var_upper_bond_diff :=  var_range_diff - 1000;
16:23:19 118  
16:23:19 119  	  IF (var_upper_bond_diff > 0) Then
16:23:19 120  	    var_l_end := var_l_end - var_upper_bond_diff;
16:23:19 121  	  END IF;
16:23:19 122  	  --Normalize the end points [END]
16:23:19 123  
16:23:19 124  	  OPEN out_result_set FOR
16:23:19 125  	    SELECT CHASE_PROFILE_ID FROM
16:23:19 126  	      (SELECT rownum rnum, q.* FROM
16:23:19 127  		 (SELECT
16:23:19 128  		    cc.CHASE_PROFILE_ID
16:23:19 129  		  FROM
16:23:19 130  		    CREDIT_CARD cc,
16:23:19 131  		    CC_UPDATE ccu
16:23:19 132  		  WHERE
16:23:19 133  		    ccu.CC_REQUEST_FILE_ID = in_request_file_id
16:23:19 134  		    AND ccu.CREDIT_CARD_ID = cc.id
16:23:19 135  		) Q
16:23:19 136  	      WHERE rownum <= var_l_end)
16:23:19 137  	    WHERE rnum >= var_l_Start;
16:23:19 138  	END CHASE_PROFILE_BY_REQ_FILE_ID;
