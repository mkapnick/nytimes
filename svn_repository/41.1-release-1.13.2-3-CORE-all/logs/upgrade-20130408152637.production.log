15:26:53 2572  
15:26:53 2573  FUNCTION IS_SUBSCRIPTION_CANCELABLE (
15:26:53 2574  	 in_subscription_id IN NUMBER
15:26:53 2575  ) RETURN NUMBER AS
15:26:53 2576  SPROC_NAME CONSTANT VARCHAR2(26) := 'IS_SUBSCRIPTION_CANCELABLE';
15:26:53 2577  -- VARIABLES
15:26:53 2578  var_is_offer_chain_cancelable NUMBER;
15:26:53 2579  --64603
15:26:53 2580  var_end_date date;
15:26:53 2581  today_date date := current_date;
15:26:53 2582  offer_id number;
15:26:53 2583  -- EXCEPTIONS
15:26:53 2584  COULD_NOT_CHECK	   EXCEPTION;
15:26:53 2585  BAD_SUBSCRIPTION_ID EXCEPTION;
15:26:53 2586  EXCEPTION_MESSAGE   VARCHAR2(1024);
15:26:53 2587  BEGIN
15:26:53 2588  
15:26:53 2589  	 BEGIN
15:26:53 2590  	   -- find offer_chain_id for given in_subscription_id
15:26:53 2591  	   SELECT OFFER_CHAIN_ID into offer_id
15:26:53 2592  	   FROM SUBSCRIPTION
15:26:53 2593  	   WHERE ID = in_subscription_id;
15:26:53 2594  	   EXCEPTION
15:26:53 2595  	     WHEN NO_DATA_FOUND THEN
15:26:53 2596  	       RAISE BAD_SUBSCRIPTION_ID;
15:26:53 2597  	     WHEN OTHERS THEN
15:26:53 2598  	       EXCEPTION_MESSAGE := SQLERRM;
15:26:53 2599  	       RAISE COULD_NOT_CHECK;
15:26:53 2600  	 END;
15:26:53 2601  
15:26:53 2602  	 -- find if it was redeemed from a gift certificate
15:26:53 2603  	 BEGIN
15:26:53 2604  	   SELECT l.end_date INTO var_end_date
15:26:53 2605  	   FROM LICENSE l, GIFT_CERTIFICATE g
15:26:53 2606  	   WHERE l.invoice_id = g.finalized_invoice_id
15:26:53 2607  	   AND l.subscription_id = in_subscription_id;
15:26:53 2608  
15:26:53 2609  	   -- if the license end_date is bigger than today, we are in the
15:26:53 2610  	   -- first period, so we cannot cancel; otherwise can cancel
15:26:53 2611  	   IF var_end_date > today_date THEN
15:26:53 2612  	       RETURN GLOBAL_CONSTANTS_V19.FALSE;
15:26:53 2613  	   ELSE
15:26:53 2614  	       RETURN GLOBAL_CONSTANTS_V19.TRUE;
15:26:53 2615  	   END IF;
15:26:53 2616  
15:26:53 2617  	   EXCEPTION
15:26:53 2618  	       -- not coming from a gift certificate,
15:26:53 2619  	       -- use old logic
15:26:53 2620  	       WHEN NO_DATA_FOUND THEN
15:26:53 2621  		   SELECT
15:26:53 2622  		       PROCS_OFFER_CHAIN_V19.IS_OFFER_CHAIN_CANCELABLE(offer_id)
15:26:53 2623  		       INTO var_is_offer_chain_cancelable
15:26:53 2624  		   FROM DUAL;
15:26:53 2625  		   RETURN var_is_offer_chain_cancelable;
15:26:53 2626  	 END;
15:26:53 2627  
15:26:53 2628  EXCEPTION
15:26:53 2629  WHEN BAD_SUBSCRIPTION_ID THEN
15:26:53 2630  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:53 2631  	   SPROC_NAME, 'No such subscription');
15:26:53 2632  WHEN COULD_NOT_CHECK THEN
15:26:53 2633  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:53 2634  	   SPROC_NAME, 'Could not check if offer chain calcelable', EXCEPTION_MESSAGE);
15:26:53 2635  WHEN OTHERS THEN
15:26:53 2636  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:53 2637  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:26:53 2638  
15:26:53 2639  END IS_SUBSCRIPTION_CANCELABLE;
