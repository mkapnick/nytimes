15:26:53 1172  -- EXCEPTIONS
15:26:53 1173  BAD_FIRST_OFFER_CHAIN	      EXCEPTION;
15:26:53 1174  BAD_SECOND_OFFER_CHAIN	      EXCEPTION;
15:26:53 1175  BEGIN
15:26:53 1176  
15:26:53 1177  	 -- Check that first offer chain exists
15:26:53 1178  	 BEGIN
15:26:53 1179  	   SELECT
15:26:53 1180  	     OFFER_CHAIN.ID into temp_offer_chain_id
15:26:53 1181  	   FROM
15:26:53 1182  	     OFFER_CHAIN
15:26:53 1183  	   WHERE
15:26:53 1184  	     OFFER_CHAIN.ID = in_offer_chain_1;
15:26:53 1185  	   EXCEPTION
15:26:53 1186  	     WHEN NO_DATA_FOUND THEN
15:26:53 1187  	       RAISE BAD_FIRST_OFFER_CHAIN;
15:26:53 1188  	 END;
15:26:53 1189  
15:26:53 1190  	 -- Check that second offer chain exists
15:26:53 1191  	 BEGIN
15:26:53 1192  	   SELECT
15:26:53 1193  	     OFFER_CHAIN.ID into temp_offer_chain_id
15:26:53 1194  	   FROM
15:26:53 1195  	     OFFER_CHAIN
15:26:53 1196  	   WHERE
15:26:53 1197  	     OFFER_CHAIN.ID = in_offer_chain_2;
15:26:53 1198  	   EXCEPTION
15:26:53 1199  	     WHEN NO_DATA_FOUND THEN
15:26:53 1200  	       RAISE BAD_SECOND_OFFER_CHAIN;
15:26:53 1201  	 END;
15:26:53 1202  
15:26:53 1203  	 PROCS_OFFER_CHAIN_V19.GET_OFF_CHAINS_SAME_PRODUCTS(
15:26:53 1204  	   in_offer_chain_1 => in_offer_chain_1,
15:26:53 1205  	   in_offer_chain_2 => in_offer_chain_2,
15:26:53 1206  	   out_result_set   => var_same_products
15:26:53 1207  	 );
15:26:53 1208  
15:26:53 1209  	 LOOP
15:26:53 1210  	   FETCH var_same_products INTO var_same_product_id, same_product_count;
15:26:53 1211  	   EXIT WHEN var_same_products%NOTFOUND;
15:26:53 1212  
15:26:53 1213  	   IF in_use_eligibility_rules = GLOBAL_CONSTANTS_V19.FALSE THEN
15:26:53 1214  	     -- Return false because this offer chains having same products
15:26:53 1215  	     RETURN GLOBAL_CONSTANTS_V19.TRUE;
15:26:53 1216  	   ELSE
15:26:53 1217  
15:26:53 1218  	     -- Get eligibility rule for given product
15:26:53 1219  	     BEGIN
15:26:53 1220  	       SELECT
15:26:53 1221  		 PRODUCT_ELIGIBILITY.VALUE into s_product_eligibility_limit
15:26:53 1222  	       FROM
15:26:53 1223  		 PRODUCT_ELIGIBILITY
15:26:53 1224  	       WHERE
15:26:53 1225  		 PRODUCT_ELIGIBILITY.PRODUCT_ID = var_same_product_id
15:26:53 1226  		 AND PRODUCT_ELIGIBILITY.NAME = PRODUCT_ELIGIBILITY_NAME;
15:26:53 1227  
15:26:53 1228  	       -- REVU: What should to be here? 1?
15:26:53 1229  	       EXCEPTION
15:26:53 1230  		 WHEN NO_DATA_FOUND THEN
15:26:53 1231  		   s_product_eligibility_limit := '1';
15:26:53 1232  	     END;
15:26:53 1233  
15:26:53 1234  	     IF UPPER(s_product_eligibility_limit) = GLOBAL_CONSTANTS_V19.MAX_CONSURRENT_PRD_UNLIM THEN
15:26:53 1235  	       RETURN GLOBAL_CONSTANTS_V19.FALSE;
15:26:53 1236  	     END IF;
15:26:53 1237  
15:26:53 1238  	     var_product_eligibility_limit := TO_NUMBER(s_product_eligibility_limit);
15:26:53 1239  
15:26:53 1240  	     -- Check for limit
15:26:53 1241  	     IF var_product_eligibility_limit < same_product_count THEN
15:26:53 1242  	       RETURN GLOBAL_CONSTANTS_V19.TRUE;
15:26:53 1243  	     END IF;
15:26:53 1244  
15:26:53 1245  	   END IF;
15:26:53 1246  	 END LOOP;
15:26:53 1247  
15:26:53 1248  	 RETURN GLOBAL_CONSTANTS_V19.FALSE;
15:26:53 1249  
15:26:53 1250  EXCEPTION
15:26:53 1251  WHEN BAD_FIRST_OFFER_CHAIN THEN
15:26:53 1252  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:53 1253  	   SPROC_NAME, 'First offer chain not found');
15:26:53 1254  WHEN BAD_SECOND_OFFER_CHAIN THEN
15:26:53 1255  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:53 1256  	   SPROC_NAME, 'Second offer chain not found');
15:26:53 1257  WHEN OTHERS THEN
15:26:53 1258  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:53 1259  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:26:53 1260  END CHECK_FOR_SAME_PRODUCTS;
