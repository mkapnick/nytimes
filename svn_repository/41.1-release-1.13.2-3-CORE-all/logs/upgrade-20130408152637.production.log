15:26:53 659  
15:26:53 660  PROCEDURE GET_SUBSCRIPTION_INFO (
15:26:53 661  /*
15:26:53 662  Throws exceptions:
15:26:53 663  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
15:26:53 664  */
15:26:53 665  	  in_subscription_id  IN  NUMBER,
15:26:53 666  	  out_result_set      OUT SYS_REFCURSOR
15:26:53 667  ) AS
15:26:53 668  SPROC_NAME CONSTANT VARCHAR2(21) := 'GET_SUBSCRIPTION_INFO';
15:26:53 669  -- VARIABLES
15:26:53 670  temp_subscription_id NUMBER;
15:26:53 671  -- EXCEPTIONS
15:26:53 672  BAD_SUBSCRIPTION_ID EXCEPTION;
15:26:53 673  BEGIN
15:26:53 674  	-- Find subscription by id
15:26:53 675  	-- Return its details
15:26:53 676  
15:26:53 677  	BEGIN
15:26:53 678  	  SELECT
15:26:53 679  	    SUBSCRIPTION.ID into temp_subscription_id
15:26:53 680  	  FROM
15:26:53 681  	    SUBSCRIPTION
15:26:53 682  	  WHERE
15:26:53 683  	    SUBSCRIPTION.ID = in_subscription_id;
15:26:53 684  	  EXCEPTION
15:26:53 685  	    WHEN NO_DATA_FOUND THEN
15:26:53 686  	      RAISE BAD_SUBSCRIPTION_ID;
15:26:53 687  	END;
15:26:53 688  
15:26:53 689  	OPEN out_result_set FOR
15:26:53 690  	SELECT
15:26:53 691  	  SUBSCRIPTION.ID AS "SUBSCRIPTION_ID",
15:26:53 692  	  SUBSCRIPTION.SUBSCRIPTION_STATUS_ID,
15:26:53 693  	  SUBSCRIPTION.PURCHASE_DATE,
15:26:53 694  	  SUBSCRIPTION.SUSPEND_DATE,
15:26:53 695  	  SUBSCRIPTION.REACTIVATION_DATE,
15:26:53 696  	  SUBSCRIPTION.CANCELLATION_DATE,
15:26:53 697  	  SUBSCRIPTION_CANCEL_REASON.VALUE as "CANCEL_TYPE",
15:26:53 698  	  OFFER_CHAIN.ID AS "OFFER_CHAIN_ID",
15:26:53 699  	  OFFER_CHAIN.NAME,
15:26:53 700  	  OFFER_CHAIN.DESCRIPTION,
15:26:53 701  	  OFFER_CHAIN.PRODUCT_URI,
15:26:53 702  	  SUBSCRIPTION.INSTRUMENT_ID,
15:26:53 703  	  SUBSCRIPTION.INSTRUMENT_TYPE_ID,
15:26:53 704  	  PROCS_SUBSCRIPTION_V19.CALC_SUBSCRIPTION_END_DATE(SUBSCRIPTION.ID) as "END_DATE",
15:26:53 705  	  PROCS_SUBSCRIPTION_V19.GET_RECENT_CHARGE(SUBSCRIPTION.ID) AS "RECENT_CHARGE",
15:26:53 706  	  PROCS_SUBSCRIPTION_V19.GET_RENEWAL_DATE(SUBSCRIPTION.ID) AS "RENEWAL_DATE",
15:26:53 707  	  PROCS_SUBSCRIPTION_V19.GET_BILLING_CYCLE(SUBSCRIPTION.ID) AS "BILLING_CYCLE",
15:26:53 708  	  (
15:26:53 709  	    SELECT
15:26:53 710  	      ACCOUNT.GROUP_ID
15:26:53 711  	      FROM ACCOUNT
15:26:53 712  	      WHERE ACCOUNT.ID = SUBSCRIPTION.ACCOUNT_ID
15:26:53 713  	  ) as "GROUP_ID",
15:26:53 714  	  (
15:26:53 715  	    SELECT
15:26:53 716  	      MAX(ENTITLEMENT_END_DATE)
15:26:53 717  	      FROM LICENSE
15:26:53 718  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:26:53 719  	  )
15:26:53 720  	  as "ENT_END_DATE",
15:26:53 721  	  (
15:26:53 722  	    SELECT
15:26:53 723  	      MIN(START_DATE)
15:26:53 724  	      FROM LICENSE
15:26:53 725  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:26:53 726  	  )
15:26:53 727  	  as "ENT_START_DATE",
15:26:53 728  	  PROCS_SUBSCRIPTION_V19.IS_SUBSCRIPTION_CANCELABLE(SUBSCRIPTION.ID) AS "IS_CANCELABLE",
15:26:53 729  	  ITUNES_RECEIPT.ID AS "ITUNES_RECEIPT_ID",
15:26:53 730  	  GROUP_ACCOUNT.ID GA_ID,
15:26:53 731  	  GROUP_ACCOUNT.SUBSCRIPTION_ID GA_SUBSCRIPTION_ID,
15:26:53 732  	  GROUP_ACCOUNT.GROUP_NAME GA_GROUP_NAME,
15:26:53 733  	  GROUP_ACCOUNT.FIRST_NAME GA_FIRST_NAME,
15:26:53 734  	  GROUP_ACCOUNT.LAST_NAME GA_LAST_NAME,
15:26:53 735  	  GROUP_ACCOUNT.EMAIL GA_EMAIL,
15:26:53 736  	  GROUP_ACCOUNT.PHONE GA_PHONE,
15:26:53 737  	  GROUP_ACCOUNT.ORGANIZATION_TYPE GA_ORGANIZATION_TYPE,
15:26:53 738  	  GROUP_ACCOUNT.SEATS GA_SEATS,
15:26:53 739  	  PROCS_GROUP_ACCOUNT_V19.F_GET_NUM_OCCUPIED_GROUP_SEATS(GROUP_ACCOUNT.ID) GA_SEATS_USED,
15:26:53 740  	  GROUP_ACCOUNT.IP GA_IP,
15:26:53 741  	  PROCS_SUBSCRIPTION_V19.GET_GIFT_CERT_CODE_BY_SUB_ID(SUBSCRIPTION.ID) gift_certificate_code,
15:26:53 742  	  PROCS_ACCOUNT_V19.GET_GRACE_START_DATE(SUBSCRIPTION.ID) GRACE_START_DATE,
15:26:53 743  	  PROCS_ACCOUNT_V19.GET_GRACE_END_DATE(SUBSCRIPTION.ID) GRACE_END_DATE
15:26:53 744  	FROM
15:26:53 745  	  SUBSCRIPTION
15:26:53 746  	  INNER JOIN OFFER_CHAIN ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
15:26:53 747  	  LEFT JOIN SUBSCRIPTION_CANCEL_REASON ON SUBSCRIPTION.SCT_ID = SUBSCRIPTION_CANCEL_REASON.ID
15:26:53 748  	  LEFT JOIN ITUNES_RECEIPT ON SUBSCRIPTION.ID = ITUNES_RECEIPT.SUBSCRIPTION_ID
15:26:53 749  	  LEFT JOIN GROUP_ACCOUNT ON SUBSCRIPTION.ID = GROUP_ACCOUNT.SUBSCRIPTION_ID
15:26:53 750  	WHERE
15:26:53 751  	  SUBSCRIPTION.ID = in_subscription_id;
15:26:53 752  
15:26:53 753  EXCEPTION
15:26:53 754  WHEN BAD_SUBSCRIPTION_ID THEN
15:26:53 755  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:53 756  	  SPROC_NAME, 'No such subscription');
15:26:53 757  WHEN OTHERS THEN
15:26:53 758  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:53 759  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:26:53 760  END GET_SUBSCRIPTION_INFO;
