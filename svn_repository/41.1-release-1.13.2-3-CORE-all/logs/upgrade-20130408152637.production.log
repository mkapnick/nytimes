15:26:53 254  		OR SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V19.SUBSCRIPTION_IN_GRACE_PERIOD)
15:26:53 255  	  )
15:26:53 256  	  LOOP
15:26:53 257  	    IF PROCS_OFFER_CHAIN_V19.CHECK_FOR_SAME_PRODUCTS(
15:26:53 258  	      in_offer_chain_1	       => in_offer_chain_id,
15:26:53 259  	      in_offer_chain_2	       => f_account_offer_chains.OFFER_CHAIN_ID,
15:26:53 260  	      in_use_eligibility_rules => GLOBAL_CONSTANTS_V19.TRUE
15:26:53 261  	    ) = GLOBAL_CONSTANTS_V19.TRUE THEN
15:26:53 262  
15:26:53 263  	      -- Find first concurrent subscription id:
15:26:53 264  	      SELECT
15:26:53 265  		ID into var_concur_subscription_id
15:26:53 266  	      FROM (
15:26:53 267  		SELECT
15:26:53 268  		  ID
15:26:53 269  		FROM
15:26:53 270  		  SUBSCRIPTION
15:26:53 271  		WHERE
15:26:53 272  		  SUBSCRIPTION.ACCOUNT_ID = var_account_id
15:26:53 273  		  AND SUBSCRIPTION.OFFER_CHAIN_ID = f_account_offer_chains.OFFER_CHAIN_ID
15:26:53 274  		  AND (
15:26:53 275  		    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V19.SUBSCRIPTION_ACTIVE
15:26:53 276  		    OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V19.SUBSCRIPTION_SUSPENDED
15:26:53 277  		    OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V19.SUBSCRIPTION_IN_GRACE_PERIOD)
15:26:53 278  		ORDER BY
15:26:53 279  		  ID
15:26:53 280  	      )
15:26:53 281  	      WHERE
15:26:53 282  		ROWNUM <= 1;
15:26:53 283  
15:26:53 284  	      RAISE PRODUCT_ALREADY_PURCHASED;
15:26:53 285  	    END IF;
15:26:53 286  	  END LOOP;
15:26:53 287  	END IF;
