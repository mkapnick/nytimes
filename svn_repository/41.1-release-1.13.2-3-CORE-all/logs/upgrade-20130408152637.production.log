15:26:53 365  	END;
15:26:53 366  
15:26:53 367  	out_subscription_id := var_new_subscription_id;
15:26:53 368  	out_invoice_id := var_new_invoice_id;
15:26:53 369  
15:26:53 370  EXCEPTION
