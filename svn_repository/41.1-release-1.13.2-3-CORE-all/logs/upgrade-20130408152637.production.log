15:26:53 1696  	       INNER JOIN PRODUCT_OFFERING ON OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = PRODUCT_OFFERING.ID
15:26:53 1697  	     GROUP BY
15:26:53 1698  	       PRODUCT_OFFERING.PRODUCT_ID
15:26:53 1699  	   ) ON PRODUCT_ID_IN_OFFER_CH_1 = PRODUCT_ID_IN_OFFER_CH_2;
