15:26:54 1461  	 in_invoice_id IN NUMBER,
15:26:54 1462  	 out_group_ids OUT SYS_REFCURSOR
15:26:54 1463  ) AS
15:26:54 1464  SPROC_NAME CONSTANT VARCHAR2(27) := 'GET_GROUPS_ID_BY_INVOICE_ID';
15:26:54 1465  -- VARIABLES
15:26:54 1466  temp_invoice_id		 NUMBER;
15:26:54 1467  var_subscrib_group_id	 NUMBER;
15:26:54 1468  var_gc_purchaser_group_id NUMBER;
15:26:54 1469  var_gc_redeemer_group_id  NUMBER;
15:26:54 1470  -- EXCEPTIONS
15:26:54 1471  BAD_INVOICE_ID	    EXCEPTION;
15:26:54 1472  CAN_NOT_FIND_ACCOUNT EXCEPTION;
15:26:54 1473  BEGIN
15:26:54 1474  
15:26:54 1475  	 BEGIN
15:26:54 1476  	   SELECT
15:26:54 1477  	     INVOICE.ID into temp_invoice_id
15:26:54 1478  	   FROM
15:26:54 1479  	     INVOICE
15:26:54 1480  	   WHERE
15:26:54 1481  	     INVOICE.ID = in_invoice_id;
15:26:54 1482  	   EXCEPTION
15:26:54 1483  	     WHEN NO_DATA_FOUND THEN
15:26:54 1484  	       RAISE BAD_INVOICE_ID;
15:26:54 1485  	 END;
15:26:54 1486  
15:26:54 1487  	 BEGIN
15:26:54 1488  	   SELECT DISTINCT
15:26:54 1489  	     ACCOUNT.GROUP_ID into var_subscrib_group_id
15:26:54 1490  	   FROM
15:26:54 1491  	     ACCOUNT
15:26:54 1492  	     INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.ACCOUNT_ID = ACCOUNT.ID
15:26:54 1493  	     INNER JOIN LICENSE ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:26:54 1494  	   WHERE
15:26:54 1495  	     LICENSE.INVOICE_ID = in_invoice_id;
15:26:54 1496  	   EXCEPTION
15:26:54 1497  	     WHEN NO_DATA_FOUND THEN
15:26:54 1498  	       var_subscrib_group_id := NULL;
15:26:54 1499  	 END;
15:26:54 1500  
15:26:54 1501  	 IF var_subscrib_group_id IS NULL THEN
15:26:54 1502  	   BEGIN
15:26:54 1503  	     SELECT
15:26:54 1504  	       GIFT_CERTIFICATE.PURCHASER_GROUP_ID,
15:26:54 1505  	       GIFT_CERTIFICATE.REDEEMER_GROUP_ID
15:26:54 1506  	       into
15:26:54 1507  	       var_gc_purchaser_group_id,
15:26:54 1508  	       var_gc_redeemer_group_id
15:26:54 1509  	     FROM
15:26:54 1510  	       GIFT_CERTIFICATE
15:26:54 1511  	     WHERE
15:26:54 1512  	       GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = in_invoice_id
15:26:54 1513  	       OR GIFT_CERTIFICATE.FINALIZED_INVOICE_ID = in_invoice_id;
15:26:54 1514  	     EXCEPTION
15:26:54 1515  	       WHEN NO_DATA_FOUND THEN
15:26:54 1516  		 var_gc_purchaser_group_id := NULL;
15:26:54 1517  		 var_gc_redeemer_group_id  := NULL;
15:26:54 1518  	   END;
15:26:54 1519  	 END IF;
15:26:54 1520  
15:26:54 1521  	 IF var_subscrib_group_id IS NULL
15:26:54 1522  	   AND var_gc_purchaser_group_id IS NULL
15:26:54 1523  	   AND var_gc_redeemer_group_id IS NULL THEN
15:26:54 1524  	     RAISE CAN_NOT_FIND_ACCOUNT;
15:26:54 1525  	 END IF;
15:26:54 1526  
15:26:54 1527  	 OPEN out_group_ids FOR
15:26:54 1528  	 SELECT GROUP_ID FROM (
15:26:54 1529  	   SELECT
15:26:54 1530  	     var_subscrib_group_id as "GROUP_ID"
15:26:54 1531  	   FROM
15:26:54 1532  	     DUAL
15:26:54 1533  	   UNION
15:26:54 1534  	   SELECT
15:26:54 1535  	     var_gc_purchaser_group_id as "GROUP_ID"
15:26:54 1536  	   FROM
15:26:54 1537  	     DUAL
15:26:54 1538  	   UNION
15:26:54 1539  	   SELECT
15:26:54 1540  	     var_gc_redeemer_group_id as "GROUP_ID"
15:26:54 1541  	   FROM
15:26:54 1542  	     DUAL
15:26:54 1543  	 )
15:26:54 1544  	 WHERE
15:26:54 1545  	   GROUP_ID IS NOT NULL;
15:26:54 1546  
15:26:54 1547  EXCEPTION
15:26:54 1548  WHEN BAD_INVOICE_ID THEN
15:26:54 1549  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 1550  	   SPROC_NAME, 'No such invoice');
15:26:54 1551  WHEN CAN_NOT_FIND_ACCOUNT THEN
15:26:54 1552  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 1553  	   SPROC_NAME, 'Could not find account for given invoice');
15:26:54 1554  WHEN OTHERS THEN
15:26:54 1555  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:54 1556  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:26:54 1557  END GET_GROUPS_ID_BY_INVOICE_ID;
