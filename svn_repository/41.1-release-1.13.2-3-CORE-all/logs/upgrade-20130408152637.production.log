15:26:54 192  	-- For now, we can disable account whenever
15:26:54 193  	IF current_account_status = GLOBAL_STATUSES_V19.ACCOUNT_DISABLED THEN
15:26:54 194  	  RAISE BAD_CURRENT_ACC_STATUS;
15:26:54 195  	END IF;
15:26:54 196  
15:26:54 197  	-- Checks for out outstanding balances
15:26:54 198  	-- CHECK: No outstanding balances. If monies are due, then we can not cancel account. Return ERROR.
15:26:54 199  	SELECT
15:26:54 200  	  COUNT(*) into var_pending_invoices_num
15:26:54 201  	FROM
15:26:54 202  	  LICENSE
15:26:54 203  	    INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:26:54 204  	    INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
15:26:54 205  	WHERE
15:26:54 206  	  SUBSCRIPTION.ACCOUNT_ID = var_account_id
15:26:54 207  	  AND INVOICE.INVOICE_STATUS_ID IN ( SELECT GLOBAL_STATUSES_V19.INVOICE_OPEN FROM DUAL );
15:26:54 208  
15:26:54 209  	IF var_pending_invoices_num > 0 THEN
15:26:54 210  	  RAISE PENDING_INVOICES_FOUND;
15:26:54 211  	END IF;
15:26:54 212  
15:26:54 213  	SELECT
15:26:54 214  	  COUNT(*) into var_active_subscriptions_num
15:26:54 215  	FROM
15:26:54 216  	  SUBSCRIPTION
15:26:54 217  	WHERE
15:26:54 218  	  SUBSCRIPTION.ACCOUNT_ID = var_account_id
15:26:54 219  	  AND (
15:26:54 220  	    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V19.SUBSCRIPTION_ACTIVE
15:26:54 221  	    OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V19.SUBSCRIPTION_SUSPENDED
15:26:54 222  	    OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V19.SUBSCRIPTION_IN_GRACE_PERIOD);
15:26:54 223  
15:26:54 224  	IF var_active_subscriptions_num > 0 THEN
15:26:54 225  	  RAISE ACCOUNT_HAS_ACIVE_SUBSCRS;
15:26:54 226  	END IF;
15:26:54 227  
15:26:54 228  	PROCS_ACCOUNT_V19.UPDATE_ACCOUNT_STATUS(
15:26:54 229  	  in_account_id        => var_account_id,
15:26:54 230  	  in_account_status_id => GLOBAL_STATUSES_V19.ACCOUNT_DISABLED,
15:26:54 231  	  in_updated_by        => in_updated_by
15:26:54 232  	);
15:26:54 233  
15:26:54 234  	-- Annotate account
15:26:54 235  	IF in_note IS NOT NULL THEN
15:26:54 236  	  BEGIN
15:26:54 237  	    PROCS_ACCOUNT_V19.ANNOTATE_ACCOUNT(
15:26:54 238  	      in_group_id   => in_group_id,
15:26:54 239  	      in_agent_id   => in_agent_id,
15:26:54 240  	      in_note	    => in_note,
15:26:54 241  	      in_created_by => in_updated_by
15:26:54 242  	    );
15:26:54 243  	    EXCEPTION
15:26:54 244  	      WHEN OTHERS THEN
15:26:54 245  		EXCEPTION_MESSAGE := SQLERRM;
15:26:54 246  		RAISE CAN_NOT_ANNOTATE_ACCOUNT;
15:26:54 247  	  END;
15:26:54 248  	END IF;
15:26:54 249  
15:26:54 250  EXCEPTION
15:26:54 251  WHEN ACCOUNT_HAS_ACIVE_SUBSCRS THEN
15:26:54 252  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 253  	  SPROC_NAME, 'Account has active or suspended subsciptions');
15:26:54 254  WHEN BAD_CURRENT_ACC_STATUS THEN
15:26:54 255  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
15:26:54 256  	  SPROC_NAME, 'Current account status is "disabled". Can not disable it one more time.');
15:26:54 257  WHEN PENDING_INVOICES_FOUND THEN
15:26:54 258  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 259  	  SPROC_NAME, 'Opened/Pending invoices founded');
15:26:54 260  WHEN BAD_ACOUNT_ID THEN
15:26:54 261  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 262  	  SPROC_NAME, 'No such account');
15:26:54 263  WHEN CAN_NOT_ANNOTATE_ACCOUNT THEN
15:26:54 264  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 265  	  SPROC_NAME, 'Could not annotate account', EXCEPTION_MESSAGE);
15:26:54 266  WHEN OTHERS THEN
15:26:54 267  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:54 268  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:26:54 269  END DISABLE_ACCOUNT;
15:26:54 270  
15:26:54 271  /******************************************************************************/
15:26:54 272  
15:26:54 273  PROCEDURE CREATE_ACTIVE_ACCOUNT(
15:26:54 274  /*
15:26:54 275  Throws exceptions:
15:26:54 276  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:54 277  APP_EXCEPTION_CODES_V19.DUPLICATE_ERROR
15:26:54 278  */
15:26:54 279  	  in_created_by IN VARCHAR2,
15:26:54 280  	  in_group_id	IN NUMBER
15:26:54 281  ) AS
15:26:54 282  -- VARIABLES
15:26:54 283  SPROC_NAME      CONSTANT VARCHAR2(21) := 'CREATE_ACTIVE_ACCOUNT';
15:26:54 284  new_account_id  NUMBER;
15:26:54 285  temp_group_id   NUMBER;
15:26:54 286  -- EXCEPTIONS
15:26:54 287  GROUP_EXISTS_EXCEPTION EXCEPTION;
15:26:54 288  EXCEPTION_MESSAGE      VARCHAR2(1024);
15:26:54 289  BEGIN
15:26:54 290  	-- Check if account already exists
15:26:54 291  	BEGIN
15:26:54 292  	  SELECT
15:26:54 293  	    ACCOUNT.GROUP_ID into temp_group_id
15:26:54 294  	  FROM
15:26:54 295  	    ACCOUNT
15:26:54 296  	  WHERE
15:26:54 297  	    ACCOUNT.GROUP_ID = in_group_id
15:26:54 298  	    AND ROWNUM <= 1;
15:26:54 299  
15:26:54 300  	  IF SQL%ROWCOUNT = 1 THEN
15:26:54 301  	    RAISE GROUP_EXISTS_EXCEPTION;
15:26:54 302  	  END IF;
15:26:54 303  
15:26:54 304  	  EXCEPTION
15:26:54 305  	    WHEN NO_DATA_FOUND THEN
15:26:54 306  	      NULL;
15:26:54 307  	END;
15:26:54 308  
15:26:54 309  	-- Insert new row into ACCOUNT table
15:26:54 310  	PROCS_ACCOUNT_CRU_V19.CREATE_ACCOUNT(
15:26:54 311  	  out_account_id	=> new_account_id,
15:26:54 312  	  in_account_status_id	=> GLOBAL_STATUSES_V19.ACCOUNT_ACTIVE,
15:26:54 313  	  in_group_id		=> in_group_id,
15:26:54 314  	  in_created_by 	=> in_created_by,
15:26:54 315  	  in_system_category_id => GLOBAL_ENUMS_V19.SYSTEM_CATEGORY_LIVE
15:26:54 316  	);
15:26:54 317  
15:26:54 318  EXCEPTION
15:26:54 319  WHEN GROUP_EXISTS_EXCEPTION THEN
15:26:54 320  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.DUPLICATE_ERROR,
15:26:54 321  	  SPROC_NAME, 'Group already exists');
15:26:54 322  WHEN OTHERS THEN
15:26:54 323  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:54 324  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:26:54 325  END CREATE_ACTIVE_ACCOUNT;
15:26:54 326  
15:26:54 327  /******************************************************************************/
15:26:54 328  
15:26:54 329  PROCEDURE REACTIVATE_ACCOUNT (
15:26:54 330  /*
15:26:54 331  Throws exceptions:
15:26:54 332  APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 333  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:54 334  */
15:26:54 335  	in_group_id   IN NUMBER,
15:26:54 336  	in_updated_by IN VARCHAR2,
15:26:54 337  	in_note       IN VARCHAR2,
15:26:54 338  	in_agent_id   IN NUMBER
15:26:54 339  ) AS
15:26:54 340  -- VARIABLES
15:26:54 341  SPROC_NAME	      CONSTANT VARCHAR2(18) := 'REACTIVATE_ACCOUNT';
15:26:54 342  var_account_id	      NUMBER;
15:26:54 343  current_account_status  NUMBER;
15:26:54 344  
15:26:54 345  -- EXCEPTIONS
15:26:54 346  BAD_CURRENT_ACC_STATUS EXCEPTION;
15:26:54 347  CAN_NOT_CREATE_NOTE    EXCEPTION;
15:26:54 348  EXCEPTION_MESSAGE      VARCHAR2(1024);
15:26:54 349  BEGIN
15:26:54 350  	-- Get account id, status
15:26:54 351  	SELECT
15:26:54 352  	  ACCOUNT.ACCOUNT_STATUS_ID,
15:26:54 353  	  ACCOUNT.ID
15:26:54 354  	INTO
15:26:54 355  	  current_account_status,
15:26:54 356  	  var_account_id
15:26:54 357  	FROM ACCOUNT
15:26:54 358  	WHERE
15:26:54 359  	  ACCOUNT.GROUP_ID = in_group_id;
15:26:54 360  
15:26:54 361  	IF current_account_status != GLOBAL_STATUSES_V19.ACCOUNT_FROZEN THEN
15:26:54 362  	  RAISE BAD_CURRENT_ACC_STATUS;
15:26:54 363  	END IF;
15:26:54 364  
15:26:54 365  	-- Change account status
15:26:54 366  	PROCS_ACCOUNT_V19.UPDATE_ACCOUNT_STATUS(
15:26:54 367  	  in_account_id        => var_account_id,
15:26:54 368  	  in_updated_by        => in_updated_by,
15:26:54 369  	  in_account_status_id => GLOBAL_STATUSES_V19.ACCOUNT_ACTIVE
15:26:54 370  	);
15:26:54 371  
15:26:54 372  	-- Add note
15:26:54 373  	BEGIN
15:26:54 374  	  PROCS_ACCOUNT_V19.ANNOTATE_ACCOUNT(
15:26:54 375  	    in_group_id   => in_group_id,
15:26:54 376  	    in_agent_id   => in_agent_id,
15:26:54 377  	    in_note	  => in_note,
15:26:54 378  	    in_created_by => in_updated_by
15:26:54 379  	  );
15:26:54 380  	  EXCEPTION
15:26:54 381  	    WHEN OTHERS THEN
15:26:54 382  	      EXCEPTION_MESSAGE := SQLERRM;
15:26:54 383  	      RAISE CAN_NOT_CREATE_NOTE;
15:26:54 384  	END;
15:26:54 385  
15:26:54 386  EXCEPTION
15:26:54 387  WHEN BAD_CURRENT_ACC_STATUS THEN
15:26:54 388  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
15:26:54 389  	  SPROC_NAME, 'Current account status is not "frozen"');
15:26:54 390  WHEN NO_DATA_FOUND THEN
15:26:54 391  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 392  	  SPROC_NAME, 'Could not find account with given group ID');
15:26:54 393  WHEN CAN_NOT_CREATE_NOTE THEN
15:26:54 394  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 395  	  SPROC_NAME, 'Could not annotate account');
15:26:54 396  WHEN OTHERS THEN
15:26:54 397  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:54 398  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:26:54 399  END REACTIVATE_ACCOUNT;
