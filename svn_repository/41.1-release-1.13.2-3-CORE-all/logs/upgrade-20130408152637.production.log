15:26:53 466  	      in_transaction_id 	=> NULL,
15:26:53 467  	      in_status_id		=> GLOBAL_STATUSES_V19.TRANSACTION_PENDING,
15:26:53 468  	      in_amount 		=> var_invoice_amount,
15:26:53 469  	      in_created_by		=> in_created_by,
15:26:53 470  	      in_order_id		=> NULL,
15:26:53 471  	      in_transaction_type_code	=> 'START_SUBSCRIPTION',
15:26:53 472  	      out_transaction_id	=> var_new_transaction_id
15:26:53 473  	    );
15:26:53 474  	    EXCEPTION
15:26:53 475  	      WHEN OTHERS THEN
15:26:53 476  		EXCEPTION_MESSAGE := SQLERRM;
15:26:53 477  		RAISE CAN_NOT_CREATE_TRANSACTION;
15:26:53 478  	  END;
15:26:53 479  
15:26:53 480  	  BEGIN
15:26:53 481  	    PROCS_CHARGE_V19.CREATE_CHARGE(
15:26:53 482  	      in_invoice_id	    => in_invoice_id,
15:26:53 483  	      in_transaction_id     => var_new_transaction_id,
15:26:53 484  	      in_instrument_type_id => in_instrument_type_id,
15:26:53 485  	      in_instrument_id	    => in_instrument_id,
