15:26:54 2266  	 in_charge_id		IN NUMBER,
15:26:54 2267  	 out_result		OUT NUMBER
15:26:54 2268  ) AS
15:26:54 2269  -- VARIABLES
15:26:54 2270  SPROC_NAME	    CONSTANT VARCHAR2(25) := 'IS_GCERT_FOR_PROPER_OFFER';
15:26:54 2271  var_invoice_id	    NUMBER;
15:26:54 2272  var_offer_chain_id   NUMBER;
15:26:54 2273  var_offer_chain_s_id NUMBER;
15:26:54 2274  -- EXCEPTIONS
15:26:54 2275  BAD_CHARGE_ID		 EXCEPTION;
15:26:54 2276  BAD_GIFT_CERTIFICATE_ID	 EXCEPTION;
15:26:54 2277  CAN_NOT_FIND_SUBSCRIPTION EXCEPTION;
15:26:54 2278  BEGIN
15:26:54 2279  
15:26:54 2280  	 BEGIN
15:26:54 2281  	   SELECT
15:26:54 2282  	     CHARGE.INVOICE_ID into var_invoice_id
15:26:54 2283  	   FROM
15:26:54 2284  	     CHARGE
15:26:54 2285  	   WHERE
15:26:54 2286  	     CHARGE.ID = in_charge_id;
