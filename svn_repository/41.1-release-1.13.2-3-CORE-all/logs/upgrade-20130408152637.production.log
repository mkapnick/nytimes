15:26:53 845  PROCEDURE GET_GRACE_LICE_FOR_FINAL_TRANS (
15:26:53 846  	in_days_before_close	 IN NUMBER,
15:26:53 847  	in_num_licenses_to_fetch IN NUMBER,
15:26:53 848  	out_result_set		 OUT SYS_REFCURSOR
15:26:53 849  ) AS
15:26:53 850  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_GRACE_LICE_FOR_FINAL_TRANS';
15:26:53 851  BEGIN
15:26:53 852  	OPEN out_result_set FOR
15:26:53 853  	SELECT
15:26:53 854  	    *
15:26:53 855  	FROM
15:26:53 856  	    (
15:26:53 857  		SELECT
15:26:53 858  		    l.id
15:26:53 859  		FROM
15:26:53 860  		    license l
15:26:53 861  		JOIN
15:26:53 862  		    invoice i
15:26:53 863  		ON
15:26:53 864  		    l.invoice_id = i.id
15:26:53 865  		WHERE
15:26:53 866  		    i.invoice_status_id = GLOBAL_STATUSES_V19.INVOICE_OPEN
15:26:53 867  		AND l.license_status_id = GLOBAL_STATUSES_V19.LICENSE_IN_GRACE_PERIOD
15:26:53 868  		AND SYSDATE + in_days_before_close >= l.grace_end_date
15:26:53 869  		AND NOT EXISTS
15:26:53 870  		    (
15:26:53 871  			SELECT
15:26:53 872  			    1
15:26:53 873  			FROM
15:26:53 874  			    charge c
15:26:53 875  			WHERE
