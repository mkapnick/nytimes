15:26:54 1609  	 in_updated_by		      IN VARCHAR2,
15:26:54 1610  	 in_is_tax_calculation_needed IN NUMBER
15:26:54 1611  ) AS
15:26:54 1612  SPROC_NAME CONSTANT VARCHAR2(29) := 'SET_IS_TAX_CALCULATION_NEEDED';
15:26:54 1613  -- VARIABLES
