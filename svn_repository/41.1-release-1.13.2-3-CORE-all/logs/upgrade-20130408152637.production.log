15:26:54 442  
15:26:54 443  PROCEDURE GET_ACCOUNT_GIFT_CERTIFICATES (
15:26:54 444  /*
15:26:54 445  IN:
15:26:54 446  instr_status:
15:26:54 447  GLOBAL_CONSTANTS_V19.TRUE - get active instruments only (default)
15:26:54 448  GLOBAL_CONSTANTS_V19.FALSE - get inactive instruments only
15:26:54 449  
15:26:54 450  Throws exceptions:
15:26:54 451  APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 452  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
15:26:54 453  */
15:26:54 454  	in_group_id	  IN NUMBER,
15:26:54 455  	out_result_gc_set OUT SYS_REFCURSOR,
15:26:54 456  	in_instr_status   IN NUMBER DEFAULT GLOBAL_CONSTANTS_V19.TRUE
15:26:54 457  ) AS
15:26:54 458  SPROC_NAME     CONSTANT VARCHAR2(29) := 'GET_ACCOUNT_GIFT_CERTIFICATES';
15:26:54 459  var_account_id NUMBER;
15:26:54 460  
15:26:54 461  -- Exceptions
15:26:54 462  WRONG_INSTR_EXCEPTION	 EXCEPTION;
15:26:54 463  BEGIN
15:26:54 464  
15:26:54 465  	-- Get account id
15:26:54 466  	SELECT
15:26:54 467  	  ACCOUNT.ID INTO var_account_id
15:26:54 468  	FROM
15:26:54 469  	  ACCOUNT
15:26:54 470  	WHERE
15:26:54 471  	  ACCOUNT.GROUP_ID = in_group_id;
15:26:54 472  
15:26:54 473  	-- Check that incoming data is correct
15:26:54 474  	IF in_instr_status != GLOBAL_CONSTANTS_V19.TRUE AND in_instr_status != GLOBAL_CONSTANTS_V19.FALSE THEN
15:26:54 475  	  RAISE WRONG_INSTR_EXCEPTION;
15:26:54 476  	END IF;
15:26:54 477  
15:26:54 478  	OPEN out_result_gc_set FOR
15:26:54 479  	SELECT
15:26:54 480  	  GIFT_CERTIFICATE.ID,
15:26:54 481  	  GIFT_CERTIFICATE.PURCHASER_GROUP_ID,
15:26:54 482  	  GIFT_CERTIFICATE.PURCHASE_INVOICE_ID,
15:26:54 483  	  GIFT_CERTIFICATE.PURCHASE_DATE,
15:26:54 484  	  GIFT_CERTIFICATE.OFFER_CHAIN_ID,
15:26:54 485  	  GIFT_CERTIFICATE.EXPIRATION_DATE,
15:26:54 486  	  GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID,
15:26:54 487  	  GIFT_CERTIFICATE.CODE,
15:26:54 488  	  GIFT_CERTIFICATE.CREATE_DATE,
15:26:54 489  	  GIFT_CERTIFICATE.CREATED_BY,
15:26:54 490  	  GIFT_CERTIFICATE.UPDATE_DATE,
15:26:54 491  	  GIFT_CERTIFICATE.UPDATED_BY,
15:26:54 492  	  GIFT_CERTIFICATE.RECIPIENT_NAME,
15:26:54 493  	  GIFT_CERTIFICATE.RECIPIENT_EMAIL,
15:26:54 494  	  GIFT_CERTIFICATE.SENDER_NAME,
15:26:54 495  	  GIFT_CERTIFICATE.SENDER_EMAIL,
15:26:54 496  	  GIFT_CERTIFICATE.REDEEMER_GROUP_ID,
15:26:54 497  	  GIFT_CERTIFICATE.REDEMPTION_DATE,
15:26:54 498  	  GIFT_CERTIFICATE.FINALIZED_INVOICE_ID,
15:26:54 499  	  GIFT_CERTIFICATE.GIFT_MESSAGE
15:26:54 500  	FROM
15:26:54 501  	  GIFT_CERTIFICATE
15:26:54 502  	WHERE
15:26:54 503  	  GIFT_CERTIFICATE.PURCHASER_GROUP_ID = in_group_id
15:26:54 504  	  AND (
15:26:54 505  		( in_instr_status = GLOBAL_CONSTANTS_V19.TRUE AND
15:26:54 506  		  (
15:26:54 507  		    GIFT_CERTIFICATE.EXPIRATION_DATE >= current_date
15:26:54 508  		    AND GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID = GLOBAL_STATUSES_V19.GIFT_CERTIFICATE_ACTIVE
15:26:54 509  		  )
15:26:54 510  		)
15:26:54 511  		OR
15:26:54 512  		(
15:26:54 513  		  in_instr_status = GLOBAL_CONSTANTS_V19.FALSE AND
15:26:54 514  		  (
15:26:54 515  		    GIFT_CERTIFICATE.EXPIRATION_DATE < current_date
15:26:54 516  		    OR GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID != GLOBAL_STATUSES_V19.GIFT_CERTIFICATE_ACTIVE
15:26:54 517  		  )
15:26:54 518  		)
15:26:54 519  	      );
15:26:54 520  
15:26:54 521  EXCEPTION
15:26:54 522  WHEN NO_DATA_FOUND THEN
15:26:54 523  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 524  	  SPROC_NAME, 'Can not find account with given group id');
15:26:54 525  WHEN WRONG_INSTR_EXCEPTION THEN
15:26:54 526  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
15:26:54 527  	  SPROC_NAME, 'Wrong gift certificate type');
15:26:54 528  WHEN OTHERS THEN
15:26:54 529  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:54 530  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:26:54 531  END GET_ACCOUNT_GIFT_CERTIFICATES;
