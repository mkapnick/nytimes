15:26:54 1658  
15:26:54 1659  PROCEDURE REFUND_INVOICE (
15:26:54 1660  	 in_invoice_id	    IN NUMBER,
15:26:54 1661  	 in_refund_amount   IN NUMBER,
15:26:54 1662  	 in_note	    IN VARCHAR2,
15:26:54 1663  	 in_created_by	    IN VARCHAR2,
15:26:54 1664  	 out_charge_id	    OUT NUMBER
15:26:54 1665  ) AS
15:26:54 1666  SPROC_NAME CONSTANT VARCHAR2(14) := 'REFUND_INVOICE';
15:26:54 1667  -- VARIABLES
15:26:54 1668  var_invoice_status_id  NUMBER;
15:26:54 1669  var_subscription_id    NUMBER;
15:26:54 1670  var_account_id	      NUMBER;
15:26:54 1671  var_group_id	      NUMBER;
15:26:54 1672  var_account_status_id  NUMBER;
15:26:54 1673  var_new_transaction_id NUMBER;
15:26:54 1674  var_instrument_type_id NUMBER;
15:26:54 1675  var_instrument_id      NUMBER;
15:26:54 1676  var_new_charge_id      NUMBER;
15:26:54 1677  var_invoice_amount     NUMBER(10,2);
15:26:54 1678  var_refunds_before     NUMBER(10,2);
15:26:54 1679  var_charges_amount     NUMBER(10,2);
15:26:54 1680  -- EXCEPTIONS
15:26:54 1681  CAN_NOT_FIND_SUBSCR_OR_GC     EXCEPTION;
15:26:54 1682  ACCOUNT_IS_FROZEN	     EXCEPTION;
15:26:54 1683  BAD_INVOICE_ID		     EXCEPTION;
15:26:54 1684  CAN_NOT_CREATE_TRANSACTION    EXCEPTION;
15:26:54 1685  CAN_NOT_CREATE_CHARGE	     EXCEPTION;
15:26:54 1686  CAN_NOT_CALC_INVOICE_AMOUNT   EXCEPTION;
15:26:54 1687  REFUND_IS_GREATER_THAN_ANOUNT EXCEPTION;
15:26:54 1688  CAN_NOT_ANNOTATE_SUBSCRIPTION EXCEPTION;
15:26:54 1689  TOT_REF_IS_GREATER_THAN_ANOUNT EXCEPTION;
15:26:54 1690  INVOICE_IS_NOT_CLOSED	     EXCEPTION;
15:26:54 1691  TOT_REF_IS_GRATER_THAN_CHARGES EXCEPTION;
15:26:54 1692  EXCEPTION_MESSAGE	      VARCHAR2(1024);
15:26:54 1693  BEGIN
15:26:54 1694  
15:26:54 1695  	 BEGIN
15:26:54 1696  	   SELECT
15:26:54 1697  	     INVOICE.INVOICE_STATUS_ID into var_invoice_status_id
15:26:54 1698  	   FROM
15:26:54 1699  	     INVOICE
15:26:54 1700  	   WHERE
15:26:54 1701  	     INVOICE.ID = in_invoice_id;
15:26:54 1702  	   EXCEPTION
15:26:54 1703  	     WHEN NO_DATA_FOUND THEN
15:26:54 1704  	       RAISE BAD_INVOICE_ID;
15:26:54 1705  	 END;
15:26:54 1706  
15:26:54 1707  	 -- Get instrument and subscription id if exists
15:26:54 1708  	 BEGIN
15:26:54 1709  	   SELECT
15:26:54 1710  	     SUBSCRIPTION.INSTRUMENT_ID,
15:26:54 1711  	     SUBSCRIPTION.INSTRUMENT_TYPE_ID,
15:26:54 1712  	     SUBSCRIPTION.ACCOUNT_ID,
15:26:54 1713  	     SUBSCRIPTION.ID
15:26:54 1714  	     into
15:26:54 1715  	     var_instrument_id,
15:26:54 1716  	     var_instrument_type_id,
15:26:54 1717  	     var_account_id,
15:26:54 1718  	     var_subscription_id
15:26:54 1719  	   FROM
15:26:54 1720  	     SUBSCRIPTION
15:26:54 1721  	     INNER JOIN LICENSE ON SUBSCRIPTION.ID = LICENSE.SUBSCRIPTION_ID
15:26:54 1722  	   WHERE
15:26:54 1723  	     LICENSE.INVOICE_ID = in_invoice_id
15:26:54 1724  	     AND ROWNUM <= 1;
15:26:54 1725  	   EXCEPTION
15:26:54 1726  	     WHEN NO_DATA_FOUND THEN
15:26:54 1727  	       BEGIN
15:26:54 1728  
15:26:54 1729  		 var_subscription_id := NULL;
15:26:54 1730  
15:26:54 1731  		 SELECT
15:26:54 1732  		   CHARGE.INSTRUMENT_ID,
15:26:54 1733  		   CHARGE.INSTRUMENT_TYPE_ID,
15:26:54 1734  		   GIFT_CERTIFICATE.PURCHASER_GROUP_ID
15:26:54 1735  		   into
15:26:54 1736  		   var_instrument_id,
15:26:54 1737  		   var_instrument_type_id,
15:26:54 1738  		   var_group_id
15:26:54 1739  		 FROM
15:26:54 1740  		   INVOICE
15:26:54 1741  		   INNER JOIN CHARGE ON INVOICE.ID = CHARGE.INVOICE_ID
15:26:54 1742  		   INNER JOIN GIFT_CERTIFICATE ON GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = INVOICE.ID
15:26:54 1743  		 WHERE
15:26:54 1744  		   INVOICE.ID = in_invoice_id
15:26:54 1745  		   AND ROWNUM <= 1;
15:26:54 1746  
15:26:54 1747  		 SELECT
15:26:54 1748  		   ACCOUNT.ID into var_account_id
15:26:54 1749  		 FROM
15:26:54 1750  		   ACCOUNT
15:26:54 1751  		 WHERE
15:26:54 1752  		   ACCOUNT.GROUP_ID = var_group_id;
15:26:54 1753  
15:26:54 1754  		 EXCEPTION
15:26:54 1755  		   WHEN NO_DATA_FOUND THEN
15:26:54 1756  		     RAISE CAN_NOT_FIND_SUBSCR_OR_GC;
15:26:54 1757  	       END;
15:26:54 1758  	 END;
15:26:54 1759  
15:26:54 1760  	 -- Check account status. It should not to be frozen
15:26:54 1761  	 SELECT
15:26:54 1762  	   ACCOUNT.ACCOUNT_STATUS_ID into var_account_status_id
15:26:54 1763  	 FROM
15:26:54 1764  	   ACCOUNT
15:26:54 1765  	 WHERE
15:26:54 1766  	   ACCOUNT.ID = var_account_id;
15:26:54 1767  
15:26:54 1768  	 IF var_account_status_id = GLOBAL_STATUSES_V19.ACCOUNT_FROZEN THEN
15:26:54 1769  	   RAISE ACCOUNT_IS_FROZEN;
15:26:54 1770  	 END IF;
15:26:54 1771  
15:26:54 1772  	 IF var_invoice_status_id != GLOBAL_STATUSES_V19.INVOICE_CLOSED THEN
15:26:54 1773  	   RAISE INVOICE_IS_NOT_CLOSED;
15:26:54 1774  	 END IF;
15:26:54 1775  
15:26:54 1776  	 BEGIN
15:26:54 1777  	   PROCS_INVOICE_V19.CALCULATE_INVOICE_AMOUNT (
15:26:54 1778  	     in_invoice_id => in_invoice_id,
15:26:54 1779  	     out_amount    => var_invoice_amount
15:26:54 1780  	   );
15:26:54 1781  	   EXCEPTION
15:26:54 1782  	     WHEN OTHERS THEN
15:26:54 1783  	       EXCEPTION_MESSAGE := SQLERRM;
15:26:54 1784  	       RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
15:26:54 1785  	 END;
15:26:54 1786  
15:26:54 1787  	 IF ( in_refund_amount > var_invoice_amount ) THEN
15:26:54 1788  	   RAISE REFUND_IS_GREATER_THAN_ANOUNT;
15:26:54 1789  	 END IF;
15:26:54 1790  
15:26:54 1791  	 SELECT /*+ STAR_TRANSFORMATION */
15:26:54 1792  	   SUM(CHARGE.CHARGE_AMOUNT) into var_refunds_before
15:26:54 1793  	 FROM
15:26:54 1794  	   CHARGE
15:26:54 1795  	 WHERE
15:26:54 1796  	   CHARGE.INVOICE_ID = in_invoice_id
15:26:54 1797  	   AND (
15:26:54 1798  	     CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V19.CHARGE_OPENED
15:26:54 1799  	     OR (
15:26:54 1800  	       CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V19.CHARGE_PROCESSED
15:26:54 1801  	       AND EXISTS (
15:26:54 1802  		 SELECT 1 FROM TRANSACTION_ATTEMPT ta where ta.transaction_id = CHARGE.TRANSACTION_ID and ta.transaction_attempt_status_id = GLOBAL_STATUSES_V19.TRANS_ATTEMPT_SUCCESS
15:26:54 1803  	       )
15:26:54 1804  	     )
15:26:54 1805  	   )
15:26:54 1806  	   AND CHARGE.CHARGE_AMOUNT < 0;
15:26:54 1807  
15:26:54 1808  	 -- Refunds are negative
15:26:54 1809  	 IF var_refunds_before IS NULL THEN var_refunds_before := 0; END IF;
15:26:54 1810  	 var_refunds_before := 0 - var_refunds_before;
15:26:54 1811  
15:26:54 1812  	 var_charges_amount := 0;
15:26:54 1813  
15:26:54 1814  	 FOR f_processed_charges IN (
15:26:54 1815  	   SELECT
15:26:54 1816  	     CHARGE.CHARGE_AMOUNT
15:26:54 1817  	   FROM
15:26:54 1818  	     CHARGE
15:26:54 1819  	   WHERE
15:26:54 1820  	     CHARGE.INVOICE_ID = in_invoice_id
15:26:54 1821  	     AND CHARGE.CHARGE_AMOUNT > 0
15:26:54 1822  	     AND CHARGE.CHARGE_STATUS_ID IN (SELECT GLOBAL_STATUSES_V19.CHARGE_PROCESSED FROM DUAL)
15:26:54 1823  	     AND EXISTS (SELECT 1 FROM TRANSACTION_ATTEMPT ta where ta.transaction_id = CHARGE.TRANSACTION_ID and ta.transaction_attempt_status_id = GLOBAL_STATUSES_V19.TRANS_ATTEMPT_SUCCESS)
15:26:54 1824  	 )
15:26:54 1825  	 LOOP
15:26:54 1826  	   var_charges_amount := var_charges_amount + f_processed_charges.CHARGE_AMOUNT;
15:26:54 1827  	 END LOOP;
15:26:54 1828  
15:26:54 1829  	 IF (in_refund_amount + var_refunds_before > var_invoice_amount) THEN
15:26:54 1830  	   RAISE TOT_REF_IS_GREATER_THAN_ANOUNT;
15:26:54 1831  	 END IF;
15:26:54 1832  
15:26:54 1833  	 IF (in_refund_amount + var_refunds_before > var_charges_amount) THEN
15:26:54 1834  	   RAISE TOT_REF_IS_GRATER_THAN_CHARGES;
15:26:54 1835  	 END IF;
15:26:54 1836  
15:26:54 1837  	 BEGIN
15:26:54 1838  	   PROCS_TRANSACTION_V19.CREATE_TRANSACTION(
15:26:54 1839  	     in_transaction_id	       => NULL,
15:26:54 1840  	     in_status_id	       => GLOBAL_STATUSES_V19.TRANSACTION_PREPARE,
15:26:54 1841  	     in_amount		       => -in_refund_amount,
15:26:54 1842  	     in_created_by	       => in_created_by,
15:26:54 1843  	     in_order_id	       => NULL,
15:26:54 1844  	     in_is_refund	       => GLOBAL_CONSTANTS_V19.TRUE,
15:26:54 1845  	     in_transaction_type_code  => 'REFUND',
15:26:54 1846  	     out_transaction_id        => var_new_transaction_id
15:26:54 1847  	   );
15:26:54 1848  	   EXCEPTION
15:26:54 1849  	     WHEN OTHERS THEN
15:26:54 1850  	       EXCEPTION_MESSAGE := SQLERRM;
15:26:54 1851  	       RAISE CAN_NOT_CREATE_TRANSACTION;
15:26:54 1852  	 END;
15:26:54 1853  
15:26:54 1854  	 BEGIN
15:26:54 1855  	   PROCS_CHARGE_V19.CREATE_CHARGE(
15:26:54 1856  	     in_invoice_id	   => in_invoice_id,
15:26:54 1857  	     in_transaction_id	   => var_new_transaction_id,
15:26:54 1858  	     in_instrument_type_id => var_instrument_type_id,
15:26:54 1859  	     in_instrument_id	   => var_instrument_id,
15:26:54 1860  	     in_charge_amount	   => -in_refund_amount,
15:26:54 1861  	     in_created_by	   => in_created_by,
15:26:54 1862  	     in_charge_status_id   => GLOBAL_STATUSES_V19.CHARGE_OPENED,
15:26:54 1863  	     out_charge_id	   => var_new_charge_id
15:26:54 1864  	   );
15:26:54 1865  	   EXCEPTION
15:26:54 1866  	     WHEN OTHERS THEN
15:26:54 1867  	       EXCEPTION_MESSAGE := SQLERRM;
15:26:54 1868  	       RAISE CAN_NOT_CREATE_CHARGE;
15:26:54 1869  	 END;
15:26:54 1870  
15:26:54 1871  	 out_charge_id := var_new_charge_id;
15:26:54 1872  
15:26:54 1873  	 IF in_note IS NOT NULL AND var_subscription_id IS NOT NULL THEN
15:26:54 1874  	   BEGIN
15:26:54 1875  	     PROCS_SUBSCRIPTION_V19.ANNOTATE_SUBSCRIPTION(
15:26:54 1876  	       in_subscription_id => var_subscription_id,
15:26:54 1877  	       in_agent_id	  => 0, -- AGENT_ID??
15:26:54 1878  	       in_note		  => in_note,
15:26:54 1879  	       in_created_by	  => in_created_by
15:26:54 1880  	     );
15:26:54 1881  	     EXCEPTION
15:26:54 1882  	       WHEN OTHERS THEN
15:26:54 1883  		 EXCEPTION_MESSAGE := SQLERRM;
15:26:54 1884  		 RAISE CAN_NOT_ANNOTATE_SUBSCRIPTION;
15:26:54 1885  	   END;
15:26:54 1886  	 END IF;
15:26:54 1887  
15:26:54 1888  EXCEPTION
15:26:54 1889  WHEN CAN_NOT_FIND_SUBSCR_OR_GC THEN
15:26:54 1890  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 1891  	   SPROC_NAME, 'Could not find subscription or GC for the inovice');
15:26:54 1892  WHEN INVOICE_IS_NOT_CLOSED THEN
15:26:54 1893  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
15:26:54 1894  	   SPROC_NAME, 'Invoice is not closed');
15:26:54 1895  WHEN ACCOUNT_IS_FROZEN THEN
15:26:54 1896  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
15:26:54 1897  	   SPROC_NAME, 'Could not refund subscription for frozen account');
15:26:54 1898  WHEN BAD_INVOICE_ID THEN
15:26:54 1899  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 1900  	   SPROC_NAME, 'No such invoice');
15:26:54 1901  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
15:26:54 1902  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 1903  	   SPROC_NAME, 'Could not calculate invoice amount', EXCEPTION_MESSAGE);
15:26:54 1904  WHEN REFUND_IS_GREATER_THAN_ANOUNT THEN
15:26:54 1905  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
15:26:54 1906  	   SPROC_NAME, 'Refund is greater than amount');
15:26:54 1907  WHEN TOT_REF_IS_GREATER_THAN_ANOUNT THEN
15:26:54 1908  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
15:26:54 1909  	   SPROC_NAME, 'There were refunds before and sum of all refunds and new refund more than invoice amount');
15:26:54 1910  WHEN TOT_REF_IS_GRATER_THAN_CHARGES THEN
15:26:54 1911  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
15:26:54 1912  	   SPROC_NAME, 'Total refund amount is greater than sum of processed charges');
15:26:54 1913  WHEN CAN_NOT_CREATE_TRANSACTION THEN
15:26:54 1914  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 1915  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
15:26:54 1916  WHEN CAN_NOT_CREATE_CHARGE THEN
15:26:54 1917  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 1918  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
15:26:54 1919  WHEN CAN_NOT_ANNOTATE_SUBSCRIPTION THEN
15:26:54 1920  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 1921  	   SPROC_NAME, 'Could not annotate subscription', EXCEPTION_MESSAGE);
15:26:54 1922  WHEN OTHERS THEN
15:26:54 1923  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:54 1924  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:26:54 1925  END REFUND_INVOICE;
