15:26:54 1327  	 in_updated_by IN VARCHAR2
15:26:54 1328  ) AS
15:26:54 1329  SPROC_NAME CONSTANT VARCHAR2(30) := 'CLOSE_INVOICE_AS_NOT_COLLECTED';
15:26:54 1330  -- VARIABLES
15:26:54 1331  temp_invoice_id NUMBER;
15:26:54 1332  -- EXCEPTIONS
15:26:54 1333  BAD_INVOICE_ID EXCEPTION;
15:26:54 1334  BEGIN
15:26:54 1335  
15:26:54 1336  	 BEGIN
15:26:54 1337  	   SELECT
15:26:54 1338  	     INVOICE.ID into temp_invoice_id
15:26:54 1339  	   FROM
15:26:54 1340  	     INVOICE
15:26:54 1341  	   WHERE
15:26:54 1342  	     INVOICE.ID = in_invoice_id;
15:26:54 1343  	   EXCEPTION
15:26:54 1344  	     WHEN NO_DATA_FOUND THEN
15:26:54 1345  	       RAISE BAD_INVOICE_ID;
15:26:54 1346  	 END;
15:26:54 1347  
15:26:54 1348  	 -- Needs to close charges. No refund.
15:26:54 1349  	 FOR f_charge_to_close IN (
15:26:54 1350  	   SELECT CHARGE.ID FROM CHARGE WHERE CHARGE.INVOICE_ID = in_invoice_id
15:26:54 1351  	 )
15:26:54 1352  	 LOOP
