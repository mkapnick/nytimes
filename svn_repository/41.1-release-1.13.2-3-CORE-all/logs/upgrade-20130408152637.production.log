15:26:54 451  
15:26:54 452  PROCEDURE DISABLE_PAYPAL (
15:26:54 453  /*
15:26:54 454  Throws exceptions:
15:26:54 455  APP_EXCEPTION_CODES_V19.NOT_FOUND
15:26:54 456  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
15:26:54 457  APP_EXCEPTION_CODES_V19.INTERNAL_ERROR
15:26:54 458  */
15:26:54 459  	in_paypal_id  IN NUMBER,
15:26:54 460  	in_updated_by IN VARCHAR2
15:26:54 461  ) AS
15:26:54 462  SPROC_NAME CONSTANT VARCHAR2(14) := 'DISABLE_PAYPAL';
15:26:54 463  -- VARIABLES
15:26:54 464  var_paypal_status_id NUMBER;
15:26:54 465  var_account_id	   NUMBER;
15:26:54 466  var_group_id	   NUMBER;
15:26:54 467  var_pending_invoices_num NUMBER;
15:26:54 468  current_def_instrument_type_id NUMBER;
15:26:54 469  current_def_instrument_id NUMBER;
15:26:54 470  -- EXCEPTIONS
15:26:54 471  BAD_PAYPAL_ID EXCEPTION;
15:26:54 472  PAYPAL_ALREADY_INACTIVE EXCEPTION;
15:26:54 473  PENDING_TRANSACTIONS_FOUND EXCEPTION;
15:26:54 474  CAN_NOT_GET_DEF_FINANCIAL EXCEPTION;
15:26:54 475  CAN_NOT_DEL_DEF_FINANCIAL EXCEPTION;
15:26:54 476  EXCEPTION_MESSAGE VARCHAR2(1024);
15:26:54 477  BEGIN
15:26:54 478  
15:26:54 479  	-- Get Paypal status
15:26:54 480  	-- Get account
15:26:54 481  	BEGIN
15:26:54 482  	  SELECT
15:26:54 483  	    PAYPAL.PAYPAL_STATUS_ID,
15:26:54 484  	    PAYPAL.ACCOUNT_ID
15:26:54 485  	    into
15:26:54 486  	    var_paypal_status_id,
15:26:54 487  	    var_account_id
15:26:54 488  	  FROM
15:26:54 489  	    PAYPAL
15:26:54 490  	  WHERE
15:26:54 491  	    PAYPAL.ID = in_paypal_id;
15:26:54 492  	  EXCEPTION
15:26:54 493  	    WHEN NO_DATA_FOUND THEN
15:26:54 494  	      RAISE BAD_PAYPAL_ID;
15:26:54 495  	END;
15:26:54 496  
15:26:54 497  	-- Get group id
15:26:54 498  	SELECT
15:26:54 499  	  ACCOUNT.GROUP_ID into var_group_id
15:26:54 500  	FROM
15:26:54 501  	  ACCOUNT
15:26:54 502  	WHERE
15:26:54 503  	  ACCOUNT.ID = var_account_id;
15:26:54 504  
15:26:54 505  	-- Card should not be disabled
15:26:54 506  	IF var_paypal_status_id = GLOBAL_STATUSES_V19.PAYPAL_INACTIVE THEN
15:26:54 507  	  RAISE PAYPAL_ALREADY_INACTIVE;
15:26:54 508  	END IF;
15:26:54 509  
15:26:54 510  	-- Looking for pending transactions associated with given credit card
15:26:54 511  	SELECT
15:26:54 512  	  COUNT(*) into var_pending_invoices_num
15:26:54 513  	FROM
15:26:54 514  	  CHARGE
15:26:54 515  	  INNER JOIN TRANSACTION ON CHARGE.TRANSACTION_ID = TRANSACTION.ID
15:26:54 516  	WHERE
15:26:54 517  	  CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V19.INSTRUMENT_PAYPAL
15:26:54 518  	  AND CHARGE.INSTRUMENT_ID = in_paypal_id
15:26:54 519  	  AND TRANSACTION.TRANSACTION_STATUS_ID = GLOBAL_STATUSES_V19.TRANSACTION_PENDING;
15:26:54 520  
15:26:54 521  	IF var_pending_invoices_num > 0 THEN
15:26:54 522  	  RAISE PENDING_TRANSACTIONS_FOUND;
15:26:54 523  	END IF;
15:26:54 524  
15:26:54 525  	-- Getting current default financial instrument
15:26:54 526  	BEGIN
15:26:54 527  	  GET_DEF_FINANCIAL_INSTRUMENT(
15:26:54 528  	    in_group_id 	   => var_group_id,
15:26:54 529  	    out_instrument_type_id => current_def_instrument_type_id,
15:26:54 530  	    out_instrument_id	   => current_def_instrument_id
15:26:54 531  	  );
15:26:54 532  	  EXCEPTION
15:26:54 533  	    WHEN OTHERS THEN
15:26:54 534  	      EXCEPTION_MESSAGE := SQLERRM;
15:26:54 535  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
15:26:54 536  	END;
15:26:54 537  
15:26:54 538  	-- Checking that credit card is not default
15:26:54 539  	IF current_def_instrument_type_id = GLOBAL_ENUMS_V19.INSTRUMENT_PAYPAL
15:26:54 540  	  AND current_def_instrument_id = in_paypal_id THEN
15:26:54 541  	  BEGIN
15:26:54 542  	    DEL_DEF_FINANCIAL_INSTRUMENT(
15:26:54 543  	      in_group_id => var_group_id
15:26:54 544  	    );
15:26:54 545  	  EXCEPTION
15:26:54 546  	    WHEN OTHERS THEN
15:26:54 547  	      EXCEPTION_MESSAGE := SQLERRM;
15:26:54 548  	      RAISE CAN_NOT_DEL_DEF_FINANCIAL;
15:26:54 549  	  END;
15:26:54 550  	END IF;
15:26:54 551  
15:26:54 552  	PROCS_FIN_INSTRUMENTS_V19.UPDATE_PAYPAL_STATUS(
15:26:54 553  	  in_paypal_id	      => in_paypal_id,
15:26:54 554  	  in_updated_by       => in_updated_by,
15:26:54 555  	  in_paypal_status_id => GLOBAL_STATUSES_V19.PAYPAL_INACTIVE
15:26:54 556  	);
15:26:54 557  
15:26:54 558  EXCEPTION
15:26:54 559  WHEN BAD_PAYPAL_ID THEN
15:26:54 560  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 561  	  SPROC_NAME, 'No such paypal');
15:26:54 562  WHEN PAYPAL_ALREADY_INACTIVE THEN
15:26:54 563  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
15:26:54 564  	  SPROC_NAME, 'Paypal already inactive');
15:26:54 565  WHEN PENDING_TRANSACTIONS_FOUND THEN
15:26:54 566  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
15:26:54 567  	  SPROC_NAME, 'Account has pending charge which are using this paypal');
15:26:54 568  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
15:26:54 569  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 570  	  SPROC_NAME, 'Could not get current default financial instrument', EXCEPTION_MESSAGE);
15:26:54 571  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
15:26:54 572  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 573  	  SPROC_NAME, 'Could not delete information about default financial instrument from account', EXCEPTION_MESSAGE);
15:26:54 574  WHEN OTHERS THEN
15:26:54 575  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:54 576  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:26:54 577  END DISABLE_PAYPAL;
