15:26:53 2930  	   EXCEPTION
15:26:53 2931  	     WHEN NO_DATA_FOUND THEN
15:26:53 2932  	       RAISE BAD_GROUP_ID;
15:26:53 2933  	 END;
15:26:53 2934  	 -- Check that offer chain id exists
15:26:53 2935  	 BEGIN
