15:26:54 823  
15:26:54 824  PROCEDURE SHOULD_MOVE_TO_GRACE(
15:26:54 825  	in_invoice_id  IN INVOICE.ID%TYPE,
15:26:54 826  	out_result     OUT NUMBER
15:26:54 827  ) AS
15:26:54 828  BEGIN
15:26:54 829  	-- if the invoice preceding the given invoice has no transaction attempts, then
15:26:54 830  	-- it is not billed out of Sartre. if so, then the associated subscription
15:26:54 831  	-- should be canceled after a final failed billing attempt--not moved to grace.
15:26:54 832  	SELECT DECODE(COUNT(1), 0, 0, 1) INTO out_result
15:26:54 833  	FROM CHARGE c
15:26:54 834  	INNER JOIN TRANSACTION t ON c.TRANSACTION_ID = t.ID
15:26:54 835  	INNER JOIN TRANSACTION_ATTEMPT ta ON ta.TRANSACTION_ID = t.ID
15:26:54 836  	WHERE c.INVOICE_ID = (
15:26:54 837  	  -- select previous invoice_id, or -1 if there is none
15:26:54 838  	  SELECT PREV_INVOICE_ID FROM (
15:26:54 839  	    SELECT i.ID, LAG(i.ID, 1, -1) OVER (ORDER BY i.CREATE_DATE) AS PREV_INVOICE_ID
15:26:54 840  	    FROM INVOICE i
15:26:54 841  	    INNER JOIN LICENSE l ON i.ID = l.INVOICE_ID
15:26:54 842  	    WHERE l.SUBSCRIPTION_ID = (
15:26:54 843  	      SELECT SUBSCRIPTION_ID FROM LICENSE WHERE INVOICE_ID = in_invoice_id
15:26:54 844  	    )
15:26:54 845  	  ) WHERE ID = in_invoice_id
15:26:54 846  	);
15:26:54 847  END SHOULD_MOVE_TO_GRACE;
