15:26:52   5  	PROCEDURE POPULATE_REQUEST_INFO(
15:26:52   6  	  in_hours_prior    IN	NUMBER,
15:26:52   7  	  in_filename	    IN	CC_REQUEST_FILE.FILE_NAME%TYPE,
15:26:52   8  	  in_creator	    IN	CC_REQUEST_FILE.UPDATED_BY%TYPE
15:26:52   9  	) AS
15:26:52  10  	SPROC_NAME CONSTANT VARCHAR2(32) := 'POPULATE_REQUEST_INFO';
15:26:52  11  	var_start_date	    DATE := SYSDATE;
15:26:52  12  	var_end_date	    DATE := var_start_date + (in_hours_prior/24);
15:26:52  13  	var_request_file_id NUMBER := 0;
15:26:52  14  	var_license_count   NUMBER := 0;
15:26:52  15  	var_cc_update_count NUMBER := 0;
15:26:52  16  	BEGIN
15:26:52  17  	    SELECT CC_REQUEST_FILE_ID_SEQ.NEXTVAL INTO var_request_file_id  FROM DUAL;
15:26:52  18  	    INSERT INTO CC_REQUEST_FILE (ID,
15:26:52  19  					 FILE_NAME,
15:26:52  20  					 CC_REQUEST_FILE_STATUS,
15:26:52  21  					 CREATE_DATE,
15:26:52  22  					 CREATED_BY,
15:26:52  23  					 UPDATE_DATE,
15:26:52  24  					 UPDATED_BY)
15:26:52  25  					 VALUES (
15:26:52  26  					 var_request_file_id,
15:26:52  27  					 in_filename,
15:26:52  28  					 'NOT_CREATED',
15:26:52  29  					 var_start_date,
15:26:52  30  					 in_creator,
15:26:52  31  					 var_start_date,
15:26:52  32  					 in_creator);
15:26:52  33  
15:26:52  34  	   FOR record IN (SELECT
15:26:52  35  			    l.ID LICENSE_ID, cc.ID CREDIT_CARD_ID
15:26:52  36  			  FROM
15:26:52  37  			    LICENSE l INNER JOIN SUBSCRIPTION s ON L.SUBSCRIPTION_ID = s.ID
15:26:52  38  				      INNER JOIN CREDIT_CARD cc ON S.INSTRUMENT_ID   = cc.ID
15:26:52  39  			  WHERE
15:26:52  40  			    s.INSTRUMENT_TYPE_ID	 = 1
15:26:52  41  			    AND cc.CREDIT_CARD_STATUS_ID = 1
15:26:52  42  			    AND s.SUBSCRIPTION_STATUS_ID = 1
15:26:52  43  			    AND l.LICENSE_STATUS_ID	 = 2
15:26:52  44  			    AND cc.CREDIT_CARD_TYPE_ID IN (2,3)
15:26:52  45  			    AND l.END_DATE BETWEEN var_start_date AND var_end_date
15:26:52  46  			    AND l.ID NOT IN (SELECT LICENSE_ID FROM CC_UPDATE))
15:26:52  47  	   LOOP
15:26:52  48  	     var_license_count := 0;
15:26:52  49  	     SELECT COUNT(1) INTO  var_license_count FROM CC_UPDATE WHERE LICENSE_ID = record.LICENSE_ID;
15:26:52  50  
15:26:52  51  	     IF var_license_count = 0 THEN
15:26:52  52  		INSERT INTO CC_UPDATE (ID,
15:26:52  53  				       CREDIT_CARD_ID,
15:26:52  54  				       LICENSE_ID,
15:26:52  55  				       CC_UPDATE_STATUS,
15:26:52  56  				       CC_REQUEST_FILE_ID,
15:26:52  57  				       CREATE_DATE,
15:26:52  58  				       UPDATE_DATE,
15:26:52  59  				       CREATED_BY,
15:26:52  60  				       UPDATED_BY
15:26:52  61  				       ) VALUES (
15:26:52  62  				       CC_UPDATE_SEQ.NEXTVAL,
15:26:52  63  				       record.CREDIT_CARD_ID,
15:26:52  64  				       record.LICENSE_ID,
15:26:52  65  				       'NOT_ADDED_TO_FILE',
15:26:52  66  				       var_request_file_id,
15:26:52  67  				       var_start_date,
15:26:52  68  				       var_start_date,
15:26:52  69  				       in_creator,
15:26:52  70  				       in_creator
15:26:52  71  				       );
15:26:52  72  	     END IF;
15:26:52  73  	   END LOOP;
15:26:52  74  
15:26:52  75  	   SELECT COUNT(1) INTO var_cc_update_count
15:26:52  76  	   FROM CC_UPDATE
15:26:52  77  	   WHERE CC_REQUEST_FILE_ID = var_request_file_id;
15:26:52  78  	   IF var_cc_update_count <= 0 THEN
15:26:52  79  	     UPDATE CC_REQUEST_FILE
15:26:52  80  	     SET CC_REQUEST_FILE_STATUS = 'EMPTY'
15:26:52  81  	     WHERE ID = var_request_file_id;
15:26:52  82  	   END IF;
15:26:52  83  
15:26:52  84  	END POPULATE_REQUEST_INFO;
