15:26:53 1001  	   + GLOBAL_CONSTANTS_V19.ONE_DAY_INTERVAL * var_offer_days )
15:26:53 1002  	   + GLOBAL_CONSTANTS_V19.ONE_MONTH_INTERVAL * var_offer_months )
15:26:53 1003  	   + GLOBAL_CONSTANTS_V19.ONE_YEAR_INTERVAL * var_offer_years;
15:26:53 1004  
15:26:53 1005  	 out_days := var_offer_end_date - in_start_date;
