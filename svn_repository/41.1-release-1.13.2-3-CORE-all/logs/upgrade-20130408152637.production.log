15:26:54 1065  /******************************************************************************/
15:26:54 1066  
15:26:54 1067  PROCEDURE PURCHASE_GIFT_CERTIFICATE (
15:26:54 1068  	 in_group_id		   IN NUMBER,
15:26:54 1069  	 in_offer_chain_id	   IN VARCHAR2,
15:26:54 1070  	 in_gift_certificate_code  IN VARCHAR2,
15:26:54 1071  	 in_created_by		   IN VARCHAR2,
15:26:54 1072  	 in_recipient_name	   IN VARCHAR2,
15:26:54 1073  	 in_recipient_email	   IN VARCHAR2,
15:26:54 1074  	 in_sender_name 	   IN VARCHAR2,
15:26:54 1075  	 in_sender_email	   IN VARCHAR2,
15:26:54 1076  	 in_gift_message	   IN VARCHAR2,
15:26:54 1077  	 in_instrument_id	   IN NUMBER,
15:26:54 1078  	 in_instrument_type_id	   IN NUMBER,
15:26:54 1079  	 in_expiration_date	   IN DATE,
15:26:54 1080  	 in_order_id		   IN VARCHAR2,
15:26:54 1081  	 in_transaction_id	   IN NUMBER
15:26:54 1082  ) AS
15:26:54 1083  SPROC_NAME	  CONSTANT VARCHAR2(25) := 'PURCHASE_GIFT_CERTIFICATE';
15:26:54 1084  -- VARIABLES
15:26:54 1085  var_gift_cert_id   NUMBER;
15:26:54 1086  var_account_id	  NUMBER;
15:26:54 1087  var_invoice_amount NUMBER (10,2);
15:26:54 1088  var_new_invoice_id NUMBER;
15:26:54 1089  var_new_charge_id  NUMBER;
15:26:54 1090  var_och_is_gc	  NUMBER;
15:26:54 1091  var_offer_chain_status_id NUMBER;
15:26:54 1092  var_is_for_redemption	 NUMBER;
15:26:54 1093  var_account_tax_exempt_id VARCHAR2(255);
15:26:54 1094  
15:26:54 1095  temp_transaction_id_count NUMBER;
15:26:54 1096  var_transaction_id	 NUMBER;
15:26:54 1097  temp_gc_code VARCHAR2(255);
15:26:54 1098  
15:26:54 1099  var_invoice_status_id NUMBER;
15:26:54 1100  -- EXCEPTIONS
15:26:54 1101  CAN_NOT_CREATE_INVOICE		   EXCEPTION;
15:26:54 1102  CAN_NOT_CREATE_TRANSACTION	   EXCEPTION;
15:26:54 1103  CAN_NOT_CREATE_CHARGE		   EXCEPTION;
15:26:54 1104  CAN_NOT_CREATE_LINE_ITEMS	   EXCEPTION;
15:26:54 1105  BAD_GROUP_ID			   EXCEPTION;
15:26:54 1106  BAD_OFFER_CHAIN_ID		   EXCEPTION;
15:26:54 1107  OCH_IS_NOT_GIFT_CERTIFICATE	   EXCEPTION;
15:26:54 1108  TRANSACTION_EXISTS		   EXCEPTION;
15:26:54 1109  GC_CODE_ALREADY_EXISTS		   EXCEPTION;
15:26:54 1110  BAD_INSTRUMENT_TYPE		   EXCEPTION;
15:26:54 1111  BAD_CREDIT_CARD_ID		   EXCEPTION;
15:26:54 1112  BAD_PAYPAL_ID			   EXCEPTION;
15:26:54 1113  CAN_NOT_CALCULATE_OCH_AMOUNT	   EXCEPTION;
15:26:54 1114  BAD_OFFER_CHAIN_STATUS		   EXCEPTION;
15:26:54 1115  OFFER_REC_NUM_LESS_THAN_ONE	   EXCEPTION;
15:26:54 1116  CAN_NOT_CALC_INVOICE_AMOUNT	   EXCEPTION;
15:26:54 1117  CAN_NOT_USE_FCINSTR		   EXCEPTION;
15:26:54 1118  CAN_NOT_PURCHASE_GC_FOR_RDMPN	   EXCEPTION;
15:26:54 1119  EXCEPTION_MESSAGE		   VARCHAR2(1024);
15:26:54 1120  BEGIN
15:26:54 1121  
15:26:54 1122  	 -- Get account id
15:26:54 1123  	 BEGIN
15:26:54 1124  	   SELECT
15:26:54 1125  	     ACCOUNT.ID,
15:26:54 1126  	     ACCOUNT.TAX_EXEMPT_ID
