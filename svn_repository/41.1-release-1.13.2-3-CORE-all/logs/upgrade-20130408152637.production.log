15:26:54 1627  	     i.id = in_invoice_id;
15:26:54 1628  	   EXCEPTION
15:26:54 1629  	     WHEN NO_DATA_FOUND THEN
15:26:54 1630  	       RAISE BAD_INVOICE_ID;
15:26:54 1631  	 END;
15:26:54 1632  
15:26:54 1633  	 BEGIN
15:26:54 1634  	   PROCS_INVOICE_CRU_V19.UPDATE_INVOICE(
15:26:54 1635  	     in_invoice_id => in_invoice_id,
15:26:54 1636  	     in_updated_by => in_updated_by,
15:26:54 1637  	     in_is_tax_calculation_needed => in_is_tax_calculation_needed
15:26:54 1638  	   );
15:26:54 1639  	   EXCEPTION
