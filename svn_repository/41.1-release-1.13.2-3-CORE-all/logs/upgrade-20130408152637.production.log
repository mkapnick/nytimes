15:26:53 740  BAD_INVOICE_ID EXCEPTION;
15:26:53 741  BEGIN
15:26:53 742  
15:26:53 743  	BEGIN
15:26:53 744  	  SELECT
15:26:53 745  	    INVOICE.ID into temp_invoice_id
15:26:53 746  	  FROM
15:26:53 747  	    INVOICE
15:26:53 748  	  WHERE
15:26:53 749  	    INVOICE.ID = in_invoice_id;
15:26:53 750  	  EXCEPTION
15:26:53 751  	    WHEN NO_DATA_FOUND THEN
15:26:53 752  	      RAISE BAD_INVOICE_ID;
15:26:53 753  	END;
