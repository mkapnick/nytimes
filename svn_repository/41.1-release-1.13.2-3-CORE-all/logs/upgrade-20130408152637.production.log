15:26:53 774  temp_subscription_id NUMBER;
15:26:53 775  BEGIN
15:26:53 776  	-- Note: A subscription has one or more associated licenses, each of which has an associated invoice.
15:26:53 777  	-- Find associated LICENSES for the subscription by "LICENSE.subscription_id"
15:26:53 778  	--   for each license
15:26:53 779  	--     get associated invoice
