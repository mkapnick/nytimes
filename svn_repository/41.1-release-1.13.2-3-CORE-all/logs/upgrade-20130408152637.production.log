15:26:54 706  	in_offer_chain_id	  IN VARCHAR2,
15:26:54 707  	in_gift_certificate_code  IN  VARCHAR2,
15:26:54 708  	in_created_by		  IN  VARCHAR2,
15:26:54 709  	in_recipient_name	  IN  VARCHAR2,
15:26:54 710  	in_recipient_email	  IN  VARCHAR2,
15:26:54 711  	in_recipient_address_id   IN NUMBER,
15:26:54 712  	in_recipient_notify_date  IN DATE,
15:26:54 713  	in_sender_name		  IN VARCHAR2,
15:26:54 714  	in_sender_email 	  IN VARCHAR2,
15:26:54 715  	in_gift_message 	  IN  VARCHAR2,
15:26:54 716  	in_expiration_date	  IN DATE,
15:26:54 717  	out_gift_certificate_id   OUT NUMBER,
15:26:54 718  	out_invoice_id		  OUT NUMBER
15:26:54 719  ) AS
15:26:54 720  SPROC_NAME CONSTANT VARCHAR2(19) := 'START_GC_PURCHASING';
15:26:54 721  -- VARIABLES
15:26:54 722  var_account_id		NUMBER;
15:26:54 723  temp_gc_code		GIFT_CERTIFICATE.CODE%TYPE;
15:26:54 724  var_och_is_gc		NUMBER;
15:26:54 725  var_offer_chain_status_id NUMBER;
15:26:54 726  var_is_for_redemption	NUMBER;
15:26:54 727  var_new_invoice_id	NUMBER;
15:26:54 728  var_gift_cert_id		NUMBER;
15:26:54 729  var_account_tax_exempt_id VARCHAR2(255);
15:26:54 730  -- EXCEPTIONS
15:26:54 731  BAD_GROUP_ID		    EXCEPTION;
15:26:54 732  GC_CODE_ALREADY_EXISTS	    EXCEPTION;
15:26:54 733  BAD_OFFER_CHAIN_ID	    EXCEPTION;
15:26:54 734  OCH_IS_NOT_GIFT_CERTIFICATE   EXCEPTION;
15:26:54 735  BAD_OFFER_CHAIN_STATUS	    EXCEPTION;
15:26:54 736  CAN_NOT_PURCHASE_GC_FOR_RDMPN EXCEPTION;
15:26:54 737  CAN_NOT_CREATE_INVOICE	    EXCEPTION;
15:26:54 738  OFFER_REC_NUM_LESS_THAN_ONE   EXCEPTION;
15:26:54 739  CAN_NOT_CREATE_LINE_ITEMS     EXCEPTION;
15:26:54 740  
15:26:54 741  EXCEPTION_MESSAGE VARCHAR2(1024);
15:26:54 742  BEGIN
15:26:54 743  	-- Get account id
15:26:54 744  	BEGIN
15:26:54 745  	  SELECT
15:26:54 746  	    ACCOUNT.ID,
15:26:54 747  	    ACCOUNT.TAX_EXEMPT_ID
15:26:54 748  	    into
15:26:54 749  	    var_account_id,
15:26:54 750  	    var_account_tax_exempt_id
15:26:54 751  	  FROM
15:26:54 752  	    ACCOUNT
15:26:54 753  	  WHERE
15:26:54 754  	    ACCOUNT.GROUP_ID = in_group_id;
15:26:54 755  	  EXCEPTION
15:26:54 756  	  WHEN NO_DATA_FOUND THEN
15:26:54 757  	    RAISE BAD_GROUP_ID;
15:26:54 758  	END;
15:26:54 759  
15:26:54 760  	-- Check for the same code
15:26:54 761  	BEGIN
15:26:54 762  	  SELECT
15:26:54 763  	    GIFT_CERTIFICATE.CODE into temp_gc_code
15:26:54 764  	  FROM
15:26:54 765  	    GIFT_CERTIFICATE
15:26:54 766  	  WHERE
15:26:54 767  	    GIFT_CERTIFICATE.CODE = in_gift_certificate_code;
15:26:54 768  
15:26:54 769  	  RAISE GC_CODE_ALREADY_EXISTS;
15:26:54 770  
15:26:54 771  	  EXCEPTION
15:26:54 772  	    WHEN NO_DATA_FOUND THEN
15:26:54 773  	      NULL;
15:26:54 774  	END;
15:26:54 775  
15:26:54 776  	-- Get offer chain flag "is_gift_certificate"
15:26:54 777  	BEGIN
15:26:54 778  	  SELECT
15:26:54 779  	    OFFER_CHAIN.IS_GIFT_CERTIFICATE,
15:26:54 780  	    OFFER_CHAIN.OFFER_CHAIN_STATUS_ID
15:26:54 781  	    into
15:26:54 782  	    var_och_is_gc,
15:26:54 783  	    var_offer_chain_status_id
15:26:54 784  	  FROM
15:26:54 785  	    OFFER_CHAIN
15:26:54 786  	  WHERE
15:26:54 787  	    OFFER_CHAIN.ID = in_offer_chain_id;
15:26:54 788  	  EXCEPTION
15:26:54 789  	  WHEN NO_DATA_FOUND THEN
15:26:54 790  	    RAISE BAD_OFFER_CHAIN_ID;
15:26:54 791  	END;
15:26:54 792  
15:26:54 793  	IF var_och_is_gc != GLOBAL_CONSTANTS_V19.TRUE
15:26:54 794  	  OR var_och_is_gc IS NULL THEN
15:26:54 795  	  RAISE OCH_IS_NOT_GIFT_CERTIFICATE;
15:26:54 796  	END IF;
15:26:54 797  
15:26:54 798  	IF var_offer_chain_status_id != GLOBAL_STATUSES_V19.OFFER_CHAIN_ACTIVE THEN
15:26:54 799  	  RAISE BAD_OFFER_CHAIN_STATUS;
15:26:54 800  	END IF;
15:26:54 801  
15:26:54 802  	-- norlov: #38151 check if the OC is for Redemption:
15:26:54 803  	SELECT
15:26:54 804  	  COUNT(*) into var_is_for_redemption
15:26:54 805  	FROM
15:26:54 806  	  OFFER_CHAIN_ELIGIBILITY
15:26:54 807  	WHERE
15:26:54 808  	  OFFER_CHAIN_ELIGIBILITY.OFFER_CHAIN_ID = in_offer_chain_id
15:26:54 809  	  AND OFFER_CHAIN_ELIGIBILITY.NAME = GLOBAL_CONSTANTS_V19.GIFT_CERTIFICATE_REQUIRED
15:26:54 810  	  AND OFFER_CHAIN_ELIGIBILITY.VALUE = GLOBAL_CONSTANTS_V19.ELIGIBILITY_FLAG_SET;
15:26:54 811  
15:26:54 812  	IF var_is_for_redemption > 0 THEN
15:26:54 813  	  RAISE CAN_NOT_PURCHASE_GC_FOR_RDMPN;
15:26:54 814  	END IF;
15:26:54 815  
15:26:54 816  	-- Create new invoice
15:26:54 817  	BEGIN
15:26:54 818  	  PROCS_INVOICE_V19.CREATE_INVOICE(
15:26:54 819  	    in_invoice_status => GLOBAL_STATUSES_V19.INVOICE_OPEN,
15:26:54 820  	    in_created_by     => in_created_by,
15:26:54 821  	    in_tax_exempt_id  => var_account_tax_exempt_id,
15:26:54 822  	    out_invoice_id    => var_new_invoice_id
15:26:54 823  	  );
15:26:54 824  	  EXCEPTION
15:26:54 825  	    WHEN OTHERS THEN
15:26:54 826  	      EXCEPTION_MESSAGE := SQLERRM;
15:26:54 827  	      RAISE CAN_NOT_CREATE_INVOICE;
15:26:54 828  	END;
15:26:54 829  
15:26:54 830  	-- Add line items for new invoice
15:26:54 831  	BEGIN
15:26:54 832  	  FOR f_offer_data IN (
15:26:54 833  	    SELECT
15:26:54 834  	      OFFER_ID,
15:26:54 835  	      NUM_RECURRENCES
15:26:54 836  	    FROM
15:26:54 837  	      OFFER_OFFER_CHAIN
15:26:54 838  	    WHERE
15:26:54 839  	      OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
15:26:54 840  	  )
15:26:54 841  	  LOOP
15:26:54 842  	    IF f_offer_data.NUM_RECURRENCES < 1 THEN
15:26:54 843  	      RAISE OFFER_REC_NUM_LESS_THAN_ONE;
15:26:54 844  	    END IF;
15:26:54 845  	    FOR i_offer_recurrences_iterator IN 1..f_offer_data.NUM_RECURRENCES
15:26:54 846  	    LOOP
15:26:54 847  	      PROCS_LINE_ITEMS_V19.ADD_LINE_ITEMS(
15:26:54 848  		in_invoice_id => var_new_invoice_id,
15:26:54 849  		in_offer_id   => f_offer_data.OFFER_ID,
15:26:54 850  		in_created_by => in_created_by
15:26:54 851  	      );
15:26:54 852  	    END LOOP;
15:26:54 853  	  END LOOP;
15:26:54 854  
15:26:54 855  	  EXCEPTION
15:26:54 856  	    WHEN OTHERS THEN
15:26:54 857  	      EXCEPTION_MESSAGE := SQLERRM;
15:26:54 858  	      RAISE CAN_NOT_CREATE_LINE_ITEMS;
15:26:54 859  	END;
15:26:54 860  
15:26:54 861  	PROCS_FIN_INSTRUMENTS_CRU_V19.CREATE_GIFT_CERTIFICATE(
15:26:54 862  	  out_gift_certificate_id	=> var_gift_cert_id,
15:26:54 863  	  in_purchaser_group_id 	=> in_group_id,
15:26:54 864  	  in_purchaser_invoice_id	=> var_new_invoice_id,
15:26:54 865  	  in_offer_chain_id		=> in_offer_chain_id,
15:26:54 866  	  in_expiration_date		=> in_expiration_date,
15:26:54 867  	  in_purchase_date		=> SYSDATE,
15:26:54 868  	  in_gift_certificate_status_id => GLOBAL_STATUSES_V19.GIFT_CERTIFICATE_ACTIVE,
15:26:54 869  	  in_code			=> in_gift_certificate_code,
15:26:54 870  	  in_created_by 		=> in_created_by,
15:26:54 871  	  in_recipient_name		=> in_recipient_name,
15:26:54 872  	  in_gift_message		=> in_gift_message,
15:26:54 873  	  in_recipient_email		=> in_recipient_email,
15:26:54 874  	  in_sender_email		=> in_sender_email,
15:26:54 875  	  in_sender_name		=> in_sender_name,
15:26:54 876  	  in_recipient_address_id	=> in_recipient_address_id,
15:26:54 877  	  in_recipient_notify_date	=> in_recipient_notify_date
15:26:54 878  	);
15:26:54 879  
15:26:54 880  	out_gift_certificate_id := var_gift_cert_id;
15:26:54 881  	out_invoice_id := var_new_invoice_id;
15:26:54 882  
15:26:54 883  EXCEPTION
15:26:54 884  WHEN BAD_OFFER_CHAIN_STATUS THEN
15:26:54 885  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
15:26:54 886  	  SPROC_NAME, 'Offer chain is not active');
15:26:54 887  WHEN GC_CODE_ALREADY_EXISTS THEN
15:26:54 888  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.DUPLICATE_ERROR,
15:26:54 889  	  SPROC_NAME, 'Gift certificate with same code already exists');
15:26:54 890  WHEN OCH_IS_NOT_GIFT_CERTIFICATE THEN
15:26:54 891  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
15:26:54 892  	  SPROC_NAME, 'This offer chain can not be used for gift certificate');
15:26:54 893  WHEN CAN_NOT_PURCHASE_GC_FOR_RDMPN THEN
15:26:54 894  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
15:26:54 895  	  SPROC_NAME, 'This offer chain can not be purchased for gift certificate since it is for redemption');
15:26:54 896  WHEN CAN_NOT_CREATE_INVOICE THEN
15:26:54 897  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 898  	  SPROC_NAME, 'Could not create new invoice', EXCEPTION_MESSAGE);
15:26:54 899  WHEN CAN_NOT_CREATE_LINE_ITEMS THEN
15:26:54 900  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 901  	  SPROC_NAME, 'Could not create line items', EXCEPTION_MESSAGE);
15:26:54 902  WHEN BAD_GROUP_ID THEN
15:26:54 903  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 904  	  SPROC_NAME, 'No such group id');
15:26:54 905  WHEN BAD_OFFER_CHAIN_ID THEN
15:26:54 906  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 907  	  SPROC_NAME, 'No such offer chain');
15:26:54 908  WHEN OFFER_REC_NUM_LESS_THAN_ONE THEN
15:26:54 909  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 910  	  SPROC_NAME, 'Some offer has recurrences number less than 1');
15:26:54 911  WHEN OTHERS THEN
15:26:54 912  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:54 913  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:26:54 914  END START_GC_PURCHASING;
