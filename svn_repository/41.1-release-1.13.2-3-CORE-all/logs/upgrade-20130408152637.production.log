15:26:54 1829  
15:26:54 1830  	 -- Select all gift certificates with given code
15:26:54 1831  	 OPEN out_result_set FOR
15:26:54 1832  	 SELECT
15:26:54 1833  	   gc.EXPIRATION_DATE,
15:26:54 1834  	   ch.name,
15:26:54 1835  	   ch.id,
15:26:54 1836  	   gc.sender_email,
15:26:54 1837  	   gc.sender_name,
15:26:54 1838  	   gc.recipient_email,
15:26:54 1839  	   gc.recipient_name,
