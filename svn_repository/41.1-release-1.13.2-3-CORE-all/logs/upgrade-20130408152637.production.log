15:26:54 1615  -- EXCEPTIONS
15:26:54 1616  BAD_INVOICE_ID	      EXCEPTION;
15:26:54 1617  CAN_NOT_UPDATE_INVOCIE EXCEPTION;
15:26:54 1618  EXCEPTION_MESSAGE      VARCHAR2(1024);
15:26:54 1619  BEGIN
