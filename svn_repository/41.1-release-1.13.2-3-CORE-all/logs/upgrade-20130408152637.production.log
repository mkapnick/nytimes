15:26:54 2703  PROCEDURE SWITCH_FINANCIAL_INSTRUMENT (
15:26:54 2704  	 in_old_fin_instrument_id   IN NUMBER,
15:26:54 2705  	 in_old_fin_instrument_type IN NUMBER,
15:26:54 2706  	 in_new_fin_instrument_id   IN NUMBER,
15:26:54 2707  	 in_new_fin_instrument_type IN NUMBER,
15:26:54 2708  	 in_updated_by		    IN VARCHAR2
