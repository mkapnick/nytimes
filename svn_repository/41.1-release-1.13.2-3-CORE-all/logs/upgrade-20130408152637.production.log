15:26:54 697  /******************************************************************************/
15:26:54 698  
15:26:54 699  PROCEDURE CALCULATE_INVOICE_CHARGEBACK (
15:26:54 700  	in_invoice_id	      IN NUMBER,
15:26:54 701  	in_chargeback_date    IN DATE,
15:26:54 702  	out_chargeback_amount OUT NUMBER
15:26:54 703  ) AS
15:26:54 704  -- VARIABLES
15:26:54 705  SPROC_NAME	     CONSTANT VARCHAR2(28) := 'CALCULATE_INVOICE_CHARGEBACK';
15:26:54 706  var_chargeback_date    DATE;
15:26:54 707  temp_invoice_id	     NUMBER;
15:26:54 708  var_licenses_number    NUMBER;
15:26:54 709  var_invoice_start_date DATE;
15:26:54 710  var_invoice_end_date   DATE;
15:26:54 711  var_offer_id	     NUMBER;
15:26:54 712  var_offer_days_interval NUMBER;
15:26:54 713  var_license_days_used  NUMBER;
15:26:54 714  var_invoice_amount     NUMBER(10,2);
15:26:54 715  var_offer_chain_id     NUMBER;
15:26:54 716  var_offer_chain_meta_data_val VARCHAR2(1024);
15:26:54 717  var_offer_chain_full_refund   NUMBER;
15:26:54 718  var_offer_chain_prorated_ref  NUMBER;
15:26:54 719  var_chargeback_calculated NUMBER;
15:26:54 720  var_max_invoice_refund	NUMBER;
15:26:54 721  -- EXCEPTIONS
15:26:54 722  BAD_INVOICE_ID		  EXCEPTION;
15:26:54 723  NO_LICENSES_FOUND_EXCEPTION EXCEPTION;
15:26:54 724  OFFER_LENGTH_IS_ZERO	  EXCEPTION;
15:26:54 725  BEGIN
15:26:54 726  
15:26:54 727  	IF in_chargeback_date IS NULL THEN
15:26:54 728  	  var_chargeback_date := PROCS_COMMON_V19.NORMALIZE_DATE(current_date);
15:26:54 729  	ELSE
15:26:54 730  	  var_chargeback_date := PROCS_COMMON_V19.NORMALIZE_DATE(in_chargeback_date);
15:26:54 731  	END IF;
15:26:54 732  
15:26:54 733  	-- Check that invoice exists
15:26:54 734  	BEGIN
15:26:54 735  	  SELECT
15:26:54 736  	    INVOICE.ID into temp_invoice_id
15:26:54 737  	  FROM
15:26:54 738  	    INVOICE
15:26:54 739  	  WHERE
15:26:54 740  	    INVOICE.ID = in_invoice_id;
15:26:54 741  	  EXCEPTION
15:26:54 742  	    WHEN NO_DATA_FOUND THEN
15:26:54 743  	      RAISE BAD_INVOICE_ID;
15:26:54 744  	END;
15:26:54 745  
15:26:54 746  	-- Check that invoice has at least one license
15:26:54 747  	SELECT
15:26:54 748  	  COUNT(*) into var_licenses_number
15:26:54 749  	FROM
15:26:54 750  	  LICENSE
15:26:54 751  	WHERE
15:26:54 752  	  LICENSE.INVOICE_ID = in_invoice_id;
15:26:54 753  
15:26:54 754  	IF var_licenses_number = 0 THEN
15:26:54 755  	  RAISE NO_LICENSES_FOUND_EXCEPTION;
15:26:54 756  	END IF;
15:26:54 757  
15:26:54 758  	SELECT
15:26:54 759  	  MIN(LICENSE.START_DATE) into var_invoice_start_date
15:26:54 760  	FROM
15:26:54 761  	  LICENSE
15:26:54 762  	WHERE
15:26:54 763  	  LICENSE.INVOICE_ID = in_invoice_id;
15:26:54 764  
15:26:54 765  	var_invoice_start_date := PROCS_COMMON_V19.NORMALIZE_DATE(var_invoice_start_date);
15:26:54 766  
15:26:54 767  	SELECT DISTINCT
15:26:54 768  	  LICENSE.OFFER_ID into var_offer_id
15:26:54 769  	FROM
15:26:54 770  	  LICENSE
15:26:54 771  	WHERE
15:26:54 772  	  LICENSE.INVOICE_ID = in_invoice_id;
15:26:54 773  
15:26:54 774  	SELECT
15:26:54 775  	  MAX (LICENSE.END_DATE) into var_invoice_end_date
15:26:54 776  	FROM
15:26:54 777  	  LICENSE
15:26:54 778  	WHERE
15:26:54 779  	  LICENSE.INVOICE_ID = in_invoice_id;
15:26:54 780  
15:26:54 781  	-- All licenses for given invoice should point into the same offer
15:26:54 782  
15:26:54 783  	PROCS_OFFER_CHAIN_V19.GET_OFFER_LENGTH_IN_DAYS(
15:26:54 784  	  in_offer_id	=> var_offer_id,
15:26:54 785  	  in_start_date => var_invoice_start_date,
15:26:54 786  	  out_days	=> var_offer_days_interval
15:26:54 787  	);
15:26:54 788  
15:26:54 789  	IF var_offer_days_interval = 0 THEN
15:26:54 790  	  RAISE OFFER_LENGTH_IS_ZERO;
15:26:54 791  	END IF;
15:26:54 792  
15:26:54 793  	PROCS_INVOICE_V19.GET_INVOICE_DAYS_USED_NUMBER(
15:26:54 794  	  in_invoice_id      => in_invoice_id,
15:26:54 795  	  in_chargeback_date => var_chargeback_date,
15:26:54 796  	  out_days_num	     => var_license_days_used
15:26:54 797  	);
15:26:54 798  
15:26:54 799  	PROCS_INVOICE_V19.CALCULATE_INVOICE_AMOUNT(
15:26:54 800  	  in_invoice_id => in_invoice_id,
15:26:54 801  	  out_amount	=> var_invoice_amount
15:26:54 802  	);
15:26:54 803  
15:26:54 804  	-- 39437
15:26:54 805  	-- Get offer chain id by invoice id
15:26:54 806  	SELECT DISTINCT
15:26:54 807  	  SUBSCRIPTION.OFFER_CHAIN_ID into var_offer_chain_id
15:26:54 808  	FROM
15:26:54 809  	  SUBSCRIPTION
15:26:54 810  	  INNER JOIN LICENSE ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:26:54 811  	WHERE
15:26:54 812  	  LICENSE.INVOICE_ID = in_invoice_id;
15:26:54 813  
15:26:54 814  	-- Get offer chain's meta data full amount value
15:26:54 815  	PROCS_OFFER_CHAIN_V19.GET_OFFER_CHAIN_MD_VALUE(
15:26:54 816  	  in_offer_chain_id => var_offer_chain_id,
15:26:54 817  	  in_meta_data_name => GLOBAL_CONSTANTS_V19.OFFER_CHAIN_FULL_REFUND,
15:26:54 818  	  out_value	    => var_offer_chain_meta_data_val
15:26:54 819  	);
15:26:54 820  	IF var_offer_chain_meta_data_val IS NULL THEN
15:26:54 821  	  var_offer_chain_full_refund := NULL;
15:26:54 822  	ELSE
15:26:54 823  	  var_offer_chain_full_refund := TO_NUMBER(var_offer_chain_meta_data_val);
15:26:54 824  	END IF;
15:26:54 825  
15:26:54 826  	-- Get offer chain's meta data prorated amount value
15:26:54 827  	PROCS_OFFER_CHAIN_V19.GET_OFFER_CHAIN_MD_VALUE(
15:26:54 828  	  in_offer_chain_id => var_offer_chain_id,
15:26:54 829  	  in_meta_data_name => GLOBAL_CONSTANTS_V19.OFFER_CHAIN_PRORATED_REFUND,
15:26:54 830  	  out_value	    => var_offer_chain_meta_data_val
15:26:54 831  	);
15:26:54 832  	IF var_offer_chain_meta_data_val IS NULL THEN
15:26:54 833  	  var_offer_chain_prorated_ref := NULL;
15:26:54 834  	ELSE
15:26:54 835  	  var_offer_chain_prorated_ref := TO_NUMBER(var_offer_chain_meta_data_val);
15:26:54 836  	END IF;
15:26:54 837  
15:26:54 838  	var_chargeback_calculated := GLOBAL_CONSTANTS_V19.FALSE;
15:26:54 839  
15:26:54 840  	IF var_offer_chain_full_refund IS NOT NULL
15:26:54 841  	   AND var_chargeback_calculated = GLOBAL_CONSTANTS_V19.FALSE THEN
15:26:54 842  	  IF var_license_days_used < var_offer_chain_full_refund THEN
15:26:54 843  	    out_chargeback_amount := var_invoice_amount;
15:26:54 844  	    var_chargeback_calculated := GLOBAL_CONSTANTS_V19.TRUE;
15:26:54 845  	  END IF;
15:26:54 846  	END IF;
15:26:54 847  
15:26:54 848  	IF var_offer_chain_prorated_ref IS NOT NULL
15:26:54 849  	   AND var_chargeback_calculated = GLOBAL_CONSTANTS_V19.FALSE THEN
15:26:54 850  	  IF var_license_days_used < var_offer_chain_prorated_ref THEN
15:26:54 851  	    out_chargeback_amount := ( var_invoice_amount * (var_offer_days_interval - var_license_days_used) ) / var_offer_days_interval;
15:26:54 852  	    var_chargeback_calculated := GLOBAL_CONSTANTS_V19.TRUE;
15:26:54 853  	  END IF;
15:26:54 854  	END IF;
15:26:54 855  
15:26:54 856  	IF var_chargeback_calculated = GLOBAL_CONSTANTS_V19.TRUE THEN
15:26:54 857  	  PROCS_INVOICE_V19.GET_MAX_REFUND(
15:26:54 858  	    in_invoice_id => in_invoice_id,
15:26:54 859  	    out_amount	  => var_max_invoice_refund
15:26:54 860  	  );
15:26:54 861  	  IF var_max_invoice_refund < out_chargeback_amount THEN
15:26:54 862  	    out_chargeback_amount := var_max_invoice_refund;
15:26:54 863  	  END IF;
15:26:54 864  	END IF;
15:26:54 865  
15:26:54 866  	IF var_chargeback_calculated = GLOBAL_CONSTANTS_V19.FALSE THEN
15:26:54 867  	  out_chargeback_amount := 0;
15:26:54 868  	END IF;
15:26:54 869  
15:26:54 870  EXCEPTION
15:26:54 871  WHEN BAD_INVOICE_ID THEN
15:26:54 872  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 873  	  SPROC_NAME, 'No such invoice');
15:26:54 874  WHEN NO_LICENSES_FOUND_EXCEPTION THEN
15:26:54 875  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 876  	  SPROC_NAME, 'No licenses found');
15:26:54 877  WHEN OFFER_LENGTH_IS_ZERO THEN
15:26:54 878  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 879  	  SPROC_NAME, 'Offer length is zero');
15:26:54 880  WHEN OTHERS THEN
15:26:54 881  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:54 882  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:26:54 883  END CALCULATE_INVOICE_CHARGEBACK;
