15:26:54 1383  	     in_transaction_id	      => f_transaction_to_close.ID,
15:26:54 1384  	     in_updated_by	      => in_updated_by,
15:26:54 1385  	     in_transaction_status_id => GLOBAL_STATUSES_V19.TRANSACTION_CLOSED
15:26:54 1386  	   );
15:26:54 1387  	 END LOOP;
15:26:54 1388  
15:26:54 1389  EXCEPTION
