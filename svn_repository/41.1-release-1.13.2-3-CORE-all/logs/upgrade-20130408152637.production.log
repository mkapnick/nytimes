15:26:53 718  /******************************************************************************/
15:26:53 719  
15:26:53 720  FUNCTION GET_NEXT_OFFER_INDEX (
15:26:53 721  /*
15:26:53 722  NULL, if not exists
15:26:53 723  */
15:26:53 724  	in_offer_chain_id	     IN NUMBER,
15:26:53 725  	in_offer_chain_current_index IN NUMBER
15:26:53 726  ) RETURN NUMBER AS
15:26:53 727  -- VARIABLES
15:26:53 728  SPROC_NAME		     CONSTANT VARCHAR2(20) := 'GET_NEXT_OFFER_INDEX';
15:26:53 729  temp_offer_chain_id	     NUMBER;
15:26:53 730  temp_offer_chain_current_index NUMBER;
15:26:53 731  var_result		     NUMBER;
15:26:53 732  -- EXCEPTIONS
15:26:53 733  BAD_OFFER_CHAIN_ID      EXCEPTION;
15:26:53 734  BAD_CURRENT_INDEX_VALUE EXCEPTION;
15:26:53 735  BEGIN
15:26:53 736  
15:26:53 737  	-- Check that offer chain exists
15:26:53 738  	BEGIN
15:26:53 739  	  SELECT
15:26:53 740  	    OFFER_CHAIN.ID into temp_offer_chain_id
15:26:53 741  	  FROM
15:26:53 742  	    OFFER_CHAIN
15:26:53 743  	  WHERE
15:26:53 744  	    OFFER_CHAIN.ID = in_offer_chain_id;
15:26:53 745  	  EXCEPTION
15:26:53 746  	    WHEN NO_DATA_FOUND THEN
15:26:53 747  	      RAISE BAD_OFFER_CHAIN_ID;
15:26:53 748  	END;
15:26:53 749  
15:26:53 750  	-- Check that current offer index exists
15:26:53 751  	BEGIN
15:26:53 752  	  SELECT
15:26:53 753  	    OFFER_OFFER_CHAIN.INDEX_VALUE into temp_offer_chain_current_index
15:26:53 754  	  FROM
15:26:53 755  	    OFFER_OFFER_CHAIN
15:26:53 756  	  WHERE
15:26:53 757  	    OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
15:26:53 758  	    AND OFFER_OFFER_CHAIN.INDEX_VALUE = in_offer_chain_current_index
15:26:53 759  	    -- TODO: delete next line
15:26:53 760  	    AND ROWNUM <= 1;
15:26:53 761  	  EXCEPTION
15:26:53 762  	    WHEN NO_DATA_FOUND THEN
15:26:53 763  	      RAISE BAD_CURRENT_INDEX_VALUE;
15:26:53 764  	END;
15:26:53 765  
15:26:53 766  	-- Get next offer index
15:26:53 767  	BEGIN
15:26:53 768  	  SELECT
15:26:53 769  	    INDEX_VALUE into var_result
15:26:53 770  	  FROM (
15:26:53 771  	    SELECT
15:26:53 772  	      OFFER_OFFER_CHAIN.INDEX_VALUE
15:26:53 773  	    FROM
15:26:53 774  	      OFFER_OFFER_CHAIN
15:26:53 775  	    WHERE
15:26:53 776  	      OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
15:26:53 777  	      AND OFFER_OFFER_CHAIN.INDEX_VALUE > in_offer_chain_current_index
15:26:53 778  	    ORDER BY
15:26:53 779  	      OFFER_OFFER_CHAIN.INDEX_VALUE ASC
15:26:53 780  	  )
15:26:53 781  	  WHERE
15:26:53 782  	    ROWNUM <= 1;
15:26:53 783  	  EXCEPTION
15:26:53 784  	    WHEN NO_DATA_FOUND THEN
15:26:53 785  	      var_result := NULL;
15:26:53 786  	END;
15:26:53 787  
15:26:53 788  	RETURN var_result;
15:26:53 789  
15:26:53 790  EXCEPTION
15:26:53 791  WHEN BAD_OFFER_CHAIN_ID THEN
15:26:53 792  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:53 793  	  SPROC_NAME, 'No such offer chain');
15:26:53 794  WHEN BAD_CURRENT_INDEX_VALUE THEN
15:26:53 795  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:53 796  	  SPROC_NAME, 'Bad current index value');
15:26:53 797  WHEN OTHERS THEN
15:26:53 798  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:53 799  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:26:53 800  END GET_NEXT_OFFER_INDEX;
