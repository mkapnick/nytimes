15:26:54 1354  	     in_charge_id	 => f_charge_to_close.ID,
15:26:54 1355  	     in_updated_by	 => in_updated_by,
15:26:54 1356  	     in_charge_status_id => GLOBAL_STATUSES_V19.CHARGE_CANCELED
15:26:54 1357  	   );
15:26:54 1358  	 END LOOP;
15:26:54 1359  
15:26:54 1360  	 --FOR f_license_to_cancel IN (
15:26:54 1361  	 --  SELECT LICENSE.ID FROM LICENSE WHERE LICENSE.INVOICE_ID = in_invoice_id AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V19.LICENSE_ACTIVE
15:26:54 1362  	 --)
15:26:54 1363  	 --LOOP
15:26:54 1364  	 --  PROCS_LICENSE_CRU_V19.UPDATE_LICENSE(
15:26:54 1365  	 --    in_license_id	     => f_license_to_cancel.ID,
15:26:54 1366  	 --    in_license_status_id  => GLOBAL_STATUSES_V19.LICENSE_CLOSED,
15:26:54 1367  	 --    in_needs_entitlements => GLOBAL_CONSTANTS_V19.FALSE,
15:26:54 1368  	 --    in_updated_by	     => in_updated_by
15:26:54 1369  	 --  );
15:26:54 1370  	 --END LOOP;
15:26:54 1371  
15:26:54 1372  	 PROCS_INVOICE_V19.UPDATE_INVOICE_STATUS(
