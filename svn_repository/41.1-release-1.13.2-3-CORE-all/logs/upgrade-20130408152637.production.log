15:26:54 595  
15:26:54 596  FUNCTION IS_INVOICE_PAYING_STARTED (
15:26:54 597  /*
15:26:54 598  Throws exceptions:
15:26:54 599  APP_EXCEPTION_CODES_V19.NOT_FOUND
15:26:54 600  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
15:26:54 601  */
15:26:54 602  	in_invoice_id IN NUMBER
15:26:54 603  ) RETURN NUMBER AS
15:26:54 604  -- VARIABLES
15:26:54 605  SPROC_NAME		 CONSTANT VARCHAR2(30) := 'IS_INVOICE_PAYING_STARTED';
15:26:54 606  temp_invoice_id		 NUMBER;
15:26:54 607  var_processed_charges_num  NUMBER;
15:26:54 608  var_processed_transac_num  NUMBER;
15:26:54 609  var_success_attempts_num	 NUMBER;
15:26:54 610  var_is_gc 		 NUMBER;
15:26:54 611  -- EXCEPTIONS
15:26:54 612  BAD_INVOICE_ID EXCEPTION;
15:26:54 613  BEGIN
15:26:54 614  
15:26:54 615  	-- Check that invoice exists
15:26:54 616  	BEGIN
15:26:54 617  	  SELECT
15:26:54 618  	    INVOICE.ID into temp_invoice_id
15:26:54 619  	  FROM
15:26:54 620  	    INVOICE
15:26:54 621  	  WHERE
15:26:54 622  	    INVOICE.ID = in_invoice_id;
15:26:54 623  	  EXCEPTION
15:26:54 624  	   WHEN NO_DATA_FOUND THEN
15:26:54 625  	     RAISE BAD_INVOICE_ID;
15:26:54 626  	END;
15:26:54 627  
15:26:54 628  	-- Check that there are tansaction attempts with status success
15:26:54 629  	SELECT
15:26:54 630  	  COUNT(1) into var_success_attempts_num
15:26:54 631  	FROM
15:26:54 632  	  TRANSACTION_ATTEMPT ta,
15:26:54 633  	  TRANSACTION t,
15:26:54 634  	  CHARGE c
15:26:54 635  	WHERE
15:26:54 636  	  ta.TRANSACTION_ATTEMPT_STATUS_ID = GLOBAL_STATUSES_V19.TRANS_ATTEMPT_SUCCESS and
15:26:54 637  	  ta.transaction_id = t.id and
15:26:54 638  	  c.transaction_id = t.id and
15:26:54 639  	  t.is_refund = GLOBAL_CONSTANTS_V19.FALSE and
15:26:54 640  	  c.invoice_id = in_invoice_id
15:26:54 641  	;
15:26:54 642  
15:26:54 643  	IF var_success_attempts_num > 0 THEN
15:26:54 644  	  RETURN GLOBAL_CONSTANTS_V19.TRUE;
15:26:54 645  	END IF;
15:26:54 646  
15:26:54 647  	SELECT
15:26:54 648  	  COUNT(1) into var_success_attempts_num
15:26:54 649  	FROM
15:26:54 650  	  TRANSACTION t,
15:26:54 651  	  CHARGE c
15:26:54 652  	WHERE
15:26:54 653  	  c.transaction_id = t.id and
15:26:54 654  	  t.is_refund = GLOBAL_CONSTANTS_V19.FALSE and
15:26:54 655  	  t.is_settled = GLOBAL_CONSTANTS_V19.TRUE and
15:26:54 656  	  c.invoice_id = in_invoice_id
15:26:54 657  	;
15:26:54 658  
15:26:54 659  	IF var_success_attempts_num > 0 THEN
15:26:54 660  	  RETURN GLOBAL_CONSTANTS_V19.TRUE;
15:26:54 661  	END IF;
15:26:54 662  
15:26:54 663  	SELECT
15:26:54 664  	  COUNT(1) into var_is_gc
15:26:54 665  	FROM
15:26:54 666  	  gift_certificate gc
15:26:54 667  	WHERE
15:26:54 668  	  gc.finalized_invoice_id = in_invoice_id
15:26:54 669  	;
15:26:54 670  
15:26:54 671  	IF var_is_gc > 0 THEN
15:26:54 672  	  RETURN GLOBAL_CONSTANTS_V19.TRUE;
15:26:54 673  	END IF;
15:26:54 674  
15:26:54 675  	RETURN GLOBAL_CONSTANTS_V19.FALSE;
15:26:54 676  
15:26:54 677  EXCEPTION
15:26:54 678  WHEN BAD_INVOICE_ID THEN
15:26:54 679  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 680  	  SPROC_NAME, 'No such invoice');
15:26:54 681  WHEN OTHERS THEN
15:26:54 682  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:54 683  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:26:54 684  END IS_INVOICE_PAYING_STARTED;
15:26:54 685  
