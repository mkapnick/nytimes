15:26:53 355  	out_result_set		      OUT SYS_REFCURSOR
15:26:53 356  ) AS
15:26:53 357  -- VARIABLES
15:26:53 358  SPROC_NAME	  CONSTANT VARCHAR2(30) := 'GET_TRNSCTN_ATTEMPTS_BY_STATUS';
15:26:53 359  temp_transaction_id NUMBER;
15:26:53 360  -- EXCEPTIONS
15:26:53 361  BAD_TRANSACTION_ID       EXCEPTION;
15:26:53 362  BAD_TRANS_ATTEMPT_STATUS EXCEPTION;
15:26:53 363  BEGIN
15:26:53 364  
15:26:53 365  	-- Check that transaction exists
15:26:53 366  	BEGIN
15:26:53 367  	  SELECT
15:26:53 368  	    TRANSACTION.ID into temp_transaction_id
15:26:53 369  	  FROM
15:26:53 370  	    TRANSACTION
15:26:53 371  	  WHERE
15:26:53 372  	    TRANSACTION.ID = in_transaction_id;
15:26:53 373  	  EXCEPTION
15:26:53 374  	    WHEN OTHERS THEN
15:26:53 375  	      RAISE BAD_TRANSACTION_ID;
15:26:53 376  	END;
15:26:53 377  
15:26:53 378  	-- Check that transaction attempt status is correct
15:26:53 379  	IF in_transaction_attempt_status != GLOBAL_STATUSES_V19.TRANS_ATTEMPT_IN_PROGRESS
15:26:53 380  	  AND in_transaction_attempt_status != GLOBAL_STATUSES_V19.TRANS_ATTEMPT_SUCCESS
15:26:53 381  	  AND in_transaction_attempt_status != GLOBAL_STATUSES_V19.TRANS_ATTEMPT_FAILED THEN
15:26:53 382  	  RAISE BAD_TRANS_ATTEMPT_STATUS;
15:26:53 383  	END IF;
15:26:53 384  
15:26:53 385  	OPEN out_result_set FOR
15:26:53 386  	SELECT
15:26:53 387  	  TRANSACTION_ATTEMPT.ID,
15:26:53 388  	  TRANSACTION_ATTEMPT.EXTERNAL_STATUS_CODE,
15:26:53 389  	  TRANSACTION_ATTEMPT.EXTERNAL_STATUS_MESSAGE,
15:26:53 390  	  TRANSACTION_ATTEMPT.CREATE_DATE,
15:26:53 391  	  TRANSACTION_ATTEMPT.CREATED_BY,
15:26:53 392  	  TRANSACTION_ATTEMPT.EXTERNAL_TRANSACTION_ID,
15:26:53 393  	  TRANSACTION_ATTEMPT.TRANSACTION_START_TIME,
15:26:53 394  	  TRANSACTION.TRANSACTION_AMOUNT,
15:26:53 395  	  TRANSACTION.ID as "TRANSACTION_ID",
15:26:53 396  	  TRANSACTION.UPDATE_DATE as "TRANSACTION_UPDATE_TIME"
15:26:53 397  	FROM
15:26:53 398  	  TRANSACTION_ATTEMPT
15:26:53 399  	  INNER JOIN TRANSACTION ON TRANSACTION_ATTEMPT.TRANSACTION_ID = TRANSACTION.ID
15:26:53 400  	WHERE
15:26:53 401  	  TRANSACTION_ATTEMPT.TRANSACTION_ID = in_transaction_id
15:26:53 402  	  AND TRANSACTION_ATTEMPT.TRANSACTION_ATTEMPT_STATUS_ID = in_transaction_attempt_status;
15:26:53 403  
15:26:53 404  EXCEPTION
15:26:53 405  WHEN BAD_TRANSACTION_ID THEN
15:26:53 406  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:53 407  	  SPROC_NAME, 'No such transaction');
15:26:53 408  WHEN BAD_TRANS_ATTEMPT_STATUS THEN
15:26:53 409  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
15:26:53 410  	  SPROC_NAME, 'Bad transaction attempt status');
15:26:53 411  WHEN OTHERS THEN
15:26:53 412  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:53 413  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:26:53 414  END GET_TRNSCTN_ATTEMPTS_BY_STATUS;
