15:26:53 2479  BAD_SUBSCRIPTION_STATUS	   EXCEPTION;
15:26:53 2480  CAN_NOT_UPDATE_SUBSCRIPTION EXCEPTION;
15:26:53 2481  ACTIVE_INVOICES_FOUND	   EXCEPTION;
15:26:53 2482  CAN_NOT_CREATE_NOTE	   EXCEPTION;
