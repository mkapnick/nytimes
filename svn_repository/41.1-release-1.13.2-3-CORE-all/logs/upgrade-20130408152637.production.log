15:26:53 1898  PROCEDURE CHECK_PRODUCT_ELIGIBILITY (
15:26:53 1899  	 in_group_id	   IN NUMBER,
15:26:53 1900  	 in_offer_chain_id IN NUMBER,
15:26:53 1901  	 out_is_eligible   OUT NUMBER,
15:26:53 1902  	 out_concurrent_subscription_id OUT NUMBER
15:26:53 1903  ) AS
15:26:53 1904  SPROC_NAME CONSTANT VARCHAR2(25) := 'CHECK_PRODUCT_ELIGIBILITY';
15:26:53 1905  -- Variables
15:26:53 1906  var_account_id  NUMBER;
15:26:53 1907  var_is_eligible NUMBER;
15:26:53 1908  var_is_gc       NUMBER;
15:26:53 1909  -- Exceptions
15:26:53 1910  BAD_GROUP_ID		   EXCEPTION;
15:26:53 1911  CAN_NOT_CHECK_SAME_PRODUCTS EXCEPTION;
15:26:53 1912  BAD_OC_ID		   EXCEPTION;
15:26:53 1913  EXCEPTION_MESSAGE	   VARCHAR(1024);
15:26:53 1914  BEGIN
15:26:53 1915  
15:26:53 1916  	 var_is_eligible := GLOBAL_CONSTANTS_V19.TRUE;
15:26:53 1917  
15:26:53 1918  	 out_concurrent_subscription_id := NULL;
15:26:53 1919  
15:26:53 1920  	 BEGIN
15:26:53 1921  	   SELECT
15:26:53 1922  	     OC.IS_GIFT_CERTIFICATE into var_is_gc
15:26:53 1923  	   FROM
15:26:53 1924  	     OFFER_CHAIN OC
15:26:53 1925  	   WHERE
15:26:53 1926  	     OC.ID = in_offer_chain_id;
15:26:53 1927  	   EXCEPTION
15:26:53 1928  	     WHEN NO_DATA_FOUND THEN
15:26:53 1929  	       RAISE BAD_OC_ID;
15:26:53 1930  	 END;
15:26:53 1931  
15:26:53 1932  	 -- only check eligibility if this is not a gift certificate
15:26:53 1933  	 IF (var_is_gc IS NULL OR var_is_gc != 1) THEN
15:26:53 1934  	   BEGIN
15:26:53 1935  	     SELECT
15:26:53 1936  	       A.ID into var_account_id
15:26:53 1937  	     FROM
15:26:53 1938  	       ACCOUNT A
15:26:53 1939  	     WHERE
15:26:53 1940  	       A.GROUP_ID = in_group_id;
15:26:53 1941  	     EXCEPTION
15:26:53 1942  	       WHEN NO_DATA_FOUND THEN
15:26:53 1943  		 RAISE BAD_GROUP_ID;
15:26:53 1944  	   END;
15:26:53 1945  
15:26:53 1946  
15:26:53 1947  	   FOR f_offer_chain IN (
15:26:53 1948  	       SELECT
15:26:53 1949  		 S.ID as SUBSCRIPTION_ID,
15:26:53 1950  		 S.OFFER_CHAIN_ID
15:26:53 1951  	       FROM
15:26:53 1952  		 SUBSCRIPTION S
15:26:53 1953  	       WHERE
15:26:53 1954  		 S.ACCOUNT_ID = var_account_id
15:26:53 1955  		 AND (S.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V19.SUBSCRIPTION_ACTIVE
15:26:53 1956  		      OR S.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V19.SUBSCRIPTION_IN_GRACE_PERIOD)
15:26:53 1957  	   )
15:26:53 1958  	   LOOP
15:26:53 1959  	     BEGIN
15:26:53 1960  	       IF (
15:26:53 1961  		 PROCS_OFFER_CHAIN_V19.CHECK_FOR_SAME_PRODUCTS(
15:26:53 1962  		   in_offer_chain_id,
15:26:53 1963  		   f_offer_chain.offer_chain_id,
15:26:53 1964  		   GLOBAL_CONSTANTS_V19.TRUE
15:26:53 1965  		 ) = GLOBAL_CONSTANTS_V19.TRUE
15:26:53 1966  	       ) THEN
15:26:53 1967  		 var_is_eligible := GLOBAL_CONSTANTS_V19.FALSE;
15:26:53 1968  		 out_concurrent_subscription_id := f_offer_chain.SUBSCRIPTION_ID;
15:26:53 1969  	       END IF;
15:26:53 1970  	       EXCEPTION
15:26:53 1971  		 WHEN OTHERS THEN
15:26:53 1972  		   EXCEPTION_MESSAGE := SQLERRM;
15:26:53 1973  		   RAISE CAN_NOT_CHECK_SAME_PRODUCTS;
15:26:53 1974  	     END;
15:26:53 1975  	   END LOOP;
15:26:53 1976  	 END IF;
15:26:53 1977  	 out_is_eligible := var_is_eligible;
15:26:53 1978  
