15:26:54 436  -- norlov: #38796
15:26:54 437  PROCEDURE GET_TRANSACTION_INVOICE (
15:26:54 438  /*
15:26:54 439  Throws exceptions:
15:26:54 440  APP_EXCEPTION_CODES_V19.NOT_FOUND
15:26:54 441  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
15:26:54 442  */
15:26:54 443  	in_transaction_id  IN  NUMBER,
15:26:54 444  	out_result_set	      OUT SYS_REFCURSOR
15:26:54 445  ) AS
15:26:54 446  SPROC_NAME CONSTANT  VARCHAR2(23) := 'GET_TRANSACTION_INVOICE';
15:26:54 447  -- VARIABLES
15:26:54 448  temp_transaction_id  NUMBER;
15:26:54 449  var_invoice_id	   NUMBER;
15:26:54 450  var_subscription_id  NUMBER;
15:26:54 451  var_offer_chain_id   NUMBER;
15:26:54 452  var_offer_chain_name VARCHAR2(255);
15:26:54 453  -- EXCEPTIONS
15:26:54 454  BAD_TRANSACTION_ID     EXCEPTION;
15:26:54 455  EXCEPTION_MESSAGE      VARCHAR2(1024);
15:26:54 456  BEGIN
15:26:54 457   -- check if there is the transaction
15:26:54 458  	BEGIN
15:26:54 459  	  SELECT
15:26:54 460  	    TRANSACTION.ID into temp_transaction_id
15:26:54 461  	  FROM
15:26:54 462  	    TRANSACTION
15:26:54 463  	  WHERE
15:26:54 464  	    TRANSACTION.ID = in_transaction_id;
15:26:54 465  	  EXCEPTION
15:26:54 466  	    WHEN NO_DATA_FOUND THEN
15:26:54 467  	      RAISE BAD_TRANSACTION_ID;
15:26:54 468  	END;
15:26:54 469  
15:26:54 470  	-- Get invoice id
15:26:54 471  	SELECT DISTINCT
15:26:54 472  	  CHARGE.INVOICE_ID into var_invoice_id
15:26:54 473  	FROM
15:26:54 474  	  CHARGE
15:26:54 475  	WHERE
15:26:54 476  	  CHARGE.TRANSACTION_ID = in_transaction_id;
15:26:54 477  
15:26:54 478  	-- Get subscription id if exists
15:26:54 479  	BEGIN
15:26:54 480  	  SELECT DISTINCT
15:26:54 481  	    LICENSE.SUBSCRIPTION_ID into var_subscription_id
15:26:54 482  	  FROM
15:26:54 483  	    LICENSE
15:26:54 484  	  WHERE
15:26:54 485  	    LICENSE.INVOICE_ID = var_invoice_id;
15:26:54 486  	  EXCEPTION
15:26:54 487  	    WHEN NO_DATA_FOUND THEN
15:26:54 488  	      var_subscription_id := NULL;
15:26:54 489  	END;
15:26:54 490  
15:26:54 491  	IF var_subscription_id IS NOT NULL THEN
15:26:54 492  	  -- Fetch offer chain from subscription
15:26:54 493  	  SELECT
15:26:54 494  	    OFFER_CHAIN.ID,
15:26:54 495  	    OFFER_CHAIN.NAME
15:26:54 496  	    into
15:26:54 497  	    var_offer_chain_id,
15:26:54 498  	    var_offer_chain_name
15:26:54 499  	  FROM
15:26:54 500  	    OFFER_CHAIN
15:26:54 501  	    INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
15:26:54 502  	  WHERE
15:26:54 503  	    SUBSCRIPTION.ID = var_subscription_id;
15:26:54 504  	ELSE
15:26:54 505  	  -- Fetch offer chain from GC
15:26:54 506  	  SELECT
15:26:54 507  	    OFFER_CHAIN.ID,
15:26:54 508  	    OFFER_CHAIN.NAME
15:26:54 509  	    into
15:26:54 510  	    var_offer_chain_id,
15:26:54 511  	    var_offer_chain_name
15:26:54 512  	  FROM
15:26:54 513  	    OFFER_CHAIN
15:26:54 514  	    INNER JOIN GIFT_CERTIFICATE ON GIFT_CERTIFICATE.OFFER_CHAIN_ID = OFFER_CHAIN.ID
15:26:54 515  	  WHERE
15:26:54 516  	    GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = var_invoice_id;
15:26:54 517  	END IF;
15:26:54 518  
15:26:54 519  	OPEN out_result_set FOR
15:26:54 520  	SELECT DISTINCT
15:26:54 521  	  var_invoice_id       AS "INVOICE_ID",
15:26:54 522  	  var_subscription_id  AS "SUBSCRIPTION_ID",
15:26:54 523  	  var_offer_chain_id   AS "OFFER_CHAIN_ID",
15:26:54 524  	  var_offer_chain_name AS "OFFER_CHAIN_NAME"
15:26:54 525  	FROM
15:26:54 526  	  DUAL;
15:26:54 527  
15:26:54 528  EXCEPTION
15:26:54 529  WHEN BAD_TRANSACTION_ID THEN
15:26:54 530  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 531  	  SPROC_NAME, 'No such transaction');
15:26:54 532  WHEN OTHERS THEN
15:26:54 533  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:54 534  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:26:54 535  END GET_TRANSACTION_INVOICE;
