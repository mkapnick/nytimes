15:26:54 1753  PROCEDURE GET_GROUP_IDS_BY_CC_INFO (
15:26:54 1754  	 in_last_four_cc IN CREDIT_CARD.LAST_FOUR_CC%TYPE,
15:26:54 1755  	 in_expiration_date IN DATE,
15:26:54 1756  	 in_country IN CREDIT_CARD.COUNTRY%TYPE DEFAULT NULL,
15:26:54 1757  	 in_postal_code IN CREDIT_CARD.POSTAL_CODE%TYPE DEFAULT NULL,
15:26:54 1758  	 in_city IN CREDIT_CARD.CITY%TYPE DEFAULT NULL,
15:26:54 1759  	 in_state IN CREDIT_CARD.STATE%TYPE DEFAULT NULL,
15:26:54 1760  	 in_credit_card_type_id IN CREDIT_CARD.CREDIT_CARD_TYPE_ID%TYPE DEFAULT NULL,
15:26:54 1761  	 in_credit_card_status_id IN CREDIT_CARD.CREDIT_CARD_STATUS_ID%TYPE DEFAULT NULL,
15:26:54 1762  	 in_lower_bound IN NUMBER DEFAULT 1,
15:26:54 1763  	 in_upper_bound IN NUMBER DEFAULT 11,
15:26:54 1764  	 out_result_set OUT SYS_REFCURSOR
15:26:54 1765  ) AS
15:26:54 1766  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_GROUP_IDS_BY_CC_INFO';
15:26:54 1767  BEGIN
15:26:54 1768  
15:26:54 1769  	 OPEN out_result_set FOR
15:26:54 1770  	     SELECT
15:26:54 1771  	       distinct /*+ first_rows(in_upper_bound-in_lower_bound) */ a.GROUP_ID GROUP_ID
15:26:54 1772  	     FROM
15:26:54 1773  	       account a,
15:26:54 1774  	       credit_card cc
15:26:54 1775  	     WHERE
15:26:54 1776  	       cc.expiration_date = in_expiration_date and
15:26:54 1777  	       cc.last_four_cc = in_last_four_cc and
15:26:54 1778  	       upper(cc.postal_code) = upper(nvl(in_postal_code, cc.postal_code)) and
15:26:54 1779  	       upper(cc.city) = upper(nvl(in_city, cc.city)) and
15:26:54 1780  	       upper(cc.state) = upper(nvl(in_state, cc.state)) and
