15:26:54 2710  SPROC_NAME CONSTANT VARCHAR2(27) := 'SWITCH_FINANCIAL_INSTRUMENT';
15:26:54 2711  -- variables
15:26:54 2712  temp_out_transaction_id NUMBER;
15:26:54 2713  temp_out_charge_id      NUMBER;
15:26:54 2714  var_accounts_count      NUMBER;
15:26:54 2715  var_transaction_type_old  "TRANSACTION".TRANSACTION_TYPE_CODE%TYPE;
15:26:54 2716  var_transaction_type	 "TRANSACTION".TRANSACTION_TYPE_CODE%TYPE;
15:26:54 2717  -- EXCEPTIONS
15:26:54 2718  BAD_OLD_CC   EXCEPTION;
15:26:54 2719  BAD_OLD_PP   EXCEPTION;
15:26:54 2720  BAD_OLD_TYPE EXCEPTION;
15:26:54 2721  BAD_NEW_CC   EXCEPTION;
15:26:54 2722  BAD_NEW_PP   EXCEPTION;
15:26:54 2723  BAD_NEW_TYPE EXCEPTION;
15:26:54 2724  DIFFERENT_OWNERS EXCEPTION;
15:26:54 2725  BEGIN
15:26:54 2726  
15:26:54 2727  	 IF in_old_fin_instrument_type = GLOBAL_ENUMS_V19.INSTRUMENT_CREDIT_CARD THEN
15:26:54 2728  	   IF PROCS_FIN_INSTRUMENTS_V19.IS_CREDIT_CARD_EXISTS(in_old_fin_instrument_id) = GLOBAL_CONSTANTS_V19.FALSE THEN
15:26:54 2729  	     -- throw exception: bad old credit card
15:26:54 2730  	     RAISE BAD_OLD_CC;
15:26:54 2731  	   END IF;
15:26:54 2732  	 ELSIF in_old_fin_instrument_type = GLOBAL_ENUMS_V19.INSTRUMENT_PAYPAL THEN
15:26:54 2733  	   IF PROCS_FIN_INSTRUMENTS_V19.IS_PAYPAL_EXISTS(in_old_fin_instrument_id) = GLOBAL_CONSTANTS_V19.FALSE THEN
15:26:54 2734  	     -- throw exception: bad old paypal
15:26:54 2735  	     RAISE BAD_OLD_PP;
15:26:54 2736  	   END IF;
15:26:54 2737  	 ELSE
15:26:54 2738  	   -- throw exception: bad instrument type
15:26:54 2739  	   RAISE BAD_OLD_TYPE;
15:26:54 2740  	 END IF;
15:26:54 2741  
15:26:54 2742  	 IF in_new_fin_instrument_type = GLOBAL_ENUMS_V19.INSTRUMENT_CREDIT_CARD THEN
15:26:54 2743  	   IF PROCS_FIN_INSTRUMENTS_V19.IS_CREDIT_CARD_EXISTS(in_new_fin_instrument_id) = GLOBAL_CONSTANTS_V19.FALSE THEN
15:26:54 2744  	     -- throw exception: bad new credit card
15:26:54 2745  	     RAISE BAD_NEW_CC;
15:26:54 2746  	   END IF;
15:26:54 2747  	 ELSIF in_new_fin_instrument_type = GLOBAL_ENUMS_V19.INSTRUMENT_PAYPAL THEN
15:26:54 2748  	   IF PROCS_FIN_INSTRUMENTS_V19.IS_PAYPAL_EXISTS(in_new_fin_instrument_id) = GLOBAL_CONSTANTS_V19.FALSE THEN
15:26:54 2749  	     -- throw exception: bad new paypal
15:26:54 2750  	     RAISE BAD_NEW_PP;
15:26:54 2751  	   END IF;
15:26:54 2752  	 ELSE
15:26:54 2753  	   -- throw exception: bad new instrument type
15:26:54 2754  	   RAISE BAD_NEW_TYPE;
15:26:54 2755  	 END IF;
15:26:54 2756  
15:26:54 2757  	 -- Check that owner of both instruments - same man
15:26:54 2758  
15:26:54 2759  	 SELECT count(1) into var_accounts_count FROM (
15:26:54 2760  	   SELECT
15:26:54 2761  	     CC.ACCOUNT_ID
15:26:54 2762  	   FROM
15:26:54 2763  	     CREDIT_CARD CC
15:26:54 2764  	   WHERE
15:26:54 2765  	     (
15:26:54 2766  	       CC.ID = in_old_fin_instrument_id
15:26:54 2767  	       AND in_old_fin_instrument_type = GLOBAL_ENUMS_V19.INSTRUMENT_CREDIT_CARD
15:26:54 2768  	     )
15:26:54 2769  	     OR
15:26:54 2770  	     (
15:26:54 2771  	       CC.ID = in_new_fin_instrument_id
15:26:54 2772  	       AND in_new_fin_instrument_type = GLOBAL_ENUMS_V19.INSTRUMENT_CREDIT_CARD
15:26:54 2773  	     )
15:26:54 2774  	   UNION
15:26:54 2775  	   SELECT
15:26:54 2776  	     PP.ACCOUNT_ID
15:26:54 2777  	   FROM
15:26:54 2778  	     PAYPAL PP
15:26:54 2779  	   WHERE
15:26:54 2780  	     (
15:26:54 2781  	       PP.ID = in_old_fin_instrument_id
15:26:54 2782  	       AND in_old_fin_instrument_type = GLOBAL_ENUMS_V19.INSTRUMENT_PAYPAL
15:26:54 2783  	     )
15:26:54 2784  	     OR
15:26:54 2785  	     (
15:26:54 2786  	       PP.ID = in_new_fin_instrument_id
15:26:54 2787  	       AND in_new_fin_instrument_type = GLOBAL_ENUMS_V19.INSTRUMENT_PAYPAL
15:26:54 2788  	     )
15:26:54 2789  	 )
15:26:54 2790  	 WHERE
15:26:54 2791  	   account_id IS NOT NULL;
15:26:54 2792  
15:26:54 2793  	 IF (var_accounts_count > 1) THEN
15:26:54 2794  	   -- Throw exception: different owners of instruments
15:26:54 2795  	   RAISE DIFFERENT_OWNERS;
15:26:54 2796  	 END IF;
15:26:54 2797  
15:26:54 2798  	 FOR f_sub IN (
15:26:54 2799  	   select
15:26:54 2800  	     s.id
15:26:54 2801  	   FROM
15:26:54 2802  	     subscription s
15:26:54 2803  	   WHERE
15:26:54 2804  	     (
15:26:54 2805  	       s.subscription_status_id = GLOBAL_STATUSES_V19.SUBSCRIPTION_ACTIVE OR
15:26:54 2806  	       s.subscription_status_id = GLOBAL_STATUSES_V19.SUBSCRIPTION_SUSPENDED OR
15:26:54 2807  	       s.subscription_status_id = GLOBAL_STATUSES_V19.SUBSCRIPTION_IN_GRACE_PERIOD
15:26:54 2808  	     )
15:26:54 2809  	     AND
15:26:54 2810  	     s.instrument_type_id = in_old_fin_instrument_type AND
15:26:54 2811  	     s.instrument_id = in_old_fin_instrument_id
15:26:54 2812  	 ) LOOP
15:26:54 2813  	   PROCS_SUBSCRIPTION_CRU_V19.UPDATE_SUBSCRIPTION(
15:26:54 2814  	     in_subscription_id => f_sub.id,
15:26:54 2815  	     in_instrument_type_id => in_new_fin_instrument_type,
15:26:54 2816  	     in_instrument_id => in_new_fin_instrument_id,
15:26:54 2817  	     in_updated_by => in_updated_by
15:26:54 2818  	   );
15:26:54 2819  	 END LOOP;
15:26:54 2820  
15:26:54 2821  	 FOR f_open_charge IN (
15:26:54 2822  	   select
15:26:54 2823  	     ch.id,
15:26:54 2824  	     ch.invoice_id,
15:26:54 2825  	     ch.transaction_id,
15:26:54 2826  	     ch.charge_amount
15:26:54 2827  	   FROM
15:26:54 2828  	     charge ch
15:26:54 2829  	   WHERE
15:26:54 2830  	     ch.instrument_type_id = in_old_fin_instrument_type
15:26:54 2831  	     AND ch.instrument_id = in_old_fin_instrument_id
15:26:54 2832  	     AND ch.charge_status_id = GLOBAL_STATUSES_V19.CHARGE_OPENED
15:26:54 2833  	 ) LOOP
15:26:54 2834  
15:26:54 2835  	   FOR f_pending_transaction IN (
15:26:54 2836  	     select
15:26:54 2837  	       id, transaction_amount, order_id, is_refund
15:26:54 2838  	     from
15:26:54 2839  	       transaction
15:26:54 2840  	     where
15:26:54 2841  	       id = f_open_charge.transaction_id
15:26:54 2842  	       and transaction_status_id = GLOBAL_STATUSES_V19.TRANSACTION_PENDING
15:26:54 2843  	   ) LOOP
15:26:54 2844  
15:26:54 2845  	     SELECT
15:26:54 2846  	       DECODE(TRANSACTION_TYPE_CODE, 'RECURRING_BILLING', 'RECURRING_BILLING_USER_UPDATE',
15:26:54 2847  					     'GRACE_PERIOD_FINAL', 'GRACE_PERIOD_USER_UPDATE',
15:26:54 2848  					     TRANSACTION_TYPE_CODE)
15:26:54 2849  	     INTO var_transaction_type
15:26:54 2850  	     FROM
15:26:54 2851  	       Transaction
15:26:54 2852  	     WHERE
15:26:54 2853  	       id = f_pending_transaction.id
15:26:54 2854  	       AND ROWNUM <= 1;
15:26:54 2855  
15:26:54 2856  	     PROCS_TRANSACTION_V19.CREATE_TRANSACTION (
15:26:54 2857  	       in_transaction_id => NULL,
15:26:54 2858  	       in_status_id  => GLOBAL_STATUSES_V19.TRANSACTION_PENDING,
15:26:54 2859  	       in_amount     => f_pending_transaction.transaction_amount,
15:26:54 2860  	       in_created_by => in_updated_by,
15:26:54 2861  	       in_order_id   => null,
15:26:54 2862  	       in_is_refund  => f_pending_transaction.is_refund,
15:26:54 2863  	       in_transaction_type_code => var_transaction_type,
15:26:54 2864  	       out_transaction_id => temp_out_transaction_id
15:26:54 2865  	     );
15:26:54 2866  
15:26:54 2867  	     PROCS_TRANSACTION_V19.UPDATE_TRANSACTION_STATUS(
15:26:54 2868  	       in_transaction_id => f_pending_transaction.id,
15:26:54 2869  	       in_updated_by	 => in_updated_by,
15:26:54 2870  	       in_transaction_status_id  => GLOBAL_STATUSES_V19.TRANSACTION_CLOSED
15:26:54 2871  	     );
15:26:54 2872  
15:26:54 2873  	     -- Create new charge
15:26:54 2874  	     PROCS_CHARGE_V19.CREATE_CHARGE (
15:26:54 2875  	       in_invoice_id	     => f_open_charge.invoice_id,
15:26:54 2876  	       in_transaction_id     => temp_out_transaction_id,
15:26:54 2877  	       in_instrument_type_id => in_new_fin_instrument_type,
15:26:54 2878  	       in_instrument_id      => in_new_fin_instrument_id,
15:26:54 2879  	       in_charge_amount      => f_open_charge.charge_amount,
15:26:54 2880  	       in_created_by	     => in_updated_by,
15:26:54 2881  	       in_charge_status_id   => GLOBAL_STATUSES_V19.CHARGE_OPENED,
15:26:54 2882  	       out_charge_id	     => temp_out_charge_id
15:26:54 2883  	     );
15:26:54 2884  	     -- Cancel old charge
15:26:54 2885  	     PROCS_CHARGE_V19.UPDATE_CHARGE_STATUS(
15:26:54 2886  	       in_charge_id	   => f_open_charge.id,
15:26:54 2887  	       in_updated_by	   => in_updated_by,
15:26:54 2888  	       in_charge_status_id => GLOBAL_STATUSES_V19.CHARGE_CANCELED
15:26:54 2889  	     );
15:26:54 2890  
15:26:54 2891  	     PROCS_ADJUSTMENTS_V19.UPDATE_INVOICE_ADJUSTMENT(
15:26:54 2892  	       IN_INVOICE_ID => f_open_charge.invoice_id,
15:26:54 2893  	       IN_ORIGINAL_CHARGE_ID => f_open_charge.id,
15:26:54 2894  	       IN_CHARGE_ID => temp_out_charge_id,
15:26:54 2895  	       IN_UPDATED_BY => in_updated_by
15:26:54 2896  	     );
15:26:54 2897  
15:26:54 2898  	   END LOOP;
15:26:54 2899  	 END LOOP;
15:26:54 2900  
15:26:54 2901  EXCEPTION
