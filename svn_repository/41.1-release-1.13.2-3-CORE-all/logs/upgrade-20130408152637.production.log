15:26:53 781  	--   end loop
15:26:53 782  	-- end
15:26:53 783  
15:26:53 784  	SELECT
15:26:53 785  	  SUBSCRIPTION.ID into temp_subscription_id
15:26:53 786  	FROM
15:26:53 787  	  SUBSCRIPTION
15:26:53 788  	WHERE
15:26:53 789  	  SUBSCRIPTION.ID = in_subscription_id;
15:26:53 790  
15:26:53 791  	OPEN out_result_set FOR
15:26:53 792  	SELECT
15:26:53 793  	  INVOICE.ID,
15:26:53 794  	  INVOICE.INVOICE_STATUS_ID,
15:26:53 795  	  INVOICE.CREATE_DATE,
15:26:53 796  	  INVOICE.CREATED_BY,
15:26:53 797  	  INVOICE.UPDATE_DATE,
15:26:53 798  	  INVOICE.UPDATED_BY,
15:26:53 799  	  INVOICE.TAX_EXEMPT_ID
15:26:53 800  	FROM
15:26:53 801  	  LICENSE
15:26:53 802  	    INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
15:26:53 803  	WHERE
15:26:53 804  	  LICENSE.SUBSCRIPTION_ID = in_subscription_id;
15:26:53 805  
15:26:53 806  EXCEPTION
15:26:53 807  WHEN NO_DATA_FOUND THEN
15:26:53 808  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:53 809  	  SPROC_NAME, 'Could not find subscription with given ID');
15:26:53 810  WHEN OTHERS THEN
15:26:53 811  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:53 812  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:26:53 813  END GET_SUBSCRIPTION_INVOICES;
