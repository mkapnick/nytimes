15:26:53 1164  PRODUCT_ELIGIBILITY_NAME CONSTANT VARCHAR2(19) := 'MAX_CONCURRENT_SUBS';
15:26:53 1165  -- VARIABLES
15:26:53 1166  temp_offer_chain_id	OFFER_CHAIN.ID%TYPE;
15:26:53 1167  var_same_products	SYS_REFCURSOR;
15:26:53 1168  var_same_product_id	NUMBER;
