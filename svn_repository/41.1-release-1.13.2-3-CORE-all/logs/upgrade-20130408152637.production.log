15:26:54 574  
15:26:54 575  PROCEDURE GET_ACCOUNT_NOTES (
15:26:54 576  /*
15:26:54 577  Throws exceptions:
15:26:54 578  APP_EXCEPTION_CODES_V19.NOT_FOUND
15:26:54 579  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
15:26:54 580  */
15:26:54 581  	  in_group_id	 IN  NUMBER,
15:26:54 582  	  out_result_set OUT SYS_REFCURSOR
15:26:54 583  ) AS
15:26:54 584  -- VARIABLES
15:26:54 585  SPROC_NAME      CONSTANT VARCHAR2(17) := 'GET_ACCOUNT_NOTES';
15:26:54 586  var_account_id NUMBER;
15:26:54 587  -- EXCEPTIONS
15:26:54 588  BAD_ACCOUNT_ID EXCEPTION;
15:26:54 589  BEGIN
15:26:54 590  
15:26:54 591  	-- Check that account is exists
15:26:54 592  	BEGIN
15:26:54 593  	  SELECT
15:26:54 594  	    ACCOUNT.ID into var_account_id
15:26:54 595  	  FROM
15:26:54 596  	    ACCOUNT
15:26:54 597  	  WHERE
15:26:54 598  	    ACCOUNT.GROUP_ID = in_group_id;
15:26:54 599  	  EXCEPTION
15:26:54 600  	    WHEN NO_DATA_FOUND THEN
15:26:54 601  	      RAISE BAD_ACCOUNT_ID;
15:26:54 602  	END;
15:26:54 603  
15:26:54 604  	-- Get account notes
