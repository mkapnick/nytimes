15:26:53 2113  
15:26:53 2114  PROCEDURE CANCEL_SUBSCRIPTION_INVOICE (
15:26:53 2115  /*
15:26:53 2116  APP_EXCEPTION_CODES_V19.NOT_FOUND
15:26:53 2117  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
15:26:53 2118  APP_EXCEPTION_CODES_V19.INTERNAL_ERROR
15:26:53 2119  */
15:26:53 2120  	 in_invoice_id	      IN NUMBER,
15:26:53 2121  	 in_updated_by	      IN VARCHAR2,
15:26:53 2122  	 in_refundable	      IN NUMBER DEFAULT GLOBAL_CONSTANTS_V19.FALSE
15:26:53 2123  ) AS
15:26:53 2124  SPROC_NAME CONSTANT VARCHAR2(27) := 'CANCEL_SUBSCRIPTION_INVOICE';
15:26:53 2125  -- VARIABLES
15:26:53 2126  temp_invoice_id	      INVOICE.ID%TYPE;
15:26:53 2127  var_chargeback_amount  NUMBER(10,2);
15:26:53 2128  ver_refund_charge_id   NUMBER;
15:26:53 2129  -- EXCEPTIONS
15:26:53 2130  BAD_INVOICE_ID		    EXCEPTION;
15:26:53 2131  CAN_NOT_CALCULATE_CHARGEBACK EXCEPTION;
15:26:53 2132  CAN_NOT_APPLY_CHARGEBACK     EXCEPTION;
15:26:53 2133  EXCEPTION_MESSAGE	    VARCHAR2(1024);
15:26:53 2134  -- STUB
15:26:53 2135  var_now DATE;
15:26:53 2136  var_revoke NUMBER;
15:26:53 2137  var_refund NUMBER;
15:26:53 2138  var_billed NUMBER;
15:26:53 2139  var_subscription_in_grace NUMBER;
15:26:53 2140  BEGIN
15:26:53 2141  
15:26:53 2142  	 var_now := sysdate;
15:26:53 2143  
15:26:53 2144  	 -- Check that invoice exists
15:26:53 2145  	 BEGIN
15:26:53 2146  	   SELECT
15:26:53 2147  	     INVOICE.ID into temp_invoice_id
15:26:53 2148  	   FROM
15:26:53 2149  	     INVOICE
15:26:53 2150  	   WHERE
15:26:53 2151  	     INVOICE.ID = in_invoice_id;
15:26:53 2152  	   EXCEPTION
15:26:53 2153  	     WHEN NO_DATA_FOUND THEN
15:26:53 2154  	       RAISE BAD_INVOICE_ID;
15:26:53 2155  	 END;
15:26:53 2156  
15:26:53 2157  	 select decode(count(1), 0, GLOBAL_CONSTANTS_V19.FALSE, GLOBAL_CONSTANTS_V19.TRUE) into var_revoke
15:26:53 2158  	 from license l, subscription s, offer_chain oc
15:26:53 2159  	 where
15:26:53 2160  	   l.subscription_id = s.id and
15:26:53 2161  	   s.offer_chain_id = oc.id and
15:26:53 2162  	   l.invoice_id = in_invoice_id and
15:26:53 2163  	   oc.revoke_entitlements = GLOBAL_CONSTANTS_V19.TRUE and
15:26:53 2164  	   rownum < 2;
15:26:53 2165  
15:26:53 2166  	 select
15:26:53 2167  	   decode(s.subscription_status_id, GLOBAL_STATUSES_V19.SUBSCRIPTION_IN_GRACE_PERIOD, 1, 0)
15:26:53 2168  	 into
15:26:53 2169  	   var_subscription_in_grace
15:26:53 2170  	 from license l, subscription s
15:26:53 2171  	 where
15:26:53 2172  	   l.subscription_id = s.id and
15:26:53 2173  	   l.invoice_id = in_invoice_id and
15:26:53 2174  	   rownum < 2;
15:26:53 2175  
15:26:53 2176  	 var_billed := PROCS_INVOICE_V19.IS_INVOICE_PAYING_STARTED(in_invoice_id);
15:26:53 2177  	 var_refund := GLOBAL_CONSTANTS_V19.FALSE;
15:26:53 2178  
15:26:53 2179  	 -- Check that transaction for given invoice not started
15:26:53 2180  	 -- if refund enabled calculate and apply chargeback
15:26:53 2181  	 IF (
15:26:53 2182  	     var_billed = GLOBAL_CONSTANTS_V19.TRUE
15:26:53 2183  	   )THEN
15:26:53 2184  	   if (in_refundable = GLOBAL_CONSTANTS_V19.TRUE) then
15:26:53 2185  	     -- If started then we need to calculate refund
15:26:53 2186  	     BEGIN
15:26:53 2187  	       PROCS_INVOICE_V19.CALCULATE_INVOICE_CHARGEBACK(
15:26:53 2188  		 in_invoice_id,
15:26:53 2189  		 var_now,
15:26:53 2190  		 var_chargeback_amount
15:26:53 2191  	       );
15:26:53 2192  	       EXCEPTION
15:26:53 2193  		 WHEN OTHERS THEN
15:26:53 2194  		   EXCEPTION_MESSAGE := SQLERRM;
15:26:53 2195  		   RAISE CAN_NOT_CALCULATE_CHARGEBACK;
15:26:53 2196  	     END;
15:26:53 2197  	     IF var_chargeback_amount > 0 THEN
15:26:53 2198  	       BEGIN
15:26:53 2199  		 PROCS_INVOICE_V19.APPLY_REFUND(
15:26:53 2200  		   in_invoice_id,
15:26:53 2201  		   var_chargeback_amount,
15:26:53 2202  		   in_updated_by,
15:26:53 2203  		   ver_refund_charge_id
15:26:53 2204  		 );
15:26:53 2205  		 EXCEPTION
15:26:53 2206  		   WHEN OTHERS THEN
