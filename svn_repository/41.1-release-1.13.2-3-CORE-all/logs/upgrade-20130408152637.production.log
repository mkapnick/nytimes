15:26:53 358  	    in_current_offer_recurr_num => 1,
15:26:53 359  	    out_license_id		=> out_new_license_id
15:26:53 360  	  );
15:26:53 361  	  EXCEPTION
15:26:53 362  	    WHEN OTHERS THEN
