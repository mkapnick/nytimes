15:26:53 517  
15:26:53 518  PROCEDURE SUSPEND_SUBSCRIPTION(
15:26:53 519  /*
15:26:53 520  Throws exceptions:
15:26:53 521  APP_EXCEPTION_CODES_V19.NOT_FOUND
15:26:53 522  APP_EXCEPTION_CODES_V19.INTERNAL_ERROR
15:26:53 523  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
15:26:53 524  */
15:26:53 525  	  in_subs_id	IN NUMBER,
15:26:53 526  	  in_updated_by IN VARCHAR2
15:26:53 527  ) AS
15:26:53 528  SPROC_NAME		  CONSTANT VARCHAR2(20) := 'SUSPEND_SUBSCRIPTION';
15:26:53 529  var_subscription_status_id  NUMBER;
15:26:53 530  var_license_id		  NUMBER;
15:26:53 531  var_offer_id		  NUMBER;
15:26:53 532  var_license_start_date	  DATE;
15:26:53 533  var_license_end_date	  DATE;
15:26:53 534  
15:26:53 535  var_entitlement_dupration   VARCHAR2(30);
15:26:53 536  var_d_entitlement_dupration NUMBER;
15:26:53 537  
15:26:53 538  var_ym_interval INTERVAL YEAR TO MONTH;
15:26:53 539  var_ds_interval INTERVAL DAY(3) TO SECOND;
15:26:53 540  
15:26:53 541  -- EXCEPTIONS
15:26:53 542  BAD_SUBSCRIPTION_ID     EXCEPTION;
15:26:53 543  BAD_SUBSCRIPTION_STATUS EXCEPTION;
15:26:53 544  NO_LICENSE_FOUND	      EXCEPTION;
15:26:53 545  NO_OFFER_FOUND	      EXCEPTION;
15:26:53 546  EXCEPTION_MESSAGE       VARCHAR2(1024);
15:26:53 547  BEGIN
15:26:53 548  	-- TODO: Finish this prcedure (in Phase II)
15:26:53 549  
15:26:53 550  	-- Get subscription by id. FAULT if no such subscription.
15:26:53 551  	-- begin TX
15:26:53 552  	--   Get for update associated license (subscription.license_id). FAULT if not found.
15:26:53 553  	--   Set status to PROCESSED.
15:26:53 554  	--   updated record.
15:26:53 555  	--   compute days remaining in the subscription: original end_date - today = days_remaining_adjustment
15:26:53 556  	--   new subscription status is SUSPENDED.
15:26:53 557  	--   suspend_date is now.
15:26:53 558  	--   update subscription record.
15:26:53 559  	-- end TX
15:26:53 560  
15:26:53 561  	BEGIN
15:26:53 562  	  SELECT
15:26:53 563  	    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID into var_subscription_status_id
15:26:53 564  	  FROM
15:26:53 565  	    SUBSCRIPTION
15:26:53 566  	  WHERE
15:26:53 567  	    SUBSCRIPTION.ID = in_subs_id;
15:26:53 568  	  EXCEPTION
15:26:53 569  	  WHEN NO_DATA_FOUND THEN
15:26:53 570  	    RAISE BAD_SUBSCRIPTION_ID;
15:26:53 571  	END;
15:26:53 572  
15:26:53 573  	IF var_subscription_status_id != GLOBAL_STATUSES_V19.SUBSCRIPTION_ACTIVE THEN
15:26:53 574  	  RAISE BAD_SUBSCRIPTION_STATUS;
15:26:53 575  	END IF;
15:26:53 576  
15:26:53 577  	BEGIN
15:26:53 578  	  SELECT
15:26:53 579  	    LICENSE.ID,
15:26:53 580  	    LICENSE.OFFER_ID,
15:26:53 581  	    LICENSE.START_DATE
15:26:53 582  	    into
15:26:53 583  	    var_license_id,
15:26:53 584  	    var_offer_id,
15:26:53 585  	    var_license_start_date
15:26:53 586  	  FROM
15:26:53 587  	    LICENSE
15:26:53 588  	  WHERE
15:26:53 589  	    LICENSE.SUBSCRIPTION_ID = in_subs_id
15:26:53 590  	      AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V19.LICENSE_ACTIVE;
15:26:53 591  	  EXCEPTION
15:26:53 592  	  WHEN NO_DATA_FOUND THEN
15:26:53 593  	    RAISE NO_LICENSE_FOUND;
15:26:53 594  	END;
15:26:53 595  
15:26:53 596  	PROCS_LICENSE_CRU_V19.UPDATE_LICENSE(
15:26:53 597  	  in_license_id        => var_license_id,
15:26:53 598  	  in_updated_by        => in_updated_by,
15:26:53 599  	  in_license_status_id => GLOBAL_STATUSES_V19.LICENSE_CLOSED
15:26:53 600  	);
15:26:53 601  
15:26:53 602  	BEGIN
15:26:53 603  	  SELECT
15:26:53 604  	    OFFER.ENTITLEMENT_DURATION into var_entitlement_dupration
15:26:53 605  	  FROM
15:26:53 606  	    OFFER
15:26:53 607  	  WHERE
15:26:53 608  	    OFFER.ID = var_offer_id;
15:26:53 609  	  EXCEPTION
15:26:53 610  	  WHEN NO_DATA_FOUND THEN
15:26:53 611  	    RAISE NO_OFFER_FOUND;
15:26:53 612  	END;
15:26:53 613  
15:26:53 614  	var_ym_interval := substr(var_entitlement_dupration, 0, 4);
15:26:53 615  	var_ds_interval := substr(var_entitlement_dupration, 4);
15:26:53 616  
15:26:53 617  	var_license_end_date := var_license_start_date + var_ym_interval + var_ds_interval;
15:26:53 618  
15:26:53 619  	var_d_entitlement_dupration := var_license_end_date - current_date;
15:26:53 620  
15:26:53 621  	PROCS_SUBSCRIPTION_CRU_V19.UPDATE_SUBSCRIPTION(
15:26:53 622  	  in_subscription_id	       => in_subs_id,
15:26:53 623  	  in_updated_by 	       => in_updated_by,
15:26:53 624  	  in_suspend_date	       => SYSDATE,
15:26:53 625  	  in_subscription_status_id    => GLOBAL_STATUSES_V19.SUBSCRIPTION_SUSPENDED,
15:26:53 626  	  in_days_remainning_ajustment => var_d_entitlement_dupration
15:26:53 627  	);
15:26:53 628  
15:26:53 629  EXCEPTION
15:26:53 630  WHEN BAD_SUBSCRIPTION_ID THEN
15:26:53 631  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:53 632  	  SPROC_NAME, 'No such subscription');
15:26:53 633  WHEN BAD_SUBSCRIPTION_STATUS THEN
15:26:53 634  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
15:26:53 635  	  SPROC_NAME, 'Subscription is not active');
15:26:53 636  WHEN NO_LICENSE_FOUND THEN
15:26:53 637  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:53 638  	  SPROC_NAME, 'Can not find license associated with given subscription ID');
15:26:53 639  WHEN NO_OFFER_FOUND THEN
15:26:53 640  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:53 641  	  SPROC_NAME, 'Can not find offer associated with given subscription ID');
15:26:53 642  WHEN OTHERS THEN
15:26:53 643  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:53 644  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:26:53 645  END SUSPEND_SUBSCRIPTION;
