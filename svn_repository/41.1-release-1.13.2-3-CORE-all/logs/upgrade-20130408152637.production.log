15:26:54 671  
15:26:54 672  PROCEDURE GET_ACCOUNT_SUBSCRIPTIONS (
15:26:54 673  	  in_group_id	 IN  NUMBER,
15:26:54 674  	  in_start_date  IN DATE,
15:26:54 675  	  in_end_date	 IN DATE,
15:26:54 676  	  in_status	 IN NUMBER,
15:26:54 677  	  in_group_account_type IN VARCHAR2,
15:26:54 678  	  out_result_set OUT SYS_REFCURSOR
15:26:54 679  ) AS
15:26:54 680  -- VARIABLES
15:26:54 681  SPROC_NAME     CONSTANT VARCHAR2(25) := 'GET_ACCOUNT_SUBSCRIPTIONS';
15:26:54 682  var_account_id NUMBER;
15:26:54 683  -- EXCEPTIONS
15:26:54 684  BAD_GROUP_ID	      EXCEPTION;
15:26:54 685  BEGIN
15:26:54 686  	-- Get account id
15:26:54 687  	BEGIN
15:26:54 688  	  SELECT
15:26:54 689  	    ACCOUNT.ID INTO var_account_id
15:26:54 690  	  FROM
15:26:54 691  	    ACCOUNT
15:26:54 692  	  WHERE
15:26:54 693  	    ACCOUNT.GROUP_ID = in_group_id;
15:26:54 694  	  EXCEPTION
15:26:54 695  	    WHEN NO_DATA_FOUND THEN
15:26:54 696  	      RAISE BAD_GROUP_ID;
15:26:54 697  	END;
15:26:54 698  
15:26:54 699  	-- Get information about account subscriptions
15:26:54 700  	OPEN out_result_set FOR
15:26:54 701  	SELECT
15:26:54 702  	  in_group_id AS "GROUP_ID",
15:26:54 703  	  SUBSCRIPTION.ID AS "SUBSCRIPTION_ID",
15:26:54 704  	  SUBSCRIPTION.SUBSCRIPTION_STATUS_ID,
15:26:54 705  	  SUBSCRIPTION.PURCHASE_DATE,
15:26:54 706  	  SUBSCRIPTION.SUSPEND_DATE,
15:26:54 707  	  SUBSCRIPTION.REACTIVATION_DATE,
15:26:54 708  	  SUBSCRIPTION.CANCELLATION_DATE,
15:26:54 709  	  SUBSCRIPTION_CANCEL_REASON.VALUE as "CANCEL_TYPE",
15:26:54 710  	  SUBSCRIPTION.INSTRUMENT_ID,
15:26:54 711  	  SUBSCRIPTION.INSTRUMENT_TYPE_ID,
15:26:54 712  	  OFFER_CHAIN.ID AS "OFFER_CHAIN_ID",
15:26:54 713  	  OFFER_CHAIN.NAME,
15:26:54 714  	  OFFER_CHAIN.DESCRIPTION,
15:26:54 715  	  OFFER_CHAIN.PRODUCT_URI,
15:26:54 716  	  PROCS_SUBSCRIPTION_V19.CALC_SUBSCRIPTION_END_DATE(SUBSCRIPTION.ID) as "END_DATE",
15:26:54 717  	  PROCS_SUBSCRIPTION_V19.GET_RECENT_CHARGE(SUBSCRIPTION.ID) AS "RECENT_CHARGE",
15:26:54 718  	  PROCS_SUBSCRIPTION_V19.GET_RENEWAL_DATE(SUBSCRIPTION.ID) AS "RENEWAL_DATE",
15:26:54 719  	  PROCS_SUBSCRIPTION_V19.GET_BILLING_CYCLE(SUBSCRIPTION.ID) AS "BILLING_CYCLE",
15:26:54 720  	  PROCS_SUBSCRIPTION_V19.IS_SUBSCRIPTION_CANCELABLE(SUBSCRIPTION.ID) AS "IS_CANCELABLE",
15:26:54 721  	  ITUNES_RECEIPT.ID AS "ITUNES_RECEIPT_ID",
15:26:54 722  	  (
15:26:54 723  	    SELECT
15:26:54 724  	      MAX(ENTITLEMENT_END_DATE)
15:26:54 725  	      FROM LICENSE
15:26:54 726  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:26:54 727  	  )
15:26:54 728  	  as "ENT_END_DATE",
15:26:54 729  	  (
15:26:54 730  	    SELECT
15:26:54 731  	      MIN(START_DATE)
15:26:54 732  	      FROM LICENSE
15:26:54 733  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:26:54 734  	  )
15:26:54 735  	  as "ENT_START_DATE",
15:26:54 736  	  GROUP_ACCOUNT.SUBSCRIPTION_ID GA_SUBSCRIPTION_ID,
15:26:54 737  	  GROUP_ACCOUNT.ID GA_ID,
15:26:54 738  	  GROUP_ACCOUNT.GROUP_NAME GA_GROUP_NAME,
15:26:54 739  	  GROUP_ACCOUNT.FIRST_NAME GA_FIRST_NAME,
15:26:54 740  	  GROUP_ACCOUNT.LAST_NAME GA_LAST_NAME,
15:26:54 741  	  GROUP_ACCOUNT.EMAIL GA_EMAIL,
15:26:54 742  	  GROUP_ACCOUNT.PHONE GA_PHONE,
15:26:54 743  	  GROUP_ACCOUNT.ORGANIZATION_TYPE GA_ORGANIZATION_TYPE,
15:26:54 744  	  GROUP_ACCOUNT.SEATS GA_SEATS,
15:26:54 745  	  PROCS_GROUP_ACCOUNT_V19.F_GET_NUM_OCCUPIED_GROUP_SEATS(GROUP_ACCOUNT.ID) GA_SEATS_USED,
15:26:54 746  	  GROUP_ACCOUNT.IP GA_IP,
15:26:54 747  	  PROCS_SUBSCRIPTION_V19.GET_GIFT_CERT_CODE_BY_SUB_ID(SUBSCRIPTION.ID) GIFT_CERTIFICATE_CODE,
15:26:54 748  	  PROCS_ACCOUNT_V19.GET_GRACE_START_DATE(SUBSCRIPTION.ID) GRACE_START_DATE,
15:26:54 749  	  PROCS_ACCOUNT_V19.GET_GRACE_END_DATE(SUBSCRIPTION.ID) GRACE_END_DATE
15:26:54 750  	FROM
15:26:54 751  	  SUBSCRIPTION
15:26:54 752  	  INNER JOIN OFFER_CHAIN ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
15:26:54 753  	  LEFT JOIN SUBSCRIPTION_CANCEL_REASON ON SUBSCRIPTION.SCT_ID = SUBSCRIPTION_CANCEL_REASON.ID
15:26:54 754  	  LEFT JOIN ITUNES_RECEIPT ON SUBSCRIPTION.ID = ITUNES_RECEIPT.SUBSCRIPTION_ID
15:26:54 755  	  LEFT JOIN GROUP_ACCOUNT ON SUBSCRIPTION.ID = GROUP_ACCOUNT.SUBSCRIPTION_ID
15:26:54 756  	WHERE
15:26:54 757  	  SUBSCRIPTION.ACCOUNT_ID = var_account_id
15:26:54 758  	  AND (SUBSCRIPTION.SCT_ID IS NULL OR SUBSCRIPTION.SCT_ID != GLOBAL_STATUSES_V19.REAL_TIME_CANCEL_REASON)
15:26:54 759  	  AND SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = NVL(in_status, SUBSCRIPTION.SUBSCRIPTION_STATUS_ID)
15:26:54 760  	  AND PROCS_COMMON_V19.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE) >= NVL(in_start_date, PROCS_COMMON_V19.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE))
15:26:54 761  	  AND PROCS_COMMON_V19.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE) <= NVL(in_end_date, PROCS_COMMON_V19.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE))
15:26:54 762  	  AND (in_group_account_type IS NULL OR OFFER_CHAIN.GROUP_ACCOUNT_TYPE_ID = in_group_account_type);
15:26:54 763  
15:26:54 764  EXCEPTION
15:26:54 765  WHEN BAD_GROUP_ID THEN
15:26:54 766  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 767  	  SPROC_NAME, 'Can not find account with given group id');
15:26:54 768  WHEN OTHERS THEN
15:26:54 769  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:54 770  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:26:54 771  END GET_ACCOUNT_SUBSCRIPTIONS;
15:26:54 772  
15:26:54 773  /******************************************************************************/
15:26:54 774  
15:26:54 775  PROCEDURE FREEZE_ACCOUNT (
15:26:54 776  /*
15:26:54 777  Throws exceptions:
15:26:54 778  APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 779  APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 780  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
15:26:54 781  */
15:26:54 782  	in_group_id   IN NUMBER,
15:26:54 783  	in_updated_by IN VARCHAR2,
15:26:54 784  	in_note       IN VARCHAR2,
15:26:54 785  	in_agent_id   IN NUMBER
15:26:54 786  ) AS
15:26:54 787  SPROC_NAME	    CONSTANT VARCHAR2(14) := 'FREEZE_ACCOUNT';
15:26:54 788  -- VARIABLES
15:26:54 789  var_account_id	    NUMBER;
15:26:54 790  var_account_status_id NUMBER;
15:26:54 791  -- EXCEPTIONS
15:26:54 792  BAD_GROUP_ID	    EXCEPTION;
15:26:54 793  BAD_ACCOUNT_STATUS_ID EXCEPTION;
15:26:54 794  CAN_NOT_CREATE_NOTE   EXCEPTION;
15:26:54 795  EXCEPTION_MESSAGE     VARCHAR2(1024);
15:26:54 796  BEGIN
15:26:54 797  
15:26:54 798  	-- Get account status, account id
15:26:54 799  	BEGIN
15:26:54 800  	  SELECT
15:26:54 801  	    ACCOUNT.ID,
15:26:54 802  	    ACCOUNT.ACCOUNT_STATUS_ID
15:26:54 803  	    into
15:26:54 804  	    var_account_id,
15:26:54 805  	    var_account_status_id
15:26:54 806  	  FROM
15:26:54 807  	    ACCOUNT
15:26:54 808  	  WHERE
15:26:54 809  	    ACCOUNT.GROUP_ID = in_group_id;
15:26:54 810  	  EXCEPTION
15:26:54 811  	    WHEN NO_DATA_FOUND THEN
15:26:54 812  	      RAISE BAD_GROUP_ID;
15:26:54 813  	END;
15:26:54 814  
15:26:54 815  	-- We can freeze only ACTIVE accounts
15:26:54 816  	IF var_account_status_id != GLOBAL_STATUSES_V19.ACCOUNT_ACTIVE
15:26:54 817  	  AND var_account_status_id != GLOBAL_STATUSES_V19.ACCOUNT_FROZEN THEN
15:26:54 818  	  RAISE BAD_ACCOUNT_STATUS_ID;
15:26:54 819  	END IF;
15:26:54 820  
15:26:54 821  	-- Set account status
15:26:54 822  	PROCS_ACCOUNT_V19.UPDATE_ACCOUNT_STATUS(
15:26:54 823  	  in_account_id        => var_account_id,
15:26:54 824  	  in_updated_by        => in_updated_by,
15:26:54 825  	  in_account_status_id => GLOBAL_STATUSES_V19.ACCOUNT_FROZEN
15:26:54 826  	);
15:26:54 827  
15:26:54 828  	-- Annotate account
15:26:54 829  	BEGIN
15:26:54 830  	  PROCS_ACCOUNT_V19.ANNOTATE_ACCOUNT(
15:26:54 831  	    in_group_id   => in_group_id,
15:26:54 832  	    in_agent_id   => in_agent_id,
15:26:54 833  	    in_note	  => in_note,
15:26:54 834  	    in_created_by => in_updated_by
15:26:54 835  	  );
15:26:54 836  	  EXCEPTION
15:26:54 837  	    WHEN OTHERS THEN
15:26:54 838  	      EXCEPTION_MESSAGE := SQLERRM;
15:26:54 839  	      RAISE CAN_NOT_CREATE_NOTE;
15:26:54 840  	END;
15:26:54 841  
15:26:54 842  EXCEPTION
15:26:54 843  WHEN BAD_GROUP_ID THEN
15:26:54 844  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 845  	  SPROC_NAME, 'No such group id');
15:26:54 846  WHEN BAD_ACCOUNT_STATUS_ID THEN
15:26:54 847  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
15:26:54 848  	  SPROC_NAME, 'Could not update this account. Status should to be active or frozen');
15:26:54 849  WHEN CAN_NOT_CREATE_NOTE THEN
15:26:54 850  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 851  	  SPROC_NAME, 'Could not annotate account', EXCEPTION_MESSAGE);
15:26:54 852  WHEN OTHERS THEN
15:26:54 853  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:54 854  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:26:54 855  END FREEZE_ACCOUNT;
15:26:54 856  
15:26:54 857  /******************************************************************************/
15:26:54 858  
15:26:54 859  PROCEDURE GET_ACCOUNT_SUBSCR_INVOICES (
15:26:54 860  /*
15:26:54 861  Throws exceptions:
15:26:54 862  APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 863  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
15:26:54 864  */
15:26:54 865  	in_group_id	   IN  NUMBER,
15:26:54 866  	in_subscription_id IN NUMBER,
15:26:54 867  	out_result_set	   OUT SYS_REFCURSOR
15:26:54 868  ) AS
15:26:54 869  SPROC_NAME	   CONSTANT VARCHAR2(27) := 'GET_ACCOUNT_SUBSCR_INVOICES';
15:26:54 870  -- VARIABLES
15:26:54 871  var_account_id	   NUMBER;
15:26:54 872  temp_subscription_id NUMBER;
15:26:54 873  -- EXCEPTIONS
15:26:54 874  BAD_GROUP_ID	  EXCEPTION;
15:26:54 875  BAD_SUBSCRIPTION_ID EXCEPTION;
15:26:54 876  BEGIN
15:26:54 877  	-- Get account id
15:26:54 878  	BEGIN
15:26:54 879  	  SELECT
15:26:54 880  	    ACCOUNT.ID into var_account_id
15:26:54 881  	  FROM
15:26:54 882  	    ACCOUNT
15:26:54 883  	  WHERE
15:26:54 884  	    ACCOUNT.GROUP_ID = in_group_id;
15:26:54 885  	  EXCEPTION
15:26:54 886  	    WHEN NO_DATA_FOUND THEN
15:26:54 887  	      RAISE BAD_GROUP_ID;
15:26:54 888  	END;
15:26:54 889  
15:26:54 890  	-- Check that subscription exists
15:26:54 891  	BEGIN
15:26:54 892  	  IF in_subscription_id IS NOT NULL THEN
15:26:54 893  	    SELECT
15:26:54 894  	      SUBSCRIPTION.ID into temp_subscription_id
15:26:54 895  	    FROM
15:26:54 896  	      SUBSCRIPTION
15:26:54 897  	    WHERE
15:26:54 898  	      SUBSCRIPTION.ID = in_subscription_id;
15:26:54 899  	  END IF;
15:26:54 900  	  EXCEPTION
15:26:54 901  	    WHEN NO_DATA_FOUND THEN
15:26:54 902  	      RAISE BAD_SUBSCRIPTION_ID;
15:26:54 903  	END;
15:26:54 904  
15:26:54 905  	OPEN out_result_set FOR
15:26:54 906  	SELECT DISTINCT
15:26:54 907  	  INVOICE.ID as "INVOICE_ID",
15:26:54 908  	  INVOICE.CREATE_DATE,
15:26:54 909  	  INVOICE.INVOICE_STATUS_ID,
15:26:54 910  	  PROCS_INVOICE_V19.F_CALCULATE_INVOICE_AMOUNT(INVOICE.ID) as "AMOUNT",
15:26:54 911  	  OFFER_CHAIN.ID as "OFFER_CHAIN_ID",
15:26:54 912  	  OFFER_CHAIN.NAME as "OFFER_CHAIN_NAME",
15:26:54 913  	  SUBSCRIPTION.ID as "SUBSCRIPTION_ID",
15:26:54 914  	  NULL as "GC_CODE",
15:26:54 915  	  NULL as "GC_ID"
15:26:54 916  	FROM
15:26:54 917  	  LICENSE
15:26:54 918  	  INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:26:54 919  	  INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
15:26:54 920  	  INNER JOIN OFFER_CHAIN ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
15:26:54 921  	WHERE
15:26:54 922  	  SUBSCRIPTION.ACCOUNT_ID = var_account_id
15:26:54 923  	  AND (SUBSCRIPTION.SCT_ID IS NULL OR SUBSCRIPTION.SCT_ID != GLOBAL_STATUSES_V19.REAL_TIME_CANCEL_REASON)
15:26:54 924  	  AND SUBSCRIPTION.ID = NVL(in_subscription_id, SUBSCRIPTION.ID);
15:26:54 925  
15:26:54 926  EXCEPTION
15:26:54 927  WHEN BAD_GROUP_ID THEN
15:26:54 928  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 929  	  SPROC_NAME, 'No such account');
15:26:54 930  WHEN BAD_SUBSCRIPTION_ID THEN
15:26:54 931  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 932  	  SPROC_NAME, 'No such subscription');
15:26:54 933  WHEN OTHERS THEN
15:26:54 934  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:54 935  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:26:54 936  END GET_ACCOUNT_SUBSCR_INVOICES;
15:26:54 937  
15:26:54 938  /******************************************************************************/
15:26:54 939  
15:26:54 940  PROCEDURE GET_ACCOUNT_GC_INVOICES (
15:26:54 941  /*
15:26:54 942  Throws exceptions:
15:26:54 943  APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 944  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
15:26:54 945  */
15:26:54 946  	in_group_id    IN  NUMBER,
15:26:54 947  	out_result_set OUT SYS_REFCURSOR
15:26:54 948  ) AS
15:26:54 949  SPROC_NAME     CONSTANT VARCHAR2(23) := 'GET_ACCOUNT_GC_INVOICES';
15:26:54 950  -- VARIABLES
15:26:54 951  var_account_id NUMBER;
15:26:54 952  -- EXCEPTIONS
15:26:54 953  BAD_GROUP_ID EXCEPTION;
15:26:54 954  BEGIN
15:26:54 955  	-- Get account id
15:26:54 956  	BEGIN
15:26:54 957  	  SELECT
15:26:54 958  	    ACCOUNT.ID into var_account_id
15:26:54 959  	  FROM
15:26:54 960  	    ACCOUNT
15:26:54 961  	  WHERE
15:26:54 962  	    ACCOUNT.GROUP_ID = in_group_id;
15:26:54 963  	  EXCEPTION
15:26:54 964  	    WHEN NO_DATA_FOUND THEN
15:26:54 965  	      RAISE BAD_GROUP_ID;
15:26:54 966  	END;
15:26:54 967  
15:26:54 968  	-- Get invoices
15:26:54 969  	OPEN out_result_set FOR
15:26:54 970  	SELECT DISTINCT
15:26:54 971  	  INVOICE.ID as "INVOICE_ID",
15:26:54 972  	  INVOICE.CREATE_DATE,
15:26:54 973  	  INVOICE.INVOICE_STATUS_ID,
15:26:54 974  	  PROCS_INVOICE_V19.F_CALCULATE_INVOICE_AMOUNT(INVOICE.ID) as "AMOUNT",
15:26:54 975  	  OFFER_CHAIN.ID as "OFFER_CHAIN_ID",
15:26:54 976  	  OFFER_CHAIN.NAME as "OFFER_CHAIN_NAME",
15:26:54 977  	  NULL as "SUBSCRIPTION_ID",
15:26:54 978  	  GIFT_CERTIFICATE.CODE as "GC_CODE",
15:26:54 979  	  GIFT_CERTIFICATE.ID as "GC_ID"
15:26:54 980  	FROM
15:26:54 981  	  GIFT_CERTIFICATE
15:26:54 982  	  INNER JOIN INVOICE ON GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = INVOICE.ID
