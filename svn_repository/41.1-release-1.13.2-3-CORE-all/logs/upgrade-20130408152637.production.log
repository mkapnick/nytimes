15:26:54 1928  
15:26:54 1929  PROCEDURE GET_PAYMENT_INFO_BY_INVOICE_ID (
15:26:54 1930  	 in_invoice_id		     IN NUMBER,
15:26:54 1931  	 out_order_id		     OUT VARCHAR2,
15:26:54 1932  	 out_external_transaction_id OUT VARCHAR2
15:26:54 1933  ) AS
15:26:54 1934  SPROC_NAME CONSTANT VARCHAR2(30) := 'GET_PEYMENT_INFO_BY_INVOICE_ID';
15:26:54 1935  -- VARIABLES
15:26:54 1936  temp_invoice_id number;
15:26:54 1937  cnt_matched_instr number := 0;
15:26:54 1938  -- EXCEPTIONS
15:26:54 1939  BAD_INVOICE_ID EXCEPTION;
15:26:54 1940  BEGIN
15:26:54 1941  
15:26:54 1942  	 BEGIN
15:26:54 1943  	   SELECT
15:26:54 1944  	     i.id into temp_invoice_id
15:26:54 1945  	   from
15:26:54 1946  	     invoice i
15:26:54 1947  	   where
15:26:54 1948  	     i.id = in_invoice_id;
15:26:54 1949  	   EXCEPTION
15:26:54 1950  	     WHEN NO_DATA_FOUND THEN
15:26:54 1951  	       RAISE BAD_INVOICE_ID;
15:26:54 1952  	 END;
15:26:54 1953  
15:26:54 1954  	 select
15:26:54 1955  	   count(1) into cnt_matched_instr
15:26:54 1956  	 from
15:26:54 1957  	   charge ch
15:26:54 1958  	 inner join
15:26:54 1959  	   subscription s
15:26:54 1960  	 on
15:26:54 1961  	   s.instrument_id = ch.instrument_id
15:26:54 1962  	 where
15:26:54 1963  	   ch.invoice_id = in_invoice_id;
15:26:54 1964  
15:26:54 1965  	 if cnt_matched_instr = 0 then
15:26:54 1966  	   out_external_transaction_id := null;
15:26:54 1967  	   out_order_id := null;
15:26:54 1968  	   return;
15:26:54 1969  	 end if;
15:26:54 1970  
15:26:54 1971  	 SELECT
15:26:54 1972  	   t.order_id,
15:26:54 1973  	   ta.external_transaction_id
15:26:54 1974  	   into
15:26:54 1975  	   out_order_id,
15:26:54 1976  	   out_external_transaction_id
15:26:54 1977  	 from
15:26:54 1978  	   charge ch
15:26:54 1979  	   inner join transaction t on ch.transaction_id = t.id
15:26:54 1980  	   inner join transaction_attempt ta on ta.transaction_id = t.id
15:26:54 1981  	 where
15:26:54 1982  	   ch.invoice_id = in_invoice_id
15:26:54 1983  	   and ta.transaction_attempt_status_id = GLOBAL_STATUSES_V19.TRANS_ATTEMPT_SUCCESS
15:26:54 1984  	   and ch.charge_amount > 0; -- We are not creating charges for the 0-amount invoices
15:26:54 1985  
15:26:54 1986  EXCEPTION
15:26:54 1987  WHEN BAD_INVOICE_ID THEN
15:26:54 1988  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 1989  	   SPROC_NAME, 'No such invoice');
15:26:54 1990  WHEN NO_DATA_FOUND THEN
15:26:54 1991  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 1992  	   SPROC_NAME, 'No payment data found');
15:26:54 1993  WHEN OTHERS THEN
