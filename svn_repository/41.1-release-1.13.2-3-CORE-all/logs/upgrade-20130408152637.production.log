15:26:53 1165  /******************************************************************************/
15:26:53 1166  
15:26:53 1167  PROCEDURE REFUND_SUBSCRIPTION (
15:26:53 1168  /*
15:26:53 1169  Throws exceptions:
15:26:53 1170  APP_EXCEPTION_CODES_V19.NOT_FOUND
15:26:53 1171  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
15:26:53 1172  */
15:26:53 1173  	 in_subscription_id IN NUMBER,
15:26:53 1174  	 in_invoice_id	    IN NUMBER,
15:26:53 1175  	 in_refund_amount   IN NUMBER,
15:26:53 1176  	 in_note	    IN VARCHAR2,
15:26:53 1177  	 in_created_by	    IN VARCHAR2,
15:26:53 1178  	 out_charge_id	    OUT NUMBER
15:26:53 1179  ) AS
15:26:53 1180  -- VARIABLES
15:26:53 1181  SPROC_NAME	      CONSTANT VARCHAR2(19) := 'REFUND_SUBSCRIPTION';
15:26:53 1182  var_invoice_status_id  NUMBER;
15:26:53 1183  var_account_id	      NUMBER;
15:26:53 1184  var_account_status_id  NUMBER;
15:26:53 1185  var_new_transaction_id NUMBER;
15:26:53 1186  var_instrument_type_id NUMBER;
15:26:53 1187  var_instrument_id      NUMBER;
15:26:53 1188  var_new_charge_id      NUMBER;
15:26:53 1189  var_invoice_amount     NUMBER(10,2);
15:26:53 1190  var_refunds_before     NUMBER(10,2);
15:26:53 1191  var_charges_amount     NUMBER(10,2);
15:26:53 1192  -- EXCEPTIONS
15:26:53 1193  BAD_SUBSCRIPTION_ID	     EXCEPTION;
15:26:53 1194  ACCOUNT_IS_FROZEN	     EXCEPTION;
15:26:53 1195  BAD_INVOICE_ID		     EXCEPTION;
15:26:53 1196  CAN_NOT_CREATE_TRANSACTION    EXCEPTION;
15:26:53 1197  CAN_NOT_CREATE_CHARGE	     EXCEPTION;
15:26:53 1198  CAN_NOT_CALC_INVOICE_AMOUNT   EXCEPTION;
15:26:53 1199  REFUND_IS_GREATER_THAN_ANOUNT EXCEPTION;
15:26:53 1200  CAN_NOT_ANNOTATE_SUBSCRIPTION EXCEPTION;
15:26:53 1201  TOT_REF_IS_GREATER_THAN_ANOUNT EXCEPTION;
15:26:53 1202  INVOICE_IS_NOT_CLOSED	     EXCEPTION;
15:26:53 1203  TOT_REF_IS_GRATER_THAN_CHARGES EXCEPTION;
15:26:53 1204  EXCEPTION_MESSAGE	      VARCHAR2(1024);
15:26:53 1205  BEGIN
15:26:53 1206  
15:26:53 1207  	 BEGIN
15:26:53 1208  	   SELECT
15:26:53 1209  	     SUBSCRIPTION.INSTRUMENT_ID,
15:26:53 1210  	     SUBSCRIPTION.INSTRUMENT_TYPE_ID,
15:26:53 1211  	     SUBSCRIPTION.ACCOUNT_ID
15:26:53 1212  	     into
15:26:53 1213  	     var_instrument_id,
15:26:53 1214  	     var_instrument_type_id,
15:26:53 1215  	     var_account_id
15:26:53 1216  	   FROM
15:26:53 1217  	     SUBSCRIPTION
15:26:53 1218  	   WHERE
15:26:53 1219  	     SUBSCRIPTION.ID = in_subscription_id;
15:26:53 1220  	   EXCEPTION
15:26:53 1221  	     WHEN NO_DATA_FOUND THEN
15:26:53 1222  	       RAISE BAD_SUBSCRIPTION_ID;
15:26:53 1223  	 END;
15:26:53 1224  
15:26:53 1225  	 -- Check account status. It should not to be frozen
15:26:53 1226  	 SELECT
15:26:53 1227  	   ACCOUNT.ACCOUNT_STATUS_ID into var_account_status_id
15:26:53 1228  	 FROM
15:26:53 1229  	   ACCOUNT
15:26:53 1230  	 WHERE
15:26:53 1231  	   ACCOUNT.ID = var_account_id;
15:26:53 1232  
15:26:53 1233  	 IF var_account_status_id = GLOBAL_STATUSES_V19.ACCOUNT_FROZEN THEN
15:26:53 1234  	   RAISE ACCOUNT_IS_FROZEN;
15:26:53 1235  	 END IF;
15:26:53 1236  
15:26:53 1237  	 BEGIN
15:26:53 1238  	   SELECT
15:26:53 1239  	     INVOICE.INVOICE_STATUS_ID into var_invoice_status_id
15:26:53 1240  	   FROM
15:26:53 1241  	     INVOICE
15:26:53 1242  	   WHERE
15:26:53 1243  	     INVOICE.ID = in_invoice_id;
15:26:53 1244  	   EXCEPTION
15:26:53 1245  	     WHEN NO_DATA_FOUND THEN
15:26:53 1246  	       RAISE BAD_INVOICE_ID;
15:26:53 1247  	 END;
15:26:53 1248  
15:26:53 1249  	 IF var_invoice_status_id != GLOBAL_STATUSES_V19.INVOICE_CLOSED THEN
15:26:53 1250  	   RAISE INVOICE_IS_NOT_CLOSED;
15:26:53 1251  	 END IF;
15:26:53 1252  
15:26:53 1253  	 BEGIN
15:26:53 1254  	   PROCS_INVOICE_V19.CALCULATE_INVOICE_AMOUNT (
15:26:53 1255  	     in_invoice_id => in_invoice_id,
15:26:53 1256  	     out_amount    => var_invoice_amount
15:26:53 1257  	   );
15:26:53 1258  	   EXCEPTION
15:26:53 1259  	     WHEN OTHERS THEN
15:26:53 1260  	       EXCEPTION_MESSAGE := SQLERRM;
15:26:53 1261  	       RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
15:26:53 1262  	 END;
15:26:53 1263  
15:26:53 1264  	 IF ( in_refund_amount > var_invoice_amount ) THEN
15:26:53 1265  	   RAISE REFUND_IS_GREATER_THAN_ANOUNT;
15:26:53 1266  	 END IF;
15:26:53 1267  
15:26:53 1268  	 SELECT /*+ STAR_TRANSFORMATION */
15:26:53 1269  	   SUM(CHARGE.CHARGE_AMOUNT) into var_refunds_before
15:26:53 1270  	 FROM
15:26:53 1271  	   CHARGE
15:26:53 1272  	 WHERE
15:26:53 1273  	   CHARGE.INVOICE_ID = in_invoice_id
15:26:53 1274  	   AND (
15:26:53 1275  	     CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V19.CHARGE_OPENED
15:26:53 1276  	     OR CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V19.CHARGE_PROCESSED
15:26:53 1277  	   )
15:26:53 1278  	   AND CHARGE.CHARGE_AMOUNT < 0;
15:26:53 1279  
15:26:53 1280  	 -- Refunds are negative
15:26:53 1281  	 var_refunds_before := -var_refunds_before;
15:26:53 1282  
15:26:53 1283  	 var_charges_amount := 0;
15:26:53 1284  
15:26:53 1285  	 FOR f_processed_charges IN (
15:26:53 1286  	   SELECT
15:26:53 1287  	     CHARGE.CHARGE_AMOUNT
15:26:53 1288  	   FROM
15:26:53 1289  	     CHARGE
15:26:53 1290  	   WHERE
15:26:53 1291  	     CHARGE.INVOICE_ID = in_invoice_id
15:26:53 1292  	     AND CHARGE.CHARGE_STATUS_ID IN (SELECT GLOBAL_STATUSES_V19.CHARGE_PROCESSED FROM DUAL)
15:26:53 1293  	 )
15:26:53 1294  	 LOOP
15:26:53 1295  	   IF f_processed_charges.CHARGE_AMOUNT > 0 THEN
15:26:53 1296  	     var_charges_amount := var_charges_amount + f_processed_charges.CHARGE_AMOUNT;
15:26:53 1297  	   END IF;
15:26:53 1298  	 END LOOP;
15:26:53 1299  
15:26:53 1300  	 IF (in_refund_amount + var_refunds_before > var_invoice_amount) THEN
15:26:53 1301  	   RAISE TOT_REF_IS_GREATER_THAN_ANOUNT;
15:26:53 1302  	 END IF;
15:26:53 1303  
15:26:53 1304  	 IF (in_refund_amount + var_refunds_before > var_charges_amount) THEN
15:26:53 1305  	   RAISE TOT_REF_IS_GRATER_THAN_CHARGES;
15:26:53 1306  	 END IF;
15:26:53 1307  
15:26:53 1308  	 BEGIN
15:26:53 1309  	   PROCS_TRANSACTION_V19.CREATE_TRANSACTION(
15:26:53 1310  	     in_transaction_id	       => NULL,
15:26:53 1311  	     in_status_id	       => GLOBAL_STATUSES_V19.TRANSACTION_PREPARE,
15:26:53 1312  	     in_amount		       => -in_refund_amount,
15:26:53 1313  	     in_created_by	       => in_created_by,
15:26:53 1314  	     in_order_id	       => NULL,
15:26:53 1315  	     in_is_refund	       => GLOBAL_CONSTANTS_V19.TRUE,
15:26:53 1316  	     in_transaction_type_code  => 'REFUND',
15:26:53 1317  	     out_transaction_id        => var_new_transaction_id
15:26:53 1318  	   );
15:26:53 1319  	   EXCEPTION
15:26:53 1320  	     WHEN OTHERS THEN
15:26:53 1321  	       EXCEPTION_MESSAGE := SQLERRM;
15:26:53 1322  	       RAISE CAN_NOT_CREATE_TRANSACTION;
15:26:53 1323  	 END;
15:26:53 1324  
15:26:53 1325  	 BEGIN
15:26:53 1326  	   PROCS_CHARGE_V19.CREATE_CHARGE(
15:26:53 1327  	     in_invoice_id	   => in_invoice_id,
15:26:53 1328  	     in_transaction_id	   => var_new_transaction_id,
15:26:53 1329  	     in_instrument_type_id => var_instrument_type_id,
15:26:53 1330  	     in_instrument_id	   => var_instrument_id,
15:26:53 1331  	     in_charge_amount	   => -in_refund_amount,
15:26:53 1332  	     in_created_by	   => in_created_by,
15:26:53 1333  	     in_charge_status_id   => GLOBAL_STATUSES_V19.CHARGE_OPENED,
15:26:53 1334  	     out_charge_id	   => var_new_charge_id
15:26:53 1335  	   );
15:26:53 1336  	   EXCEPTION
15:26:53 1337  	     WHEN OTHERS THEN
15:26:53 1338  	       EXCEPTION_MESSAGE := SQLERRM;
15:26:53 1339  	       RAISE CAN_NOT_CREATE_CHARGE;
15:26:53 1340  	 END;
15:26:53 1341  
15:26:53 1342  	 IF in_note IS NOT NULL THEN
15:26:53 1343  	   BEGIN
15:26:53 1344  	     PROCS_SUBSCRIPTION_V19.ANNOTATE_SUBSCRIPTION(
15:26:53 1345  	       in_subscription_id => in_subscription_id,
15:26:53 1346  	       in_agent_id	  => 0, -- AGENT_ID??
15:26:53 1347  	       in_note		  => in_note,
15:26:53 1348  	       in_created_by	  => in_created_by
15:26:53 1349  	     );
15:26:53 1350  	     EXCEPTION
15:26:53 1351  	       WHEN OTHERS THEN
15:26:53 1352  		 EXCEPTION_MESSAGE := SQLERRM;
15:26:53 1353  		 RAISE CAN_NOT_ANNOTATE_SUBSCRIPTION;
15:26:53 1354  	   END;
15:26:53 1355  	 END IF;
15:26:53 1356  
15:26:53 1357  	 out_charge_id := var_new_charge_id;
15:26:53 1358  
15:26:53 1359  EXCEPTION
15:26:53 1360  WHEN INVOICE_IS_NOT_CLOSED THEN
15:26:53 1361  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
15:26:53 1362  	   SPROC_NAME, 'Invoice is not closed');
15:26:53 1363  WHEN BAD_SUBSCRIPTION_ID THEN
15:26:53 1364  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:53 1365  	   SPROC_NAME, 'No such subscription');
15:26:53 1366  WHEN ACCOUNT_IS_FROZEN THEN
15:26:53 1367  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
15:26:53 1368  	   SPROC_NAME, 'Could not refund subscription for frozen account');
15:26:53 1369  WHEN BAD_INVOICE_ID THEN
15:26:53 1370  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:53 1371  	   SPROC_NAME, 'No such invoice');
15:26:53 1372  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
15:26:53 1373  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:53 1374  	   SPROC_NAME, 'Could not calculate invoice amount', EXCEPTION_MESSAGE);
15:26:53 1375  WHEN REFUND_IS_GREATER_THAN_ANOUNT THEN
15:26:53 1376  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
15:26:53 1377  	   SPROC_NAME, 'Refund is greater than amount');
15:26:53 1378  WHEN TOT_REF_IS_GREATER_THAN_ANOUNT THEN
15:26:53 1379  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
15:26:53 1380  	   SPROC_NAME, 'There were refunds before and sum of all refunds and new refund more than invoice amount');
15:26:53 1381  WHEN TOT_REF_IS_GRATER_THAN_CHARGES THEN
15:26:53 1382  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
15:26:53 1383  	   SPROC_NAME, 'Total refund amount is greater than sum of processed charges');
15:26:53 1384  WHEN CAN_NOT_CREATE_TRANSACTION THEN
15:26:53 1385  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:53 1386  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
15:26:53 1387  WHEN CAN_NOT_CREATE_CHARGE THEN
15:26:53 1388  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:53 1389  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
15:26:53 1390  WHEN CAN_NOT_ANNOTATE_SUBSCRIPTION THEN
15:26:53 1391  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:53 1392  	   SPROC_NAME, 'Could not annotate subscription', EXCEPTION_MESSAGE);
15:26:53 1393  WHEN OTHERS THEN
15:26:53 1394  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:53 1395  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:26:53 1396  END REFUND_SUBSCRIPTION;
