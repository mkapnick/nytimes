15:26:53 935  /******************************************************************************/
15:26:53 936  
15:26:53 937  FUNCTION GET_RENEWAL_DATE (
15:26:53 938  /*
15:26:53 939  Throws exceptions:
15:26:53 940  APP_EXCEPTION_CODES_V19.NOT_FOUND
15:26:53 941  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
15:26:53 942  */
15:26:53 943  	in_subscription_id in NUMBER
15:26:53 944  ) RETURN DATE AS
15:26:53 945  -- VARIABLES
15:26:53 946  SPROC_NAME	      CONSTANT VARCHAR2(16) := 'GET_RENEWAL_DATE';
15:26:53 947  var_subscription_status NUMBER;
15:26:53 948  var_licenses_count      NUMBER;
15:26:53 949  var_license_end_date    DATE;
15:26:53 950  var_last_offer_id       NUMBER;
15:26:53 951  var_offer_chain_id      NUMBER;
15:26:53 952  var_last_license_id     NUMBER;
15:26:53 953  var_current_offer_index NUMBER;
15:26:53 954  var_current_offer_recurr_num NUMBER;
15:26:53 955  var_offer_recurr_num    NUMBER;
15:26:53 956  -- EXCEPTIONS
15:26:53 957  BAD_SUBSCRIPTION_ID EXCEPTION;
15:26:53 958  NO_LICENSES_FOUND EXCEPTION;
15:26:53 959  BEGIN
15:26:53 960  
15:26:53 961  	-- Get subscription id and offer chain id
15:26:53 962  	BEGIN
15:26:53 963  	  SELECT
15:26:53 964  	    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID,
15:26:53 965  	    SUBSCRIPTION.OFFER_CHAIN_ID
15:26:53 966  	    into
15:26:53 967  	    var_subscription_status,
15:26:53 968  	    var_offer_chain_id
15:26:53 969  	  FROM
15:26:53 970  	    SUBSCRIPTION
15:26:53 971  	  WHERE
15:26:53 972  	    SUBSCRIPTION.ID = in_subscription_id;
15:26:53 973  	  EXCEPTION
15:26:53 974  	    WHEN NO_DATA_FOUND THEN
15:26:53 975  	      RAISE BAD_SUBSCRIPTION_ID;
15:26:53 976  	END;
15:26:53 977  
15:26:53 978  	IF var_subscription_status != GLOBAL_STATUSES_V19.SUBSCRIPTION_ACTIVE THEN
15:26:53 979  	  -- TODO: Is suspended subscription has renewal date? (For the phase II)
15:26:53 980  	  -- AND var_subscription_status != GLOBAL_STATUSES_V19.SUBSCRIPTION_SUSPENDED THEN
15:26:53 981  	  RETURN NULL;
15:26:53 982  	END IF;
15:26:53 983  
15:26:53 984  	BEGIN
15:26:53 985  	  SELECT
15:26:53 986  	    LICENSE_ID,
15:26:53 987  	    END_DATE,
15:26:53 988  	    OFFER_ID,
15:26:53 989  	    CURRENT_OFFER_INDEX,
15:26:53 990  	    CURRENT_OFFER_RECURR_NUM
15:26:53 991  	    into
15:26:53 992  	    var_last_license_id,
15:26:53 993  	    var_license_end_date,
15:26:53 994  	    var_last_offer_id,
15:26:53 995  	    var_current_offer_index,
15:26:53 996  	    var_current_offer_recurr_num
15:26:53 997  	  FROM
15:26:53 998  	    (
15:26:53 999  	      SELECT
15:26:53 1000  		 LICENSE.ID as "LICENSE_ID",
15:26:53 1001  		 LICENSE.END_DATE,
15:26:53 1002  		 LICENSE.OFFER_ID,
15:26:53 1003  		 LICENSE.CURRENT_OFFER_INDEX,
15:26:53 1004  		 LICENSE.CURRENT_OFFER_RECURR_NUM
15:26:53 1005  	       FROM
15:26:53 1006  		 LICENSE
15:26:53 1007  	       WHERE
15:26:53 1008  		 LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V19.LICENSE_ACTIVE
15:26:53 1009  		 AND LICENSE.SUBSCRIPTION_ID = in_subscription_id
15:26:53 1010  	       ORDER BY END_DATE DESC
15:26:53 1011  	     )
15:26:53 1012  	     INNER JOIN OFFER ON OFFER_ID = OFFER.ID
15:26:53 1013  	   WHERE
15:26:53 1014  	     ROWNUM <= 1;
15:26:53 1015  
15:26:53 1016  	   EXCEPTION
15:26:53 1017  	     WHEN NO_DATA_FOUND THEN
15:26:53 1018  	       -- RAISE NO_LICENSES_FOUND;
15:26:53 1019  	       RETURN NULL;
15:26:53 1020  	 END;
15:26:53 1021  
15:26:53 1022  	 SELECT
15:26:53 1023  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES into var_offer_recurr_num
15:26:53 1024  	 FROM
15:26:53 1025  	   OFFER_OFFER_CHAIN
15:26:53 1026  	 WHERE
15:26:53 1027  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
15:26:53 1028  	   AND OFFER_OFFER_CHAIN.OFFER_ID = var_last_offer_id;
15:26:53 1029  
15:26:53 1030  	 IF PROCS_OFFER_CHAIN_V19.GET_NEXT_OFFER_INDEX(var_offer_chain_id, var_current_offer_index) IS NULL
15:26:53 1031  	   AND var_offer_recurr_num = var_current_offer_recurr_num THEN
15:26:53 1032  	   -- There is no next offer for this subscription
15:26:53 1033  	   RETURN NULL;
15:26:53 1034  	 END IF;
15:26:53 1035  
15:26:53 1036  	 RETURN var_license_end_date;
15:26:53 1037  
15:26:53 1038  EXCEPTION
15:26:53 1039  WHEN BAD_SUBSCRIPTION_ID THEN
15:26:53 1040  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:53 1041  	   SPROC_NAME, 'No such subscription');
15:26:53 1042  WHEN NO_LICENSES_FOUND THEN
15:26:53 1043  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:53 1044  	   SPROC_NAME, 'No licenses found');
15:26:53 1045  WHEN OTHERS THEN
15:26:53 1046  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:53 1047  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:26:53 1048  END GET_RENEWAL_DATE;
