15:26:54 1810  ) AS
15:26:54 1811  -- VARIABLES
15:26:54 1812  SPROC_NAME		CONSTANT VARCHAR2(28) := 'GET_GIFT_CERTIFICATE_BY_CODE';
15:26:54 1813  temp_gift_certificate_id NUMBER;
15:26:54 1814  -- EXCEPTIONS
15:26:54 1815  BAD_GIFT_CERTIFICATE_CODE EXCEPTION;
15:26:54 1816  BEGIN
15:26:54 1817  
15:26:54 1818  	 BEGIN
15:26:54 1819  	   SELECT
15:26:54 1820  	     GIFT_CERTIFICATE.ID into temp_gift_certificate_id
15:26:54 1821  	   FROM
15:26:54 1822  	     GIFT_CERTIFICATE
15:26:54 1823  	   WHERE
15:26:54 1824  	     GIFT_CERTIFICATE.CODE = in_code;
