15:26:53 673  	  SPROC_NAME, 'No such offer chain');
15:26:53 674  WHEN CAN_NOT_GET_FIRST_OFFER THEN
15:26:53 675  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:53 676  	  SPROC_NAME, 'Could not find first offer', EXCEPTION_MESSAGE);
15:26:53 677  WHEN OTHERS THEN
15:26:53 678  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:53 679  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:26:53 680  END CALCULATE_OFFER_CHAIN_AMOUNT;
