15:26:54 391  	  CHARGE.CHARGE_STATUS_ID
15:26:54 392  	FROM
15:26:54 393  	  CHARGE
15:26:54 394  	WHERE
15:26:54 395  	  CHARGE.INVOICE_ID = in_invoice_id
15:26:54 396  	  AND CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V19.INSTRUMENT_PAYPAL;
15:26:54 397  
15:26:54 398  	-- get all credir cards for given invoice
15:26:54 399  	OPEN out_cc_charges_set FOR
15:26:54 400  	SELECT
15:26:54 401  	  CHARGE.ID as "CHARGE_ID",
15:26:54 402  	  CHARGE.CHARGE_AMOUNT,
15:26:54 403  	  CHARGE.INSTRUMENT_ID,
15:26:54 404  	  CHARGE.CHARGE_STATUS_ID
15:26:54 405  	FROM
15:26:54 406  	  CHARGE
15:26:54 407  	WHERE
15:26:54 408  	  CHARGE.INVOICE_ID = in_invoice_id
15:26:54 409  	  AND CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V19.INSTRUMENT_CREDIT_CARD;
15:26:54 410  
15:26:54 411  	OPEN out_gc_charges_set FOR
15:26:54 412  	SELECT
15:26:54 413  	  CHARGE.ID as "CHARGE_ID",
15:26:54 414  	  CHARGE.CHARGE_AMOUNT,
15:26:54 415  	  CHARGE.INSTRUMENT_ID,
15:26:54 416  	  CHARGE.CHARGE_STATUS_ID
15:26:54 417  	FROM
15:26:54 418  	  CHARGE
15:26:54 419  	WHERE
15:26:54 420  	  CHARGE.INVOICE_ID = in_invoice_id
15:26:54 421  	  AND CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V19.INSTRUMENT_GIFT_CERTIFICATE;
15:26:54 422  
15:26:54 423  EXCEPTION
