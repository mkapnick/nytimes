15:26:54 580  
15:26:54 581  PROCEDURE UPDATE_CREDIT_CARD (
15:26:54 582  /*
15:26:54 583  Throws exceptions:
15:26:54 584  APP_EXCEPTION_CODES_V19.NOT_FOUND
15:26:54 585  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
15:26:54 586  */
15:26:54 587  	in_credit_card_id	  IN NUMBER,
15:26:54 588  	in_updated_by		  IN VARCHAR2,
15:26:54 589  	in_instrument_name	  IN VARCHAR2,
15:26:54 590  	in_is_default		  IN NUMBER
15:26:54 591  ) AS
15:26:54 592  SPROC_NAME CONSTANT VARCHAR2(18) := 'UPDATE_CREDIT_CARD';
15:26:54 593  -- VARIABLES
15:26:54 594  var_account_id NUMBER;
15:26:54 595  var_group_id   NUMBER;
15:26:54 596  temp_cc_rownum NUMBER;
15:26:54 597  current_def_instrument_type_id NUMBER;
15:26:54 598  current_def_instrument_id      NUMBER;
15:26:54 599  -- EXCEPTION
15:26:54 600  CAN_NOT_SET_DEF_FINANCIAL  EXCEPTION;
15:26:54 601  BAD_IS_DEFAULT_VALUE	 EXCEPTION;
15:26:54 602  CAN_NOT_GET_DEF_FINANCIAL  EXCEPTION;
15:26:54 603  CAN_NOT_DEL_DEF_FINANCIAL  EXCEPTION;
15:26:54 604  EXCEPTION_MESSAGE 	 VARCHAR2(1024);
15:26:54 605  BEGIN
15:26:54 606  
15:26:54 607  	-- Get account id
15:26:54 608  	SELECT
15:26:54 609  	  CREDIT_CARD.ACCOUNT_ID
15:26:54 610  	  into
15:26:54 611  	  var_account_id
15:26:54 612  	FROM
15:26:54 613  	  CREDIT_CARD
15:26:54 614  	WHERE
15:26:54 615  	  CREDIT_CARD.ID = in_credit_card_id;
15:26:54 616  
15:26:54 617  	-- Get group id
15:26:54 618  	SELECT
15:26:54 619  	  ACCOUNT.GROUP_ID into var_group_id
15:26:54 620  	FROM
15:26:54 621  	  ACCOUNT
15:26:54 622  	WHERE
15:26:54 623  	  ACCOUNT.ID = var_account_id;
15:26:54 624  
15:26:54 625  	-- Check that passed data is correct
15:26:54 626  	IF in_is_default != GLOBAL_CONSTANTS_V19.TRUE
15:26:54 627  	  AND in_is_default != GLOBAL_CONSTANTS_V19.FALSE
15:26:54 628  	  AND in_is_default IS NOT NULL THEN
15:26:54 629  	  RAISE BAD_IS_DEFAULT_VALUE;
15:26:54 630  	END IF;
15:26:54 631  
15:26:54 632  	-- Update credit card
15:26:54 633  	IF in_instrument_name IS NOT NULL THEN
15:26:54 634  	  PROCS_FIN_INSTRUMENTS_CRU_V19.UPDATE_CREDIT_CARD(
15:26:54 635  	    in_credit_card_id  => in_credit_card_id,
15:26:54 636  	    in_updated_by      => in_updated_by,
15:26:54 637  	    in_instrument_name => in_instrument_name
15:26:54 638  	  );
15:26:54 639  	END IF;
15:26:54 640  
15:26:54 641  	-- Set default financial instrument
15:26:54 642  	IF in_is_default = GLOBAL_CONSTANTS_V19.TRUE THEN
15:26:54 643  	  BEGIN
15:26:54 644  	    PROCS_FIN_INSTRUMENTS_V19.SET_DEF_FINANCIAL_INSTRUMENT(
15:26:54 645  	      in_group_id	    => var_group_id,
15:26:54 646  	      in_instrument_type_id => GLOBAL_ENUMS_V19.INSTRUMENT_CREDIT_CARD,
15:26:54 647  	      in_instrument_id	    => in_credit_card_id,
15:26:54 648  	      in_updated_by	    => in_updated_by
15:26:54 649  	    );
15:26:54 650  	    EXCEPTION
15:26:54 651  	      WHEN OTHERS THEN
15:26:54 652  		EXCEPTION_MESSAGE := SQLERRM;
15:26:54 653  		RAISE CAN_NOT_SET_DEF_FINANCIAL;
15:26:54 654  	  END;
15:26:54 655  	END IF;
15:26:54 656  
15:26:54 657  	-- Set default financial instrument
15:26:54 658  	IF in_is_default = GLOBAL_CONSTANTS_V19.FALSE THEN
15:26:54 659  	  BEGIN
15:26:54 660  	    GET_DEF_FINANCIAL_INSTRUMENT(
15:26:54 661  	      in_group_id	     => var_group_id,
15:26:54 662  	      out_instrument_type_id => current_def_instrument_type_id,
15:26:54 663  	      out_instrument_id      => current_def_instrument_id
15:26:54 664  	    );
15:26:54 665  	  EXCEPTION
15:26:54 666  	    WHEN OTHERS THEN
15:26:54 667  	      EXCEPTION_MESSAGE := SQLERRM;
15:26:54 668  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
15:26:54 669  	  END;
15:26:54 670  	  IF current_def_instrument_type_id = GLOBAL_ENUMS_V19.INSTRUMENT_CREDIT_CARD
15:26:54 671  	    AND current_def_instrument_id = in_credit_card_id THEN
15:26:54 672  	    BEGIN
15:26:54 673  	      DEL_DEF_FINANCIAL_INSTRUMENT(
15:26:54 674  		in_group_id => var_group_id
15:26:54 675  	      );
15:26:54 676  	      EXCEPTION
15:26:54 677  		WHEN OTHERS THEN
15:26:54 678  		  EXCEPTION_MESSAGE := SQLERRM;
15:26:54 679  		  RAISE CAN_NOT_DEL_DEF_FINANCIAL;
15:26:54 680  	    END;
15:26:54 681  	  END IF;
15:26:54 682  	END IF;
15:26:54 683  
15:26:54 684  EXCEPTION
15:26:54 685  WHEN NO_DATA_FOUND THEN
15:26:54 686  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 687  	  SPROC_NAME, 'No such credit card');
15:26:54 688  WHEN CAN_NOT_SET_DEF_FINANCIAL THEN
15:26:54 689  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 690  	  SPROC_NAME, 'Could not set default financial instrument for account', EXCEPTION_MESSAGE);
15:26:54 691  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
15:26:54 692  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 693  	  SPROC_NAME, 'Could not get default financial instrument for account', EXCEPTION_MESSAGE);
15:26:54 694  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
15:26:54 695  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 696  	  SPROC_NAME, 'Could not delete information about default financial instrument', EXCEPTION_MESSAGE);
15:26:54 697  WHEN OTHERS THEN
15:26:54 698  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:54 699  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:26:54 700  END UPDATE_CREDIT_CARD;
