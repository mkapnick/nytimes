15:26:54 1391  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 1392  	   SPROC_NAME, 'No such invoice');
15:26:54 1393  WHEN OTHERS THEN
15:26:54 1394  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:54 1395  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:26:54 1396  END CLOSE_INVOICE_AS_NOT_COLLECTED;
