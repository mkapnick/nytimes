15:26:53 695  /******************************************************************************/
15:26:53 696  
15:26:53 697  PROCEDURE GET_ENDED_GC_LICENSES (
15:26:53 698  	out_result_set		OUT SYS_REFCURSOR,
15:26:53 699  	in_hours_number 	IN NUMBER DEFAULT 14*24,
15:26:53 700  	in_num_rows		IN NUMBER DEFAULT 10000,
15:26:53 701  	in_process_name IN PROCESS_RETRY_THROTTLE.PROCESS_NAME%TYPE
15:26:53 702  ) AS
15:26:53 703  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_ENDED_GC_INVOICES';
15:26:53 704  var_days		NUMBER;
15:26:53 705  var_hours 	NUMBER;
15:26:53 706  var_time_interval INTERVAL DAY (3) TO SECOND;
15:26:53 707  BEGIN
15:26:53 708  	var_hours := mod(in_hours_number,24);
15:26:53 709  	var_days := (in_hours_number - var_hours) / 24;
15:26:53 710  	var_time_interval := var_days||' '||var_hours||':0:0';
15:26:53 711  	OPEN out_result_set FOR
15:26:53 712  SELECT * FROM
15:26:53 713  (
15:26:53 714  	SELECT
15:26:53 715  	  l.ID,
15:26:53 716  	  l.CREATE_DATE,
15:26:53 717  	  l.CREATED_BY,
15:26:53 718  	  l.CURRENT_OFFER_INDEX,
15:26:53 719  	  l.CURRENT_OFFER_RECURR_NUM,
15:26:53 720  	  l.END_DATE,
15:26:53 721  	  l.ENTITLEMENT_END_DATE,
15:26:53 722  	  l.INVOICE_ID,
15:26:53 723  	  l.IS_EXTENSION,
15:26:53 724  	  l.LICENSE_STATUS_ID,
15:26:53 725  	  l.OFFER_ID,
15:26:53 726  	  l.START_DATE,
15:26:53 727  	  l.SUBSCRIPTION_ID,
15:26:53 728  	  l.UPDATE_DATE,
15:26:53 729  	  l.UPDATED_BY
15:26:53 730  	FROM
15:26:53 731  	  GIFT_CERTIFICATE gc
15:26:53 732  	  INNER JOIN INVOICE i ON i.id = gc.PURCHASE_INVOICE_ID
15:26:53 733  	  INNER JOIN LICENSE l ON l.invoice_id = i.id
15:26:53 734  	  LEFT JOIN SUBSCRIPTION s ON s.id = l.subscription_id
15:26:53 735  	WHERE
15:26:53 736  	  l.LICENSE_STATUS_ID != GLOBAL_STATUSES_V19.LICENSE_ACTIVE
15:26:53 737  	  AND l.ENTITLEMENT_END_DATE <= (current_timestamp)
15:26:53 738  	  AND l.ENTITLEMENT_END_DATE > (current_timestamp - var_time_interval)
15:26:53 739  	  AND s.subscription_status_id = GLOBAL_STATUSES_V19.SUBSCRIPTION_CLOSED
15:26:53 740  	  AND NOT EXISTS (
15:26:53 741  	    SELECT NULL
15:26:53 742  	    FROM PROCESS_RETRY_THROTTLE
15:26:53 743  	    WHERE PROCESS_NAME = in_process_name
15:26:53 744  	      AND GENERIC_ID = l.ID
15:26:53 745  	  )
15:26:53 746  	  AND ROWNUM <= in_num_rows*10
15:26:53 747  	  ORDER BY dbms_random.value
15:26:53 748  ) WHERE
15:26:53 749  	  ROWNUM <= in_num_rows
15:26:53 750  	  GROUP BY SUBSCRIPTION_ID;
15:26:53 751  EXCEPTION
15:26:53 752  WHEN OTHERS THEN
15:26:53 753  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:53 754  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:26:53 755  END GET_ENDED_GC_LICENSES;
15:26:53 756  
