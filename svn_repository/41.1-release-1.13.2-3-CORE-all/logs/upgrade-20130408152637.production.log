15:26:54 1783  	       cc.credit_card_type_id = nvl(in_credit_card_type_id, cc.credit_card_type_id) and
15:26:54 1784  	       a.id = cc.account_id and
15:26:54 1785  	       rownum >= in_lower_bound and
15:26:54 1786  	       rownum <= in_upper_bound
15:26:54 1787  	   ;
15:26:54 1788  
