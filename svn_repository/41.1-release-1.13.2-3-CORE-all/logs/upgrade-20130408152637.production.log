15:26:53 2251  	       in_license_id	     => f_license_to_cancel.ID,
15:26:53 2252  	       in_license_status_id  => GLOBAL_STATUSES_V19.LICENSE_CLOSED,
15:26:53 2253  	       in_needs_entitlements => GLOBAL_CONSTANTS_V19.TRUE,
15:26:53 2254  	       in_updated_by	     => in_updated_by,
15:26:53 2255  	       in_entitlement_end_date => var_now
15:26:53 2256  	     );
15:26:53 2257  	   END LOOP;
15:26:53 2258  	 ELSE
15:26:53 2259  	   FOR f_license_to_cancel IN (
15:26:53 2260  	     SELECT LICENSE.ID FROM LICENSE WHERE LICENSE.INVOICE_ID = in_invoice_id AND LICENSE.LICENSE_STATUS_ID in (GLOBAL_STATUSES_V19.LICENSE_ACTIVE, GLOBAL_STATUSES_V19.LICENSE_IN_GRACE_PERIOD)
15:26:53 2261  	   )
15:26:53 2262  	   LOOP
15:26:53 2263  	     PROCS_LICENSE_CRU_V19.UPDATE_LICENSE(
15:26:53 2264  	       in_license_id	     => f_license_to_cancel.ID,
15:26:53 2265  	       in_license_status_id  => GLOBAL_STATUSES_V19.LICENSE_CLOSED,
15:26:53 2266  	       in_updated_by	     => in_updated_by
15:26:53 2267  	     );
15:26:53 2268  	   END LOOP;
15:26:53 2269  	 END IF;
15:26:53 2270  
15:26:53 2271  
15:26:53 2272  EXCEPTION
15:26:53 2273  WHEN BAD_INVOICE_ID THEN
15:26:53 2274  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:53 2275  	   SPROC_NAME, 'No such invoice');
15:26:53 2276  WHEN CAN_NOT_CALCULATE_CHARGEBACK THEN
