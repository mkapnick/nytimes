15:26:53  30  	  function getRefundAmount(in_line_item_id line_item.id%type)
15:26:53  31  	      return line_item.amount%type
15:26:53  32  	  is
15:26:53  33  	      v_li_total  line_item.amount%type;
15:26:53  34  	      v_inv_total line_item.amount%type;
15:26:53  35  	      v_ref_total line_item.amount%type;
15:26:53  36  	  begin
15:26:53  37  
15:26:53  38  	      for li in (
15:26:53  39  		  select li.invoice_id, po.*
15:26:53  40  		  from line_item li
15:26:53  41  			  join core_owner.product_offering po on li.product_offer_id = po.id
15:26:53  42  		  where li.id = in_line_item_id
15:26:53  43  	      )
15:26:53  44  	      loop
15:26:53  45  		  v_li_total := (li.quantity * li.unit_price) - getDiscountAmount(in_line_item_id);
15:26:53  46  
15:26:53  47  		  v_inv_total := 0;
15:26:53  48  		  v_ref_total := 0;
15:26:53  49  		  for x in (
15:26:53  50  		      select case when c.charge_amount < 0 then -1 else 1 end type, sum(c.charge_amount) total
15:26:53  51  		      from charge c
15:26:53  52  		      where c.invoice_id = li.invoice_id
15:26:53  53  		      group by case when c.charge_amount < 0 then -1 else 1 end
15:26:53  54  		  )
15:26:53  55  		  loop
