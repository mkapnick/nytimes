15:26:54  39  PROCEDURE GET_UNREDEEMED_GCS (
15:26:54  40  	out_result_set		OUT SYS_REFCURSOR,
15:26:54  41  	in_hours_number 	IN NUMBER DEFAULT 14*24,
15:26:54  42  	in_num_rows		IN NUMBER DEFAULT 10000,
15:26:54  43  	in_process_name IN PROCESS_RETRY_THROTTLE.PROCESS_NAME%TYPE
15:26:54  44  ) AS
15:26:54  45  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_UNREDEEMED_GCS';
15:26:54  46  BEGIN
15:26:54  47  	OPEN out_result_set FOR
15:26:54  48  SELECT * FROM
15:26:54  49  (
15:26:54  50  	SELECT
15:26:54  51  	  gc.EXPIRATION_DATE,
15:26:54  52  	  ch.name,
15:26:54  53  	  ch.id offer_chain_id,
15:26:54  54  	  gc.sender_email,
15:26:54  55  	  gc.sender_name,
15:26:54  56  	  gc.recipient_email,
15:26:54  57  	  gc.recipient_name,
15:26:54  58  	  gc.purchase_date,
15:26:54  59  	  gc.redemption_date,
15:26:54  60  	  gc.purchaser_group_id,
15:26:54  61  	  gc.redeemer_group_id,
15:26:54  62  	  gc.code,
15:26:54  63  	  gc.gift_message,
15:26:54  64  	  gc.recipient_notify_date,
15:26:54  65  	  gc.id
15:26:54  66  	FROM
15:26:54  67  	  GIFT_CERTIFICATE gc,
15:26:54  68  	  OFFER_CHAIN ch
15:26:54  69  	WHERE
15:26:54  70  	  ch.id = gc.offer_chain_id
15:26:54  71  	  AND gc.RECIPIENT_NOTIFY_DATE is not null
15:26:54  72  	  AND gc.RECIPIENT_NOTIFY_DATE >= (sysdate - in_hours_number/24)
15:26:54  73  	  AND gc.RECIPIENT_NOTIFY_DATE < (sysdate - (in_hours_number-72)/24)
15:26:54  74  	  AND gc.redeemer_group_id is null
15:26:54  75  	  AND NOT EXISTS(
15:26:54  76  	    SELECT NULL
15:26:54  77  	    FROM PROCESS_RETRY_THROTTLE
15:26:54  78  	    WHERE PROCESS_NAME = in_process_name
15:26:54  79  	      AND GENERIC_ID = gc.id
15:26:54  80  	  ) AND EXISTS(
15:26:54  81  	    SELECT NULL
15:26:54  82  	    FROM
15:26:54  83  	      charge c,
15:26:54  84  	      transaction_attempt ta,
15:26:54  85  	      transaction t
15:26:54  86  	    WHERE
15:26:54  87  	      c.invoice_id = gc.purchase_invoice_id and
15:26:54  88  	      c.transaction_id = t.id and
15:26:54  89  	      t.id = ta.transaction_id and
15:26:54  90  	      ta.transaction_attempt_status_id = GLOBAL_STATUSES_V19.TRANS_ATTEMPT_SUCCESS
15:26:54  91  	  ) AND NOT EXISTS (
15:26:54  92  	    SELECT NULL
15:26:54  93  	    FROM
15:26:54  94  	      charge c,
15:26:54  95  	      transaction t
15:26:54  96  	    WHERE
15:26:54  97  	      c.invoice_id = gc.purchase_invoice_id and
15:26:54  98  	      c.transaction_id = t.id and
15:26:54  99  	      t.is_refund = GLOBAL_CONSTANTS_V19.TRUE
15:26:54 100  	  )
15:26:54 101  	  AND ROWNUM <= in_num_rows*10
15:26:54 102  	  ORDER BY dbms_random.value
15:26:54 103  ) WHERE
15:26:54 104  	  ROWNUM <= in_num_rows;
15:26:54 105  EXCEPTION
15:26:54 106  WHEN OTHERS THEN
15:26:54 107  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:54 108  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:26:54 109  END GET_UNREDEEMED_GCS;
