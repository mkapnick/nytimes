15:26:53 1016  -- norlov: #38796
15:26:53 1017  PROCEDURE GET_TRANSACTIONS (
15:26:53 1018  /*
15:26:53 1019  Throws exceptions:
15:26:53 1020  APP_EXCEPTION_CODES_V19.NOT_FOUND
15:26:53 1021  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
15:26:53 1022  */
15:26:53 1023  	 in_group_id	       IN  NUMBER,
15:26:53 1024  	 in_invoice_id	       IN NUMBER DEFAULT NULL,
15:26:53 1025  	 in_subscription_id    IN NUMBER DEFAULT NULL,
15:26:53 1026  	 in_start_date	       IN TRANSACTION.CREATE_DATE%TYPE DEFAULT NULL,
15:26:53 1027  	 in_end_date	       IN TRANSACTION.CREATE_DATE%TYPE DEFAULT NULL,
15:26:53 1028  	 in_transaction_status IN NUMBER DEFAULT NULL,
15:26:53 1029  	 out_result_set        OUT SYS_REFCURSOR
15:26:53 1030  ) AS
15:26:53 1031  SPROC_NAME CONSTANT VARCHAR2(16) := 'GET_TRANSACTIONS';
15:26:53 1032  -- VARIABLES
15:26:53 1033  var_account_id ACCOUNT.ID%TYPE;
15:26:53 1034  statement VARCHAR2(2000);
15:26:53 1035  -- EXCEPTIONS
15:26:53 1036  BAD_GROUP_ID   EXCEPTION;
15:26:53 1037  BEGIN
15:26:53 1038  	-- check group id
15:26:53 1039  	BEGIN
15:26:53 1040  	   SELECT
15:26:53 1041  	     ACCOUNT.ID into var_account_id
15:26:53 1042  	   FROM
15:26:53 1043  	     ACCOUNT
15:26:53 1044  	   WHERE
15:26:53 1045  	     ACCOUNT.GROUP_ID = in_group_id;
15:26:53 1046  	   EXCEPTION
15:26:53 1047  	     WHEN NO_DATA_FOUND THEN
15:26:53 1048  	       RAISE BAD_GROUP_ID;
15:26:53 1049  	 END;
15:26:53 1050  
15:26:53 1051  	 OPEN out_result_set FOR
15:26:53 1052  	 SELECT
15:26:53 1053  	   TRANSACTION.ID,
15:26:53 1054  	   TRANSACTION.TRANSACTION_STATUS_ID,
15:26:53 1055  	   TRANSACTION.TRANSACTION_AMOUNT,
15:26:53 1056  	   TRANSACTION.CREATE_DATE,
15:26:53 1057  	   TRANSACTION.CREATED_BY,
15:26:53 1058  	   TRANSACTION.IS_REFUND,
15:26:53 1059  	   GET_TRANSACTION_TAX_AMOUNT(TRANSACTION.ID) as TRANSACTION_TAX_AMOUNT,
15:26:53 1060  	   GET_TRANSACTION_INTRL_TAXES(TRANSACTION.ID) as INTERNATIONAL_TOTAL
15:26:53 1061  	 FROM
15:26:53 1062  	   TRANSACTION
15:26:53 1063  	   INNER JOIN CHARGE ON TRANSACTION.ID = CHARGE.TRANSACTION_ID
15:26:53 1064  	   INNER JOIN INVOICE ON INVOICE.ID = CHARGE.INVOICE_ID
15:26:53 1065  	 WHERE
15:26:53 1066  	   -- Filter by invoice ID
15:26:53 1067  	   (
15:26:53 1068  	     INVOICE.ID IN (
15:26:53 1069  	       -- Gift certificate invoices
15:26:53 1070  	       SELECT
15:26:53 1071  		 GIFT_CERTIFICATE.PURCHASE_INVOICE_ID
15:26:53 1072  	       FROM
15:26:53 1073  		 GIFT_CERTIFICATE
15:26:53 1074  	       WHERE
15:26:53 1075  		 GIFT_CERTIFICATE.PURCHASER_GROUP_ID = in_group_id
15:26:53 1076  		 -- If subscription_id is set then return nothing
15:26:53 1077  		 AND EXISTS (SELECT 1 FROM DUAL WHERE in_subscription_id IS NULL)
15:26:53 1078  	     )
15:26:53 1079  	     OR
15:26:53 1080  	     INVOICE.ID IN (
15:26:53 1081  	       SELECT
15:26:53 1082  		 LICENSE.INVOICE_ID
15:26:53 1083  	       FROM
15:26:53 1084  		 LICENSE
15:26:53 1085  		 INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:26:53 1086  	       WHERE
15:26:53 1087  		 SUBSCRIPTION.ACCOUNT_ID = var_account_id
15:26:53 1088  		 -- Filter by subscription id
15:26:53 1089  		 AND SUBSCRIPTION.ID = NVL(in_subscription_id, SUBSCRIPTION.ID)
15:26:53 1090  	     )
15:26:53 1091  	   )
15:26:53 1092  	   -- Filter by invoice id
15:26:53 1093  	   AND INVOICE.ID = NVL(in_invoice_id, INVOICE.ID)
15:26:53 1094  	   -- Filter by start date
15:26:53 1095  	   AND TRANSACTION.CREATE_DATE >= NVL(in_start_date, TRANSACTION.CREATE_DATE)
15:26:53 1096  	   -- Filter by end date
15:26:53 1097  	   AND TRANSACTION.CREATE_DATE <= NVL(in_end_date, TRANSACTION.CREATE_DATE)
15:26:53 1098  	   -- Filter by transaction status
15:26:53 1099  	   AND TRANSACTION.TRANSACTION_STATUS_ID IN ( SELECT NVL(in_transaction_status, TRANSACTION.TRANSACTION_STATUS_ID) FROM DUAL);
15:26:53 1100  
15:26:53 1101  /*
15:26:53 1102  
15:26:53 1103  statement :=  'select distinct '||CHR(10)
15:26:53 1104  || ' TRANSACTION.ID,'||CHR(10)
15:26:53 1105  || ' TRANSACTION.TRANSACTION_STATUS_ID, '||CHR(10)
15:26:53 1106  || ' TRANSACTION.TRANSACTION_AMOUNT, '||CHR(10)
15:26:53 1107  || ' TRANSACTION.CREATE_DATE, '||CHR(10)
15:26:53 1108  || ' TRANSACTION.CREATED_BY, '||CHR(10)
15:26:53 1109  || ' TRANSACTION.IS_REFUND from TRANSACTION '||CHR(10)
15:26:53 1110  || ' inner join CHARGE on (CHARGE.TRANSACTION_ID = TRANSACTION.ID)'||CHR(10)
15:26:53 1111  || ' inner join INVOICE on (INVOICE.ID = CHARGE.INVOICE_ID)'||CHR(10)
15:26:53 1112  || ' inner join LICENSE on (LICENSE.INVOICE_ID = INVOICE.ID)'||CHR(10)
15:26:53 1113  || ' inner join SUBSCRIPTION on (SUBSCRIPTION.ID = LICENSE.SUBSCRIPTION_ID)'||CHR(10)
15:26:53 1114  || ' inner join ACCOUNT on (ACCOUNT.ID = SUBSCRIPTION.ACCOUNT_ID)'||CHR(10)
15:26:53 1115  || ' where ACCOUNT.GROUP_ID = '||in_group_id;
15:26:53 1116  
15:26:53 1117  IF (in_transaction_status IS NOT NULL) THEN
15:26:53 1118  	 statement := statement || CHR(10) || 'AND TRANSACTION.TRANSACTION_STATUS_ID=' || in_transaction_status;
15:26:53 1119  END IF;
15:26:53 1120  
15:26:53 1121  IF (in_invoice_id IS NOT NULL) THEN
15:26:53 1122  	 statement := statement || CHR(10) || 'AND INVOICE.ID=' || in_invoice_id;
15:26:53 1123  END IF;
15:26:53 1124  
15:26:53 1125  IF (in_subscription_id IS NOT NULL) THEN
15:26:53 1126  	 statement := statement || CHR(10) || 'AND SUBSCRIPTION.ID=' || in_subscription_id;
15:26:53 1127  END IF;
15:26:53 1128  
15:26:53 1129  IF (in_start_date IS NOT NULL) THEN
15:26:53 1130  	 statement := statement || CHR(10) || 'AND TRANSACTION.CREATE_DATE>= TO_DATE(''' || TO_CHAR(in_start_date,'yyyy/mm/dd:hh:mi:ss') || ''',''yyyy/mm/dd:hh:mi:ss'')';  -- norlov: ??
15:26:53 1131  END IF;
15:26:53 1132  
15:26:53 1133  IF (in_end_date IS NOT NULL) THEN
15:26:53 1134  	 statement := statement || CHR(10) || 'AND TRANSACTION.CREATE_DATE<= TO_DATE(''' || TO_CHAR(in_end_date,'yyyy/mm/dd:hh:mi:ss') || ''',''yyyy/mm/dd:hh:mi:ss'')'; -- norlov: ??
15:26:53 1135  END IF;
15:26:53 1136  dbms_output.put_line(statement);
15:26:53 1137  OPEN out_result_set FOR statement;
15:26:53 1138  
15:26:53 1139  */
15:26:53 1140  
15:26:53 1141  EXCEPTION
15:26:53 1142  WHEN BAD_GROUP_ID THEN
15:26:53 1143  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:53 1144  	   SPROC_NAME, 'No such group');
15:26:53 1145  WHEN OTHERS THEN
