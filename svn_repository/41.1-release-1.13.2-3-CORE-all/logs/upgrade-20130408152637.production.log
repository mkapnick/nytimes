15:26:54 2289  	       RAISE BAD_CHARGE_ID;
15:26:54 2290  	 END;
15:26:54 2291  
15:26:54 2292  	 BEGIN
15:26:54 2293  	   SELECT
15:26:54 2294  	     GIFT_CERTIFICATE.OFFER_CHAIN_ID into var_offer_chain_id
15:26:54 2295  	   FROM
15:26:54 2296  	     GIFT_CERTIFICATE
15:26:54 2297  	   WHERE
15:26:54 2298  	     GIFT_CERTIFICATE.ID = in_gift_certificate_id;
15:26:54 2299  	   EXCEPTION
15:26:54 2300  	     WHEN NO_DATA_FOUND THEN
15:26:54 2301  	       RAISE BAD_GIFT_CERTIFICATE_ID;
15:26:54 2302  	 END;
15:26:54 2303  
15:26:54 2304  	 BEGIN
15:26:54 2305  	   SELECT
15:26:54 2306  	     SUBSCRIPTION.OFFER_CHAIN_ID into var_offer_chain_s_id
15:26:54 2307  	   FROM
15:26:54 2308  	     SUBSCRIPTION
15:26:54 2309  	   WHERE
15:26:54 2310  	     SUBSCRIPTION.ID IN (
15:26:54 2311  	       SELECT DISTINCT
15:26:54 2312  		 LICENSE.SUBSCRIPTION_ID
15:26:54 2313  	       FROM
15:26:54 2314  		 LICENSE
15:26:54 2315  	       WHERE
15:26:54 2316  		 LICENSE.INVOICE_ID = var_invoice_id
15:26:54 2317  	     );
15:26:54 2318  	   EXCEPTION
15:26:54 2319  	     WHEN NO_DATA_FOUND THEN
15:26:54 2320  	       RAISE CAN_NOT_FIND_SUBSCRIPTION;
15:26:54 2321  	 END;
15:26:54 2322  
15:26:54 2323  	 IF var_offer_chain_s_id = var_offer_chain_id THEN
15:26:54 2324  	   out_result := GLOBAL_CONSTANTS_V19.TRUE;
15:26:54 2325  	 ELSE
15:26:54 2326  	   out_result := GLOBAL_CONSTANTS_V19.FALSE;
15:26:54 2327  	 END IF;
15:26:54 2328  
15:26:54 2329  EXCEPTION
