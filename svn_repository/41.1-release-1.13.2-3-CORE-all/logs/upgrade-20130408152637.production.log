15:26:53 1050  /******************************************************************************/
15:26:53 1051  
15:26:53 1052  FUNCTION GET_RECENT_CHARGE (
15:26:53 1053  /*
15:26:53 1054  Throws exceptions:
15:26:53 1055  APP_EXCEPTION_CODES_V19.NOT_FOUND
15:26:53 1056  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
15:26:53 1057  */
15:26:53 1058  	 in_subscription_id IN NUMBER
15:26:53 1059  ) RETURN NUMBER AS
15:26:53 1060  -- VARIABLES
15:26:53 1061  SPROC_NAME	    CONSTANT VARCHAR2(17) := 'GET_RECENT_CHARGE';
15:26:53 1062  temp_subscription_id NUMBER;
15:26:53 1063  var_recent_charge    NUMBER(10,2);
15:26:53 1064  
15:26:53 1065  -- EXCEPTIONS
15:26:53 1066  BAD_SUBSCRIPTION_ID EXCEPTION;
15:26:53 1067  BEGIN
15:26:53 1068  
15:26:53 1069  	 BEGIN
15:26:53 1070  	   SELECT
15:26:53 1071  	     SUBSCRIPTION.ID into temp_subscription_id
15:26:53 1072  	   FROM
15:26:53 1073  	     SUBSCRIPTION
15:26:53 1074  	   WHERE
15:26:53 1075  	     SUBSCRIPTION.ID = in_subscription_id;
15:26:53 1076  	   EXCEPTION
15:26:53 1077  	     WHEN NO_DATA_FOUND THEN
15:26:53 1078  	       RAISE BAD_SUBSCRIPTION_ID;
15:26:53 1079  	 END;
15:26:53 1080  
15:26:53 1081  	 BEGIN
15:26:53 1082  	   SELECT
15:26:53 1083  	     CHARGE.CHARGE_AMOUNT into var_recent_charge
15:26:53 1084  	   FROM
15:26:53 1085  	     LICENSE
15:26:53 1086  	     INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:26:53 1087  	     INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
15:26:53 1088  	     INNER JOIN CHARGE ON CHARGE.INVOICE_ID = INVOICE.ID
15:26:53 1089  	   WHERE
15:26:53 1090  	     -- TODO: Review
15:26:53 1091  	     -- LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V19.LICENSE_PROCESSED
15:26:53 1092  	     -- AND
15:26:53 1093  	     SUBSCRIPTION.ID = in_subscription_id
15:26:53 1094  	     AND CHARGE.CHARGE_AMOUNT >= 0
15:26:53 1095  	     AND ROWNUM <= 1
15:26:53 1096  	   ORDER BY
15:26:53 1097  	     LICENSE.ID ASC, CHARGE.ID DESC;
15:26:53 1098  	   EXCEPTION
15:26:53 1099  	     WHEN NO_DATA_FOUND THEN
15:26:53 1100  	       var_recent_charge := 0;
15:26:53 1101  	 END;
15:26:53 1102  
15:26:53 1103  	 RETURN var_recent_charge;
15:26:53 1104  
15:26:53 1105  EXCEPTION
15:26:53 1106  WHEN BAD_SUBSCRIPTION_ID THEN
15:26:53 1107  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:53 1108  	   SPROC_NAME, 'No such subscription');
15:26:53 1109  WHEN OTHERS THEN
15:26:53 1110  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:53 1111  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:26:53 1112  END GET_RECENT_CHARGE;
