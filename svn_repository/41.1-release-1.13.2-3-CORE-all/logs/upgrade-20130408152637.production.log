15:26:53   9  	in_invoice_id IN NUMBER,
15:26:53  10  	in_offer_id   IN NUMBER,
15:26:53  11  	in_created_by IN VARCHAR2
15:26:53  12  ) AS
15:26:53  13  -- VARIABLES
15:26:53  14  SPROC_NAME      CONSTANT VARCHAR2(14) := 'ADD_LINE_ITEMS';
15:26:53  15  temp_invoice_id NUMBER;
15:26:53  16  temp_offer_id   NUMBER;
15:26:53  17  
15:26:53  18  var_line_item_data SYS_REFCURSOR;
15:26:53  19  var_new_line_item_id NUMBER;
15:26:53  20  var_product_unit_price NUMBER (10,6);
15:26:53  21  var_product_offering_price NUMBER(10,6);
15:26:53  22  var_product_offering_oprice NUMBER(10,6);
15:26:53  23  var_product_quantity NUMBER;
15:26:53  24  var_product_offering_id NUMBER;
15:26:53  25  
15:26:53  26  var_line_item_price	  NUMBER(10,2);
15:26:53  27  var_discount_fixed_amount   NUMBER(10,6);
15:26:53  28  var_discount_percent_amount NUMBER(10,2);
15:26:53  29  
15:26:53  30  
15:26:53  31  -- EXCEPTIONS
15:26:53  32  BAD_INVOICE_ID EXCEPTION;
15:26:53  33  BAD_OFFER_ID EXCEPTION;
15:26:53  34  BAD_DISCOUNT EXCEPTION;
15:26:53  35  BEGIN
15:26:53  36  
15:26:53  37  	-- Check that given invoice exists
15:26:53  38  	BEGIN
15:26:53  39  	  SELECT
15:26:53  40  	    INVOICE.ID into temp_invoice_id
15:26:53  41  	  FROM
15:26:53  42  	    INVOICE
15:26:53  43  	  WHERE
15:26:53  44  	    INVOICE.ID = in_invoice_id;
15:26:53  45  
15:26:53  46  	  EXCEPTION
15:26:53  47  	    WHEN NO_DATA_FOUND THEN
15:26:53  48  	      RAISE BAD_INVOICE_ID;
15:26:53  49  	END;
15:26:53  50  
15:26:53  51  	-- Check that given offer exists
15:26:53  52  	BEGIN
15:26:53  53  	  SELECT
15:26:53  54  	    OFFER.ID into temp_offer_id
15:26:53  55  	  FROM
15:26:53  56  	    OFFER
15:26:53  57  	  WHERE
15:26:53  58  	    OFFER.ID = in_offer_id;
15:26:53  59  
15:26:53  60  	  EXCEPTION
15:26:53  61  	    WHEN NO_DATA_FOUND THEN
15:26:53  62  	      RAISE BAD_OFFER_ID;
15:26:53  63  	END;
15:26:53  64  
15:26:53  65  	-- Get product_offering data
15:26:53  66  	OPEN var_line_item_data FOR
15:26:53  67  	SELECT
15:26:53  68  	  PRODUCT_OFFERING.ID,
15:26:53  69  	  PRODUCT_OFFERING.UNIT_PRICE,
15:26:53  70  	  PRODUCT_OFFERING.QUANTITY
15:26:53  71  	FROM
15:26:53  72  	  OFFER_PRODUCT_OFFERING
15:26:53  73  	  INNER JOIN PRODUCT_OFFERING ON OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = PRODUCT_OFFERING.ID
15:26:53  74  	WHERE
15:26:53  75  	  OFFER_PRODUCT_OFFERING.OFFER_ID = in_offer_id;
15:26:53  76  
15:26:53  77  	-- insert line items
15:26:53  78  	-- add discounts to line items
15:26:53  79  	LOOP
15:26:53  80  	  FETCH var_line_item_data INTO
15:26:53  81  	    var_product_offering_id,
15:26:53  82  	    var_product_unit_price,
15:26:53  83  	    var_product_quantity;
15:26:53  84  	  EXIT WHEN var_line_item_data%NOTFOUND;
15:26:53  85  
15:26:53  86  	  var_product_offering_oprice := var_product_unit_price * var_product_quantity;
15:26:53  87  	  var_product_offering_price := var_product_offering_oprice;
15:26:53  88  
15:26:53  89  	  -- Apply discounts to line_item
15:26:53  90  	  BEGIN
15:26:53  91  	    SELECT
15:26:53  92  	      SUM (DISCOUNT.FIXED_AMOUNT) into var_discount_fixed_amount
15:26:53  93  	    FROM
15:26:53  94  	      DISCOUNT_PRODUCT_OFFERING
15:26:53  95  	      INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
15:26:53  96  	    WHERE
15:26:53  97  	      DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
15:26:53  98  	      AND DISCOUNT.FIXED_AMOUNT IS NOT NULL;
15:26:53  99  	    EXCEPTION
15:26:53 100  	    WHEN NO_DATA_FOUND THEN
15:26:53 101  	      var_discount_fixed_amount := NULL;
15:26:53 102  	  END;
15:26:53 103  
15:26:53 104  	  BEGIN
15:26:53 105  	    SELECT
15:26:53 106  	      SUM (DISCOUNT.PERCENT_AMOUNT) into var_discount_percent_amount
15:26:53 107  	    FROM
15:26:53 108  	      DISCOUNT_PRODUCT_OFFERING
15:26:53 109  	      INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
15:26:53 110  	    WHERE
15:26:53 111  	      DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
15:26:53 112  	      AND DISCOUNT.PERCENT_AMOUNT IS NOT NULL;
15:26:53 113  	    EXCEPTION
15:26:53 114  	    WHEN NO_DATA_FOUND THEN
15:26:53 115  	      var_discount_percent_amount := NULL;
15:26:53 116  	  END;
15:26:53 117  
15:26:53 118  	  IF (var_discount_percent_amount IS NOT NULL) THEN
15:26:53 119  	    var_product_offering_price := var_product_offering_price * ( 1 - var_discount_percent_amount / 100 );
15:26:53 120  	  END IF;
15:26:53 121  
15:26:53 122  	  IF (var_discount_fixed_amount IS NOT NULL) THEN
15:26:53 123  	    var_product_offering_price := var_product_offering_price - var_discount_fixed_amount;
15:26:53 124  	  END IF;
15:26:53 125  
15:26:53 126  	  var_line_item_price := PROCS_COMMON_V19.ROUND_10_6_TO_10_2(var_product_offering_price);
15:26:53 127  
15:26:53 128  	  IF (var_line_item_price < 0) THEN
15:26:53 129  	      RAISE BAD_DISCOUNT;
15:26:53 130  	  END IF;
15:26:53 131  
15:26:53 132  	  var_new_line_item_id := NULL;
15:26:53 133  	  PROCS_LINE_ITEMS_CRU_V19.CREATE_LINE_ITEM(
15:26:53 134  	    inout_line_item_id	=> var_new_line_item_id,
15:26:53 135  	    in_product_offer_id => var_product_offering_id,
15:26:53 136  	    in_invoice_id	=> in_invoice_id,
15:26:53 137  	    in_amount		=> var_line_item_price,
15:26:53 138  	    in_created_by	=> in_created_by,
15:26:53 139  	    in_discount_amount	=> var_product_offering_oprice - var_line_item_price,
15:26:53 140  	    in_taxes_amount	=> NULL
15:26:53 141  	  );
15:26:53 142  
15:26:53 143  	  FOR f_discount IN (
15:26:53 144  	    SELECT
15:26:53 145  	      DISCOUNT.ID
