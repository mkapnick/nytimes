15:26:52  88  	PROCEDURE CHASE_PROFILE_BY_REQ_FILE_ID(
15:26:52  89  	  in_request_file_id IN CC_UPDATE.CC_REQUEST_FILE_ID%TYPE,
15:26:52  90  	  in_start	     IN NUMBER,
15:26:52  91  	  in_end	     IN NUMBER,
15:26:52  92  	  out_result_set     OUT SYS_REFCURSOR
15:26:52  93  	) AS
15:26:52  94  	SPROC_NAME CONSTANT VARCHAR2(32) := 'CHASE_PROFILE_BY_REQ_FILE_ID';
15:26:52  95  	var_range_diff	    NUMBER := 0;
15:26:52  96  	var_upper_bond_diff NUMBER := 0;
15:26:52  97  	var_l_start	    NUMBER := 0;
15:26:52  98  	var_l_end	    NUMBER := 0;
15:26:52  99  	BEGIN
15:26:52 100  	  --Normalize the end points [START]
15:26:52 101  	  IF (in_start IS NULL OR in_start < 0) Then
15:26:52 102  	    var_l_start := 0;
15:26:52 103  	  ELSE
15:26:52 104  	    var_l_start := in_start;
15:26:52 105  	  END IF;
15:26:52 106  
15:26:52 107  	  IF (in_end IS NULL) Then
15:26:52 108  	    var_l_end := 500;
15:26:52 109  	  ELSE
15:26:52 110  	    var_l_end := in_end;
15:26:52 111  	  END IF;
15:26:52 112  
15:26:52 113  	  var_l_start := var_l_start + 1;
15:26:52 114  	  var_l_end   := var_l_end   + 1;
15:26:52 115  
15:26:52 116  	  var_range_diff  := var_l_end - var_l_start;
15:26:52 117  	  var_upper_bond_diff :=  var_range_diff - 1000;
15:26:52 118  
15:26:52 119  	  IF (var_upper_bond_diff > 0) Then
15:26:52 120  	    var_l_end := var_l_end - var_upper_bond_diff;
15:26:52 121  	  END IF;
15:26:52 122  	  --Normalize the end points [END]
15:26:52 123  
15:26:52 124  	  OPEN out_result_set FOR
15:26:52 125  	    SELECT CHASE_PROFILE_ID FROM
15:26:52 126  	      (SELECT rownum rnum, q.* FROM
15:26:52 127  		 (SELECT
15:26:52 128  		    cc.CHASE_PROFILE_ID
15:26:52 129  		  FROM
15:26:52 130  		    CREDIT_CARD cc,
15:26:52 131  		    CC_UPDATE ccu
15:26:52 132  		  WHERE
15:26:52 133  		    ccu.CC_REQUEST_FILE_ID = in_request_file_id
15:26:52 134  		    AND ccu.CREDIT_CARD_ID = cc.id
15:26:52 135  		) Q
15:26:52 136  	      WHERE rownum <= var_l_end)
15:26:52 137  	    WHERE rnum >= var_l_Start;
15:26:52 138  	END CHASE_PROFILE_BY_REQ_FILE_ID;
