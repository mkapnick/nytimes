15:26:53 494  		RAISE CAN_NOT_CREATE_CHARGE;
15:26:53 495  	  END;
15:26:53 496  	END IF;
15:26:53 497  
15:26:53 498  EXCEPTION
15:26:53 499  WHEN CAN_NOT_USE_FCINSTR THEN
15:26:53 500  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
15:26:53 501  	  SPROC_NAME, 'Could not use "free charge instrument" for non-zero invoice');
