15:26:53 212  
15:26:53 213  	-- if user have any active existing subscriptions to the offer chain
15:26:53 214  	-- and if MAX_CONCURRENT_SUBS <= [user's subscription count for the offer chain]
15:26:53 215  	-- then deny purchase
15:26:53 216  	var_max_concurrent_subscrs := PROCS_OFFER_CHAIN_V19.GET_OFFER_CHAIN_MAX_CONC_SUBSC(in_offer_chain_id);
15:26:53 217  	IF var_max_concurrent_subscrs != GLOBAL_CONSTANTS_V19.INFINITY
15:26:53 218  	  AND var_max_concurrent_subscrs <= var_same_offer_chains_num THEN
15:26:53 219  	  -- Find first concurrent subscription id:
15:26:53 220  	  SELECT
15:26:53 221  	    ID into var_concur_subscription_id
15:26:53 222  	  FROM (
15:26:53 223  	    SELECT
15:26:53 224  	      ID
15:26:53 225  	    FROM
15:26:53 226  	      SUBSCRIPTION
15:26:53 227  	    WHERE
15:26:53 228  	      SUBSCRIPTION.ACCOUNT_ID = var_account_id
15:26:53 229  	      AND SUBSCRIPTION.OFFER_CHAIN_ID = in_offer_chain_id
15:26:53 230  	      AND (SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V19.SUBSCRIPTION_ACTIVE
15:26:53 231  		   OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V19.SUBSCRIPTION_IN_GRACE_PERIOD)
15:26:53 232  	    ORDER BY
15:26:53 233  	      ID
15:26:53 234  	  )
15:26:53 235  	  WHERE
15:26:53 236  	    ROWNUM <= 1;
15:26:53 237  	  RAISE LIMIT_REACHED;
15:26:53 238  	END IF;
15:26:53 239  
15:26:53 240  	-- if user does not have any active existing subscriptions to the offer chain
15:26:53 241  	-- and if product from the offer chain is already owned from another offer chain
15:26:53 242  	-- then deny purchase
15:26:53 243  	IF (in_check_dupe_products != GLOBAL_CONSTANTS_V19.FALSE) THEN
15:26:53 244  	  FOR f_account_offer_chains IN (
15:26:53 245  	    SELECT DISTINCT
15:26:53 246  	      OFFER_CHAIN_ID
15:26:53 247  	    FROM
15:26:53 248  	      SUBSCRIPTION
