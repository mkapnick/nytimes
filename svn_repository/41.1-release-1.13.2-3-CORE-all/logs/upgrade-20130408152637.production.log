15:26:53 974  var_offer_months	 NUMBER;
15:26:53 975  var_offer_days	 NUMBER;
15:26:53 976  var_offer_end_date DATE;
15:26:53 977  -- EXCEPTIONS
15:26:53 978  BAD_OFFER_ID EXCEPTION;
15:26:53 979  BEGIN
15:26:53 980  
15:26:53 981  	BEGIN
15:26:53 982  	  SELECT
15:26:53 983  	    OFFER.ENTITLEMENT_DURATION into var_offer_duration
15:26:53 984  	  FROM
15:26:53 985  	    OFFER
15:26:53 986  	  WHERE
15:26:53 987  	    OFFER.ID = in_offer_id;
15:26:53 988  	  EXCEPTION
15:26:53 989  	    WHEN NO_DATA_FOUND THEN
15:26:53 990  	      RAISE BAD_OFFER_ID;
15:26:53 991  	END;
15:26:53 992  
15:26:53 993  	PROCS_COMMON_V19.ISO8601DURATION_TO_INTERVALS (
15:26:53 994  	  var_offer_duration,
15:26:53 995  	  var_offer_years,
15:26:53 996  	  var_offer_months,
15:26:53 997  	  var_offer_days
15:26:53 998  	);
15:26:53 999  
