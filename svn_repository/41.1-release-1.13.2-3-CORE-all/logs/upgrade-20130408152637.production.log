15:26:53 1399  
15:26:53 1400  PROCEDURE ADD_SUBSCRIPTION_EXTENSION (
15:26:53 1401  /*
15:26:53 1402  APP_EXCEPTION_CODES_V19.NOT_FOUND
15:26:53 1403  APP_EXCEPTION_CODES_V19.INTERNAL_ERROR
15:26:53 1404  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
15:26:53 1405  */
15:26:53 1406  	 in_subscription_id	 IN NUMBER,
15:26:53 1407  	 in_effective_start_date IN DATE,
15:26:53 1408  	 in_effective_end_date	 IN DATE,
15:26:53 1409  	 in_note		 IN VARCHAR2,
15:26:53 1410  	 in_updated_by		 IN VARCHAR2
15:26:53 1411  ) AS
15:26:53 1412  -- VARIABLES
15:26:53 1413  SPROC_NAME		    CONSTANT VARCHAR2(26) := 'ADD_SUBSCRIPTION_EXTENSION';
15:26:53 1414  temp_subscription_id	    NUMBER;
15:26:53 1415  var_current_license_id	    NUMBER;
15:26:53 1416  var_current_license_start_date DATE;
15:26:53 1417  var_current_license_end_date DATE;
15:26:53 1418  var_current_offer_id	    NUMBER;
15:26:53 1419  var_current_invoice_id	    NUMBER;
15:26:53 1420  var_current_date 	    DATE;
15:26:53 1421  var_current_offer_index	    NUMBER;
15:26:53 1422  var_current_offer_recurr_num NUMBER;
15:26:53 1423  var_account_tax_exempt_id    VARCHAR2(255);
15:26:53 1424  
15:26:53 1425  var_free_invoice_id NUMBER;
15:26:53 1426  var_free_license_id NUMBER;
15:26:53 1427  var_new_license_id  NUMBER;
15:26:53 1428  var_ext_license_id  NUMBER;
15:26:53 1429  -- EXCEPTIONS
15:26:53 1430  BAD_SUBSCRIPTION_ID	     EXCEPTION;
15:26:53 1431  CAN_NOT_FIND_OFFER_OR_LICENSE EXCEPTION;
15:26:53 1432  CAN_NOT_CHANGE_LICENSE_STATUS EXCEPTION;
15:26:53 1433  CAN_NOT_CREATE_INVOICE	     EXCEPTION;
15:26:53 1434  CAN_NOT_CREATE_NEW_LICENSE    EXCEPTION;
15:26:53 1435  CAN_NOT_CREATE_END_LICENSE    EXCEPTION;
15:26:53 1436  CAN_NOT_ANNOTATE_SUBSCRIPTION EXCEPTION;
15:26:53 1437  EXTENS_START_DATE_IS_TOO_FAR  EXCEPTION;
15:26:53 1438  EXT_START_DATE_LATER_THEN_END EXCEPTION;
15:26:53 1439  EXTENS_START_DATE_IS_TOO_SMALL EXCEPTION;
15:26:53 1440  EXCEPTION_MESSAGE	     VARCHAR2(1024);
15:26:53 1441  BEGIN
15:26:53 1442  
15:26:53 1443  	 var_current_date := PROCS_COMMON_V19.NORMALIZE_DATE(SYSDATE);
15:26:53 1444  
15:26:53 1445  	 -- Check that subscription exists
15:26:53 1446  	 BEGIN
15:26:53 1447  	   SELECT
15:26:53 1448  	     SUBSCRIPTION.ID into temp_subscription_id
15:26:53 1449  	   FROM
15:26:53 1450  	     SUBSCRIPTION
15:26:53 1451  	   WHERE
15:26:53 1452  	     SUBSCRIPTION.ID = in_subscription_id;
15:26:53 1453  	   EXCEPTION
15:26:53 1454  	     WHEN NO_DATA_FOUND THEN
15:26:53 1455  	       RAISE BAD_SUBSCRIPTION_ID;
15:26:53 1456  	 END;
15:26:53 1457  
15:26:53 1458  	 -- Get account tax exempt id
15:26:53 1459  	 SELECT DISTINCT
15:26:53 1460  	   ACCOUNT.TAX_EXEMPT_ID into var_account_tax_exempt_id
15:26:53 1461  	 FROM
15:26:53 1462  	   ACCOUNT
15:26:53 1463  	   INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.ACCOUNT_ID = ACCOUNT.ID
15:26:53 1464  	 WHERE
15:26:53 1465  	   SUBSCRIPTION.ID = in_subscription_id;
15:26:53 1466  
15:26:53 1467  	 -- Select current data
15:26:53 1468  	 BEGIN
15:26:53 1469  	   SELECT
15:26:53 1470  	     LICENSE.ID,
15:26:53 1471  	     LICENSE.START_DATE,
15:26:53 1472  	     LICENSE.END_DATE,
15:26:53 1473  	     LICENSE.CURRENT_OFFER_INDEX,
15:26:53 1474  	     LICENSE.CURRENT_OFFER_RECURR_NUM,
15:26:53 1475  	     OFFER.ID,
15:26:53 1476  	     INVOICE.ID
15:26:53 1477  	   INTO
15:26:53 1478  	     var_current_license_id,
15:26:53 1479  	     var_current_license_start_date,
15:26:53 1480  	     var_current_license_end_date,
15:26:53 1481  	     var_current_offer_index,
15:26:53 1482  	     var_current_offer_recurr_num,
15:26:53 1483  	     var_current_offer_id,
15:26:53 1484  	     var_current_invoice_id
15:26:53 1485  	   FROM
15:26:53 1486  	     LICENSE
15:26:53 1487  	     INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:26:53 1488  	     INNER JOIN OFFER ON LICENSE.OFFER_ID = OFFER.ID
15:26:53 1489  	     INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
15:26:53 1490  	   WHERE
15:26:53 1491  	     SUBSCRIPTION.ID = in_subscription_id
15:26:53 1492  	     AND PROCS_COMMON_V19.NORMALIZE_DATE(LICENSE.END_DATE) > var_current_date
15:26:53 1493  	     AND PROCS_COMMON_V19.NORMALIZE_DATE(LICENSE.START_DATE) <= var_current_date
15:26:53 1494  	     AND ROWNUM <= 1
15:26:53 1495  	   ORDER BY
15:26:53 1496  	     LICENSE.ID DESC;
15:26:53 1497  	   EXCEPTION
15:26:53 1498  	     WHEN NO_DATA_FOUND THEN
15:26:53 1499  	       RAISE CAN_NOT_FIND_OFFER_OR_LICENSE;
15:26:53 1500  	 END;
15:26:53 1501  
15:26:53 1502  	 IF var_current_license_end_date < in_effective_start_date THEN
15:26:53 1503  	   RAISE EXTENS_START_DATE_IS_TOO_FAR;
15:26:53 1504  	 END IF;
15:26:53 1505  
15:26:53 1506  	 IF var_current_license_start_date > in_effective_start_date THEN
15:26:53 1507  	   RAISE EXTENS_START_DATE_IS_TOO_SMALL;
15:26:53 1508  	 END IF;
15:26:53 1509  
15:26:53 1510  	 IF in_effective_start_date > in_effective_end_date THEN
15:26:53 1511  	   RAISE EXT_START_DATE_LATER_THEN_END;
15:26:53 1512  	 END IF;
15:26:53 1513  
15:26:53 1514  	 -- Closing curent license
15:26:53 1515  	 BEGIN
15:26:53 1516  	   PROCS_LICENSE_CRU_V19.UPDATE_LICENSE(
15:26:53 1517  	     in_license_id	   => var_current_license_id,
15:26:53 1518  	     in_updated_by	   => in_updated_by,
15:26:53 1519  	     in_license_status_id  => GLOBAL_STATUSES_V19.LICENSE_CLOSED,
15:26:53 1520  	     in_end_date	   => in_effective_start_date,
15:26:53 1521  	     in_needs_entitlements => GLOBAL_CONSTANTS_V19.TRUE
15:26:53 1522  	   );
15:26:53 1523  	   EXCEPTION
15:26:53 1524  	     WHEN OTHERS THEN
15:26:53 1525  	       EXCEPTION_MESSAGE := SQLERRM;
15:26:53 1526  	       RAISE CAN_NOT_CHANGE_LICENSE_STATUS;
15:26:53 1527  	 END;
15:26:53 1528  
15:26:53 1529  	 -- Creating new "free" invoice
15:26:53 1530  	 BEGIN
15:26:53 1531  	   PROCS_INVOICE_V19.CREATE_INVOICE(
15:26:53 1532  	     in_invoice_status => GLOBAL_STATUSES_V19.INVOICE_CLOSED,
15:26:53 1533  	     in_created_by     => in_updated_by,
15:26:53 1534  	     in_tax_exempt_id  => var_account_tax_exempt_id,
15:26:53 1535  	     out_invoice_id    => var_free_invoice_id
15:26:53 1536  	   );
15:26:53 1537  	   EXCEPTION
15:26:53 1538  	     WHEN OTHERS THEN
15:26:53 1539  	       EXCEPTION_MESSAGE := SQLERRM;
15:26:53 1540  	       RAISE CAN_NOT_CREATE_INVOICE;
15:26:53 1541  	 END;
15:26:53 1542  
15:26:53 1543  	 -- Creating new "free" license
15:26:53 1544  	 BEGIN
15:26:53 1545  	   PROCS_LICENSE_V19.CREATE_LICENSE (
15:26:53 1546  	     in_status_id		 => GLOBAL_STATUSES_V19.LICENSE_ACTIVE,
15:26:53 1547  	     in_needs_entitlements	 => GLOBAL_CONSTANTS_V19.TRUE,
15:26:53 1548  	     in_start_date		 => in_effective_start_date,
15:26:53 1549  	     in_end_date		 => in_effective_end_date,
15:26:53 1550  	     in_offer_id		 => var_current_offer_id,
15:26:53 1551  	     in_subscription_id 	 => in_subscription_id,
15:26:53 1552  	     in_invoice_id		 => var_free_invoice_id,
15:26:53 1553  	     in_created_by		 => in_updated_by,
15:26:53 1554  	     in_is_extension		 => GLOBAL_CONSTANTS_V19.TRUE,
15:26:53 1555  	     in_current_offer_index	 => var_current_offer_index,
15:26:53 1556  	     in_current_offer_recurr_num => var_current_offer_recurr_num,
15:26:53 1557  	     out_license_id		 => var_free_license_id
15:26:53 1558  	   );
15:26:53 1559  	   EXCEPTION
15:26:53 1560  	     WHEN OTHERS THEN
15:26:53 1561  	       EXCEPTION_MESSAGE := SQLERRM;
15:26:53 1562  	       RAISE CAN_NOT_CREATE_NEW_LICENSE;
15:26:53 1563  	 END;
15:26:53 1564  
15:26:53 1565  	 -- Creating new license
15:26:53 1566  	 IF PROCS_COMMON_V19.NORMALIZE_DATE(var_current_license_end_date) >
15:26:53 1567  	    PROCS_COMMON_V19.NORMALIZE_DATE(in_effective_start_date) THEN
15:26:53 1568  	   BEGIN
15:26:53 1569  	     PROCS_LICENSE_V19.CREATE_LICENSE (
15:26:53 1570  	       in_status_id		   => GLOBAL_STATUSES_V19.LICENSE_ACTIVE,
15:26:53 1571  	       in_needs_entitlements	   => GLOBAL_CONSTANTS_V19.TRUE,
15:26:53 1572  	       in_start_date		   => in_effective_end_date,
15:26:53 1573  	       in_end_date		   => var_current_license_end_date + (in_effective_end_date - in_effective_start_date),
15:26:53 1574  	       in_offer_id		   => var_current_offer_id,
15:26:53 1575  	       in_subscription_id	   => in_subscription_id,
15:26:53 1576  	       in_invoice_id		   => var_current_invoice_id,
15:26:53 1577  	       in_created_by		   => in_updated_by,
15:26:53 1578  	       in_is_extension		   => GLOBAL_CONSTANTS_V19.FALSE,
15:26:53 1579  	       in_current_offer_index	   => var_current_offer_index,
15:26:53 1580  	       in_current_offer_recurr_num => var_current_offer_recurr_num,
15:26:53 1581  	       out_license_id		   => var_ext_license_id
15:26:53 1582  	     );
15:26:53 1583  	     EXCEPTION
15:26:53 1584  	       WHEN OTHERS THEN
15:26:53 1585  		 EXCEPTION_MESSAGE := SQLERRM;
15:26:53 1586  		 RAISE CAN_NOT_CREATE_END_LICENSE;
15:26:53 1587  	   END;
15:26:53 1588  	 END IF;
15:26:53 1589  
15:26:53 1590  	 -- Create new note for subscription
15:26:53 1591  	 BEGIN
15:26:53 1592  	   PROCS_SUBSCRIPTION_V19.ANNOTATE_SUBSCRIPTION (
15:26:53 1593  	     in_subscription_id => in_subscription_id,
15:26:53 1594  	     in_agent_id	=> 0, -- FIXME: What should to be here (agent id)?
15:26:53 1595  	     in_note		=> in_note,
15:26:53 1596  	     in_created_by	=> in_updated_by
15:26:53 1597  	   );
15:26:53 1598  	   EXCEPTION
15:26:53 1599  	     WHEN OTHERS THEN
15:26:53 1600  	       EXCEPTION_MESSAGE := SQLERRM;
15:26:53 1601  	       RAISE CAN_NOT_ANNOTATE_SUBSCRIPTION;
15:26:53 1602  	 END;
15:26:53 1603  
15:26:53 1604  EXCEPTION
15:26:53 1605  WHEN EXT_START_DATE_LATER_THEN_END THEN
15:26:53 1606  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
15:26:53 1607  	   SPROC_NAME, 'Extension start date is bigger then end date');
15:26:53 1608  WHEN EXTENS_START_DATE_IS_TOO_FAR THEN
15:26:53 1609  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
15:26:53 1610  	   SPROC_NAME, 'Extension start date is too far');
15:26:53 1611  WHEN EXTENS_START_DATE_IS_TOO_SMALL THEN
15:26:53 1612  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
15:26:53 1613  	   SPROC_NAME, 'Extension start date is too small');
15:26:53 1614  WHEN BAD_SUBSCRIPTION_ID THEN
15:26:53 1615  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:53 1616  	   SPROC_NAME, 'No such subscription');
15:26:53 1617  WHEN CAN_NOT_FIND_OFFER_OR_LICENSE THEN
15:26:53 1618  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:53 1619  	   SPROC_NAME, 'Could not find license and/or offer for given subscription');
15:26:53 1620  WHEN CAN_NOT_CHANGE_LICENSE_STATUS THEN
15:26:53 1621  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:53 1622  	   SPROC_NAME, 'Could not change license status', EXCEPTION_MESSAGE);
15:26:53 1623  WHEN CAN_NOT_CREATE_INVOICE THEN
15:26:53 1624  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:53 1625  	   SPROC_NAME, 'Could not create new invoice', EXCEPTION_MESSAGE);
15:26:53 1626  WHEN CAN_NOT_CREATE_NEW_LICENSE THEN
15:26:53 1627  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:53 1628  	   SPROC_NAME, 'Could not create new license', EXCEPTION_MESSAGE);
15:26:53 1629  WHEN CAN_NOT_CREATE_END_LICENSE THEN
15:26:53 1630  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:53 1631  	   SPROC_NAME, 'Could not create last license', EXCEPTION_MESSAGE);
15:26:53 1632  WHEN CAN_NOT_ANNOTATE_SUBSCRIPTION THEN
15:26:53 1633  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:53 1634  	   SPROC_NAME, 'Could not create new note for subscription', EXCEPTION_MESSAGE);
15:26:53 1635  WHEN OTHERS THEN
15:26:53 1636  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:53 1637  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:26:53 1638  END ADD_SUBSCRIPTION_EXTENSION;
15:26:53 1639  
15:26:53 1640  /******************************************************************************/
15:26:53 1641  
15:26:53 1642  FUNCTION CALC_SUBSCRIPTION_END_DATE (
15:26:53 1643  /*
15:26:53 1644  APP_EXCEPTION_CODES_V19.NOT_FOUND
15:26:53 1645  APP_EXCEPTION_CODES_V19.INTERNAL_ERROR
15:26:53 1646  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
15:26:53 1647  Returns:
15:26:53 1648  NULL if it is impossible to calculate end date (for example,
15:26:53 1649  	 offer chain includes offer with infinity recurrences number)
15:26:53 1650  DATE else
15:26:53 1651  */
15:26:53 1652  	 in_subscription_id IN NUMBER
15:26:53 1653  ) RETURN DATE AS
15:26:53 1654  -- VARIABLES
15:26:53 1655  SPROC_NAME		     CONSTANT VARCHAR2(26) := 'CALC_SUBSCRIPTION_END_DATE';
15:26:53 1656  last_license_id		     NUMBER;
15:26:53 1657  last_license_end_date	     DATE;
15:26:53 1658  last_license_offer_id	     NUMBER;
15:26:53 1659  last_license_offer_index      NUMBER;
15:26:53 1660  last_license_offer_recurr_num NUMBER;
15:26:53 1661  var_last_license_id	     NUMBER;
15:26:53 1662  var_offer_chain_id	     NUMBER;
15:26:53 1663  var_current_offer_rec_number  NUMBER;
15:26:53 1664  var_next_offers_set	     SYS_REFCURSOR;
15:26:53 1665  var_next_offer_duration	     VARCHAR2(30);
15:26:53 1666  var_next_offer_recur	     NUMBER;
15:26:53 1667  var_infinity_offers_number    NUMBER;
15:26:53 1668  
15:26:53 1669  var_result_date DATE;
15:26:53 1670  
15:26:53 1671  var_ym_interval	INTERVAL YEAR TO MONTH;
15:26:53 1672  var_ds_interval	INTERVAL DAY(3) TO SECOND;
15:26:53 1673  var_offer_years	NUMBER;
15:26:53 1674  var_offer_months NUMBER;
15:26:53 1675  var_offer_days	NUMBER;
15:26:53 1676  
15:26:53 1677  -- EXCEPTIONS
15:26:53 1678  BAD_SUBSCRIPTION_ID	  EXCEPTION;
15:26:53 1679  CAN_NOT_FIND_LAST_LICENSE  EXCEPTION;
15:26:53 1680  CAN_NOT_CALC_OFFER_LENGTH  EXCEPTION;
15:26:53 1681  CAN_NOT_CALC_OFFER_LENGTH2 EXCEPTION;
15:26:53 1682  EXCEPTION_MESSAGE	  VARCHAR2(1024);
15:26:53 1683  BEGIN
15:26:53 1684  
15:26:53 1685  	 BEGIN
15:26:53 1686  	   SELECT
15:26:53 1687  	     SUBSCRIPTION.OFFER_CHAIN_ID into var_offer_chain_id
15:26:53 1688  	   FROM
15:26:53 1689  	     SUBSCRIPTION
15:26:53 1690  	   WHERE
15:26:53 1691  	     SUBSCRIPTION.ID = in_subscription_id;
15:26:53 1692  	   EXCEPTION
15:26:53 1693  	     WHEN NO_DATA_FOUND THEN
15:26:53 1694  	       RAISE BAD_SUBSCRIPTION_ID;
15:26:53 1695  	 END;
15:26:53 1696  
15:26:53 1697  	 BEGIN
15:26:53 1698  	   SELECT
15:26:53 1699  	     ID into var_last_license_id
15:26:53 1700  	   FROM
15:26:53 1701  	     (
15:26:53 1702  	       SELECT
15:26:53 1703  		 LICENSE.ID
15:26:53 1704  	       FROM
15:26:53 1705  		 LICENSE
15:26:53 1706  	       WHERE
15:26:53 1707  		 LICENSE.SUBSCRIPTION_ID = in_subscription_id
15:26:53 1708  	       ORDER BY
15:26:53 1709  		 LICENSE.END_DATE DESC
15:26:53 1710  	     )
15:26:53 1711  	   WHERE
15:26:53 1712  	     ROWNUM <= 1;
15:26:53 1713  	   EXCEPTION
15:26:53 1714  	     WHEN NO_DATA_FOUND THEN
15:26:53 1715  	       RAISE CAN_NOT_FIND_LAST_LICENSE;
15:26:53 1716  	 END;
15:26:53 1717  
15:26:53 1718  	 SELECT
15:26:53 1719  	   COUNT(*) into var_infinity_offers_number
15:26:53 1720  	 FROM
15:26:53 1721  	   OFFER_OFFER_CHAIN
15:26:53 1722  	 WHERE
15:26:53 1723  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
15:26:53 1724  	   AND OFFER_OFFER_CHAIN.NUM_RECURRENCES = GLOBAL_ENUMS_V19.OFFER_REC_INFINITY;
15:26:53 1725  
15:26:53 1726  	 IF var_infinity_offers_number > 0 THEN
15:26:53 1727  	   RETURN NULL;
15:26:53 1728  	 END IF;
15:26:53 1729  
15:26:53 1730  	 BEGIN
15:26:53 1731  	   SELECT
15:26:53 1732  	     LICENSE.ID,
15:26:53 1733  	     LICENSE.END_DATE,
15:26:53 1734  	     LICENSE.CURRENT_OFFER_INDEX,
15:26:53 1735  	     LICENSE.CURRENT_OFFER_RECURR_NUM,
15:26:53 1736  	     LICENSE.OFFER_ID
15:26:53 1737  	     into
15:26:53 1738  	     last_license_id,
15:26:53 1739  	     last_license_end_date,
15:26:53 1740  	     last_license_offer_index,
15:26:53 1741  	     last_license_offer_recurr_num,
15:26:53 1742  	     last_license_offer_id
15:26:53 1743  	   FROM
15:26:53 1744  	     LICENSE
15:26:53 1745  	   WHERE
15:26:53 1746  	     LICENSE.ID = var_last_license_id;
15:26:53 1747  	   EXCEPTION
15:26:53 1748  	     WHEN NO_DATA_FOUND THEN
15:26:53 1749  	       RAISE CAN_NOT_FIND_LAST_LICENSE;
15:26:53 1750  	 END;
15:26:53 1751  
15:26:53 1752  	 var_result_date := last_license_end_date;
15:26:53 1753  
15:26:53 1754  	 -- Find current recurrence number
15:26:53 1755  	 SELECT
15:26:53 1756  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES into var_current_offer_rec_number
15:26:53 1757  	 FROM
15:26:53 1758  	   OFFER_OFFER_CHAIN
15:26:53 1759  	 WHERE
15:26:53 1760  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
15:26:53 1761  	   AND OFFER_OFFER_CHAIN.OFFER_ID = last_license_offer_id
15:26:53 1762  	   AND OFFER_OFFER_CHAIN.INDEX_VALUE = last_license_offer_index;
15:26:53 1763  
15:26:53 1764  	 IF var_current_offer_rec_number > last_license_offer_recurr_num THEN
15:26:53 1765  	   BEGIN
15:26:53 1766  	     PROCS_OFFER_CHAIN_V19.GET_OFFER_LENGTH(
15:26:53 1767  	       last_license_offer_id,
15:26:53 1768  	       var_offer_years,
15:26:53 1769  	       var_offer_months,
15:26:53 1770  	       var_offer_days
15:26:53 1771  	     );
15:26:53 1772  
15:26:53 1773  	     var_ym_interval := var_offer_years||'-'||var_offer_months;
15:26:53 1774  	     var_ds_interval := var_offer_days||' 0:0:0';
15:26:53 1775  
15:26:53 1776  	     var_result_date := var_result_date
15:26:53 1777  	       + ( var_ym_interval * ( var_current_offer_rec_number - last_license_offer_recurr_num ) )
15:26:53 1778  	       + ( var_ds_interval * ( var_current_offer_rec_number - last_license_offer_recurr_num ) );
15:26:53 1779  	     EXCEPTION
15:26:53 1780  	       WHEN OTHERS THEN
15:26:53 1781  		 EXCEPTION_MESSAGE := SQLERRM;
15:26:53 1782  		 RAISE CAN_NOT_CALC_OFFER_LENGTH;
15:26:53 1783  	   END;
15:26:53 1784  	 END IF;
15:26:53 1785  
15:26:53 1786  	 OPEN var_next_offers_set FOR
15:26:53 1787  	 SELECT
15:26:53 1788  	   OFFER.ENTITLEMENT_DURATION,
15:26:53 1789  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES
15:26:53 1790  	 FROM
15:26:53 1791  	   OFFER_OFFER_CHAIN
15:26:53 1792  	   INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
15:26:53 1793  	 WHERE
15:26:53 1794  	   OFFER_OFFER_CHAIN.INDEX_VALUE > last_license_offer_index
15:26:53 1795  	   AND OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id;
15:26:53 1796  
15:26:53 1797  	 LOOP
15:26:53 1798  	   FETCH var_next_offers_set into var_next_offer_duration, var_next_offer_recur;
15:26:53 1799  	   EXIT WHEN var_next_offers_set%NOTFOUND;
15:26:53 1800  	   BEGIN
15:26:53 1801  	     PROCS_COMMON_V19.ISO8601DURATION_TO_INTERVALS(
15:26:53 1802  	       var_next_offer_duration,
15:26:53 1803  	       var_offer_years,
15:26:53 1804  	       var_offer_months,
15:26:53 1805  	       var_offer_days
15:26:53 1806  	     );
15:26:53 1807  
15:26:53 1808  	     var_ym_interval := var_offer_years||'-'||var_offer_months;
15:26:53 1809  	     var_ds_interval := var_offer_days||' 0:0:0';
15:26:53 1810  
15:26:53 1811  	     var_result_date := var_result_date
15:26:53 1812  	       + ( var_ym_interval * var_next_offer_recur )
15:26:53 1813  	       + ( var_ds_interval * var_next_offer_recur );
15:26:53 1814  	     EXCEPTION
15:26:53 1815  	       WHEN OTHERS THEN
15:26:53 1816  		 EXCEPTION_MESSAGE := SQLERRM;
15:26:53 1817  		 RAISE CAN_NOT_CALC_OFFER_LENGTH2;
15:26:53 1818  	   END;
15:26:53 1819  	 END LOOP;
15:26:53 1820  
15:26:53 1821  	 RETURN var_result_date;
15:26:53 1822  
15:26:53 1823  EXCEPTION
15:26:53 1824  WHEN BAD_SUBSCRIPTION_ID THEN
15:26:53 1825  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:53 1826  	   SPROC_NAME, 'No such subscription');
15:26:53 1827  WHEN CAN_NOT_FIND_LAST_LICENSE THEN
15:26:53 1828  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:53 1829  	   SPROC_NAME, 'Could not find last license for given subscription');
15:26:53 1830  WHEN CAN_NOT_CALC_OFFER_LENGTH THEN
15:26:53 1831  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:53 1832  	   SPROC_NAME, 'Could not calculate offer length', EXCEPTION_MESSAGE);
15:26:53 1833  WHEN CAN_NOT_CALC_OFFER_LENGTH2 THEN
15:26:53 1834  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:53 1835  	   SPROC_NAME, 'Could not calculate last offer length', EXCEPTION_MESSAGE);
15:26:53 1836  WHEN OTHERS THEN
15:26:53 1837  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:53 1838  	   SPROC_NAME, 'Unkown error', SQLERRM);
15:26:53 1839  END CALC_SUBSCRIPTION_END_DATE;
