15:26:53 877  			AND c.charge_status_id = GLOBAL_STATUSES_V19.CHARGE_OPENED)
15:26:53 878  		AND rownum <= in_num_licenses_to_fetch * 10
15:26:53 879  		ORDER BY
15:26:53 880  		    dbms_random.value)
15:26:53 881  	WHERE
15:26:53 882  	    rownum <= in_num_licenses_to_fetch;
15:26:53 883  END GET_GRACE_LICE_FOR_FINAL_TRANS;
