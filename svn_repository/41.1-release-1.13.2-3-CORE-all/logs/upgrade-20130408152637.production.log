15:26:53 2288  
15:26:53 2289  PROCEDURE FINALIZE_CANCELATION (
15:26:53 2290  /*
15:26:53 2291  APP_EXCEPTION_CODES_V19.NOT_FOUND
15:26:53 2292  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
15:26:53 2293  APP_EXCEPTION_CODES_V19.INTERNAL_ERROR
15:26:53 2294  */
15:26:53 2295  --  in_subscription_id	 IN SUBSCRIPTION.ID%TYPE,
15:26:53 2296  --  in_cancelation_reason IN SUBSCRIPTION_CANCEL_REASON.VALUE%TYPE,
15:26:53 2297  --  in_cancelation_date	 IN SUBSCRIPTION.CANCELLATION_DATE%TYPE,
15:26:53 2298  --  in_note		 IN SUBSCRIPTION_NOTE.NOTE%TYPE,
15:26:53 2299  --  in_agent_id		 IN SUBSCRIPTION_NOTE.AGENT_ID%TYPE,
15:26:53 2300  --  in_updated_by	 IN SUBSCRIPTION.UPDATED_BY%TYPE
15:26:53 2301  	 in_subscription_id    IN NUMBER,
15:26:53 2302  	 in_cancelation_reason IN VARCHAR2,
15:26:53 2303  	 in_cancelation_date   IN DATE,
15:26:53 2304  	 in_note	       IN VARCHAR2,
15:26:53 2305  	 in_agent_id	       IN NUMBER,
15:26:53 2306  	 in_updated_by	       IN VARCHAR2
15:26:53 2307  ) AS
15:26:53 2308  SPROC_NAME CONSTANT VARCHAR2(20) := 'FINALIZE_CANCELATION';
15:26:53 2309  -- VARIABLES
15:26:53 2310  var_current_subscr_status SUBSCRIPTION.SUBSCRIPTION_STATUS_ID%TYPE;
15:26:53 2311  var_sct_id		 SUBSCRIPTION.SCT_ID%TYPE;
15:26:53 2312  var_active_invoices_count NUMBER;
15:26:53 2313  var_license_to_disgrace	 LICENSE.ID%TYPE;
15:26:53 2314  var_now			 DATE := SYSDATE;
15:26:53 2315  -- EXCEPTIONS
15:26:53 2316  BAD_SUBSCRIPTION_ID	   EXCEPTION;
15:26:53 2317  BAD_SUBSCRIPTION_STATUS	   EXCEPTION;
15:26:53 2318  BAD_CANCELATION_REASON	   EXCEPTION;
15:26:53 2319  CAN_NOT_UPDATE_SUBSCRIPTION EXCEPTION;
15:26:53 2320  ACTIVE_INVOICES_FOUND	   EXCEPTION;
15:26:53 2321  CAN_NOT_CREATE_NOTE	   EXCEPTION;
15:26:53 2322  EXCEPTION_MESSAGE	   VARCHAR2(1024);
15:26:53 2323  BEGIN
15:26:53 2324  
15:26:53 2325  	 -- Get current subscription status
15:26:53 2326  	 BEGIN
15:26:53 2327  	   SELECT
15:26:53 2328  	     SUBSCRIPTION.SUBSCRIPTION_STATUS_ID into var_current_subscr_status
15:26:53 2329  	   FROM
15:26:53 2330  	     SUBSCRIPTION
15:26:53 2331  	   WHERE
15:26:53 2332  	     SUBSCRIPTION.ID = in_subscription_id;
15:26:53 2333  	   EXCEPTION
15:26:53 2334  	     WHEN NO_DATA_FOUND THEN
15:26:53 2335  	       RAISE BAD_SUBSCRIPTION_ID;
15:26:53 2336  	 END;
15:26:53 2337  
15:26:53 2338  	 -- Check that subscription reason is correct
15:26:53 2339  	 BEGIN
15:26:53 2340  	   SELECT
15:26:53 2341  	     SUBSCRIPTION_CANCEL_REASON.ID into var_sct_id
15:26:53 2342  	   FROM
15:26:53 2343  	     SUBSCRIPTION_CANCEL_REASON
15:26:53 2344  	   WHERE
15:26:53 2345  	     SUBSCRIPTION_CANCEL_REASON.VALUE LIKE in_cancelation_reason
15:26:53 2346  	     AND ROWNUM <= 1;
15:26:53 2347  	   EXCEPTION
15:26:53 2348  	     WHEN NO_DATA_FOUND THEN
15:26:53 2349  	       RAISE BAD_CANCELATION_REASON;
15:26:53 2350  	 END;
15:26:53 2351  
15:26:53 2352  	 -- Check for invoices with active licenses
15:26:53 2353  	 SELECT
15:26:53 2354  	   COUNT(*) into var_active_invoices_count
15:26:53 2355  	 FROM
15:26:53 2356  	   LICENSE
15:26:53 2357  	   INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
15:26:53 2358  	 WHERE
15:26:53 2359  	   LICENSE.LICENSE_STATUS_ID in (GLOBAL_STATUSES_V19.LICENSE_ACTIVE, GLOBAL_STATUSES_V19.LICENSE_IN_GRACE_PERIOD)
15:26:53 2360  	   AND LICENSE.SUBSCRIPTION_ID = in_subscription_id;
15:26:53 2361  
15:26:53 2362  	 IF var_active_invoices_count > 0 THEN
15:26:53 2363  	   RAISE ACTIVE_INVOICES_FOUND;
15:26:53 2364  	 END IF;
15:26:53 2365  
15:26:53 2366  	 -- Check that subscription is active
15:26:53 2367  	 IF var_current_subscr_status != GLOBAL_STATUSES_V19.SUBSCRIPTION_ACTIVE AND
15:26:53 2368  	    var_current_subscr_status != GLOBAL_STATUSES_V19.SUBSCRIPTION_SUSPENDED AND
15:26:53 2369  	    var_current_subscr_status != GLOBAL_STATUSES_V19.SUBSCRIPTION_IN_GRACE_PERIOD THEN
15:26:53 2370  	   RAISE BAD_SUBSCRIPTION_STATUS;
15:26:53 2371  	 END IF;
15:26:53 2372  
15:26:53 2373  	 -- Update subscription data
15:26:53 2374  	 BEGIN
15:26:53 2375  	   PROCS_SUBSCRIPTION_CRU_V19.UPDATE_SUBSCRIPTION(
15:26:53 2376  	     in_subscription_id        => in_subscription_id,
15:26:53 2377  	     in_subscription_status_id => GLOBAL_STATUSES_V19.SUBSCRIPTION_CANCELED,
15:26:53 2378  	     in_cancelation_date       => in_cancelation_date,
15:26:53 2379  	     in_updated_by	       => in_updated_by,
15:26:53 2380  	     in_sct_id		       => var_sct_id
15:26:53 2381  	   );
15:26:53 2382  	   EXCEPTION
15:26:53 2383  	     WHEN OTHERS THEN
15:26:53 2384  	       EXCEPTION_MESSAGE := SQLERRM;
15:26:53 2385  	       RAISE CAN_NOT_UPDATE_SUBSCRIPTION;
15:26:53 2386  	 END;
15:26:53 2387  
15:26:53 2388  	 -- Terminate grace period for licenses in grace [SAR-31]
15:26:53 2389  	 BEGIN
15:26:53 2390  	   SELECT
15:26:53 2391  	     LICENSE.ID into var_license_to_disgrace
15:26:53 2392  	   FROM
15:26:53 2393  	     LICENSE
15:26:53 2394  	   WHERE
15:26:53 2395  	     LICENSE.SUBSCRIPTION_ID = in_subscription_id
15:26:53 2396  	     AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V19.LICENSE_IN_GRACE_PERIOD
15:26:53 2397  	     AND ROWNUM <= 1
15:26:53 2398  	   ORDER BY
15:26:53 2399  	     CREATE_DATE DESC;
15:26:53 2400  	 EXCEPTION
15:26:53 2401  	   WHEN NO_DATA_FOUND THEN
15:26:53 2402  	     var_license_to_disgrace := NULL;
15:26:53 2403  	 END;
15:26:53 2404  
15:26:53 2405  	 IF var_license_to_disgrace IS NOT NULL THEN
15:26:53 2406  	   PROCS_LICENSE_CRU_V19.UPDATE_LICENSE(
15:26:53 2407  	     in_license_id	     => var_license_to_disgrace,
15:26:53 2408  	     in_license_status_id    => GLOBAL_STATUSES_V19.LICENSE_CLOSED,
15:26:53 2409  	     in_updated_by	     => in_updated_by
15:26:53 2410  	   );
15:26:53 2411  	 END IF;
15:26:53 2412  
15:26:53 2413  	 -- Annotate subscription
15:26:53 2414  	 IF in_note IS NOT NULL THEN
15:26:53 2415  	   BEGIN
15:26:53 2416  	     PROCS_SUBSCRIPTION_V19.ANNOTATE_SUBSCRIPTION(
15:26:53 2417  	       in_subscription_id,
15:26:53 2418  	       in_agent_id,
15:26:53 2419  	       in_note,
15:26:53 2420  	       in_updated_by
15:26:53 2421  	     );
15:26:53 2422  	     EXCEPTION
15:26:53 2423  	      WHEN OTHERS THEN
15:26:53 2424  		EXCEPTION_MESSAGE := SQLERRM;
15:26:53 2425  		RAISE CAN_NOT_CREATE_NOTE;
15:26:53 2426  	   END;
15:26:53 2427  	 END IF;
15:26:53 2428  
15:26:53 2429  EXCEPTION
15:26:53 2430  WHEN BAD_SUBSCRIPTION_ID THEN
15:26:53 2431  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:53 2432  	   SPROC_NAME, 'No such subscription');
15:26:53 2433  WHEN BAD_SUBSCRIPTION_STATUS THEN
15:26:53 2434  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
15:26:53 2435  	   SPROC_NAME, 'Bad current subscription status');
15:26:53 2436  WHEN BAD_CANCELATION_REASON THEN
15:26:53 2437  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
15:26:53 2438  	   SPROC_NAME, 'Bad cancellation reason');
15:26:53 2439  WHEN CAN_NOT_UPDATE_SUBSCRIPTION THEN
15:26:53 2440  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:53 2441  	   SPROC_NAME, 'Could not update subscription data', EXCEPTION_MESSAGE);
15:26:53 2442  WHEN ACTIVE_INVOICES_FOUND THEN
15:26:53 2443  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
15:26:53 2444  	   SPROC_NAME, 'Invoices with active licenses found');
15:26:53 2445  WHEN CAN_NOT_CREATE_NOTE THEN
15:26:53 2446  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:53 2447  	   SPROC_NAME, 'Could not annotate subscription', EXCEPTION_MESSAGE);
15:26:53 2448  --WHEN OTHERS THEN
15:26:53 2449  --  PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:53 2450  --    SPROC_NAME, 'Unknown error', SQLERRM);
15:26:53 2451  END FINALIZE_CANCELATION;
