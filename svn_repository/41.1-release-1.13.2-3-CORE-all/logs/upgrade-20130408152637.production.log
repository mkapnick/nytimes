15:26:54 2331  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 2332  	   SPROC_NAME, 'No such charge');
15:26:54 2333  WHEN BAD_GIFT_CERTIFICATE_ID THEN
15:26:54 2334  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 2335  	   SPROC_NAME, 'No such gift certificate');
15:26:54 2336  WHEN CAN_NOT_FIND_SUBSCRIPTION THEN
15:26:54 2337  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 2338  	   SPROC_NAME, 'Could not find subscription for given charge');
15:26:54 2339  WHEN OTHERS THEN
15:26:54 2340  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:54 2341  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:26:54 2342  END IS_GCERT_FOR_PROPER_OFFER;
