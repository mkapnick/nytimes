15:26:53   6  	  function getDiscountAmount(in_line_item_id line_item.id%type)
15:26:53   7  	      return line_item.amount%type
15:26:53   8  	  is
15:26:53   9  	      v_discount  line_item.amount%type := 0;
15:26:53  10  	  begin
15:26:53  11  	      for x in (
15:26:53  12  		  select d.id discount_id
15:26:53  13  			,nvl(d.fixed_amount, d.percent_amount * (po.quantity * po.unit_price)) discount_amount
15:26:53  14  		  from discount d
15:26:53  15  		      join discount_line_item dli	      on dli.discount_id = d.id
15:26:53  16  		      join discount_product_offering dop      on dop.discount_id = d.id
15:26:53  17  		      join product_offering po		      on po.id = dop.product_offering_id
15:26:53  18  		      join line_item li 		      on li.id = dli.line_item_id and li.product_offer_id = po.id
15:26:53  19  		  where dli.line_item_id = in_line_item_id
15:26:53  20  	      )
15:26:53  21  	      loop
15:26:53  22  		  v_discount := v_discount + x.discount_amount;
15:26:53  23  	      end loop;
15:26:53  24  
15:26:53  25  	      return v_discount;
15:26:53  26  	  end;
