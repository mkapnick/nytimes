15:26:53 2211  	     END IF;
15:26:53 2212  	   end if;
15:26:53 2213  	 ELSE
15:26:53 2214  
15:26:53 2215  	   FOR f_transaction_to_close IN (
15:26:53 2216  	     SELECT DISTINCT CHARGE.TRANSACTION_ID AS "ID" FROM CHARGE WHERE CHARGE.INVOICE_ID = in_invoice_id and CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V19.CHARGE_OPENED
15:26:53 2217  	   )
15:26:53 2218  	   LOOP
15:26:53 2219  	     PROCS_TRANSACTION_V19.UPDATE_TRANSACTION_STATUS(
15:26:53 2220  	       in_transaction_id	=> f_transaction_to_close.ID,
15:26:53 2221  	       in_updated_by		=> in_updated_by,
15:26:53 2222  	       in_transaction_status_id => GLOBAL_STATUSES_V19.TRANSACTION_CLOSED
15:26:53 2223  	     );
15:26:53 2224  	   END LOOP;
15:26:53 2225  	   -- Needs to close charges. No refund.
15:26:53 2226  	   FOR f_charge_to_close IN (
15:26:53 2227  	     SELECT CHARGE.ID FROM CHARGE WHERE CHARGE.INVOICE_ID = in_invoice_id and CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V19.CHARGE_OPENED
15:26:53 2228  	   )
15:26:53 2229  	   LOOP
15:26:53 2230  	     PROCS_CHARGE_V19.UPDATE_CHARGE_STATUS(
15:26:53 2231  	       in_charge_id	   => f_charge_to_close.ID,
15:26:53 2232  	       in_updated_by	   => in_updated_by,
15:26:53 2233  	       in_charge_status_id => GLOBAL_STATUSES_V19.CHARGE_CANCELED
15:26:53 2234  	     );
15:26:53 2235  	   END LOOP;
15:26:53 2236  
15:26:53 2237  	   PROCS_INVOICE_V19.UPDATE_INVOICE_STATUS(
15:26:53 2238  	     in_invoice_id		    => in_invoice_id,
15:26:53 2239  	     in_updated_by		    => in_updated_by,
15:26:53 2240  	     in_invoice_status_id	    => GLOBAL_STATUSES_V19.INVOICE_CLOSED
15:26:53 2241  	   );
15:26:53 2242  
15:26:53 2243  	 END IF;
15:26:53 2244  	 -- update licenses
15:26:53 2245  	 IF(var_revoke = GLOBAL_CONSTANTS_V19.TRUE OR var_chargeback_amount > 0 OR (var_subscription_in_grace = GLOBAL_CONSTANTS_V19.FALSE AND var_billed = GLOBAL_CONSTANTS_V19.FALSE)) THEN
15:26:53 2246  	   FOR f_license_to_cancel IN (
15:26:53 2247  	     SELECT LICENSE.ID FROM LICENSE WHERE LICENSE.INVOICE_ID = in_invoice_id AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V19.LICENSE_ACTIVE
15:26:53 2248  	   )
15:26:53 2249  	   LOOP
