15:26:53 2956  	     OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V19.SUBSCRIPTION_IN_GRACE_PERIOD);
15:26:53 2957  
15:26:53 2958  EXCEPTION
15:26:53 2959  WHEN BAD_GROUP_ID THEN
15:26:53 2960  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:53 2961  	   SPROC_NAME, 'No such transaction id');
15:26:53 2962  WHEN BAD_OFFER_CHAIN_ID THEN
15:26:53 2963  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
