15:26:53 106  FUNCTION CREATE_NEW_TRANSFER_SET
15:26:53 107  	  (
15:26:53 108  	      in_set_maximum NUMBER)
15:26:53 109  	  RETURN core_owner.polling_sync.set_id%type
15:26:53 110  IS
15:26:53 111  	  pragma autonomous_transaction;
15:26:53 112  	  v_set_id core_owner.polling_sync.set_id%type;
15:26:53 113  BEGIN
15:26:53 114  	  SELECT
15:26:53 115  	      pollsync_setid_seq.nextval
15:26:53 116  	  INTO
15:26:53 117  	      v_set_id
15:26:53 118  	  FROM
15:26:53 119  	      dual;
15:26:53 120  	  update
15:26:53 121  	    core_owner.polling_sync ps
15:26:53 122  	  set
15:26:53 123  	    ps.set_id = v_set_id
15:26:53 124  	  where
15:26:53 125  	    ps.set_id IS NULL and
15:26:53 126  	    rownum <= in_set_maximum
15:26:53 127  	  ;
15:26:53 128  	  COMMIT;
15:26:53 129  	  RETURN v_set_id;
15:26:53 130  END;
15:26:53 131  --------------------------------------------------------------------------------
15:26:53 132  FUNCTION RETREIVE_TRANSFER_SET
15:26:53 133  	  (
15:26:53 134  	      in_set_id core_owner.polling_sync.set_id%type)
15:26:53 135  	  RETURN sys_refcursor
15:26:53 136  IS
15:26:53 137  	  v_refcursor sys_refcursor;
15:26:53 138  BEGIN
15:26:53 139  	  UPDATE
15:26:53 140  	      core_owner.polling_sync ps
15:26:53 141  	  SET
15:26:53 142  	      ps.last_send_date = sysdate,
15:26:53 143  	      ps.num_calls = ps.num_calls + 1
15:26:53 144  	  WHERE
15:26:53 145  	      ps.set_id = in_set_id ;
15:26:53 146  	  OPEN v_refcursor FOR
15:26:53 147  	  SELECT
15:26:53 148  	      ps.set_id,
15:26:53 149  	      ps.group_id,
15:26:53 150  	      ps.event_type,
15:26:53 151  	      ps.event_date
15:26:53 152  	  FROM
15:26:53 153  	      core_owner.polling_sync ps
15:26:53 154  	  WHERE
15:26:53 155  	      ps.set_id = in_set_id
15:26:53 156  	  AND ps.group_id IS NOT NULL ;
15:26:53 157  	  RETURN v_refcursor;
15:26:53 158  END;
15:26:53 159  --------------------------------------------------------------------------------
15:26:53 160  PROCEDURE GET_TRANSFER_SET
15:26:53 161  	  (
15:26:53 162  	      in_set_maximum NUMBER,
15:26:53 163  	      in_max_retries NUMBER,
15:26:53 164  	      out_refcursor OUT sys_refcursor)
15:26:53 165  IS
15:26:53 166  	  v_set_id core_owner.polling_sync.set_id%type;
15:26:53 167  BEGIN
15:26:53 168  	  /* Look for previously sent but unconfirmed sets and
15:26:53 169  	  send again until max_retries calls */
15:26:53 170  	  FOR x IN
15:26:53 171  	  (
15:26:53 172  	      SELECT
15:26:53 173  		  ps.set_id,
15:26:53 174  		  COUNT( *) set_size
15:26:53 175  	      FROM
15:26:53 176  		  core_owner.polling_sync ps
15:26:53 177  	      WHERE
15:26:53 178  		  ps.event_date > sysdate - 14
15:26:53 179  	      AND ps.confirm_date IS NULL
15:26:53 180  	      AND ps.last_send_date IS NOT NULL
15:26:53 181  	      AND ps.num_calls < in_max_retries
15:26:53 182  	      GROUP BY
15:26:53 183  		  ps.set_id
15:26:53 184  	      ORDER BY
15:26:53 185  		  ps.set_id
15:26:53 186  	  )
15:26:53 187  	  LOOP
15:26:53 188  	      out_refcursor := Retreive_Transfer_Set(x.set_id) ;
15:26:53 189  	      RETURN;
15:26:53 190  	  END LOOP;
15:26:53 191  	  v_set_id := Create_New_Transfer_Set(in_set_maximum) ;
15:26:53 192  	  out_refcursor := Retreive_Transfer_Set(v_set_id) ;
15:26:53 193  	  RETURN;
15:26:53 194  END;
15:26:53 195  --------------------------------------------------------------------------------
15:26:53 196  PROCEDURE CONFIRM_TRANSFER_SET
15:26:53 197  	  (
15:26:53 198  	      in_set_id core_owner.polling_sync.set_id%type)
15:26:53 199  IS
15:26:53 200  	  v_unconfirmable EXCEPTION;
15:26:53 201  BEGIN
15:26:53 202  	  UPDATE
15:26:53 203  	      core_owner.polling_sync ps
15:26:53 204  	  SET
15:26:53 205  	      ps.confirm_date = systimestamp
15:26:53 206  	  WHERE
15:26:53 207  	      ps.set_id = in_set_id
15:26:53 208  	  AND ps.confirm_date IS NULL ;
15:26:53 209  	  IF(sql%rowcount < 1) THEN
15:26:53 210  	      raise v_unconfirmable;
15:26:53 211  	  END IF;
15:26:53 212  END;
15:26:53 213  PROCEDURE SET_LAST_RUN(ts in timestamp)
