15:26:53 507  	  SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
15:26:53 508  WHEN CAN_NOT_CREATE_CHARGE THEN
15:26:53 509  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:53 510  	  SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
15:26:53 511  WHEN OTHERS THEN
15:26:53 512  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:53 513  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:26:53 514  END FINALIZE_SUBSCRIPTION_CREATION;
