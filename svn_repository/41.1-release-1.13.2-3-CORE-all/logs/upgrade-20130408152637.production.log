15:26:54 368  	  ACCOUNT
15:26:54 369  	WHERE
15:26:54 370  	  ACCOUNT.ID = var_account_id;
15:26:54 371  
15:26:54 372  	-- Card should to be active
15:26:54 373  	IF var_credit_card_status != GLOBAL_STATUSES_V19.CREDIT_CARD_ACTIVE THEN
15:26:54 374  	  RAISE BAD_CC_STATUS;
15:26:54 375  	END IF;
15:26:54 376  
15:26:54 377  	-- Looking for pending transactions associated with given credit card
15:26:54 378  	SELECT
15:26:54 379  	  COUNT(*) into var_pending_invoices_num
15:26:54 380  	FROM
15:26:54 381  	  CHARGE
15:26:54 382  	  INNER JOIN TRANSACTION ON CHARGE.TRANSACTION_ID = TRANSACTION.ID
15:26:54 383  	WHERE
15:26:54 384  	  CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V19.INSTRUMENT_CREDIT_CARD
15:26:54 385  	  AND CHARGE.INSTRUMENT_ID = in_credit_card_id
15:26:54 386  	  AND TRANSACTION.TRANSACTION_STATUS_ID = GLOBAL_STATUSES_V19.TRANSACTION_PENDING;
15:26:54 387  
15:26:54 388  	IF var_pending_invoices_num > 0 THEN
15:26:54 389  	  RAISE PENDING_TRANSACTIONS_FOUNDED;
15:26:54 390  	END IF;
15:26:54 391  
15:26:54 392  	-- Getting current default financial instrument
15:26:54 393  	BEGIN
15:26:54 394  	  GET_DEF_FINANCIAL_INSTRUMENT(
15:26:54 395  	    in_group_id 	   => var_group_id,
15:26:54 396  	    out_instrument_type_id => current_def_instrument_type_id,
15:26:54 397  	    out_instrument_id	   => current_def_instrument_id
15:26:54 398  	  );
15:26:54 399  	  EXCEPTION
15:26:54 400  	    WHEN OTHERS THEN
15:26:54 401  	      EXCEPTION_MESSAGE := SQLERRM;
15:26:54 402  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
15:26:54 403  	END;
15:26:54 404  
15:26:54 405  	-- Checking that credit card is not default
15:26:54 406  	IF current_def_instrument_type_id = GLOBAL_ENUMS_V19.INSTRUMENT_CREDIT_CARD
15:26:54 407  	  AND current_def_instrument_id = in_credit_card_id THEN
15:26:54 408  	  BEGIN
15:26:54 409  	    DEL_DEF_FINANCIAL_INSTRUMENT(
15:26:54 410  	      in_group_id => var_group_id
15:26:54 411  	    );
15:26:54 412  	  EXCEPTION
15:26:54 413  	    WHEN OTHERS THEN
15:26:54 414  	      EXCEPTION_MESSAGE := SQLERRM;
15:26:54 415  	      RAISE CAN_NOT_DEL_DEF_FINANCIAL;
15:26:54 416  	  END;
15:26:54 417  	END IF;
15:26:54 418  
15:26:54 419  	-- Update credit card status
15:26:54 420  	PROCS_FIN_INSTRUMENTS_V19.UPDATE_CREDIT_CARD_STATUS(
15:26:54 421  	  in_credit_card_id	   => in_credit_card_id,
15:26:54 422  	  in_updated_by 	   => in_updated_by,
15:26:54 423  	  in_credit_card_status_id => GLOBAL_STATUSES_V19.CREDIT_CARD_DISABLED
15:26:54 424  	);
15:26:54 425  
15:26:54 426  EXCEPTION
15:26:54 427  WHEN NO_DATA_FOUND THEN
15:26:54 428  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 429  	  SPROC_NAME, 'No such credit card');
15:26:54 430  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
15:26:54 431  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 432  	  SPROC_NAME, 'Could not get current default financial instrument', EXCEPTION_MESSAGE);
15:26:54 433  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
15:26:54 434  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 435  	  SPROC_NAME, 'Could not delete information about default financial instrument from account', EXCEPTION_MESSAGE);
15:26:54 436  WHEN BAD_CC_STATUS THEN
15:26:54 437  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
15:26:54 438  	  SPROC_NAME, 'Credit card is not active');
15:26:54 439  WHEN PENDING_TRANSACTIONS_FOUNDED THEN
15:26:54 440  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
15:26:54 441  	  SPROC_NAME, 'Account has pending charge which is using this card');
15:26:54 442  WHEN CAN_NOT_DISABLE_CREDIT_CARD THEN
15:26:54 443  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 444  	  SPROC_NAME, 'Could not disable this credit card', EXCEPTION_MESSAGE);
15:26:54 445  WHEN OTHERS THEN
15:26:54 446  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:54 447  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:26:54 448  END DISABLE_CREDIT_CARD;
