15:26:54 916  /******************************************************************************/
15:26:54 917  
15:26:54 918  PROCEDURE FINALIZE_GC_PURCHASING (
15:26:54 919  	in_invoice_id	      IN NUMBER,
15:26:54 920  	in_created_by	      IN VARCHAR2,
15:26:54 921  	in_instrument_id      IN NUMBER,
15:26:54 922  	in_instrument_type_id IN NUMBER,
15:26:54 923  	in_order_id	      IN VARCHAR2,
15:26:54 924  	in_transaction_id     IN NUMBER,
15:26:54 925  	out_charge_amount     OUT NUMBER
15:26:54 926  ) AS
15:26:54 927  SPROC_NAME CONSTANT VARCHAR2(22) := 'FINALIZE_GC_PURCHASING';
15:26:54 928  -- VARIABLES
15:26:54 929  temp_transaction_id_count NUMBER;
15:26:54 930  var_invoice_amount	NUMBER(10,2);
15:26:54 931  var_transaction_id	NUMBER;
15:26:54 932  var_new_charge_id 	NUMBER;
15:26:54 933  -- EXCEPTIONS
15:26:54 934  BAD_CREDIT_CARD_ID	  EXCEPTION;
15:26:54 935  BAD_PAYPAL_ID		  EXCEPTION;
15:26:54 936  BAD_INSTRUMENT_TYPE	  EXCEPTION;
15:26:54 937  TRANSACTION_EXISTS	  EXCEPTION;
15:26:54 938  CAN_NOT_CALC_INVOICE_AMOUNT EXCEPTION;
15:26:54 939  CAN_NOT_USE_FCINSTR	  EXCEPTION;
15:26:54 940  CAN_NOT_CREATE_TRANSACTION  EXCEPTION;
15:26:54 941  CAN_NOT_CREATE_CHARGE	  EXCEPTION;
15:26:54 942  EXCEPTION_MESSAGE   VARCHAR2(1024);
15:26:54 943  BEGIN
15:26:54 944  
15:26:54 945  	-- Check that instrument exists
15:26:54 946  	IF in_instrument_type_id = GLOBAL_ENUMS_V19.INSTRUMENT_CREDIT_CARD THEN
15:26:54 947  	  IF IS_CREDIT_CARD_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V19.FALSE THEN
15:26:54 948  	    RAISE BAD_CREDIT_CARD_ID;
15:26:54 949  	  END IF;
15:26:54 950  	ELSIF in_instrument_type_id = GLOBAL_ENUMS_V19.INSTRUMENT_PAYPAL THEN
15:26:54 951  	  IF IS_PAYPAL_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V19.FALSE THEN
15:26:54 952  	    RAISE BAD_PAYPAL_ID;
15:26:54 953  	  END IF;
15:26:54 954  	ELSIF in_instrument_type_id = GLOBAL_ENUMS_V19.INSTRUMENT_ZCI_INSTRUMENT THEN
15:26:54 955  	  NULL;
15:26:54 956  	ELSE
15:26:54 957  	  RAISE BAD_INSTRUMENT_TYPE;
15:26:54 958  	END IF;
15:26:54 959  
15:26:54 960  	-- Check that transaction with given id do not exists
15:26:54 961  	SELECT
15:26:54 962  	  COUNT(*) into temp_transaction_id_count
15:26:54 963  	FROM
15:26:54 964  	  TRANSACTION
15:26:54 965  	WHERE
15:26:54 966  	  TRANSACTION.ID = in_transaction_id;
15:26:54 967  
15:26:54 968  	IF temp_transaction_id_count > 0 THEN
15:26:54 969  	  RAISE TRANSACTION_EXISTS;
15:26:54 970  	END IF;
15:26:54 971  
15:26:54 972  	-- Calculate new invoice amount
15:26:54 973  	BEGIN
15:26:54 974  	  PROCS_INVOICE_V19.CALCULATE_INVOICE_AMOUNT(in_invoice_id, var_invoice_amount);
15:26:54 975  	  EXCEPTION
15:26:54 976  	    WHEN OTHERS THEN
15:26:54 977  	      EXCEPTION_MESSAGE := SQLERRM;
15:26:54 978  	      RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
15:26:54 979  	END;
15:26:54 980  
15:26:54 981  	IF var_invoice_amount > 0
15:26:54 982  	  AND in_instrument_type_id = GLOBAL_ENUMS_V19.INSTRUMENT_ZCI_INSTRUMENT THEN
15:26:54 983  	  RAISE CAN_NOT_USE_FCINSTR;
15:26:54 984  	END IF;
15:26:54 985  
15:26:54 986  	-- If invoice amount iz 0 then we need to set status for this invoice to PROCCESSED
15:26:54 987  	IF var_invoice_amount = 0 THEN
15:26:54 988  	  PROCS_INVOICE_CRU_V19.UPDATE_INVOICE(
15:26:54 989  	    in_invoice_id		   => in_invoice_id,
15:26:54 990  	    in_updated_by		   => in_created_by,
15:26:54 991  	    in_invoice_status_id	   => GLOBAL_STATUSES_V19.INVOICE_CLOSED
15:26:54 992  	  );
15:26:54 993  	END IF;
15:26:54 994  
15:26:54 995  	IF var_invoice_amount > 0 THEN
15:26:54 996  	  -- Create transaction
15:26:54 997  	  BEGIN
15:26:54 998  	    PROCS_TRANSACTION_V19.CREATE_TRANSACTION(
15:26:54 999  	      in_transaction_id        => in_transaction_id,
15:26:54 1000  	       in_status_id		=> GLOBAL_STATUSES_V19.TRANSACTION_PENDING,
15:26:54 1001  	       in_amount		=> var_invoice_amount,
15:26:54 1002  	       in_created_by		=> in_created_by,
15:26:54 1003  	       in_order_id		=> in_order_id,
15:26:54 1004  	       in_transaction_type_code => 'GIFT_CERTIFICATE_PURCHASE',
15:26:54 1005  	       out_transaction_id	=> var_transaction_id
15:26:54 1006  	     );
15:26:54 1007  	     EXCEPTION
15:26:54 1008  	       WHEN OTHERS THEN
15:26:54 1009  		 EXCEPTION_MESSAGE := SQLERRM;
15:26:54 1010  		 RAISE CAN_NOT_CREATE_TRANSACTION;
15:26:54 1011  	   END;
15:26:54 1012  
15:26:54 1013  	   -- Create charge
15:26:54 1014  	   BEGIN
15:26:54 1015  	     PROCS_CHARGE_V19.CREATE_CHARGE(
15:26:54 1016  	       in_invoice_id	     => in_invoice_id,
15:26:54 1017  	       in_transaction_id     => var_transaction_id,
15:26:54 1018  	       in_instrument_type_id => in_instrument_type_id,
15:26:54 1019  	       in_instrument_id      => in_instrument_id,
15:26:54 1020  	       in_charge_amount      => var_invoice_amount,
15:26:54 1021  	       in_created_by	     => in_created_by,
15:26:54 1022  	       in_charge_status_id   => GLOBAL_STATUSES_V19.CHARGE_OPENED,
15:26:54 1023  	       out_charge_id	     => var_new_charge_id
15:26:54 1024  	     );
15:26:54 1025  	     out_charge_amount := var_invoice_amount;
15:26:54 1026  	     EXCEPTION
15:26:54 1027  	       WHEN OTHERS THEN
15:26:54 1028  		 EXCEPTION_MESSAGE := SQLERRM;
15:26:54 1029  		 RAISE CAN_NOT_CREATE_CHARGE;
15:26:54 1030  	   END;
15:26:54 1031  	 ELSE
15:26:54 1032  	   out_charge_amount := 0;
15:26:54 1033  	 END IF;
15:26:54 1034  
15:26:54 1035  EXCEPTION
15:26:54 1036  WHEN CAN_NOT_USE_FCINSTR THEN
15:26:54 1037  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.STATE_ERROR,
15:26:54 1038  	   SPROC_NAME, 'Could not use "free charge instrument" for non-zero invoice');
15:26:54 1039  WHEN BAD_CREDIT_CARD_ID THEN
15:26:54 1040  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 1041  	   SPROC_NAME, 'Bad credit card id');
15:26:54 1042  WHEN BAD_PAYPAL_ID THEN
15:26:54 1043  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 1044  	   SPROC_NAME, 'Bad paypal id');
15:26:54 1045  WHEN BAD_INSTRUMENT_TYPE THEN
15:26:54 1046  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INVALID_PARAMETER,
15:26:54 1047  	   SPROC_NAME, 'Bad instrument type');
15:26:54 1048  WHEN TRANSACTION_EXISTS THEN
15:26:54 1049  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.DUPLICATE_ERROR,
15:26:54 1050  	   SPROC_NAME, 'Transaction with given id already exists');
15:26:54 1051  WHEN CAN_NOT_CREATE_TRANSACTION THEN
15:26:54 1052  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 1053  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
15:26:54 1054  WHEN CAN_NOT_CREATE_CHARGE THEN
15:26:54 1055  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 1056  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
15:26:54 1057  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
15:26:54 1058  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 1059  	   SPROC_NAME, 'Could not calculate amount for new invoice', EXCEPTION_MESSAGE);
15:26:54 1060  WHEN OTHERS THEN
15:26:54 1061  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:54 1062  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:26:54 1063  END FINALIZE_GC_PURCHASING;
