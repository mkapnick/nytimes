15:26:54 369  	-- get all line items for given invoice
15:26:54 370  	OPEN out_line_items_set FOR
15:26:54 371  	SELECT
15:26:54 372  	  LINE_ITEM.AMOUNT,
15:26:54 373  	  LINE_ITEM.ID,
15:26:54 374  	  LINE_ITEM.CREATED_BY,
15:26:54 375  	  LINE_ITEM.CREATE_DATE,
15:26:54 376  	  LINE_ITEM.DISCOUNT_AMOUNT,
15:26:54 377  	  LINE_ITEM.TAXES_AMOUNT,
15:26:54 378  	  LINE_ITEM.PRODUCT_OFFER_ID,
15:26:54 379  	  LINE_ITEM.INVOICE_ID
15:26:54 380  	FROM
15:26:54 381  	  LINE_ITEM
15:26:54 382  	WHERE
