15:26:53 439  /******************************************************************************/
15:26:53 440  
15:26:53 441  FUNCTION CALCULATE_OFFER_CHAIN_END_DATE (
15:26:53 442  /*
15:26:53 443  Throws exceptions (codes):
15:26:53 444  APP_EXCEPTION_CODES_V19.NOT_FOUND
15:26:53 445  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
15:26:53 446  */
15:26:53 447  	in_offer_chain_id	  IN NUMBER,
15:26:53 448  	in_offer_chain_start_date IN DATE
15:26:53 449  ) RETURN DATE AS
15:26:53 450  -- VARIABLES
15:26:53 451  SPROC_NAME	     CONSTANT VARCHAR2(30) := 'CALCULATE_OFFER_CHAIN_END_DATE';
15:26:53 452  temp_offer_chain_id    NUMBER;
15:26:53 453  var_offer_chain_length NUMBER;
15:26:53 454  var_offer_duration     VARCHAR2(30);
15:26:53 455  var_offer_recurrences  NUMBER;
15:26:53 456  var_end_date	     DATE;
15:26:53 457  
15:26:53 458  var_offer_ym_interval INTERVAL YEAR TO MONTH;
15:26:53 459  var_offer_ds_interval INTERVAL DAY(3) TO SECOND;
15:26:53 460  var_offer_years	    NUMBER;
15:26:53 461  var_offer_months	    NUMBER;
15:26:53 462  var_offer_days	    NUMBER;
15:26:53 463  var_infinity_offers_count NUMBER;
15:26:53 464  
15:26:53 465  var_offers_set SYS_REFCURSOR;
15:26:53 466  
15:26:53 467  -- EXCEPTIONS
15:26:53 468  BAD_OFFER_CHAIN_ID EXCEPTION;
15:26:53 469  BEGIN
15:26:53 470  
15:26:53 471  	var_end_date := in_offer_chain_start_date;
15:26:53 472  
15:26:53 473  	-- Check that offer chain exists
15:26:53 474  	BEGIN
15:26:53 475  	  SELECT
15:26:53 476  	    OFFER_CHAIN.ID into temp_offer_chain_id
15:26:53 477  	  FROM
15:26:53 478  	    OFFER_CHAIN
15:26:53 479  	  WHERE
15:26:53 480  	    OFFER_CHAIN.ID = in_offer_chain_id;
15:26:53 481  	  EXCEPTION
15:26:53 482  	    WHEN NO_DATA_FOUND
15:26:53 483  	      THEN RAISE BAD_OFFER_CHAIN_ID;
15:26:53 484  	END;
15:26:53 485  
15:26:53 486  	SELECT
15:26:53 487  	  COUNT(*) into var_infinity_offers_count
15:26:53 488  	FROM
15:26:53 489  	  OFFER_OFFER_CHAIN
15:26:53 490  	WHERE
15:26:53 491  	  OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
15:26:53 492  	  AND OFFER_OFFER_CHAIN.NUM_RECURRENCES = GLOBAL_ENUMS_V19.OFFER_REC_INFINITY;
15:26:53 493  
15:26:53 494  	IF var_infinity_offers_count > 0 THEN
15:26:53 495  	  -- Offer chain contains offers with infinity num of recurrences
15:26:53 496  	  RETURN NULL;
15:26:53 497  	END IF;
15:26:53 498  
15:26:53 499  	-- Select offers durations
15:26:53 500  	OPEN var_offers_set FOR
15:26:53 501  	SELECT
15:26:53 502  	  OFFER.ENTITLEMENT_DURATION,
15:26:53 503  	  OFFER_OFFER_CHAIN.NUM_RECURRENCES
15:26:53 504  	FROM
15:26:53 505  	  OFFER_OFFER_CHAIN
15:26:53 506  	  INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
15:26:53 507  	WHERE
15:26:53 508  	  OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id;
15:26:53 509  
15:26:53 510  	-- Calculate sum of offers durations
15:26:53 511  	LOOP
15:26:53 512  	  FETCH var_offers_set INTO var_offer_duration, var_offer_recurrences;
15:26:53 513  	  EXIT WHEN var_offers_set%NOTFOUND;
15:26:53 514  	  PROCS_COMMON_V19.ISO8601DURATION_TO_INTERVALS(var_offer_duration, var_offer_years, var_offer_months, var_offer_days);
15:26:53 515  	  var_offer_ym_interval := var_offer_years||'-'||var_offer_months;
15:26:53 516  	  var_offer_ds_interval := var_offer_days||' 0:0:0';
15:26:53 517  	  var_end_date := var_end_date + ( var_offer_ym_interval * ( var_offer_recurrences + 1) ) + ( var_offer_ds_interval * ( var_offer_recurrences + 1) );
15:26:53 518  	END LOOP;
15:26:53 519  
15:26:53 520  	RETURN var_end_date;
15:26:53 521  
15:26:53 522  EXCEPTION
15:26:53 523  WHEN BAD_OFFER_CHAIN_ID THEN
15:26:53 524  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:53 525  	  SPROC_NAME, 'No such offer chain');
15:26:53 526  WHEN OTHERS THEN
15:26:53 527  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:53 528  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:26:53 529  END CALCULATE_OFFER_CHAIN_END_DATE;
15:26:53 530  
15:26:53 531  /******************************************************************************/
15:26:53 532  
15:26:53 533  FUNCTION CALCULATE_OFFER_AMOUNT (
15:26:53 534  	in_offer_id IN NUMBER
15:26:53 535  ) RETURN NUMBER AS
15:26:53 536  -- VARIABLES
15:26:53 537  SPROC_NAME    CONSTANT VARCHAR2(22) := 'CALCULATE_OFFER_AMOUNT';
15:26:53 538  temp_offer_id NUMBER;
15:26:53 539  
15:26:53 540  var_product_offering_set	    SYS_REFCURSOR;
15:26:53 541  var_product_offering_id	    NUMBER;
15:26:53 542  var_product_offering_price    NUMBER(10,6);
15:26:53 543  var_product_offering_t_amount NUMBER(10,6);
15:26:53 544  var_product_offering_quantity NUMBER;
15:26:53 545  
15:26:53 546  var_total_amount NUMBER(10,6);
15:26:53 547  var_final_amount NUMBER(10,2);
15:26:53 548  
15:26:53 549  var_percent_discount NUMBER(10,2);
15:26:53 550  var_fixed_discount NUMBER(10,6);
15:26:53 551  
15:26:53 552  -- EXCEPTIONS
15:26:53 553  BAD_OFFER_ID EXCEPTION;
15:26:53 554  BEGIN
15:26:53 555  
15:26:53 556  	BEGIN
15:26:53 557  	  SELECT
15:26:53 558  	    OFFER.ID into temp_offer_id
15:26:53 559  	  FROM
15:26:53 560  	    OFFER
15:26:53 561  	  WHERE
15:26:53 562  	    OFFER.ID = in_offer_id;
15:26:53 563  	  EXCEPTION
15:26:53 564  	    WHEN NO_DATA_FOUND THEN
15:26:53 565  	      RAISE BAD_OFFER_ID;
15:26:53 566  	END;
15:26:53 567  
15:26:53 568  	OPEN var_product_offering_set FOR
15:26:53 569  	SELECT
15:26:53 570  	  OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID,
15:26:53 571  	  PRODUCT_OFFERING.UNIT_PRICE,
15:26:53 572  	  PRODUCT_OFFERING.QUANTITY
15:26:53 573  	FROM
15:26:53 574  	  OFFER_PRODUCT_OFFERING
15:26:53 575  	  INNER JOIN PRODUCT_OFFERING ON OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = PRODUCT_OFFERING.ID
15:26:53 576  	WHERE
15:26:53 577  	  OFFER_PRODUCT_OFFERING.OFFER_ID = in_offer_id;
15:26:53 578  
15:26:53 579  	var_total_amount := 0;
15:26:53 580  
15:26:53 581  	LOOP
15:26:53 582  	  FETCH var_product_offering_set into
15:26:53 583  	    var_product_offering_id,
15:26:53 584  	    var_product_offering_price,
15:26:53 585  	    var_product_offering_quantity;
15:26:53 586  	  EXIT WHEN var_product_offering_set%NOTFOUND;
15:26:53 587  
15:26:53 588  	  SELECT
15:26:53 589  	    SUM(DISCOUNT.FIXED_AMOUNT) into var_fixed_discount
15:26:53 590  	  FROM
15:26:53 591  	    DISCOUNT_PRODUCT_OFFERING
15:26:53 592  	    INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
15:26:53 593  	  WHERE
15:26:53 594  	    DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
15:26:53 595  	    AND DISCOUNT.FIXED_AMOUNT IS NOT NULL;
15:26:53 596  
15:26:53 597  	  SELECT
15:26:53 598  	    SUM(DISCOUNT.PERCENT_AMOUNT) into var_percent_discount
15:26:53 599  	  FROM
15:26:53 600  	    DISCOUNT_PRODUCT_OFFERING
15:26:53 601  	    INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
15:26:53 602  	  WHERE
15:26:53 603  	    DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
15:26:53 604  	    AND DISCOUNT.PERCENT_AMOUNT IS NOT NULL;
15:26:53 605  
15:26:53 606  	  var_product_offering_t_amount := var_product_offering_price * var_product_offering_quantity;
15:26:53 607  
15:26:53 608  	  IF var_percent_discount IS NOT NULL THEN
15:26:53 609  	    var_product_offering_t_amount := var_product_offering_t_amount - ( var_product_offering_t_amount * var_percent_discount / 100 );
15:26:53 610  	  END IF;
15:26:53 611  
15:26:53 612  	  IF var_fixed_discount IS NOT NULL THEN
15:26:53 613  	    var_product_offering_t_amount := var_product_offering_t_amount - var_fixed_discount;
15:26:53 614  	  END IF;
15:26:53 615  
15:26:53 616  	  var_total_amount := var_total_amount + var_product_offering_t_amount;
15:26:53 617  	END LOOP;
15:26:53 618  	var_final_amount := var_total_amount;
15:26:53 619  	RETURN var_final_amount;
15:26:53 620  
15:26:53 621  EXCEPTION
15:26:53 622  WHEN BAD_OFFER_ID THEN
15:26:53 623  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:53 624  	  SPROC_NAME, 'No such offer');
15:26:53 625  WHEN OTHERS THEN
15:26:53 626  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:53 627  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:26:53 628  END CALCULATE_OFFER_AMOUNT;
15:26:53 629  
15:26:53 630  /******************************************************************************/
15:26:53 631  
15:26:53 632  FUNCTION CALCULATE_OFFER_CHAIN_AMOUNT (
15:26:53 633  	in_offer_chain_id IN NUMBER
15:26:53 634  ) RETURN NUMBER AS
15:26:53 635  -- VARIABLES
15:26:53 636  SPROC_NAME	     CONSTANT VARCHAR2(28) := 'CALCULATE_OFFER_CHAIN_AMOUNT';
15:26:53 637  temp_offer_chain_id    NUMBER;
15:26:53 638  var_first_offer_id     NUMBER;
15:26:53 639  -- EXCEPTIONS
15:26:53 640  BAD_OFFER_CHAIN_ID      EXCEPTION;
15:26:53 641  CAN_NOT_GET_FIRST_OFFER EXCEPTION;
15:26:53 642  EXCEPTION_MESSAGE       VARCHAR2(1024);
15:26:53 643  BEGIN
15:26:53 644  
15:26:53 645  	BEGIN
15:26:53 646  	  SELECT
15:26:53 647  	    OFFER_CHAIN.ID into temp_offer_chain_id
