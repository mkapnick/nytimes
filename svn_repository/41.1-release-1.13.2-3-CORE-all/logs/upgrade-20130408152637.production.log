15:26:54 1374  	   in_updated_by		  => in_updated_by,
15:26:54 1375  	   in_invoice_status_id 	  => GLOBAL_STATUSES_V19.INVOICE_CLOSED
15:26:54 1376  	 );
15:26:54 1377  
15:26:54 1378  	 FOR f_transaction_to_close IN (
