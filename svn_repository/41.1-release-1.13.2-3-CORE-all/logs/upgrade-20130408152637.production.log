15:26:53 417  PROCEDURE FINALIZE_SUBSCRIPTION_CREATION (
15:26:53 418  	in_subscription_id    IN NUMBER,
15:26:53 419  	in_invoice_id	      IN NUMBER,
15:26:53 420  	in_instrument_type_id IN NUMBER,
15:26:53 421  	in_instrument_id      IN NUMBER,
15:26:53 422  	in_created_by	      IN VARCHAR2
15:26:53 423  ) AS
15:26:53 424  SPROC_NAME CONSTANT VARCHAR2(30) := 'FINALIZE_SUBSCRIPTION_CREATION';
15:26:53 425  -- VARIABLES
15:26:53 426  var_invoice_amount     NUMBER(10, 2);
15:26:53 427  var_new_transaction_id NUMBER;
15:26:53 428  var_new_charge_id      NUMBER;
15:26:53 429  -- EXCEPTIONS
15:26:53 430  CAN_NOT_USE_FCINSTR	  EXCEPTION;
15:26:53 431  CAN_NOT_CALC_INVOICE_AMOUNT EXCEPTION;
15:26:53 432  CAN_NOT_CREATE_TRANSACTION  EXCEPTION;
15:26:53 433  CAN_NOT_CREATE_CHARGE	  EXCEPTION;
15:26:53 434  
15:26:53 435  EXCEPTION_MESSAGE VARCHAR2(1024);
15:26:53 436  BEGIN
15:26:53 437  
15:26:53 438  	-- Calculate invoice amount ( + discounts, taxes)
15:26:53 439  	BEGIN
15:26:53 440  	  PROCS_INVOICE_V19.CALCULATE_INVOICE_AMOUNT(
15:26:53 441  	    in_invoice_id => in_invoice_id,
15:26:53 442  	    out_amount	  => var_invoice_amount
15:26:53 443  	  );
15:26:53 444  	  EXCEPTION
15:26:53 445  	   WHEN OTHERS THEN
15:26:53 446  	     EXCEPTION_MESSAGE := SQLERRM;
15:26:53 447  	     RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
15:26:53 448  	END;
15:26:53 449  
15:26:53 450  	IF in_instrument_type_id = GLOBAL_ENUMS_V19.INSTRUMENT_ZCI_INSTRUMENT
15:26:53 451  	  AND var_invoice_amount > 0 THEN
15:26:53 452  	  RAISE CAN_NOT_USE_FCINSTR;
15:26:53 453  	END IF;
15:26:53 454  
15:26:53 455  	IF var_invoice_amount = 0 THEN
15:26:53 456  	  -- UPDATE INVOICE. SET STATUS TO PROCESSED
15:26:53 457  	  PROCS_INVOICE_CRU_V19.UPDATE_INVOICE(
15:26:53 458  	    in_invoice_id		   => in_invoice_id,
15:26:53 459  	    in_updated_by		   => in_created_by,
15:26:53 460  	    in_invoice_status_id	   => GLOBAL_STATUSES_V19.INVOICE_CLOSED
15:26:53 461  	  );
