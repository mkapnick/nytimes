15:26:53 216  	  UPDATE POLLING_SYNC_LASTRUN
15:26:53 217  	  SET last_run = current_timestamp;
15:26:53 218  	  IF ( sql%rowcount = 0 )
15:26:53 219  	  THEN
15:26:53 220  	    INSERT INTO POLLING_SYNC_LASTRUN VALUES (ts);
15:26:53 221  	  END if;
15:26:53 222  	  COMMIT;
15:26:53 223  END;
15:26:53 224  PROCEDURE GET_LAST_RUN(ts out timestamp)
15:26:53 225  IS
15:26:53 226  BEGIN
