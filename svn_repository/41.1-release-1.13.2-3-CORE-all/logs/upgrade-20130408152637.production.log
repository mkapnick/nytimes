15:26:53 2823  PROCEDURE ARE_REFUNDS_PENDING_FOR_SUBSCR (
15:26:53 2824  /*
15:26:53 2825  APP_EXCEPTION_CODES_V19.NOT_FOUND
15:26:53 2826  APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR
15:26:53 2827  */
15:26:53 2828  	 in_subscription_id IN NUMBER,
15:26:53 2829  	 out_result	    OUT NUMBER
15:26:53 2830  ) AS
15:26:53 2831  SPROC_NAME CONSTANT VARCHAR2(30) := 'ARE_REFUNDS_PENDING_FOR_SUBSCR';
15:26:53 2832  -- VARIABLES
15:26:53 2833  temp_subscription_id NUMBER;
15:26:53 2834  var_local_result     NUMBER;
15:26:53 2835  -- EXCEPTIONS
15:26:53 2836  BAD_SUBSCRIPTION_ID EXCEPTION;
15:26:53 2837  BEGIN
15:26:53 2838  
15:26:53 2839  	 -- Check that subscription exists
15:26:53 2840  	 BEGIN
15:26:53 2841  	   SELECT
15:26:53 2842  	     SUBSCRIPTION.ID into temp_subscription_id
15:26:53 2843  	   FROM
15:26:53 2844  	     SUBSCRIPTION
15:26:53 2845  	   WHERE
15:26:53 2846  	     SUBSCRIPTION.ID = in_subscription_id;
15:26:53 2847  	   EXCEPTION
15:26:53 2848  	     WHEN NO_DATA_FOUND THEN
15:26:53 2849  	       RAISE BAD_SUBSCRIPTION_ID;
15:26:53 2850  	 END;
15:26:53 2851  
15:26:53 2852  	 var_local_result := NULL;
15:26:53 2853  
15:26:53 2854  	 -- Check charges for each invoice associated with gived subscription
15:26:53 2855  	 FOR f_invoice IN (
15:26:53 2856  	   SELECT DISTINCT
15:26:53 2857  	     LICENSE.INVOICE_ID as "ID"
15:26:53 2858  	   FROM
15:26:53 2859  	     LICENSE
15:26:53 2860  	   WHERE
15:26:53 2861  	     LICENSE.SUBSCRIPTION_ID = in_subscription_id
15:26:53 2862  	     AND LICENSE.LICENSE_STATUS_ID IN ( SELECT GLOBAL_STATUSES_V19.LICENSE_ACTIVE FROM DUAL )
15:26:53 2863  	 )
15:26:53 2864  	 LOOP
15:26:53 2865  
15:26:53 2866  	   -- Check each charge in invoice
15:26:53 2867  	   FOR f_charge IN (
15:26:53 2868  	     SELECT
15:26:53 2869  	       CHARGE.ID,
15:26:53 2870  	       CHARGE.CHARGE_STATUS_ID,
15:26:53 2871  	       CHARGE.CHARGE_AMOUNT
15:26:53 2872  	     FROM
15:26:53 2873  	       CHARGE
15:26:53 2874  	     WHERE
15:26:53 2875  	       CHARGE.INVOICE_ID = f_invoice.ID
15:26:53 2876  	   )
15:26:53 2877  	   LOOP
15:26:53 2878  
15:26:53 2879  	     -- Charge amount < 0     => it is a refund
15:26:53 2880  	     -- Charge status is OPEN => means that it is not processed yet
15:26:53 2881  	     IF f_charge.CHARGE_AMOUNT < 0
15:26:53 2882  		AND f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V19.CHARGE_OPENED THEN
15:26:53 2883  	       var_local_result := GLOBAL_CONSTANTS_V19.TRUE;
15:26:53 2884  	     END IF;
15:26:53 2885  
15:26:53 2886  	   END LOOP;
15:26:53 2887  
15:26:53 2888  	 END LOOP;
15:26:53 2889  
15:26:53 2890  	 IF var_local_result IS NULL THEN
15:26:53 2891  	   out_result := GLOBAL_CONSTANTS_V19.FALSE;
15:26:53 2892  	 ELSE
15:26:53 2893  	   out_result := GLOBAL_CONSTANTS_V19.TRUE;
15:26:53 2894  	 END IF;
15:26:53 2895  
15:26:53 2896  EXCEPTION
15:26:53 2897  WHEN BAD_SUBSCRIPTION_ID THEN
15:26:53 2898  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:53 2899  	   SPROC_NAME, 'No such transaction id');
15:26:53 2900  WHEN OTHERS THEN
15:26:53 2901  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:53 2902  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:26:53 2903  END ARE_REFUNDS_PENDING_FOR_SUBSCR;
