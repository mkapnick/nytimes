15:26:54 886  
15:26:54 887  PROCEDURE APPLY_REFUND (
15:26:54 888  	in_invoice_id	     IN NUMBER,
15:26:54 889  	in_chargeback_amount IN NUMBER,
15:26:54 890  	in_created_by	     IN VARCHAR2,
15:26:54 891  	out_charge_id	     OUT NUMBER
15:26:54 892  ) AS
15:26:54 893  -- VARIABLES
15:26:54 894  SPROC_NAME		 CONSTANT VARCHAR2(16) := 'APPLY_CHARGEBACK';
15:26:54 895  temp_invoice_id		 NUMBER;
15:26:54 896  var_total_charges_amount	 NUMBER(10,2);
15:26:54 897  var_charge_amount_to_apply NUMBER(10,2);
15:26:54 898  var_transaction_id	 NUMBER;
15:26:54 899  var_instrument_type_id	 NUMBER;
15:26:54 900  var_instrument_id 	 NUMBER;
15:26:54 901  var_charge_id		 NUMBER;
15:26:54 902  -- EXCEPTIONS
15:26:54 903  BAD_INVOICE_ID		 EXCEPTION;
15:26:54 904  CAN_NOT_CREATE_TRANSACTION EXCEPTION;
15:26:54 905  CAN_NOT_FIND_INSTRUMENT	 EXCEPTION;
15:26:54 906  CAN_NOT_CREATE_CHARGE	 EXCEPTION;
15:26:54 907  EXCEPTION_MESSAGE 	 VARCHAR2(1024);
15:26:54 908  BEGIN
15:26:54 909  
15:26:54 910  	BEGIN
15:26:54 911  	  SELECT
15:26:54 912  	    INVOICE.ID into temp_invoice_id
15:26:54 913  	  FROM
15:26:54 914  	    INVOICE
15:26:54 915  	  WHERE
15:26:54 916  	    INVOICE.ID = in_invoice_id;
15:26:54 917  	  EXCEPTION
15:26:54 918  	    WHEN NO_DATA_FOUND THEN
15:26:54 919  	      RAISE BAD_INVOICE_ID;
15:26:54 920  	END;
15:26:54 921  
15:26:54 922  	SELECT
15:26:54 923  	  SUM (CHARGE.CHARGE_AMOUNT) into var_total_charges_amount
15:26:54 924  	FROM
15:26:54 925  	  CHARGE
15:26:54 926  	WHERE
15:26:54 927  	  CHARGE.INVOICE_ID = in_invoice_id
15:26:54 928  	  AND CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V19.CHARGE_PROCESSED
15:26:54 929  	  AND CHARGE.INSTRUMENT_TYPE_ID != GLOBAL_ENUMS_V19.INSTRUMENT_GIFT_CERTIFICATE;
15:26:54 930  
15:26:54 931  	-- FIXME: Maybe whe should throw exception here?
15:26:54 932  	IF var_total_charges_amount < in_chargeback_amount THEN
15:26:54 933  	  var_charge_amount_to_apply := var_total_charges_amount;
15:26:54 934  	ELSE
15:26:54 935  	  var_charge_amount_to_apply := in_chargeback_amount;
15:26:54 936  	END IF;
15:26:54 937  
15:26:54 938  	BEGIN
15:26:54 939  	  PROCS_TRANSACTION_V19.CREATE_TRANSACTION(
15:26:54 940  	    in_transaction_id	      => NULL,
15:26:54 941  	    in_status_id	      => GLOBAL_STATUSES_V19.TRANSACTION_PREPARE,
15:26:54 942  	    in_amount		      => -var_charge_amount_to_apply,
15:26:54 943  	    in_created_by	      => in_created_by,
15:26:54 944  	    in_order_id 	      => NULL,
15:26:54 945  	    in_is_refund	      => GLOBAL_CONSTANTS_V19.TRUE,
15:26:54 946  	    in_transaction_type_code  => 'REFUND',
15:26:54 947  	    out_transaction_id	      => var_transaction_id
15:26:54 948  	  );
15:26:54 949  	  EXCEPTION
15:26:54 950  	    WHEN OTHERS THEN
15:26:54 951  	      EXCEPTION_MESSAGE := SQLERRM;
15:26:54 952  	      RAISE CAN_NOT_CREATE_TRANSACTION;
15:26:54 953  	END;
15:26:54 954  
15:26:54 955  	BEGIN
15:26:54 956  	  SELECT
15:26:54 957  	    C.INSTRUMENT_TYPE_ID,
15:26:54 958  	    C.INSTRUMENT_ID
15:26:54 959  	    into
15:26:54 960  	    var_instrument_type_id,
15:26:54 961  	    var_instrument_id
15:26:54 962  	  FROM
15:26:54 963  	    CHARGE C,
15:26:54 964  	    TRANSACTION_ATTEMPT TA,
15:26:54 965  	    TRANSACTION T
15:26:54 966  	  WHERE
15:26:54 967  	    C.INVOICE_ID = in_invoice_id and
15:26:54 968  	    C.TRANSACTION_ID = T.ID and
15:26:54 969  	    TA.TRANSACTION_ID = T.ID and
15:26:54 970  	    TA.TRANSACTION_ATTEMPT_STATUS_ID = GLOBAL_STATUSES_V19.TRANS_ATTEMPT_SUCCESS and
15:26:54 971  	    T.IS_REFUND = GLOBAL_CONSTANTS_V19.FALSE and
15:26:54 972  	    T.TRANSACTION_AMOUNT >= 0 and
15:26:54 973  	    rownum < 2;
15:26:54 974  	EXCEPTION
15:26:54 975  	    WHEN NO_DATA_FOUND THEN
15:26:54 976  	      RAISE CAN_NOT_FIND_INSTRUMENT;
15:26:54 977  	END;
15:26:54 978  	BEGIN
15:26:54 979  	  PROCS_CHARGE_V19.CREATE_CHARGE(
15:26:54 980  	    in_invoice_id	  => in_invoice_id,
15:26:54 981  	    in_transaction_id	  => var_transaction_id,
15:26:54 982  	    in_instrument_type_id => var_instrument_type_id,
15:26:54 983  	    in_instrument_id	  => var_instrument_id,
15:26:54 984  	    in_charge_amount	  => -var_charge_amount_to_apply,
15:26:54 985  	    in_created_by	  => in_created_by,
15:26:54 986  	    in_charge_status_id   => GLOBAL_STATUSES_V19.CHARGE_OPENED,
15:26:54 987  	    out_charge_id	  => var_charge_id
15:26:54 988  	  );
15:26:54 989  	  EXCEPTION
15:26:54 990  	    WHEN OTHERS THEN
15:26:54 991  	      EXCEPTION_MESSAGE := SQLERRM;
15:26:54 992  	      RAISE CAN_NOT_CREATE_CHARGE;
15:26:54 993  	END;
15:26:54 994  
15:26:54 995  	out_charge_id := var_charge_id;
15:26:54 996  
15:26:54 997  EXCEPTION
15:26:54 998  WHEN BAD_INVOICE_ID THEN
15:26:54 999  	PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 1000  	   SPROC_NAME, 'No such invoice');
15:26:54 1001  WHEN CAN_NOT_CREATE_TRANSACTION THEN
15:26:54 1002  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 1003  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
15:26:54 1004  WHEN CAN_NOT_FIND_INSTRUMENT THEN
15:26:54 1005  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 1006  	   SPROC_NAME, 'Could not find financial instrument');
15:26:54 1007  WHEN CAN_NOT_CREATE_CHARGE THEN
15:26:54 1008  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.INTERNAL_ERROR,
15:26:54 1009  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
15:26:54 1010  WHEN OTHERS THEN
15:26:54 1011  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:54 1012  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:26:54 1013  END APPLY_REFUND;
15:26:54 1014  
15:26:54 1015  /******************************************************************************/
15:26:54 1016  
15:26:54 1017  PROCEDURE GET_MAX_REFUND (
15:26:54 1018  	 in_invoice_id IN NUMBER,
15:26:54 1019  	 out_amount    OUT NUMBER
15:26:54 1020  ) AS
15:26:54 1021  SPROC_NAME CONSTANT VARCHAR2(14) := 'GET_MAX_REFUND';
15:26:54 1022  -- VARIABLES
15:26:54 1023  temp_invoice_id	       NUMBER;
15:26:54 1024  var_invoice_refunds_sum NUMBER(10,2);
15:26:54 1025  var_invoice_charges_sum NUMBER(10,2);
15:26:54 1026  -- EXCEPTIONS
15:26:54 1027  BAD_INVOICE_ID EXCEPTION;
15:26:54 1028  BEGIN
15:26:54 1029  
15:26:54 1030  	 BEGIN
15:26:54 1031  	   SELECT
15:26:54 1032  	     INVOICE.ID into temp_invoice_id
15:26:54 1033  	   FROM
15:26:54 1034  	     INVOICE
15:26:54 1035  	   WHERE
15:26:54 1036  	     INVOICE.ID = in_invoice_id;
15:26:54 1037  	   EXCEPTION
15:26:54 1038  	     WHEN NO_DATA_FOUND THEN
15:26:54 1039  	       RAISE BAD_INVOICE_ID;
15:26:54 1040  	 END;
15:26:54 1041  
15:26:54 1042  	 var_invoice_refunds_sum := 0;
15:26:54 1043  	 var_invoice_charges_sum := 0;
15:26:54 1044  
15:26:54 1045  	 FOR f_charge IN (
15:26:54 1046  	   SELECT
15:26:54 1047  	     CHARGE.ID,
15:26:54 1048  	     CHARGE.CHARGE_STATUS_ID,
15:26:54 1049  	     CHARGE.CHARGE_AMOUNT,
15:26:54 1050  	     CHARGE.TRANSACTION_ID
15:26:54 1051  	   FROM
15:26:54 1052  	     CHARGE
15:26:54 1053  	   WHERE
15:26:54 1054  	     CHARGE.INVOICE_ID = in_invoice_id
15:26:54 1055  	 )
15:26:54 1056  	 LOOP
15:26:54 1057  	   -- If charge.status = canceled then continue
15:26:54 1058  	   IF f_charge.CHARGE_STATUS_ID != GLOBAL_STATUSES_V19.CHARGE_CANCELED THEN
15:26:54 1059  
15:26:54 1060  	     IF f_charge.CHARGE_AMOUNT > 0 THEN
15:26:54 1061  	       IF f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V19.CHARGE_PROCESSED
15:26:54 1062  		  AND PROCS_TRANSACTION_V19.IS_TRANSACTION_COLLECTED(f_charge.TRANSACTION_ID) = GLOBAL_CONSTANTS_V19.TRUE THEN
15:26:54 1063  		 -- Transaction collected
15:26:54 1064  		 var_invoice_charges_sum := var_invoice_charges_sum + f_charge.CHARGE_AMOUNT;
15:26:54 1065  	       ELSE
15:26:54 1066  		 -- Transaction is not collected. Do nothing
15:26:54 1067  		 NULL;
15:26:54 1068  	       END IF;
15:26:54 1069  	     ELSE
15:26:54 1070  	       IF f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V19.CHARGE_PROCESSED
15:26:54 1071  		  AND PROCS_TRANSACTION_V19.IS_TRANSACTION_COLLECTED(f_charge.TRANSACTION_ID) = GLOBAL_CONSTANTS_V19.FALSE THEN
15:26:54 1072  		 -- If charge is processed transaction is not collected then do nothing
15:26:54 1073  		 NULL;
15:26:54 1074  	       ELSE
15:26:54 1075  		 var_invoice_refunds_sum := var_invoice_refunds_sum + f_charge.CHARGE_AMOUNT;
15:26:54 1076  	       END IF;
15:26:54 1077  	     END IF;
15:26:54 1078  
15:26:54 1079  	   END IF;
15:26:54 1080  	 END LOOP;
15:26:54 1081  
15:26:54 1082  	 -- Refunds are negative
15:26:54 1083  	 var_invoice_refunds_sum := 0 - var_invoice_refunds_sum;
15:26:54 1084  
15:26:54 1085  	 out_amount := var_invoice_charges_sum - var_invoice_refunds_sum;
15:26:54 1086  
15:26:54 1087  EXCEPTION
15:26:54 1088  WHEN BAD_INVOICE_ID THEN
15:26:54 1089  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 1090  	   SPROC_NAME, 'No such invoice');
15:26:54 1091  WHEN OTHERS THEN
15:26:54 1092  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:54 1093  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:26:54 1094  END GET_MAX_REFUND;
15:26:54 1095  
15:26:54 1096  /******************************************************************************/
15:26:54 1097  
15:26:54 1098  PROCEDURE GET_INVOICE_DAYS_USED_NUMBER (
15:26:54 1099  	 in_invoice_id	     IN NUMBER,
15:26:54 1100  	 in_chargeback_date  IN DATE DEFAULT SYSDATE,
15:26:54 1101  	 out_days_num	     OUT NUMBER
15:26:54 1102  ) AS
15:26:54 1103  SPROC_NAME CONSTANT VARCHAR2(28) := 'GET_INVOICE_DAYS_USED_NUMBER';
15:26:54 1104  -- VARIABLES
15:26:54 1105  temp_invoice_id	      NUMBER;
15:26:54 1106  var_license_start_date DATE;
15:26:54 1107  var_license_end_date   DATE;
15:26:54 1108  var_chargeback_date    DATE;
15:26:54 1109  var_invoice_days_used  NUMBER;
15:26:54 1110  -- EXCEPTIONS
15:26:54 1111  BAD_INVOICE_ID EXCEPTION;
15:26:54 1112  BEGIN
15:26:54 1113  
15:26:54 1114  	 var_chargeback_date := NVL(in_chargeback_date, SYSDATE);
15:26:54 1115  
15:26:54 1116  	 BEGIN
15:26:54 1117  	   SELECT
15:26:54 1118  	     INVOICE.ID into temp_invoice_id
15:26:54 1119  	   FROM
15:26:54 1120  	     INVOICE
15:26:54 1121  	   WHERE
15:26:54 1122  	     INVOICE.ID = in_invoice_id;
15:26:54 1123  	   EXCEPTION
15:26:54 1124  	     WHEN NO_DATA_FOUND THEN
15:26:54 1125  	       RAISE BAD_INVOICE_ID;
15:26:54 1126  	 END;
15:26:54 1127  
15:26:54 1128  	 var_invoice_days_used := 0;
15:26:54 1129  
15:26:54 1130  	 FOR f_license IN (
15:26:54 1131  	   SELECT
15:26:54 1132  	     LICENSE.START_DATE,
15:26:54 1133  	     LICENSE.END_DATE
15:26:54 1134  	   FROM
15:26:54 1135  	     LICENSE
15:26:54 1136  	   WHERE
15:26:54 1137  	     LICENSE.INVOICE_ID = in_invoice_id
15:26:54 1138  	     AND LICENSE.IS_EXTENSION = GLOBAL_CONSTANTS_V19.FALSE
15:26:54 1139  	 )
15:26:54 1140  	 LOOP
15:26:54 1141  	   var_license_start_date := PROCS_COMMON_V19.NORMALIZE_DATE(f_license.START_DATE);
15:26:54 1142  	   var_license_end_date := PROCS_COMMON_V19.NORMALIZE_DATE(f_license.END_DATE);
15:26:54 1143  
15:26:54 1144  	   IF var_license_start_date <= var_chargeback_date THEN
15:26:54 1145  	     IF var_license_end_date <= var_chargeback_date THEN
15:26:54 1146  	       -- License is passed
15:26:54 1147  	       var_invoice_days_used := var_invoice_days_used + (var_license_end_date - var_license_start_date);
15:26:54 1148  	     ELSE
15:26:54 1149  	       -- This is current license
15:26:54 1150  	       var_invoice_days_used := var_invoice_days_used + (var_chargeback_date - var_license_start_date);
15:26:54 1151  	     END IF;
15:26:54 1152  	   ELSE
15:26:54 1153  	     -- if var_license_start_date > in_chargeback_date then do nothing
15:26:54 1154  	     NULL;
15:26:54 1155  	   END IF;
15:26:54 1156  	 END LOOP;
15:26:54 1157  
15:26:54 1158  	 out_days_num := var_invoice_days_used;
15:26:54 1159  
15:26:54 1160  EXCEPTION
15:26:54 1161  WHEN BAD_INVOICE_ID THEN
15:26:54 1162  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.NOT_FOUND,
15:26:54 1163  	   SPROC_NAME, 'No such invoice');
15:26:54 1164  WHEN OTHERS THEN
15:26:54 1165  	 PROCS_COMMON_V19.THROW_EXCEPTION(APP_EXCEPTION_CODES_V19.UNKNOWN_ERROR,
15:26:54 1166  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:26:54 1167  END GET_INVOICE_DAYS_USED_NUMBER;
15:26:54 1168  
15:26:54 1169  /******************************************************************************/
15:26:54 1170  
15:26:54 1171  PROCEDURE GET_INVOICE_LINE_ITEMS (
15:26:54 1172  	 in_invoice_id	IN NUMBER,
15:26:54 1173  	 out_result_set OUT SYS_REFCURSOR
15:26:54 1174  ) AS
15:26:54 1175  SPROC_NAME CONSTANT VARCHAR2(22) := 'GET_INVOICE_LINE_ITEMS';
15:26:54 1176  -- VARIABLES
15:26:54 1177  temp_invoice_id NUMBER;
15:26:54 1178  -- EXCEPTIONS
15:26:54 1179  BAD_INVOICE_ID EXCEPTION;
15:26:54 1180  BEGIN
15:26:54 1181  
15:26:54 1182  	 BEGIN
15:26:54 1183  	   SELECT
15:26:54 1184  	     INVOICE.ID into temp_invoice_id
15:26:54 1185  	   FROM
15:26:54 1186  	     INVOICE
15:26:54 1187  	   WHERE
15:26:54 1188  	     INVOICE.ID = in_invoice_id;
15:26:54 1189  	   EXCEPTION
15:26:54 1190  	     WHEN NO_DATA_FOUND THEN
