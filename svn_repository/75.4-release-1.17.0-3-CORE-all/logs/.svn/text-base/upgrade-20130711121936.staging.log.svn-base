12:19:51 2225  	 in_charge_id		IN NUMBER,
12:19:51 2226  	 out_result		OUT NUMBER
12:19:51 2227  ) AS
12:19:51 2228  -- VARIABLES
12:19:51 2229  SPROC_NAME	    CONSTANT VARCHAR2(25) := 'IS_GCERT_FOR_PROPER_OFFER';
12:19:51 2230  var_invoice_id	    NUMBER;
12:19:51 2231  var_offer_chain_id   NUMBER;
12:19:51 2232  var_offer_chain_s_id NUMBER;
12:19:51 2233  -- EXCEPTIONS
12:19:51 2234  BAD_CHARGE_ID		 EXCEPTION;
12:19:51 2235  BAD_GIFT_CERTIFICATE_ID	 EXCEPTION;
12:19:51 2236  CAN_NOT_FIND_SUBSCRIPTION EXCEPTION;
12:19:51 2237  BEGIN
12:19:51 2238  
12:19:51 2239  	 BEGIN
12:19:51 2240  	   SELECT
12:19:51 2241  	     CHARGE.INVOICE_ID into var_invoice_id
12:19:51 2242  	   FROM
12:19:51 2243  	     CHARGE
12:19:51 2244  	   WHERE
12:19:51 2245  	     CHARGE.ID = in_charge_id;
12:19:51 2246  	   EXCEPTION
12:19:51 2247  	     WHEN NO_DATA_FOUND THEN
12:19:51 2248  	       RAISE BAD_CHARGE_ID;
12:19:51 2249  	 END;
12:19:51 2250  
12:19:51 2251  	 BEGIN
12:19:51 2252  	   SELECT
12:19:51 2253  	     GIFT_CERTIFICATE.OFFER_CHAIN_ID into var_offer_chain_id
12:19:51 2254  	   FROM
12:19:51 2255  	     GIFT_CERTIFICATE
12:19:51 2256  	   WHERE
12:19:51 2257  	     GIFT_CERTIFICATE.ID = in_gift_certificate_id;
12:19:51 2258  	   EXCEPTION
12:19:51 2259  	     WHEN NO_DATA_FOUND THEN
12:19:51 2260  	       RAISE BAD_GIFT_CERTIFICATE_ID;
12:19:51 2261  	 END;
12:19:51 2262  
12:19:51 2263  	 BEGIN
12:19:51 2264  	   SELECT
12:19:51 2265  	     SUBSCRIPTION.OFFER_CHAIN_ID into var_offer_chain_s_id
12:19:51 2266  	   FROM
12:19:51 2267  	     SUBSCRIPTION
12:19:51 2268  	   WHERE
12:19:51 2269  	     SUBSCRIPTION.ID IN (
12:19:51 2270  	       SELECT DISTINCT
12:19:51 2271  		 LICENSE.SUBSCRIPTION_ID
12:19:51 2272  	       FROM
12:19:51 2273  		 LICENSE
12:19:51 2274  	       WHERE
12:19:51 2275  		 LICENSE.INVOICE_ID = var_invoice_id
12:19:51 2276  	     );
12:19:51 2277  	   EXCEPTION
12:19:51 2278  	     WHEN NO_DATA_FOUND THEN
12:19:51 2279  	       RAISE CAN_NOT_FIND_SUBSCRIPTION;
12:19:51 2280  	 END;
12:19:51 2281  
12:19:51 2282  	 IF var_offer_chain_s_id = var_offer_chain_id THEN
12:19:51 2283  	   out_result := GLOBAL_CONSTANTS_V23.TRUE;
12:19:51 2284  	 ELSE
12:19:51 2285  	   out_result := GLOBAL_CONSTANTS_V23.FALSE;
12:19:51 2286  	 END IF;
12:19:51 2287  
12:19:51 2288  EXCEPTION
12:19:51 2289  WHEN BAD_CHARGE_ID THEN
12:19:51 2290  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 2291  	   SPROC_NAME, 'No such charge');
12:19:51 2292  WHEN BAD_GIFT_CERTIFICATE_ID THEN
12:19:51 2293  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
