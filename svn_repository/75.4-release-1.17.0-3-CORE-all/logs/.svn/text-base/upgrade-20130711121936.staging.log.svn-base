12:19:51 2498  EXCEPTION_MESSAGE   VARCHAR2(1024);
12:19:51 2499  BEGIN
12:19:51 2500  
12:19:51 2501  	 BEGIN
12:19:51 2502  	   -- find offer_chain_id for given in_subscription_id
12:19:51 2503  	   SELECT OFFER_CHAIN_ID into offer_id
12:19:51 2504  	   FROM SUBSCRIPTION
12:19:51 2505  	   WHERE ID = in_subscription_id;
12:19:51 2506  	   EXCEPTION
12:19:51 2507  	     WHEN NO_DATA_FOUND THEN
12:19:51 2508  	       RAISE BAD_SUBSCRIPTION_ID;
12:19:51 2509  	     WHEN OTHERS THEN
12:19:51 2510  	       EXCEPTION_MESSAGE := SQLERRM;
12:19:51 2511  	       RAISE COULD_NOT_CHECK;
12:19:51 2512  	 END;
12:19:51 2513  
12:19:51 2514  	 -- find if it was redeemed from a gift certificate
12:19:51 2515  	 BEGIN
12:19:51 2516  	   SELECT l.end_date INTO var_end_date
12:19:51 2517  	   FROM LICENSE l, GIFT_CERTIFICATE g
12:19:51 2518  	   WHERE l.invoice_id = g.finalized_invoice_id
12:19:51 2519  	   AND l.subscription_id = in_subscription_id;
12:19:51 2520  
12:19:51 2521  	   -- if the license end_date is bigger than today, we are in the
12:19:51 2522  	   -- first period, so we cannot cancel; otherwise can cancel
12:19:51 2523  	   IF var_end_date > today_date THEN
12:19:51 2524  	       RETURN GLOBAL_CONSTANTS_V23.FALSE;
12:19:51 2525  	   ELSE
12:19:51 2526  	       RETURN GLOBAL_CONSTANTS_V23.TRUE;
12:19:51 2527  	   END IF;
12:19:51 2528  
12:19:51 2529  	   EXCEPTION
12:19:51 2530  	       -- not coming from a gift certificate,
12:19:51 2531  	       -- use old logic
12:19:51 2532  	       WHEN NO_DATA_FOUND THEN
12:19:51 2533  		   SELECT
12:19:51 2534  		       PROCS_OFFER_CHAIN_V23.IS_OFFER_CHAIN_CANCELABLE(offer_id)
12:19:51 2535  		       INTO var_is_offer_chain_cancelable
12:19:51 2536  		   FROM DUAL;
12:19:51 2537  		   RETURN var_is_offer_chain_cancelable;
12:19:51 2538  	 END;
12:19:51 2539  
12:19:51 2540  EXCEPTION
12:19:51 2541  WHEN BAD_SUBSCRIPTION_ID THEN
12:19:51 2542  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 2543  	   SPROC_NAME, 'No such subscription');
12:19:51 2544  WHEN COULD_NOT_CHECK THEN
12:19:51 2545  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 2546  	   SPROC_NAME, 'Could not check if offer chain calcelable', EXCEPTION_MESSAGE);
12:19:51 2547  WHEN OTHERS THEN
12:19:51 2548  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 2549  	   SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 2550  
12:19:51 2551  END IS_SUBSCRIPTION_CANCELABLE;
