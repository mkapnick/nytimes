12:19:51 960  FUNCTION GET_TRANSACTION_TAX_AMOUNT (
12:19:51 961  	in_transaction_id IN NUMBER
12:19:51 962  ) RETURN NUMBER AS
12:19:51 963  SPROC_NAME CONSTANT VARCHAR2(26) := 'GET_TRANSACTION_TAX_AMOUNT';
12:19:51 964  -- Variables
12:19:51 965  var_tax_amount NUMBER(10,2);
12:19:51 966  BEGIN
12:19:51 967  
12:19:51 968  	SELECT
12:19:51 969  	  SUM(LI.TAXES_AMOUNT) into var_tax_amount
12:19:51 970  	FROM
12:19:51 971  	  LINE_ITEM LI
12:19:51 972  	  INNER JOIN INVOICE I ON LI.INVOICE_ID = I.ID
12:19:51 973  	  INNER JOIN CHARGE CH ON CH.INVOICE_ID = I.ID
12:19:51 974  	WHERE
12:19:51 975  	  CH.TRANSACTION_ID = in_transaction_id;
12:19:51 976  
12:19:51 977  	IF var_tax_amount IS NULL THEN
12:19:51 978  	  var_tax_amount := 0;
12:19:51 979  	END IF;
12:19:51 980  
12:19:51 981  	RETURN var_tax_amount;
12:19:51 982  
12:19:51 983  END GET_TRANSACTION_TAX_AMOUNT;
