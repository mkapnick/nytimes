12:19:51 530  	    OFFER_OFFER_CHAIN
12:19:51 531  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
12:19:51 532  	  WHERE
12:19:51 533  	    OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
12:19:51 534  	    AND OFFER_OFFER_CHAIN.INDEX_VALUE = var_next_offer_index;
12:19:51 535  	END IF;
12:19:51 536  
12:19:51 537  EXCEPTION
