12:19:51 867  		AND SYSDATE + in_days_before_close >= l.grace_end_date
12:19:51 868  		AND NOT EXISTS
12:19:51 869  		    (
12:19:51 870  			SELECT
12:19:51 871  			    1
12:19:51 872  			FROM
