12:19:51 436  -- norlov: #38796
12:19:51 437  PROCEDURE GET_TRANSACTION_INVOICE (
12:19:51 438  /*
12:19:51 439  Throws exceptions:
12:19:51 440  APP_EXCEPTION_CODES_V23.NOT_FOUND
12:19:51 441  APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR
12:19:51 442  */
12:19:51 443  	in_transaction_id  IN  NUMBER,
12:19:51 444  	out_result_set	      OUT SYS_REFCURSOR
12:19:51 445  ) AS
12:19:51 446  SPROC_NAME CONSTANT  VARCHAR2(23) := 'GET_TRANSACTION_INVOICE';
12:19:51 447  -- VARIABLES
12:19:51 448  temp_transaction_id  NUMBER;
12:19:51 449  var_invoice_id	   NUMBER;
12:19:51 450  var_subscription_id  NUMBER;
12:19:51 451  var_offer_chain_id   NUMBER;
12:19:51 452  var_offer_chain_name VARCHAR2(255);
12:19:51 453  -- EXCEPTIONS
12:19:51 454  BAD_TRANSACTION_ID     EXCEPTION;
12:19:51 455  EXCEPTION_MESSAGE      VARCHAR2(1024);
12:19:51 456  BEGIN
12:19:51 457   -- check if there is the transaction
12:19:51 458  	BEGIN
12:19:51 459  	  SELECT
12:19:51 460  	    TRANSACTION.ID into temp_transaction_id
12:19:51 461  	  FROM
12:19:51 462  	    TRANSACTION
12:19:51 463  	  WHERE
12:19:51 464  	    TRANSACTION.ID = in_transaction_id;
12:19:51 465  	  EXCEPTION
12:19:51 466  	    WHEN NO_DATA_FOUND THEN
12:19:51 467  	      RAISE BAD_TRANSACTION_ID;
12:19:51 468  	END;
12:19:51 469  
12:19:51 470  	-- Get invoice id
12:19:51 471  	SELECT DISTINCT
12:19:51 472  	  CHARGE.INVOICE_ID into var_invoice_id
12:19:51 473  	FROM
12:19:51 474  	  CHARGE
12:19:51 475  	WHERE
12:19:51 476  	  CHARGE.TRANSACTION_ID = in_transaction_id;
12:19:51 477  
12:19:51 478  	-- Get subscription id if exists
12:19:51 479  	BEGIN
12:19:51 480  	  SELECT DISTINCT
12:19:51 481  	    LICENSE.SUBSCRIPTION_ID into var_subscription_id
12:19:51 482  	  FROM
12:19:51 483  	    LICENSE
12:19:51 484  	  WHERE
12:19:51 485  	    LICENSE.INVOICE_ID = var_invoice_id;
12:19:51 486  	  EXCEPTION
12:19:51 487  	    WHEN NO_DATA_FOUND THEN
12:19:51 488  	      var_subscription_id := NULL;
12:19:51 489  	END;
12:19:51 490  
12:19:51 491  	IF var_subscription_id IS NOT NULL THEN
12:19:51 492  	  -- Fetch offer chain from subscription
12:19:51 493  	  SELECT
12:19:51 494  	    OFFER_CHAIN.ID,
12:19:51 495  	    OFFER_CHAIN.NAME
12:19:51 496  	    into
12:19:51 497  	    var_offer_chain_id,
12:19:51 498  	    var_offer_chain_name
12:19:51 499  	  FROM
12:19:51 500  	    OFFER_CHAIN
12:19:51 501  	    INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
12:19:51 502  	  WHERE
12:19:51 503  	    SUBSCRIPTION.ID = var_subscription_id;
12:19:51 504  	ELSE
12:19:51 505  	  -- Fetch offer chain from GC
12:19:51 506  	  SELECT
12:19:51 507  	    OFFER_CHAIN.ID,
12:19:51 508  	    OFFER_CHAIN.NAME
12:19:51 509  	    into
12:19:51 510  	    var_offer_chain_id,
12:19:51 511  	    var_offer_chain_name
12:19:51 512  	  FROM
12:19:51 513  	    OFFER_CHAIN
12:19:51 514  	    INNER JOIN GIFT_CERTIFICATE ON GIFT_CERTIFICATE.OFFER_CHAIN_ID = OFFER_CHAIN.ID
12:19:51 515  	  WHERE
12:19:51 516  	    GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = var_invoice_id;
12:19:51 517  	END IF;
12:19:51 518  
12:19:51 519  	OPEN out_result_set FOR
12:19:51 520  	SELECT DISTINCT
12:19:51 521  	  var_invoice_id       AS "INVOICE_ID",
12:19:51 522  	  var_subscription_id  AS "SUBSCRIPTION_ID",
12:19:51 523  	  var_offer_chain_id   AS "OFFER_CHAIN_ID",
12:19:51 524  	  var_offer_chain_name AS "OFFER_CHAIN_NAME"
12:19:51 525  	FROM
12:19:51 526  	  DUAL;
12:19:51 527  
12:19:51 528  EXCEPTION
12:19:51 529  WHEN BAD_TRANSACTION_ID THEN
12:19:51 530  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 531  	  SPROC_NAME, 'No such transaction');
12:19:51 532  WHEN OTHERS THEN
12:19:51 533  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 534  	  SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 535  END GET_TRANSACTION_INVOICE;
