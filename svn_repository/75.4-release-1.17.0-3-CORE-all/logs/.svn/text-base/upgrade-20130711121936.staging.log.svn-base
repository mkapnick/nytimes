12:19:52 620  	  SUBSCRIPTION.INSTRUMENT_ID,
12:19:52 621  	  SUBSCRIPTION.INSTRUMENT_TYPE_ID,
12:19:52 622  	  OFFER_CHAIN.ID AS "OFFER_CHAIN_ID",
12:19:52 623  	  OFFER_CHAIN.NAME,
12:19:52 624  	  OFFER_CHAIN.DESCRIPTION,
12:19:52 625  	  OFFER_CHAIN.PRODUCT_URI,
12:19:52 626  	  PROCS_SUBSCRIPTION_V23.CALC_SUBSCRIPTION_END_DATE(SUBSCRIPTION.ID) as "END_DATE",
12:19:52 627  	  PROCS_SUBSCRIPTION_V23.GET_RECENT_CHARGE(SUBSCRIPTION.ID) AS "RECENT_CHARGE",
12:19:52 628  	  PROCS_SUBSCRIPTION_V23.GET_RENEWAL_DATE(SUBSCRIPTION.ID) AS "RENEWAL_DATE",
12:19:52 629  	  PROCS_SUBSCRIPTION_V23.GET_BILLING_CYCLE(SUBSCRIPTION.ID) AS "BILLING_CYCLE",
12:19:52 630  	  PROCS_SUBSCRIPTION_V23.IS_SUBSCRIPTION_CANCELABLE(SUBSCRIPTION.ID) AS "IS_CANCELABLE",
12:19:52 631  	  ITUNES_RECEIPT.ID AS "ITUNES_RECEIPT_ID",
12:19:52 632  	  (
12:19:52 633  	    SELECT
12:19:52 634  	      MAX(ENTITLEMENT_END_DATE)
12:19:52 635  	      FROM LICENSE
12:19:52 636  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
12:19:52 637  	  )
12:19:52 638  	  as "ENT_END_DATE",
12:19:52 639  	  (
12:19:52 640  	    SELECT
12:19:52 641  	      MIN(START_DATE)
12:19:52 642  	      FROM LICENSE
12:19:52 643  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
12:19:52 644  	  )
12:19:52 645  	  as "ENT_START_DATE",
12:19:52 646  	  GROUP_ACCOUNT.SUBSCRIPTION_ID GA_SUBSCRIPTION_ID,
12:19:52 647  	  GROUP_ACCOUNT.ID GA_ID,
12:19:52 648  	  GROUP_ACCOUNT.GROUP_NAME GA_GROUP_NAME,
12:19:52 649  	  GROUP_ACCOUNT.FIRST_NAME GA_FIRST_NAME,
12:19:52 650  	  GROUP_ACCOUNT.LAST_NAME GA_LAST_NAME,
12:19:52 651  	  GROUP_ACCOUNT.EMAIL GA_EMAIL,
12:19:52 652  	  GROUP_ACCOUNT.PHONE GA_PHONE,
12:19:52 653  	  GROUP_ACCOUNT.ORGANIZATION_TYPE GA_ORGANIZATION_TYPE,
12:19:52 654  	  GROUP_ACCOUNT.SEATS GA_SEATS,
12:19:52 655  	  PROCS_GROUP_ACCOUNT_V23.F_GET_NUM_OCCUPIED_GROUP_SEATS(GROUP_ACCOUNT.ID) GA_SEATS_USED,
12:19:52 656  	  GROUP_ACCOUNT.IP GA_IP,
12:19:52 657  	  PROCS_SUBSCRIPTION_V23.GET_GIFT_CERT_CODE_BY_SUB_ID(SUBSCRIPTION.ID) GIFT_CERTIFICATE_CODE,
12:19:52 658  	  PROCS_ACCOUNT_V23.GET_GRACE_START_DATE(SUBSCRIPTION.ID) GRACE_START_DATE,
12:19:52 659  	  PROCS_ACCOUNT_V23.GET_GRACE_END_DATE(SUBSCRIPTION.ID) GRACE_END_DATE
12:19:52 660  	FROM
12:19:52 661  	  SUBSCRIPTION
12:19:52 662  	  INNER JOIN OFFER_CHAIN ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
12:19:52 663  	  LEFT JOIN SUBSCRIPTION_CANCEL_REASON ON SUBSCRIPTION.SCT_ID = SUBSCRIPTION_CANCEL_REASON.ID
12:19:52 664  	  LEFT JOIN ITUNES_RECEIPT ON SUBSCRIPTION.ID = ITUNES_RECEIPT.SUBSCRIPTION_ID
12:19:52 665  	  LEFT JOIN GROUP_ACCOUNT ON SUBSCRIPTION.ID = GROUP_ACCOUNT.SUBSCRIPTION_ID
12:19:52 666  	WHERE
12:19:52 667  	  SUBSCRIPTION.ACCOUNT_ID = var_account_id
12:19:52 668  	  AND (SUBSCRIPTION.SCT_ID IS NULL OR SUBSCRIPTION.SCT_ID != GLOBAL_STATUSES_V23.REAL_TIME_CANCEL_REASON)
12:19:52 669  	  AND SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = NVL(in_status, SUBSCRIPTION.SUBSCRIPTION_STATUS_ID)
12:19:52 670  	  AND PROCS_COMMON_V23.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE) >= NVL(in_start_date, PROCS_COMMON_V23.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE))
12:19:52 671  	  AND PROCS_COMMON_V23.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE) <= NVL(in_end_date, PROCS_COMMON_V23.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE))
12:19:52 672  	  AND (in_group_account_type IS NULL OR OFFER_CHAIN.GROUP_ACCOUNT_TYPE_ID = in_group_account_type);
12:19:52 673  
12:19:52 674  EXCEPTION
12:19:52 675  WHEN BAD_GROUP_ID THEN
12:19:52 676  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:52 677  	  SPROC_NAME, 'Can not find account with given group id');
12:19:52 678  WHEN OTHERS THEN
12:19:52 679  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:52 680  	  SPROC_NAME, 'Unknown error', SQLERRM);
12:19:52 681  END GET_ACCOUNT_SUBSCRIPTIONS;
