12:19:51 106  FUNCTION CREATE_NEW_TRANSFER_SET
12:19:51 107  	  (
12:19:51 108  	      in_set_maximum NUMBER)
12:19:51 109  	  RETURN core_owner.polling_sync.set_id%type
12:19:51 110  IS
12:19:51 111  	  pragma autonomous_transaction;
12:19:51 112  	  v_set_id core_owner.polling_sync.set_id%type;
12:19:51 113  BEGIN
12:19:51 114  	  SELECT
12:19:51 115  	      pollsync_setid_seq.nextval
12:19:51 116  	  INTO
