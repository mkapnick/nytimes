12:19:51 1226  temp_invoice_id NUMBER;
12:19:51 1227  -- EXCEPTIONS
12:19:51 1228  BAD_INVOICE_ID EXCEPTION;
12:19:51 1229  BEGIN
12:19:51 1230  
12:19:51 1231  	 BEGIN
12:19:51 1232  	   SELECT
12:19:51 1233  	     INVOICE.ID into temp_invoice_id
12:19:51 1234  	   FROM
12:19:51 1235  	     INVOICE
