12:19:52 369  	out_Result_Set	    OUT Sys_Refcursor
12:19:52 370  ) AS
12:19:52 371  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_SUBSCRIPTION_SHARES';
12:19:52 372  range_diff NUMBER := 0;
12:19:52 373  upper_bond_diff NUMBER := 0;
12:19:52 374  l_start NUMBER := 0;
12:19:52 375  l_end   NUMBER := 0;
12:19:52 376  BEGIN
12:19:52 377  	-- Normalize the end points [START]
12:19:52 378  	IF (in_start IS NULL OR in_start < 0) Then
12:19:52 379  	  l_start := 0;
12:19:52 380  	ELSE
12:19:52 381  	  l_start := in_start;
12:19:52 382  	END IF;
12:19:52 383  
12:19:52 384  	IF (in_end IS NULL) Then
12:19:52 385  	  l_end := 11;
12:19:52 386  	ELSE
12:19:52 387  	  l_end := in_end;
12:19:52 388  	END IF;
12:19:52 389  
12:19:52 390  	l_start := l_start + 1;
12:19:52 391  	l_end	:= l_end   + 1;
12:19:52 392  
12:19:52 393  	range_diff := l_end - l_start;
12:19:52 394  	upper_bond_diff :=  range_diff - GLOBAL_CONSTANTS_V23.MAX_RETURN_COUNT;
12:19:52 395  
12:19:52 396  	IF (upper_bond_diff > 0) Then
12:19:52 397  	  l_end := l_end - upper_bond_diff;
12:19:52 398  	END IF;
12:19:52 399  	-- Normalize the end points [END]
12:19:52 400  
12:19:52 401  	BEGIN
12:19:52 402  	   OPEN out_result_set FOR
12:19:52 403  	   SELECT *
12:19:52 404  	   FROM
12:19:52 405  	     (SELECT rownum rnum, Q.*
12:19:52 406  	      FROM
12:19:52 407  	       (SELECT
12:19:52 408  		  ss.ID,
12:19:52 409  		  ss.GROUP_ACCOUNT_ID,
12:19:52 410  		  ss.BORROWER_ACCOUNT_ID,
12:19:52 411  		  ss.IP_ADDRESS,
12:19:52 412  		  ss.START_DATE,
12:19:52 413  		  ss.END_DATE,
12:19:52 414  		  ss.CREATED_BY,
12:19:52 415  		  ss.CREATE_DATE,
12:19:52 416  		  ss.UPDATED_BY,
12:19:52 417  		  ss.UPDATE_DATE,
12:19:52 418  		  a.GROUP_ID AS BORROWER_GROUP_ID
12:19:52 419  		FROM
12:19:52 420  		  SUBSCRIPTION_SHARE ss,
12:19:52 421  		  ACCOUNT a,
12:19:52 422  		  LICENSE l,
12:19:52 423  		  GROUP_ACCOUNT ga
12:19:52 424  		WHERE
12:19:52 425  		  ss.GROUP_ACCOUNT_ID = in_group_account_id AND
12:19:52 426  		  ss.GROUP_ACCOUNT_ID = ga.ID AND
12:19:52 427  		  GA.SUBSCRIPTION_ID = l.SUBSCRIPTION_ID AND
12:19:52 428  		  SYSDATE BETWEEN l.START_DATE AND l.ENTITLEMENT_END_DATE AND
12:19:52 429  		  SYSDATE BETWEEN ss.START_DATE AND ss.END_DATE AND
12:19:52 430  		  ss.BORROWER_ACCOUNT_ID  = a.ID
12:19:52 431  	      ) Q
12:19:52 432  	    WHERE rownum <= l_end)
12:19:52 433  	  WHERE rnum >= l_start;
12:19:52 434  	END;
12:19:52 435  EXCEPTION
