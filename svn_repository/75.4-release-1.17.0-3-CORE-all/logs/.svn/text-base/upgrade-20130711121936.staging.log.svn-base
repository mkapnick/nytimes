12:19:51 451  
12:19:51 452  PROCEDURE DISABLE_PAYPAL (
12:19:51 453  /*
12:19:51 454  Throws exceptions:
12:19:51 455  APP_EXCEPTION_CODES_V23.NOT_FOUND
12:19:51 456  APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR
12:19:51 457  APP_EXCEPTION_CODES_V23.INTERNAL_ERROR
12:19:51 458  */
12:19:51 459  	in_paypal_id  IN NUMBER,
12:19:51 460  	in_updated_by IN VARCHAR2
12:19:51 461  ) AS
12:19:51 462  SPROC_NAME CONSTANT VARCHAR2(14) := 'DISABLE_PAYPAL';
12:19:51 463  -- VARIABLES
12:19:51 464  var_paypal_status_id NUMBER;
12:19:51 465  var_account_id	   NUMBER;
12:19:51 466  var_group_id	   NUMBER;
12:19:51 467  var_pending_invoices_num NUMBER;
12:19:51 468  current_def_instrument_type_id NUMBER;
12:19:51 469  current_def_instrument_id NUMBER;
12:19:51 470  -- EXCEPTIONS
12:19:51 471  BAD_PAYPAL_ID EXCEPTION;
12:19:51 472  PAYPAL_ALREADY_INACTIVE EXCEPTION;
12:19:51 473  PENDING_TRANSACTIONS_FOUND EXCEPTION;
12:19:51 474  CAN_NOT_GET_DEF_FINANCIAL EXCEPTION;
12:19:51 475  CAN_NOT_DEL_DEF_FINANCIAL EXCEPTION;
12:19:51 476  EXCEPTION_MESSAGE VARCHAR2(1024);
12:19:51 477  BEGIN
12:19:51 478  
12:19:51 479  	-- Get Paypal status
12:19:51 480  	-- Get account
12:19:51 481  	BEGIN
12:19:51 482  	  SELECT
12:19:51 483  	    PAYPAL.PAYPAL_STATUS_ID,
12:19:51 484  	    PAYPAL.ACCOUNT_ID
12:19:51 485  	    into
12:19:51 486  	    var_paypal_status_id,
12:19:51 487  	    var_account_id
12:19:51 488  	  FROM
12:19:51 489  	    PAYPAL
12:19:51 490  	  WHERE
12:19:51 491  	    PAYPAL.ID = in_paypal_id;
12:19:51 492  	  EXCEPTION
12:19:51 493  	    WHEN NO_DATA_FOUND THEN
12:19:51 494  	      RAISE BAD_PAYPAL_ID;
12:19:51 495  	END;
12:19:51 496  
12:19:51 497  	-- Get group id
12:19:51 498  	SELECT
12:19:51 499  	  ACCOUNT.GROUP_ID into var_group_id
12:19:51 500  	FROM
12:19:51 501  	  ACCOUNT
12:19:51 502  	WHERE
12:19:51 503  	  ACCOUNT.ID = var_account_id;
12:19:51 504  
12:19:51 505  	-- Card should not be disabled
12:19:51 506  	IF var_paypal_status_id = GLOBAL_STATUSES_V23.PAYPAL_INACTIVE THEN
12:19:51 507  	  RAISE PAYPAL_ALREADY_INACTIVE;
12:19:51 508  	END IF;
12:19:51 509  
12:19:51 510  	-- Looking for pending transactions associated with given credit card
12:19:51 511  	SELECT
12:19:51 512  	  COUNT(*) into var_pending_invoices_num
12:19:51 513  	FROM
12:19:51 514  	  CHARGE
12:19:51 515  	  INNER JOIN TRANSACTION ON CHARGE.TRANSACTION_ID = TRANSACTION.ID
12:19:51 516  	WHERE
12:19:51 517  	  CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V23.INSTRUMENT_PAYPAL
12:19:51 518  	  AND CHARGE.INSTRUMENT_ID = in_paypal_id
12:19:51 519  	  AND TRANSACTION.TRANSACTION_STATUS_ID = GLOBAL_STATUSES_V23.TRANSACTION_PENDING;
12:19:51 520  
12:19:51 521  	IF var_pending_invoices_num > 0 THEN
12:19:51 522  	  RAISE PENDING_TRANSACTIONS_FOUND;
12:19:51 523  	END IF;
12:19:51 524  
12:19:51 525  	-- Getting current default financial instrument
12:19:51 526  	BEGIN
12:19:51 527  	  GET_DEF_FINANCIAL_INSTRUMENT(
12:19:51 528  	    in_group_id 	   => var_group_id,
12:19:51 529  	    out_instrument_type_id => current_def_instrument_type_id,
12:19:51 530  	    out_instrument_id	   => current_def_instrument_id
12:19:51 531  	  );
12:19:51 532  	  EXCEPTION
12:19:51 533  	    WHEN OTHERS THEN
12:19:51 534  	      EXCEPTION_MESSAGE := SQLERRM;
12:19:51 535  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
12:19:51 536  	END;
12:19:51 537  
12:19:51 538  	-- Checking that credit card is not default
12:19:51 539  	IF current_def_instrument_type_id = GLOBAL_ENUMS_V23.INSTRUMENT_PAYPAL
12:19:51 540  	  AND current_def_instrument_id = in_paypal_id THEN
12:19:51 541  	  BEGIN
12:19:51 542  	    DEL_DEF_FINANCIAL_INSTRUMENT(
12:19:51 543  	      in_group_id => var_group_id
12:19:51 544  	    );
12:19:51 545  	  EXCEPTION
12:19:51 546  	    WHEN OTHERS THEN
12:19:51 547  	      EXCEPTION_MESSAGE := SQLERRM;
12:19:51 548  	      RAISE CAN_NOT_DEL_DEF_FINANCIAL;
12:19:51 549  	  END;
12:19:51 550  	END IF;
12:19:51 551  
12:19:51 552  	PROCS_FIN_INSTRUMENTS_V23.UPDATE_PAYPAL_STATUS(
12:19:51 553  	  in_paypal_id	      => in_paypal_id,
12:19:51 554  	  in_updated_by       => in_updated_by,
12:19:51 555  	  in_paypal_status_id => GLOBAL_STATUSES_V23.PAYPAL_INACTIVE
12:19:51 556  	);
12:19:51 557  
12:19:51 558  EXCEPTION
12:19:51 559  WHEN BAD_PAYPAL_ID THEN
12:19:51 560  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 561  	  SPROC_NAME, 'No such paypal');
12:19:51 562  WHEN PAYPAL_ALREADY_INACTIVE THEN
12:19:51 563  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
12:19:51 564  	  SPROC_NAME, 'Paypal already inactive');
12:19:51 565  WHEN PENDING_TRANSACTIONS_FOUND THEN
12:19:51 566  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
12:19:51 567  	  SPROC_NAME, 'Account has pending charge which are using this paypal');
12:19:51 568  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
12:19:51 569  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 570  	  SPROC_NAME, 'Could not get current default financial instrument', EXCEPTION_MESSAGE);
12:19:51 571  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
12:19:51 572  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 573  	  SPROC_NAME, 'Could not delete information about default financial instrument from account', EXCEPTION_MESSAGE);
12:19:51 574  WHEN OTHERS THEN
12:19:51 575  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 576  	  SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 577  END DISABLE_PAYPAL;
12:19:51 578  
12:19:51 579  /******************************************************************************/
12:19:51 580  
12:19:51 581  PROCEDURE UPDATE_CREDIT_CARD (
12:19:51 582  /*
12:19:51 583  Throws exceptions:
12:19:51 584  APP_EXCEPTION_CODES_V23.NOT_FOUND
12:19:51 585  APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR
12:19:51 586  */
12:19:51 587  	in_credit_card_id	  IN NUMBER,
12:19:51 588  	in_updated_by		  IN VARCHAR2,
12:19:51 589  	in_instrument_name	  IN VARCHAR2,
12:19:51 590  	in_is_default		  IN NUMBER
12:19:51 591  ) AS
12:19:51 592  SPROC_NAME CONSTANT VARCHAR2(18) := 'UPDATE_CREDIT_CARD';
12:19:51 593  -- VARIABLES
12:19:51 594  var_account_id NUMBER;
12:19:51 595  var_group_id   NUMBER;
12:19:51 596  temp_cc_rownum NUMBER;
12:19:51 597  current_def_instrument_type_id NUMBER;
12:19:51 598  current_def_instrument_id      NUMBER;
12:19:51 599  -- EXCEPTION
12:19:51 600  CAN_NOT_SET_DEF_FINANCIAL  EXCEPTION;
12:19:51 601  BAD_IS_DEFAULT_VALUE	 EXCEPTION;
12:19:51 602  CAN_NOT_GET_DEF_FINANCIAL  EXCEPTION;
12:19:51 603  CAN_NOT_DEL_DEF_FINANCIAL  EXCEPTION;
12:19:51 604  EXCEPTION_MESSAGE 	 VARCHAR2(1024);
12:19:51 605  BEGIN
12:19:51 606  
12:19:51 607  	-- Get account id
12:19:51 608  	SELECT
12:19:51 609  	  CREDIT_CARD.ACCOUNT_ID
12:19:51 610  	  into
12:19:51 611  	  var_account_id
12:19:51 612  	FROM
12:19:51 613  	  CREDIT_CARD
12:19:51 614  	WHERE
12:19:51 615  	  CREDIT_CARD.ID = in_credit_card_id;
12:19:51 616  
12:19:51 617  	-- Get group id
12:19:51 618  	SELECT
12:19:51 619  	  ACCOUNT.GROUP_ID into var_group_id
12:19:51 620  	FROM
12:19:51 621  	  ACCOUNT
12:19:51 622  	WHERE
12:19:51 623  	  ACCOUNT.ID = var_account_id;
12:19:51 624  
12:19:51 625  	-- Check that passed data is correct
12:19:51 626  	IF in_is_default != GLOBAL_CONSTANTS_V23.TRUE
12:19:51 627  	  AND in_is_default != GLOBAL_CONSTANTS_V23.FALSE
12:19:51 628  	  AND in_is_default IS NOT NULL THEN
12:19:51 629  	  RAISE BAD_IS_DEFAULT_VALUE;
12:19:51 630  	END IF;
12:19:51 631  
12:19:51 632  	-- Update credit card
12:19:51 633  	IF in_instrument_name IS NOT NULL THEN
12:19:51 634  	  PROCS_FIN_INSTRUMENTS_CRU_V23.UPDATE_CREDIT_CARD(
12:19:51 635  	    in_credit_card_id  => in_credit_card_id,
12:19:51 636  	    in_updated_by      => in_updated_by,
12:19:51 637  	    in_instrument_name => in_instrument_name
12:19:51 638  	  );
12:19:51 639  	END IF;
12:19:51 640  
12:19:51 641  	-- Set default financial instrument
12:19:51 642  	IF in_is_default = GLOBAL_CONSTANTS_V23.TRUE THEN
12:19:51 643  	  BEGIN
12:19:51 644  	    PROCS_FIN_INSTRUMENTS_V23.SET_DEF_FINANCIAL_INSTRUMENT(
12:19:51 645  	      in_group_id	    => var_group_id,
12:19:51 646  	      in_instrument_type_id => GLOBAL_ENUMS_V23.INSTRUMENT_CREDIT_CARD,
12:19:51 647  	      in_instrument_id	    => in_credit_card_id,
12:19:51 648  	      in_updated_by	    => in_updated_by
12:19:51 649  	    );
12:19:51 650  	    EXCEPTION
12:19:51 651  	      WHEN OTHERS THEN
12:19:51 652  		EXCEPTION_MESSAGE := SQLERRM;
12:19:51 653  		RAISE CAN_NOT_SET_DEF_FINANCIAL;
12:19:51 654  	  END;
12:19:51 655  	END IF;
12:19:51 656  
12:19:51 657  	-- Set default financial instrument
12:19:51 658  	IF in_is_default = GLOBAL_CONSTANTS_V23.FALSE THEN
12:19:51 659  	  BEGIN
12:19:51 660  	    GET_DEF_FINANCIAL_INSTRUMENT(
12:19:51 661  	      in_group_id	     => var_group_id,
12:19:51 662  	      out_instrument_type_id => current_def_instrument_type_id,
12:19:51 663  	      out_instrument_id      => current_def_instrument_id
12:19:51 664  	    );
12:19:51 665  	  EXCEPTION
12:19:51 666  	    WHEN OTHERS THEN
12:19:51 667  	      EXCEPTION_MESSAGE := SQLERRM;
12:19:51 668  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
12:19:51 669  	  END;
12:19:51 670  	  IF current_def_instrument_type_id = GLOBAL_ENUMS_V23.INSTRUMENT_CREDIT_CARD
12:19:51 671  	    AND current_def_instrument_id = in_credit_card_id THEN
12:19:51 672  	    BEGIN
12:19:51 673  	      DEL_DEF_FINANCIAL_INSTRUMENT(
12:19:51 674  		in_group_id => var_group_id
12:19:51 675  	      );
12:19:51 676  	      EXCEPTION
12:19:51 677  		WHEN OTHERS THEN
12:19:51 678  		  EXCEPTION_MESSAGE := SQLERRM;
12:19:51 679  		  RAISE CAN_NOT_DEL_DEF_FINANCIAL;
12:19:51 680  	    END;
12:19:51 681  	  END IF;
12:19:51 682  	END IF;
12:19:51 683  
12:19:51 684  EXCEPTION
12:19:51 685  WHEN NO_DATA_FOUND THEN
12:19:51 686  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 687  	  SPROC_NAME, 'No such credit card');
12:19:51 688  WHEN CAN_NOT_SET_DEF_FINANCIAL THEN
12:19:51 689  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 690  	  SPROC_NAME, 'Could not set default financial instrument for account', EXCEPTION_MESSAGE);
12:19:51 691  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
12:19:51 692  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 693  	  SPROC_NAME, 'Could not get default financial instrument for account', EXCEPTION_MESSAGE);
12:19:51 694  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
12:19:51 695  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 696  	  SPROC_NAME, 'Could not delete information about default financial instrument', EXCEPTION_MESSAGE);
12:19:51 697  WHEN OTHERS THEN
12:19:51 698  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 699  	  SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 700  END UPDATE_CREDIT_CARD;
