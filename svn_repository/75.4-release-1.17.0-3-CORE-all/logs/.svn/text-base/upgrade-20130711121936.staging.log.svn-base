12:19:51 1821  -- VARIABLES
12:19:51 1822  SPROC_NAME		CONSTANT VARCHAR2(28) := 'GET_GIFT_CERTIFICATE_BY_CODE';
12:19:51 1823  temp_gift_certificate_id NUMBER;
12:19:51 1824  -- EXCEPTIONS
12:19:51 1825  BAD_GIFT_CERTIFICATE_CODE EXCEPTION;
12:19:51 1826  BEGIN
12:19:51 1827  
12:19:51 1828  	 BEGIN
