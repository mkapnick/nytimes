12:19:51 1111  	 END;
12:19:51 1112  
12:19:51 1113  	 SELECT
12:19:51 1114  	   OFFER.ENTITLEMENT_DURATION into var_offer_duration
12:19:51 1115  	 FROM
12:19:51 1116  	   LICENSE
12:19:51 1117  	   INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
12:19:51 1118  	   INNER JOIN OFFER ON LICENSE.OFFER_ID = OFFER.ID
12:19:51 1119  	 WHERE
12:19:51 1120  	   --LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V23.LICENSE_PROCESSED
12:19:51 1121  	   --AND
12:19:51 1122  	   SUBSCRIPTION.ID = in_subscription_id
12:19:51 1123  	   AND ROWNUM <= 1
12:19:51 1124  	 ORDER BY
12:19:51 1125  	   LICENSE.ID ASC;
12:19:51 1126  
12:19:51 1127  	 RETURN var_offer_duration;
12:19:51 1128  
12:19:51 1129  EXCEPTION
