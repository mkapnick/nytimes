12:19:51 919  
12:19:51 920  FUNCTION GET_RENEWAL_DATE (
12:19:51 921  	in_subscription_id in NUMBER
12:19:51 922  ) RETURN DATE AS
12:19:51 923  -- VARIABLES
12:19:51 924  SPROC_NAME	      CONSTANT VARCHAR2(16) := 'GET_RENEWAL_DATE';
12:19:51 925  var_subscription_status NUMBER;
12:19:51 926  var_licenses_count      NUMBER;
12:19:51 927  var_license_end_date    DATE;
12:19:51 928  var_last_offer_id       NUMBER;
12:19:51 929  var_offer_chain_id      NUMBER;
12:19:51 930  var_last_license_id     NUMBER;
12:19:51 931  var_current_offer_index NUMBER;
12:19:51 932  var_current_offer_recurr_num NUMBER;
12:19:51 933  var_offer_recurr_num    NUMBER;
12:19:51 934  -- EXCEPTIONS
12:19:51 935  BAD_SUBSCRIPTION_ID EXCEPTION;
12:19:51 936  NO_LICENSES_FOUND EXCEPTION;
12:19:51 937  BEGIN
12:19:51 938  
12:19:51 939  	-- Get subscription id and offer chain id
12:19:51 940  	BEGIN
12:19:51 941  	  SELECT
12:19:51 942  	    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID,
12:19:51 943  	    SUBSCRIPTION.OFFER_CHAIN_ID
12:19:51 944  	    into
12:19:51 945  	    var_subscription_status,
12:19:51 946  	    var_offer_chain_id
12:19:51 947  	  FROM
12:19:51 948  	    SUBSCRIPTION
12:19:51 949  	  WHERE
12:19:51 950  	    SUBSCRIPTION.ID = in_subscription_id;
12:19:51 951  	  EXCEPTION
12:19:51 952  	    WHEN NO_DATA_FOUND THEN
12:19:51 953  	      RAISE BAD_SUBSCRIPTION_ID;
12:19:51 954  	END;
12:19:51 955  
12:19:51 956  	IF var_subscription_status != GLOBAL_STATUSES_V23.SUBSCRIPTION_ACTIVE THEN
12:19:51 957  	  -- TODO: Is suspended subscription has renewal date? (For the phase II)
12:19:51 958  	  -- AND var_subscription_status != GLOBAL_STATUSES_V23.SUBSCRIPTION_SUSPENDED THEN
12:19:51 959  	  RETURN NULL;
12:19:51 960  	END IF;
12:19:51 961  
12:19:51 962  	BEGIN
12:19:51 963  	  SELECT
12:19:51 964  	    LICENSE_ID,
12:19:51 965  	    END_DATE,
12:19:51 966  	    OFFER_ID,
12:19:51 967  	    CURRENT_OFFER_INDEX,
12:19:51 968  	    CURRENT_OFFER_RECURR_NUM
12:19:51 969  	    into
12:19:51 970  	    var_last_license_id,
12:19:51 971  	    var_license_end_date,
12:19:51 972  	    var_last_offer_id,
12:19:51 973  	    var_current_offer_index,
12:19:51 974  	    var_current_offer_recurr_num
12:19:51 975  	  FROM
12:19:51 976  	    (
12:19:51 977  	      SELECT
12:19:51 978  		LICENSE.ID as "LICENSE_ID",
12:19:51 979  		LICENSE.END_DATE,
12:19:51 980  		LICENSE.OFFER_ID,
12:19:51 981  		LICENSE.CURRENT_OFFER_INDEX,
12:19:51 982  		LICENSE.CURRENT_OFFER_RECURR_NUM
12:19:51 983  	      FROM
12:19:51 984  		LICENSE
12:19:51 985  	      WHERE
12:19:51 986  		LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V23.LICENSE_ACTIVE
12:19:51 987  		AND LICENSE.SUBSCRIPTION_ID = in_subscription_id
12:19:51 988  	      ORDER BY END_DATE DESC
12:19:51 989  	    )
12:19:51 990  	    INNER JOIN OFFER ON OFFER_ID = OFFER.ID
12:19:51 991  	  WHERE
12:19:51 992  	    ROWNUM <= 1;
12:19:51 993  
12:19:51 994  	  EXCEPTION
12:19:51 995  	    WHEN NO_DATA_FOUND THEN
12:19:51 996  	      -- RAISE NO_LICENSES_FOUND;
12:19:51 997  	      RETURN NULL;
12:19:51 998  	END;
12:19:51 999  
12:19:51 1000  	 SELECT
12:19:51 1001  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES into var_offer_recurr_num
12:19:51 1002  	 FROM
12:19:51 1003  	   OFFER_OFFER_CHAIN
12:19:51 1004  	 WHERE
12:19:51 1005  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
12:19:51 1006  	   AND OFFER_OFFER_CHAIN.OFFER_ID = var_last_offer_id;
12:19:51 1007  
12:19:51 1008  	 IF PROCS_OFFER_CHAIN_V23.GET_NEXT_OFFER_INDEX(var_offer_chain_id, var_current_offer_index) IS NULL
12:19:51 1009  	   AND var_offer_recurr_num = var_current_offer_recurr_num THEN
12:19:51 1010  	   -- There is no next offer for this subscription
12:19:51 1011  	   RETURN NULL;
12:19:51 1012  	 END IF;
12:19:51 1013  
12:19:51 1014  	 RETURN var_license_end_date;
12:19:51 1015  
12:19:51 1016  EXCEPTION
12:19:51 1017  WHEN BAD_SUBSCRIPTION_ID THEN
12:19:51 1018  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 1019  	   SPROC_NAME, 'No such subscription');
12:19:51 1020  WHEN NO_LICENSES_FOUND THEN
12:19:51 1021  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 1022  	   SPROC_NAME, 'No licenses found');
12:19:51 1023  WHEN OTHERS THEN
12:19:51 1024  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 1025  	   SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 1026  END GET_RENEWAL_DATE;
12:19:51 1027  
12:19:51 1028  /******************************************************************************/
12:19:51 1029  
12:19:51 1030  FUNCTION GET_RECENT_CHARGE (
12:19:51 1031  	 in_subscription_id IN NUMBER
12:19:51 1032  ) RETURN NUMBER AS
12:19:51 1033  -- VARIABLES
12:19:51 1034  SPROC_NAME	    CONSTANT VARCHAR2(17) := 'GET_RECENT_CHARGE';
12:19:51 1035  temp_subscription_id NUMBER;
12:19:51 1036  var_recent_charge    NUMBER(10,2);
12:19:51 1037  
12:19:51 1038  -- EXCEPTIONS
12:19:51 1039  BAD_SUBSCRIPTION_ID EXCEPTION;
12:19:51 1040  BEGIN
12:19:51 1041  
12:19:51 1042  	 BEGIN
12:19:51 1043  	   SELECT
12:19:51 1044  	     SUBSCRIPTION.ID into temp_subscription_id
12:19:51 1045  	   FROM
12:19:51 1046  	     SUBSCRIPTION
12:19:51 1047  	   WHERE
12:19:51 1048  	     SUBSCRIPTION.ID = in_subscription_id;
12:19:51 1049  	   EXCEPTION
12:19:51 1050  	     WHEN NO_DATA_FOUND THEN
12:19:51 1051  	       RAISE BAD_SUBSCRIPTION_ID;
12:19:51 1052  	 END;
12:19:51 1053  
12:19:51 1054  	 BEGIN
12:19:51 1055  	   SELECT
12:19:51 1056  	     CHARGE.CHARGE_AMOUNT into var_recent_charge
12:19:51 1057  	   FROM
12:19:51 1058  	     LICENSE
12:19:51 1059  	     INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
12:19:51 1060  	     INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
12:19:51 1061  	     INNER JOIN CHARGE ON CHARGE.INVOICE_ID = INVOICE.ID
12:19:51 1062  	   WHERE
12:19:51 1063  	     -- TODO: Review
12:19:51 1064  	     -- LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V23.LICENSE_PROCESSED
12:19:51 1065  	     -- AND
12:19:51 1066  	     SUBSCRIPTION.ID = in_subscription_id
12:19:51 1067  	     AND CHARGE.CHARGE_AMOUNT >= 0
12:19:51 1068  	     AND ROWNUM <= 1
12:19:51 1069  	   ORDER BY
12:19:51 1070  	     LICENSE.ID ASC, CHARGE.ID DESC;
12:19:51 1071  	   EXCEPTION
12:19:51 1072  	     WHEN NO_DATA_FOUND THEN
12:19:51 1073  	       var_recent_charge := 0;
12:19:51 1074  	 END;
12:19:51 1075  
12:19:51 1076  	 RETURN var_recent_charge;
12:19:51 1077  
12:19:51 1078  EXCEPTION
12:19:51 1079  WHEN BAD_SUBSCRIPTION_ID THEN
12:19:51 1080  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 1081  	   SPROC_NAME, 'No such subscription');
12:19:51 1082  WHEN OTHERS THEN
12:19:51 1083  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 1084  	   SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 1085  END GET_RECENT_CHARGE;
12:19:51 1086  
12:19:51 1087  /******************************************************************************/
12:19:51 1088  
12:19:51 1089  FUNCTION GET_BILLING_CYCLE (
12:19:51 1090  	 in_subscription_id IN NUMBER
12:19:51 1091  ) RETURN VARCHAR2 AS
12:19:51 1092  -- VARIABLES
12:19:51 1093  SPROC_NAME	    CONSTANT VARCHAR2(17) := 'GET_BILLING_CYCLE';
12:19:51 1094  temp_subscription_id NUMBER;
12:19:51 1095  var_offer_duration   VARCHAR2(30);
12:19:51 1096  
12:19:51 1097  -- EXCEPTIONS
12:19:51 1098  BAD_SUBSCRIPTION_ID EXCEPTION;
12:19:51 1099  BEGIN
12:19:51 1100  
12:19:51 1101  	 BEGIN
12:19:51 1102  	   SELECT
12:19:51 1103  	     SUBSCRIPTION.ID into temp_subscription_id
12:19:51 1104  	   FROM
12:19:51 1105  	     SUBSCRIPTION
12:19:51 1106  	   WHERE
12:19:51 1107  	     SUBSCRIPTION.ID = in_subscription_id;
12:19:51 1108  	   EXCEPTION
