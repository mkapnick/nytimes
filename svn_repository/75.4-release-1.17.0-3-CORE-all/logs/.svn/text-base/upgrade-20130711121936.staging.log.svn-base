12:19:51 121  PROCEDURE LOCK_ACCOUNT (
12:19:51 122  	in_group_id    IN NUMBER,
12:19:51 123  	in_lock_key    IN VARCHAR2,
12:19:51 124  	in_seconds_num IN NUMBER,
12:19:51 125  	in_created_by  IN VARCHAR2,
12:19:51 126  	in_reason      IN VARCHAR2
12:19:51 127  ) AS
12:19:51 128  SPROC_NAME CONSTANT VARCHAR2(12) := 'LOCK_ACCOUNT';
12:19:51 129  -- CONSTANTS
12:19:51 130  one_second_interval CONSTANT INTERVAL DAY TO SECOND := INTERVAL '0 00:00:01' DAY TO SECOND;
12:19:51 131  -- VARIABLES
12:19:51 132  var_account_id NUMBER;
12:19:51 133  -- arosolovskiy: using data variable instead of direct get data calls (#38860)
12:19:51 134  var_date		DATE := SYSDATE;
12:19:51 135  var_lock_end_date DATE;
12:19:51 136  -- EXCEPTIONS
12:19:51 137  BAD_GROUP_ID   EXCEPTION;
12:19:51 138  ALREADY_LOCKED EXCEPTION;
12:19:51 139  BEGIN
12:19:51 140  
12:19:51 141  	BEGIN
12:19:51 142  	  SELECT
12:19:51 143  	    ACCOUNT.ID into var_account_id
12:19:51 144  	  FROM
12:19:51 145  	    ACCOUNT
12:19:51 146  	  WHERE
12:19:51 147  	    ACCOUNT.GROUP_ID = in_group_id;
12:19:51 148  	  EXCEPTION
12:19:51 149  	    WHEN NO_DATA_FOUND THEN
12:19:51 150  	      RAISE BAD_GROUP_ID;
12:19:51 151  	END;
12:19:51 152  
12:19:51 153  	var_lock_end_date := var_date + ( in_seconds_num * one_second_interval );
12:19:51 154  
12:19:51 155  	BEGIN
12:19:51 156  
12:19:51 157  	  INSERT INTO ACCOUNT_LOCK (
12:19:51 158  	    ACCOUNT_ID,
12:19:51 159  	    LOCK_KEY,
12:19:51 160  	    END_DATE,
12:19:51 161  	    CREATED_BY,
12:19:51 162  	    REASON
12:19:51 163  	  ) VALUES (
12:19:51 164  	    var_account_id,
12:19:51 165  	    in_lock_key,
12:19:51 166  	    var_lock_end_date,
12:19:51 167  	    in_created_by,
12:19:51 168  	    in_reason
12:19:51 169  	  );
12:19:51 170  
12:19:51 171  	  EXCEPTION
12:19:51 172  	    WHEN DUP_VAL_ON_INDEX THEN
12:19:51 173  	      -- This rows was created before... I will try to update it
12:19:51 174  	      BEGIN
12:19:51 175  
12:19:51 176  		UPDATE
12:19:51 177  		  ACCOUNT_LOCK
12:19:51 178  		SET
12:19:51 179  		  ACCOUNT_LOCK.LOCK_KEY = in_lock_key,
12:19:51 180  		  ACCOUNT_LOCK.END_DATE = var_lock_end_date,
12:19:51 181  		  ACCOUNT_LOCK.CREATED_BY = in_created_by,
12:19:51 182  		  ACCOUNT_LOCK.REASON = in_reason
12:19:51 183  		WHERE
12:19:51 184  		  ACCOUNT_LOCK.ACCOUNT_ID = var_account_id
12:19:51 185  		  AND ACCOUNT_LOCK.END_DATE <= var_date;
12:19:51 186  
12:19:51 187  		IF SQL%ROWCOUNT = 0 THEN
12:19:51 188  		  RAISE ALREADY_LOCKED;
12:19:51 189  		END IF;
12:19:51 190  
12:19:51 191  	      END;
12:19:51 192  	END;
12:19:51 193  
12:19:51 194  EXCEPTION
12:19:51 195  WHEN BAD_GROUP_ID THEN
12:19:51 196  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 197  	  SPROC_NAME, 'No such group id');
12:19:51 198  WHEN ALREADY_LOCKED THEN
12:19:51 199  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
12:19:51 200  	  SPROC_NAME, 'Account already locked');
12:19:51 201  WHEN OTHERS THEN
12:19:51 202  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 203  	  SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 204  END LOCK_ACCOUNT;
