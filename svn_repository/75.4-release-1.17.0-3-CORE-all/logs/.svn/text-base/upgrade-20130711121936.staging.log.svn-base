12:19:51 2731  PROCEDURE ARE_REFUNDS_PENDING_FOR_SUBSCR (
12:19:51 2732  	 in_subscription_id IN NUMBER,
12:19:51 2733  	 out_result	    OUT NUMBER
12:19:51 2734  ) AS
12:19:51 2735  SPROC_NAME CONSTANT VARCHAR2(30) := 'ARE_REFUNDS_PENDING_FOR_SUBSCR';
12:19:51 2736  -- VARIABLES
12:19:51 2737  temp_subscription_id NUMBER;
12:19:51 2738  var_local_result     NUMBER;
12:19:51 2739  -- EXCEPTIONS
12:19:51 2740  BAD_SUBSCRIPTION_ID EXCEPTION;
12:19:51 2741  BEGIN
12:19:51 2742  
12:19:51 2743  	 -- Check that subscription exists
12:19:51 2744  	 BEGIN
12:19:51 2745  	   SELECT
12:19:51 2746  	     SUBSCRIPTION.ID into temp_subscription_id
12:19:51 2747  	   FROM
12:19:51 2748  	     SUBSCRIPTION
12:19:51 2749  	   WHERE
12:19:51 2750  	     SUBSCRIPTION.ID = in_subscription_id;
12:19:51 2751  	   EXCEPTION
12:19:51 2752  	     WHEN NO_DATA_FOUND THEN
12:19:51 2753  	       RAISE BAD_SUBSCRIPTION_ID;
12:19:51 2754  	 END;
12:19:51 2755  
12:19:51 2756  	 var_local_result := NULL;
12:19:51 2757  
12:19:51 2758  	 -- Check charges for each invoice associated with gived subscription
12:19:51 2759  	 FOR f_invoice IN (
12:19:51 2760  	   SELECT DISTINCT
12:19:51 2761  	     LICENSE.INVOICE_ID as "ID"
12:19:51 2762  	   FROM
12:19:51 2763  	     LICENSE
12:19:51 2764  	   WHERE
12:19:51 2765  	     LICENSE.SUBSCRIPTION_ID = in_subscription_id
12:19:51 2766  	     AND LICENSE.LICENSE_STATUS_ID IN ( SELECT GLOBAL_STATUSES_V23.LICENSE_ACTIVE FROM DUAL )
12:19:51 2767  	 )
12:19:51 2768  	 LOOP
12:19:51 2769  
12:19:51 2770  	   -- Check each charge in invoice
12:19:51 2771  	   FOR f_charge IN (
12:19:51 2772  	     SELECT
12:19:51 2773  	       CHARGE.ID,
12:19:51 2774  	       CHARGE.CHARGE_STATUS_ID,
12:19:51 2775  	       CHARGE.CHARGE_AMOUNT
12:19:51 2776  	     FROM
12:19:51 2777  	       CHARGE
12:19:51 2778  	     WHERE
12:19:51 2779  	       CHARGE.INVOICE_ID = f_invoice.ID
12:19:51 2780  	   )
12:19:51 2781  	   LOOP
12:19:51 2782  
12:19:51 2783  	     -- Charge amount < 0     => it is a refund
12:19:51 2784  	     -- Charge status is OPEN => means that it is not processed yet
12:19:51 2785  	     IF f_charge.CHARGE_AMOUNT < 0
12:19:51 2786  		AND f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V23.CHARGE_OPENED THEN
12:19:51 2787  	       var_local_result := GLOBAL_CONSTANTS_V23.TRUE;
12:19:51 2788  	     END IF;
12:19:51 2789  
12:19:51 2790  	   END LOOP;
12:19:51 2791  
12:19:51 2792  	 END LOOP;
12:19:51 2793  
12:19:51 2794  	 IF var_local_result IS NULL THEN
12:19:51 2795  	   out_result := GLOBAL_CONSTANTS_V23.FALSE;
12:19:51 2796  	 ELSE
12:19:51 2797  	   out_result := GLOBAL_CONSTANTS_V23.TRUE;
12:19:51 2798  	 END IF;
12:19:51 2799  
12:19:51 2800  EXCEPTION
12:19:51 2801  WHEN BAD_SUBSCRIPTION_ID THEN
12:19:51 2802  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 2803  	   SPROC_NAME, 'No such transaction id');
12:19:51 2804  WHEN OTHERS THEN
12:19:51 2805  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 2806  	   SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 2807  END ARE_REFUNDS_PENDING_FOR_SUBSCR;
