12:19:52 430  	  GIFT_CERTIFICATE.PURCHASER_GROUP_ID = in_group_id
12:19:52 431  	  AND (
12:19:52 432  		( in_instr_status = GLOBAL_CONSTANTS_V23.TRUE AND
12:19:52 433  		  (
12:19:52 434  		    GIFT_CERTIFICATE.EXPIRATION_DATE >= current_date
12:19:52 435  		    AND GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID = GLOBAL_STATUSES_V23.GIFT_CERTIFICATE_ACTIVE
12:19:52 436  		  )
12:19:52 437  		)
12:19:52 438  		OR
12:19:52 439  		(
12:19:52 440  		  in_instr_status = GLOBAL_CONSTANTS_V23.FALSE AND
12:19:52 441  		  (
12:19:52 442  		    GIFT_CERTIFICATE.EXPIRATION_DATE < current_date
12:19:52 443  		    OR GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID != GLOBAL_STATUSES_V23.GIFT_CERTIFICATE_ACTIVE
12:19:52 444  		  )
12:19:52 445  		)
12:19:52 446  	      );
12:19:52 447  
12:19:52 448  EXCEPTION
