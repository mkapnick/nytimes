12:19:51 853  		    in_created_by => in_created_by
12:19:51 854  		);
12:19:51 855  	      END LOOP;
12:19:51 856  	      END LOOP;
12:19:51 857  
12:19:51 858  	      EXCEPTION
12:19:51 859  	      WHEN OTHERS THEN
12:19:51 860  	      EXCEPTION_MESSAGE := SQLERRM;
12:19:51 861  	      RAISE CAN_NOT_CREATE_LINE_ITEMS;
12:19:51 862  	    END;
12:19:51 863  
12:19:51 864  	    BEGIN
12:19:51 865  	      PROCS_FIN_INSTRUMENTS_CRU_V23.CREATE_GIFT_CERTIFICATE(
12:19:51 866  		  out_gift_certificate_id	=> var_gift_cert_id,
12:19:51 867  		  in_purchaser_group_id 	=> in_group_id,
12:19:51 868  		  in_purchaser_invoice_id	=> var_new_invoice_id,
12:19:51 869  		  in_offer_chain_id		=> in_offer_chain_id,
12:19:51 870  		  in_expiration_date		=> in_expiration_date,
12:19:51 871  		  in_purchase_date		=> SYSDATE,
12:19:51 872  		  in_gift_certificate_status_id => GLOBAL_STATUSES_V23.GIFT_CERTIFICATE_ACTIVE,
12:19:51 873  		  in_code			=> in_gift_certificate_code,
12:19:51 874  		  in_created_by 		=> in_created_by,
12:19:51 875  		  in_recipient_name		=> in_recipient_name,
12:19:51 876  		  in_gift_message		=> in_gift_message,
12:19:51 877  		  in_recipient_email		=> in_recipient_email,
12:19:51 878  		  in_sender_email		=> in_sender_email,
12:19:51 879  		  in_sender_name		=> in_sender_name,
12:19:51 880  		  in_recipient_address_id	=> in_recipient_address_id,
12:19:51 881  		  in_recipient_notify_date	=> in_recipient_notify_date,
12:19:51 882  		  in_campaign			=> in_campaign,
12:19:51 883  		  in_reference_code		=> in_reference_code
12:19:51 884  	      );
12:19:51 885  	      EXCEPTION WHEN DUP_VAL_ON_INDEX THEN RAISE REF_CODE_ALREADY_EXISTS;
12:19:51 886  	    END;
12:19:51 887  
12:19:51 888  	    out_gift_certificate_id := var_gift_cert_id;
12:19:51 889  	    out_invoice_id := var_new_invoice_id;
12:19:51 890  
12:19:51 891  	    EXCEPTION
12:19:51 892  	    WHEN BAD_OFFER_CHAIN_STATUS THEN
12:19:51 893  	    PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
12:19:51 894  					     SPROC_NAME, 'Offer chain is not active');
