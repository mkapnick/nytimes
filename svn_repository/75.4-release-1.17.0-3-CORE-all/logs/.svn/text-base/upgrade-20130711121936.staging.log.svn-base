12:19:51 1658  
12:19:51 1659  PROCEDURE REFUND_INVOICE (
12:19:51 1660  	 in_invoice_id	    IN NUMBER,
12:19:51 1661  	 in_refund_amount   IN NUMBER,
12:19:51 1662  	 in_note	    IN VARCHAR2,
12:19:51 1663  	 in_created_by	    IN VARCHAR2,
12:19:51 1664  	 out_charge_id	    OUT NUMBER
12:19:51 1665  ) AS
12:19:51 1666  SPROC_NAME CONSTANT VARCHAR2(14) := 'REFUND_INVOICE';
12:19:51 1667  -- VARIABLES
12:19:51 1668  var_invoice_status_id  NUMBER;
12:19:51 1669  var_subscription_id    NUMBER;
12:19:51 1670  var_account_id	      NUMBER;
12:19:51 1671  var_group_id	      NUMBER;
12:19:51 1672  var_account_status_id  NUMBER;
12:19:51 1673  var_new_transaction_id NUMBER;
12:19:51 1674  var_instrument_type_id NUMBER;
12:19:51 1675  var_instrument_id      NUMBER;
12:19:51 1676  var_new_charge_id      NUMBER;
12:19:51 1677  var_invoice_amount     NUMBER(10,2);
12:19:51 1678  var_refunds_before     NUMBER(10,2);
12:19:51 1679  var_charges_amount     NUMBER(10,2);
12:19:51 1680  -- EXCEPTIONS
12:19:51 1681  CAN_NOT_FIND_SUBSCR_OR_GC     EXCEPTION;
12:19:51 1682  ACCOUNT_IS_FROZEN	     EXCEPTION;
12:19:51 1683  BAD_INVOICE_ID		     EXCEPTION;
12:19:51 1684  CAN_NOT_CREATE_TRANSACTION    EXCEPTION;
12:19:51 1685  CAN_NOT_CREATE_CHARGE	     EXCEPTION;
12:19:51 1686  CAN_NOT_CALC_INVOICE_AMOUNT   EXCEPTION;
12:19:51 1687  REFUND_IS_GREATER_THAN_ANOUNT EXCEPTION;
12:19:51 1688  CAN_NOT_ANNOTATE_SUBSCRIPTION EXCEPTION;
12:19:51 1689  TOT_REF_IS_GREATER_THAN_ANOUNT EXCEPTION;
12:19:51 1690  INVOICE_IS_NOT_CLOSED	     EXCEPTION;
12:19:51 1691  TOT_REF_IS_GRATER_THAN_CHARGES EXCEPTION;
12:19:51 1692  EXCEPTION_MESSAGE	      VARCHAR2(1024);
12:19:51 1693  BEGIN
12:19:51 1694  
12:19:51 1695  	 BEGIN
12:19:51 1696  	   SELECT
12:19:51 1697  	     INVOICE.INVOICE_STATUS_ID into var_invoice_status_id
12:19:51 1698  	   FROM
12:19:51 1699  	     INVOICE
12:19:51 1700  	   WHERE
12:19:51 1701  	     INVOICE.ID = in_invoice_id;
12:19:51 1702  	   EXCEPTION
12:19:51 1703  	     WHEN NO_DATA_FOUND THEN
12:19:51 1704  	       RAISE BAD_INVOICE_ID;
12:19:51 1705  	 END;
12:19:51 1706  
12:19:51 1707  	 -- Get instrument and subscription id if exists
12:19:51 1708  	 BEGIN
12:19:51 1709  	   SELECT
12:19:51 1710  	     SUBSCRIPTION.INSTRUMENT_ID,
12:19:51 1711  	     SUBSCRIPTION.INSTRUMENT_TYPE_ID,
12:19:51 1712  	     SUBSCRIPTION.ACCOUNT_ID,
12:19:51 1713  	     SUBSCRIPTION.ID
12:19:51 1714  	     into
12:19:51 1715  	     var_instrument_id,
12:19:51 1716  	     var_instrument_type_id,
12:19:51 1717  	     var_account_id,
12:19:51 1718  	     var_subscription_id
12:19:51 1719  	   FROM
12:19:51 1720  	     SUBSCRIPTION
12:19:51 1721  	     INNER JOIN LICENSE ON SUBSCRIPTION.ID = LICENSE.SUBSCRIPTION_ID
12:19:51 1722  	   WHERE
12:19:51 1723  	     LICENSE.INVOICE_ID = in_invoice_id
12:19:51 1724  	     AND ROWNUM <= 1;
12:19:51 1725  	   EXCEPTION
12:19:51 1726  	     WHEN NO_DATA_FOUND THEN
12:19:51 1727  	       BEGIN
12:19:51 1728  
12:19:51 1729  		 var_subscription_id := NULL;
12:19:51 1730  
12:19:51 1731  		 SELECT
12:19:51 1732  		   CHARGE.INSTRUMENT_ID,
12:19:51 1733  		   CHARGE.INSTRUMENT_TYPE_ID,
12:19:51 1734  		   GIFT_CERTIFICATE.PURCHASER_GROUP_ID
12:19:51 1735  		   into
12:19:51 1736  		   var_instrument_id,
12:19:51 1737  		   var_instrument_type_id,
12:19:51 1738  		   var_group_id
12:19:51 1739  		 FROM
12:19:51 1740  		   INVOICE
12:19:51 1741  		   INNER JOIN CHARGE ON INVOICE.ID = CHARGE.INVOICE_ID
12:19:51 1742  		   INNER JOIN GIFT_CERTIFICATE ON GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = INVOICE.ID
12:19:51 1743  		 WHERE
12:19:51 1744  		   INVOICE.ID = in_invoice_id
12:19:51 1745  		   AND ROWNUM <= 1;
12:19:51 1746  
12:19:51 1747  		 SELECT
12:19:51 1748  		   ACCOUNT.ID into var_account_id
12:19:51 1749  		 FROM
12:19:51 1750  		   ACCOUNT
12:19:51 1751  		 WHERE
12:19:51 1752  		   ACCOUNT.GROUP_ID = var_group_id;
12:19:51 1753  
12:19:51 1754  		 EXCEPTION
12:19:51 1755  		   WHEN NO_DATA_FOUND THEN
12:19:51 1756  		     RAISE CAN_NOT_FIND_SUBSCR_OR_GC;
12:19:51 1757  	       END;
12:19:51 1758  	 END;
12:19:51 1759  
12:19:51 1760  	 -- Check account status. It should not to be frozen
12:19:51 1761  	 SELECT
12:19:51 1762  	   ACCOUNT.ACCOUNT_STATUS_ID into var_account_status_id
12:19:51 1763  	 FROM
12:19:51 1764  	   ACCOUNT
12:19:51 1765  	 WHERE
12:19:51 1766  	   ACCOUNT.ID = var_account_id;
12:19:51 1767  
12:19:51 1768  	 IF var_account_status_id = GLOBAL_STATUSES_V23.ACCOUNT_FROZEN THEN
12:19:51 1769  	   RAISE ACCOUNT_IS_FROZEN;
12:19:51 1770  	 END IF;
12:19:51 1771  
12:19:51 1772  	 IF var_invoice_status_id != GLOBAL_STATUSES_V23.INVOICE_CLOSED THEN
12:19:51 1773  	   RAISE INVOICE_IS_NOT_CLOSED;
12:19:51 1774  	 END IF;
12:19:51 1775  
12:19:51 1776  	 BEGIN
12:19:51 1777  	   PROCS_INVOICE_V23.CALCULATE_INVOICE_AMOUNT (
12:19:51 1778  	     in_invoice_id => in_invoice_id,
12:19:51 1779  	     out_amount    => var_invoice_amount
12:19:51 1780  	   );
12:19:51 1781  	   EXCEPTION
12:19:51 1782  	     WHEN OTHERS THEN
12:19:51 1783  	       EXCEPTION_MESSAGE := SQLERRM;
12:19:51 1784  	       RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
12:19:51 1785  	 END;
12:19:51 1786  
12:19:51 1787  	 IF ( in_refund_amount > var_invoice_amount ) THEN
12:19:51 1788  	   RAISE REFUND_IS_GREATER_THAN_ANOUNT;
12:19:51 1789  	 END IF;
12:19:51 1790  
12:19:51 1791  	 SELECT /*+ STAR_TRANSFORMATION */
12:19:51 1792  	   SUM(CHARGE.CHARGE_AMOUNT) into var_refunds_before
12:19:51 1793  	 FROM
12:19:51 1794  	   CHARGE
12:19:51 1795  	 WHERE
12:19:51 1796  	   CHARGE.INVOICE_ID = in_invoice_id
12:19:51 1797  	   AND (
12:19:51 1798  	     CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V23.CHARGE_OPENED
12:19:51 1799  	     OR (
12:19:51 1800  	       CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V23.CHARGE_PROCESSED
12:19:51 1801  	       AND EXISTS (
12:19:51 1802  		 SELECT 1 FROM TRANSACTION_ATTEMPT ta where ta.transaction_id = CHARGE.TRANSACTION_ID and ta.transaction_attempt_status_id = GLOBAL_STATUSES_V23.TRANS_ATTEMPT_SUCCESS
12:19:51 1803  	       )
12:19:51 1804  	     )
12:19:51 1805  	   )
12:19:51 1806  	   AND CHARGE.CHARGE_AMOUNT < 0;
12:19:51 1807  
12:19:51 1808  	 -- Refunds are negative
12:19:51 1809  	 IF var_refunds_before IS NULL THEN var_refunds_before := 0; END IF;
12:19:51 1810  	 var_refunds_before := 0 - var_refunds_before;
12:19:51 1811  
12:19:51 1812  	 var_charges_amount := 0;
12:19:51 1813  
12:19:51 1814  	 FOR f_processed_charges IN (
12:19:51 1815  	   SELECT
12:19:51 1816  	     CHARGE.CHARGE_AMOUNT
12:19:51 1817  	   FROM
12:19:51 1818  	     CHARGE
12:19:51 1819  	   WHERE
12:19:51 1820  	     CHARGE.INVOICE_ID = in_invoice_id
12:19:51 1821  	     AND CHARGE.CHARGE_AMOUNT > 0
12:19:51 1822  	     AND CHARGE.CHARGE_STATUS_ID IN (SELECT GLOBAL_STATUSES_V23.CHARGE_PROCESSED FROM DUAL)
12:19:51 1823  	     AND EXISTS (SELECT 1 FROM TRANSACTION_ATTEMPT ta where ta.transaction_id = CHARGE.TRANSACTION_ID and ta.transaction_attempt_status_id = GLOBAL_STATUSES_V23.TRANS_ATTEMPT_SUCCESS)
12:19:51 1824  	 )
12:19:51 1825  	 LOOP
12:19:51 1826  	   var_charges_amount := var_charges_amount + f_processed_charges.CHARGE_AMOUNT;
12:19:51 1827  	 END LOOP;
12:19:51 1828  
12:19:51 1829  	 IF (in_refund_amount + var_refunds_before > var_invoice_amount) THEN
12:19:51 1830  	   RAISE TOT_REF_IS_GREATER_THAN_ANOUNT;
12:19:51 1831  	 END IF;
12:19:51 1832  
12:19:51 1833  	 IF (in_refund_amount + var_refunds_before > var_charges_amount) THEN
12:19:51 1834  	   RAISE TOT_REF_IS_GRATER_THAN_CHARGES;
12:19:51 1835  	 END IF;
12:19:51 1836  
12:19:51 1837  	 BEGIN
12:19:51 1838  	   PROCS_TRANSACTION_V23.CREATE_TRANSACTION(
12:19:51 1839  	     in_transaction_id	       => NULL,
12:19:51 1840  	     in_status_id	       => GLOBAL_STATUSES_V23.TRANSACTION_PREPARE,
12:19:51 1841  	     in_amount		       => -in_refund_amount,
12:19:51 1842  	     in_created_by	       => in_created_by,
12:19:51 1843  	     in_order_id	       => NULL,
12:19:51 1844  	     in_is_refund	       => GLOBAL_CONSTANTS_V23.TRUE,
12:19:51 1845  	     in_transaction_type_code  => 'REFUND',
12:19:51 1846  	     out_transaction_id        => var_new_transaction_id
12:19:51 1847  	   );
12:19:51 1848  	   EXCEPTION
12:19:51 1849  	     WHEN OTHERS THEN
12:19:51 1850  	       EXCEPTION_MESSAGE := SQLERRM;
12:19:51 1851  	       RAISE CAN_NOT_CREATE_TRANSACTION;
12:19:51 1852  	 END;
12:19:51 1853  
12:19:51 1854  	 BEGIN
12:19:51 1855  	   PROCS_CHARGE_V23.CREATE_CHARGE(
12:19:51 1856  	     in_invoice_id	   => in_invoice_id,
12:19:51 1857  	     in_transaction_id	   => var_new_transaction_id,
12:19:51 1858  	     in_instrument_type_id => var_instrument_type_id,
12:19:51 1859  	     in_instrument_id	   => var_instrument_id,
12:19:51 1860  	     in_charge_amount	   => -in_refund_amount,
12:19:51 1861  	     in_created_by	   => in_created_by,
12:19:51 1862  	     in_charge_status_id   => GLOBAL_STATUSES_V23.CHARGE_OPENED,
12:19:51 1863  	     out_charge_id	   => var_new_charge_id
12:19:51 1864  	   );
12:19:51 1865  	   EXCEPTION
12:19:51 1866  	     WHEN OTHERS THEN
12:19:51 1867  	       EXCEPTION_MESSAGE := SQLERRM;
12:19:51 1868  	       RAISE CAN_NOT_CREATE_CHARGE;
12:19:51 1869  	 END;
12:19:51 1870  
12:19:51 1871  	 out_charge_id := var_new_charge_id;
12:19:51 1872  
12:19:51 1873  	 IF in_note IS NOT NULL AND var_subscription_id IS NOT NULL THEN
12:19:51 1874  	   BEGIN
12:19:51 1875  	     PROCS_SUBSCRIPTION_V23.ANNOTATE_SUBSCRIPTION(
12:19:51 1876  	       in_subscription_id => var_subscription_id,
12:19:51 1877  	       in_agent_id	  => 0, -- AGENT_ID??
12:19:51 1878  	       in_note		  => in_note,
12:19:51 1879  	       in_created_by	  => in_created_by
12:19:51 1880  	     );
12:19:51 1881  	     EXCEPTION
12:19:51 1882  	       WHEN OTHERS THEN
12:19:51 1883  		 EXCEPTION_MESSAGE := SQLERRM;
12:19:51 1884  		 RAISE CAN_NOT_ANNOTATE_SUBSCRIPTION;
12:19:51 1885  	   END;
12:19:51 1886  	 END IF;
12:19:51 1887  
12:19:51 1888  EXCEPTION
12:19:51 1889  WHEN CAN_NOT_FIND_SUBSCR_OR_GC THEN
12:19:51 1890  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 1891  	   SPROC_NAME, 'Could not find subscription or GC for the inovice');
12:19:51 1892  WHEN INVOICE_IS_NOT_CLOSED THEN
12:19:51 1893  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
12:19:51 1894  	   SPROC_NAME, 'Invoice is not closed');
12:19:51 1895  WHEN ACCOUNT_IS_FROZEN THEN
12:19:51 1896  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
12:19:51 1897  	   SPROC_NAME, 'Could not refund subscription for frozen account');
12:19:51 1898  WHEN BAD_INVOICE_ID THEN
12:19:51 1899  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 1900  	   SPROC_NAME, 'No such invoice');
12:19:51 1901  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
12:19:51 1902  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 1903  	   SPROC_NAME, 'Could not calculate invoice amount', EXCEPTION_MESSAGE);
12:19:51 1904  WHEN REFUND_IS_GREATER_THAN_ANOUNT THEN
12:19:51 1905  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INVALID_PARAMETER,
12:19:51 1906  	   SPROC_NAME, 'Refund is greater than amount');
12:19:51 1907  WHEN TOT_REF_IS_GREATER_THAN_ANOUNT THEN
12:19:51 1908  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INVALID_PARAMETER,
12:19:51 1909  	   SPROC_NAME, 'There were refunds before and sum of all refunds and new refund more than invoice amount');
12:19:51 1910  WHEN TOT_REF_IS_GRATER_THAN_CHARGES THEN
12:19:51 1911  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INVALID_PARAMETER,
12:19:51 1912  	   SPROC_NAME, 'Total refund amount is greater than sum of processed charges');
12:19:51 1913  WHEN CAN_NOT_CREATE_TRANSACTION THEN
12:19:51 1914  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 1915  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
12:19:51 1916  WHEN CAN_NOT_CREATE_CHARGE THEN
12:19:51 1917  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 1918  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
12:19:51 1919  WHEN CAN_NOT_ANNOTATE_SUBSCRIPTION THEN
12:19:51 1920  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 1921  	   SPROC_NAME, 'Could not annotate subscription', EXCEPTION_MESSAGE);
12:19:51 1922  WHEN OTHERS THEN
12:19:51 1923  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 1924  	   SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 1925  END REFUND_INVOICE;
