12:19:51 698  
12:19:51 699  PROCEDURE CALCULATE_INVOICE_CHARGEBACK (
12:19:51 700  	in_invoice_id	      IN NUMBER,
12:19:51 701  	in_chargeback_date    IN DATE,
12:19:51 702  	out_chargeback_amount OUT NUMBER
12:19:51 703  ) AS
12:19:51 704  -- VARIABLES
12:19:51 705  SPROC_NAME	     CONSTANT VARCHAR2(28) := 'CALCULATE_INVOICE_CHARGEBACK';
12:19:51 706  var_chargeback_date    DATE;
12:19:51 707  temp_invoice_id	     NUMBER;
12:19:51 708  var_licenses_number    NUMBER;
12:19:51 709  var_invoice_start_date DATE;
12:19:51 710  var_invoice_end_date   DATE;
12:19:51 711  var_offer_id	     NUMBER;
12:19:51 712  var_offer_days_interval NUMBER;
12:19:51 713  var_license_days_used  NUMBER;
12:19:51 714  var_invoice_amount     NUMBER(10,2);
12:19:51 715  var_offer_chain_id     NUMBER;
12:19:51 716  var_offer_chain_meta_data_val VARCHAR2(1024);
12:19:51 717  var_offer_chain_full_refund   NUMBER;
12:19:51 718  var_offer_chain_prorated_ref  NUMBER;
12:19:51 719  var_chargeback_calculated NUMBER;
12:19:51 720  var_max_invoice_refund	NUMBER;
12:19:51 721  -- EXCEPTIONS
12:19:51 722  BAD_INVOICE_ID		  EXCEPTION;
12:19:51 723  NO_LICENSES_FOUND_EXCEPTION EXCEPTION;
12:19:51 724  OFFER_LENGTH_IS_ZERO	  EXCEPTION;
12:19:51 725  BEGIN
12:19:51 726  
12:19:51 727  	IF in_chargeback_date IS NULL THEN
12:19:51 728  	  var_chargeback_date := PROCS_COMMON_V23.NORMALIZE_DATE(current_date);
12:19:51 729  	ELSE
12:19:51 730  	  var_chargeback_date := PROCS_COMMON_V23.NORMALIZE_DATE(in_chargeback_date);
12:19:51 731  	END IF;
12:19:51 732  
12:19:51 733  	-- Check that invoice exists
12:19:51 734  	BEGIN
12:19:51 735  	  SELECT
12:19:51 736  	    INVOICE.ID into temp_invoice_id
12:19:51 737  	  FROM
12:19:51 738  	    INVOICE
12:19:51 739  	  WHERE
12:19:51 740  	    INVOICE.ID = in_invoice_id;
12:19:51 741  	  EXCEPTION
12:19:51 742  	    WHEN NO_DATA_FOUND THEN
12:19:51 743  	      RAISE BAD_INVOICE_ID;
12:19:51 744  	END;
12:19:51 745  
12:19:51 746  	-- Check that invoice has at least one license
12:19:51 747  	SELECT
12:19:51 748  	  COUNT(*) into var_licenses_number
12:19:51 749  	FROM
12:19:51 750  	  LICENSE
12:19:51 751  	WHERE
12:19:51 752  	  LICENSE.INVOICE_ID = in_invoice_id;
12:19:51 753  
12:19:51 754  	IF var_licenses_number = 0 THEN
12:19:51 755  	  RAISE NO_LICENSES_FOUND_EXCEPTION;
12:19:51 756  	END IF;
12:19:51 757  
12:19:51 758  	SELECT
12:19:51 759  	  MIN(LICENSE.START_DATE) into var_invoice_start_date
12:19:51 760  	FROM
12:19:51 761  	  LICENSE
12:19:51 762  	WHERE
12:19:51 763  	  LICENSE.INVOICE_ID = in_invoice_id;
12:19:51 764  
12:19:51 765  	var_invoice_start_date := PROCS_COMMON_V23.NORMALIZE_DATE(var_invoice_start_date);
12:19:51 766  
12:19:51 767  	SELECT DISTINCT
12:19:51 768  	  LICENSE.OFFER_ID into var_offer_id
12:19:51 769  	FROM
12:19:51 770  	  LICENSE
12:19:51 771  	WHERE
12:19:51 772  	  LICENSE.INVOICE_ID = in_invoice_id;
12:19:51 773  
12:19:51 774  	SELECT
12:19:51 775  	  MAX (LICENSE.END_DATE) into var_invoice_end_date
12:19:51 776  	FROM
12:19:51 777  	  LICENSE
12:19:51 778  	WHERE
12:19:51 779  	  LICENSE.INVOICE_ID = in_invoice_id;
12:19:51 780  
12:19:51 781  	-- All licenses for given invoice should point into the same offer
12:19:51 782  
12:19:51 783  	PROCS_OFFER_CHAIN_V23.GET_OFFER_LENGTH_IN_DAYS(
12:19:51 784  	  in_offer_id	=> var_offer_id,
12:19:51 785  	  in_start_date => var_invoice_start_date,
12:19:51 786  	  out_days	=> var_offer_days_interval
12:19:51 787  	);
12:19:51 788  
12:19:51 789  	IF var_offer_days_interval = 0 THEN
12:19:51 790  	  RAISE OFFER_LENGTH_IS_ZERO;
12:19:51 791  	END IF;
12:19:51 792  
12:19:51 793  	PROCS_INVOICE_V23.GET_INVOICE_DAYS_USED_NUMBER(
12:19:51 794  	  in_invoice_id      => in_invoice_id,
12:19:51 795  	  in_chargeback_date => var_chargeback_date,
12:19:51 796  	  out_days_num	     => var_license_days_used
12:19:51 797  	);
12:19:51 798  
12:19:51 799  	PROCS_INVOICE_V23.CALCULATE_INVOICE_AMOUNT(
12:19:51 800  	  in_invoice_id => in_invoice_id,
12:19:51 801  	  out_amount	=> var_invoice_amount
12:19:51 802  	);
12:19:51 803  
12:19:51 804  	-- 39437
12:19:51 805  	-- Get offer chain id by invoice id
12:19:51 806  	SELECT DISTINCT
12:19:51 807  	  SUBSCRIPTION.OFFER_CHAIN_ID into var_offer_chain_id
12:19:51 808  	FROM
12:19:51 809  	  SUBSCRIPTION
12:19:51 810  	  INNER JOIN LICENSE ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
12:19:51 811  	WHERE
12:19:51 812  	  LICENSE.INVOICE_ID = in_invoice_id;
12:19:51 813  
12:19:51 814  	-- Get offer chain's meta data full amount value
12:19:51 815  	PROCS_OFFER_CHAIN_V23.GET_OFFER_CHAIN_MD_VALUE(
12:19:51 816  	  in_offer_chain_id => var_offer_chain_id,
12:19:51 817  	  in_meta_data_name => GLOBAL_CONSTANTS_V23.OFFER_CHAIN_FULL_REFUND,
12:19:51 818  	  out_value	    => var_offer_chain_meta_data_val
12:19:51 819  	);
12:19:51 820  	IF var_offer_chain_meta_data_val IS NULL THEN
12:19:51 821  	  var_offer_chain_full_refund := NULL;
12:19:51 822  	ELSE
12:19:51 823  	  var_offer_chain_full_refund := TO_NUMBER(var_offer_chain_meta_data_val);
12:19:51 824  	END IF;
12:19:51 825  
12:19:51 826  	-- Get offer chain's meta data prorated amount value
12:19:51 827  	PROCS_OFFER_CHAIN_V23.GET_OFFER_CHAIN_MD_VALUE(
12:19:51 828  	  in_offer_chain_id => var_offer_chain_id,
12:19:51 829  	  in_meta_data_name => GLOBAL_CONSTANTS_V23.OFFER_CHAIN_PRORATED_REFUND,
12:19:51 830  	  out_value	    => var_offer_chain_meta_data_val
12:19:51 831  	);
12:19:51 832  	IF var_offer_chain_meta_data_val IS NULL THEN
12:19:51 833  	  var_offer_chain_prorated_ref := NULL;
12:19:51 834  	ELSE
12:19:51 835  	  var_offer_chain_prorated_ref := TO_NUMBER(var_offer_chain_meta_data_val);
12:19:51 836  	END IF;
12:19:51 837  
12:19:51 838  	var_chargeback_calculated := GLOBAL_CONSTANTS_V23.FALSE;
12:19:51 839  
12:19:51 840  	IF var_offer_chain_full_refund IS NOT NULL
12:19:51 841  	   AND var_chargeback_calculated = GLOBAL_CONSTANTS_V23.FALSE THEN
12:19:51 842  	  IF var_license_days_used < var_offer_chain_full_refund THEN
12:19:51 843  	    out_chargeback_amount := var_invoice_amount;
12:19:51 844  	    var_chargeback_calculated := GLOBAL_CONSTANTS_V23.TRUE;
12:19:51 845  	  END IF;
12:19:51 846  	END IF;
12:19:51 847  
12:19:51 848  	IF var_offer_chain_prorated_ref IS NOT NULL
12:19:51 849  	   AND var_chargeback_calculated = GLOBAL_CONSTANTS_V23.FALSE THEN
12:19:51 850  	  IF var_license_days_used < var_offer_chain_prorated_ref THEN
12:19:51 851  	    out_chargeback_amount := ( var_invoice_amount * (var_offer_days_interval - var_license_days_used) ) / var_offer_days_interval;
12:19:51 852  	    var_chargeback_calculated := GLOBAL_CONSTANTS_V23.TRUE;
12:19:51 853  	  END IF;
12:19:51 854  	END IF;
12:19:51 855  
12:19:51 856  	IF var_chargeback_calculated = GLOBAL_CONSTANTS_V23.TRUE THEN
12:19:51 857  	  PROCS_INVOICE_V23.GET_MAX_REFUND(
12:19:51 858  	    in_invoice_id => in_invoice_id,
12:19:51 859  	    out_amount	  => var_max_invoice_refund
12:19:51 860  	  );
12:19:51 861  	  IF var_max_invoice_refund < out_chargeback_amount THEN
12:19:51 862  	    out_chargeback_amount := var_max_invoice_refund;
12:19:51 863  	  END IF;
12:19:51 864  	END IF;
12:19:51 865  
12:19:51 866  	IF var_chargeback_calculated = GLOBAL_CONSTANTS_V23.FALSE THEN
12:19:51 867  	  out_chargeback_amount := 0;
12:19:51 868  	END IF;
12:19:51 869  
12:19:51 870  EXCEPTION
12:19:51 871  WHEN BAD_INVOICE_ID THEN
12:19:51 872  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 873  	  SPROC_NAME, 'No such invoice');
12:19:51 874  WHEN NO_LICENSES_FOUND_EXCEPTION THEN
12:19:51 875  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 876  	  SPROC_NAME, 'No licenses found');
12:19:51 877  WHEN OFFER_LENGTH_IS_ZERO THEN
12:19:51 878  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 879  	  SPROC_NAME, 'Offer length is zero');
12:19:51 880  WHEN OTHERS THEN
12:19:51 881  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 882  	  SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 883  END CALCULATE_INVOICE_CHARGEBACK;
