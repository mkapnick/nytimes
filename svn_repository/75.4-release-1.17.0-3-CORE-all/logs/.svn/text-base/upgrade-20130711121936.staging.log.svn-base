12:19:51 985  /******************************************************************************/
12:19:51 986  
12:19:51 987  FUNCTION GET_TRANSACTION_INTRL_TAXES (
12:19:51 988  	in_transaction_id IN NUMBER
12:19:51 989  ) RETURN NUMBER AS
12:19:51 990  SPROC_NAME CONSTANT VARCHAR2(27) := 'GET_TRANSACTION_INTRL_TAXES';
12:19:51 991  -- Variables
12:19:51 992  var_intrl_tax_amount NUMBER(10, 2);
12:19:51 993  BEGIN
12:19:51 994  
12:19:51 995  	SELECT SUM(T.CALCULATED_AMOUNT) into var_intrl_tax_amount
12:19:51 996  	FROM
12:19:51 997  	  LINE_ITEM LI
12:19:51 998  	  INNER JOIN INVOICE I ON LI.INVOICE_ID = I.ID
12:19:51 999  	  INNER JOIN CHARGE CH ON CH.INVOICE_ID=  I.ID
12:19:51 1000  	   INNER JOIN TAX T ON T.LINE_ITEM_ID = LI.ID
12:19:51 1001  	 WHERE
12:19:51 1002  	   CH.TRANSACTION_ID = in_transaction_id
12:19:51 1003  	   AND T.TAX_TYPE_ID IN (
12:19:51 1004  	     SELECT GLOBAL_ENUMS_V23.TAX_TYPE_VAT FROM DUAL
12:19:51 1005  	   );
12:19:51 1006  
12:19:51 1007  	 IF var_intrl_tax_amount IS NULL THEN
12:19:51 1008  	   var_intrl_tax_amount := 0;
12:19:51 1009  	 END IF;
12:19:51 1010  
12:19:51 1011  	 RETURN var_intrl_tax_amount;
12:19:51 1012  
12:19:51 1013  END GET_TRANSACTION_INTRL_TAXES;
