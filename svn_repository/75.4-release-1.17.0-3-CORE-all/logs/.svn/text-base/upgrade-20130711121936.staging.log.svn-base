12:19:51 112  		      ocmd.value = in_product_id AND
12:19:51 113  		      rownum < 2
12:19:51 114  		)
12:19:51 115  	      ;
12:19:51 116  
12:19:51 117  	EXCEPTION
12:19:51 118  	  WHEN OTHERS THEN
12:19:51 119  	    PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 120  	      SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 121  	END GET_ITUNES_ENTITLEMENTS;
