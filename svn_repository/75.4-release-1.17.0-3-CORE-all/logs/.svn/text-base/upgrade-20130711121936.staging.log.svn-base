12:19:51 1929  PROCEDURE GET_PAYMENT_INFO_BY_INVOICE_ID (
12:19:51 1930  	 in_invoice_id		     IN NUMBER,
12:19:51 1931  	 out_order_id		     OUT VARCHAR2,
12:19:51 1932  	 out_external_transaction_id OUT VARCHAR2
12:19:51 1933  ) AS
12:19:51 1934  SPROC_NAME CONSTANT VARCHAR2(30) := 'GET_PEYMENT_INFO_BY_INVOICE_ID';
12:19:51 1935  -- VARIABLES
12:19:51 1936  temp_invoice_id number;
12:19:51 1937  cnt_matched_instr number := 0;
12:19:51 1938  -- EXCEPTIONS
12:19:51 1939  BAD_INVOICE_ID EXCEPTION;
12:19:51 1940  BEGIN
12:19:51 1941  
12:19:51 1942  	 BEGIN
12:19:51 1943  	   SELECT
12:19:51 1944  	     i.id into temp_invoice_id
12:19:51 1945  	   from
12:19:51 1946  	     invoice i
12:19:51 1947  	   where
12:19:51 1948  	     i.id = in_invoice_id;
12:19:51 1949  	   EXCEPTION
12:19:51 1950  	     WHEN NO_DATA_FOUND THEN
12:19:51 1951  	       RAISE BAD_INVOICE_ID;
12:19:51 1952  	 END;
12:19:51 1953  
12:19:51 1954  	 select
12:19:51 1955  	   count(1) into cnt_matched_instr
12:19:51 1956  	 from
12:19:51 1957  	   charge ch
12:19:51 1958  	 inner join
12:19:51 1959  	   subscription s
12:19:51 1960  	 on
12:19:51 1961  	   s.instrument_id = ch.instrument_id
12:19:51 1962  	 where
12:19:51 1963  	   ch.invoice_id = in_invoice_id;
12:19:51 1964  
12:19:51 1965  	 if cnt_matched_instr = 0 then
12:19:51 1966  	   out_external_transaction_id := null;
12:19:51 1967  	   out_order_id := null;
12:19:51 1968  	   return;
12:19:51 1969  	 end if;
12:19:51 1970  
12:19:51 1971  	 SELECT
12:19:51 1972  	   t.order_id,
12:19:51 1973  	   ta.external_transaction_id
12:19:51 1974  	   into
12:19:51 1975  	   out_order_id,
12:19:51 1976  	   out_external_transaction_id
12:19:51 1977  	 from
12:19:51 1978  	   charge ch
12:19:51 1979  	   inner join transaction t on ch.transaction_id = t.id
12:19:51 1980  	   inner join transaction_attempt ta on ta.transaction_id = t.id
12:19:51 1981  	 where
12:19:51 1982  	   ch.invoice_id = in_invoice_id
12:19:51 1983  	   and ta.transaction_attempt_status_id = GLOBAL_STATUSES_V23.TRANS_ATTEMPT_SUCCESS
12:19:51 1984  	   and ch.charge_amount > 0; -- We are not creating charges for the 0-amount invoices
12:19:51 1985  
12:19:51 1986  EXCEPTION
12:19:51 1987  WHEN BAD_INVOICE_ID THEN
12:19:51 1988  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 1989  	   SPROC_NAME, 'No such invoice');
12:19:51 1990  WHEN NO_DATA_FOUND THEN
12:19:51 1991  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 1992  	   SPROC_NAME, 'No payment data found');
12:19:51 1993  WHEN OTHERS THEN
12:19:51 1994  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 1995  	   SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 1996  END GET_PAYMENT_INFO_BY_INVOICE_ID;
