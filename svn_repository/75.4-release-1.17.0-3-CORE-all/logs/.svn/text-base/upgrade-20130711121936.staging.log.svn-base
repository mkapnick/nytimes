12:19:52 303  	OPEN c(in_start_group_id, in_end_group_id);
12:19:52 304  	WHILE c%ISOPEN LOOP
12:19:52 305  	  FETCH c INTO gid;
12:19:52 306  	  IF c%NOTFOUND THEN
12:19:52 307  	   CLOSE c;
12:19:52 308  	  END IF;
12:19:52 309  	  TEST_DELETE_USER_ACCOUNT(gid);
