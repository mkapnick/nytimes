12:19:51  39  PROCEDURE GET_UNREDEEMED_GCS (
12:19:51  40  	out_result_set		OUT SYS_REFCURSOR,
12:19:51  41  	in_hours_number 	IN NUMBER DEFAULT 14*24,
12:19:51  42  	in_num_rows		IN NUMBER DEFAULT 10000,
12:19:51  43  	in_process_name IN PROCESS_RETRY_THROTTLE.PROCESS_NAME%TYPE
12:19:51  44  ) AS
12:19:51  45  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_UNREDEEMED_GCS';
12:19:51  46  BEGIN
12:19:51  47  	OPEN out_result_set FOR
12:19:51  48  SELECT * FROM
12:19:51  49  (
12:19:51  50  	SELECT
12:19:51  51  	  gc.EXPIRATION_DATE,
12:19:51  52  	  ch.name,
12:19:51  53  	  ch.id offer_chain_id,
12:19:51  54  	  gc.sender_email,
12:19:51  55  	  gc.sender_name,
12:19:51  56  	  gc.recipient_email,
12:19:51  57  	  gc.recipient_name,
12:19:51  58  	  gc.purchase_date,
12:19:51  59  	  gc.redemption_date,
12:19:51  60  	  gc.purchaser_group_id,
12:19:51  61  	  gc.redeemer_group_id,
12:19:51  62  	  gc.code,
12:19:51  63  	  gc.gift_message,
12:19:51  64  	  gc.recipient_notify_date,
12:19:51  65  	  gc.id
12:19:51  66  	FROM
12:19:51  67  	  GIFT_CERTIFICATE gc,
12:19:51  68  	  OFFER_CHAIN ch
12:19:51  69  	WHERE
12:19:51  70  	  ch.id = gc.offer_chain_id
12:19:51  71  	  AND gc.RECIPIENT_NOTIFY_DATE is not null
12:19:51  72  	  AND gc.RECIPIENT_NOTIFY_DATE >= (sysdate - in_hours_number/24)
12:19:51  73  	  AND gc.RECIPIENT_NOTIFY_DATE < (sysdate - (in_hours_number-72)/24)
12:19:51  74  	  AND gc.redeemer_group_id is null
12:19:51  75  	  AND NOT EXISTS(
12:19:51  76  	    SELECT NULL
12:19:51  77  	    FROM PROCESS_RETRY_THROTTLE
12:19:51  78  	    WHERE PROCESS_NAME = in_process_name
12:19:51  79  	      AND GENERIC_ID = gc.id
12:19:51  80  	  ) AND EXISTS(
12:19:51  81  	    SELECT NULL
12:19:51  82  	    FROM
12:19:51  83  	      charge c,
12:19:51  84  	      transaction_attempt ta,
12:19:51  85  	      transaction t
12:19:51  86  	    WHERE
12:19:51  87  	      c.invoice_id = gc.purchase_invoice_id and
12:19:51  88  	      c.transaction_id = t.id and
12:19:51  89  	      t.id = ta.transaction_id and
12:19:51  90  	      ta.transaction_attempt_status_id = GLOBAL_STATUSES_V23.TRANS_ATTEMPT_SUCCESS
12:19:51  91  	  ) AND NOT EXISTS (
12:19:51  92  	    SELECT NULL
12:19:51  93  	    FROM
12:19:51  94  	      charge c,
12:19:51  95  	      transaction t
12:19:51  96  	    WHERE
12:19:51  97  	      c.invoice_id = gc.purchase_invoice_id and
12:19:51  98  	      c.transaction_id = t.id and
12:19:51  99  	      t.is_refund = GLOBAL_CONSTANTS_V23.TRUE
12:19:51 100  	  )
12:19:51 101  	  AND ROWNUM <= in_num_rows*10
12:19:51 102  	  ORDER BY dbms_random.value
12:19:51 103  ) WHERE
12:19:51 104  	  ROWNUM <= in_num_rows;
12:19:51 105  EXCEPTION
12:19:51 106  WHEN OTHERS THEN
12:19:51 107  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 108  	  SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 109  END GET_UNREDEEMED_GCS;
