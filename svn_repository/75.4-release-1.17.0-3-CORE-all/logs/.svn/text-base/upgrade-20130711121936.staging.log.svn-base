12:19:51 209  PROCEDURE GET_ENDING_LICENSES (
12:19:51 210  	in_hours_number IN NUMBER,
12:19:51 211  	out_result_set	OUT SYS_REFCURSOR
12:19:51 212  ) AS
12:19:51 213  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_ENDING_LICENSES';
12:19:51 214  -- VARIABLES
12:19:51 215  var_days		NUMBER;
12:19:51 216  var_hours 	NUMBER;
12:19:51 217  var_time_interval INTERVAL DAY (3) TO SECOND;
12:19:51 218  BEGIN
12:19:51 219  
12:19:51 220  	var_hours := mod(in_hours_number,24);
12:19:51 221  	var_days := (in_hours_number - var_hours) / 24;
12:19:51 222  	var_time_interval := var_days||' '||var_hours||':0:0';
12:19:51 223  
12:19:51 224  	OPEN out_result_set FOR
12:19:51 225  SELECT * FROM
12:19:51 226  (
12:19:51 227  	SELECT
12:19:51 228  	  LICENSE.ID,
12:19:51 229  	  LICENSE.CREATE_DATE,
12:19:51 230  	  LICENSE.CREATED_BY,
12:19:51 231  	  LICENSE.CURRENT_OFFER_INDEX,
12:19:51 232  	  LICENSE.CURRENT_OFFER_RECURR_NUM,
12:19:51 233  	  LICENSE.END_DATE,
12:19:51 234  	  LICENSE.ENTITLEMENT_END_DATE,
12:19:51 235  	  LICENSE.INVOICE_ID,
12:19:51 236  	  LICENSE.IS_EXTENSION,
12:19:51 237  	  LICENSE.LICENSE_STATUS_ID,
12:19:51 238  	  LICENSE.OFFER_ID,
12:19:51 239  	  LICENSE.START_DATE,
12:19:51 240  	  LICENSE.SUBSCRIPTION_ID,
12:19:51 241  	  LICENSE.UPDATE_DATE,
12:19:51 242  	  LICENSE.UPDATED_BY
12:19:51 243  	FROM
12:19:51 244  	  LICENSE
12:19:51 245  	WHERE
12:19:51 246  	  TO_DATE(LICENSE.END_DATE) <= (current_timestamp + var_time_interval)
12:19:51 247  	  AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V23.LICENSE_ACTIVE
12:19:51 248  	  AND NOT EXISTS
12:19:51 249  	  (
12:19:51 250  	    SELECT NULL
12:19:51 251  	    FROM PROCESS_RETRY_THROTTLE
12:19:51 252  	    WHERE PROCESS_NAME = SPROC_NAME
12:19:51 253  	      AND GENERIC_ID = LICENSE.ID
12:19:51 254  	  )
12:19:51 255  	  AND ROWNUM <= 40000
12:19:51 256  	  ORDER BY dbms_random.value
12:19:51 257  ) WHERE
12:19:51 258  	  ROWNUM <= 4000;
12:19:51 259  EXCEPTION
