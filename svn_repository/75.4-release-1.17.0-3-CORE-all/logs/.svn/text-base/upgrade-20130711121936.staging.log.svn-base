12:19:51 1839  
12:19:51 1840  	 -- Select all gift certificates with given code
12:19:51 1841  	 OPEN out_result_set FOR
12:19:51 1842  	 SELECT
12:19:51 1843  	   gc.EXPIRATION_DATE,
12:19:51 1844  	   ch.name,
12:19:51 1845  	   ch.id,
12:19:51 1846  	   gc.sender_email,
12:19:51 1847  	   gc.sender_name,
12:19:51 1848  	   gc.recipient_email,
12:19:51 1849  	   gc.recipient_name,
12:19:51 1850  	   gc.purchase_date,
12:19:51 1851  	   gc.redemption_date,
12:19:51 1852  	   gc.purchaser_group_id,
12:19:51 1853  	   gc.redeemer_group_id,
12:19:51 1854  	   gc.gift_message,
12:19:51 1855  	   ocmd.value redemption_offer_chain_ids,
12:19:51 1856  	   s.offer_chain_id redeemed_offer_chain_id,
12:19:51 1857  	   gc.recipient_notify_date,
12:19:51 1858  	   gc.gift_certificate_status_id,
12:19:51 1859  	   gc.purchase_invoice_id,
12:19:51 1860  	   gc.finalized_invoice_id,
12:19:51 1861  	   gccr.reference_code
12:19:51 1862  	 FROM
12:19:51 1863  	   GIFT_CERTIFICATE gc
12:19:51 1864  	 INNER JOIN OFFER_CHAIN ch ON ch.id = gc.offer_chain_id
12:19:51 1865  	 INNER JOIN OFFER_CHAIN_META_DATA ocmd ON gc.offer_chain_id = ocmd.offer_chain_id AND ocmd.name = 'redemption offer chain id'
12:19:51 1866  	 LEFT JOIN LICENSE l ON l.invoice_id = gc.finalized_invoice_id
12:19:51 1867  	 LEFT JOIN SUBSCRIPTION s ON l.subscription_id = s.id
12:19:51 1868  	 LEFT JOIN GC_CAMPAIGN_AND_REF gccr ON gc.id = gccr.GC_ID
12:19:51 1869  	 WHERE
