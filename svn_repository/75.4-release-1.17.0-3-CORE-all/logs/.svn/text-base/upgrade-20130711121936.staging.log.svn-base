12:19:51 1076  
12:19:51 1077  PROCEDURE PURCHASE_GIFT_CERTIFICATE (
12:19:51 1078  	 in_group_id		   IN NUMBER,
12:19:51 1079  	 in_offer_chain_id	   IN VARCHAR2,
12:19:51 1080  	 in_gift_certificate_code  IN VARCHAR2,
12:19:51 1081  	 in_created_by		   IN VARCHAR2,
12:19:51 1082  	 in_recipient_name	   IN VARCHAR2,
12:19:51 1083  	 in_recipient_email	   IN VARCHAR2,
12:19:51 1084  	 in_sender_name 	   IN VARCHAR2,
12:19:51 1085  	 in_sender_email	   IN VARCHAR2,
12:19:51 1086  	 in_gift_message	   IN VARCHAR2,
12:19:51 1087  	 in_instrument_id	   IN NUMBER,
12:19:51 1088  	 in_instrument_type_id	   IN NUMBER,
12:19:51 1089  	 in_expiration_date	   IN DATE,
12:19:51 1090  	 in_order_id		   IN VARCHAR2,
12:19:51 1091  	 in_transaction_id	   IN NUMBER
12:19:51 1092  ) AS
12:19:51 1093  SPROC_NAME	  CONSTANT VARCHAR2(25) := 'PURCHASE_GIFT_CERTIFICATE';
12:19:51 1094  -- VARIABLES
12:19:51 1095  var_gift_cert_id   NUMBER;
12:19:51 1096  var_account_id	  NUMBER;
12:19:51 1097  var_invoice_amount NUMBER (10,2);
12:19:51 1098  var_new_invoice_id NUMBER;
12:19:51 1099  var_new_charge_id  NUMBER;
12:19:51 1100  var_och_is_gc	  NUMBER;
12:19:51 1101  var_offer_chain_status_id NUMBER;
12:19:51 1102  var_is_for_redemption	 NUMBER;
12:19:51 1103  var_account_tax_exempt_id VARCHAR2(255);
12:19:51 1104  
12:19:51 1105  temp_transaction_id_count NUMBER;
12:19:51 1106  var_transaction_id	 NUMBER;
12:19:51 1107  temp_gc_code VARCHAR2(255);
12:19:51 1108  
12:19:51 1109  var_invoice_status_id NUMBER;
12:19:51 1110  -- EXCEPTIONS
12:19:51 1111  CAN_NOT_CREATE_INVOICE		   EXCEPTION;
12:19:51 1112  CAN_NOT_CREATE_TRANSACTION	   EXCEPTION;
12:19:51 1113  CAN_NOT_CREATE_CHARGE		   EXCEPTION;
12:19:51 1114  CAN_NOT_CREATE_LINE_ITEMS	   EXCEPTION;
12:19:51 1115  BAD_GROUP_ID			   EXCEPTION;
12:19:51 1116  BAD_OFFER_CHAIN_ID		   EXCEPTION;
12:19:51 1117  OCH_IS_NOT_GIFT_CERTIFICATE	   EXCEPTION;
12:19:51 1118  TRANSACTION_EXISTS		   EXCEPTION;
12:19:51 1119  GC_CODE_ALREADY_EXISTS		   EXCEPTION;
12:19:51 1120  BAD_INSTRUMENT_TYPE		   EXCEPTION;
12:19:51 1121  BAD_CREDIT_CARD_ID		   EXCEPTION;
12:19:51 1122  BAD_PAYPAL_ID			   EXCEPTION;
12:19:51 1123  CAN_NOT_CALCULATE_OCH_AMOUNT	   EXCEPTION;
12:19:51 1124  BAD_OFFER_CHAIN_STATUS		   EXCEPTION;
12:19:51 1125  OFFER_REC_NUM_LESS_THAN_ONE	   EXCEPTION;
12:19:51 1126  CAN_NOT_CALC_INVOICE_AMOUNT	   EXCEPTION;
12:19:51 1127  CAN_NOT_USE_FCINSTR		   EXCEPTION;
12:19:51 1128  CAN_NOT_PURCHASE_GC_FOR_RDMPN	   EXCEPTION;
12:19:51 1129  EXCEPTION_MESSAGE		   VARCHAR2(1024);
12:19:51 1130  BEGIN
12:19:51 1131  
12:19:51 1132  	 -- Get account id
12:19:51 1133  	 BEGIN
12:19:51 1134  	   SELECT
12:19:51 1135  	     ACCOUNT.ID,
12:19:51 1136  	     ACCOUNT.TAX_EXEMPT_ID
12:19:51 1137  	     into
12:19:51 1138  	     var_account_id,
12:19:51 1139  	     var_account_tax_exempt_id
12:19:51 1140  	   FROM
12:19:51 1141  	     ACCOUNT
12:19:51 1142  	   WHERE
12:19:51 1143  	     ACCOUNT.GROUP_ID = in_group_id;
12:19:51 1144  	   EXCEPTION
12:19:51 1145  	   WHEN NO_DATA_FOUND THEN
12:19:51 1146  	     RAISE BAD_GROUP_ID;
12:19:51 1147  	 END;
12:19:51 1148  
12:19:51 1149  	 -- Check that instrument exists
12:19:51 1150  	 IF in_instrument_type_id = GLOBAL_ENUMS_V23.INSTRUMENT_CREDIT_CARD THEN
12:19:51 1151  	   IF IS_CREDIT_CARD_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V23.FALSE THEN
12:19:51 1152  	     RAISE BAD_CREDIT_CARD_ID;
12:19:51 1153  	   END IF;
12:19:51 1154  	 ELSIF in_instrument_type_id = GLOBAL_ENUMS_V23.INSTRUMENT_PAYPAL THEN
12:19:51 1155  	   IF IS_PAYPAL_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V23.FALSE THEN
