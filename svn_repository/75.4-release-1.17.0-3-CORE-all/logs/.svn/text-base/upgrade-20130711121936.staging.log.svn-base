12:19:51 1119  BAD_SECOND_OFFER_CHAIN	      EXCEPTION;
12:19:51 1120  BEGIN
12:19:51 1121  
12:19:51 1122  	 -- Check that first offer chain exists
12:19:51 1123  	 BEGIN
12:19:51 1124  	   SELECT
12:19:51 1125  	     OFFER_CHAIN.ID into temp_offer_chain_id
12:19:51 1126  	   FROM
12:19:51 1127  	     OFFER_CHAIN
12:19:51 1128  	   WHERE
12:19:51 1129  	     OFFER_CHAIN.ID = in_offer_chain_1;
12:19:51 1130  	   EXCEPTION
12:19:51 1131  	     WHEN NO_DATA_FOUND THEN
12:19:51 1132  	       RAISE BAD_FIRST_OFFER_CHAIN;
12:19:51 1133  	 END;
12:19:51 1134  
12:19:51 1135  	 -- Check that second offer chain exists
12:19:51 1136  	 BEGIN
12:19:51 1137  	   SELECT
12:19:51 1138  	     OFFER_CHAIN.ID into temp_offer_chain_id
12:19:51 1139  	   FROM
12:19:51 1140  	     OFFER_CHAIN
12:19:51 1141  	   WHERE
12:19:51 1142  	     OFFER_CHAIN.ID = in_offer_chain_2;
12:19:51 1143  	   EXCEPTION
12:19:51 1144  	     WHEN NO_DATA_FOUND THEN
12:19:51 1145  	       RAISE BAD_SECOND_OFFER_CHAIN;
12:19:51 1146  	 END;
12:19:51 1147  
12:19:51 1148  	 PROCS_OFFER_CHAIN_V23.GET_OFF_CHAINS_SAME_PRODUCTS(
12:19:51 1149  	   in_offer_chain_1 => in_offer_chain_1,
12:19:51 1150  	   in_offer_chain_2 => in_offer_chain_2,
12:19:51 1151  	   out_result_set   => var_same_products
12:19:51 1152  	 );
