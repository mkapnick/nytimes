12:19:51   4  	PROCEDURE GET_ALL_ENTITLEMENTS(
12:19:51   5  	  in_group_id	 IN  NUMBER,
12:19:51   6  	  out_result_set OUT SYS_REFCURSOR)
12:19:51   7  	AS
12:19:51   8  	  -- CONSTANTS
12:19:51   9  	  SPROC_NAME	  CONSTANT VARCHAR2(20) := 'GET_ALL_ENTITLEMENTS';
12:19:51  10  	  -- EXCEPTIONS
12:19:51  11  	  NOT_FOUND	  EXCEPTION;
12:19:51  12  	  UNKNOWN_ERROR   EXCEPTION;
12:19:51  13  	  -- VARIABLES
12:19:51  14  	  var_subs	  SYS_REFCURSOR;
12:19:51  15  	BEGIN
12:19:51  16  	  OPEN out_result_set FOR
12:19:51  17  
12:19:51  18  	SELECT
12:19:51  19  	  P.ENTITLEMENT,
12:19:51  20  	  P.NAME,
12:19:51  21  	  0 INHERITED,
12:19:51  22  	  P.IS_SHAREABLE,
12:19:51  23  	  MAX(LIC.ENTITLEMENT_END_DATE) EXPIRES
12:19:51  24  	FROM
12:19:51  25  	  SUBSCRIPTION SB
12:19:51  26  	  INNER JOIN ACCOUNT AC ON AC.ID = SB.ACCOUNT_ID
12:19:51  27  	  INNER JOIN LICENSE LIC ON LIC.SUBSCRIPTION_ID = SB.ID
12:19:51  28  	  INNER JOIN OFFER_PRODUCT_OFFERING OPO ON OPO.OFFER_ID = LIC.OFFER_ID
12:19:51  29  	  INNER JOIN PRODUCT_OFFERING PO ON PO.ID = OPO.PRODUCT_OFFERING_ID
12:19:51  30  	  INNER JOIN PRODUCT P ON P.ID = PO.PRODUCT_ID
12:19:51  31  	WHERE
12:19:51  32  	  LIC.ENTITLEMENT_END_DATE >= TRUNC(SYSDATE)
12:19:51  33  	  AND LIC.START_DATE <= SYSDATE
12:19:51  34  	  AND AC.GROUP_ID = in_group_id
12:19:51  35  	GROUP BY
12:19:51  36  	  P.ENTITLEMENT, 0, P.IS_SHAREABLE, P.NAME
12:19:51  37  UNION ALL
12:19:51  38  	SELECT
12:19:51  39  	  P.ENTITLEMENT,
12:19:51  40  	  P.NAME,
12:19:51  41  	  1 INHERITED,
12:19:51  42  	  P.IS_SHAREABLE,
12:19:51  43  	  MAX(LEAST(SS.END_DATE, LIC.ENTITLEMENT_END_DATE)) EXPIRES
12:19:51  44  	FROM
12:19:51  45  	  ACCOUNT BORROWER,
12:19:51  46  	  SUBSCRIPTION S,
12:19:51  47  	  LICENSE LIC,
12:19:51  48  	  OFFER_PRODUCT_OFFERING OPO,
12:19:51  49  	  PRODUCT_OFFERING PO,
12:19:51  50  	  PRODUCT P,
12:19:51  51  	  GROUP_ACCOUNT GA,
12:19:51  52  	  SUBSCRIPTION_SHARE SS
12:19:51  53  	WHERE
12:19:51  54  	  BORROWER.GROUP_ID = in_group_id
12:19:51  55  	  AND LIC.SUBSCRIPTION_ID = S.ID
12:19:51  56  	  AND OPO.OFFER_ID = LIC.OFFER_ID
12:19:51  57  	  AND PO.ID = OPO.PRODUCT_OFFERING_ID
12:19:51  58  	  AND PO.PRODUCT_ID = P.ID
12:19:51  59  	  AND GA.SUBSCRIPTION_ID = S.ID
12:19:51  60  	  AND SS.BORROWER_ACCOUNT_ID = BORROWER.ID
12:19:51  61  	  AND SS.GROUP_ACCOUNT_ID = GA.ID
12:19:51  62  	  AND SYSDATE BETWEEN SS.START_DATE AND SS.END_DATE
12:19:51  63  	  AND SYSDATE BETWEEN LIC.START_DATE AND LIC.ENTITLEMENT_END_DATE
12:19:51  64  	  AND P.IS_SHAREABLE = 1
12:19:51  65  	GROUP BY
12:19:51  66  	  P.ENTITLEMENT, 0, P.IS_SHAREABLE, P.NAME;
12:19:51  67  
12:19:51  68  	EXCEPTION
12:19:51  69  	  WHEN OTHERS THEN
12:19:51  70  	    PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51  71  	      SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51  72  	END GET_ALL_ENTITLEMENTS;
12:19:51  73  
12:19:51  74  	PROCEDURE GET_ITUNES_ENTITLEMENTS(
12:19:51  75  	  in_product_id IN CORE_OWNER.ITUNES_RECEIPT.PRODUCT_ID%TYPE,
