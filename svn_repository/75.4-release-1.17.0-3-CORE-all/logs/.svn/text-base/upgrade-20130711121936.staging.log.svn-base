12:19:51 416  
12:19:51 417  PROCEDURE FINALIZE_SUBSCRIPTION_CREATION (
12:19:51 418  	in_subscription_id    IN NUMBER,
12:19:51 419  	in_invoice_id	      IN NUMBER,
12:19:51 420  	in_instrument_type_id IN NUMBER,
12:19:51 421  	in_instrument_id      IN NUMBER,
12:19:51 422  	in_created_by	      IN VARCHAR2
12:19:51 423  ) AS
12:19:51 424  SPROC_NAME CONSTANT VARCHAR2(30) := 'FINALIZE_SUBSCRIPTION_CREATION';
12:19:51 425  -- VARIABLES
12:19:51 426  var_invoice_amount     NUMBER(10, 2);
12:19:51 427  var_new_transaction_id NUMBER;
12:19:51 428  var_new_charge_id      NUMBER;
12:19:51 429  -- EXCEPTIONS
12:19:51 430  CAN_NOT_USE_FCINSTR	  EXCEPTION;
12:19:51 431  CAN_NOT_CALC_INVOICE_AMOUNT EXCEPTION;
12:19:51 432  CAN_NOT_CREATE_TRANSACTION  EXCEPTION;
12:19:51 433  CAN_NOT_CREATE_CHARGE	  EXCEPTION;
12:19:51 434  
12:19:51 435  EXCEPTION_MESSAGE VARCHAR2(1024);
12:19:51 436  BEGIN
12:19:51 437  
12:19:51 438  	-- Calculate invoice amount ( + discounts, taxes)
12:19:51 439  	BEGIN
12:19:51 440  	  PROCS_INVOICE_V23.CALCULATE_INVOICE_AMOUNT(
12:19:51 441  	    in_invoice_id => in_invoice_id,
12:19:51 442  	    out_amount	  => var_invoice_amount
12:19:51 443  	  );
12:19:51 444  	  EXCEPTION
12:19:51 445  	   WHEN OTHERS THEN
12:19:51 446  	     EXCEPTION_MESSAGE := SQLERRM;
12:19:51 447  	     RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
12:19:51 448  	END;
12:19:51 449  
12:19:51 450  	IF in_instrument_type_id = GLOBAL_ENUMS_V23.INSTRUMENT_ZCI_INSTRUMENT
12:19:51 451  	  AND var_invoice_amount > 0 THEN
12:19:51 452  	  RAISE CAN_NOT_USE_FCINSTR;
12:19:51 453  	END IF;
12:19:51 454  
12:19:51 455  	IF var_invoice_amount = 0 THEN
12:19:51 456  	  -- UPDATE INVOICE. SET STATUS TO PROCESSED
12:19:51 457  	  PROCS_INVOICE_CRU_V23.UPDATE_INVOICE(
12:19:51 458  	    in_invoice_id		   => in_invoice_id,
12:19:51 459  	    in_updated_by		   => in_created_by,
12:19:51 460  	    in_invoice_status_id	   => GLOBAL_STATUSES_V23.INVOICE_CLOSED
12:19:51 461  	  );
12:19:51 462  	ELSE
12:19:51 463  	  -- Create transaction and charge
12:19:51 464  	  BEGIN
12:19:51 465  	    PROCS_TRANSACTION_V23.CREATE_TRANSACTION(
12:19:51 466  	      in_transaction_id 	=> NULL,
12:19:51 467  	      in_status_id		=> GLOBAL_STATUSES_V23.TRANSACTION_PENDING,
12:19:51 468  	      in_amount 		=> var_invoice_amount,
12:19:51 469  	      in_created_by		=> in_created_by,
12:19:51 470  	      in_order_id		=> NULL,
12:19:51 471  	      in_transaction_type_code	=> 'START_SUBSCRIPTION',
12:19:51 472  	      out_transaction_id	=> var_new_transaction_id
12:19:51 473  	    );
12:19:51 474  	    EXCEPTION
12:19:51 475  	      WHEN OTHERS THEN
12:19:51 476  		EXCEPTION_MESSAGE := SQLERRM;
12:19:51 477  		RAISE CAN_NOT_CREATE_TRANSACTION;
12:19:51 478  	  END;
12:19:51 479  
12:19:51 480  	  BEGIN
12:19:51 481  	    PROCS_CHARGE_V23.CREATE_CHARGE(
12:19:51 482  	      in_invoice_id	    => in_invoice_id,
12:19:51 483  	      in_transaction_id     => var_new_transaction_id,
12:19:51 484  	      in_instrument_type_id => in_instrument_type_id,
12:19:51 485  	      in_instrument_id	    => in_instrument_id,
12:19:51 486  	      in_charge_amount	    => var_invoice_amount,
12:19:51 487  	      in_created_by	    => in_created_by,
12:19:51 488  	      in_charge_status_id   => GLOBAL_STATUSES_V23.CHARGE_OPENED,
12:19:51 489  	      out_charge_id	    => var_new_charge_id
12:19:51 490  	    );
12:19:51 491  	    EXCEPTION
12:19:51 492  	      WHEN OTHERS THEN
12:19:51 493  		EXCEPTION_MESSAGE := SQLERRM;
12:19:51 494  		RAISE CAN_NOT_CREATE_CHARGE;
12:19:51 495  	  END;
12:19:51 496  	END IF;
12:19:51 497  
12:19:51 498  EXCEPTION
12:19:51 499  WHEN CAN_NOT_USE_FCINSTR THEN
12:19:51 500  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
12:19:51 501  	  SPROC_NAME, 'Could not use "free charge instrument" for non-zero invoice');
12:19:51 502  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
12:19:51 503  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 504  	  SPROC_NAME, 'Could not calculate invoice amount', EXCEPTION_MESSAGE);
12:19:51 505  WHEN CAN_NOT_CREATE_TRANSACTION THEN
12:19:51 506  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 507  	  SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
12:19:51 508  WHEN CAN_NOT_CREATE_CHARGE THEN
12:19:51 509  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 510  	  SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
12:19:51 511  WHEN OTHERS THEN
12:19:51 512  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 513  	  SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 514  END FINALIZE_SUBSCRIPTION_CREATION;
