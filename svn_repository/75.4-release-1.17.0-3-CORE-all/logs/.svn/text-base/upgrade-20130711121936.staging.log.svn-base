12:19:52 295   CURSOR c (v_from NUMBER, v_to NUMBER) IS SELECT ACCOUNT.GROUP_ID FROM ACCOUNT WHERE GROUP_ID BETWEEN v_from AND v_to;
12:19:52 296  BEGIN
12:19:52 297  -- arosolovskiy refactoring: call delete_user_account only "COUNT(group_id) WHERE ...." times instead of "in_end_group_id - in_start_group_id" times;
12:19:52 298  	/*
12:19:52 299  	FOR var_group_id IN in_start_group_id..in_end_group_id
