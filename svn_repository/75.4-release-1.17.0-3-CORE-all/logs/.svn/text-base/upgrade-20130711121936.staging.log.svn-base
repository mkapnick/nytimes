12:19:51 291  	  LICENSE.CURRENT_OFFER_INDEX,
12:19:51 292  	  LICENSE.CURRENT_OFFER_RECURR_NUM,
12:19:51 293  	  LICENSE.END_DATE,
12:19:51 294  	  LICENSE.ENTITLEMENT_END_DATE,
12:19:51 295  	  LICENSE.INVOICE_ID,
12:19:51 296  	  LICENSE.IS_EXTENSION,
12:19:51 297  	  LICENSE.LICENSE_STATUS_ID,
12:19:51 298  	  LICENSE.OFFER_ID,
12:19:51 299  	  LICENSE.START_DATE,
12:19:51 300  	  LICENSE.SUBSCRIPTION_ID,
12:19:51 301  	  LICENSE.UPDATE_DATE,
12:19:51 302  	  LICENSE.UPDATED_BY
12:19:51 303  	FROM
12:19:51 304  	  LICENSE
12:19:51 305  	WHERE
