12:19:52 399  
12:19:52 400  	-- Check that incoming data is correct
12:19:52 401  	IF in_instr_status != GLOBAL_CONSTANTS_V23.TRUE AND in_instr_status != GLOBAL_CONSTANTS_V23.FALSE THEN
12:19:52 402  	  RAISE WRONG_INSTR_EXCEPTION;
12:19:52 403  	END IF;
12:19:52 404  
12:19:52 405  	OPEN out_result_gc_set FOR
12:19:52 406  	SELECT
12:19:52 407  	  GIFT_CERTIFICATE.ID,
12:19:52 408  	  GIFT_CERTIFICATE.PURCHASER_GROUP_ID,
12:19:52 409  	  GIFT_CERTIFICATE.PURCHASE_INVOICE_ID,
