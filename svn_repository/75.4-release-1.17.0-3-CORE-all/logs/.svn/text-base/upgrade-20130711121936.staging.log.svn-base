12:19:51 299  	    in_invoice_status => GLOBAL_STATUSES_V23.INVOICE_OPEN,
12:19:51 300  	    in_created_by     => in_created_by,
12:19:51 301  	    in_tax_exempt_id  => var_account_tax_exempt_id,
12:19:51 302  	    out_invoice_id    => var_new_invoice_id
12:19:51 303  	  );
12:19:51 304  	  EXCEPTION
12:19:51 305  	    WHEN OTHERS THEN
12:19:51 306  	      EXCEPTION_MESSAGE := SQLERRM;
12:19:51 307  	      RAISE CAN_NOT_CREATE_INVOICE;
12:19:51 308  	END;
12:19:51 309  
12:19:51 310  	BEGIN
12:19:51 311  	  PROCS_LINE_ITEMS_V23.ADD_LINE_ITEMS(
12:19:51 312  	    in_invoice_id => var_new_invoice_id,
12:19:51 313  	    in_offer_id   => var_first_offer_id,
12:19:51 314  	    in_created_by => in_created_by
12:19:51 315  	  );
12:19:51 316  	  EXCEPTION
12:19:51 317  	    WHEN OTHERS THEN
12:19:51 318  	      EXCEPTION_MESSAGE := SQLERRM;
12:19:51 319  	      RAISE CAN_NOT_CREATE_LINE_ITEMS;
12:19:51 320  	END;
12:19:51 321  
12:19:51 322  	PROCS_SUBSCRIPTION_CRU_V23.CREATE_SUBSCRIPTION(
12:19:51 323  	  out_subscription_id	    => var_new_subscription_id,
12:19:51 324  	  in_account_id 	    => var_account_id,
12:19:51 325  	  in_purchase_date	    => var_date,
12:19:51 326  	  in_offer_chain_id	    => in_offer_chain_id,
12:19:51 327  	  in_created_by 	    => in_created_by,
12:19:51 328  	  in_instrument_type_id     => in_instrument_type_id,
12:19:51 329  	  in_instrument_id	    => in_instrument_id,
12:19:51 330  	  in_subscription_status_id => GLOBAL_STATUSES_V23.SUBSCRIPTION_ACTIVE
12:19:51 331  	);
12:19:51 332  
12:19:51 333  	BEGIN
12:19:51 334  	  PROCS_SUBSCRIPTION_V23.ANNOTATE_SUBSCRIPTION(
12:19:51 335  	    in_subscription_id => var_new_subscription_id,
12:19:51 336  	    in_agent_id        => in_agent_id,
12:19:51 337  	    in_note	       => in_note,
12:19:51 338  	    in_created_by      => in_created_by
12:19:51 339  	  );
12:19:51 340  	  EXCEPTION
12:19:51 341  	   WHEN OTHERS THEN
12:19:51 342  	     EXCEPTION_MESSAGE := SQLERRM;
12:19:51 343  	     RAISE CAN_NOT_CREATE_NOTE;
12:19:51 344  	END;
12:19:51 345  
12:19:51 346  	BEGIN
12:19:51 347  	  PROCS_LICENSE_V23.CREATE_LICENSE(
12:19:51 348  	    in_status_id		=> GLOBAL_STATUSES_V23.LICENSE_ACTIVE,
12:19:51 349  	    in_needs_entitlements	=> GLOBAL_CONSTANTS_V23.TRUE,
12:19:51 350  	    in_start_date		=> var_date,
12:19:51 351  	    in_end_date 		=> NULL, -- Will be calculated automatically
12:19:51 352  	    in_offer_id 		=> var_first_offer_id,
12:19:51 353  	    in_subscription_id		=> var_new_subscription_id,
12:19:51 354  	    in_invoice_id		=> var_new_invoice_id,
12:19:51 355  	    in_created_by		=> in_created_by,
12:19:51 356  	    in_is_extension		=> GLOBAL_CONSTANTS_V23.FALSE,
12:19:51 357  	    in_current_offer_index	=> PROCS_OFFER_CHAIN_V23.GET_FIRST_OFFER_INDEX(in_offer_chain_id),
12:19:51 358  	    in_current_offer_recurr_num => 1,
12:19:51 359  	    out_license_id		=> out_new_license_id
12:19:51 360  	  );
12:19:51 361  	  EXCEPTION
12:19:51 362  	    WHEN OTHERS THEN
12:19:51 363  	      EXCEPTION_MESSAGE := SQLERRM;
12:19:51 364  	      RAISE CAN_NOT_CREATE_LICENSE;
12:19:51 365  	END;
12:19:51 366  
12:19:51 367  	out_subscription_id := var_new_subscription_id;
12:19:51 368  	out_invoice_id := var_new_invoice_id;
12:19:51 369  
12:19:51 370  EXCEPTION
12:19:51 371  WHEN BAD_OFFER_CHAIN_STATUS THEN
12:19:51 372  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
12:19:51 373  	  SPROC_NAME, 'Offer chain is not active');
12:19:51 374  WHEN LIMIT_REACHED THEN
12:19:51 375  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.CONFLICT_ERROR,
12:19:51 376  	  SPROC_NAME, 'Limit reached for given offer chain. Concurrent subscription id: ' || var_concur_subscription_id);
12:19:51 377  WHEN CAN_NOT_CREATE_FOR_DISABLE THEN
12:19:51 378  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
12:19:51 379  	  SPROC_NAME, 'Can not create subsscription for disabled account');
12:19:51 380  WHEN CAN_NOT_SUBSCRIBE_TO_GC THEN
12:19:51 381  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
12:19:51 382  	  SPROC_NAME, 'Can not subscribe to Offer Chain for Gift Certificate');
12:19:51 383  WHEN CAN_NOT_SUBSCRIBE_TO_RGC THEN
12:19:51 384  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
