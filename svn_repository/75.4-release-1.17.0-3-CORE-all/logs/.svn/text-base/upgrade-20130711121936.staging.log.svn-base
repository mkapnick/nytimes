12:19:51 847  	      END IF;
12:19:51 848  	      FOR i_offer_recurrences_iterator IN 1..f_offer_data.NUM_RECURRENCES
12:19:51 849  	      LOOP
12:19:51 850  		PROCS_LINE_ITEMS_V23.ADD_LINE_ITEMS(
