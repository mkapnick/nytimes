12:19:51 760  
12:19:51 761  PROCEDURE GET_SUBSCRIPTION_INVOICES (
12:19:51 762  	in_subscription_id  IN	NUMBER,
12:19:51 763  	out_result_set	    OUT SYS_REFCURSOR
12:19:51 764  ) AS
12:19:51 765  SPROC_NAME	   CONSTANT VARCHAR2(25) := 'GET_SUBSCRIPTION_INVOICES';
12:19:51 766  temp_subscription_id NUMBER;
12:19:51 767  BEGIN
12:19:51 768  	-- Note: A subscription has one or more associated licenses, each of which has an associated invoice.
12:19:51 769  	-- Find associated LICENSES for the subscription by "LICENSE.subscription_id"
12:19:51 770  	--   for each license
12:19:51 771  	--     get associated invoice
12:19:51 772  	--     add to results list
12:19:51 773  	--   end loop
12:19:51 774  	-- end
12:19:51 775  
12:19:51 776  	SELECT
12:19:51 777  	  SUBSCRIPTION.ID into temp_subscription_id
12:19:51 778  	FROM
12:19:51 779  	  SUBSCRIPTION
12:19:51 780  	WHERE
12:19:51 781  	  SUBSCRIPTION.ID = in_subscription_id;
12:19:51 782  
12:19:51 783  	OPEN out_result_set FOR
12:19:51 784  	SELECT
12:19:51 785  	  INVOICE.ID,
12:19:51 786  	  INVOICE.INVOICE_STATUS_ID,
12:19:51 787  	  INVOICE.CREATE_DATE,
12:19:51 788  	  INVOICE.CREATED_BY,
12:19:51 789  	  INVOICE.UPDATE_DATE,
12:19:51 790  	  INVOICE.UPDATED_BY,
12:19:51 791  	  INVOICE.TAX_EXEMPT_ID
12:19:51 792  	FROM
12:19:51 793  	  LICENSE
12:19:51 794  	    INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
12:19:51 795  	WHERE
12:19:51 796  	  LICENSE.SUBSCRIPTION_ID = in_subscription_id;
12:19:51 797  
12:19:51 798  EXCEPTION
12:19:51 799  WHEN NO_DATA_FOUND THEN
12:19:51 800  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 801  	  SPROC_NAME, 'Could not find subscription with given ID');
12:19:51 802  WHEN OTHERS THEN
12:19:51 803  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 804  	  SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 805  END GET_SUBSCRIPTION_INVOICES;
12:19:51 806  
