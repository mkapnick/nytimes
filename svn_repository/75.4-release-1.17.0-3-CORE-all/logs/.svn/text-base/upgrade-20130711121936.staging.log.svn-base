12:19:51 327  
12:19:51 328  PROCEDURE GET_RECURRING_OFFER (
12:19:51 329  	in_license_id  IN NUMBER,
12:19:51 330  	out_result_set OUT SYS_REFCURSOR
12:19:51 331  ) AS
12:19:51 332  -- VARIABLES
12:19:51 333  SPROC_NAME		    CONSTANT VARCHAR2(19) := 'GET_RECURRING_OFFER';
12:19:51 334  var_offer_chain_id	    NUMBER;
12:19:51 335  var_offer_id		    NUMBER;
12:19:51 336  var_offer_num_recurrences     NUMBER;
12:19:51 337  var_license_cur_offer_rec_num NUMBER;
12:19:51 338  var_offer_index		    NUMBER;
12:19:51 339  -- EXCEPTIONS
12:19:51 340  BAD_LICENSE_ID	     EXCEPTION;
12:19:51 341  CAN_NOT_GET_OFFER_INFO EXCEPTION;
12:19:51 342  BEGIN
12:19:51 343  
12:19:51 344  	BEGIN
12:19:51 345  	  SELECT
12:19:51 346  	    SUBSCRIPTION.OFFER_CHAIN_ID,
12:19:51 347  	    LICENSE.OFFER_ID,
12:19:51 348  	    LICENSE.CURRENT_OFFER_RECURR_NUM
12:19:51 349  	    into
12:19:51 350  	    var_offer_chain_id,
12:19:51 351  	    var_offer_id,
12:19:51 352  	    var_license_cur_offer_rec_num
12:19:51 353  	  FROM
12:19:51 354  	    LICENSE
12:19:51 355  	    INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
12:19:51 356  	  WHERE
12:19:51 357  	    LICENSE.ID = in_license_id;
12:19:51 358  	  EXCEPTION
12:19:51 359  	    WHEN NO_DATA_FOUND THEN
12:19:51 360  	      RAISE BAD_LICENSE_ID;
12:19:51 361  	END;
12:19:51 362  
12:19:51 363  	BEGIN
12:19:51 364  	  SELECT
12:19:51 365  	    OFFER_OFFER_CHAIN.NUM_RECURRENCES,
12:19:51 366  	    OFFER_OFFER_CHAIN.INDEX_VALUE
12:19:51 367  	    into
12:19:51 368  	    var_offer_num_recurrences,
12:19:51 369  	    var_offer_index
12:19:51 370  	  FROM
12:19:51 371  	    OFFER_OFFER_CHAIN
12:19:51 372  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
12:19:51 373  	  WHERE
12:19:51 374  	    OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
12:19:51 375  	    AND OFFER_OFFER_CHAIN.OFFER_ID = var_offer_id;
12:19:51 376  	  EXCEPTION
12:19:51 377  	    WHEN NO_DATA_FOUND THEN
12:19:51 378  	      RAISE CAN_NOT_GET_OFFER_INFO;
12:19:51 379  	END;
12:19:51 380  
12:19:51 381  	IF var_offer_num_recurrences = 0 THEN
12:19:51 382  	  --out_result_set := NULL;
12:19:51 383  	  -- TODO: Remove this hardcode
12:19:51 384  	  OPEN out_result_set FOR
12:19:51 385  	  SELECT
12:19:51 386  	    OFFER.ID,
12:19:51 387  	    OFFER.OFFER_STATUS_ID,
12:19:51 388  	    OFFER.ENTITLEMENT_DURATION,
12:19:51 389  	    OFFER.CREATED_BY,
12:19:51 390  	    OFFER.CREATE_DATE,
12:19:51 391  	    OFFER.UPDATED_BY,
12:19:51 392  	    OFFER.UPDATE_DATE,
12:19:51 393  	    var_offer_num_recurrences as "RECURRENCE_NUMBER",
12:19:51 394  	    var_offer_index as "OFFER_INDEX"
12:19:51 395  	  FROM
12:19:51 396  	    OFFER_OFFER_CHAIN
12:19:51 397  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
12:19:51 398  	  WHERE ROWNUM = 0;
12:19:51 399  	ELSIF var_license_cur_offer_rec_num = var_offer_num_recurrences THEN
12:19:51 400  	  --out_result_set := NULL;
12:19:51 401  	  -- TODO: Remove this hardcode
12:19:51 402  	  OPEN out_result_set FOR
12:19:51 403  	  SELECT
12:19:51 404  	    OFFER.ID,
12:19:51 405  	    OFFER.OFFER_STATUS_ID,
12:19:51 406  	    OFFER.ENTITLEMENT_DURATION,
12:19:51 407  	    OFFER.CREATED_BY,
12:19:51 408  	    OFFER.CREATE_DATE,
12:19:51 409  	    OFFER.UPDATED_BY,
12:19:51 410  	    OFFER.UPDATE_DATE,
12:19:51 411  	    var_offer_num_recurrences as "RECURRENCE_NUMBER",
12:19:51 412  	    var_offer_index as "OFFER_INDEX"
12:19:51 413  	  FROM
12:19:51 414  	    OFFER_OFFER_CHAIN
12:19:51 415  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
12:19:51 416  	  WHERE ROWNUM = 0;
12:19:51 417  	ELSE
12:19:51 418  	  OPEN out_result_set FOR
12:19:51 419  	  SELECT
12:19:51 420  	    OFFER.ID,
12:19:51 421  	    OFFER.OFFER_STATUS_ID,
12:19:51 422  	    OFFER.ENTITLEMENT_DURATION,
12:19:51 423  	    OFFER.CREATED_BY,
12:19:51 424  	    OFFER.CREATE_DATE,
12:19:51 425  	    OFFER.UPDATED_BY,
12:19:51 426  	    OFFER.UPDATE_DATE,
12:19:51 427  	    var_offer_num_recurrences as "RECURRENCE_NUMBER",
12:19:51 428  	    var_offer_index as "OFFER_INDEX"
12:19:51 429  	  FROM
12:19:51 430  	    OFFER
12:19:51 431  	  WHERE
12:19:51 432  	    OFFER.ID = var_offer_id;
12:19:51 433  	END IF;
12:19:51 434  
12:19:51 435  EXCEPTION
12:19:51 436  WHEN BAD_LICENSE_ID THEN
12:19:51 437  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 438  	  SPROC_NAME, 'No such license');
12:19:51 439  WHEN CAN_NOT_GET_OFFER_INFO THEN
12:19:51 440  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 441  	  SPROC_NAME, 'Could not get offer information. Offer id = '||var_offer_id||', Offer chain id = '||var_offer_chain_id);
12:19:51 442  WHEN OTHERS THEN
12:19:51 443  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 444  	  SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 445  END GET_RECURRING_OFFER;
