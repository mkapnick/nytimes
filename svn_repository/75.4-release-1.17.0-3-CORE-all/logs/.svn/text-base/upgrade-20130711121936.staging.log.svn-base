12:19:51 1015  /******************************************************************************/
12:19:51 1016  
12:19:51 1017  PROCEDURE GET_MAX_REFUND (
12:19:51 1018  	 in_invoice_id IN NUMBER,
12:19:51 1019  	 out_amount    OUT NUMBER
12:19:51 1020  ) AS
12:19:51 1021  SPROC_NAME CONSTANT VARCHAR2(14) := 'GET_MAX_REFUND';
12:19:51 1022  -- VARIABLES
12:19:51 1023  temp_invoice_id	       NUMBER;
12:19:51 1024  var_invoice_refunds_sum NUMBER(10,2);
12:19:51 1025  var_invoice_charges_sum NUMBER(10,2);
12:19:51 1026  -- EXCEPTIONS
12:19:51 1027  BAD_INVOICE_ID EXCEPTION;
12:19:51 1028  BEGIN
12:19:51 1029  
12:19:51 1030  	 BEGIN
12:19:51 1031  	   SELECT
12:19:51 1032  	     INVOICE.ID into temp_invoice_id
12:19:51 1033  	   FROM
12:19:51 1034  	     INVOICE
12:19:51 1035  	   WHERE
12:19:51 1036  	     INVOICE.ID = in_invoice_id;
12:19:51 1037  	   EXCEPTION
12:19:51 1038  	     WHEN NO_DATA_FOUND THEN
12:19:51 1039  	       RAISE BAD_INVOICE_ID;
12:19:51 1040  	 END;
12:19:51 1041  
12:19:51 1042  	 var_invoice_refunds_sum := 0;
12:19:51 1043  	 var_invoice_charges_sum := 0;
12:19:51 1044  
12:19:51 1045  	 FOR f_charge IN (
12:19:51 1046  	   SELECT
12:19:51 1047  	     CHARGE.ID,
12:19:51 1048  	     CHARGE.CHARGE_STATUS_ID,
12:19:51 1049  	     CHARGE.CHARGE_AMOUNT,
12:19:51 1050  	     CHARGE.TRANSACTION_ID
12:19:51 1051  	   FROM
12:19:51 1052  	     CHARGE
12:19:51 1053  	   WHERE
12:19:51 1054  	     CHARGE.INVOICE_ID = in_invoice_id
12:19:51 1055  	 )
12:19:51 1056  	 LOOP
12:19:51 1057  	   -- If charge.status = canceled then continue
12:19:51 1058  	   IF f_charge.CHARGE_STATUS_ID != GLOBAL_STATUSES_V23.CHARGE_CANCELED THEN
12:19:51 1059  
12:19:51 1060  	     IF f_charge.CHARGE_AMOUNT > 0 THEN
12:19:51 1061  	       IF f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V23.CHARGE_PROCESSED
12:19:51 1062  		  AND PROCS_TRANSACTION_V23.IS_TRANSACTION_COLLECTED(f_charge.TRANSACTION_ID) = GLOBAL_CONSTANTS_V23.TRUE THEN
12:19:51 1063  		 -- Transaction collected
12:19:51 1064  		 var_invoice_charges_sum := var_invoice_charges_sum + f_charge.CHARGE_AMOUNT;
12:19:51 1065  	       ELSE
12:19:51 1066  		 -- Transaction is not collected. Do nothing
12:19:51 1067  		 NULL;
12:19:51 1068  	       END IF;
12:19:51 1069  	     ELSE
12:19:51 1070  	       IF f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V23.CHARGE_PROCESSED
12:19:51 1071  		  AND PROCS_TRANSACTION_V23.IS_TRANSACTION_COLLECTED(f_charge.TRANSACTION_ID) = GLOBAL_CONSTANTS_V23.FALSE THEN
12:19:51 1072  		 -- If charge is processed transaction is not collected then do nothing
12:19:51 1073  		 NULL;
12:19:51 1074  	       ELSE
12:19:51 1075  		 var_invoice_refunds_sum := var_invoice_refunds_sum + f_charge.CHARGE_AMOUNT;
12:19:51 1076  	       END IF;
12:19:51 1077  	     END IF;
12:19:51 1078  
12:19:51 1079  	   END IF;
12:19:51 1080  	 END LOOP;
12:19:51 1081  
12:19:51 1082  	 -- Refunds are negative
12:19:51 1083  	 var_invoice_refunds_sum := 0 - var_invoice_refunds_sum;
12:19:51 1084  
12:19:51 1085  	 out_amount := var_invoice_charges_sum - var_invoice_refunds_sum;
12:19:51 1086  
12:19:51 1087  EXCEPTION
12:19:51 1088  WHEN BAD_INVOICE_ID THEN
12:19:51 1089  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 1090  	   SPROC_NAME, 'No such invoice');
12:19:51 1091  WHEN OTHERS THEN
12:19:51 1092  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 1093  	   SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 1094  END GET_MAX_REFUND;
12:19:51 1095  
