12:19:51 318  PROCEDURE DISABLE_CREDIT_CARD (
12:19:51 319  /*
12:19:51 320  Throws exceptions:
12:19:51 321  APP_EXCEPTION_CODES_V23.NOT_FOUND
12:19:51 322  APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR
12:19:51 323  APP_EXCEPTION_CODES_V23.INTERNAL_ERROR
12:19:51 324  */
12:19:51 325  	in_credit_card_id IN NUMBER,
12:19:51 326  	in_updated_by	  IN VARCHAR2
12:19:51 327  ) AS
12:19:51 328  SPROC_NAME		   CONSTANT VARCHAR2(19) := 'DISABLE_CREDIT_CARD';
12:19:51 329  -- VARIBLES
12:19:51 330  var_account_id		     NUMBER;
12:19:51 331  var_group_id		     NUMBER;
12:19:51 332  var_credit_card_status	     NUMBER;
12:19:51 333  var_pending_transactions_num   NUMBER;
12:19:51 334  var_pending_invoices_num	     NUMBER;
12:19:51 335  current_def_instrument_type_id NUMBER;
12:19:51 336  current_def_instrument_id      NUMBER;
12:19:51 337  -- EXCEPTIONS
12:19:51 338  BAD_CC_STATUS		   EXCEPTION;
12:19:51 339  PENDING_TRANSACTIONS_FOUNDED EXCEPTION;
12:19:51 340  CAN_NOT_GET_DEF_FINANCIAL    EXCEPTION;
12:19:51 341  CAN_NOT_DEL_DEF_FINANCIAL    EXCEPTION;
12:19:51 342  CAN_NOT_DISABLE_CREDIT_CARD  EXCEPTION;
12:19:51 343  EXCEPTION_MESSAGE 	   VARCHAR2(1024);
12:19:51 344  BEGIN
12:19:51 345  
12:19:51 346  	-- Get credit card status
12:19:51 347  	-- Get account id
12:19:51 348  	SELECT
12:19:51 349  	  CREDIT_CARD.CREDIT_CARD_STATUS_ID,
12:19:51 350  	  CREDIT_CARD.ACCOUNT_ID
12:19:51 351  	  into
12:19:51 352  	  var_credit_card_status,
12:19:51 353  	  var_account_id
12:19:51 354  	FROM
12:19:51 355  	  CREDIT_CARD
12:19:51 356  	WHERE
12:19:51 357  	  CREDIT_CARD.ID = in_credit_card_id;
12:19:51 358  
12:19:51 359  	-- Check that we can disable this credit card (STUB)
12:19:51 360  	IF F_CAN_DISABLE_CREDIT_CARD(in_credit_card_id) = GLOBAL_CONSTANTS_V23.FALSE THEN
12:19:51 361  	  RAISE CAN_NOT_DISABLE_CREDIT_CARD;
12:19:51 362  	END IF;
12:19:51 363  
12:19:51 364  	-- Get account id
12:19:51 365  	SELECT
12:19:51 366  	  ACCOUNT.GROUP_ID into var_group_id
12:19:51 367  	FROM
12:19:51 368  	  ACCOUNT
12:19:51 369  	WHERE
12:19:51 370  	  ACCOUNT.ID = var_account_id;
12:19:51 371  
12:19:51 372  	-- Card should to be active
12:19:51 373  	IF var_credit_card_status != GLOBAL_STATUSES_V23.CREDIT_CARD_ACTIVE THEN
12:19:51 374  	  RAISE BAD_CC_STATUS;
12:19:51 375  	END IF;
12:19:51 376  
12:19:51 377  	-- Looking for pending transactions associated with given credit card
12:19:51 378  	SELECT
12:19:51 379  	  COUNT(*) into var_pending_invoices_num
12:19:51 380  	FROM
12:19:51 381  	  CHARGE
12:19:51 382  	  INNER JOIN TRANSACTION ON CHARGE.TRANSACTION_ID = TRANSACTION.ID
12:19:51 383  	WHERE
12:19:51 384  	  CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V23.INSTRUMENT_CREDIT_CARD
12:19:51 385  	  AND CHARGE.INSTRUMENT_ID = in_credit_card_id
12:19:51 386  	  AND TRANSACTION.TRANSACTION_STATUS_ID = GLOBAL_STATUSES_V23.TRANSACTION_PENDING;
12:19:51 387  
12:19:51 388  	IF var_pending_invoices_num > 0 THEN
12:19:51 389  	  RAISE PENDING_TRANSACTIONS_FOUNDED;
12:19:51 390  	END IF;
12:19:51 391  
12:19:51 392  	-- Getting current default financial instrument
12:19:51 393  	BEGIN
12:19:51 394  	  GET_DEF_FINANCIAL_INSTRUMENT(
12:19:51 395  	    in_group_id 	   => var_group_id,
12:19:51 396  	    out_instrument_type_id => current_def_instrument_type_id,
12:19:51 397  	    out_instrument_id	   => current_def_instrument_id
12:19:51 398  	  );
12:19:51 399  	  EXCEPTION
12:19:51 400  	    WHEN OTHERS THEN
12:19:51 401  	      EXCEPTION_MESSAGE := SQLERRM;
12:19:51 402  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
12:19:51 403  	END;
12:19:51 404  
12:19:51 405  	-- Checking that credit card is not default
12:19:51 406  	IF current_def_instrument_type_id = GLOBAL_ENUMS_V23.INSTRUMENT_CREDIT_CARD
12:19:51 407  	  AND current_def_instrument_id = in_credit_card_id THEN
12:19:51 408  	  BEGIN
12:19:51 409  	    DEL_DEF_FINANCIAL_INSTRUMENT(
12:19:51 410  	      in_group_id => var_group_id
12:19:51 411  	    );
12:19:51 412  	  EXCEPTION
12:19:51 413  	    WHEN OTHERS THEN
12:19:51 414  	      EXCEPTION_MESSAGE := SQLERRM;
12:19:51 415  	      RAISE CAN_NOT_DEL_DEF_FINANCIAL;
12:19:51 416  	  END;
12:19:51 417  	END IF;
12:19:51 418  
12:19:51 419  	-- Update credit card status
12:19:51 420  	PROCS_FIN_INSTRUMENTS_V23.UPDATE_CREDIT_CARD_STATUS(
12:19:51 421  	  in_credit_card_id	   => in_credit_card_id,
12:19:51 422  	  in_updated_by 	   => in_updated_by,
12:19:51 423  	  in_credit_card_status_id => GLOBAL_STATUSES_V23.CREDIT_CARD_DISABLED
12:19:51 424  	);
12:19:51 425  
12:19:51 426  EXCEPTION
12:19:51 427  WHEN NO_DATA_FOUND THEN
12:19:51 428  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 429  	  SPROC_NAME, 'No such credit card');
12:19:51 430  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
12:19:51 431  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 432  	  SPROC_NAME, 'Could not get current default financial instrument', EXCEPTION_MESSAGE);
12:19:51 433  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
12:19:51 434  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 435  	  SPROC_NAME, 'Could not delete information about default financial instrument from account', EXCEPTION_MESSAGE);
12:19:51 436  WHEN BAD_CC_STATUS THEN
12:19:51 437  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
12:19:51 438  	  SPROC_NAME, 'Credit card is not active');
12:19:51 439  WHEN PENDING_TRANSACTIONS_FOUNDED THEN
12:19:51 440  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
12:19:51 441  	  SPROC_NAME, 'Account has pending charge which is using this card');
12:19:51 442  WHEN CAN_NOT_DISABLE_CREDIT_CARD THEN
12:19:51 443  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 444  	  SPROC_NAME, 'Could not disable this credit card', EXCEPTION_MESSAGE);
12:19:51 445  WHEN OTHERS THEN
12:19:51 446  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 447  	  SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 448  END DISABLE_CREDIT_CARD;
