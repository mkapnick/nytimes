12:19:51 1138  /******************************************************************************/
12:19:51 1139  
12:19:51 1140  PROCEDURE REFUND_SUBSCRIPTION (
12:19:51 1141  	 in_subscription_id IN NUMBER,
12:19:51 1142  	 in_invoice_id	    IN NUMBER,
12:19:51 1143  	 in_refund_amount   IN NUMBER,
12:19:51 1144  	 in_note	    IN VARCHAR2,
12:19:51 1145  	 in_created_by	    IN VARCHAR2,
12:19:51 1146  	 out_charge_id	    OUT NUMBER
12:19:51 1147  ) AS
12:19:51 1148  -- VARIABLES
12:19:51 1149  SPROC_NAME	      CONSTANT VARCHAR2(19) := 'REFUND_SUBSCRIPTION';
12:19:51 1150  var_invoice_status_id  NUMBER;
12:19:51 1151  var_account_id	      NUMBER;
12:19:51 1152  var_account_status_id  NUMBER;
12:19:51 1153  var_new_transaction_id NUMBER;
12:19:51 1154  var_instrument_type_id NUMBER;
12:19:51 1155  var_instrument_id      NUMBER;
12:19:51 1156  var_new_charge_id      NUMBER;
12:19:51 1157  var_invoice_amount     NUMBER(10,2);
12:19:51 1158  var_refunds_before     NUMBER(10,2);
12:19:51 1159  var_charges_amount     NUMBER(10,2);
12:19:51 1160  -- EXCEPTIONS
12:19:51 1161  BAD_SUBSCRIPTION_ID	     EXCEPTION;
12:19:51 1162  ACCOUNT_IS_FROZEN	     EXCEPTION;
12:19:51 1163  BAD_INVOICE_ID		     EXCEPTION;
12:19:51 1164  CAN_NOT_CREATE_TRANSACTION    EXCEPTION;
12:19:51 1165  CAN_NOT_CREATE_CHARGE	     EXCEPTION;
12:19:51 1166  CAN_NOT_CALC_INVOICE_AMOUNT   EXCEPTION;
12:19:51 1167  REFUND_IS_GREATER_THAN_ANOUNT EXCEPTION;
12:19:51 1168  CAN_NOT_ANNOTATE_SUBSCRIPTION EXCEPTION;
12:19:51 1169  TOT_REF_IS_GREATER_THAN_ANOUNT EXCEPTION;
12:19:51 1170  INVOICE_IS_NOT_CLOSED	     EXCEPTION;
12:19:51 1171  TOT_REF_IS_GRATER_THAN_CHARGES EXCEPTION;
12:19:51 1172  EXCEPTION_MESSAGE	      VARCHAR2(1024);
12:19:51 1173  BEGIN
12:19:51 1174  
12:19:51 1175  	 BEGIN
12:19:51 1176  	   SELECT
12:19:51 1177  	     SUBSCRIPTION.INSTRUMENT_ID,
12:19:51 1178  	     SUBSCRIPTION.INSTRUMENT_TYPE_ID,
12:19:51 1179  	     SUBSCRIPTION.ACCOUNT_ID
12:19:51 1180  	     into
12:19:51 1181  	     var_instrument_id,
12:19:51 1182  	     var_instrument_type_id,
12:19:51 1183  	     var_account_id
12:19:51 1184  	   FROM
12:19:51 1185  	     SUBSCRIPTION
12:19:51 1186  	   WHERE
12:19:51 1187  	     SUBSCRIPTION.ID = in_subscription_id;
12:19:51 1188  	   EXCEPTION
12:19:51 1189  	     WHEN NO_DATA_FOUND THEN
12:19:51 1190  	       RAISE BAD_SUBSCRIPTION_ID;
12:19:51 1191  	 END;
12:19:51 1192  
12:19:51 1193  	 -- Check account status. It should not to be frozen
12:19:51 1194  	 SELECT
12:19:51 1195  	   ACCOUNT.ACCOUNT_STATUS_ID into var_account_status_id
12:19:51 1196  	 FROM
12:19:51 1197  	   ACCOUNT
12:19:51 1198  	 WHERE
12:19:51 1199  	   ACCOUNT.ID = var_account_id;
12:19:51 1200  
12:19:51 1201  	 IF var_account_status_id = GLOBAL_STATUSES_V23.ACCOUNT_FROZEN THEN
12:19:51 1202  	   RAISE ACCOUNT_IS_FROZEN;
12:19:51 1203  	 END IF;
12:19:51 1204  
12:19:51 1205  	 BEGIN
12:19:51 1206  	   SELECT
12:19:51 1207  	     INVOICE.INVOICE_STATUS_ID into var_invoice_status_id
12:19:51 1208  	   FROM
12:19:51 1209  	     INVOICE
12:19:51 1210  	   WHERE
12:19:51 1211  	     INVOICE.ID = in_invoice_id;
12:19:51 1212  	   EXCEPTION
12:19:51 1213  	     WHEN NO_DATA_FOUND THEN
12:19:51 1214  	       RAISE BAD_INVOICE_ID;
12:19:51 1215  	 END;
12:19:51 1216  
12:19:51 1217  	 IF var_invoice_status_id != GLOBAL_STATUSES_V23.INVOICE_CLOSED THEN
12:19:51 1218  	   RAISE INVOICE_IS_NOT_CLOSED;
12:19:51 1219  	 END IF;
12:19:51 1220  
12:19:51 1221  	 BEGIN
12:19:51 1222  	   PROCS_INVOICE_V23.CALCULATE_INVOICE_AMOUNT (
12:19:51 1223  	     in_invoice_id => in_invoice_id,
12:19:51 1224  	     out_amount    => var_invoice_amount
12:19:51 1225  	   );
12:19:51 1226  	   EXCEPTION
12:19:51 1227  	     WHEN OTHERS THEN
12:19:51 1228  	       EXCEPTION_MESSAGE := SQLERRM;
12:19:51 1229  	       RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
12:19:51 1230  	 END;
12:19:51 1231  
12:19:51 1232  	 IF ( in_refund_amount > var_invoice_amount ) THEN
12:19:51 1233  	   RAISE REFUND_IS_GREATER_THAN_ANOUNT;
12:19:51 1234  	 END IF;
12:19:51 1235  
12:19:51 1236  	 SELECT /*+ STAR_TRANSFORMATION */
12:19:51 1237  	   SUM(CHARGE.CHARGE_AMOUNT) into var_refunds_before
12:19:51 1238  	 FROM
12:19:51 1239  	   CHARGE
12:19:51 1240  	 WHERE
12:19:51 1241  	   CHARGE.INVOICE_ID = in_invoice_id
12:19:51 1242  	   AND (
12:19:51 1243  	     CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V23.CHARGE_OPENED
12:19:51 1244  	     OR CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V23.CHARGE_PROCESSED
12:19:51 1245  	   )
12:19:51 1246  	   AND CHARGE.CHARGE_AMOUNT < 0;
12:19:51 1247  
12:19:51 1248  	 -- Refunds are negative
12:19:51 1249  	 var_refunds_before := -var_refunds_before;
12:19:51 1250  
12:19:51 1251  	 var_charges_amount := 0;
12:19:51 1252  
12:19:51 1253  	 FOR f_processed_charges IN (
12:19:51 1254  	   SELECT
12:19:51 1255  	     CHARGE.CHARGE_AMOUNT
12:19:51 1256  	   FROM
12:19:51 1257  	     CHARGE
12:19:51 1258  	   WHERE
12:19:51 1259  	     CHARGE.INVOICE_ID = in_invoice_id
12:19:51 1260  	     AND CHARGE.CHARGE_STATUS_ID IN (SELECT GLOBAL_STATUSES_V23.CHARGE_PROCESSED FROM DUAL)
12:19:51 1261  	 )
12:19:51 1262  	 LOOP
12:19:51 1263  	   IF f_processed_charges.CHARGE_AMOUNT > 0 THEN
12:19:51 1264  	     var_charges_amount := var_charges_amount + f_processed_charges.CHARGE_AMOUNT;
12:19:51 1265  	   END IF;
12:19:51 1266  	 END LOOP;
12:19:51 1267  
12:19:51 1268  	 IF (in_refund_amount + var_refunds_before > var_invoice_amount) THEN
12:19:51 1269  	   RAISE TOT_REF_IS_GREATER_THAN_ANOUNT;
12:19:51 1270  	 END IF;
12:19:51 1271  
12:19:51 1272  	 IF (in_refund_amount + var_refunds_before > var_charges_amount) THEN
12:19:51 1273  	   RAISE TOT_REF_IS_GRATER_THAN_CHARGES;
12:19:51 1274  	 END IF;
12:19:51 1275  
12:19:51 1276  	 BEGIN
12:19:51 1277  	   PROCS_TRANSACTION_V23.CREATE_TRANSACTION(
12:19:51 1278  	     in_transaction_id	       => NULL,
12:19:51 1279  	     in_status_id	       => GLOBAL_STATUSES_V23.TRANSACTION_PREPARE,
12:19:51 1280  	     in_amount		       => -in_refund_amount,
12:19:51 1281  	     in_created_by	       => in_created_by,
12:19:51 1282  	     in_order_id	       => NULL,
12:19:51 1283  	     in_is_refund	       => GLOBAL_CONSTANTS_V23.TRUE,
12:19:51 1284  	     in_transaction_type_code  => 'REFUND',
12:19:51 1285  	     out_transaction_id        => var_new_transaction_id
12:19:51 1286  	   );
12:19:51 1287  	   EXCEPTION
12:19:51 1288  	     WHEN OTHERS THEN
12:19:51 1289  	       EXCEPTION_MESSAGE := SQLERRM;
12:19:51 1290  	       RAISE CAN_NOT_CREATE_TRANSACTION;
12:19:51 1291  	 END;
12:19:51 1292  
12:19:51 1293  	 BEGIN
12:19:51 1294  	   PROCS_CHARGE_V23.CREATE_CHARGE(
12:19:51 1295  	     in_invoice_id	   => in_invoice_id,
12:19:51 1296  	     in_transaction_id	   => var_new_transaction_id,
12:19:51 1297  	     in_instrument_type_id => var_instrument_type_id,
12:19:51 1298  	     in_instrument_id	   => var_instrument_id,
12:19:51 1299  	     in_charge_amount	   => -in_refund_amount,
12:19:51 1300  	     in_created_by	   => in_created_by,
12:19:51 1301  	     in_charge_status_id   => GLOBAL_STATUSES_V23.CHARGE_OPENED,
12:19:51 1302  	     out_charge_id	   => var_new_charge_id
12:19:51 1303  	   );
12:19:51 1304  	   EXCEPTION
12:19:51 1305  	     WHEN OTHERS THEN
12:19:51 1306  	       EXCEPTION_MESSAGE := SQLERRM;
12:19:51 1307  	       RAISE CAN_NOT_CREATE_CHARGE;
12:19:51 1308  	 END;
12:19:51 1309  
12:19:51 1310  	 IF in_note IS NOT NULL THEN
12:19:51 1311  	   BEGIN
12:19:51 1312  	     PROCS_SUBSCRIPTION_V23.ANNOTATE_SUBSCRIPTION(
12:19:51 1313  	       in_subscription_id => in_subscription_id,
12:19:51 1314  	       in_agent_id	  => 0, -- AGENT_ID??
12:19:51 1315  	       in_note		  => in_note,
12:19:51 1316  	       in_created_by	  => in_created_by
12:19:51 1317  	     );
12:19:51 1318  	     EXCEPTION
12:19:51 1319  	       WHEN OTHERS THEN
12:19:51 1320  		 EXCEPTION_MESSAGE := SQLERRM;
12:19:51 1321  		 RAISE CAN_NOT_ANNOTATE_SUBSCRIPTION;
12:19:51 1322  	   END;
12:19:51 1323  	 END IF;
12:19:51 1324  
12:19:51 1325  	 out_charge_id := var_new_charge_id;
12:19:51 1326  
12:19:51 1327  EXCEPTION
12:19:51 1328  WHEN INVOICE_IS_NOT_CLOSED THEN
12:19:51 1329  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
12:19:51 1330  	   SPROC_NAME, 'Invoice is not closed');
12:19:51 1331  WHEN BAD_SUBSCRIPTION_ID THEN
12:19:51 1332  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 1333  	   SPROC_NAME, 'No such subscription');
12:19:51 1334  WHEN ACCOUNT_IS_FROZEN THEN
12:19:51 1335  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
12:19:51 1336  	   SPROC_NAME, 'Could not refund subscription for frozen account');
12:19:51 1337  WHEN BAD_INVOICE_ID THEN
12:19:51 1338  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 1339  	   SPROC_NAME, 'No such invoice');
12:19:51 1340  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
12:19:51 1341  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 1342  	   SPROC_NAME, 'Could not calculate invoice amount', EXCEPTION_MESSAGE);
12:19:51 1343  WHEN REFUND_IS_GREATER_THAN_ANOUNT THEN
12:19:51 1344  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INVALID_PARAMETER,
12:19:51 1345  	   SPROC_NAME, 'Refund is greater than amount');
12:19:51 1346  WHEN TOT_REF_IS_GREATER_THAN_ANOUNT THEN
12:19:51 1347  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INVALID_PARAMETER,
12:19:51 1348  	   SPROC_NAME, 'There were refunds before and sum of all refunds and new refund more than invoice amount');
12:19:51 1349  WHEN TOT_REF_IS_GRATER_THAN_CHARGES THEN
12:19:51 1350  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INVALID_PARAMETER,
12:19:51 1351  	   SPROC_NAME, 'Total refund amount is greater than sum of processed charges');
12:19:51 1352  WHEN CAN_NOT_CREATE_TRANSACTION THEN
12:19:51 1353  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 1354  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
12:19:51 1355  WHEN CAN_NOT_CREATE_CHARGE THEN
12:19:51 1356  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 1357  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
12:19:51 1358  WHEN CAN_NOT_ANNOTATE_SUBSCRIPTION THEN
12:19:51 1359  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 1360  	   SPROC_NAME, 'Could not annotate subscription', EXCEPTION_MESSAGE);
12:19:51 1361  WHEN OTHERS THEN
12:19:51 1362  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 1363  	   SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 1364  END REFUND_SUBSCRIPTION;
