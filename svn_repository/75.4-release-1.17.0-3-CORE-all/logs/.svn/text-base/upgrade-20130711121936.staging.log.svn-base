12:19:51 1132  	   SPROC_NAME, 'No such subscription');
12:19:51 1133  WHEN OTHERS THEN
12:19:51 1134  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 1135  	   SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 1136  END GET_BILLING_CYCLE;
