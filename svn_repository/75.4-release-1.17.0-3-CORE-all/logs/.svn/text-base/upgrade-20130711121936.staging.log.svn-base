12:19:51 1016  -- norlov: #38796
12:19:51 1017  PROCEDURE GET_TRANSACTIONS (
12:19:51 1018  /*
12:19:51 1019  Throws exceptions:
12:19:51 1020  APP_EXCEPTION_CODES_V23.NOT_FOUND
12:19:51 1021  APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR
12:19:51 1022  */
12:19:51 1023  	 in_group_id	       IN  NUMBER,
12:19:51 1024  	 in_invoice_id	       IN NUMBER DEFAULT NULL,
12:19:51 1025  	 in_subscription_id    IN NUMBER DEFAULT NULL,
12:19:51 1026  	 in_start_date	       IN TRANSACTION.CREATE_DATE%TYPE DEFAULT NULL,
12:19:51 1027  	 in_end_date	       IN TRANSACTION.CREATE_DATE%TYPE DEFAULT NULL,
12:19:51 1028  	 in_transaction_status IN NUMBER DEFAULT NULL,
12:19:51 1029  	 out_result_set        OUT SYS_REFCURSOR
12:19:51 1030  ) AS
12:19:51 1031  SPROC_NAME CONSTANT VARCHAR2(16) := 'GET_TRANSACTIONS';
12:19:51 1032  -- VARIABLES
12:19:51 1033  var_account_id ACCOUNT.ID%TYPE;
12:19:51 1034  statement VARCHAR2(2000);
12:19:51 1035  -- EXCEPTIONS
12:19:51 1036  BAD_GROUP_ID   EXCEPTION;
12:19:51 1037  BEGIN
12:19:51 1038  	-- check group id
12:19:51 1039  	BEGIN
12:19:51 1040  	   SELECT
12:19:51 1041  	     ACCOUNT.ID into var_account_id
12:19:51 1042  	   FROM
12:19:51 1043  	     ACCOUNT
12:19:51 1044  	   WHERE
12:19:51 1045  	     ACCOUNT.GROUP_ID = in_group_id;
12:19:51 1046  	   EXCEPTION
12:19:51 1047  	     WHEN NO_DATA_FOUND THEN
12:19:51 1048  	       RAISE BAD_GROUP_ID;
12:19:51 1049  	 END;
12:19:51 1050  
12:19:51 1051  	 OPEN out_result_set FOR
12:19:51 1052  	 SELECT
12:19:51 1053  	   TRANSACTION.ID,
12:19:51 1054  	   TRANSACTION.TRANSACTION_STATUS_ID,
12:19:51 1055  	   TRANSACTION.TRANSACTION_AMOUNT,
12:19:51 1056  	   TRANSACTION.CREATE_DATE,
12:19:51 1057  	   TRANSACTION.CREATED_BY,
12:19:51 1058  	   TRANSACTION.IS_REFUND,
12:19:51 1059  	   GET_TRANSACTION_TAX_AMOUNT(TRANSACTION.ID) as TRANSACTION_TAX_AMOUNT,
12:19:51 1060  	   GET_TRANSACTION_INTRL_TAXES(TRANSACTION.ID) as INTERNATIONAL_TOTAL
12:19:51 1061  	 FROM
12:19:51 1062  	   TRANSACTION
12:19:51 1063  	   INNER JOIN CHARGE ON TRANSACTION.ID = CHARGE.TRANSACTION_ID
12:19:51 1064  	   INNER JOIN INVOICE ON INVOICE.ID = CHARGE.INVOICE_ID
12:19:51 1065  	 WHERE
12:19:51 1066  	   -- Filter by invoice ID
12:19:51 1067  	   (
12:19:51 1068  	     INVOICE.ID IN (
12:19:51 1069  	       -- Gift certificate invoices
12:19:51 1070  	       SELECT
12:19:51 1071  		 GIFT_CERTIFICATE.PURCHASE_INVOICE_ID
12:19:51 1072  	       FROM
12:19:51 1073  		 GIFT_CERTIFICATE
12:19:51 1074  	       WHERE
12:19:51 1075  		 GIFT_CERTIFICATE.PURCHASER_GROUP_ID = in_group_id
12:19:51 1076  		 -- If subscription_id is set then return nothing
12:19:51 1077  		 AND EXISTS (SELECT 1 FROM DUAL WHERE in_subscription_id IS NULL)
12:19:51 1078  	     )
12:19:51 1079  	     OR
12:19:51 1080  	     INVOICE.ID IN (
12:19:51 1081  	       SELECT
12:19:51 1082  		 LICENSE.INVOICE_ID
12:19:51 1083  	       FROM
12:19:51 1084  		 LICENSE
12:19:51 1085  		 INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
12:19:51 1086  	       WHERE
12:19:51 1087  		 SUBSCRIPTION.ACCOUNT_ID = var_account_id
12:19:51 1088  		 -- Filter by subscription id
12:19:51 1089  		 AND SUBSCRIPTION.ID = NVL(in_subscription_id, SUBSCRIPTION.ID)
12:19:51 1090  	     )
12:19:51 1091  	   )
12:19:51 1092  	   -- Filter by invoice id
12:19:51 1093  	   AND INVOICE.ID = NVL(in_invoice_id, INVOICE.ID)
12:19:51 1094  	   -- Filter by start date
12:19:51 1095  	   AND TRANSACTION.CREATE_DATE >= NVL(in_start_date, TRANSACTION.CREATE_DATE)
12:19:51 1096  	   -- Filter by end date
12:19:51 1097  	   AND TRANSACTION.CREATE_DATE <= NVL(in_end_date, TRANSACTION.CREATE_DATE)
12:19:51 1098  	   -- Filter by transaction status
12:19:51 1099  	   AND TRANSACTION.TRANSACTION_STATUS_ID IN ( SELECT NVL(in_transaction_status, TRANSACTION.TRANSACTION_STATUS_ID) FROM DUAL);
12:19:51 1100  
12:19:51 1101  /*
12:19:51 1102  
12:19:51 1103  statement :=  'select distinct '||CHR(10)
12:19:51 1104  || ' TRANSACTION.ID,'||CHR(10)
12:19:51 1105  || ' TRANSACTION.TRANSACTION_STATUS_ID, '||CHR(10)
12:19:51 1106  || ' TRANSACTION.TRANSACTION_AMOUNT, '||CHR(10)
12:19:51 1107  || ' TRANSACTION.CREATE_DATE, '||CHR(10)
12:19:51 1108  || ' TRANSACTION.CREATED_BY, '||CHR(10)
12:19:51 1109  || ' TRANSACTION.IS_REFUND from TRANSACTION '||CHR(10)
12:19:51 1110  || ' inner join CHARGE on (CHARGE.TRANSACTION_ID = TRANSACTION.ID)'||CHR(10)
12:19:51 1111  || ' inner join INVOICE on (INVOICE.ID = CHARGE.INVOICE_ID)'||CHR(10)
12:19:51 1112  || ' inner join LICENSE on (LICENSE.INVOICE_ID = INVOICE.ID)'||CHR(10)
12:19:51 1113  || ' inner join SUBSCRIPTION on (SUBSCRIPTION.ID = LICENSE.SUBSCRIPTION_ID)'||CHR(10)
12:19:51 1114  || ' inner join ACCOUNT on (ACCOUNT.ID = SUBSCRIPTION.ACCOUNT_ID)'||CHR(10)
12:19:51 1115  || ' where ACCOUNT.GROUP_ID = '||in_group_id;
12:19:51 1116  
12:19:51 1117  IF (in_transaction_status IS NOT NULL) THEN
12:19:51 1118  	 statement := statement || CHR(10) || 'AND TRANSACTION.TRANSACTION_STATUS_ID=' || in_transaction_status;
12:19:51 1119  END IF;
12:19:51 1120  
12:19:51 1121  IF (in_invoice_id IS NOT NULL) THEN
12:19:51 1122  	 statement := statement || CHR(10) || 'AND INVOICE.ID=' || in_invoice_id;
12:19:51 1123  END IF;
12:19:51 1124  
12:19:51 1125  IF (in_subscription_id IS NOT NULL) THEN
12:19:51 1126  	 statement := statement || CHR(10) || 'AND SUBSCRIPTION.ID=' || in_subscription_id;
