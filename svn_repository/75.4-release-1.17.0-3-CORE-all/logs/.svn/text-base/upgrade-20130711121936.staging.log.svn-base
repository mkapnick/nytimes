12:19:51 1727  PROCEDURE CHECK_PRODUCT_ELIGIBILITY (
12:19:51 1728  	 in_group_id	   IN NUMBER,
12:19:51 1729  	 in_offer_chain_id IN NUMBER,
12:19:51 1730  	 out_is_eligible   OUT NUMBER,
12:19:51 1731  	 out_concurrent_subscription_id OUT NUMBER
12:19:51 1732  ) AS
12:19:51 1733  SPROC_NAME CONSTANT VARCHAR2(25) := 'CHECK_PRODUCT_ELIGIBILITY';
12:19:51 1734  -- Variables
12:19:51 1735  var_account_id  NUMBER;
12:19:51 1736  var_is_eligible NUMBER;
12:19:51 1737  var_is_gc       NUMBER;
12:19:51 1738  -- Exceptions
12:19:51 1739  BAD_GROUP_ID		   EXCEPTION;
12:19:51 1740  CAN_NOT_CHECK_SAME_PRODUCTS EXCEPTION;
12:19:51 1741  BAD_OC_ID		   EXCEPTION;
12:19:51 1742  EXCEPTION_MESSAGE	   VARCHAR(1024);
12:19:51 1743  BEGIN
12:19:51 1744  
12:19:51 1745  	 var_is_eligible := GLOBAL_CONSTANTS_V23.TRUE;
12:19:51 1746  
12:19:51 1747  	 out_concurrent_subscription_id := NULL;
12:19:51 1748  
12:19:51 1749  	 BEGIN
12:19:51 1750  	   SELECT
12:19:51 1751  	     OC.IS_GIFT_CERTIFICATE into var_is_gc
12:19:51 1752  	   FROM
12:19:51 1753  	     OFFER_CHAIN OC
12:19:51 1754  	   WHERE
12:19:51 1755  	     OC.ID = in_offer_chain_id;
12:19:51 1756  	   EXCEPTION
12:19:51 1757  	     WHEN NO_DATA_FOUND THEN
12:19:51 1758  	       RAISE BAD_OC_ID;
12:19:51 1759  	 END;
12:19:51 1760  
12:19:51 1761  	 -- only check eligibility if this is not a gift certificate
12:19:51 1762  	 IF (var_is_gc IS NULL OR var_is_gc != 1) THEN
12:19:51 1763  	   BEGIN
12:19:51 1764  	     SELECT
12:19:51 1765  	       A.ID into var_account_id
12:19:51 1766  	     FROM
12:19:51 1767  	       ACCOUNT A
12:19:51 1768  	     WHERE
12:19:51 1769  	       A.GROUP_ID = in_group_id;
12:19:51 1770  	     EXCEPTION
12:19:51 1771  	       WHEN NO_DATA_FOUND THEN
12:19:51 1772  		 RAISE BAD_GROUP_ID;
12:19:51 1773  	   END;
12:19:51 1774  
12:19:51 1775  
12:19:51 1776  	   FOR f_offer_chain IN (
12:19:51 1777  	       SELECT
12:19:51 1778  		 S.ID as SUBSCRIPTION_ID,
12:19:51 1779  		 S.OFFER_CHAIN_ID
12:19:51 1780  	       FROM
12:19:51 1781  		 SUBSCRIPTION S
12:19:51 1782  	       WHERE
12:19:51 1783  		 S.ACCOUNT_ID = var_account_id
12:19:51 1784  		 AND (S.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V23.SUBSCRIPTION_ACTIVE
12:19:51 1785  		      OR S.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V23.SUBSCRIPTION_IN_GRACE_PERIOD)
12:19:51 1786  	   )
12:19:51 1787  	   LOOP
12:19:51 1788  	     BEGIN
12:19:51 1789  	       IF (
12:19:51 1790  		 PROCS_OFFER_CHAIN_V23.CHECK_FOR_SAME_PRODUCTS(
12:19:51 1791  		   in_offer_chain_id,
12:19:51 1792  		   f_offer_chain.offer_chain_id,
12:19:51 1793  		   GLOBAL_CONSTANTS_V23.TRUE
12:19:51 1794  		 ) = GLOBAL_CONSTANTS_V23.TRUE
12:19:51 1795  	       ) THEN
12:19:51 1796  		 var_is_eligible := GLOBAL_CONSTANTS_V23.FALSE;
12:19:51 1797  		 out_concurrent_subscription_id := f_offer_chain.SUBSCRIPTION_ID;
12:19:51 1798  	       END IF;
12:19:51 1799  	       EXCEPTION
12:19:51 1800  		 WHEN OTHERS THEN
12:19:51 1801  		   EXCEPTION_MESSAGE := SQLERRM;
12:19:51 1802  		   RAISE CAN_NOT_CHECK_SAME_PRODUCTS;
12:19:51 1803  	     END;
12:19:51 1804  	   END LOOP;
12:19:51 1805  	 END IF;
12:19:51 1806  	 out_is_eligible := var_is_eligible;
12:19:51 1807  
12:19:51 1808  EXCEPTION
12:19:51 1809  WHEN BAD_GROUP_ID THEN
12:19:51 1810  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 1811  	   SPROC_NAME, 'No such offer chain', SQLERRM);
12:19:51 1812  WHEN BAD_OC_ID THEN
12:19:51 1813  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 1814  	   SPROC_NAME, 'No such offer chain', SQLERRM);
12:19:51 1815  WHEN CAN_NOT_CHECK_SAME_PRODUCTS THEN
12:19:51 1816  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 1817  	   SPROC_NAME, 'Could not check offers for same products', EXCEPTION_MESSAGE);
12:19:51 1818  WHEN OTHERS THEN
12:19:51 1819  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 1820  	   SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 1821  END CHECK_PRODUCT_ELIGIBILITY;
