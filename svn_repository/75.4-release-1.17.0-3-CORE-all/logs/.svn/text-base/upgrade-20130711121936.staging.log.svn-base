12:19:51 261  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 262  	  SPROC_NAME, 'Unknown error', SQLERRM);
