12:19:51 2010  	 where
12:19:51 2011  	   oc.id = s.offer_chain_id and
12:19:51 2012  	   s.id = l.subscription_id and
12:19:51 2013  	   l.invoice_id = i.id and
12:19:51 2014  	   oc.revoke_entitlements = GLOBAL_CONSTANTS_V23.TRUE and
12:19:51 2015  	   i.id = in_invoice_id and
12:19:51 2016  	   rownum < 2
12:19:51 2017  	 ;
12:19:51 2018  END IS_REVOKE_ENTITLEMENTS;
