12:19:51 1998  PROCEDURE IS_REVOKE_ENTITLEMENTS(
12:19:51 1999  	 in_invoice_id IN NUMBER,
12:19:51 2000  	 out_is_revoke OUT NUMBER
12:19:51 2001  ) AS
12:19:51 2002  BEGIN
