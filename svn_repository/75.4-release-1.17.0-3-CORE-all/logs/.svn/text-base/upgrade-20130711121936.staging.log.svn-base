12:19:51 1653  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 1654  	   SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 1655  END SET_IS_TAX_CALCULATION_NEEDED;
