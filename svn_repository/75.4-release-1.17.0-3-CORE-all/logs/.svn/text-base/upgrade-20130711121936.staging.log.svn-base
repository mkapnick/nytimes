12:19:51 2889  /*
12:19:51 2890  	   Note, this returns paypal records which are in the given window of time
12:19:51 2891  	   based on their creation date, not their expiration date, because we've seen
12:19:51 2892  	   suspicious values in the data for expiration dates. The java layer should
12:19:51 2893  	   inspect the expiration dates and decide whether or not to act on the data.
12:19:51 2894  */
12:19:51 2895  
12:19:51 2896  PROCEDURE GET_EXPIRING_PAYPAL (
12:19:51 2897  	 in_expire_window_days	 IN NUMBER,
12:19:51 2898  	 in_creation_limit_days  IN NUMBER,
12:19:51 2899  	 in_retry_throttle_name  IN PROCESS_RETRY_THROTTLE.PROCESS_NAME%TYPE,
12:19:51 2900  	 out_result_set 	 OUT SYS_REFCURSOR
12:19:51 2901  ) AS
12:19:51 2902  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_EXPIRING_PAYPAL';
12:19:51 2903  var_rows_to_return CONSTANT NUMBER := 1000;
12:19:51 2904  BEGIN
12:19:51 2905  	 OPEN out_result_set FOR
12:19:51 2906  	 SELECT * FROM (
12:19:51 2907  	       SELECT
12:19:51 2908  	       ac.group_id	   AS group_id,
12:19:51 2909  	       pp.id		   AS paypal_id,
12:19:51 2910  	       sb.id		   AS subscription_id,
12:19:51 2911  	       pp.create_date	   AS paypal_creation_date,
12:19:51 2912  	       pp.expiration_date  AS paypal_expiration_date,
12:19:51 2913  	       lc.end_date	   AS license_end_date
12:19:51 2914  	   FROM license lc
12:19:51 2915  	   INNER JOIN subscription sb
12:19:51 2916  	   ON lc.subscription_id = sb.id
12:19:51 2917  	   INNER JOIN paypal pp
12:19:51 2918  	   ON pp.account_id = sb.account_id
12:19:51 2919  	   INNER JOIN account ac
12:19:51 2920  	   ON pp.account_id = ac.id
12:19:51 2921  	   WHERE SYSDATE <= lc.end_date
12:19:51 2922  	     AND SYSDATE + in_expire_window_days > lc.end_date
12:19:51 2923  	     AND pp.paypal_status_id = GLOBAL_STATUSES_V23.PAYPAL_ACTIVE
12:19:51 2924  	     AND lc.license_status_id IN (GLOBAL_STATUSES_V23.LICENSE_ACTIVE,
12:19:51 2925  					  GLOBAL_STATUSES_V23.LICENSE_IN_GRACE_PERIOD)
12:19:51 2926  	     AND pp.id = sb.instrument_id
12:19:51 2927  	     AND sb.instrument_type_id = GLOBAL_ENUMS_V23.INSTRUMENT_PAYPAL
12:19:51 2928  	     AND sb.subscription_status_id IN (GLOBAL_STATUSES_V23.SUBSCRIPTION_ACTIVE,
12:19:51 2929  					       GLOBAL_STATUSES_V23.SUBSCRIPTION_IN_GRACE_PERIOD)
12:19:51 2930  	     AND pp.create_date + in_creation_limit_days <= lc.end_date
12:19:51 2931  	     AND NOT EXISTS(
12:19:51 2932  	       SELECT NULL
12:19:51 2933  	       FROM process_retry_throttle rt
12:19:51 2934  	       WHERE rt.process_name = in_retry_throttle_name
12:19:51 2935  		 AND rt.generic_id = pp.id
12:19:51 2936  	     )
12:19:51 2937  	     AND ROWNUM <= 10 * var_rows_to_return
12:19:51 2938  	   ORDER BY dbms_random.value
12:19:51 2939  	 )
12:19:51 2940  	 WHERE ROWNUM <= var_rows_to_return;
12:19:51 2941  END GET_EXPIRING_PAYPAL;
