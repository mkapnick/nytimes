12:19:51 441  FUNCTION CALCULATE_OFFER_CHAIN_END_DATE (
12:19:51 442  	in_offer_chain_id	  IN NUMBER,
12:19:51 443  	in_offer_chain_start_date IN DATE
12:19:51 444  ) RETURN DATE AS
12:19:51 445  -- VARIABLES
12:19:51 446  SPROC_NAME	     CONSTANT VARCHAR2(30) := 'CALCULATE_OFFER_CHAIN_END_DATE';
12:19:51 447  temp_offer_chain_id    NUMBER;
12:19:51 448  var_offer_chain_length NUMBER;
12:19:51 449  var_offer_duration     VARCHAR2(30);
12:19:51 450  var_offer_recurrences  NUMBER;
12:19:51 451  var_end_date	     DATE;
12:19:51 452  
12:19:51 453  var_offer_ym_interval INTERVAL YEAR TO MONTH;
12:19:51 454  var_offer_ds_interval INTERVAL DAY(3) TO SECOND;
12:19:51 455  var_offer_years	    NUMBER;
12:19:51 456  var_offer_months	    NUMBER;
12:19:51 457  var_offer_days	    NUMBER;
12:19:51 458  var_infinity_offers_count NUMBER;
12:19:51 459  
12:19:51 460  var_offers_set SYS_REFCURSOR;
12:19:51 461  
12:19:51 462  -- EXCEPTIONS
12:19:51 463  BAD_OFFER_CHAIN_ID EXCEPTION;
12:19:51 464  BEGIN
12:19:51 465  
12:19:51 466  	var_end_date := in_offer_chain_start_date;
12:19:51 467  
12:19:51 468  	-- Check that offer chain exists
12:19:51 469  	BEGIN
12:19:51 470  	  SELECT
12:19:51 471  	    OFFER_CHAIN.ID into temp_offer_chain_id
12:19:51 472  	  FROM
12:19:51 473  	    OFFER_CHAIN
12:19:51 474  	  WHERE
12:19:51 475  	    OFFER_CHAIN.ID = in_offer_chain_id;
12:19:51 476  	  EXCEPTION
12:19:51 477  	    WHEN NO_DATA_FOUND
12:19:51 478  	      THEN RAISE BAD_OFFER_CHAIN_ID;
12:19:51 479  	END;
12:19:51 480  
12:19:51 481  	SELECT
12:19:51 482  	  COUNT(*) into var_infinity_offers_count
12:19:51 483  	FROM
12:19:51 484  	  OFFER_OFFER_CHAIN
12:19:51 485  	WHERE
12:19:51 486  	  OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
12:19:51 487  	  AND OFFER_OFFER_CHAIN.NUM_RECURRENCES = GLOBAL_ENUMS_V23.OFFER_REC_INFINITY;
12:19:51 488  
12:19:51 489  	IF var_infinity_offers_count > 0 THEN
12:19:51 490  	  -- Offer chain contains offers with infinity num of recurrences
12:19:51 491  	  RETURN NULL;
12:19:51 492  	END IF;
12:19:51 493  
12:19:51 494  	-- Select offers durations
12:19:51 495  	OPEN var_offers_set FOR
12:19:51 496  	SELECT
12:19:51 497  	  OFFER.ENTITLEMENT_DURATION,
12:19:51 498  	  OFFER_OFFER_CHAIN.NUM_RECURRENCES
12:19:51 499  	FROM
12:19:51 500  	  OFFER_OFFER_CHAIN
12:19:51 501  	  INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
12:19:51 502  	WHERE
12:19:51 503  	  OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id;
12:19:51 504  
12:19:51 505  	-- Calculate sum of offers durations
12:19:51 506  	LOOP
12:19:51 507  	  FETCH var_offers_set INTO var_offer_duration, var_offer_recurrences;
12:19:51 508  	  EXIT WHEN var_offers_set%NOTFOUND;
12:19:51 509  	  PROCS_COMMON_V23.ISO8601DURATION_TO_INTERVALS(var_offer_duration, var_offer_years, var_offer_months, var_offer_days);
12:19:51 510  	  var_offer_ym_interval := var_offer_years||'-'||var_offer_months;
12:19:51 511  	  var_offer_ds_interval := var_offer_days||' 0:0:0';
12:19:51 512  	  var_end_date := var_end_date + ( var_offer_ym_interval * ( var_offer_recurrences + 1) ) + ( var_offer_ds_interval * ( var_offer_recurrences + 1) );
12:19:51 513  	END LOOP;
12:19:51 514  
12:19:51 515  	RETURN var_end_date;
12:19:51 516  
12:19:51 517  EXCEPTION
12:19:51 518  WHEN BAD_OFFER_CHAIN_ID THEN
12:19:51 519  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 520  	  SPROC_NAME, 'No such offer chain');
12:19:51 521  WHEN OTHERS THEN
12:19:51 522  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 523  	  SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 524  END CALCULATE_OFFER_CHAIN_END_DATE;
12:19:51 525  
12:19:51 526  FUNCTION CALCULATE_OFFER_AMOUNT (
12:19:51 527  	in_offer_id IN NUMBER
12:19:51 528  ) RETURN NUMBER AS
12:19:51 529  -- VARIABLES
12:19:51 530  SPROC_NAME    CONSTANT VARCHAR2(22) := 'CALCULATE_OFFER_AMOUNT';
12:19:51 531  temp_offer_id NUMBER;
12:19:51 532  
12:19:51 533  var_product_offering_set	    SYS_REFCURSOR;
12:19:51 534  var_product_offering_id	    NUMBER;
12:19:51 535  var_product_offering_price    NUMBER(10,6);
12:19:51 536  var_product_offering_t_amount NUMBER(10,6);
12:19:51 537  var_product_offering_quantity NUMBER;
12:19:51 538  
12:19:51 539  var_total_amount NUMBER(10,6);
12:19:51 540  var_final_amount NUMBER(10,2);
12:19:51 541  
12:19:51 542  var_percent_discount NUMBER(10,2);
12:19:51 543  var_fixed_discount NUMBER(10,6);
12:19:51 544  
12:19:51 545  -- EXCEPTIONS
12:19:51 546  BAD_OFFER_ID EXCEPTION;
12:19:51 547  BEGIN
12:19:51 548  
12:19:51 549  	BEGIN
12:19:51 550  	  SELECT
12:19:51 551  	    OFFER.ID into temp_offer_id
12:19:51 552  	  FROM
12:19:51 553  	    OFFER
12:19:51 554  	  WHERE
12:19:51 555  	    OFFER.ID = in_offer_id;
12:19:51 556  	  EXCEPTION
12:19:51 557  	    WHEN NO_DATA_FOUND THEN
12:19:51 558  	      RAISE BAD_OFFER_ID;
12:19:51 559  	END;
12:19:51 560  
12:19:51 561  	OPEN var_product_offering_set FOR
12:19:51 562  	SELECT
12:19:51 563  	  OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID,
12:19:51 564  	  PRODUCT_OFFERING.UNIT_PRICE,
12:19:51 565  	  PRODUCT_OFFERING.QUANTITY
12:19:51 566  	FROM
12:19:51 567  	  OFFER_PRODUCT_OFFERING
12:19:51 568  	  INNER JOIN PRODUCT_OFFERING ON OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = PRODUCT_OFFERING.ID
12:19:51 569  	WHERE
12:19:51 570  	  OFFER_PRODUCT_OFFERING.OFFER_ID = in_offer_id;
12:19:51 571  
12:19:51 572  	var_total_amount := 0;
12:19:51 573  
12:19:51 574  	LOOP
12:19:51 575  	  FETCH var_product_offering_set into
12:19:51 576  	    var_product_offering_id,
12:19:51 577  	    var_product_offering_price,
12:19:51 578  	    var_product_offering_quantity;
12:19:51 579  	  EXIT WHEN var_product_offering_set%NOTFOUND;
12:19:51 580  
12:19:51 581  	  SELECT
12:19:51 582  	    SUM(DISCOUNT.FIXED_AMOUNT) into var_fixed_discount
12:19:51 583  	  FROM
12:19:51 584  	    DISCOUNT_PRODUCT_OFFERING
12:19:51 585  	    INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
12:19:51 586  	  WHERE
12:19:51 587  	    DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
12:19:51 588  	    AND DISCOUNT.FIXED_AMOUNT IS NOT NULL;
12:19:51 589  
12:19:51 590  	  SELECT
12:19:51 591  	    SUM(DISCOUNT.PERCENT_AMOUNT) into var_percent_discount
12:19:51 592  	  FROM
12:19:51 593  	    DISCOUNT_PRODUCT_OFFERING
12:19:51 594  	    INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
12:19:51 595  	  WHERE
12:19:51 596  	    DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
12:19:51 597  	    AND DISCOUNT.PERCENT_AMOUNT IS NOT NULL;
12:19:51 598  
12:19:51 599  	  var_product_offering_t_amount := var_product_offering_price * var_product_offering_quantity;
12:19:51 600  
12:19:51 601  	  IF var_percent_discount IS NOT NULL THEN
12:19:51 602  	    var_product_offering_t_amount := var_product_offering_t_amount - ( var_product_offering_t_amount * var_percent_discount / 100 );
12:19:51 603  	  END IF;
12:19:51 604  
12:19:51 605  	  IF var_fixed_discount IS NOT NULL THEN
12:19:51 606  	    var_product_offering_t_amount := var_product_offering_t_amount - var_fixed_discount;
12:19:51 607  	  END IF;
12:19:51 608  
12:19:51 609  	  var_total_amount := var_total_amount + var_product_offering_t_amount;
12:19:51 610  	END LOOP;
12:19:51 611  	var_final_amount := var_total_amount;
12:19:51 612  	RETURN var_final_amount;
12:19:51 613  
12:19:51 614  EXCEPTION
12:19:51 615  WHEN BAD_OFFER_ID THEN
12:19:51 616  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 617  	  SPROC_NAME, 'No such offer');
12:19:51 618  WHEN OTHERS THEN
12:19:51 619  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 620  	  SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 621  END CALCULATE_OFFER_AMOUNT;
