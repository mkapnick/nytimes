12:19:51 886  
12:19:51 887  PROCEDURE APPLY_REFUND (
12:19:51 888  	in_invoice_id	     IN NUMBER,
12:19:51 889  	in_chargeback_amount IN NUMBER,
12:19:51 890  	in_created_by	     IN VARCHAR2,
12:19:51 891  	out_charge_id	     OUT NUMBER
12:19:51 892  ) AS
12:19:51 893  -- VARIABLES
12:19:51 894  SPROC_NAME		 CONSTANT VARCHAR2(16) := 'APPLY_CHARGEBACK';
12:19:51 895  temp_invoice_id		 NUMBER;
12:19:51 896  var_total_charges_amount	 NUMBER(10,2);
12:19:51 897  var_charge_amount_to_apply NUMBER(10,2);
12:19:51 898  var_transaction_id	 NUMBER;
12:19:51 899  var_instrument_type_id	 NUMBER;
12:19:51 900  var_instrument_id 	 NUMBER;
12:19:51 901  var_charge_id		 NUMBER;
12:19:51 902  -- EXCEPTIONS
12:19:51 903  BAD_INVOICE_ID		 EXCEPTION;
12:19:51 904  CAN_NOT_CREATE_TRANSACTION EXCEPTION;
12:19:51 905  CAN_NOT_FIND_INSTRUMENT	 EXCEPTION;
12:19:51 906  CAN_NOT_CREATE_CHARGE	 EXCEPTION;
12:19:51 907  EXCEPTION_MESSAGE 	 VARCHAR2(1024);
12:19:51 908  BEGIN
12:19:51 909  
12:19:51 910  	BEGIN
12:19:51 911  	  SELECT
12:19:51 912  	    INVOICE.ID into temp_invoice_id
12:19:51 913  	  FROM
12:19:51 914  	    INVOICE
12:19:51 915  	  WHERE
12:19:51 916  	    INVOICE.ID = in_invoice_id;
12:19:51 917  	  EXCEPTION
12:19:51 918  	    WHEN NO_DATA_FOUND THEN
12:19:51 919  	      RAISE BAD_INVOICE_ID;
12:19:51 920  	END;
12:19:51 921  
12:19:51 922  	SELECT
12:19:51 923  	  SUM (CHARGE.CHARGE_AMOUNT) into var_total_charges_amount
12:19:51 924  	FROM
12:19:51 925  	  CHARGE
12:19:51 926  	WHERE
12:19:51 927  	  CHARGE.INVOICE_ID = in_invoice_id
12:19:51 928  	  AND CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V23.CHARGE_PROCESSED
12:19:51 929  	  AND CHARGE.INSTRUMENT_TYPE_ID != GLOBAL_ENUMS_V23.INSTRUMENT_GIFT_CERTIFICATE;
12:19:51 930  
12:19:51 931  	-- FIXME: Maybe whe should throw exception here?
12:19:51 932  	IF var_total_charges_amount < in_chargeback_amount THEN
12:19:51 933  	  var_charge_amount_to_apply := var_total_charges_amount;
12:19:51 934  	ELSE
12:19:51 935  	  var_charge_amount_to_apply := in_chargeback_amount;
12:19:51 936  	END IF;
12:19:51 937  
12:19:51 938  	BEGIN
12:19:51 939  	  PROCS_TRANSACTION_V23.CREATE_TRANSACTION(
12:19:51 940  	    in_transaction_id	      => NULL,
12:19:51 941  	    in_status_id	      => GLOBAL_STATUSES_V23.TRANSACTION_PREPARE,
12:19:51 942  	    in_amount		      => -var_charge_amount_to_apply,
12:19:51 943  	    in_created_by	      => in_created_by,
12:19:51 944  	    in_order_id 	      => NULL,
12:19:51 945  	    in_is_refund	      => GLOBAL_CONSTANTS_V23.TRUE,
12:19:51 946  	    in_transaction_type_code  => 'REFUND',
12:19:51 947  	    out_transaction_id	      => var_transaction_id
12:19:51 948  	  );
12:19:51 949  	  EXCEPTION
12:19:51 950  	    WHEN OTHERS THEN
12:19:51 951  	      EXCEPTION_MESSAGE := SQLERRM;
12:19:51 952  	      RAISE CAN_NOT_CREATE_TRANSACTION;
12:19:51 953  	END;
12:19:51 954  
12:19:51 955  	BEGIN
12:19:51 956  	  SELECT
12:19:51 957  	    C.INSTRUMENT_TYPE_ID,
12:19:51 958  	    C.INSTRUMENT_ID
12:19:51 959  	    into
12:19:51 960  	    var_instrument_type_id,
12:19:51 961  	    var_instrument_id
12:19:51 962  	  FROM
12:19:51 963  	    CHARGE C,
12:19:51 964  	    TRANSACTION_ATTEMPT TA,
12:19:51 965  	    TRANSACTION T
12:19:51 966  	  WHERE
12:19:51 967  	    C.INVOICE_ID = in_invoice_id and
12:19:51 968  	    C.TRANSACTION_ID = T.ID and
12:19:51 969  	    TA.TRANSACTION_ID = T.ID and
12:19:51 970  	    TA.TRANSACTION_ATTEMPT_STATUS_ID = GLOBAL_STATUSES_V23.TRANS_ATTEMPT_SUCCESS and
12:19:51 971  	    T.IS_REFUND = GLOBAL_CONSTANTS_V23.FALSE and
12:19:51 972  	    T.TRANSACTION_AMOUNT >= 0 and
12:19:51 973  	    rownum < 2;
12:19:51 974  	EXCEPTION
12:19:51 975  	    WHEN NO_DATA_FOUND THEN
12:19:51 976  	      RAISE CAN_NOT_FIND_INSTRUMENT;
12:19:51 977  	END;
12:19:51 978  	BEGIN
12:19:51 979  	  PROCS_CHARGE_V23.CREATE_CHARGE(
12:19:51 980  	    in_invoice_id	  => in_invoice_id,
12:19:51 981  	    in_transaction_id	  => var_transaction_id,
12:19:51 982  	    in_instrument_type_id => var_instrument_type_id,
12:19:51 983  	    in_instrument_id	  => var_instrument_id,
12:19:51 984  	    in_charge_amount	  => -var_charge_amount_to_apply,
12:19:51 985  	    in_created_by	  => in_created_by,
12:19:51 986  	    in_charge_status_id   => GLOBAL_STATUSES_V23.CHARGE_OPENED,
12:19:51 987  	    out_charge_id	  => var_charge_id
12:19:51 988  	  );
12:19:51 989  	  EXCEPTION
12:19:51 990  	    WHEN OTHERS THEN
12:19:51 991  	      EXCEPTION_MESSAGE := SQLERRM;
12:19:51 992  	      RAISE CAN_NOT_CREATE_CHARGE;
12:19:51 993  	END;
12:19:51 994  
12:19:51 995  	out_charge_id := var_charge_id;
12:19:51 996  
12:19:51 997  EXCEPTION
12:19:51 998  WHEN BAD_INVOICE_ID THEN
12:19:51 999  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 1000  	   SPROC_NAME, 'No such invoice');
12:19:51 1001  WHEN CAN_NOT_CREATE_TRANSACTION THEN
12:19:51 1002  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 1003  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
12:19:51 1004  WHEN CAN_NOT_FIND_INSTRUMENT THEN
12:19:51 1005  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 1006  	   SPROC_NAME, 'Could not find financial instrument');
12:19:51 1007  WHEN CAN_NOT_CREATE_CHARGE THEN
12:19:51 1008  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 1009  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
12:19:51 1010  WHEN OTHERS THEN
12:19:51 1011  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 1012  	   SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 1013  END APPLY_REFUND;
