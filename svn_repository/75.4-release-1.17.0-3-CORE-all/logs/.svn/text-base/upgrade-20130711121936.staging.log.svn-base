12:19:51   5  	PROCEDURE POPULATE_REQUEST_INFO(
12:19:51   6  	  in_hours_prior    IN	NUMBER,
12:19:51   7  	  in_filename	    IN	CC_REQUEST_FILE.FILE_NAME%TYPE,
12:19:51   8  	  in_creator	    IN	CC_REQUEST_FILE.UPDATED_BY%TYPE
12:19:51   9  	) AS
12:19:51  10  	SPROC_NAME CONSTANT VARCHAR2(32) := 'POPULATE_REQUEST_INFO';
12:19:51  11  	var_start_date	    DATE := SYSDATE;
12:19:51  12  	var_end_date	    DATE := var_start_date + (in_hours_prior/24);
12:19:51  13  	var_request_file_id NUMBER := 0;
12:19:51  14  	var_license_count   NUMBER := 0;
12:19:51  15  	var_cc_update_count NUMBER := 0;
12:19:51  16  	BEGIN
12:19:51  17  	    SELECT CC_REQUEST_FILE_ID_SEQ.NEXTVAL INTO var_request_file_id  FROM DUAL;
12:19:51  18  	    INSERT INTO CC_REQUEST_FILE (ID,
12:19:51  19  					 FILE_NAME,
12:19:51  20  					 CC_REQUEST_FILE_STATUS,
12:19:51  21  					 CREATE_DATE,
12:19:51  22  					 CREATED_BY,
12:19:51  23  					 UPDATE_DATE,
12:19:51  24  					 UPDATED_BY)
12:19:51  25  					 VALUES (
12:19:51  26  					 var_request_file_id,
12:19:51  27  					 in_filename,
12:19:51  28  					 'NOT_CREATED',
12:19:51  29  					 var_start_date,
12:19:51  30  					 in_creator,
12:19:51  31  					 var_start_date,
12:19:51  32  					 in_creator);
12:19:51  33  
12:19:51  34  	   FOR record IN (SELECT
12:19:51  35  			    l.ID LICENSE_ID, cc.ID CREDIT_CARD_ID
12:19:51  36  			  FROM
12:19:51  37  			    LICENSE l INNER JOIN SUBSCRIPTION s ON L.SUBSCRIPTION_ID = s.ID
12:19:51  38  				      INNER JOIN CREDIT_CARD cc ON S.INSTRUMENT_ID   = cc.ID
12:19:51  39  			  WHERE
12:19:51  40  			    s.INSTRUMENT_TYPE_ID	 = 1
12:19:51  41  			    AND cc.CREDIT_CARD_STATUS_ID = 1
12:19:51  42  			    AND s.SUBSCRIPTION_STATUS_ID = 1
12:19:51  43  			    AND l.LICENSE_STATUS_ID	 = 2
12:19:51  44  			    AND cc.CREDIT_CARD_TYPE_ID IN (2,3)
12:19:51  45  			    AND l.END_DATE BETWEEN var_start_date AND var_end_date
12:19:51  46  			    AND l.ID NOT IN (SELECT LICENSE_ID FROM CC_UPDATE))
12:19:51  47  	   LOOP
12:19:51  48  	     var_license_count := 0;
12:19:51  49  	     SELECT COUNT(1) INTO  var_license_count FROM CC_UPDATE WHERE LICENSE_ID = record.LICENSE_ID;
12:19:51  50  
12:19:51  51  	     IF var_license_count = 0 THEN
12:19:51  52  		INSERT INTO CC_UPDATE (ID,
12:19:51  53  				       CREDIT_CARD_ID,
12:19:51  54  				       LICENSE_ID,
12:19:51  55  				       CC_UPDATE_STATUS,
12:19:51  56  				       CC_REQUEST_FILE_ID,
12:19:51  57  				       CREATE_DATE,
12:19:51  58  				       UPDATE_DATE,
12:19:51  59  				       CREATED_BY,
12:19:51  60  				       UPDATED_BY
12:19:51  61  				       ) VALUES (
12:19:51  62  				       CC_UPDATE_SEQ.NEXTVAL,
12:19:51  63  				       record.CREDIT_CARD_ID,
12:19:51  64  				       record.LICENSE_ID,
12:19:51  65  				       'NOT_ADDED_TO_FILE',
12:19:51  66  				       var_request_file_id,
12:19:51  67  				       var_start_date,
12:19:51  68  				       var_start_date,
12:19:51  69  				       in_creator,
12:19:51  70  				       in_creator
12:19:51  71  				       );
12:19:51  72  	     END IF;
12:19:51  73  	   END LOOP;
12:19:51  74  
12:19:51  75  	   SELECT COUNT(1) INTO var_cc_update_count
12:19:51  76  	   FROM CC_UPDATE
12:19:51  77  	   WHERE CC_REQUEST_FILE_ID = var_request_file_id;
12:19:51  78  	   IF var_cc_update_count <= 0 THEN
12:19:51  79  	     UPDATE CC_REQUEST_FILE
12:19:51  80  	     SET CC_REQUEST_FILE_STATUS = 'EMPTY'
12:19:51  81  	     WHERE ID = var_request_file_id;
12:19:51  82  	   END IF;
12:19:51  83  
12:19:51  84  	END POPULATE_REQUEST_INFO;
