12:19:51 1097  
12:19:51 1098  PROCEDURE GET_INVOICE_DAYS_USED_NUMBER (
12:19:51 1099  	 in_invoice_id	     IN NUMBER,
12:19:51 1100  	 in_chargeback_date  IN DATE DEFAULT SYSDATE,
12:19:51 1101  	 out_days_num	     OUT NUMBER
12:19:51 1102  ) AS
12:19:51 1103  SPROC_NAME CONSTANT VARCHAR2(28) := 'GET_INVOICE_DAYS_USED_NUMBER';
12:19:51 1104  -- VARIABLES
12:19:51 1105  temp_invoice_id	      NUMBER;
12:19:51 1106  var_license_start_date DATE;
12:19:51 1107  var_license_end_date   DATE;
12:19:51 1108  var_chargeback_date    DATE;
12:19:51 1109  var_invoice_days_used  NUMBER;
12:19:51 1110  -- EXCEPTIONS
12:19:51 1111  BAD_INVOICE_ID EXCEPTION;
12:19:51 1112  BEGIN
12:19:51 1113  
12:19:51 1114  	 var_chargeback_date := NVL(in_chargeback_date, SYSDATE);
12:19:51 1115  
12:19:51 1116  	 BEGIN
12:19:51 1117  	   SELECT
12:19:51 1118  	     INVOICE.ID into temp_invoice_id
12:19:51 1119  	   FROM
12:19:51 1120  	     INVOICE
12:19:51 1121  	   WHERE
12:19:51 1122  	     INVOICE.ID = in_invoice_id;
12:19:51 1123  	   EXCEPTION
12:19:51 1124  	     WHEN NO_DATA_FOUND THEN
12:19:51 1125  	       RAISE BAD_INVOICE_ID;
12:19:51 1126  	 END;
12:19:51 1127  
12:19:51 1128  	 var_invoice_days_used := 0;
12:19:51 1129  
12:19:51 1130  	 FOR f_license IN (
12:19:51 1131  	   SELECT
12:19:51 1132  	     LICENSE.START_DATE,
12:19:51 1133  	     LICENSE.END_DATE
12:19:51 1134  	   FROM
12:19:51 1135  	     LICENSE
12:19:51 1136  	   WHERE
12:19:51 1137  	     LICENSE.INVOICE_ID = in_invoice_id
12:19:51 1138  	     AND LICENSE.IS_EXTENSION = GLOBAL_CONSTANTS_V23.FALSE
12:19:51 1139  	 )
12:19:51 1140  	 LOOP
12:19:51 1141  	   var_license_start_date := PROCS_COMMON_V23.NORMALIZE_DATE(f_license.START_DATE);
12:19:51 1142  	   var_license_end_date := PROCS_COMMON_V23.NORMALIZE_DATE(f_license.END_DATE);
12:19:51 1143  
12:19:51 1144  	   IF var_license_start_date <= var_chargeback_date THEN
12:19:51 1145  	     IF var_license_end_date <= var_chargeback_date THEN
12:19:51 1146  	       -- License is passed
12:19:51 1147  	       var_invoice_days_used := var_invoice_days_used + (var_license_end_date - var_license_start_date);
12:19:51 1148  	     ELSE
12:19:51 1149  	       -- This is current license
12:19:51 1150  	       var_invoice_days_used := var_invoice_days_used + (var_chargeback_date - var_license_start_date);
12:19:51 1151  	     END IF;
12:19:51 1152  	   ELSE
12:19:51 1153  	     -- if var_license_start_date > in_chargeback_date then do nothing
12:19:51 1154  	     NULL;
12:19:51 1155  	   END IF;
12:19:51 1156  	 END LOOP;
12:19:51 1157  
12:19:51 1158  	 out_days_num := var_invoice_days_used;
12:19:51 1159  
12:19:51 1160  EXCEPTION
12:19:51 1161  WHEN BAD_INVOICE_ID THEN
12:19:51 1162  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 1163  	   SPROC_NAME, 'No such invoice');
12:19:51 1164  WHEN OTHERS THEN
12:19:51 1165  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 1166  	   SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 1167  END GET_INVOICE_DAYS_USED_NUMBER;
