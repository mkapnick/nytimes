12:19:51 1366  /******************************************************************************/
12:19:51 1367  
12:19:51 1368  PROCEDURE ADD_SUBSCRIPTION_EXTENSION (
12:19:51 1369  	 in_subscription_id	 IN NUMBER,
12:19:51 1370  	 in_effective_start_date IN DATE,
12:19:51 1371  	 in_effective_end_date	 IN DATE,
12:19:51 1372  	 in_note		 IN VARCHAR2,
12:19:51 1373  	 in_updated_by		 IN VARCHAR2
12:19:51 1374  ) AS
12:19:51 1375  -- VARIABLES
12:19:51 1376  SPROC_NAME		    CONSTANT VARCHAR2(26) := 'ADD_SUBSCRIPTION_EXTENSION';
12:19:51 1377  temp_subscription_id	    NUMBER;
12:19:51 1378  var_current_license_id	    NUMBER;
12:19:51 1379  var_current_license_start_date DATE;
12:19:51 1380  var_current_license_end_date DATE;
12:19:51 1381  var_current_offer_id	    NUMBER;
12:19:51 1382  var_current_invoice_id	    NUMBER;
12:19:51 1383  var_current_date 	    DATE;
12:19:51 1384  var_current_offer_index	    NUMBER;
12:19:51 1385  var_current_offer_recurr_num NUMBER;
12:19:51 1386  var_account_tax_exempt_id    VARCHAR2(255);
12:19:51 1387  
12:19:51 1388  var_free_invoice_id NUMBER;
12:19:51 1389  var_free_license_id NUMBER;
12:19:51 1390  var_new_license_id  NUMBER;
12:19:51 1391  var_ext_license_id  NUMBER;
12:19:51 1392  -- EXCEPTIONS
12:19:51 1393  BAD_SUBSCRIPTION_ID	     EXCEPTION;
12:19:51 1394  CAN_NOT_FIND_OFFER_OR_LICENSE EXCEPTION;
12:19:51 1395  CAN_NOT_CHANGE_LICENSE_STATUS EXCEPTION;
12:19:51 1396  CAN_NOT_CREATE_INVOICE	     EXCEPTION;
12:19:51 1397  CAN_NOT_CREATE_NEW_LICENSE    EXCEPTION;
12:19:51 1398  CAN_NOT_CREATE_END_LICENSE    EXCEPTION;
12:19:51 1399  CAN_NOT_ANNOTATE_SUBSCRIPTION EXCEPTION;
12:19:51 1400  EXTENS_START_DATE_IS_TOO_FAR  EXCEPTION;
12:19:51 1401  EXT_START_DATE_LATER_THEN_END EXCEPTION;
12:19:51 1402  EXTENS_START_DATE_IS_TOO_SMALL EXCEPTION;
12:19:51 1403  EXCEPTION_MESSAGE	     VARCHAR2(1024);
12:19:51 1404  BEGIN
12:19:51 1405  
12:19:51 1406  	 var_current_date := PROCS_COMMON_V23.NORMALIZE_DATE(SYSDATE);
12:19:51 1407  
12:19:51 1408  	 -- Check that subscription exists
12:19:51 1409  	 BEGIN
12:19:51 1410  	   SELECT
12:19:51 1411  	     SUBSCRIPTION.ID into temp_subscription_id
12:19:51 1412  	   FROM
12:19:51 1413  	     SUBSCRIPTION
12:19:51 1414  	   WHERE
12:19:51 1415  	     SUBSCRIPTION.ID = in_subscription_id;
12:19:51 1416  	   EXCEPTION
12:19:51 1417  	     WHEN NO_DATA_FOUND THEN
12:19:51 1418  	       RAISE BAD_SUBSCRIPTION_ID;
12:19:51 1419  	 END;
12:19:51 1420  
12:19:51 1421  	 -- Get account tax exempt id
12:19:51 1422  	 SELECT DISTINCT
12:19:51 1423  	   ACCOUNT.TAX_EXEMPT_ID into var_account_tax_exempt_id
12:19:51 1424  	 FROM
12:19:51 1425  	   ACCOUNT
12:19:51 1426  	   INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.ACCOUNT_ID = ACCOUNT.ID
12:19:51 1427  	 WHERE
12:19:51 1428  	   SUBSCRIPTION.ID = in_subscription_id;
12:19:51 1429  
12:19:51 1430  	 -- Select current data
12:19:51 1431  	 BEGIN
12:19:51 1432  	   SELECT
12:19:51 1433  	     LICENSE.ID,
12:19:51 1434  	     LICENSE.START_DATE,
12:19:51 1435  	     LICENSE.END_DATE,
12:19:51 1436  	     LICENSE.CURRENT_OFFER_INDEX,
12:19:51 1437  	     LICENSE.CURRENT_OFFER_RECURR_NUM,
12:19:51 1438  	     OFFER.ID,
12:19:51 1439  	     INVOICE.ID
12:19:51 1440  	   INTO
12:19:51 1441  	     var_current_license_id,
12:19:51 1442  	     var_current_license_start_date,
12:19:51 1443  	     var_current_license_end_date,
12:19:51 1444  	     var_current_offer_index,
12:19:51 1445  	     var_current_offer_recurr_num,
12:19:51 1446  	     var_current_offer_id,
12:19:51 1447  	     var_current_invoice_id
12:19:51 1448  	   FROM
12:19:51 1449  	     LICENSE
12:19:51 1450  	     INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
12:19:51 1451  	     INNER JOIN OFFER ON LICENSE.OFFER_ID = OFFER.ID
12:19:51 1452  	     INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
12:19:51 1453  	   WHERE
12:19:51 1454  	     SUBSCRIPTION.ID = in_subscription_id
12:19:51 1455  	     AND PROCS_COMMON_V23.NORMALIZE_DATE(LICENSE.END_DATE) > var_current_date
12:19:51 1456  	     AND PROCS_COMMON_V23.NORMALIZE_DATE(LICENSE.START_DATE) <= var_current_date
12:19:51 1457  	     AND ROWNUM <= 1
12:19:51 1458  	   ORDER BY
12:19:51 1459  	     LICENSE.ID DESC;
12:19:51 1460  	   EXCEPTION
12:19:51 1461  	     WHEN NO_DATA_FOUND THEN
12:19:51 1462  	       RAISE CAN_NOT_FIND_OFFER_OR_LICENSE;
12:19:51 1463  	 END;
12:19:51 1464  
12:19:51 1465  	 IF var_current_license_end_date < in_effective_start_date THEN
12:19:51 1466  	   RAISE EXTENS_START_DATE_IS_TOO_FAR;
12:19:51 1467  	 END IF;
12:19:51 1468  
12:19:51 1469  	 IF var_current_license_start_date > in_effective_start_date THEN
12:19:51 1470  	   RAISE EXTENS_START_DATE_IS_TOO_SMALL;
12:19:51 1471  	 END IF;
12:19:51 1472  
12:19:51 1473  	 IF in_effective_start_date > in_effective_end_date THEN
12:19:51 1474  	   RAISE EXT_START_DATE_LATER_THEN_END;
12:19:51 1475  	 END IF;
12:19:51 1476  
12:19:51 1477  	 -- Closing curent license
12:19:51 1478  	 BEGIN
12:19:51 1479  	   PROCS_LICENSE_CRU_V23.UPDATE_LICENSE(
12:19:51 1480  	     in_license_id	   => var_current_license_id,
12:19:51 1481  	     in_updated_by	   => in_updated_by,
12:19:51 1482  	     in_license_status_id  => GLOBAL_STATUSES_V23.LICENSE_CLOSED,
12:19:51 1483  	     in_end_date	   => in_effective_start_date,
12:19:51 1484  	     in_needs_entitlements => GLOBAL_CONSTANTS_V23.TRUE
12:19:51 1485  	   );
12:19:51 1486  	   EXCEPTION
12:19:51 1487  	     WHEN OTHERS THEN
12:19:51 1488  	       EXCEPTION_MESSAGE := SQLERRM;
12:19:51 1489  	       RAISE CAN_NOT_CHANGE_LICENSE_STATUS;
12:19:51 1490  	 END;
12:19:51 1491  
12:19:51 1492  	 -- Creating new "free" invoice
12:19:51 1493  	 BEGIN
12:19:51 1494  	   PROCS_INVOICE_V23.CREATE_INVOICE(
12:19:51 1495  	     in_invoice_status => GLOBAL_STATUSES_V23.INVOICE_CLOSED,
12:19:51 1496  	     in_created_by     => in_updated_by,
12:19:51 1497  	     in_tax_exempt_id  => var_account_tax_exempt_id,
12:19:51 1498  	     out_invoice_id    => var_free_invoice_id
12:19:51 1499  	   );
12:19:51 1500  	   EXCEPTION
12:19:51 1501  	     WHEN OTHERS THEN
12:19:51 1502  	       EXCEPTION_MESSAGE := SQLERRM;
12:19:51 1503  	       RAISE CAN_NOT_CREATE_INVOICE;
12:19:51 1504  	 END;
12:19:51 1505  
12:19:51 1506  	 -- Creating new "free" license
12:19:51 1507  	 BEGIN
12:19:51 1508  	   PROCS_LICENSE_V23.CREATE_LICENSE (
12:19:51 1509  	     in_status_id		 => GLOBAL_STATUSES_V23.LICENSE_ACTIVE,
12:19:51 1510  	     in_needs_entitlements	 => GLOBAL_CONSTANTS_V23.TRUE,
12:19:51 1511  	     in_start_date		 => in_effective_start_date,
12:19:51 1512  	     in_end_date		 => in_effective_end_date,
12:19:51 1513  	     in_offer_id		 => var_current_offer_id,
12:19:51 1514  	     in_subscription_id 	 => in_subscription_id,
12:19:51 1515  	     in_invoice_id		 => var_free_invoice_id,
12:19:51 1516  	     in_created_by		 => in_updated_by,
12:19:51 1517  	     in_is_extension		 => GLOBAL_CONSTANTS_V23.TRUE,
12:19:51 1518  	     in_current_offer_index	 => var_current_offer_index,
12:19:51 1519  	     in_current_offer_recurr_num => var_current_offer_recurr_num,
12:19:51 1520  	     out_license_id		 => var_free_license_id
12:19:51 1521  	   );
12:19:51 1522  	   EXCEPTION
12:19:51 1523  	     WHEN OTHERS THEN
12:19:51 1524  	       EXCEPTION_MESSAGE := SQLERRM;
12:19:51 1525  	       RAISE CAN_NOT_CREATE_NEW_LICENSE;
12:19:51 1526  	 END;
12:19:51 1527  
12:19:51 1528  	 -- Creating new license
12:19:51 1529  	 IF PROCS_COMMON_V23.NORMALIZE_DATE(var_current_license_end_date) >
12:19:51 1530  	    PROCS_COMMON_V23.NORMALIZE_DATE(in_effective_start_date) THEN
12:19:51 1531  	   BEGIN
12:19:51 1532  	     PROCS_LICENSE_V23.CREATE_LICENSE (
12:19:51 1533  	       in_status_id		   => GLOBAL_STATUSES_V23.LICENSE_ACTIVE,
12:19:51 1534  	       in_needs_entitlements	   => GLOBAL_CONSTANTS_V23.TRUE,
12:19:51 1535  	       in_start_date		   => in_effective_end_date,
12:19:51 1536  	       in_end_date		   => var_current_license_end_date + (in_effective_end_date - in_effective_start_date),
12:19:51 1537  	       in_offer_id		   => var_current_offer_id,
12:19:51 1538  	       in_subscription_id	   => in_subscription_id,
12:19:51 1539  	       in_invoice_id		   => var_current_invoice_id,
12:19:51 1540  	       in_created_by		   => in_updated_by,
12:19:51 1541  	       in_is_extension		   => GLOBAL_CONSTANTS_V23.FALSE,
12:19:51 1542  	       in_current_offer_index	   => var_current_offer_index,
12:19:51 1543  	       in_current_offer_recurr_num => var_current_offer_recurr_num,
12:19:51 1544  	       out_license_id		   => var_ext_license_id
12:19:51 1545  	     );
12:19:51 1546  	     EXCEPTION
12:19:51 1547  	       WHEN OTHERS THEN
12:19:51 1548  		 EXCEPTION_MESSAGE := SQLERRM;
12:19:51 1549  		 RAISE CAN_NOT_CREATE_END_LICENSE;
12:19:51 1550  	   END;
12:19:51 1551  	 END IF;
12:19:51 1552  
12:19:51 1553  	 -- Create new note for subscription
12:19:51 1554  	 BEGIN
12:19:51 1555  	   PROCS_SUBSCRIPTION_V23.ANNOTATE_SUBSCRIPTION (
12:19:51 1556  	     in_subscription_id => in_subscription_id,
12:19:51 1557  	     in_agent_id	=> 0, -- FIXME: What should to be here (agent id)?
12:19:51 1558  	     in_note		=> in_note,
12:19:51 1559  	     in_created_by	=> in_updated_by
12:19:51 1560  	   );
12:19:51 1561  	   EXCEPTION
12:19:51 1562  	     WHEN OTHERS THEN
12:19:51 1563  	       EXCEPTION_MESSAGE := SQLERRM;
12:19:51 1564  	       RAISE CAN_NOT_ANNOTATE_SUBSCRIPTION;
12:19:51 1565  	 END;
12:19:51 1566  
12:19:51 1567  EXCEPTION
12:19:51 1568  WHEN EXT_START_DATE_LATER_THEN_END THEN
12:19:51 1569  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INVALID_PARAMETER,
12:19:51 1570  	   SPROC_NAME, 'Extension start date is bigger then end date');
12:19:51 1571  WHEN EXTENS_START_DATE_IS_TOO_FAR THEN
12:19:51 1572  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INVALID_PARAMETER,
12:19:51 1573  	   SPROC_NAME, 'Extension start date is too far');
12:19:51 1574  WHEN EXTENS_START_DATE_IS_TOO_SMALL THEN
12:19:51 1575  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INVALID_PARAMETER,
12:19:51 1576  	   SPROC_NAME, 'Extension start date is too small');
12:19:51 1577  WHEN BAD_SUBSCRIPTION_ID THEN
12:19:51 1578  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 1579  	   SPROC_NAME, 'No such subscription');
12:19:51 1580  WHEN CAN_NOT_FIND_OFFER_OR_LICENSE THEN
12:19:51 1581  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 1582  	   SPROC_NAME, 'Could not find license and/or offer for given subscription');
12:19:51 1583  WHEN CAN_NOT_CHANGE_LICENSE_STATUS THEN
12:19:51 1584  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 1585  	   SPROC_NAME, 'Could not change license status', EXCEPTION_MESSAGE);
12:19:51 1586  WHEN CAN_NOT_CREATE_INVOICE THEN
12:19:51 1587  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 1588  	   SPROC_NAME, 'Could not create new invoice', EXCEPTION_MESSAGE);
12:19:51 1589  WHEN CAN_NOT_CREATE_NEW_LICENSE THEN
12:19:51 1590  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 1591  	   SPROC_NAME, 'Could not create new license', EXCEPTION_MESSAGE);
12:19:51 1592  WHEN CAN_NOT_CREATE_END_LICENSE THEN
12:19:51 1593  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 1594  	   SPROC_NAME, 'Could not create last license', EXCEPTION_MESSAGE);
12:19:51 1595  WHEN CAN_NOT_ANNOTATE_SUBSCRIPTION THEN
12:19:51 1596  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 1597  	   SPROC_NAME, 'Could not create new note for subscription', EXCEPTION_MESSAGE);
12:19:51 1598  WHEN OTHERS THEN
12:19:51 1599  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 1600  	   SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 1601  END ADD_SUBSCRIPTION_EXTENSION;
12:19:51 1602  
12:19:51 1603  /******************************************************************************/
12:19:51 1604  
12:19:51 1605  FUNCTION CALC_SUBSCRIPTION_END_DATE (
12:19:51 1606  	 in_subscription_id IN NUMBER
12:19:51 1607  ) RETURN DATE AS
12:19:51 1608  -- VARIABLES
12:19:51 1609  SPROC_NAME		     CONSTANT VARCHAR2(26) := 'CALC_SUBSCRIPTION_END_DATE';
12:19:51 1610  last_license_id		     NUMBER;
12:19:51 1611  last_license_end_date	     DATE;
12:19:51 1612  last_license_offer_id	     NUMBER;
12:19:51 1613  last_license_offer_index      NUMBER;
12:19:51 1614  last_license_offer_recurr_num NUMBER;
12:19:51 1615  var_last_license_id	     NUMBER;
12:19:51 1616  var_offer_chain_id	     NUMBER;
12:19:51 1617  var_current_offer_rec_number  NUMBER;
12:19:51 1618  var_next_offers_set	     SYS_REFCURSOR;
12:19:51 1619  var_next_offer_duration	     VARCHAR2(30);
12:19:51 1620  var_next_offer_recur	     NUMBER;
12:19:51 1621  var_infinity_offers_number    NUMBER;
12:19:51 1622  
12:19:51 1623  var_result_date DATE;
12:19:51 1624  
12:19:51 1625  var_ym_interval	INTERVAL YEAR TO MONTH;
12:19:51 1626  var_ds_interval	INTERVAL DAY(3) TO SECOND;
12:19:51 1627  var_offer_years	NUMBER;
12:19:51 1628  var_offer_months NUMBER;
12:19:51 1629  var_offer_days	NUMBER;
12:19:51 1630  
12:19:51 1631  -- EXCEPTIONS
12:19:51 1632  BAD_SUBSCRIPTION_ID	  EXCEPTION;
12:19:51 1633  CAN_NOT_FIND_LAST_LICENSE  EXCEPTION;
12:19:51 1634  CAN_NOT_CALC_OFFER_LENGTH  EXCEPTION;
12:19:51 1635  CAN_NOT_CALC_OFFER_LENGTH2 EXCEPTION;
12:19:51 1636  EXCEPTION_MESSAGE	  VARCHAR2(1024);
12:19:51 1637  BEGIN
12:19:51 1638  
12:19:51 1639  	 BEGIN
12:19:51 1640  	   SELECT
12:19:51 1641  	     SUBSCRIPTION.OFFER_CHAIN_ID into var_offer_chain_id
12:19:51 1642  	   FROM
12:19:51 1643  	     SUBSCRIPTION
12:19:51 1644  	   WHERE
12:19:51 1645  	     SUBSCRIPTION.ID = in_subscription_id;
12:19:51 1646  	   EXCEPTION
12:19:51 1647  	     WHEN NO_DATA_FOUND THEN
12:19:51 1648  	       RAISE BAD_SUBSCRIPTION_ID;
12:19:51 1649  	 END;
12:19:51 1650  
12:19:51 1651  	 BEGIN
12:19:51 1652  	   SELECT
12:19:51 1653  	     ID into var_last_license_id
12:19:51 1654  	   FROM
12:19:51 1655  	     (
12:19:51 1656  	       SELECT
12:19:51 1657  		 LICENSE.ID
12:19:51 1658  	       FROM
12:19:51 1659  		 LICENSE
12:19:51 1660  	       WHERE
12:19:51 1661  		 LICENSE.SUBSCRIPTION_ID = in_subscription_id
12:19:51 1662  	       ORDER BY
12:19:51 1663  		 LICENSE.END_DATE DESC
12:19:51 1664  	     )
12:19:51 1665  	   WHERE
12:19:51 1666  	     ROWNUM <= 1;
12:19:51 1667  	   EXCEPTION
12:19:51 1668  	     WHEN NO_DATA_FOUND THEN
12:19:51 1669  	       RAISE CAN_NOT_FIND_LAST_LICENSE;
12:19:51 1670  	 END;
12:19:51 1671  
12:19:51 1672  	 SELECT
12:19:51 1673  	   COUNT(*) into var_infinity_offers_number
12:19:51 1674  	 FROM
12:19:51 1675  	   OFFER_OFFER_CHAIN
12:19:51 1676  	 WHERE
12:19:51 1677  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
12:19:51 1678  	   AND OFFER_OFFER_CHAIN.NUM_RECURRENCES = GLOBAL_ENUMS_V23.OFFER_REC_INFINITY;
12:19:51 1679  
12:19:51 1680  	 IF var_infinity_offers_number > 0 THEN
12:19:51 1681  	   RETURN NULL;
12:19:51 1682  	 END IF;
12:19:51 1683  
12:19:51 1684  	 BEGIN
12:19:51 1685  	   SELECT
12:19:51 1686  	     LICENSE.ID,
12:19:51 1687  	     LICENSE.END_DATE,
12:19:51 1688  	     LICENSE.CURRENT_OFFER_INDEX,
12:19:51 1689  	     LICENSE.CURRENT_OFFER_RECURR_NUM,
12:19:51 1690  	     LICENSE.OFFER_ID
12:19:51 1691  	     into
12:19:51 1692  	     last_license_id,
12:19:51 1693  	     last_license_end_date,
12:19:51 1694  	     last_license_offer_index,
12:19:51 1695  	     last_license_offer_recurr_num,
12:19:51 1696  	     last_license_offer_id
12:19:51 1697  	   FROM
12:19:51 1698  	     LICENSE
12:19:51 1699  	   WHERE
12:19:51 1700  	     LICENSE.ID = var_last_license_id;
12:19:51 1701  	   EXCEPTION
12:19:51 1702  	     WHEN NO_DATA_FOUND THEN
12:19:51 1703  	       RAISE CAN_NOT_FIND_LAST_LICENSE;
12:19:51 1704  	 END;
12:19:51 1705  
12:19:51 1706  	 var_result_date := last_license_end_date;
12:19:51 1707  
12:19:51 1708  	 -- Find current recurrence number
12:19:51 1709  	 SELECT
12:19:51 1710  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES into var_current_offer_rec_number
12:19:51 1711  	 FROM
12:19:51 1712  	   OFFER_OFFER_CHAIN
12:19:51 1713  	 WHERE
12:19:51 1714  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
12:19:51 1715  	   AND OFFER_OFFER_CHAIN.OFFER_ID = last_license_offer_id
12:19:51 1716  	   AND OFFER_OFFER_CHAIN.INDEX_VALUE = last_license_offer_index;
12:19:51 1717  
12:19:51 1718  	 IF var_current_offer_rec_number > last_license_offer_recurr_num THEN
12:19:51 1719  	   BEGIN
12:19:51 1720  	     PROCS_OFFER_CHAIN_V23.GET_OFFER_LENGTH(
12:19:51 1721  	       last_license_offer_id,
12:19:51 1722  	       var_offer_years,
12:19:51 1723  	       var_offer_months,
12:19:51 1724  	       var_offer_days
12:19:51 1725  	     );
12:19:51 1726  
12:19:51 1727  	     var_ym_interval := var_offer_years||'-'||var_offer_months;
12:19:51 1728  	     var_ds_interval := var_offer_days||' 0:0:0';
12:19:51 1729  
12:19:51 1730  	     var_result_date := var_result_date
12:19:51 1731  	       + ( var_ym_interval * ( var_current_offer_rec_number - last_license_offer_recurr_num ) )
12:19:51 1732  	       + ( var_ds_interval * ( var_current_offer_rec_number - last_license_offer_recurr_num ) );
12:19:51 1733  	     EXCEPTION
12:19:51 1734  	       WHEN OTHERS THEN
12:19:51 1735  		 EXCEPTION_MESSAGE := SQLERRM;
12:19:51 1736  		 RAISE CAN_NOT_CALC_OFFER_LENGTH;
12:19:51 1737  	   END;
12:19:51 1738  	 END IF;
12:19:51 1739  
12:19:51 1740  	 OPEN var_next_offers_set FOR
12:19:51 1741  	 SELECT
12:19:51 1742  	   OFFER.ENTITLEMENT_DURATION,
12:19:51 1743  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES
12:19:51 1744  	 FROM
12:19:51 1745  	   OFFER_OFFER_CHAIN
12:19:51 1746  	   INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
12:19:51 1747  	 WHERE
12:19:51 1748  	   OFFER_OFFER_CHAIN.INDEX_VALUE > last_license_offer_index
12:19:51 1749  	   AND OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id;
12:19:51 1750  
12:19:51 1751  	 LOOP
12:19:51 1752  	   FETCH var_next_offers_set into var_next_offer_duration, var_next_offer_recur;
12:19:51 1753  	   EXIT WHEN var_next_offers_set%NOTFOUND;
12:19:51 1754  	   BEGIN
12:19:51 1755  	     PROCS_COMMON_V23.ISO8601DURATION_TO_INTERVALS(
12:19:51 1756  	       var_next_offer_duration,
12:19:51 1757  	       var_offer_years,
12:19:51 1758  	       var_offer_months,
12:19:51 1759  	       var_offer_days
12:19:51 1760  	     );
12:19:51 1761  
12:19:51 1762  	     var_ym_interval := var_offer_years||'-'||var_offer_months;
12:19:51 1763  	     var_ds_interval := var_offer_days||' 0:0:0';
12:19:51 1764  
12:19:51 1765  	     var_result_date := var_result_date
12:19:51 1766  	       + ( var_ym_interval * var_next_offer_recur )
12:19:51 1767  	       + ( var_ds_interval * var_next_offer_recur );
12:19:51 1768  	     EXCEPTION
12:19:51 1769  	       WHEN OTHERS THEN
12:19:51 1770  		 EXCEPTION_MESSAGE := SQLERRM;
12:19:51 1771  		 RAISE CAN_NOT_CALC_OFFER_LENGTH2;
12:19:51 1772  	   END;
12:19:51 1773  	 END LOOP;
12:19:51 1774  
12:19:51 1775  	 RETURN var_result_date;
12:19:51 1776  
12:19:51 1777  EXCEPTION
12:19:51 1778  WHEN BAD_SUBSCRIPTION_ID THEN
12:19:51 1779  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 1780  	   SPROC_NAME, 'No such subscription');
12:19:51 1781  WHEN CAN_NOT_FIND_LAST_LICENSE THEN
12:19:51 1782  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 1783  	   SPROC_NAME, 'Could not find last license for given subscription');
12:19:51 1784  WHEN CAN_NOT_CALC_OFFER_LENGTH THEN
12:19:51 1785  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 1786  	   SPROC_NAME, 'Could not calculate offer length', EXCEPTION_MESSAGE);
12:19:51 1787  WHEN CAN_NOT_CALC_OFFER_LENGTH2 THEN
12:19:51 1788  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 1789  	   SPROC_NAME, 'Could not calculate last offer length', EXCEPTION_MESSAGE);
12:19:51 1790  WHEN OTHERS THEN
12:19:51 1791  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 1792  	   SPROC_NAME, 'Unkown error', SQLERRM);
12:19:51 1793  END CALC_SUBSCRIPTION_END_DATE;
