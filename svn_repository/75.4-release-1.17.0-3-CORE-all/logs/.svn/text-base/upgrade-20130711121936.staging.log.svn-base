12:19:52 453  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INVALID_PARAMETER,
12:19:52 454  	  SPROC_NAME, 'Wrong gift certificate type');
12:19:52 455  WHEN OTHERS THEN
12:19:52 456  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:52 457  	  SPROC_NAME, 'Unknown error', SQLERRM);
