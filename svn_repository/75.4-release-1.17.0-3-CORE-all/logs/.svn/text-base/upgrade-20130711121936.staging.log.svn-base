12:19:51 396  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 397  	  SPROC_NAME, 'Could not create invoice', EXCEPTION_MESSAGE);
12:19:51 398  WHEN CAN_NOT_CREATE_LINE_ITEMS THEN
12:19:51 399  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 400  	  SPROC_NAME, 'Could not create line items', EXCEPTION_MESSAGE);
12:19:51 401  WHEN CAN_NOT_CREATE_LICENSE THEN
12:19:51 402  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 403  	  SPROC_NAME, 'Could not create new license', EXCEPTION_MESSAGE);
12:19:51 404  WHEN BAD_GROUP_ID THEN
12:19:51 405  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 406  	  SPROC_NAME, 'Bad group id');
12:19:51 407  WHEN CAN_NOT_CREATE_NOTE THEN
12:19:51 408  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
