12:19:51  58  	    OFFER.ID = in_offer_id;
12:19:51  59  
12:19:51  60  	  EXCEPTION
12:19:51  61  	    WHEN NO_DATA_FOUND THEN
12:19:51  62  	      RAISE BAD_OFFER_ID;
12:19:51  63  	END;
12:19:51  64  
12:19:51  65  	-- Get product_offering data
12:19:51  66  	OPEN var_line_item_data FOR
12:19:51  67  	SELECT
12:19:51  68  	  PRODUCT_OFFERING.ID,
12:19:51  69  	  PRODUCT_OFFERING.UNIT_PRICE,
12:19:51  70  	  PRODUCT_OFFERING.QUANTITY
12:19:51  71  	FROM
12:19:51  72  	  OFFER_PRODUCT_OFFERING
12:19:51  73  	  INNER JOIN PRODUCT_OFFERING ON OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = PRODUCT_OFFERING.ID
12:19:51  74  	WHERE
12:19:51  75  	  OFFER_PRODUCT_OFFERING.OFFER_ID = in_offer_id;
12:19:51  76  
12:19:51  77  	-- insert line items
12:19:51  78  	-- add discounts to line items
12:19:51  79  	LOOP
12:19:51  80  	  FETCH var_line_item_data INTO
12:19:51  81  	    var_product_offering_id,
12:19:51  82  	    var_product_unit_price,
12:19:51  83  	    var_product_quantity;
12:19:51  84  	  EXIT WHEN var_line_item_data%NOTFOUND;
12:19:51  85  
12:19:51  86  	  var_product_offering_oprice := var_product_unit_price * var_product_quantity;
12:19:51  87  	  var_product_offering_price := var_product_offering_oprice;
12:19:51  88  
12:19:51  89  	  -- Apply discounts to line_item
12:19:51  90  	  BEGIN
12:19:51  91  	    SELECT
12:19:51  92  	      SUM (DISCOUNT.FIXED_AMOUNT) into var_discount_fixed_amount
12:19:51  93  	    FROM
12:19:51  94  	      DISCOUNT_PRODUCT_OFFERING
12:19:51  95  	      INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
12:19:51  96  	    WHERE
12:19:51  97  	      DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
12:19:51  98  	      AND DISCOUNT.FIXED_AMOUNT IS NOT NULL;
12:19:51  99  	    EXCEPTION
12:19:51 100  	    WHEN NO_DATA_FOUND THEN
12:19:51 101  	      var_discount_fixed_amount := NULL;
12:19:51 102  	  END;
12:19:51 103  
12:19:51 104  	  BEGIN
12:19:51 105  	    SELECT
12:19:51 106  	      SUM (DISCOUNT.PERCENT_AMOUNT) into var_discount_percent_amount
12:19:51 107  	    FROM
12:19:51 108  	      DISCOUNT_PRODUCT_OFFERING
12:19:51 109  	      INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
12:19:51 110  	    WHERE
12:19:51 111  	      DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
12:19:51 112  	      AND DISCOUNT.PERCENT_AMOUNT IS NOT NULL;
12:19:51 113  	    EXCEPTION
12:19:51 114  	    WHEN NO_DATA_FOUND THEN
12:19:51 115  	      var_discount_percent_amount := NULL;
12:19:51 116  	  END;
12:19:51 117  
12:19:51 118  	  IF (var_discount_percent_amount IS NOT NULL) THEN
12:19:51 119  	    var_product_offering_price := var_product_offering_price * ( 1 - var_discount_percent_amount / 100 );
12:19:51 120  	  END IF;
12:19:51 121  
12:19:51 122  	  IF (var_discount_fixed_amount IS NOT NULL) THEN
12:19:51 123  	    var_product_offering_price := var_product_offering_price - var_discount_fixed_amount;
12:19:51 124  	  END IF;
12:19:51 125  
12:19:51 126  	  var_line_item_price := PROCS_COMMON_V23.ROUND_10_6_TO_10_2(var_product_offering_price);
12:19:51 127  
12:19:51 128  	  IF (var_line_item_price < 0) THEN
12:19:51 129  	      RAISE BAD_DISCOUNT;
12:19:51 130  	  END IF;
12:19:51 131  
12:19:51 132  	  var_new_line_item_id := NULL;
12:19:51 133  	  PROCS_LINE_ITEMS_CRU_V23.CREATE_LINE_ITEM(
12:19:51 134  	    inout_line_item_id	=> var_new_line_item_id,
12:19:51 135  	    in_product_offer_id => var_product_offering_id,
12:19:51 136  	    in_invoice_id	=> in_invoice_id,
12:19:51 137  	    in_amount		=> var_line_item_price,
12:19:51 138  	    in_created_by	=> in_created_by,
12:19:51 139  	    in_discount_amount	=> var_product_offering_oprice - var_line_item_price,
12:19:51 140  	    in_taxes_amount	=> NULL
12:19:51 141  	  );
12:19:51 142  
12:19:51 143  	  FOR f_discount IN (
12:19:51 144  	    SELECT
12:19:51 145  	      DISCOUNT.ID
12:19:51 146  	    FROM
12:19:51 147  	      DISCOUNT_PRODUCT_OFFERING
12:19:51 148  	      INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
12:19:51 149  	    WHERE
12:19:51 150  	      DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
12:19:51 151  	  )
12:19:51 152  	  LOOP
12:19:51 153  	    PROCS_LINE_ITEMS_CRU_V23.CREATE_DISCOUNT_LINE_ITEM(
12:19:51 154  	      in_discount_id =>  f_discount.ID,
12:19:51 155  	      in_line_item_id => var_new_line_item_id
12:19:51 156  	    );
12:19:51 157  	  END LOOP;
12:19:51 158  	END LOOP;
12:19:51 159  
12:19:51 160  EXCEPTION
12:19:51 161  WHEN BAD_INVOICE_ID THEN
12:19:51 162  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 163  	  SPROC_NAME, 'No such license');
12:19:51 164  WHEN BAD_OFFER_ID THEN
12:19:51 165  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 166  	  SPROC_NAME, 'No such offer');
12:19:51 167  WHEN BAD_DISCOUNT THEN
12:19:51 168  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 169  	  SPROC_NAME, 'Bad Discount');
12:19:51 170  WHEN OTHERS THEN
