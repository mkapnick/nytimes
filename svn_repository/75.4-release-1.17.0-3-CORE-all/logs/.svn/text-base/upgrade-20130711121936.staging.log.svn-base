12:19:51 1164  	       RETURN GLOBAL_CONSTANTS_V23.TRUE;
12:19:51 1165  	     END IF;
12:19:51 1166  
12:19:51 1167  	   END IF;
12:19:51 1168  	 END LOOP;
12:19:51 1169  
12:19:51 1170  	 RETURN GLOBAL_CONSTANTS_V23.FALSE;
12:19:51 1171  
12:19:51 1172  EXCEPTION
12:19:51 1173  WHEN BAD_FIRST_OFFER_CHAIN THEN
12:19:51 1174  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 1175  	   SPROC_NAME, 'First offer chain not found');
12:19:51 1176  WHEN BAD_SECOND_OFFER_CHAIN THEN
12:19:51 1177  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 1178  	   SPROC_NAME, 'Second offer chain not found');
12:19:51 1179  WHEN OTHERS THEN
12:19:51 1180  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 1181  	   SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 1182  END CHECK_FOR_SAME_PRODUCTS;
