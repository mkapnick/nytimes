12:19:51 733  
12:19:51 734  	-- Check that current offer index exists
12:19:51 735  	BEGIN
12:19:51 736  	  SELECT
12:19:51 737  	    OFFER_OFFER_CHAIN.INDEX_VALUE into temp_offer_chain_current_index
12:19:51 738  	  FROM
12:19:51 739  	    OFFER_OFFER_CHAIN
12:19:51 740  	  WHERE
12:19:51 741  	    OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
12:19:51 742  	    AND OFFER_OFFER_CHAIN.INDEX_VALUE = in_offer_chain_current_index
12:19:51 743  	    -- TODO: delete next line
12:19:51 744  	    AND ROWNUM <= 1;
12:19:51 745  	  EXCEPTION
12:19:51 746  	    WHEN NO_DATA_FOUND THEN
12:19:51 747  	      RAISE BAD_CURRENT_INDEX_VALUE;
12:19:51 748  	END;
12:19:51 749  
12:19:51 750  	-- Get next offer index
12:19:51 751  	BEGIN
12:19:51 752  	  SELECT
12:19:51 753  	    INDEX_VALUE into var_result
12:19:51 754  	  FROM (
12:19:51 755  	    SELECT
12:19:51 756  	      OFFER_OFFER_CHAIN.INDEX_VALUE
12:19:51 757  	    FROM
12:19:51 758  	      OFFER_OFFER_CHAIN
12:19:51 759  	    WHERE
12:19:51 760  	      OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
12:19:51 761  	      AND OFFER_OFFER_CHAIN.INDEX_VALUE > in_offer_chain_current_index
12:19:51 762  	    ORDER BY
12:19:51 763  	      OFFER_OFFER_CHAIN.INDEX_VALUE ASC
12:19:51 764  	  )
12:19:51 765  	  WHERE
12:19:51 766  	    ROWNUM <= 1;
12:19:51 767  	  EXCEPTION
12:19:51 768  	    WHEN NO_DATA_FOUND THEN
12:19:51 769  	      var_result := NULL;
12:19:51 770  	END;
12:19:51 771  
12:19:51 772  	RETURN var_result;
12:19:51 773  
12:19:51 774  EXCEPTION
12:19:51 775  WHEN BAD_OFFER_CHAIN_ID THEN
12:19:51 776  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 777  	  SPROC_NAME, 'No such offer chain');
12:19:51 778  WHEN BAD_CURRENT_INDEX_VALUE THEN
12:19:51 779  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 780  	  SPROC_NAME, 'Bad current index value');
12:19:51 781  WHEN OTHERS THEN
12:19:51 782  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 783  	  SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 784  END GET_NEXT_OFFER_INDEX;
