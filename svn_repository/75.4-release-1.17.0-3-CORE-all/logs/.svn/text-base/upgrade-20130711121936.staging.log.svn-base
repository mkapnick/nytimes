12:19:51 2051  
12:19:51 2052  PROCEDURE CANCEL_SUBSCRIPTION_INVOICE (
12:19:51 2053  	 in_invoice_id	      IN NUMBER,
12:19:51 2054  	 in_updated_by	      IN VARCHAR2,
12:19:51 2055  	 in_refundable	      IN NUMBER DEFAULT GLOBAL_CONSTANTS_V23.FALSE
12:19:51 2056  ) AS
12:19:51 2057  SPROC_NAME CONSTANT VARCHAR2(27) := 'CANCEL_SUBSCRIPTION_INVOICE';
12:19:51 2058  -- VARIABLES
12:19:51 2059  temp_invoice_id	      INVOICE.ID%TYPE;
12:19:51 2060  var_chargeback_amount  NUMBER(10,2);
12:19:51 2061  ver_refund_charge_id   NUMBER;
12:19:51 2062  -- EXCEPTIONS
12:19:51 2063  BAD_INVOICE_ID		    EXCEPTION;
12:19:51 2064  CAN_NOT_CALCULATE_CHARGEBACK EXCEPTION;
12:19:51 2065  CAN_NOT_APPLY_CHARGEBACK     EXCEPTION;
12:19:51 2066  EXCEPTION_MESSAGE	    VARCHAR2(1024);
12:19:51 2067  -- STUB
12:19:51 2068  var_now DATE;
12:19:51 2069  var_revoke NUMBER;
12:19:51 2070  var_refund NUMBER;
12:19:51 2071  var_billed NUMBER;
12:19:51 2072  var_subscription_in_grace NUMBER;
12:19:51 2073  BEGIN
12:19:51 2074  
12:19:51 2075  	 var_now := sysdate;
12:19:51 2076  
12:19:51 2077  	 -- Check that invoice exists
12:19:51 2078  	 BEGIN
12:19:51 2079  	   SELECT
12:19:51 2080  	     INVOICE.ID into temp_invoice_id
12:19:51 2081  	   FROM
12:19:51 2082  	     INVOICE
12:19:51 2083  	   WHERE
12:19:51 2084  	     INVOICE.ID = in_invoice_id;
12:19:51 2085  	   EXCEPTION
12:19:51 2086  	     WHEN NO_DATA_FOUND THEN
12:19:51 2087  	       RAISE BAD_INVOICE_ID;
12:19:51 2088  	 END;
12:19:51 2089  
12:19:51 2090  	 select decode(count(1), 0, GLOBAL_CONSTANTS_V23.FALSE, GLOBAL_CONSTANTS_V23.TRUE) into var_revoke
12:19:51 2091  	 from license l, subscription s, offer_chain oc
12:19:51 2092  	 where
12:19:51 2093  	   l.subscription_id = s.id and
12:19:51 2094  	   s.offer_chain_id = oc.id and
12:19:51 2095  	   l.invoice_id = in_invoice_id and
12:19:51 2096  	   oc.revoke_entitlements = GLOBAL_CONSTANTS_V23.TRUE and
12:19:51 2097  	   rownum < 2;
12:19:51 2098  
12:19:51 2099  	 select
12:19:51 2100  	   decode(s.subscription_status_id, GLOBAL_STATUSES_V23.SUBSCRIPTION_IN_GRACE_PERIOD, 1, 0)
12:19:51 2101  	 into
12:19:51 2102  	   var_subscription_in_grace
12:19:51 2103  	 from license l, subscription s
12:19:51 2104  	 where
12:19:51 2105  	   l.subscription_id = s.id and
12:19:51 2106  	   l.invoice_id = in_invoice_id and
12:19:51 2107  	   rownum < 2;
12:19:51 2108  
12:19:51 2109  	 var_billed := PROCS_INVOICE_V23.IS_INVOICE_PAYING_STARTED(in_invoice_id);
12:19:51 2110  	 var_refund := GLOBAL_CONSTANTS_V23.FALSE;
12:19:51 2111  
12:19:51 2112  	 -- Check that transaction for given invoice not started
12:19:51 2113  	 -- if refund enabled calculate and apply chargeback
12:19:51 2114  	 IF (
12:19:51 2115  	     var_billed = GLOBAL_CONSTANTS_V23.TRUE
12:19:51 2116  	   )THEN
12:19:51 2117  	   if (in_refundable = GLOBAL_CONSTANTS_V23.TRUE) then
12:19:51 2118  	     -- If started then we need to calculate refund
12:19:51 2119  	     BEGIN
12:19:51 2120  	       PROCS_INVOICE_V23.CALCULATE_INVOICE_CHARGEBACK(
12:19:51 2121  		 in_invoice_id,
