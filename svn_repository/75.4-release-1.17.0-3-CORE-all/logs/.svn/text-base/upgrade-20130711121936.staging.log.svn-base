12:19:51 144  	IF var_account_status = GLOBAL_STATUSES_V23.ACCOUNT_DISABLED THEN
12:19:51 145  	  RAISE CAN_NOT_CREATE_FOR_DISABLE;
12:19:51 146  	END IF;
12:19:51 147  
12:19:51 148  	-- Get offer chain status
12:19:51 149  	BEGIN
12:19:51 150  	  SELECT
12:19:51 151  	    OFFER_CHAIN.OFFER_CHAIN_STATUS_ID,
12:19:51 152  	    OFFER_CHAIN.IS_GIFT_CERTIFICATE
12:19:51 153  	    into
12:19:51 154  	    var_offer_chain_status_id,
12:19:51 155  	    var_is_gift_certificate
12:19:51 156  	  FROM
12:19:51 157  	    OFFER_CHAIN
12:19:51 158  	  WHERE
12:19:51 159  	    OFFER_CHAIN.ID = in_offer_chain_id;
12:19:51 160  
12:19:51 161  	  EXCEPTION
12:19:51 162  	    WHEN NO_DATA_FOUND THEN
12:19:51 163  	      RAISE BAD_OFFER_CHAIN;
12:19:51 164  	END;
12:19:51 165  
12:19:51 166  	-- Could not subscribe to inactive/disabled offer chain
12:19:51 167  	IF var_offer_chain_status_id != GLOBAL_STATUSES_V23.OFFER_CHAIN_ACTIVE THEN
12:19:51 168  	  RAISE BAD_OFFER_CHAIN_STATUS;
12:19:51 169  	END IF;
12:19:51 170  
12:19:51 171  	-- Can not subscribe to Offer Chain for a Gift Certfiicate
12:19:51 172  	IF var_is_gift_certificate = GLOBAL_CONSTANTS_V23.TRUE THEN
12:19:51 173  	  RAISE CAN_NOT_SUBSCRIBE_TO_GC;
12:19:51 174  	END IF;
12:19:51 175  
12:19:51 176  	-- check if the OC is for Redemption:
12:19:51 177  	SELECT
12:19:51 178  	  COUNT(*) into var_is_for_redemption
12:19:51 179  	FROM
12:19:51 180  	  OFFER_CHAIN_ELIGIBILITY
12:19:51 181  	WHERE
12:19:51 182  	  OFFER_CHAIN_ELIGIBILITY.OFFER_CHAIN_ID = in_offer_chain_id
12:19:51 183  	  AND OFFER_CHAIN_ELIGIBILITY.NAME = GLOBAL_CONSTANTS_V23.GIFT_CERTIFICATE_REQUIRED
12:19:51 184  	  AND OFFER_CHAIN_ELIGIBILITY.VALUE = GLOBAL_CONSTANTS_V23.ELIGIBILITY_FLAG_SET;
