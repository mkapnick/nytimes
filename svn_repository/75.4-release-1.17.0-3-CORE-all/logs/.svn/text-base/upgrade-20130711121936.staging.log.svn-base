12:19:51 659  
12:19:51 660  PROCEDURE GET_SUBSCRIPTION_INFO (
12:19:51 661  	  in_subscription_id  IN  NUMBER,
12:19:51 662  	  out_result_set      OUT SYS_REFCURSOR
12:19:51 663  ) AS
12:19:51 664  SPROC_NAME CONSTANT VARCHAR2(21) := 'GET_SUBSCRIPTION_INFO';
12:19:51 665  -- VARIABLES
12:19:51 666  temp_subscription_id NUMBER;
12:19:51 667  -- EXCEPTIONS
12:19:51 668  BAD_SUBSCRIPTION_ID EXCEPTION;
12:19:51 669  BEGIN
12:19:51 670  	-- Find subscription by id
12:19:51 671  	-- Return its details
12:19:51 672  
12:19:51 673  	BEGIN
12:19:51 674  	  SELECT
12:19:51 675  	    SUBSCRIPTION.ID into temp_subscription_id
12:19:51 676  	  FROM
12:19:51 677  	    SUBSCRIPTION
12:19:51 678  	  WHERE
12:19:51 679  	    SUBSCRIPTION.ID = in_subscription_id;
12:19:51 680  	  EXCEPTION
12:19:51 681  	    WHEN NO_DATA_FOUND THEN
12:19:51 682  	      RAISE BAD_SUBSCRIPTION_ID;
12:19:51 683  	END;
12:19:51 684  
12:19:51 685  	OPEN out_result_set FOR
12:19:51 686  	SELECT
12:19:51 687  	  SUBSCRIPTION.ACCOUNT_ID,
12:19:51 688  	  SUBSCRIPTION.ID AS "SUBSCRIPTION_ID",
12:19:51 689  	  SUBSCRIPTION.SUBSCRIPTION_STATUS_ID,
12:19:51 690  	  SUBSCRIPTION.PURCHASE_DATE,
12:19:51 691  	  SUBSCRIPTION.SUSPEND_DATE,
12:19:51 692  	  SUBSCRIPTION.REACTIVATION_DATE,
12:19:51 693  	  SUBSCRIPTION.CANCELLATION_DATE,
12:19:51 694  	  SUBSCRIPTION_CANCEL_REASON.VALUE as "CANCEL_TYPE",
12:19:51 695  	  OFFER_CHAIN.ID AS "OFFER_CHAIN_ID",
12:19:51 696  	  OFFER_CHAIN.NAME,
12:19:51 697  	  OFFER_CHAIN.DESCRIPTION,
12:19:51 698  	  OFFER_CHAIN.PRODUCT_URI,
12:19:51 699  	  SUBSCRIPTION.INSTRUMENT_ID,
12:19:51 700  	  SUBSCRIPTION.INSTRUMENT_TYPE_ID,
12:19:51 701  	  PROCS_SUBSCRIPTION_V23.CALC_SUBSCRIPTION_END_DATE(SUBSCRIPTION.ID) as "END_DATE",
12:19:51 702  	  PROCS_SUBSCRIPTION_V23.GET_RECENT_CHARGE(SUBSCRIPTION.ID) AS "RECENT_CHARGE",
12:19:51 703  	  PROCS_SUBSCRIPTION_V23.GET_RENEWAL_DATE(SUBSCRIPTION.ID) AS "RENEWAL_DATE",
12:19:51 704  	  PROCS_SUBSCRIPTION_V23.GET_BILLING_CYCLE(SUBSCRIPTION.ID) AS "BILLING_CYCLE",
12:19:51 705  	  (
12:19:51 706  	    SELECT
12:19:51 707  	      ACCOUNT.GROUP_ID
12:19:51 708  	      FROM ACCOUNT
12:19:51 709  	      WHERE ACCOUNT.ID = SUBSCRIPTION.ACCOUNT_ID
12:19:51 710  	  ) as "GROUP_ID",
12:19:51 711  	  (
12:19:51 712  	    SELECT
12:19:51 713  	      MAX(ENTITLEMENT_END_DATE)
12:19:51 714  	      FROM LICENSE
12:19:51 715  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
12:19:51 716  	  )
12:19:51 717  	  as "ENT_END_DATE",
12:19:51 718  	  (
12:19:51 719  	    SELECT
12:19:51 720  	      MIN(START_DATE)
12:19:51 721  	      FROM LICENSE
12:19:51 722  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
12:19:51 723  	  )
12:19:51 724  	  as "ENT_START_DATE",
12:19:51 725  	  PROCS_SUBSCRIPTION_V23.IS_SUBSCRIPTION_CANCELABLE(SUBSCRIPTION.ID) AS "IS_CANCELABLE",
12:19:51 726  	  ITUNES_RECEIPT.ID AS "ITUNES_RECEIPT_ID",
12:19:51 727  	  GROUP_ACCOUNT.ID GA_ID,
12:19:51 728  	  GROUP_ACCOUNT.SUBSCRIPTION_ID GA_SUBSCRIPTION_ID,
12:19:51 729  	  GROUP_ACCOUNT.GROUP_NAME GA_GROUP_NAME,
12:19:51 730  	  GROUP_ACCOUNT.FIRST_NAME GA_FIRST_NAME,
12:19:51 731  	  GROUP_ACCOUNT.LAST_NAME GA_LAST_NAME,
12:19:51 732  	  GROUP_ACCOUNT.EMAIL GA_EMAIL,
12:19:51 733  	  GROUP_ACCOUNT.PHONE GA_PHONE,
12:19:51 734  	  GROUP_ACCOUNT.ORGANIZATION_TYPE GA_ORGANIZATION_TYPE,
12:19:51 735  	  GROUP_ACCOUNT.SEATS GA_SEATS,
12:19:51 736  	  PROCS_GROUP_ACCOUNT_V23.F_GET_NUM_OCCUPIED_GROUP_SEATS(GROUP_ACCOUNT.ID) GA_SEATS_USED,
12:19:51 737  	  GROUP_ACCOUNT.IP GA_IP,
12:19:51 738  	  PROCS_SUBSCRIPTION_V23.GET_GIFT_CERT_CODE_BY_SUB_ID(SUBSCRIPTION.ID) gift_certificate_code,
12:19:51 739  	  PROCS_ACCOUNT_V23.GET_GRACE_START_DATE(SUBSCRIPTION.ID) GRACE_START_DATE,
12:19:51 740  	  PROCS_ACCOUNT_V23.GET_GRACE_END_DATE(SUBSCRIPTION.ID) GRACE_END_DATE
12:19:51 741  	FROM
12:19:51 742  	  SUBSCRIPTION
12:19:51 743  	  INNER JOIN OFFER_CHAIN ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
12:19:51 744  	  LEFT JOIN SUBSCRIPTION_CANCEL_REASON ON SUBSCRIPTION.SCT_ID = SUBSCRIPTION_CANCEL_REASON.ID
12:19:51 745  	  LEFT JOIN ITUNES_RECEIPT ON SUBSCRIPTION.ID = ITUNES_RECEIPT.SUBSCRIPTION_ID
12:19:51 746  	  LEFT JOIN GROUP_ACCOUNT ON SUBSCRIPTION.ID = GROUP_ACCOUNT.SUBSCRIPTION_ID
12:19:51 747  	WHERE
12:19:51 748  	  SUBSCRIPTION.ID = in_subscription_id;
12:19:51 749  
12:19:51 750  EXCEPTION
12:19:51 751  WHEN BAD_SUBSCRIPTION_ID THEN
12:19:51 752  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 753  	  SPROC_NAME, 'No such subscription');
12:19:51 754  WHEN OTHERS THEN
12:19:51 755  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 756  	  SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 757  END GET_SUBSCRIPTION_INFO;
12:19:51 758  
