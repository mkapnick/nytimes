12:19:51 1154  	 LOOP
12:19:51 1155  	   FETCH var_same_products INTO var_same_product_id, same_product_count;
12:19:51 1156  	   EXIT WHEN var_same_products%NOTFOUND;
12:19:51 1157  
12:19:51 1158  	   IF in_use_eligibility_rules = GLOBAL_CONSTANTS_V23.FALSE THEN
12:19:51 1159  	     -- Return false because this offer chains having same products
