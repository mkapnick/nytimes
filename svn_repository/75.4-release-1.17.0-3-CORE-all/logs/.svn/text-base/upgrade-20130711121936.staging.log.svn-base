12:19:51 927  
12:19:51 928  	PROCEDURE FINALIZE_GC_PURCHASING (
12:19:51 929  	  in_invoice_id 	IN NUMBER,
12:19:51 930  	  in_created_by 	IN VARCHAR2,
12:19:51 931  	  in_instrument_id	IN NUMBER,
12:19:51 932  	  in_instrument_type_id IN NUMBER,
12:19:51 933  	  in_order_id		IN VARCHAR2,
12:19:51 934  	  in_transaction_id	IN NUMBER,
12:19:51 935  	  out_charge_amount	OUT NUMBER
12:19:51 936  	) AS
12:19:51 937  	  SPROC_NAME CONSTANT VARCHAR2(22) := 'FINALIZE_GC_PURCHASING';
12:19:51 938  -- VARIABLES
12:19:51 939  	  temp_transaction_id_count NUMBER;
12:19:51 940  	  var_invoice_amount	    NUMBER(10,2);
12:19:51 941  	  var_transaction_id	    NUMBER;
12:19:51 942  	  var_new_charge_id	    NUMBER;
12:19:51 943  -- EXCEPTIONS
12:19:51 944  	    BAD_CREDIT_CARD_ID		EXCEPTION;
12:19:51 945  	    BAD_PAYPAL_ID		EXCEPTION;
12:19:51 946  	    BAD_INSTRUMENT_TYPE 	EXCEPTION;
12:19:51 947  	    TRANSACTION_EXISTS		EXCEPTION;
12:19:51 948  	    CAN_NOT_CALC_INVOICE_AMOUNT EXCEPTION;
12:19:51 949  	    CAN_NOT_USE_FCINSTR 	EXCEPTION;
12:19:51 950  	    CAN_NOT_CREATE_TRANSACTION	EXCEPTION;
12:19:51 951  	    CAN_NOT_CREATE_CHARGE	EXCEPTION;
12:19:51 952  	  EXCEPTION_MESSAGE   VARCHAR2(1024);
12:19:51 953  	  BEGIN
12:19:51 954  
12:19:51 955  -- Check that instrument exists
12:19:51 956  	    IF in_instrument_type_id = GLOBAL_ENUMS_V23.INSTRUMENT_CREDIT_CARD THEN
12:19:51 957  	      IF IS_CREDIT_CARD_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V23.FALSE THEN
12:19:51 958  		RAISE BAD_CREDIT_CARD_ID;
12:19:51 959  	      END IF;
12:19:51 960  	    ELSIF in_instrument_type_id = GLOBAL_ENUMS_V23.INSTRUMENT_PAYPAL THEN
12:19:51 961  	      IF IS_PAYPAL_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V23.FALSE THEN
12:19:51 962  		RAISE BAD_PAYPAL_ID;
12:19:51 963  	      END IF;
12:19:51 964  	    ELSIF in_instrument_type_id = GLOBAL_ENUMS_V23.INSTRUMENT_ZCI_INSTRUMENT THEN
12:19:51 965  	      NULL;
12:19:51 966  	    ELSE
12:19:51 967  	      RAISE BAD_INSTRUMENT_TYPE;
12:19:51 968  	    END IF;
12:19:51 969  
12:19:51 970  -- Check that transaction with given id do not exists
12:19:51 971  	    SELECT
12:19:51 972  	      COUNT(*) into temp_transaction_id_count
12:19:51 973  	    FROM
12:19:51 974  	      TRANSACTION
12:19:51 975  	    WHERE
12:19:51 976  	      TRANSACTION.ID = in_transaction_id;
12:19:51 977  
12:19:51 978  	    IF temp_transaction_id_count > 0 THEN
12:19:51 979  	      RAISE TRANSACTION_EXISTS;
12:19:51 980  	    END IF;
12:19:51 981  
12:19:51 982  -- Calculate new invoice amount
12:19:51 983  	    BEGIN
12:19:51 984  	      PROCS_INVOICE_V23.CALCULATE_INVOICE_AMOUNT(in_invoice_id, var_invoice_amount);
12:19:51 985  	      EXCEPTION
12:19:51 986  	      WHEN OTHERS THEN
12:19:51 987  	      EXCEPTION_MESSAGE := SQLERRM;
12:19:51 988  	      RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
12:19:51 989  	    END;
12:19:51 990  
12:19:51 991  	    IF var_invoice_amount > 0
12:19:51 992  	       AND in_instrument_type_id = GLOBAL_ENUMS_V23.INSTRUMENT_ZCI_INSTRUMENT THEN
12:19:51 993  	      RAISE CAN_NOT_USE_FCINSTR;
12:19:51 994  	    END IF;
12:19:51 995  
12:19:51 996  -- If invoice amount iz 0 then we need to set status for this invoice to PROCCESSED
12:19:51 997  	    IF var_invoice_amount = 0 THEN
12:19:51 998  	      PROCS_INVOICE_CRU_V23.UPDATE_INVOICE(
12:19:51 999  		  in_invoice_id 		 => in_invoice_id,
12:19:51 1000  		   in_updated_by		  => in_created_by,
12:19:51 1001  		   in_invoice_status_id 	  => GLOBAL_STATUSES_V23.INVOICE_CLOSED
12:19:51 1002  	       );
12:19:51 1003  	     END IF;
12:19:51 1004  
12:19:51 1005  	     IF var_invoice_amount > 0 THEN
12:19:51 1006  -- Create transaction
12:19:51 1007  	       BEGIN
12:19:51 1008  		 PROCS_TRANSACTION_V23.CREATE_TRANSACTION(
12:19:51 1009  		     in_transaction_id	      => in_transaction_id,
12:19:51 1010  		     in_status_id	      => GLOBAL_STATUSES_V23.TRANSACTION_PENDING,
12:19:51 1011  		     in_amount		      => var_invoice_amount,
12:19:51 1012  		     in_created_by	      => in_created_by,
12:19:51 1013  		     in_order_id	      => in_order_id,
12:19:51 1014  		     in_transaction_type_code => 'GIFT_CERTIFICATE_PURCHASE',
12:19:51 1015  		     out_transaction_id       => var_transaction_id
12:19:51 1016  		 );
12:19:51 1017  		 EXCEPTION
12:19:51 1018  		 WHEN OTHERS THEN
12:19:51 1019  		 EXCEPTION_MESSAGE := SQLERRM;
12:19:51 1020  		 RAISE CAN_NOT_CREATE_TRANSACTION;
12:19:51 1021  	       END;
12:19:51 1022  
12:19:51 1023  -- Create charge
12:19:51 1024  	       BEGIN
12:19:51 1025  		 PROCS_CHARGE_V23.CREATE_CHARGE(
12:19:51 1026  		     in_invoice_id	   => in_invoice_id,
12:19:51 1027  		     in_transaction_id	   => var_transaction_id,
12:19:51 1028  		     in_instrument_type_id => in_instrument_type_id,
12:19:51 1029  		     in_instrument_id	   => in_instrument_id,
12:19:51 1030  		     in_charge_amount	   => var_invoice_amount,
12:19:51 1031  		     in_created_by	   => in_created_by,
12:19:51 1032  		     in_charge_status_id   => GLOBAL_STATUSES_V23.CHARGE_OPENED,
12:19:51 1033  		     out_charge_id	   => var_new_charge_id
12:19:51 1034  		 );
12:19:51 1035  		 out_charge_amount := var_invoice_amount;
12:19:51 1036  		 EXCEPTION
12:19:51 1037  		 WHEN OTHERS THEN
12:19:51 1038  		 EXCEPTION_MESSAGE := SQLERRM;
12:19:51 1039  		 RAISE CAN_NOT_CREATE_CHARGE;
12:19:51 1040  	       END;
12:19:51 1041  	     ELSE
12:19:51 1042  	       out_charge_amount := 0;
12:19:51 1043  	     END IF;
12:19:51 1044  
12:19:51 1045  	     EXCEPTION
12:19:51 1046  	     WHEN CAN_NOT_USE_FCINSTR THEN
12:19:51 1047  	     PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
12:19:51 1048  					      SPROC_NAME, 'Could not use "free charge instrument" for non-zero invoice');
12:19:51 1049  	     WHEN BAD_CREDIT_CARD_ID THEN
12:19:51 1050  	     PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 1051  					      SPROC_NAME, 'Bad credit card id');
12:19:51 1052  	     WHEN BAD_PAYPAL_ID THEN
12:19:51 1053  	     PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 1054  					      SPROC_NAME, 'Bad paypal id');
12:19:51 1055  	     WHEN BAD_INSTRUMENT_TYPE THEN
12:19:51 1056  	     PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INVALID_PARAMETER,
12:19:51 1057  					      SPROC_NAME, 'Bad instrument type');
12:19:51 1058  	     WHEN TRANSACTION_EXISTS THEN
12:19:51 1059  	     PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.DUPLICATE_ERROR,
12:19:51 1060  					      SPROC_NAME, 'Transaction with given id already exists');
12:19:51 1061  	     WHEN CAN_NOT_CREATE_TRANSACTION THEN
12:19:51 1062  	     PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 1063  					      SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
12:19:51 1064  	     WHEN CAN_NOT_CREATE_CHARGE THEN
12:19:51 1065  	     PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 1066  					      SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
12:19:51 1067  	     WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
12:19:51 1068  	     PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:51 1069  					      SPROC_NAME, 'Could not calculate amount for new invoice', EXCEPTION_MESSAGE);
12:19:51 1070  	     WHEN OTHERS THEN
12:19:51 1071  	     PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 1072  					      SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 1073  	   END FINALIZE_GC_PURCHASING;
