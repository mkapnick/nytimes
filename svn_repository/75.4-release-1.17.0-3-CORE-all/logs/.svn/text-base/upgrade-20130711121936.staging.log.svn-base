12:19:51 186  	IF var_is_for_redemption > 0 THEN
12:19:51 187  	  RAISE CAN_NOT_SUBSCRIBE_TO_RGC;
12:19:51 188  	END IF;
12:19:51 189  
12:19:51 190  	SELECT
12:19:51 191  	  COUNT(*) into var_same_offer_chains_num
12:19:51 192  	FROM
12:19:51 193  	  SUBSCRIPTION
12:19:51 194  	WHERE
12:19:51 195  	  SUBSCRIPTION.ACCOUNT_ID = var_account_id
12:19:51 196  	  AND SUBSCRIPTION.OFFER_CHAIN_ID = in_offer_chain_id
12:19:51 197  	  AND (SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V23.SUBSCRIPTION_ACTIVE
12:19:51 198  	       OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V23.SUBSCRIPTION_IN_GRACE_PERIOD);
12:19:51 199  
12:19:51 200  	-- ELIGIBILITY LOGIC CHANGED TO:
12:19:51 201  	-- FOR EACH offer chain eligibility rule in OC:
12:19:51 202  	--   IF offer chain eligibility rule fails:
12:19:51 203  	--     deny purchase;
12:19:51 204  	--   END IF
12:19:51 205  	-- END FOR
12:19:51 206  	-- FOR EACH product eligibility rule in OC:
12:19:51 207  	--   IF product eligibilty rule fails:
12:19:51 208  	--     deny purchase;
12:19:51 209  	--   END IF
12:19:51 210  	-- END FOR
12:19:51 211  	-- allow purchase;
12:19:51 212  
12:19:51 213  	-- if user have any active existing subscriptions to the offer chain
12:19:51 214  	-- and if MAX_CONCURRENT_SUBS <= [user's subscription count for the offer chain]
12:19:51 215  	-- then deny purchase
12:19:51 216  	var_max_concurrent_subscrs := PROCS_OFFER_CHAIN_V23.GET_OFFER_CHAIN_MAX_CONC_SUBSC(in_offer_chain_id);
12:19:51 217  	IF var_max_concurrent_subscrs != GLOBAL_CONSTANTS_V23.INFINITY
12:19:51 218  	  AND var_max_concurrent_subscrs <= var_same_offer_chains_num THEN
12:19:51 219  	  -- Find first concurrent subscription id:
12:19:51 220  	  SELECT
12:19:51 221  	    ID into var_concur_subscription_id
12:19:51 222  	  FROM (
12:19:51 223  	    SELECT
12:19:51 224  	      ID
12:19:51 225  	    FROM
12:19:51 226  	      SUBSCRIPTION
12:19:51 227  	    WHERE
12:19:51 228  	      SUBSCRIPTION.ACCOUNT_ID = var_account_id
12:19:51 229  	      AND SUBSCRIPTION.OFFER_CHAIN_ID = in_offer_chain_id
12:19:51 230  	      AND (SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V23.SUBSCRIPTION_ACTIVE
12:19:51 231  		   OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V23.SUBSCRIPTION_IN_GRACE_PERIOD)
12:19:51 232  	    ORDER BY
12:19:51 233  	      ID
12:19:51 234  	  )
12:19:51 235  	  WHERE
12:19:51 236  	    ROWNUM <= 1;
12:19:51 237  	  RAISE LIMIT_REACHED;
12:19:51 238  	END IF;
12:19:51 239  
12:19:51 240  	-- if user does not have any active existing subscriptions to the offer chain
12:19:51 241  	-- and if product from the offer chain is already owned from another offer chain
12:19:51 242  	-- then deny purchase
12:19:51 243  	IF (in_check_dupe_products != GLOBAL_CONSTANTS_V23.FALSE) THEN
12:19:51 244  	  FOR f_account_offer_chains IN (
12:19:51 245  	    SELECT DISTINCT
12:19:51 246  	      OFFER_CHAIN_ID
12:19:51 247  	    FROM
12:19:51 248  	      SUBSCRIPTION
12:19:51 249  	    WHERE
12:19:51 250  	      ACCOUNT_ID = var_account_id
12:19:51 251  	      AND (
12:19:51 252  		SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V23.SUBSCRIPTION_ACTIVE
12:19:51 253  		OR SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V23.SUBSCRIPTION_SUSPENDED
12:19:51 254  		OR SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V23.SUBSCRIPTION_IN_GRACE_PERIOD)
12:19:51 255  	  )
12:19:51 256  	  LOOP
12:19:51 257  	    IF PROCS_OFFER_CHAIN_V23.CHECK_FOR_SAME_PRODUCTS(
12:19:51 258  	      in_offer_chain_1	       => in_offer_chain_id,
12:19:51 259  	      in_offer_chain_2	       => f_account_offer_chains.OFFER_CHAIN_ID,
12:19:51 260  	      in_use_eligibility_rules => GLOBAL_CONSTANTS_V23.TRUE
12:19:51 261  	    ) = GLOBAL_CONSTANTS_V23.TRUE THEN
12:19:51 262  
12:19:51 263  	      -- Find first concurrent subscription id:
12:19:51 264  	      SELECT
12:19:51 265  		ID into var_concur_subscription_id
12:19:51 266  	      FROM (
12:19:51 267  		SELECT
12:19:51 268  		  ID
12:19:51 269  		FROM
12:19:51 270  		  SUBSCRIPTION
12:19:51 271  		WHERE
12:19:51 272  		  SUBSCRIPTION.ACCOUNT_ID = var_account_id
12:19:51 273  		  AND SUBSCRIPTION.OFFER_CHAIN_ID = f_account_offer_chains.OFFER_CHAIN_ID
12:19:51 274  		  AND (
12:19:51 275  		    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V23.SUBSCRIPTION_ACTIVE
12:19:51 276  		    OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V23.SUBSCRIPTION_SUSPENDED
12:19:51 277  		    OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V23.SUBSCRIPTION_IN_GRACE_PERIOD)
12:19:51 278  		ORDER BY
12:19:51 279  		  ID
12:19:51 280  	      )
12:19:51 281  	      WHERE
12:19:51 282  		ROWNUM <= 1;
12:19:51 283  
12:19:51 284  	      RAISE PRODUCT_ALREADY_PURCHASED;
12:19:51 285  	    END IF;
12:19:51 286  	  END LOOP;
12:19:51 287  	END IF;
12:19:51 288  
12:19:51 289  	BEGIN
12:19:51 290  	  PROCS_OFFER_CHAIN_V23.GET_FIRST_OFFER(in_offer_chain_id, var_first_offer_id);
12:19:51 291  	  EXCEPTION
12:19:51 292  	    WHEN OTHERS THEN
12:19:51 293  	      EXCEPTION_MESSAGE := SQLERRM;
12:19:51 294  	      RAISE CAN_NOT_GET_FIRST_OFFER_CHAIN;
12:19:51 295  	END;
12:19:51 296  
12:19:51 297  	BEGIN
