12:19:51 695  
12:19:51 696  PROCEDURE GET_ENDED_GC_LICENSES (
12:19:51 697  	out_result_set		OUT SYS_REFCURSOR,
12:19:51 698  	in_hours_number 	IN NUMBER DEFAULT 14*24,
12:19:51 699  	in_num_rows		IN NUMBER DEFAULT 10000,
12:19:51 700  	in_process_name IN PROCESS_RETRY_THROTTLE.PROCESS_NAME%TYPE
12:19:51 701  ) AS
12:19:51 702  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_ENDED_GC_INVOICES';
12:19:51 703  var_days		NUMBER;
12:19:51 704  var_hours 	NUMBER;
12:19:51 705  var_time_interval INTERVAL DAY (3) TO SECOND;
12:19:51 706  BEGIN
12:19:51 707  	var_hours := mod(in_hours_number,24);
12:19:51 708  	var_days := (in_hours_number - var_hours) / 24;
12:19:51 709  	var_time_interval := var_days||' '||var_hours||':0:0';
12:19:51 710  	OPEN out_result_set FOR
12:19:51 711  SELECT * FROM
12:19:51 712  (
12:19:51 713  	SELECT
12:19:51 714  	  l.ID,
12:19:51 715  	  l.CREATE_DATE,
12:19:51 716  	  l.CREATED_BY,
12:19:51 717  	  l.CURRENT_OFFER_INDEX,
12:19:51 718  	  l.CURRENT_OFFER_RECURR_NUM,
12:19:51 719  	  l.END_DATE,
