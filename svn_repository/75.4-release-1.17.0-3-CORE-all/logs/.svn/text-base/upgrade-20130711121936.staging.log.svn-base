12:19:52 804  PROCEDURE SHOULD_MOVE_TO_GRACE(
12:19:52 805  	in_invoice_id  IN INVOICE.ID%TYPE,
12:19:52 806  	out_result     OUT NUMBER
12:19:52 807  ) AS
12:19:52 808  BEGIN
12:19:52 809  	-- if the invoice preceding the given invoice has no transaction attempts, then
12:19:52 810  	-- it is not billed out of Sartre. if so, then the associated subscription
12:19:52 811  	-- should be canceled after a final failed billing attempt--not moved to grace.
12:19:52 812  	SELECT DECODE(COUNT(1), 0, 0, 1) INTO out_result
12:19:52 813  	FROM CHARGE c
12:19:52 814  	INNER JOIN TRANSACTION t ON c.TRANSACTION_ID = t.ID
12:19:52 815  	INNER JOIN TRANSACTION_ATTEMPT ta ON ta.TRANSACTION_ID = t.ID
12:19:52 816  	WHERE c.INVOICE_ID = (
12:19:52 817  	  -- select previous invoice_id, or -1 if there is none
12:19:52 818  	  SELECT PREV_INVOICE_ID FROM (
12:19:52 819  	    SELECT i.ID, LAG(i.ID, 1, -1) OVER (ORDER BY i.CREATE_DATE) AS PREV_INVOICE_ID
12:19:52 820  	    FROM INVOICE i
12:19:52 821  	    INNER JOIN LICENSE l ON i.ID = l.INVOICE_ID
12:19:52 822  	    WHERE l.SUBSCRIPTION_ID = (
12:19:52 823  	      SELECT SUBSCRIPTION_ID FROM LICENSE WHERE INVOICE_ID = in_invoice_id
12:19:52 824  	    )
12:19:52 825  	  ) WHERE ID = in_invoice_id
12:19:52 826  	);
12:19:52 827  END SHOULD_MOVE_TO_GRACE;
12:19:52 828  
12:19:52 829  /****************************************************************************/
