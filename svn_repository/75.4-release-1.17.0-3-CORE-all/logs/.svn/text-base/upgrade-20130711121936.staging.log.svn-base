12:19:51 595  
12:19:51 596  FUNCTION IS_INVOICE_PAYING_STARTED (
12:19:51 597  /*
12:19:51 598  Throws exceptions:
12:19:51 599  APP_EXCEPTION_CODES_V23.NOT_FOUND
12:19:51 600  APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR
12:19:51 601  */
12:19:51 602  	in_invoice_id IN NUMBER
12:19:51 603  ) RETURN NUMBER AS
12:19:51 604  -- VARIABLES
12:19:51 605  SPROC_NAME		 CONSTANT VARCHAR2(30) := 'IS_INVOICE_PAYING_STARTED';
12:19:51 606  temp_invoice_id		 NUMBER;
12:19:51 607  var_processed_charges_num  NUMBER;
12:19:51 608  var_processed_transac_num  NUMBER;
12:19:51 609  var_success_attempts_num	 NUMBER;
12:19:51 610  var_is_gc 		 NUMBER;
12:19:51 611  -- EXCEPTIONS
12:19:51 612  BAD_INVOICE_ID EXCEPTION;
12:19:51 613  BEGIN
12:19:51 614  
12:19:51 615  	-- Check that invoice exists
12:19:51 616  	BEGIN
12:19:51 617  	  SELECT
12:19:51 618  	    INVOICE.ID into temp_invoice_id
12:19:51 619  	  FROM
12:19:51 620  	    INVOICE
12:19:51 621  	  WHERE
12:19:51 622  	    INVOICE.ID = in_invoice_id;
12:19:51 623  	  EXCEPTION
12:19:51 624  	   WHEN NO_DATA_FOUND THEN
12:19:51 625  	     RAISE BAD_INVOICE_ID;
12:19:51 626  	END;
12:19:51 627  
12:19:51 628  	-- Check that there are tansaction attempts with status success
12:19:51 629  	SELECT
12:19:51 630  	  COUNT(1) into var_success_attempts_num
12:19:51 631  	FROM
12:19:51 632  	  TRANSACTION_ATTEMPT ta,
12:19:51 633  	  TRANSACTION t,
12:19:51 634  	  CHARGE c
12:19:51 635  	WHERE
12:19:51 636  	  ta.TRANSACTION_ATTEMPT_STATUS_ID = GLOBAL_STATUSES_V23.TRANS_ATTEMPT_SUCCESS and
12:19:51 637  	  ta.transaction_id = t.id and
12:19:51 638  	  c.transaction_id = t.id and
12:19:51 639  	  t.is_refund = GLOBAL_CONSTANTS_V23.FALSE and
12:19:51 640  	  c.invoice_id = in_invoice_id
12:19:51 641  	;
12:19:51 642  
12:19:51 643  	IF var_success_attempts_num > 0 THEN
12:19:51 644  	  RETURN GLOBAL_CONSTANTS_V23.TRUE;
12:19:51 645  	END IF;
12:19:51 646  
12:19:51 647  	SELECT
12:19:51 648  	  COUNT(1) into var_success_attempts_num
12:19:51 649  	FROM
12:19:51 650  	  TRANSACTION t,
12:19:51 651  	  CHARGE c
12:19:51 652  	WHERE
12:19:51 653  	  c.transaction_id = t.id and
12:19:51 654  	  t.is_refund = GLOBAL_CONSTANTS_V23.FALSE and
12:19:51 655  	  t.is_settled = GLOBAL_CONSTANTS_V23.TRUE and
12:19:51 656  	  c.invoice_id = in_invoice_id
12:19:51 657  	;
12:19:51 658  
12:19:51 659  	IF var_success_attempts_num > 0 THEN
12:19:51 660  	  RETURN GLOBAL_CONSTANTS_V23.TRUE;
12:19:51 661  	END IF;
12:19:51 662  
12:19:51 663  	SELECT
12:19:51 664  	  COUNT(1) into var_is_gc
12:19:51 665  	FROM
12:19:51 666  	  gift_certificate gc
12:19:51 667  	WHERE
12:19:51 668  	  gc.finalized_invoice_id = in_invoice_id
12:19:51 669  	;
12:19:51 670  
12:19:51 671  	IF var_is_gc > 0 THEN
12:19:51 672  	  RETURN GLOBAL_CONSTANTS_V23.TRUE;
12:19:51 673  	END IF;
12:19:51 674  
12:19:51 675  	RETURN GLOBAL_CONSTANTS_V23.FALSE;
12:19:51 676  
12:19:51 677  EXCEPTION
12:19:51 678  WHEN BAD_INVOICE_ID THEN
12:19:51 679  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 680  	  SPROC_NAME, 'No such invoice');
12:19:51 681  WHEN OTHERS THEN
12:19:51 682  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 683  	  SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 684  END IS_INVOICE_PAYING_STARTED;
