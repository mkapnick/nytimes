12:19:51 517  
12:19:51 518  PROCEDURE SUSPEND_SUBSCRIPTION(
12:19:51 519  /*
12:19:51 520  Throws exceptions:
12:19:51 521  APP_EXCEPTION_CODES_V23.NOT_FOUND
12:19:51 522  APP_EXCEPTION_CODES_V23.INTERNAL_ERROR
12:19:51 523  APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR
12:19:51 524  */
12:19:51 525  	  in_subs_id	IN NUMBER,
12:19:51 526  	  in_updated_by IN VARCHAR2
12:19:51 527  ) AS
12:19:51 528  SPROC_NAME		  CONSTANT VARCHAR2(20) := 'SUSPEND_SUBSCRIPTION';
12:19:51 529  var_subscription_status_id  NUMBER;
12:19:51 530  var_license_id		  NUMBER;
12:19:51 531  var_offer_id		  NUMBER;
12:19:51 532  var_license_start_date	  DATE;
12:19:51 533  var_license_end_date	  DATE;
12:19:51 534  
12:19:51 535  var_entitlement_dupration   VARCHAR2(30);
12:19:51 536  var_d_entitlement_dupration NUMBER;
12:19:51 537  
12:19:51 538  var_ym_interval INTERVAL YEAR TO MONTH;
12:19:51 539  var_ds_interval INTERVAL DAY(3) TO SECOND;
12:19:51 540  
12:19:51 541  -- EXCEPTIONS
12:19:51 542  BAD_SUBSCRIPTION_ID     EXCEPTION;
12:19:51 543  BAD_SUBSCRIPTION_STATUS EXCEPTION;
12:19:51 544  NO_LICENSE_FOUND	      EXCEPTION;
12:19:51 545  NO_OFFER_FOUND	      EXCEPTION;
12:19:51 546  EXCEPTION_MESSAGE       VARCHAR2(1024);
12:19:51 547  BEGIN
12:19:51 548  	-- TODO: Finish this prcedure (in Phase II)
12:19:51 549  
12:19:51 550  	-- Get subscription by id. FAULT if no such subscription.
12:19:51 551  	-- begin TX
12:19:51 552  	--   Get for update associated license (subscription.license_id). FAULT if not found.
12:19:51 553  	--   Set status to PROCESSED.
12:19:51 554  	--   updated record.
12:19:51 555  	--   compute days remaining in the subscription: original end_date - today = days_remaining_adjustment
12:19:51 556  	--   new subscription status is SUSPENDED.
12:19:51 557  	--   suspend_date is now.
12:19:51 558  	--   update subscription record.
12:19:51 559  	-- end TX
12:19:51 560  
12:19:51 561  	BEGIN
12:19:51 562  	  SELECT
12:19:51 563  	    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID into var_subscription_status_id
12:19:51 564  	  FROM
12:19:51 565  	    SUBSCRIPTION
12:19:51 566  	  WHERE
12:19:51 567  	    SUBSCRIPTION.ID = in_subs_id;
12:19:51 568  	  EXCEPTION
12:19:51 569  	  WHEN NO_DATA_FOUND THEN
12:19:51 570  	    RAISE BAD_SUBSCRIPTION_ID;
12:19:51 571  	END;
12:19:51 572  
12:19:51 573  	IF var_subscription_status_id != GLOBAL_STATUSES_V23.SUBSCRIPTION_ACTIVE THEN
12:19:51 574  	  RAISE BAD_SUBSCRIPTION_STATUS;
12:19:51 575  	END IF;
12:19:51 576  
12:19:51 577  	BEGIN
12:19:51 578  	  SELECT
12:19:51 579  	    LICENSE.ID,
12:19:51 580  	    LICENSE.OFFER_ID,
12:19:51 581  	    LICENSE.START_DATE
12:19:51 582  	    into
12:19:51 583  	    var_license_id,
12:19:51 584  	    var_offer_id,
12:19:51 585  	    var_license_start_date
12:19:51 586  	  FROM
12:19:51 587  	    LICENSE
12:19:51 588  	  WHERE
12:19:51 589  	    LICENSE.SUBSCRIPTION_ID = in_subs_id
12:19:51 590  	      AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V23.LICENSE_ACTIVE;
12:19:51 591  	  EXCEPTION
12:19:51 592  	  WHEN NO_DATA_FOUND THEN
12:19:51 593  	    RAISE NO_LICENSE_FOUND;
12:19:51 594  	END;
12:19:51 595  
12:19:51 596  	PROCS_LICENSE_CRU_V23.UPDATE_LICENSE(
12:19:51 597  	  in_license_id        => var_license_id,
12:19:51 598  	  in_updated_by        => in_updated_by,
12:19:51 599  	  in_license_status_id => GLOBAL_STATUSES_V23.LICENSE_CLOSED
12:19:51 600  	);
12:19:51 601  
12:19:51 602  	BEGIN
12:19:51 603  	  SELECT
12:19:51 604  	    OFFER.ENTITLEMENT_DURATION into var_entitlement_dupration
12:19:51 605  	  FROM
12:19:51 606  	    OFFER
12:19:51 607  	  WHERE
12:19:51 608  	    OFFER.ID = var_offer_id;
12:19:51 609  	  EXCEPTION
12:19:51 610  	  WHEN NO_DATA_FOUND THEN
12:19:51 611  	    RAISE NO_OFFER_FOUND;
12:19:51 612  	END;
12:19:51 613  
12:19:51 614  	var_ym_interval := substr(var_entitlement_dupration, 0, 4);
12:19:51 615  	var_ds_interval := substr(var_entitlement_dupration, 4);
12:19:51 616  
12:19:51 617  	var_license_end_date := var_license_start_date + var_ym_interval + var_ds_interval;
12:19:51 618  
12:19:51 619  	var_d_entitlement_dupration := var_license_end_date - current_date;
12:19:51 620  
12:19:51 621  	PROCS_SUBSCRIPTION_CRU_V23.UPDATE_SUBSCRIPTION(
12:19:51 622  	  in_subscription_id	       => in_subs_id,
12:19:51 623  	  in_updated_by 	       => in_updated_by,
12:19:51 624  	  in_suspend_date	       => SYSDATE,
12:19:51 625  	  in_subscription_status_id    => GLOBAL_STATUSES_V23.SUBSCRIPTION_SUSPENDED,
12:19:51 626  	  in_days_remainning_ajustment => var_d_entitlement_dupration
12:19:51 627  	);
12:19:51 628  
12:19:51 629  EXCEPTION
12:19:51 630  WHEN BAD_SUBSCRIPTION_ID THEN
12:19:51 631  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 632  	  SPROC_NAME, 'No such subscription');
12:19:51 633  WHEN BAD_SUBSCRIPTION_STATUS THEN
12:19:51 634  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
12:19:51 635  	  SPROC_NAME, 'Subscription is not active');
12:19:51 636  WHEN NO_LICENSE_FOUND THEN
12:19:51 637  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 638  	  SPROC_NAME, 'Can not find license associated with given subscription ID');
12:19:51 639  WHEN NO_OFFER_FOUND THEN
12:19:51 640  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:51 641  	  SPROC_NAME, 'Can not find offer associated with given subscription ID');
12:19:51 642  WHEN OTHERS THEN
12:19:51 643  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:51 644  	  SPROC_NAME, 'Unknown error', SQLERRM);
12:19:51 645  END SUSPEND_SUBSCRIPTION;
