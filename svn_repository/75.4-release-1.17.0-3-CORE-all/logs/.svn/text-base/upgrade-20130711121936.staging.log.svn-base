12:19:52 759  PROCEDURE GET_ACCOUNT_SUBSCR_INVOICES (
12:19:52 760  	in_group_id	   IN  NUMBER,
12:19:52 761  	in_subscription_id IN NUMBER,
12:19:52 762  	out_result_set	   OUT SYS_REFCURSOR
12:19:52 763  ) AS
12:19:52 764  SPROC_NAME	   CONSTANT VARCHAR2(27) := 'GET_ACCOUNT_SUBSCR_INVOICES';
12:19:52 765  -- VARIABLES
12:19:52 766  var_account_id	   NUMBER;
12:19:52 767  temp_subscription_id NUMBER;
12:19:52 768  -- EXCEPTIONS
12:19:52 769  BAD_GROUP_ID	  EXCEPTION;
12:19:52 770  BAD_SUBSCRIPTION_ID EXCEPTION;
12:19:52 771  BEGIN
12:19:52 772  	-- Get account id
12:19:52 773  	BEGIN
12:19:52 774  	  SELECT
12:19:52 775  	    ACCOUNT.ID into var_account_id
12:19:52 776  	  FROM
12:19:52 777  	    ACCOUNT
12:19:52 778  	  WHERE
12:19:52 779  	    ACCOUNT.GROUP_ID = in_group_id;
12:19:52 780  	  EXCEPTION
12:19:52 781  	    WHEN NO_DATA_FOUND THEN
12:19:52 782  	      RAISE BAD_GROUP_ID;
12:19:52 783  	END;
12:19:52 784  
12:19:52 785  	-- Check that subscription exists
12:19:52 786  	BEGIN
12:19:52 787  	  IF in_subscription_id IS NOT NULL THEN
12:19:52 788  	    SELECT
12:19:52 789  	      SUBSCRIPTION.ID into temp_subscription_id
12:19:52 790  	    FROM
12:19:52 791  	      SUBSCRIPTION
12:19:52 792  	    WHERE
12:19:52 793  	      SUBSCRIPTION.ID = in_subscription_id;
12:19:52 794  	  END IF;
12:19:52 795  	  EXCEPTION
12:19:52 796  	    WHEN NO_DATA_FOUND THEN
12:19:52 797  	      RAISE BAD_SUBSCRIPTION_ID;
12:19:52 798  	END;
12:19:52 799  
12:19:52 800  	OPEN out_result_set FOR
12:19:52 801  	SELECT DISTINCT
12:19:52 802  	  INVOICE.ID as "INVOICE_ID",
12:19:52 803  	  INVOICE.CREATE_DATE,
12:19:52 804  	  INVOICE.INVOICE_STATUS_ID,
12:19:52 805  	  PROCS_INVOICE_V23.F_CALCULATE_INVOICE_AMOUNT(INVOICE.ID) as "AMOUNT",
12:19:52 806  	  OFFER_CHAIN.ID as "OFFER_CHAIN_ID",
12:19:52 807  	  OFFER_CHAIN.NAME as "OFFER_CHAIN_NAME",
12:19:52 808  	  SUBSCRIPTION.ID as "SUBSCRIPTION_ID",
12:19:52 809  	  NULL as "GC_CODE",
12:19:52 810  	  NULL as "GC_ID"
12:19:52 811  	FROM
12:19:52 812  	  LICENSE
12:19:52 813  	  INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
12:19:52 814  	  INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
12:19:52 815  	  INNER JOIN OFFER_CHAIN ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
12:19:52 816  	WHERE
12:19:52 817  	  SUBSCRIPTION.ACCOUNT_ID = var_account_id
12:19:52 818  	  AND (SUBSCRIPTION.SCT_ID IS NULL OR SUBSCRIPTION.SCT_ID != GLOBAL_STATUSES_V23.REAL_TIME_CANCEL_REASON)
12:19:52 819  	  AND SUBSCRIPTION.ID = NVL(in_subscription_id, SUBSCRIPTION.ID);
12:19:52 820  
12:19:52 821  EXCEPTION
12:19:52 822  WHEN BAD_GROUP_ID THEN
12:19:52 823  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:52 824  	  SPROC_NAME, 'No such account');
12:19:52 825  WHEN BAD_SUBSCRIPTION_ID THEN
12:19:52 826  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:52 827  	  SPROC_NAME, 'No such subscription');
12:19:52 828  WHEN OTHERS THEN
12:19:52 829  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:52 830  	  SPROC_NAME, 'Unknown error', SQLERRM);
12:19:52 831  END GET_ACCOUNT_SUBSCR_INVOICES;
