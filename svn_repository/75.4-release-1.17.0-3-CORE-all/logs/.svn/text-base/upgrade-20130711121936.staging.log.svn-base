12:19:51 702  /******************************************************************************/
12:19:51 703  
12:19:51 704  	PROCEDURE START_GC_PURCHASING (
12:19:51 705  	  in_group_id		    IN NUMBER,
12:19:51 706  	  in_offer_chain_id	    IN VARCHAR2,
12:19:51 707  	  in_gift_certificate_code  IN	VARCHAR2,
12:19:51 708  	  in_created_by 	    IN	VARCHAR2,
12:19:51 709  	  in_recipient_name	    IN	VARCHAR2,
12:19:51 710  	  in_recipient_email	    IN	VARCHAR2,
12:19:51 711  	  in_recipient_address_id   IN NUMBER,
12:19:51 712  	  in_recipient_notify_date  IN DATE,
12:19:51 713  	  in_sender_name	    IN VARCHAR2,
12:19:51 714  	  in_sender_email	    IN VARCHAR2,
12:19:51 715  	  in_gift_message	    IN	VARCHAR2,
12:19:51 716  	  in_expiration_date	    IN DATE,
12:19:51 717  	  in_campaign		    IN VARCHAR2,
12:19:51 718  	  in_reference_code	    IN VARCHAR2,
12:19:51 719  	  out_gift_certificate_id   OUT NUMBER,
12:19:51 720  	  out_invoice_id	    OUT NUMBER
12:19:51 721  	) AS
12:19:51 722  	  SPROC_NAME CONSTANT VARCHAR2(19) := 'START_GC_PURCHASING';
12:19:51 723  -- VARIABLES
12:19:51 724  	  var_account_id	    NUMBER;
12:19:51 725  	  temp_gc_code		    GIFT_CERTIFICATE.CODE%TYPE;
12:19:51 726  	  var_och_is_gc 	    NUMBER;
12:19:51 727  	  var_offer_chain_status_id NUMBER;
12:19:51 728  	  var_is_for_redemption     NUMBER;
12:19:51 729  	  var_new_invoice_id	    NUMBER;
12:19:51 730  	  var_gift_cert_id	    NUMBER;
12:19:51 731  	  var_account_tax_exempt_id VARCHAR2(255);
12:19:51 732  -- EXCEPTIONS
12:19:51 733  	    BAD_GROUP_ID		  EXCEPTION;
12:19:51 734  	    GC_CODE_ALREADY_EXISTS	  EXCEPTION;
12:19:51 735  	    BAD_OFFER_CHAIN_ID		  EXCEPTION;
12:19:51 736  	    OCH_IS_NOT_GIFT_CERTIFICATE   EXCEPTION;
12:19:51 737  	    BAD_OFFER_CHAIN_STATUS	  EXCEPTION;
12:19:51 738  	    CAN_NOT_PURCHASE_GC_FOR_RDMPN EXCEPTION;
12:19:51 739  	    CAN_NOT_CREATE_INVOICE	  EXCEPTION;
12:19:51 740  	    OFFER_REC_NUM_LESS_THAN_ONE   EXCEPTION;
12:19:51 741  	    CAN_NOT_CREATE_LINE_ITEMS	  EXCEPTION;
12:19:51 742  	    REF_CODE_ALREADY_EXISTS	  EXCEPTION;
12:19:51 743  
12:19:51 744  	  EXCEPTION_MESSAGE VARCHAR2(1024);
12:19:51 745  	  BEGIN
12:19:51 746  -- Get account id
12:19:51 747  	    BEGIN
12:19:51 748  	      SELECT
12:19:51 749  		ACCOUNT.ID,
12:19:51 750  		ACCOUNT.TAX_EXEMPT_ID
12:19:51 751  	      into
12:19:51 752  		var_account_id,
12:19:51 753  		var_account_tax_exempt_id
12:19:51 754  	      FROM
12:19:51 755  		ACCOUNT
12:19:51 756  	      WHERE
12:19:51 757  		ACCOUNT.GROUP_ID = in_group_id;
12:19:51 758  	      EXCEPTION
12:19:51 759  	      WHEN NO_DATA_FOUND THEN
12:19:51 760  	      RAISE BAD_GROUP_ID;
12:19:51 761  	    END;
12:19:51 762  
12:19:51 763  -- Check for the same code
12:19:51 764  	    BEGIN
12:19:51 765  	      SELECT
12:19:51 766  		GIFT_CERTIFICATE.CODE into temp_gc_code
12:19:51 767  	      FROM
12:19:51 768  		GIFT_CERTIFICATE
12:19:51 769  	      WHERE
12:19:51 770  		GIFT_CERTIFICATE.CODE = in_gift_certificate_code;
12:19:51 771  
12:19:51 772  	      RAISE GC_CODE_ALREADY_EXISTS;
12:19:51 773  
12:19:51 774  	      EXCEPTION
12:19:51 775  	      WHEN NO_DATA_FOUND THEN
12:19:51 776  	      NULL;
12:19:51 777  	    END;
12:19:51 778  
12:19:51 779  -- Get offer chain flag "is_gift_certificate"
12:19:51 780  	    BEGIN
12:19:51 781  	      SELECT
12:19:51 782  		OFFER_CHAIN.IS_GIFT_CERTIFICATE,
12:19:51 783  		OFFER_CHAIN.OFFER_CHAIN_STATUS_ID
12:19:51 784  	      into
12:19:51 785  		var_och_is_gc,
12:19:51 786  		var_offer_chain_status_id
12:19:51 787  	      FROM
12:19:51 788  		OFFER_CHAIN
12:19:51 789  	      WHERE
12:19:51 790  		OFFER_CHAIN.ID = in_offer_chain_id;
12:19:51 791  	      EXCEPTION
12:19:51 792  	      WHEN NO_DATA_FOUND THEN
12:19:51 793  	      RAISE BAD_OFFER_CHAIN_ID;
12:19:51 794  	    END;
12:19:51 795  
12:19:51 796  	    IF var_och_is_gc != GLOBAL_CONSTANTS_V23.TRUE
12:19:51 797  	       OR var_och_is_gc IS NULL THEN
12:19:51 798  	      RAISE OCH_IS_NOT_GIFT_CERTIFICATE;
12:19:51 799  	    END IF;
12:19:51 800  
12:19:51 801  	    IF var_offer_chain_status_id != GLOBAL_STATUSES_V23.OFFER_CHAIN_ACTIVE THEN
12:19:51 802  	      RAISE BAD_OFFER_CHAIN_STATUS;
12:19:51 803  	    END IF;
12:19:51 804  
12:19:51 805  -- norlov: #38151 check if the OC is for Redemption:
12:19:51 806  	    SELECT
12:19:51 807  	      COUNT(*) into var_is_for_redemption
12:19:51 808  	    FROM
12:19:51 809  	      OFFER_CHAIN_ELIGIBILITY
12:19:51 810  	    WHERE
12:19:51 811  	      OFFER_CHAIN_ELIGIBILITY.OFFER_CHAIN_ID = in_offer_chain_id
12:19:51 812  	      AND OFFER_CHAIN_ELIGIBILITY.NAME = GLOBAL_CONSTANTS_V23.GIFT_CERTIFICATE_REQUIRED
12:19:51 813  	      AND OFFER_CHAIN_ELIGIBILITY.VALUE = GLOBAL_CONSTANTS_V23.ELIGIBILITY_FLAG_SET;
12:19:51 814  
