12:19:51 120  	  update
12:19:51 121  	    core_owner.polling_sync ps
12:19:51 122  	  set
12:19:51 123  	    ps.set_id = v_set_id
12:19:51 124  	  where
12:19:51 125  	    ps.set_id IS NULL and
12:19:51 126  	    rownum <= in_set_maximum
12:19:51 127  	  ;
12:19:51 128  	  COMMIT;
12:19:51 129  	  RETURN v_set_id;
12:19:51 130  END;
12:19:51 131  --------------------------------------------------------------------------------
12:19:51 132  FUNCTION RETREIVE_TRANSFER_SET
12:19:51 133  	  (
12:19:51 134  	      in_set_id core_owner.polling_sync.set_id%type)
12:19:51 135  	  RETURN sys_refcursor
12:19:51 136  IS
12:19:51 137  	  v_refcursor sys_refcursor;
12:19:51 138  BEGIN
12:19:51 139  	  UPDATE
12:19:51 140  	      core_owner.polling_sync ps
12:19:51 141  	  SET
12:19:51 142  	      ps.last_send_date = sysdate,
12:19:51 143  	      ps.num_calls = ps.num_calls + 1
12:19:51 144  	  WHERE
12:19:51 145  	      ps.set_id = in_set_id ;
12:19:51 146  	  OPEN v_refcursor FOR
12:19:51 147  	  SELECT
12:19:51 148  	      ps.set_id,
12:19:51 149  	      ps.group_id,
12:19:51 150  	      ps.event_type,
12:19:51 151  	      ps.event_date
12:19:51 152  	  FROM
12:19:51 153  	      core_owner.polling_sync ps
12:19:51 154  	  WHERE
12:19:51 155  	      ps.set_id = in_set_id
12:19:51 156  	  AND ps.group_id IS NOT NULL ;
12:19:51 157  	  RETURN v_refcursor;
12:19:51 158  END;
12:19:51 159  --------------------------------------------------------------------------------
12:19:51 160  PROCEDURE GET_TRANSFER_SET
12:19:51 161  	  (
12:19:51 162  	      in_set_maximum NUMBER,
12:19:51 163  	      in_max_retries NUMBER,
12:19:51 164  	      out_refcursor OUT sys_refcursor)
12:19:51 165  IS
12:19:51 166  	  v_set_id core_owner.polling_sync.set_id%type;
12:19:51 167  BEGIN
12:19:51 168  	  /* Look for previously sent but unconfirmed sets and
12:19:51 169  	  send again until max_retries calls */
12:19:51 170  	  FOR x IN
12:19:51 171  	  (
12:19:51 172  	      SELECT
12:19:51 173  		  ps.set_id,
12:19:51 174  		  COUNT( *) set_size
12:19:51 175  	      FROM
12:19:51 176  		  core_owner.polling_sync ps
12:19:51 177  	      WHERE
12:19:51 178  		  ps.event_date > sysdate - 14
12:19:51 179  	      AND ps.confirm_date IS NULL
12:19:51 180  	      AND ps.last_send_date IS NOT NULL
12:19:51 181  	      AND ps.num_calls < in_max_retries
12:19:51 182  	      GROUP BY
12:19:51 183  		  ps.set_id
12:19:51 184  	      ORDER BY
12:19:51 185  		  ps.set_id
12:19:51 186  	  )
12:19:51 187  	  LOOP
12:19:51 188  	      out_refcursor := Retreive_Transfer_Set(x.set_id) ;
12:19:51 189  	      RETURN;
12:19:51 190  	  END LOOP;
12:19:51 191  	  v_set_id := Create_New_Transfer_Set(in_set_maximum) ;
12:19:51 192  	  out_refcursor := Retreive_Transfer_Set(v_set_id) ;
12:19:51 193  	  RETURN;
12:19:51 194  END;
12:19:51 195  --------------------------------------------------------------------------------
12:19:51 196  PROCEDURE CONFIRM_TRANSFER_SET
12:19:51 197  	  (
12:19:51 198  	      in_set_id core_owner.polling_sync.set_id%type)
12:19:51 199  IS
12:19:51 200  	  v_unconfirmable EXCEPTION;
12:19:51 201  BEGIN
12:19:51 202  	  UPDATE
12:19:51 203  	      core_owner.polling_sync ps
12:19:51 204  	  SET
12:19:51 205  	      ps.confirm_date = systimestamp
12:19:51 206  	  WHERE
12:19:51 207  	      ps.set_id = in_set_id
12:19:51 208  	  AND ps.confirm_date IS NULL ;
12:19:51 209  	  IF(sql%rowcount < 1) THEN
12:19:51 210  	      raise v_unconfirmable;
12:19:51 211  	  END IF;
12:19:51 212  END;
12:19:51 213  PROCEDURE SET_LAST_RUN(ts in timestamp)
12:19:51 214  IS
12:19:51 215  BEGIN
12:19:51 216  	  UPDATE POLLING_SYNC_LASTRUN
12:19:51 217  	  SET last_run = current_timestamp;
12:19:51 218  	  IF ( sql%rowcount = 0 )
12:19:51 219  	  THEN
12:19:51 220  	    INSERT INTO POLLING_SYNC_LASTRUN VALUES (ts);
12:19:51 221  	  END if;
12:19:51 222  	  COMMIT;
12:19:51 223  END;
12:19:51 224  PROCEDURE GET_LAST_RUN(ts out timestamp)
12:19:51 225  IS
12:19:51 226  BEGIN
12:19:51 227  	  SELECT LAST_RUN INTO ts
12:19:51 228  	  FROM POLLING_SYNC_LASTRUN
12:19:51 229  	  WHERE ROWNUM < 2;
12:19:51 230  EXCEPTION
12:19:51 231  	WHEN NO_DATA_FOUND
12:19:51 232  	THEN
12:19:51 233  	  ts := current_timestamp;
12:19:51 234  END;
12:19:51 235  END PROCS_POLLING_SYNC;
