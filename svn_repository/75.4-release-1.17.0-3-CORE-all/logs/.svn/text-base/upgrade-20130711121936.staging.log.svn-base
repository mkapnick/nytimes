12:19:51  88  	PROCEDURE CHASE_PROFILE_BY_REQ_FILE_ID(
12:19:51  89  	  in_request_file_id IN CC_UPDATE.CC_REQUEST_FILE_ID%TYPE,
12:19:51  90  	  in_start	     IN NUMBER,
12:19:51  91  	  in_end	     IN NUMBER,
12:19:51  92  	  out_result_set     OUT SYS_REFCURSOR
12:19:51  93  	) AS
12:19:51  94  	SPROC_NAME CONSTANT VARCHAR2(32) := 'CHASE_PROFILE_BY_REQ_FILE_ID';
12:19:51  95  	var_range_diff	    NUMBER := 0;
12:19:51  96  	var_upper_bond_diff NUMBER := 0;
12:19:51  97  	var_l_start	    NUMBER := 0;
12:19:51  98  	var_l_end	    NUMBER := 0;
12:19:51  99  	BEGIN
12:19:51 100  	  --Normalize the end points [START]
12:19:51 101  	  IF (in_start IS NULL OR in_start < 0) Then
12:19:51 102  	    var_l_start := 0;
12:19:51 103  	  ELSE
12:19:51 104  	    var_l_start := in_start;
12:19:51 105  	  END IF;
12:19:51 106  
12:19:51 107  	  IF (in_end IS NULL) Then
12:19:51 108  	    var_l_end := 500;
12:19:51 109  	  ELSE
12:19:51 110  	    var_l_end := in_end;
12:19:51 111  	  END IF;
12:19:51 112  
12:19:51 113  	  var_l_start := var_l_start + 1;
12:19:51 114  	  var_l_end   := var_l_end   + 1;
12:19:51 115  
12:19:51 116  	  var_range_diff  := var_l_end - var_l_start;
12:19:51 117  	  var_upper_bond_diff :=  var_range_diff - 1000;
12:19:51 118  
12:19:51 119  	  IF (var_upper_bond_diff > 0) Then
12:19:51 120  	    var_l_end := var_l_end - var_upper_bond_diff;
12:19:51 121  	  END IF;
12:19:51 122  	  --Normalize the end points [END]
12:19:51 123  
12:19:51 124  	  OPEN out_result_set FOR
12:19:51 125  	    SELECT CHASE_PROFILE_ID FROM
12:19:51 126  	      (SELECT rownum rnum, q.* FROM
12:19:51 127  		 (SELECT
12:19:51 128  		    cc.CHASE_PROFILE_ID
12:19:51 129  		  FROM
12:19:51 130  		    CREDIT_CARD cc,
12:19:51 131  		    CC_UPDATE ccu
12:19:51 132  		  WHERE
12:19:51 133  		    ccu.CC_REQUEST_FILE_ID = in_request_file_id
12:19:51 134  		    AND ccu.CREDIT_CARD_ID = cc.id
12:19:51 135  		) Q
12:19:51 136  	      WHERE rownum <= var_l_end)
12:19:51 137  	    WHERE rnum >= var_l_Start;
12:19:51 138  	END CHASE_PROFILE_BY_REQ_FILE_ID;
