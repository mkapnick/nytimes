12:19:52 683  PROCEDURE FREEZE_ACCOUNT (
12:19:52 684  	in_group_id   IN NUMBER,
12:19:52 685  	in_updated_by IN VARCHAR2,
12:19:52 686  	in_note       IN VARCHAR2,
12:19:52 687  	in_agent_id   IN NUMBER
12:19:52 688  ) AS
12:19:52 689  SPROC_NAME	    CONSTANT VARCHAR2(14) := 'FREEZE_ACCOUNT';
12:19:52 690  -- VARIABLES
12:19:52 691  var_account_id	    NUMBER;
12:19:52 692  var_account_status_id NUMBER;
12:19:52 693  -- EXCEPTIONS
12:19:52 694  BAD_GROUP_ID	    EXCEPTION;
12:19:52 695  BAD_ACCOUNT_STATUS_ID EXCEPTION;
12:19:52 696  CAN_NOT_CREATE_NOTE   EXCEPTION;
12:19:52 697  EXCEPTION_MESSAGE     VARCHAR2(1024);
12:19:52 698  BEGIN
12:19:52 699  
12:19:52 700  	-- Get account status, account id
12:19:52 701  	BEGIN
12:19:52 702  	  SELECT
12:19:52 703  	    ACCOUNT.ID,
12:19:52 704  	    ACCOUNT.ACCOUNT_STATUS_ID
12:19:52 705  	    into
12:19:52 706  	    var_account_id,
12:19:52 707  	    var_account_status_id
12:19:52 708  	  FROM
12:19:52 709  	    ACCOUNT
12:19:52 710  	  WHERE
12:19:52 711  	    ACCOUNT.GROUP_ID = in_group_id;
12:19:52 712  	  EXCEPTION
12:19:52 713  	    WHEN NO_DATA_FOUND THEN
12:19:52 714  	      RAISE BAD_GROUP_ID;
12:19:52 715  	END;
12:19:52 716  
12:19:52 717  	-- We can freeze only ACTIVE accounts
12:19:52 718  	IF var_account_status_id != GLOBAL_STATUSES_V23.ACCOUNT_ACTIVE
12:19:52 719  	  AND var_account_status_id != GLOBAL_STATUSES_V23.ACCOUNT_FROZEN THEN
12:19:52 720  	  RAISE BAD_ACCOUNT_STATUS_ID;
12:19:52 721  	END IF;
12:19:52 722  
12:19:52 723  	-- Set account status
12:19:52 724  	PROCS_ACCOUNT_V23.UPDATE_ACCOUNT_STATUS(
12:19:52 725  	  in_account_id        => var_account_id,
12:19:52 726  	  in_updated_by        => in_updated_by,
12:19:52 727  	  in_account_status_id => GLOBAL_STATUSES_V23.ACCOUNT_FROZEN
12:19:52 728  	);
12:19:52 729  
12:19:52 730  	-- Annotate account
12:19:52 731  	BEGIN
12:19:52 732  	  PROCS_ACCOUNT_V23.ANNOTATE_ACCOUNT(
12:19:52 733  	    in_group_id   => in_group_id,
12:19:52 734  	    in_agent_id   => in_agent_id,
12:19:52 735  	    in_note	  => in_note,
12:19:52 736  	    in_created_by => in_updated_by
12:19:52 737  	  );
12:19:52 738  	  EXCEPTION
12:19:52 739  	    WHEN OTHERS THEN
12:19:52 740  	      EXCEPTION_MESSAGE := SQLERRM;
12:19:52 741  	      RAISE CAN_NOT_CREATE_NOTE;
12:19:52 742  	END;
12:19:52 743  
12:19:52 744  EXCEPTION
12:19:52 745  WHEN BAD_GROUP_ID THEN
12:19:52 746  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:52 747  	  SPROC_NAME, 'No such group id');
12:19:52 748  WHEN BAD_ACCOUNT_STATUS_ID THEN
12:19:52 749  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
12:19:52 750  	  SPROC_NAME, 'Could not update this account. Status should to be active or frozen');
12:19:52 751  WHEN CAN_NOT_CREATE_NOTE THEN
12:19:52 752  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:52 753  	  SPROC_NAME, 'Could not annotate account', EXCEPTION_MESSAGE);
12:19:52 754  WHEN OTHERS THEN
12:19:52 755  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:52 756  	  SPROC_NAME, 'Unknown error', SQLERRM);
12:19:52 757  END FREEZE_ACCOUNT;
