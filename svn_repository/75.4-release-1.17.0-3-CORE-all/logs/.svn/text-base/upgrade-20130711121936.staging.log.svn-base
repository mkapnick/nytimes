12:19:51 166  temp_invoice_id	     NUMBER;
12:19:51 167  var_total_amount	     NUMBER(10,6);
12:19:51 168  var_final_amount	     NUMBER(10,2);
12:19:51 169  var_line_item_amount   NUMBER(10,6);
