12:19:52 275  PROCEDURE REACTIVATE_ACCOUNT (
12:19:52 276  	in_group_id   IN NUMBER,
12:19:52 277  	in_updated_by IN VARCHAR2,
12:19:52 278  	in_note       IN VARCHAR2,
12:19:52 279  	in_agent_id   IN NUMBER
12:19:52 280  ) AS
12:19:52 281  -- VARIABLES
12:19:52 282  SPROC_NAME	      CONSTANT VARCHAR2(18) := 'REACTIVATE_ACCOUNT';
12:19:52 283  var_account_id	      NUMBER;
12:19:52 284  current_account_status  NUMBER;
12:19:52 285  
12:19:52 286  -- EXCEPTIONS
12:19:52 287  BAD_CURRENT_ACC_STATUS EXCEPTION;
12:19:52 288  CAN_NOT_CREATE_NOTE    EXCEPTION;
12:19:52 289  EXCEPTION_MESSAGE      VARCHAR2(1024);
12:19:52 290  BEGIN
12:19:52 291  	-- Get account id, status
12:19:52 292  	SELECT
12:19:52 293  	  ACCOUNT.ACCOUNT_STATUS_ID,
12:19:52 294  	  ACCOUNT.ID
12:19:52 295  	INTO
12:19:52 296  	  current_account_status,
12:19:52 297  	  var_account_id
12:19:52 298  	FROM ACCOUNT
12:19:52 299  	WHERE
12:19:52 300  	  ACCOUNT.GROUP_ID = in_group_id;
12:19:52 301  
12:19:52 302  	IF current_account_status != GLOBAL_STATUSES_V23.ACCOUNT_FROZEN THEN
12:19:52 303  	  RAISE BAD_CURRENT_ACC_STATUS;
12:19:52 304  	END IF;
12:19:52 305  
12:19:52 306  	-- Change account status
12:19:52 307  	PROCS_ACCOUNT_V23.UPDATE_ACCOUNT_STATUS(
12:19:52 308  	  in_account_id        => var_account_id,
12:19:52 309  	  in_updated_by        => in_updated_by,
12:19:52 310  	  in_account_status_id => GLOBAL_STATUSES_V23.ACCOUNT_ACTIVE
12:19:52 311  	);
12:19:52 312  
12:19:52 313  	-- Add note
12:19:52 314  	BEGIN
12:19:52 315  	  PROCS_ACCOUNT_V23.ANNOTATE_ACCOUNT(
12:19:52 316  	    in_group_id   => in_group_id,
12:19:52 317  	    in_agent_id   => in_agent_id,
12:19:52 318  	    in_note	  => in_note,
12:19:52 319  	    in_created_by => in_updated_by
12:19:52 320  	  );
12:19:52 321  	  EXCEPTION
12:19:52 322  	    WHEN OTHERS THEN
12:19:52 323  	      EXCEPTION_MESSAGE := SQLERRM;
12:19:52 324  	      RAISE CAN_NOT_CREATE_NOTE;
12:19:52 325  	END;
12:19:52 326  
12:19:52 327  EXCEPTION
12:19:52 328  WHEN BAD_CURRENT_ACC_STATUS THEN
12:19:52 329  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
12:19:52 330  	  SPROC_NAME, 'Current account status is not "frozen"');
12:19:52 331  WHEN NO_DATA_FOUND THEN
12:19:52 332  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:52 333  	  SPROC_NAME, 'Could not find account with given group ID');
12:19:52 334  WHEN CAN_NOT_CREATE_NOTE THEN
12:19:52 335  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
12:19:52 336  	  SPROC_NAME, 'Could not annotate account');
12:19:52 337  WHEN OTHERS THEN
12:19:52 338  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:52 339  	  SPROC_NAME, 'Unknown error', SQLERRM);
12:19:52 340  END REACTIVATE_ACCOUNT;
