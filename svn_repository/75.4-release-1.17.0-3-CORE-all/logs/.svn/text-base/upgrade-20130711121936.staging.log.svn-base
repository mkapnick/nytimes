12:19:52 1216  -- VARIABLES
12:19:52 1217  temp_invoice_id		 NUMBER;
12:19:52 1218  var_subscrib_group_id	 NUMBER;
12:19:52 1219  var_gc_purchaser_group_id NUMBER;
12:19:52 1220  var_gc_redeemer_group_id  NUMBER;
12:19:52 1221  -- EXCEPTIONS
12:19:52 1222  BAD_INVOICE_ID	    EXCEPTION;
12:19:52 1223  CAN_NOT_FIND_ACCOUNT EXCEPTION;
12:19:52 1224  BEGIN
12:19:52 1225  
12:19:52 1226  	 BEGIN
12:19:52 1227  	   SELECT
12:19:52 1228  	     INVOICE.ID into temp_invoice_id
12:19:52 1229  	   FROM
12:19:52 1230  	     INVOICE
12:19:52 1231  	   WHERE
12:19:52 1232  	     INVOICE.ID = in_invoice_id;
12:19:52 1233  	   EXCEPTION
12:19:52 1234  	     WHEN NO_DATA_FOUND THEN
12:19:52 1235  	       RAISE BAD_INVOICE_ID;
12:19:52 1236  	 END;
12:19:52 1237  
12:19:52 1238  	 BEGIN
12:19:52 1239  	   SELECT DISTINCT
12:19:52 1240  	     ACCOUNT.GROUP_ID into var_subscrib_group_id
12:19:52 1241  	   FROM
12:19:52 1242  	     ACCOUNT
12:19:52 1243  	     INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.ACCOUNT_ID = ACCOUNT.ID
12:19:52 1244  	     INNER JOIN LICENSE ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
12:19:52 1245  	   WHERE
12:19:52 1246  	     LICENSE.INVOICE_ID = in_invoice_id;
12:19:52 1247  	   EXCEPTION
12:19:52 1248  	     WHEN NO_DATA_FOUND THEN
12:19:52 1249  	       var_subscrib_group_id := NULL;
12:19:52 1250  	 END;
12:19:52 1251  
12:19:52 1252  	 IF var_subscrib_group_id IS NULL THEN
12:19:52 1253  	   BEGIN
12:19:52 1254  	     SELECT
12:19:52 1255  	       GIFT_CERTIFICATE.PURCHASER_GROUP_ID,
12:19:52 1256  	       GIFT_CERTIFICATE.REDEEMER_GROUP_ID
12:19:52 1257  	       into
12:19:52 1258  	       var_gc_purchaser_group_id,
12:19:52 1259  	       var_gc_redeemer_group_id
12:19:52 1260  	     FROM
12:19:52 1261  	       GIFT_CERTIFICATE
12:19:52 1262  	     WHERE
12:19:52 1263  	       GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = in_invoice_id
12:19:52 1264  	       OR GIFT_CERTIFICATE.FINALIZED_INVOICE_ID = in_invoice_id;
12:19:52 1265  	     EXCEPTION
12:19:52 1266  	       WHEN NO_DATA_FOUND THEN
12:19:52 1267  		 var_gc_purchaser_group_id := NULL;
12:19:52 1268  		 var_gc_redeemer_group_id  := NULL;
12:19:52 1269  	   END;
12:19:52 1270  	 END IF;
12:19:52 1271  
12:19:52 1272  	 IF var_subscrib_group_id IS NULL
12:19:52 1273  	   AND var_gc_purchaser_group_id IS NULL
12:19:52 1274  	   AND var_gc_redeemer_group_id IS NULL THEN
12:19:52 1275  	     RAISE CAN_NOT_FIND_ACCOUNT;
12:19:52 1276  	 END IF;
12:19:52 1277  
12:19:52 1278  	 OPEN out_group_ids FOR
12:19:52 1279  	 SELECT GROUP_ID FROM (
12:19:52 1280  	   SELECT
12:19:52 1281  	     var_subscrib_group_id as "GROUP_ID"
12:19:52 1282  	   FROM
12:19:52 1283  	     DUAL
12:19:52 1284  	   UNION
12:19:52 1285  	   SELECT
12:19:52 1286  	     var_gc_purchaser_group_id as "GROUP_ID"
12:19:52 1287  	   FROM
12:19:52 1288  	     DUAL
12:19:52 1289  	   UNION
12:19:52 1290  	   SELECT
12:19:52 1291  	     var_gc_redeemer_group_id as "GROUP_ID"
12:19:52 1292  	   FROM
12:19:52 1293  	     DUAL
12:19:52 1294  	 )
12:19:52 1295  	 WHERE
12:19:52 1296  	   GROUP_ID IS NOT NULL;
12:19:52 1297  
12:19:52 1298  EXCEPTION
12:19:52 1299  WHEN BAD_INVOICE_ID THEN
12:19:52 1300  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:52 1301  	   SPROC_NAME, 'No such invoice');
12:19:52 1302  WHEN CAN_NOT_FIND_ACCOUNT THEN
12:19:52 1303  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
12:19:52 1304  	   SPROC_NAME, 'Could not find account for given invoice');
12:19:52 1305  WHEN OTHERS THEN
12:19:52 1306  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
12:19:52 1307  	   SPROC_NAME, 'Unknown error', SQLERRM);
