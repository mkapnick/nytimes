17:13:54 441  FUNCTION CALCULATE_OFFER_CHAIN_END_DATE (
17:13:54 442  	in_offer_chain_id	  IN NUMBER,
17:13:54 443  	in_offer_chain_start_date IN DATE
17:13:54 444  ) RETURN DATE AS
17:13:54 445  -- VARIABLES
17:13:54 446  SPROC_NAME	     CONSTANT VARCHAR2(30) := 'CALCULATE_OFFER_CHAIN_END_DATE';
17:13:54 447  temp_offer_chain_id    NUMBER;
17:13:54 448  var_offer_chain_length NUMBER;
17:13:54 449  var_offer_duration     VARCHAR2(30);
17:13:54 450  var_offer_recurrences  NUMBER;
17:13:54 451  var_end_date	     DATE;
17:13:54 452  
17:13:54 453  var_offer_ym_interval INTERVAL YEAR TO MONTH;
17:13:54 454  var_offer_ds_interval INTERVAL DAY(3) TO SECOND;
17:13:54 455  var_offer_years	    NUMBER;
17:13:54 456  var_offer_months	    NUMBER;
17:13:54 457  var_offer_days	    NUMBER;
17:13:54 458  var_infinity_offers_count NUMBER;
17:13:54 459  
17:13:54 460  var_offers_set SYS_REFCURSOR;
17:13:54 461  
17:13:54 462  -- EXCEPTIONS
17:13:54 463  BAD_OFFER_CHAIN_ID EXCEPTION;
17:13:54 464  BEGIN
17:13:54 465  
17:13:54 466  	var_end_date := in_offer_chain_start_date;
17:13:54 467  
17:13:54 468  	-- Check that offer chain exists
17:13:54 469  	BEGIN
17:13:54 470  	  SELECT
17:13:54 471  	    OFFER_CHAIN.ID into temp_offer_chain_id
17:13:54 472  	  FROM
17:13:54 473  	    OFFER_CHAIN
17:13:54 474  	  WHERE
17:13:54 475  	    OFFER_CHAIN.ID = in_offer_chain_id;
17:13:54 476  	  EXCEPTION
17:13:54 477  	    WHEN NO_DATA_FOUND
17:13:54 478  	      THEN RAISE BAD_OFFER_CHAIN_ID;
17:13:54 479  	END;
17:13:54 480  
17:13:54 481  	SELECT
17:13:54 482  	  COUNT(*) into var_infinity_offers_count
17:13:54 483  	FROM
17:13:54 484  	  OFFER_OFFER_CHAIN
17:13:54 485  	WHERE
17:13:54 486  	  OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
17:13:54 487  	  AND OFFER_OFFER_CHAIN.NUM_RECURRENCES = GLOBAL_ENUMS_V23.OFFER_REC_INFINITY;
17:13:54 488  
17:13:54 489  	IF var_infinity_offers_count > 0 THEN
17:13:54 490  	  -- Offer chain contains offers with infinity num of recurrences
17:13:54 491  	  RETURN NULL;
17:13:54 492  	END IF;
17:13:54 493  
17:13:54 494  	-- Select offers durations
17:13:54 495  	OPEN var_offers_set FOR
17:13:54 496  	SELECT
17:13:54 497  	  OFFER.ENTITLEMENT_DURATION,
17:13:54 498  	  OFFER_OFFER_CHAIN.NUM_RECURRENCES
17:13:54 499  	FROM
17:13:54 500  	  OFFER_OFFER_CHAIN
17:13:54 501  	  INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
17:13:54 502  	WHERE
17:13:54 503  	  OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id;
17:13:54 504  
17:13:54 505  	-- Calculate sum of offers durations
17:13:54 506  	LOOP
17:13:54 507  	  FETCH var_offers_set INTO var_offer_duration, var_offer_recurrences;
17:13:54 508  	  EXIT WHEN var_offers_set%NOTFOUND;
17:13:54 509  	  PROCS_COMMON_V23.ISO8601DURATION_TO_INTERVALS(var_offer_duration, var_offer_years, var_offer_months, var_offer_days);
17:13:54 510  	  var_offer_ym_interval := var_offer_years||'-'||var_offer_months;
17:13:54 511  	  var_offer_ds_interval := var_offer_days||' 0:0:0';
17:13:54 512  	  var_end_date := var_end_date + ( var_offer_ym_interval * ( var_offer_recurrences + 1) ) + ( var_offer_ds_interval * ( var_offer_recurrences + 1) );
17:13:54 513  	END LOOP;
17:13:54 514  
17:13:54 515  	RETURN var_end_date;
17:13:54 516  
17:13:54 517  EXCEPTION
17:13:54 518  WHEN BAD_OFFER_CHAIN_ID THEN
17:13:54 519  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 520  	  SPROC_NAME, 'No such offer chain');
17:13:54 521  WHEN OTHERS THEN
17:13:54 522  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 523  	  SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 524  END CALCULATE_OFFER_CHAIN_END_DATE;
