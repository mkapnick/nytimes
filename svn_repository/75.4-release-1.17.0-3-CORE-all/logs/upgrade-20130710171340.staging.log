17:13:53  88  	PROCEDURE CHASE_PROFILE_BY_REQ_FILE_ID(
17:13:53  89  	  in_request_file_id IN CC_UPDATE.CC_REQUEST_FILE_ID%TYPE,
17:13:53  90  	  in_start	     IN NUMBER,
17:13:53  91  	  in_end	     IN NUMBER,
17:13:53  92  	  out_result_set     OUT SYS_REFCURSOR
17:13:53  93  	) AS
17:13:53  94  	SPROC_NAME CONSTANT VARCHAR2(32) := 'CHASE_PROFILE_BY_REQ_FILE_ID';
17:13:53  95  	var_range_diff	    NUMBER := 0;
17:13:53  96  	var_upper_bond_diff NUMBER := 0;
17:13:53  97  	var_l_start	    NUMBER := 0;
17:13:53  98  	var_l_end	    NUMBER := 0;
17:13:53  99  	BEGIN
17:13:53 100  	  --Normalize the end points [START]
17:13:53 101  	  IF (in_start IS NULL OR in_start < 0) Then
17:13:53 102  	    var_l_start := 0;
17:13:53 103  	  ELSE
17:13:53 104  	    var_l_start := in_start;
17:13:53 105  	  END IF;
17:13:53 106  
17:13:53 107  	  IF (in_end IS NULL) Then
17:13:53 108  	    var_l_end := 500;
17:13:53 109  	  ELSE
17:13:53 110  	    var_l_end := in_end;
17:13:53 111  	  END IF;
17:13:53 112  
17:13:53 113  	  var_l_start := var_l_start + 1;
17:13:53 114  	  var_l_end   := var_l_end   + 1;
17:13:53 115  
17:13:53 116  	  var_range_diff  := var_l_end - var_l_start;
17:13:53 117  	  var_upper_bond_diff :=  var_range_diff - 1000;
17:13:53 118  
17:13:53 119  	  IF (var_upper_bond_diff > 0) Then
17:13:53 120  	    var_l_end := var_l_end - var_upper_bond_diff;
17:13:53 121  	  END IF;
17:13:53 122  	  --Normalize the end points [END]
17:13:53 123  
17:13:53 124  	  OPEN out_result_set FOR
17:13:53 125  	    SELECT CHASE_PROFILE_ID FROM
17:13:53 126  	      (SELECT rownum rnum, q.* FROM
17:13:53 127  		 (SELECT
17:13:53 128  		    cc.CHASE_PROFILE_ID
17:13:53 129  		  FROM
17:13:53 130  		    CREDIT_CARD cc,
17:13:53 131  		    CC_UPDATE ccu
17:13:53 132  		  WHERE
17:13:53 133  		    ccu.CC_REQUEST_FILE_ID = in_request_file_id
17:13:53 134  		    AND ccu.CREDIT_CARD_ID = cc.id
17:13:53 135  		) Q
17:13:53 136  	      WHERE rownum <= var_l_end)
17:13:53 137  	    WHERE rnum >= var_l_Start;
17:13:53 138  	END CHASE_PROFILE_BY_REQ_FILE_ID;
