17:13:54 517  
17:13:54 518  PROCEDURE SUSPEND_SUBSCRIPTION(
17:13:54 519  /*
17:13:54 520  Throws exceptions:
17:13:54 521  APP_EXCEPTION_CODES_V23.NOT_FOUND
17:13:54 522  APP_EXCEPTION_CODES_V23.INTERNAL_ERROR
17:13:54 523  APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR
17:13:54 524  */
17:13:54 525  	  in_subs_id	IN NUMBER,
17:13:54 526  	  in_updated_by IN VARCHAR2
17:13:54 527  ) AS
17:13:54 528  SPROC_NAME		  CONSTANT VARCHAR2(20) := 'SUSPEND_SUBSCRIPTION';
17:13:54 529  var_subscription_status_id  NUMBER;
17:13:54 530  var_license_id		  NUMBER;
17:13:54 531  var_offer_id		  NUMBER;
17:13:54 532  var_license_start_date	  DATE;
17:13:54 533  var_license_end_date	  DATE;
17:13:54 534  
17:13:54 535  var_entitlement_dupration   VARCHAR2(30);
17:13:54 536  var_d_entitlement_dupration NUMBER;
17:13:54 537  
17:13:54 538  var_ym_interval INTERVAL YEAR TO MONTH;
17:13:54 539  var_ds_interval INTERVAL DAY(3) TO SECOND;
17:13:54 540  
17:13:54 541  -- EXCEPTIONS
17:13:54 542  BAD_SUBSCRIPTION_ID     EXCEPTION;
17:13:54 543  BAD_SUBSCRIPTION_STATUS EXCEPTION;
17:13:54 544  NO_LICENSE_FOUND	      EXCEPTION;
17:13:54 545  NO_OFFER_FOUND	      EXCEPTION;
17:13:54 546  EXCEPTION_MESSAGE       VARCHAR2(1024);
17:13:54 547  BEGIN
17:13:54 548  	-- TODO: Finish this prcedure (in Phase II)
17:13:54 549  
17:13:54 550  	-- Get subscription by id. FAULT if no such subscription.
17:13:54 551  	-- begin TX
17:13:54 552  	--   Get for update associated license (subscription.license_id). FAULT if not found.
17:13:54 553  	--   Set status to PROCESSED.
17:13:54 554  	--   updated record.
17:13:54 555  	--   compute days remaining in the subscription: original end_date - today = days_remaining_adjustment
17:13:54 556  	--   new subscription status is SUSPENDED.
17:13:54 557  	--   suspend_date is now.
17:13:54 558  	--   update subscription record.
17:13:54 559  	-- end TX
17:13:54 560  
17:13:54 561  	BEGIN
17:13:54 562  	  SELECT
17:13:54 563  	    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID into var_subscription_status_id
17:13:54 564  	  FROM
17:13:54 565  	    SUBSCRIPTION
17:13:54 566  	  WHERE
17:13:54 567  	    SUBSCRIPTION.ID = in_subs_id;
17:13:54 568  	  EXCEPTION
17:13:54 569  	  WHEN NO_DATA_FOUND THEN
17:13:54 570  	    RAISE BAD_SUBSCRIPTION_ID;
17:13:54 571  	END;
17:13:54 572  
17:13:54 573  	IF var_subscription_status_id != GLOBAL_STATUSES_V23.SUBSCRIPTION_ACTIVE THEN
17:13:54 574  	  RAISE BAD_SUBSCRIPTION_STATUS;
17:13:54 575  	END IF;
17:13:54 576  
17:13:54 577  	BEGIN
17:13:54 578  	  SELECT
17:13:54 579  	    LICENSE.ID,
17:13:54 580  	    LICENSE.OFFER_ID,
17:13:54 581  	    LICENSE.START_DATE
17:13:54 582  	    into
17:13:54 583  	    var_license_id,
17:13:54 584  	    var_offer_id,
17:13:54 585  	    var_license_start_date
17:13:54 586  	  FROM
17:13:54 587  	    LICENSE
17:13:54 588  	  WHERE
17:13:54 589  	    LICENSE.SUBSCRIPTION_ID = in_subs_id
17:13:54 590  	      AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V23.LICENSE_ACTIVE;
17:13:54 591  	  EXCEPTION
17:13:54 592  	  WHEN NO_DATA_FOUND THEN
17:13:54 593  	    RAISE NO_LICENSE_FOUND;
17:13:54 594  	END;
17:13:54 595  
17:13:54 596  	PROCS_LICENSE_CRU_V23.UPDATE_LICENSE(
17:13:54 597  	  in_license_id        => var_license_id,
17:13:54 598  	  in_updated_by        => in_updated_by,
17:13:54 599  	  in_license_status_id => GLOBAL_STATUSES_V23.LICENSE_CLOSED
17:13:54 600  	);
17:13:54 601  
17:13:54 602  	BEGIN
17:13:54 603  	  SELECT
17:13:54 604  	    OFFER.ENTITLEMENT_DURATION into var_entitlement_dupration
17:13:54 605  	  FROM
17:13:54 606  	    OFFER
17:13:54 607  	  WHERE
17:13:54 608  	    OFFER.ID = var_offer_id;
17:13:54 609  	  EXCEPTION
17:13:54 610  	  WHEN NO_DATA_FOUND THEN
17:13:54 611  	    RAISE NO_OFFER_FOUND;
17:13:54 612  	END;
17:13:54 613  
17:13:54 614  	var_ym_interval := substr(var_entitlement_dupration, 0, 4);
17:13:54 615  	var_ds_interval := substr(var_entitlement_dupration, 4);
17:13:54 616  
17:13:54 617  	var_license_end_date := var_license_start_date + var_ym_interval + var_ds_interval;
17:13:54 618  
17:13:54 619  	var_d_entitlement_dupration := var_license_end_date - current_date;
17:13:54 620  
17:13:54 621  	PROCS_SUBSCRIPTION_CRU_V23.UPDATE_SUBSCRIPTION(
17:13:54 622  	  in_subscription_id	       => in_subs_id,
17:13:54 623  	  in_updated_by 	       => in_updated_by,
17:13:54 624  	  in_suspend_date	       => SYSDATE,
17:13:54 625  	  in_subscription_status_id    => GLOBAL_STATUSES_V23.SUBSCRIPTION_SUSPENDED,
17:13:54 626  	  in_days_remainning_ajustment => var_d_entitlement_dupration
17:13:54 627  	);
17:13:54 628  
17:13:54 629  EXCEPTION
17:13:54 630  WHEN BAD_SUBSCRIPTION_ID THEN
17:13:54 631  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 632  	  SPROC_NAME, 'No such subscription');
17:13:54 633  WHEN BAD_SUBSCRIPTION_STATUS THEN
17:13:54 634  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
17:13:54 635  	  SPROC_NAME, 'Subscription is not active');
17:13:54 636  WHEN NO_LICENSE_FOUND THEN
17:13:54 637  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 638  	  SPROC_NAME, 'Can not find license associated with given subscription ID');
17:13:54 639  WHEN NO_OFFER_FOUND THEN
17:13:54 640  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 641  	  SPROC_NAME, 'Can not find offer associated with given subscription ID');
17:13:54 642  WHEN OTHERS THEN
17:13:54 643  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 644  	  SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 645  END SUSPEND_SUBSCRIPTION;
