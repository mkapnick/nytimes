17:13:54 927  
17:13:54 928  	PROCEDURE FINALIZE_GC_PURCHASING (
17:13:54 929  	  in_invoice_id 	IN NUMBER,
17:13:54 930  	  in_created_by 	IN VARCHAR2,
17:13:54 931  	  in_instrument_id	IN NUMBER,
17:13:54 932  	  in_instrument_type_id IN NUMBER,
17:13:54 933  	  in_order_id		IN VARCHAR2,
17:13:54 934  	  in_transaction_id	IN NUMBER,
17:13:54 935  	  out_charge_amount	OUT NUMBER
17:13:54 936  	) AS
17:13:54 937  	  SPROC_NAME CONSTANT VARCHAR2(22) := 'FINALIZE_GC_PURCHASING';
17:13:54 938  -- VARIABLES
17:13:54 939  	  temp_transaction_id_count NUMBER;
17:13:54 940  	  var_invoice_amount	    NUMBER(10,2);
17:13:54 941  	  var_transaction_id	    NUMBER;
17:13:54 942  	  var_new_charge_id	    NUMBER;
17:13:54 943  -- EXCEPTIONS
17:13:54 944  	    BAD_CREDIT_CARD_ID		EXCEPTION;
17:13:54 945  	    BAD_PAYPAL_ID		EXCEPTION;
17:13:54 946  	    BAD_INSTRUMENT_TYPE 	EXCEPTION;
17:13:54 947  	    TRANSACTION_EXISTS		EXCEPTION;
17:13:54 948  	    CAN_NOT_CALC_INVOICE_AMOUNT EXCEPTION;
17:13:54 949  	    CAN_NOT_USE_FCINSTR 	EXCEPTION;
17:13:54 950  	    CAN_NOT_CREATE_TRANSACTION	EXCEPTION;
17:13:54 951  	    CAN_NOT_CREATE_CHARGE	EXCEPTION;
17:13:54 952  	  EXCEPTION_MESSAGE   VARCHAR2(1024);
17:13:54 953  	  BEGIN
17:13:54 954  
17:13:54 955  -- Check that instrument exists
17:13:54 956  	    IF in_instrument_type_id = GLOBAL_ENUMS_V23.INSTRUMENT_CREDIT_CARD THEN
17:13:54 957  	      IF IS_CREDIT_CARD_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V23.FALSE THEN
17:13:54 958  		RAISE BAD_CREDIT_CARD_ID;
17:13:54 959  	      END IF;
17:13:54 960  	    ELSIF in_instrument_type_id = GLOBAL_ENUMS_V23.INSTRUMENT_PAYPAL THEN
17:13:54 961  	      IF IS_PAYPAL_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V23.FALSE THEN
17:13:54 962  		RAISE BAD_PAYPAL_ID;
17:13:54 963  	      END IF;
17:13:54 964  	    ELSIF in_instrument_type_id = GLOBAL_ENUMS_V23.INSTRUMENT_ZCI_INSTRUMENT THEN
17:13:54 965  	      NULL;
17:13:54 966  	    ELSE
17:13:54 967  	      RAISE BAD_INSTRUMENT_TYPE;
17:13:54 968  	    END IF;
17:13:54 969  
17:13:54 970  -- Check that transaction with given id do not exists
17:13:54 971  	    SELECT
17:13:54 972  	      COUNT(*) into temp_transaction_id_count
17:13:54 973  	    FROM
17:13:54 974  	      TRANSACTION
17:13:54 975  	    WHERE
17:13:54 976  	      TRANSACTION.ID = in_transaction_id;
17:13:54 977  
17:13:54 978  	    IF temp_transaction_id_count > 0 THEN
17:13:54 979  	      RAISE TRANSACTION_EXISTS;
17:13:54 980  	    END IF;
17:13:54 981  
17:13:54 982  -- Calculate new invoice amount
17:13:54 983  	    BEGIN
17:13:54 984  	      PROCS_INVOICE_V23.CALCULATE_INVOICE_AMOUNT(in_invoice_id, var_invoice_amount);
17:13:54 985  	      EXCEPTION
17:13:54 986  	      WHEN OTHERS THEN
17:13:54 987  	      EXCEPTION_MESSAGE := SQLERRM;
17:13:54 988  	      RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
17:13:54 989  	    END;
17:13:54 990  
17:13:54 991  	    IF var_invoice_amount > 0
17:13:54 992  	       AND in_instrument_type_id = GLOBAL_ENUMS_V23.INSTRUMENT_ZCI_INSTRUMENT THEN
17:13:54 993  	      RAISE CAN_NOT_USE_FCINSTR;
17:13:54 994  	    END IF;
17:13:54 995  
17:13:54 996  -- If invoice amount iz 0 then we need to set status for this invoice to PROCCESSED
17:13:54 997  	    IF var_invoice_amount = 0 THEN
17:13:54 998  	      PROCS_INVOICE_CRU_V23.UPDATE_INVOICE(
17:13:54 999  		  in_invoice_id 		 => in_invoice_id,
17:13:54 1000  		   in_updated_by		  => in_created_by,
17:13:54 1001  		   in_invoice_status_id 	  => GLOBAL_STATUSES_V23.INVOICE_CLOSED
17:13:54 1002  	       );
17:13:54 1003  	     END IF;
17:13:54 1004  
17:13:54 1005  	     IF var_invoice_amount > 0 THEN
17:13:54 1006  -- Create transaction
17:13:54 1007  	       BEGIN
17:13:54 1008  		 PROCS_TRANSACTION_V23.CREATE_TRANSACTION(
17:13:54 1009  		     in_transaction_id	      => in_transaction_id,
17:13:54 1010  		     in_status_id	      => GLOBAL_STATUSES_V23.TRANSACTION_PENDING,
17:13:54 1011  		     in_amount		      => var_invoice_amount,
17:13:54 1012  		     in_created_by	      => in_created_by,
17:13:54 1013  		     in_order_id	      => in_order_id,
17:13:54 1014  		     in_transaction_type_code => 'GIFT_CERTIFICATE_PURCHASE',
17:13:54 1015  		     out_transaction_id       => var_transaction_id
17:13:54 1016  		 );
17:13:54 1017  		 EXCEPTION
17:13:54 1018  		 WHEN OTHERS THEN
17:13:54 1019  		 EXCEPTION_MESSAGE := SQLERRM;
17:13:54 1020  		 RAISE CAN_NOT_CREATE_TRANSACTION;
17:13:54 1021  	       END;
17:13:54 1022  
17:13:54 1023  -- Create charge
17:13:54 1024  	       BEGIN
17:13:54 1025  		 PROCS_CHARGE_V23.CREATE_CHARGE(
17:13:54 1026  		     in_invoice_id	   => in_invoice_id,
17:13:54 1027  		     in_transaction_id	   => var_transaction_id,
17:13:54 1028  		     in_instrument_type_id => in_instrument_type_id,
17:13:54 1029  		     in_instrument_id	   => in_instrument_id,
17:13:54 1030  		     in_charge_amount	   => var_invoice_amount,
17:13:54 1031  		     in_created_by	   => in_created_by,
17:13:54 1032  		     in_charge_status_id   => GLOBAL_STATUSES_V23.CHARGE_OPENED,
17:13:54 1033  		     out_charge_id	   => var_new_charge_id
17:13:54 1034  		 );
17:13:54 1035  		 out_charge_amount := var_invoice_amount;
17:13:54 1036  		 EXCEPTION
17:13:54 1037  		 WHEN OTHERS THEN
17:13:54 1038  		 EXCEPTION_MESSAGE := SQLERRM;
17:13:54 1039  		 RAISE CAN_NOT_CREATE_CHARGE;
17:13:54 1040  	       END;
17:13:54 1041  	     ELSE
17:13:54 1042  	       out_charge_amount := 0;
17:13:54 1043  	     END IF;
17:13:54 1044  
17:13:54 1045  	     EXCEPTION
17:13:54 1046  	     WHEN CAN_NOT_USE_FCINSTR THEN
17:13:54 1047  	     PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
17:13:54 1048  					      SPROC_NAME, 'Could not use "free charge instrument" for non-zero invoice');
17:13:54 1049  	     WHEN BAD_CREDIT_CARD_ID THEN
17:13:54 1050  	     PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 1051  					      SPROC_NAME, 'Bad credit card id');
17:13:54 1052  	     WHEN BAD_PAYPAL_ID THEN
17:13:54 1053  	     PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 1054  					      SPROC_NAME, 'Bad paypal id');
17:13:54 1055  	     WHEN BAD_INSTRUMENT_TYPE THEN
17:13:54 1056  	     PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INVALID_PARAMETER,
17:13:54 1057  					      SPROC_NAME, 'Bad instrument type');
17:13:54 1058  	     WHEN TRANSACTION_EXISTS THEN
17:13:54 1059  	     PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.DUPLICATE_ERROR,
17:13:54 1060  					      SPROC_NAME, 'Transaction with given id already exists');
17:13:54 1061  	     WHEN CAN_NOT_CREATE_TRANSACTION THEN
17:13:54 1062  	     PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 1063  					      SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
17:13:54 1064  	     WHEN CAN_NOT_CREATE_CHARGE THEN
17:13:54 1065  	     PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 1066  					      SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
17:13:54 1067  	     WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
17:13:54 1068  	     PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 1069  					      SPROC_NAME, 'Could not calculate amount for new invoice', EXCEPTION_MESSAGE);
17:13:54 1070  	     WHEN OTHERS THEN
17:13:54 1071  	     PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 1072  					      SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 1073  	   END FINALIZE_GC_PURCHASING;
