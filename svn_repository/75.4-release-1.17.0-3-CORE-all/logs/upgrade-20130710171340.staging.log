17:13:54 1119  BAD_SECOND_OFFER_CHAIN	      EXCEPTION;
17:13:54 1120  BEGIN
17:13:54 1121  
17:13:54 1122  	 -- Check that first offer chain exists
17:13:54 1123  	 BEGIN
17:13:54 1124  	   SELECT
17:13:54 1125  	     OFFER_CHAIN.ID into temp_offer_chain_id
17:13:54 1126  	   FROM
17:13:54 1127  	     OFFER_CHAIN
17:13:54 1128  	   WHERE
17:13:54 1129  	     OFFER_CHAIN.ID = in_offer_chain_1;
17:13:54 1130  	   EXCEPTION
17:13:54 1131  	     WHEN NO_DATA_FOUND THEN
17:13:54 1132  	       RAISE BAD_FIRST_OFFER_CHAIN;
17:13:54 1133  	 END;
17:13:54 1134  
17:13:54 1135  	 -- Check that second offer chain exists
17:13:54 1136  	 BEGIN
17:13:54 1137  	   SELECT
17:13:54 1138  	     OFFER_CHAIN.ID into temp_offer_chain_id
17:13:54 1139  	   FROM
17:13:54 1140  	     OFFER_CHAIN
17:13:54 1141  	   WHERE
17:13:54 1142  	     OFFER_CHAIN.ID = in_offer_chain_2;
17:13:54 1143  	   EXCEPTION
17:13:54 1144  	     WHEN NO_DATA_FOUND THEN
17:13:54 1145  	       RAISE BAD_SECOND_OFFER_CHAIN;
17:13:54 1146  	 END;
17:13:54 1147  
17:13:54 1148  	 PROCS_OFFER_CHAIN_V23.GET_OFF_CHAINS_SAME_PRODUCTS(
17:13:54 1149  	   in_offer_chain_1 => in_offer_chain_1,
17:13:54 1150  	   in_offer_chain_2 => in_offer_chain_2,
17:13:54 1151  	   out_result_set   => var_same_products
17:13:54 1152  	 );
17:13:54 1153  
17:13:54 1154  	 LOOP
17:13:54 1155  	   FETCH var_same_products INTO var_same_product_id, same_product_count;
17:13:54 1156  	   EXIT WHEN var_same_products%NOTFOUND;
17:13:54 1157  
17:13:54 1158  	   IF in_use_eligibility_rules = GLOBAL_CONSTANTS_V23.FALSE THEN
17:13:54 1159  	     -- Return false because this offer chains having same products
