17:13:54 2889  /*
17:13:54 2890  	   Note, this returns paypal records which are in the given window of time
17:13:54 2891  	   based on their creation date, not their expiration date, because we've seen
17:13:54 2892  	   suspicious values in the data for expiration dates. The java layer should
17:13:54 2893  	   inspect the expiration dates and decide whether or not to act on the data.
17:13:54 2894  */
17:13:54 2895  
17:13:54 2896  PROCEDURE GET_EXPIRING_PAYPAL (
17:13:54 2897  	 in_expire_window_days	 IN NUMBER,
17:13:54 2898  	 in_creation_limit_days  IN NUMBER,
17:13:54 2899  	 in_retry_throttle_name  IN PROCESS_RETRY_THROTTLE.PROCESS_NAME%TYPE,
17:13:54 2900  	 out_result_set 	 OUT SYS_REFCURSOR
17:13:54 2901  ) AS
17:13:54 2902  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_EXPIRING_PAYPAL';
17:13:54 2903  var_rows_to_return CONSTANT NUMBER := 1000;
17:13:54 2904  BEGIN
17:13:54 2905  	 OPEN out_result_set FOR
17:13:54 2906  	 SELECT * FROM (
17:13:54 2907  	       SELECT
17:13:54 2908  	       ac.group_id	   AS group_id,
17:13:54 2909  	       pp.id		   AS paypal_id,
17:13:54 2910  	       sb.id		   AS subscription_id,
17:13:54 2911  	       pp.create_date	   AS paypal_creation_date,
17:13:54 2912  	       pp.expiration_date  AS paypal_expiration_date,
17:13:54 2913  	       lc.end_date	   AS license_end_date
17:13:54 2914  	   FROM license lc
17:13:54 2915  	   INNER JOIN subscription sb
17:13:54 2916  	   ON lc.subscription_id = sb.id
17:13:54 2917  	   INNER JOIN paypal pp
17:13:54 2918  	   ON pp.account_id = sb.account_id
17:13:54 2919  	   INNER JOIN account ac
17:13:54 2920  	   ON pp.account_id = ac.id
17:13:54 2921  	   WHERE SYSDATE <= lc.end_date
17:13:54 2922  	     AND SYSDATE + in_expire_window_days > lc.end_date
17:13:54 2923  	     AND pp.paypal_status_id = GLOBAL_STATUSES_V23.PAYPAL_ACTIVE
17:13:54 2924  	     AND lc.license_status_id IN (GLOBAL_STATUSES_V23.LICENSE_ACTIVE,
17:13:54 2925  					  GLOBAL_STATUSES_V23.LICENSE_IN_GRACE_PERIOD)
17:13:54 2926  	     AND pp.id = sb.instrument_id
17:13:54 2927  	     AND sb.instrument_type_id = GLOBAL_ENUMS_V23.INSTRUMENT_PAYPAL
17:13:54 2928  	     AND sb.subscription_status_id IN (GLOBAL_STATUSES_V23.SUBSCRIPTION_ACTIVE,
17:13:54 2929  					       GLOBAL_STATUSES_V23.SUBSCRIPTION_IN_GRACE_PERIOD)
17:13:54 2930  	     AND pp.create_date + in_creation_limit_days <= lc.end_date
17:13:54 2931  	     AND NOT EXISTS(
17:13:54 2932  	       SELECT NULL
17:13:54 2933  	       FROM process_retry_throttle rt
17:13:54 2934  	       WHERE rt.process_name = in_retry_throttle_name
17:13:54 2935  		 AND rt.generic_id = pp.id
17:13:54 2936  	     )
17:13:54 2937  	     AND ROWNUM <= 10 * var_rows_to_return
17:13:54 2938  	   ORDER BY dbms_random.value
17:13:54 2939  	 )
17:13:54 2940  	 WHERE ROWNUM <= var_rows_to_return;
17:13:54 2941  END GET_EXPIRING_PAYPAL;
