17:13:54 318  PROCEDURE DISABLE_CREDIT_CARD (
17:13:54 319  /*
17:13:54 320  Throws exceptions:
17:13:54 321  APP_EXCEPTION_CODES_V23.NOT_FOUND
17:13:54 322  APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR
17:13:54 323  APP_EXCEPTION_CODES_V23.INTERNAL_ERROR
17:13:54 324  */
17:13:54 325  	in_credit_card_id IN NUMBER,
17:13:54 326  	in_updated_by	  IN VARCHAR2
17:13:54 327  ) AS
17:13:54 328  SPROC_NAME		   CONSTANT VARCHAR2(19) := 'DISABLE_CREDIT_CARD';
17:13:54 329  -- VARIBLES
17:13:54 330  var_account_id		     NUMBER;
17:13:54 331  var_group_id		     NUMBER;
17:13:54 332  var_credit_card_status	     NUMBER;
17:13:54 333  var_pending_transactions_num   NUMBER;
17:13:54 334  var_pending_invoices_num	     NUMBER;
17:13:54 335  current_def_instrument_type_id NUMBER;
17:13:54 336  current_def_instrument_id      NUMBER;
17:13:54 337  -- EXCEPTIONS
17:13:54 338  BAD_CC_STATUS		   EXCEPTION;
17:13:54 339  PENDING_TRANSACTIONS_FOUNDED EXCEPTION;
17:13:54 340  CAN_NOT_GET_DEF_FINANCIAL    EXCEPTION;
17:13:54 341  CAN_NOT_DEL_DEF_FINANCIAL    EXCEPTION;
17:13:54 342  CAN_NOT_DISABLE_CREDIT_CARD  EXCEPTION;
17:13:54 343  EXCEPTION_MESSAGE 	   VARCHAR2(1024);
17:13:54 344  BEGIN
17:13:54 345  
17:13:54 346  	-- Get credit card status
17:13:54 347  	-- Get account id
17:13:54 348  	SELECT
17:13:54 349  	  CREDIT_CARD.CREDIT_CARD_STATUS_ID,
17:13:54 350  	  CREDIT_CARD.ACCOUNT_ID
17:13:54 351  	  into
17:13:54 352  	  var_credit_card_status,
17:13:54 353  	  var_account_id
17:13:54 354  	FROM
17:13:54 355  	  CREDIT_CARD
17:13:54 356  	WHERE
17:13:54 357  	  CREDIT_CARD.ID = in_credit_card_id;
17:13:54 358  
17:13:54 359  	-- Check that we can disable this credit card (STUB)
17:13:54 360  	IF F_CAN_DISABLE_CREDIT_CARD(in_credit_card_id) = GLOBAL_CONSTANTS_V23.FALSE THEN
17:13:54 361  	  RAISE CAN_NOT_DISABLE_CREDIT_CARD;
17:13:54 362  	END IF;
17:13:54 363  
17:13:54 364  	-- Get account id
17:13:54 365  	SELECT
17:13:54 366  	  ACCOUNT.GROUP_ID into var_group_id
17:13:54 367  	FROM
17:13:54 368  	  ACCOUNT
17:13:54 369  	WHERE
17:13:54 370  	  ACCOUNT.ID = var_account_id;
17:13:54 371  
17:13:54 372  	-- Card should to be active
17:13:54 373  	IF var_credit_card_status != GLOBAL_STATUSES_V23.CREDIT_CARD_ACTIVE THEN
17:13:54 374  	  RAISE BAD_CC_STATUS;
17:13:54 375  	END IF;
17:13:54 376  
17:13:54 377  	-- Looking for pending transactions associated with given credit card
17:13:54 378  	SELECT
17:13:54 379  	  COUNT(*) into var_pending_invoices_num
17:13:54 380  	FROM
17:13:54 381  	  CHARGE
17:13:54 382  	  INNER JOIN TRANSACTION ON CHARGE.TRANSACTION_ID = TRANSACTION.ID
17:13:54 383  	WHERE
17:13:54 384  	  CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V23.INSTRUMENT_CREDIT_CARD
17:13:54 385  	  AND CHARGE.INSTRUMENT_ID = in_credit_card_id
17:13:54 386  	  AND TRANSACTION.TRANSACTION_STATUS_ID = GLOBAL_STATUSES_V23.TRANSACTION_PENDING;
17:13:54 387  
17:13:54 388  	IF var_pending_invoices_num > 0 THEN
17:13:54 389  	  RAISE PENDING_TRANSACTIONS_FOUNDED;
17:13:54 390  	END IF;
17:13:54 391  
17:13:54 392  	-- Getting current default financial instrument
17:13:54 393  	BEGIN
17:13:54 394  	  GET_DEF_FINANCIAL_INSTRUMENT(
17:13:54 395  	    in_group_id 	   => var_group_id,
17:13:54 396  	    out_instrument_type_id => current_def_instrument_type_id,
17:13:54 397  	    out_instrument_id	   => current_def_instrument_id
17:13:54 398  	  );
17:13:54 399  	  EXCEPTION
17:13:54 400  	    WHEN OTHERS THEN
17:13:54 401  	      EXCEPTION_MESSAGE := SQLERRM;
17:13:54 402  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
17:13:54 403  	END;
17:13:54 404  
17:13:54 405  	-- Checking that credit card is not default
17:13:54 406  	IF current_def_instrument_type_id = GLOBAL_ENUMS_V23.INSTRUMENT_CREDIT_CARD
17:13:54 407  	  AND current_def_instrument_id = in_credit_card_id THEN
17:13:54 408  	  BEGIN
17:13:54 409  	    DEL_DEF_FINANCIAL_INSTRUMENT(
17:13:54 410  	      in_group_id => var_group_id
17:13:54 411  	    );
17:13:54 412  	  EXCEPTION
17:13:54 413  	    WHEN OTHERS THEN
17:13:54 414  	      EXCEPTION_MESSAGE := SQLERRM;
17:13:54 415  	      RAISE CAN_NOT_DEL_DEF_FINANCIAL;
17:13:54 416  	  END;
17:13:54 417  	END IF;
17:13:54 418  
17:13:54 419  	-- Update credit card status
17:13:54 420  	PROCS_FIN_INSTRUMENTS_V23.UPDATE_CREDIT_CARD_STATUS(
17:13:54 421  	  in_credit_card_id	   => in_credit_card_id,
17:13:54 422  	  in_updated_by 	   => in_updated_by,
17:13:54 423  	  in_credit_card_status_id => GLOBAL_STATUSES_V23.CREDIT_CARD_DISABLED
17:13:54 424  	);
17:13:54 425  
17:13:54 426  EXCEPTION
17:13:54 427  WHEN NO_DATA_FOUND THEN
17:13:54 428  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 429  	  SPROC_NAME, 'No such credit card');
17:13:54 430  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
17:13:54 431  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 432  	  SPROC_NAME, 'Could not get current default financial instrument', EXCEPTION_MESSAGE);
17:13:54 433  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
17:13:54 434  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 435  	  SPROC_NAME, 'Could not delete information about default financial instrument from account', EXCEPTION_MESSAGE);
17:13:54 436  WHEN BAD_CC_STATUS THEN
17:13:54 437  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
17:13:54 438  	  SPROC_NAME, 'Credit card is not active');
17:13:54 439  WHEN PENDING_TRANSACTIONS_FOUNDED THEN
17:13:54 440  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
17:13:54 441  	  SPROC_NAME, 'Account has pending charge which is using this card');
17:13:54 442  WHEN CAN_NOT_DISABLE_CREDIT_CARD THEN
17:13:54 443  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 444  	  SPROC_NAME, 'Could not disable this credit card', EXCEPTION_MESSAGE);
17:13:54 445  WHEN OTHERS THEN
17:13:54 446  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 447  	  SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 448  END DISABLE_CREDIT_CARD;
