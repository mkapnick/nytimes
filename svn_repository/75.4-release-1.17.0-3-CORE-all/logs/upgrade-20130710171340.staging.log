17:13:54 803  
17:13:54 804  PROCEDURE SHOULD_MOVE_TO_GRACE(
17:13:54 805  	in_invoice_id  IN INVOICE.ID%TYPE,
17:13:54 806  	out_result     OUT NUMBER
17:13:54 807  ) AS
17:13:54 808  BEGIN
17:13:54 809  	-- if the invoice preceding the given invoice has no transaction attempts, then
17:13:54 810  	-- it is not billed out of Sartre. if so, then the associated subscription
17:13:54 811  	-- should be canceled after a final failed billing attempt--not moved to grace.
17:13:54 812  	SELECT DECODE(COUNT(1), 0, 0, 1) INTO out_result
17:13:54 813  	FROM CHARGE c
17:13:54 814  	INNER JOIN TRANSACTION t ON c.TRANSACTION_ID = t.ID
17:13:54 815  	INNER JOIN TRANSACTION_ATTEMPT ta ON ta.TRANSACTION_ID = t.ID
17:13:54 816  	WHERE c.INVOICE_ID = (
17:13:54 817  	  -- select previous invoice_id, or -1 if there is none
17:13:54 818  	  SELECT PREV_INVOICE_ID FROM (
17:13:54 819  	    SELECT i.ID, LAG(i.ID, 1, -1) OVER (ORDER BY i.CREATE_DATE) AS PREV_INVOICE_ID
17:13:54 820  	    FROM INVOICE i
17:13:54 821  	    INNER JOIN LICENSE l ON i.ID = l.INVOICE_ID
17:13:54 822  	    WHERE l.SUBSCRIPTION_ID = (
17:13:54 823  	      SELECT SUBSCRIPTION_ID FROM LICENSE WHERE INVOICE_ID = in_invoice_id
17:13:54 824  	    )
17:13:54 825  	  ) WHERE ID = in_invoice_id
17:13:54 826  	);
17:13:54 827  END SHOULD_MOVE_TO_GRACE;
17:13:54 828  
17:13:54 829  /****************************************************************************/
17:13:54 830  
17:13:54 831  PROCEDURE MOVE_TO_GRACE(
