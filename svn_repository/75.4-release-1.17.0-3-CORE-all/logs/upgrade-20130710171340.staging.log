17:13:54 303  	OPEN c(in_start_group_id, in_end_group_id);
17:13:54 304  	WHILE c%ISOPEN LOOP
17:13:54 305  	  FETCH c INTO gid;
17:13:54 306  	  IF c%NOTFOUND THEN
17:13:54 307  	   CLOSE c;
17:13:54 308  	  END IF;
17:13:54 309  	  TEST_DELETE_USER_ACCOUNT(gid);
