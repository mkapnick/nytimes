17:13:54 595  
17:13:54 596  FUNCTION IS_INVOICE_PAYING_STARTED (
17:13:54 597  /*
17:13:54 598  Throws exceptions:
17:13:54 599  APP_EXCEPTION_CODES_V23.NOT_FOUND
17:13:54 600  APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR
17:13:54 601  */
17:13:54 602  	in_invoice_id IN NUMBER
17:13:54 603  ) RETURN NUMBER AS
17:13:54 604  -- VARIABLES
17:13:54 605  SPROC_NAME		 CONSTANT VARCHAR2(30) := 'IS_INVOICE_PAYING_STARTED';
17:13:54 606  temp_invoice_id		 NUMBER;
17:13:54 607  var_processed_charges_num  NUMBER;
17:13:54 608  var_processed_transac_num  NUMBER;
17:13:54 609  var_success_attempts_num	 NUMBER;
17:13:54 610  var_is_gc 		 NUMBER;
17:13:54 611  -- EXCEPTIONS
17:13:54 612  BAD_INVOICE_ID EXCEPTION;
17:13:54 613  BEGIN
17:13:54 614  
17:13:54 615  	-- Check that invoice exists
17:13:54 616  	BEGIN
17:13:54 617  	  SELECT
17:13:54 618  	    INVOICE.ID into temp_invoice_id
17:13:54 619  	  FROM
17:13:54 620  	    INVOICE
17:13:54 621  	  WHERE
17:13:54 622  	    INVOICE.ID = in_invoice_id;
17:13:54 623  	  EXCEPTION
17:13:54 624  	   WHEN NO_DATA_FOUND THEN
17:13:54 625  	     RAISE BAD_INVOICE_ID;
17:13:54 626  	END;
17:13:54 627  
17:13:54 628  	-- Check that there are tansaction attempts with status success
17:13:54 629  	SELECT
17:13:54 630  	  COUNT(1) into var_success_attempts_num
17:13:54 631  	FROM
17:13:54 632  	  TRANSACTION_ATTEMPT ta,
17:13:54 633  	  TRANSACTION t,
17:13:54 634  	  CHARGE c
17:13:54 635  	WHERE
17:13:54 636  	  ta.TRANSACTION_ATTEMPT_STATUS_ID = GLOBAL_STATUSES_V23.TRANS_ATTEMPT_SUCCESS and
17:13:54 637  	  ta.transaction_id = t.id and
17:13:54 638  	  c.transaction_id = t.id and
17:13:54 639  	  t.is_refund = GLOBAL_CONSTANTS_V23.FALSE and
17:13:54 640  	  c.invoice_id = in_invoice_id
17:13:54 641  	;
17:13:54 642  
17:13:54 643  	IF var_success_attempts_num > 0 THEN
17:13:54 644  	  RETURN GLOBAL_CONSTANTS_V23.TRUE;
17:13:54 645  	END IF;
17:13:54 646  
17:13:54 647  	SELECT
17:13:54 648  	  COUNT(1) into var_success_attempts_num
17:13:54 649  	FROM
17:13:54 650  	  TRANSACTION t,
17:13:54 651  	  CHARGE c
17:13:54 652  	WHERE
17:13:54 653  	  c.transaction_id = t.id and
17:13:54 654  	  t.is_refund = GLOBAL_CONSTANTS_V23.FALSE and
17:13:54 655  	  t.is_settled = GLOBAL_CONSTANTS_V23.TRUE and
17:13:54 656  	  c.invoice_id = in_invoice_id
17:13:54 657  	;
17:13:54 658  
17:13:54 659  	IF var_success_attempts_num > 0 THEN
17:13:54 660  	  RETURN GLOBAL_CONSTANTS_V23.TRUE;
17:13:54 661  	END IF;
17:13:54 662  
17:13:54 663  	SELECT
17:13:54 664  	  COUNT(1) into var_is_gc
17:13:54 665  	FROM
17:13:54 666  	  gift_certificate gc
17:13:54 667  	WHERE
17:13:54 668  	  gc.finalized_invoice_id = in_invoice_id
17:13:54 669  	;
17:13:54 670  
17:13:54 671  	IF var_is_gc > 0 THEN
17:13:54 672  	  RETURN GLOBAL_CONSTANTS_V23.TRUE;
17:13:54 673  	END IF;
17:13:54 674  
17:13:54 675  	RETURN GLOBAL_CONSTANTS_V23.FALSE;
17:13:54 676  
17:13:54 677  EXCEPTION
17:13:54 678  WHEN BAD_INVOICE_ID THEN
17:13:54 679  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 680  	  SPROC_NAME, 'No such invoice');
17:13:54 681  WHEN OTHERS THEN
17:13:54 682  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 683  	  SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 684  END IS_INVOICE_PAYING_STARTED;
