17:13:54 697  /******************************************************************************/
17:13:54 698  
17:13:54 699  PROCEDURE CALCULATE_INVOICE_CHARGEBACK (
17:13:54 700  	in_invoice_id	      IN NUMBER,
17:13:54 701  	in_chargeback_date    IN DATE,
17:13:54 702  	out_chargeback_amount OUT NUMBER
17:13:54 703  ) AS
17:13:54 704  -- VARIABLES
17:13:54 705  SPROC_NAME	     CONSTANT VARCHAR2(28) := 'CALCULATE_INVOICE_CHARGEBACK';
17:13:54 706  var_chargeback_date    DATE;
17:13:54 707  temp_invoice_id	     NUMBER;
17:13:54 708  var_licenses_number    NUMBER;
17:13:54 709  var_invoice_start_date DATE;
17:13:54 710  var_invoice_end_date   DATE;
17:13:54 711  var_offer_id	     NUMBER;
17:13:54 712  var_offer_days_interval NUMBER;
17:13:54 713  var_license_days_used  NUMBER;
17:13:54 714  var_invoice_amount     NUMBER(10,2);
17:13:54 715  var_offer_chain_id     NUMBER;
17:13:54 716  var_offer_chain_meta_data_val VARCHAR2(1024);
17:13:54 717  var_offer_chain_full_refund   NUMBER;
17:13:54 718  var_offer_chain_prorated_ref  NUMBER;
17:13:54 719  var_chargeback_calculated NUMBER;
17:13:54 720  var_max_invoice_refund	NUMBER;
17:13:54 721  -- EXCEPTIONS
17:13:54 722  BAD_INVOICE_ID		  EXCEPTION;
17:13:54 723  NO_LICENSES_FOUND_EXCEPTION EXCEPTION;
17:13:54 724  OFFER_LENGTH_IS_ZERO	  EXCEPTION;
17:13:54 725  BEGIN
17:13:54 726  
17:13:54 727  	IF in_chargeback_date IS NULL THEN
17:13:54 728  	  var_chargeback_date := PROCS_COMMON_V23.NORMALIZE_DATE(current_date);
17:13:54 729  	ELSE
17:13:54 730  	  var_chargeback_date := PROCS_COMMON_V23.NORMALIZE_DATE(in_chargeback_date);
17:13:54 731  	END IF;
17:13:54 732  
17:13:54 733  	-- Check that invoice exists
17:13:54 734  	BEGIN
17:13:54 735  	  SELECT
17:13:54 736  	    INVOICE.ID into temp_invoice_id
17:13:54 737  	  FROM
17:13:54 738  	    INVOICE
17:13:54 739  	  WHERE
17:13:54 740  	    INVOICE.ID = in_invoice_id;
17:13:54 741  	  EXCEPTION
17:13:54 742  	    WHEN NO_DATA_FOUND THEN
17:13:54 743  	      RAISE BAD_INVOICE_ID;
17:13:54 744  	END;
17:13:54 745  
17:13:54 746  	-- Check that invoice has at least one license
17:13:54 747  	SELECT
17:13:54 748  	  COUNT(*) into var_licenses_number
17:13:54 749  	FROM
17:13:54 750  	  LICENSE
17:13:54 751  	WHERE
17:13:54 752  	  LICENSE.INVOICE_ID = in_invoice_id;
17:13:54 753  
17:13:54 754  	IF var_licenses_number = 0 THEN
17:13:54 755  	  RAISE NO_LICENSES_FOUND_EXCEPTION;
17:13:54 756  	END IF;
17:13:54 757  
17:13:54 758  	SELECT
17:13:54 759  	  MIN(LICENSE.START_DATE) into var_invoice_start_date
17:13:54 760  	FROM
17:13:54 761  	  LICENSE
17:13:54 762  	WHERE
17:13:54 763  	  LICENSE.INVOICE_ID = in_invoice_id;
17:13:54 764  
17:13:54 765  	var_invoice_start_date := PROCS_COMMON_V23.NORMALIZE_DATE(var_invoice_start_date);
17:13:54 766  
17:13:54 767  	SELECT DISTINCT
17:13:54 768  	  LICENSE.OFFER_ID into var_offer_id
17:13:54 769  	FROM
17:13:54 770  	  LICENSE
17:13:54 771  	WHERE
17:13:54 772  	  LICENSE.INVOICE_ID = in_invoice_id;
17:13:54 773  
17:13:54 774  	SELECT
17:13:54 775  	  MAX (LICENSE.END_DATE) into var_invoice_end_date
17:13:54 776  	FROM
17:13:54 777  	  LICENSE
17:13:54 778  	WHERE
17:13:54 779  	  LICENSE.INVOICE_ID = in_invoice_id;
17:13:54 780  
17:13:54 781  	-- All licenses for given invoice should point into the same offer
17:13:54 782  
17:13:54 783  	PROCS_OFFER_CHAIN_V23.GET_OFFER_LENGTH_IN_DAYS(
17:13:54 784  	  in_offer_id	=> var_offer_id,
17:13:54 785  	  in_start_date => var_invoice_start_date,
17:13:54 786  	  out_days	=> var_offer_days_interval
17:13:54 787  	);
17:13:54 788  
17:13:54 789  	IF var_offer_days_interval = 0 THEN
17:13:54 790  	  RAISE OFFER_LENGTH_IS_ZERO;
17:13:54 791  	END IF;
17:13:54 792  
17:13:54 793  	PROCS_INVOICE_V23.GET_INVOICE_DAYS_USED_NUMBER(
17:13:54 794  	  in_invoice_id      => in_invoice_id,
17:13:54 795  	  in_chargeback_date => var_chargeback_date,
17:13:54 796  	  out_days_num	     => var_license_days_used
17:13:54 797  	);
17:13:54 798  
17:13:54 799  	PROCS_INVOICE_V23.CALCULATE_INVOICE_AMOUNT(
17:13:54 800  	  in_invoice_id => in_invoice_id,
17:13:54 801  	  out_amount	=> var_invoice_amount
17:13:54 802  	);
17:13:54 803  
17:13:54 804  	-- 39437
17:13:54 805  	-- Get offer chain id by invoice id
17:13:54 806  	SELECT DISTINCT
17:13:54 807  	  SUBSCRIPTION.OFFER_CHAIN_ID into var_offer_chain_id
17:13:54 808  	FROM
17:13:54 809  	  SUBSCRIPTION
17:13:54 810  	  INNER JOIN LICENSE ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
17:13:54 811  	WHERE
17:13:54 812  	  LICENSE.INVOICE_ID = in_invoice_id;
17:13:54 813  
17:13:54 814  	-- Get offer chain's meta data full amount value
17:13:54 815  	PROCS_OFFER_CHAIN_V23.GET_OFFER_CHAIN_MD_VALUE(
17:13:54 816  	  in_offer_chain_id => var_offer_chain_id,
17:13:54 817  	  in_meta_data_name => GLOBAL_CONSTANTS_V23.OFFER_CHAIN_FULL_REFUND,
17:13:54 818  	  out_value	    => var_offer_chain_meta_data_val
17:13:54 819  	);
17:13:54 820  	IF var_offer_chain_meta_data_val IS NULL THEN
17:13:54 821  	  var_offer_chain_full_refund := NULL;
17:13:54 822  	ELSE
17:13:54 823  	  var_offer_chain_full_refund := TO_NUMBER(var_offer_chain_meta_data_val);
17:13:54 824  	END IF;
17:13:54 825  
17:13:54 826  	-- Get offer chain's meta data prorated amount value
17:13:54 827  	PROCS_OFFER_CHAIN_V23.GET_OFFER_CHAIN_MD_VALUE(
17:13:54 828  	  in_offer_chain_id => var_offer_chain_id,
17:13:54 829  	  in_meta_data_name => GLOBAL_CONSTANTS_V23.OFFER_CHAIN_PRORATED_REFUND,
17:13:54 830  	  out_value	    => var_offer_chain_meta_data_val
17:13:54 831  	);
17:13:54 832  	IF var_offer_chain_meta_data_val IS NULL THEN
17:13:54 833  	  var_offer_chain_prorated_ref := NULL;
17:13:54 834  	ELSE
17:13:54 835  	  var_offer_chain_prorated_ref := TO_NUMBER(var_offer_chain_meta_data_val);
17:13:54 836  	END IF;
17:13:54 837  
17:13:54 838  	var_chargeback_calculated := GLOBAL_CONSTANTS_V23.FALSE;
17:13:54 839  
17:13:54 840  	IF var_offer_chain_full_refund IS NOT NULL
17:13:54 841  	   AND var_chargeback_calculated = GLOBAL_CONSTANTS_V23.FALSE THEN
17:13:54 842  	  IF var_license_days_used < var_offer_chain_full_refund THEN
17:13:54 843  	    out_chargeback_amount := var_invoice_amount;
17:13:54 844  	    var_chargeback_calculated := GLOBAL_CONSTANTS_V23.TRUE;
17:13:54 845  	  END IF;
17:13:54 846  	END IF;
17:13:54 847  
17:13:54 848  	IF var_offer_chain_prorated_ref IS NOT NULL
17:13:54 849  	   AND var_chargeback_calculated = GLOBAL_CONSTANTS_V23.FALSE THEN
17:13:54 850  	  IF var_license_days_used < var_offer_chain_prorated_ref THEN
17:13:54 851  	    out_chargeback_amount := ( var_invoice_amount * (var_offer_days_interval - var_license_days_used) ) / var_offer_days_interval;
17:13:54 852  	    var_chargeback_calculated := GLOBAL_CONSTANTS_V23.TRUE;
17:13:54 853  	  END IF;
17:13:54 854  	END IF;
17:13:54 855  
17:13:54 856  	IF var_chargeback_calculated = GLOBAL_CONSTANTS_V23.TRUE THEN
17:13:54 857  	  PROCS_INVOICE_V23.GET_MAX_REFUND(
17:13:54 858  	    in_invoice_id => in_invoice_id,
17:13:54 859  	    out_amount	  => var_max_invoice_refund
17:13:54 860  	  );
17:13:54 861  	  IF var_max_invoice_refund < out_chargeback_amount THEN
17:13:54 862  	    out_chargeback_amount := var_max_invoice_refund;
17:13:54 863  	  END IF;
17:13:54 864  	END IF;
17:13:54 865  
17:13:54 866  	IF var_chargeback_calculated = GLOBAL_CONSTANTS_V23.FALSE THEN
17:13:54 867  	  out_chargeback_amount := 0;
17:13:54 868  	END IF;
17:13:54 869  
17:13:54 870  EXCEPTION
17:13:54 871  WHEN BAD_INVOICE_ID THEN
17:13:54 872  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 873  	  SPROC_NAME, 'No such invoice');
17:13:54 874  WHEN NO_LICENSES_FOUND_EXCEPTION THEN
17:13:54 875  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 876  	  SPROC_NAME, 'No licenses found');
17:13:54 877  WHEN OFFER_LENGTH_IS_ZERO THEN
17:13:54 878  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 879  	  SPROC_NAME, 'Offer length is zero');
17:13:54 880  WHEN OTHERS THEN
17:13:54 881  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 882  	  SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 883  END CALCULATE_INVOICE_CHARGEBACK;
