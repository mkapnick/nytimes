17:13:54 830  	      RAISE CAN_NOT_CREATE_INVOICE;
17:13:54 831  	    END;
17:13:54 832  
17:13:54 833  -- Add line items for new invoice
17:13:54 834  	    BEGIN
17:13:54 835  	      FOR f_offer_data IN (
17:13:54 836  	      SELECT
17:13:54 837  		OFFER_ID,
17:13:54 838  		NUM_RECURRENCES
17:13:54 839  	      FROM
17:13:54 840  		OFFER_OFFER_CHAIN
17:13:54 841  	      WHERE
17:13:54 842  		OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
17:13:54 843  	      )
17:13:54 844  	      LOOP
17:13:54 845  	      IF f_offer_data.NUM_RECURRENCES < 1 THEN
17:13:54 846  		RAISE OFFER_REC_NUM_LESS_THAN_ONE;
17:13:54 847  	      END IF;
17:13:54 848  	      FOR i_offer_recurrences_iterator IN 1..f_offer_data.NUM_RECURRENCES
17:13:54 849  	      LOOP
17:13:54 850  		PROCS_LINE_ITEMS_V23.ADD_LINE_ITEMS(
17:13:54 851  		    in_invoice_id => var_new_invoice_id,
17:13:54 852  		    in_offer_id   => f_offer_data.OFFER_ID,
17:13:54 853  		    in_created_by => in_created_by
17:13:54 854  		);
17:13:54 855  	      END LOOP;
17:13:54 856  	      END LOOP;
17:13:54 857  
17:13:54 858  	      EXCEPTION
17:13:54 859  	      WHEN OTHERS THEN
17:13:54 860  	      EXCEPTION_MESSAGE := SQLERRM;
17:13:54 861  	      RAISE CAN_NOT_CREATE_LINE_ITEMS;
17:13:54 862  	    END;
17:13:54 863  
17:13:54 864  	    BEGIN
17:13:54 865  	      PROCS_FIN_INSTRUMENTS_CRU_V23.CREATE_GIFT_CERTIFICATE(
17:13:54 866  		  out_gift_certificate_id	=> var_gift_cert_id,
17:13:54 867  		  in_purchaser_group_id 	=> in_group_id,
17:13:54 868  		  in_purchaser_invoice_id	=> var_new_invoice_id,
17:13:54 869  		  in_offer_chain_id		=> in_offer_chain_id,
17:13:54 870  		  in_expiration_date		=> in_expiration_date,
17:13:54 871  		  in_purchase_date		=> SYSDATE,
17:13:54 872  		  in_gift_certificate_status_id => GLOBAL_STATUSES_V23.GIFT_CERTIFICATE_ACTIVE,
17:13:54 873  		  in_code			=> in_gift_certificate_code,
17:13:54 874  		  in_created_by 		=> in_created_by,
17:13:54 875  		  in_recipient_name		=> in_recipient_name,
17:13:54 876  		  in_gift_message		=> in_gift_message,
17:13:54 877  		  in_recipient_email		=> in_recipient_email,
17:13:54 878  		  in_sender_email		=> in_sender_email,
17:13:54 879  		  in_sender_name		=> in_sender_name,
17:13:54 880  		  in_recipient_address_id	=> in_recipient_address_id,
17:13:54 881  		  in_recipient_notify_date	=> in_recipient_notify_date,
17:13:54 882  		  in_campaign			=> in_campaign,
17:13:54 883  		  in_reference_code		=> in_reference_code
17:13:54 884  	      );
17:13:54 885  	      EXCEPTION WHEN DUP_VAL_ON_INDEX THEN RAISE REF_CODE_ALREADY_EXISTS;
17:13:54 886  	    END;
17:13:54 887  
17:13:54 888  	    out_gift_certificate_id := var_gift_cert_id;
17:13:54 889  	    out_invoice_id := var_new_invoice_id;
17:13:54 890  
17:13:54 891  	    EXCEPTION
17:13:54 892  	    WHEN BAD_OFFER_CHAIN_STATUS THEN
17:13:54 893  	    PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
17:13:54 894  					     SPROC_NAME, 'Offer chain is not active');
17:13:54 895  	    WHEN GC_CODE_ALREADY_EXISTS THEN
17:13:54 896  	    PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.DUPLICATE_ERROR,
17:13:54 897  					     SPROC_NAME, 'Gift certificate with same code already exists');
17:13:54 898  	    WHEN OCH_IS_NOT_GIFT_CERTIFICATE THEN
17:13:54 899  	    PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INVALID_PARAMETER,
17:13:54 900  					     SPROC_NAME, 'This offer chain can not be used for gift certificate');
17:13:54 901  	    WHEN CAN_NOT_PURCHASE_GC_FOR_RDMPN THEN
17:13:54 902  	    PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INVALID_PARAMETER,
17:13:54 903  					     SPROC_NAME, 'This offer chain can not be purchased for gift certificate since it is for redemption');
17:13:54 904  	    WHEN CAN_NOT_CREATE_INVOICE THEN
17:13:54 905  	    PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 906  					     SPROC_NAME, 'Could not create new invoice', EXCEPTION_MESSAGE);
17:13:54 907  	    WHEN CAN_NOT_CREATE_LINE_ITEMS THEN
17:13:54 908  	    PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 909  					     SPROC_NAME, 'Could not create line items', EXCEPTION_MESSAGE);
17:13:54 910  	    WHEN BAD_GROUP_ID THEN
