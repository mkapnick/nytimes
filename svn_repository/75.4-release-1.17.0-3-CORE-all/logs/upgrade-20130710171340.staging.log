17:13:54 1727  PROCEDURE CHECK_PRODUCT_ELIGIBILITY (
17:13:54 1728  	 in_group_id	   IN NUMBER,
17:13:54 1729  	 in_offer_chain_id IN NUMBER,
17:13:54 1730  	 out_is_eligible   OUT NUMBER,
17:13:54 1731  	 out_concurrent_subscription_id OUT NUMBER
17:13:54 1732  ) AS
17:13:54 1733  SPROC_NAME CONSTANT VARCHAR2(25) := 'CHECK_PRODUCT_ELIGIBILITY';
17:13:54 1734  -- Variables
17:13:54 1735  var_account_id  NUMBER;
17:13:54 1736  var_is_eligible NUMBER;
17:13:54 1737  var_is_gc       NUMBER;
17:13:54 1738  -- Exceptions
17:13:54 1739  BAD_GROUP_ID		   EXCEPTION;
17:13:54 1740  CAN_NOT_CHECK_SAME_PRODUCTS EXCEPTION;
17:13:54 1741  BAD_OC_ID		   EXCEPTION;
17:13:54 1742  EXCEPTION_MESSAGE	   VARCHAR(1024);
17:13:54 1743  BEGIN
17:13:54 1744  
17:13:54 1745  	 var_is_eligible := GLOBAL_CONSTANTS_V23.TRUE;
17:13:54 1746  
17:13:54 1747  	 out_concurrent_subscription_id := NULL;
17:13:54 1748  
17:13:54 1749  	 BEGIN
17:13:54 1750  	   SELECT
17:13:54 1751  	     OC.IS_GIFT_CERTIFICATE into var_is_gc
17:13:54 1752  	   FROM
17:13:54 1753  	     OFFER_CHAIN OC
17:13:54 1754  	   WHERE
17:13:54 1755  	     OC.ID = in_offer_chain_id;
17:13:54 1756  	   EXCEPTION
17:13:54 1757  	     WHEN NO_DATA_FOUND THEN
17:13:54 1758  	       RAISE BAD_OC_ID;
17:13:54 1759  	 END;
17:13:54 1760  
17:13:54 1761  	 -- only check eligibility if this is not a gift certificate
17:13:54 1762  	 IF (var_is_gc IS NULL OR var_is_gc != 1) THEN
17:13:54 1763  	   BEGIN
17:13:54 1764  	     SELECT
17:13:54 1765  	       A.ID into var_account_id
17:13:54 1766  	     FROM
17:13:54 1767  	       ACCOUNT A
17:13:54 1768  	     WHERE
17:13:54 1769  	       A.GROUP_ID = in_group_id;
17:13:54 1770  	     EXCEPTION
17:13:54 1771  	       WHEN NO_DATA_FOUND THEN
17:13:54 1772  		 RAISE BAD_GROUP_ID;
17:13:54 1773  	   END;
17:13:54 1774  
17:13:54 1775  
17:13:54 1776  	   FOR f_offer_chain IN (
17:13:54 1777  	       SELECT
17:13:54 1778  		 S.ID as SUBSCRIPTION_ID,
17:13:54 1779  		 S.OFFER_CHAIN_ID
17:13:54 1780  	       FROM
17:13:54 1781  		 SUBSCRIPTION S
17:13:54 1782  	       WHERE
17:13:54 1783  		 S.ACCOUNT_ID = var_account_id
17:13:54 1784  		 AND (S.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V23.SUBSCRIPTION_ACTIVE
17:13:54 1785  		      OR S.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V23.SUBSCRIPTION_IN_GRACE_PERIOD)
17:13:54 1786  	   )
17:13:54 1787  	   LOOP
17:13:54 1788  	     BEGIN
17:13:54 1789  	       IF (
17:13:54 1790  		 PROCS_OFFER_CHAIN_V23.CHECK_FOR_SAME_PRODUCTS(
17:13:54 1791  		   in_offer_chain_id,
17:13:54 1792  		   f_offer_chain.offer_chain_id,
17:13:54 1793  		   GLOBAL_CONSTANTS_V23.TRUE
17:13:54 1794  		 ) = GLOBAL_CONSTANTS_V23.TRUE
17:13:54 1795  	       ) THEN
17:13:54 1796  		 var_is_eligible := GLOBAL_CONSTANTS_V23.FALSE;
17:13:54 1797  		 out_concurrent_subscription_id := f_offer_chain.SUBSCRIPTION_ID;
17:13:54 1798  	       END IF;
17:13:54 1799  	       EXCEPTION
17:13:54 1800  		 WHEN OTHERS THEN
17:13:54 1801  		   EXCEPTION_MESSAGE := SQLERRM;
17:13:54 1802  		   RAISE CAN_NOT_CHECK_SAME_PRODUCTS;
17:13:54 1803  	     END;
17:13:54 1804  	   END LOOP;
17:13:54 1805  	 END IF;
17:13:54 1806  	 out_is_eligible := var_is_eligible;
17:13:54 1807  
17:13:54 1808  EXCEPTION
17:13:54 1809  WHEN BAD_GROUP_ID THEN
17:13:54 1810  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 1811  	   SPROC_NAME, 'No such offer chain', SQLERRM);
17:13:54 1812  WHEN BAD_OC_ID THEN
17:13:54 1813  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 1814  	   SPROC_NAME, 'No such offer chain', SQLERRM);
17:13:54 1815  WHEN CAN_NOT_CHECK_SAME_PRODUCTS THEN
17:13:54 1816  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 1817  	   SPROC_NAME, 'Could not check offers for same products', EXCEPTION_MESSAGE);
17:13:54 1818  WHEN OTHERS THEN
17:13:54 1819  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 1820  	   SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 1821  END CHECK_PRODUCT_ELIGIBILITY;
