17:13:54 960  FUNCTION GET_TRANSACTION_TAX_AMOUNT (
17:13:54 961  	in_transaction_id IN NUMBER
17:13:54 962  ) RETURN NUMBER AS
17:13:54 963  SPROC_NAME CONSTANT VARCHAR2(26) := 'GET_TRANSACTION_TAX_AMOUNT';
17:13:54 964  -- Variables
17:13:54 965  var_tax_amount NUMBER(10,2);
17:13:54 966  BEGIN
17:13:54 967  
17:13:54 968  	SELECT
17:13:54 969  	  SUM(LI.TAXES_AMOUNT) into var_tax_amount
17:13:54 970  	FROM
17:13:54 971  	  LINE_ITEM LI
17:13:54 972  	  INNER JOIN INVOICE I ON LI.INVOICE_ID = I.ID
17:13:54 973  	  INNER JOIN CHARGE CH ON CH.INVOICE_ID = I.ID
17:13:54 974  	WHERE
17:13:54 975  	  CH.TRANSACTION_ID = in_transaction_id;
17:13:54 976  
17:13:54 977  	IF var_tax_amount IS NULL THEN
17:13:54 978  	  var_tax_amount := 0;
17:13:54 979  	END IF;
17:13:54 980  
17:13:54 981  	RETURN var_tax_amount;
17:13:54 982  
17:13:54 983  END GET_TRANSACTION_TAX_AMOUNT;
