17:13:54 1998  PROCEDURE IS_REVOKE_ENTITLEMENTS(
17:13:54 1999  	 in_invoice_id IN NUMBER,
17:13:54 2000  	 out_is_revoke OUT NUMBER
17:13:54 2001  ) AS
17:13:54 2002  BEGIN
17:13:54 2003  	 SELECT DECODE(COUNT(1), 0, GLOBAL_CONSTANTS_V23.FALSE, GLOBAL_CONSTANTS_V23.TRUE)
17:13:54 2004  	   into out_is_revoke
17:13:54 2005  	 FROM
17:13:54 2006  	   offer_chain oc,
17:13:54 2007  	   subscription s,
17:13:54 2008  	   license l,
17:13:54 2009  	   invoice i
17:13:54 2010  	 where
17:13:54 2011  	   oc.id = s.offer_chain_id and
17:13:54 2012  	   s.id = l.subscription_id and
17:13:54 2013  	   l.invoice_id = i.id and
17:13:54 2014  	   oc.revoke_entitlements = GLOBAL_CONSTANTS_V23.TRUE and
17:13:54 2015  	   i.id = in_invoice_id and
17:13:54 2016  	   rownum < 2
17:13:54 2017  	 ;
17:13:54 2018  END IS_REVOKE_ENTITLEMENTS;
