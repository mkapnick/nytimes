17:13:54 1929  PROCEDURE GET_PAYMENT_INFO_BY_INVOICE_ID (
17:13:54 1930  	 in_invoice_id		     IN NUMBER,
17:13:54 1931  	 out_order_id		     OUT VARCHAR2,
17:13:54 1932  	 out_external_transaction_id OUT VARCHAR2
17:13:54 1933  ) AS
17:13:54 1934  SPROC_NAME CONSTANT VARCHAR2(30) := 'GET_PEYMENT_INFO_BY_INVOICE_ID';
17:13:54 1935  -- VARIABLES
17:13:54 1936  temp_invoice_id number;
17:13:54 1937  cnt_matched_instr number := 0;
17:13:54 1938  -- EXCEPTIONS
17:13:54 1939  BAD_INVOICE_ID EXCEPTION;
17:13:54 1940  BEGIN
17:13:54 1941  
17:13:54 1942  	 BEGIN
17:13:54 1943  	   SELECT
17:13:54 1944  	     i.id into temp_invoice_id
17:13:54 1945  	   from
17:13:54 1946  	     invoice i
17:13:54 1947  	   where
17:13:54 1948  	     i.id = in_invoice_id;
17:13:54 1949  	   EXCEPTION
17:13:54 1950  	     WHEN NO_DATA_FOUND THEN
17:13:54 1951  	       RAISE BAD_INVOICE_ID;
17:13:54 1952  	 END;
17:13:54 1953  
17:13:54 1954  	 select
17:13:54 1955  	   count(1) into cnt_matched_instr
17:13:54 1956  	 from
17:13:54 1957  	   charge ch
17:13:54 1958  	 inner join
17:13:54 1959  	   subscription s
17:13:54 1960  	 on
17:13:54 1961  	   s.instrument_id = ch.instrument_id
17:13:54 1962  	 where
17:13:54 1963  	   ch.invoice_id = in_invoice_id;
17:13:54 1964  
17:13:54 1965  	 if cnt_matched_instr = 0 then
17:13:54 1966  	   out_external_transaction_id := null;
17:13:54 1967  	   out_order_id := null;
17:13:54 1968  	   return;
17:13:54 1969  	 end if;
17:13:54 1970  
17:13:54 1971  	 SELECT
17:13:54 1972  	   t.order_id,
17:13:54 1973  	   ta.external_transaction_id
17:13:54 1974  	   into
17:13:54 1975  	   out_order_id,
17:13:54 1976  	   out_external_transaction_id
17:13:54 1977  	 from
17:13:54 1978  	   charge ch
17:13:54 1979  	   inner join transaction t on ch.transaction_id = t.id
17:13:54 1980  	   inner join transaction_attempt ta on ta.transaction_id = t.id
17:13:54 1981  	 where
17:13:54 1982  	   ch.invoice_id = in_invoice_id
17:13:54 1983  	   and ta.transaction_attempt_status_id = GLOBAL_STATUSES_V23.TRANS_ATTEMPT_SUCCESS
17:13:54 1984  	   and ch.charge_amount > 0; -- We are not creating charges for the 0-amount invoices
17:13:54 1985  
17:13:54 1986  EXCEPTION
17:13:54 1987  WHEN BAD_INVOICE_ID THEN
17:13:54 1988  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 1989  	   SPROC_NAME, 'No such invoice');
17:13:54 1990  WHEN NO_DATA_FOUND THEN
17:13:54 1991  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 1992  	   SPROC_NAME, 'No payment data found');
17:13:54 1993  WHEN OTHERS THEN
17:13:54 1994  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 1995  	   SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 1996  END GET_PAYMENT_INFO_BY_INVOICE_ID;
