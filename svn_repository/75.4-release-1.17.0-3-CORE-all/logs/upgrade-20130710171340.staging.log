17:13:54 291  	  LICENSE.CURRENT_OFFER_INDEX,
17:13:54 292  	  LICENSE.CURRENT_OFFER_RECURR_NUM,
17:13:54 293  	  LICENSE.END_DATE,
17:13:54 294  	  LICENSE.ENTITLEMENT_END_DATE,
17:13:54 295  	  LICENSE.INVOICE_ID,
17:13:54 296  	  LICENSE.IS_EXTENSION,
17:13:54 297  	  LICENSE.LICENSE_STATUS_ID,
17:13:54 298  	  LICENSE.OFFER_ID,
17:13:54 299  	  LICENSE.START_DATE,
17:13:54 300  	  LICENSE.SUBSCRIPTION_ID,
17:13:54 301  	  LICENSE.UPDATE_DATE,
17:13:54 302  	  LICENSE.UPDATED_BY
17:13:54 303  	FROM
