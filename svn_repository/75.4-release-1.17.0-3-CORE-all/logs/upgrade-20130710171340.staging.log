17:13:54 526  FUNCTION CALCULATE_OFFER_AMOUNT (
17:13:54 527  	in_offer_id IN NUMBER
17:13:54 528  ) RETURN NUMBER AS
17:13:54 529  -- VARIABLES
17:13:54 530  SPROC_NAME    CONSTANT VARCHAR2(22) := 'CALCULATE_OFFER_AMOUNT';
17:13:54 531  temp_offer_id NUMBER;
17:13:54 532  
17:13:54 533  var_product_offering_set	    SYS_REFCURSOR;
17:13:54 534  var_product_offering_id	    NUMBER;
17:13:54 535  var_product_offering_price    NUMBER(10,6);
17:13:54 536  var_product_offering_t_amount NUMBER(10,6);
17:13:54 537  var_product_offering_quantity NUMBER;
17:13:54 538  
17:13:54 539  var_total_amount NUMBER(10,6);
17:13:54 540  var_final_amount NUMBER(10,2);
17:13:54 541  
17:13:54 542  var_percent_discount NUMBER(10,2);
17:13:54 543  var_fixed_discount NUMBER(10,6);
17:13:54 544  
17:13:54 545  -- EXCEPTIONS
17:13:54 546  BAD_OFFER_ID EXCEPTION;
17:13:54 547  BEGIN
17:13:54 548  
17:13:54 549  	BEGIN
17:13:54 550  	  SELECT
17:13:54 551  	    OFFER.ID into temp_offer_id
17:13:54 552  	  FROM
17:13:54 553  	    OFFER
17:13:54 554  	  WHERE
17:13:54 555  	    OFFER.ID = in_offer_id;
17:13:54 556  	  EXCEPTION
17:13:54 557  	    WHEN NO_DATA_FOUND THEN
17:13:54 558  	      RAISE BAD_OFFER_ID;
17:13:54 559  	END;
17:13:54 560  
17:13:54 561  	OPEN var_product_offering_set FOR
17:13:54 562  	SELECT
17:13:54 563  	  OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID,
17:13:54 564  	  PRODUCT_OFFERING.UNIT_PRICE,
17:13:54 565  	  PRODUCT_OFFERING.QUANTITY
17:13:54 566  	FROM
17:13:54 567  	  OFFER_PRODUCT_OFFERING
17:13:54 568  	  INNER JOIN PRODUCT_OFFERING ON OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = PRODUCT_OFFERING.ID
17:13:54 569  	WHERE
17:13:54 570  	  OFFER_PRODUCT_OFFERING.OFFER_ID = in_offer_id;
17:13:54 571  
17:13:54 572  	var_total_amount := 0;
17:13:54 573  
17:13:54 574  	LOOP
17:13:54 575  	  FETCH var_product_offering_set into
17:13:54 576  	    var_product_offering_id,
17:13:54 577  	    var_product_offering_price,
17:13:54 578  	    var_product_offering_quantity;
17:13:54 579  	  EXIT WHEN var_product_offering_set%NOTFOUND;
17:13:54 580  
17:13:54 581  	  SELECT
17:13:54 582  	    SUM(DISCOUNT.FIXED_AMOUNT) into var_fixed_discount
17:13:54 583  	  FROM
17:13:54 584  	    DISCOUNT_PRODUCT_OFFERING
17:13:54 585  	    INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
17:13:54 586  	  WHERE
17:13:54 587  	    DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
17:13:54 588  	    AND DISCOUNT.FIXED_AMOUNT IS NOT NULL;
17:13:54 589  
17:13:54 590  	  SELECT
17:13:54 591  	    SUM(DISCOUNT.PERCENT_AMOUNT) into var_percent_discount
17:13:54 592  	  FROM
17:13:54 593  	    DISCOUNT_PRODUCT_OFFERING
17:13:54 594  	    INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
17:13:54 595  	  WHERE
17:13:54 596  	    DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
17:13:54 597  	    AND DISCOUNT.PERCENT_AMOUNT IS NOT NULL;
17:13:54 598  
17:13:54 599  	  var_product_offering_t_amount := var_product_offering_price * var_product_offering_quantity;
17:13:54 600  
17:13:54 601  	  IF var_percent_discount IS NOT NULL THEN
17:13:54 602  	    var_product_offering_t_amount := var_product_offering_t_amount - ( var_product_offering_t_amount * var_percent_discount / 100 );
17:13:54 603  	  END IF;
17:13:54 604  
17:13:54 605  	  IF var_fixed_discount IS NOT NULL THEN
17:13:54 606  	    var_product_offering_t_amount := var_product_offering_t_amount - var_fixed_discount;
17:13:54 607  	  END IF;
17:13:54 608  
17:13:54 609  	  var_total_amount := var_total_amount + var_product_offering_t_amount;
17:13:54 610  	END LOOP;
17:13:54 611  	var_final_amount := var_total_amount;
17:13:54 612  	RETURN var_final_amount;
17:13:54 613  
17:13:54 614  EXCEPTION
17:13:54 615  WHEN BAD_OFFER_ID THEN
17:13:54 616  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 617  	  SPROC_NAME, 'No such offer');
17:13:54 618  WHEN OTHERS THEN
17:13:54 619  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 620  	  SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 621  END CALCULATE_OFFER_AMOUNT;
