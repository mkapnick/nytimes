17:13:54 1094  temp_subscription_id NUMBER;
17:13:54 1095  var_offer_duration   VARCHAR2(30);
17:13:54 1096  
17:13:54 1097  -- EXCEPTIONS
17:13:54 1098  BAD_SUBSCRIPTION_ID EXCEPTION;
17:13:54 1099  BEGIN
17:13:54 1100  
17:13:54 1101  	 BEGIN
17:13:54 1102  	   SELECT
17:13:54 1103  	     SUBSCRIPTION.ID into temp_subscription_id
