17:13:54 2498  EXCEPTION_MESSAGE   VARCHAR2(1024);
17:13:54 2499  BEGIN
17:13:54 2500  
17:13:54 2501  	 BEGIN
17:13:54 2502  	   -- find offer_chain_id for given in_subscription_id
17:13:54 2503  	   SELECT OFFER_CHAIN_ID into offer_id
17:13:54 2504  	   FROM SUBSCRIPTION
17:13:54 2505  	   WHERE ID = in_subscription_id;
17:13:54 2506  	   EXCEPTION
17:13:54 2507  	     WHEN NO_DATA_FOUND THEN
17:13:54 2508  	       RAISE BAD_SUBSCRIPTION_ID;
17:13:54 2509  	     WHEN OTHERS THEN
17:13:54 2510  	       EXCEPTION_MESSAGE := SQLERRM;
17:13:54 2511  	       RAISE COULD_NOT_CHECK;
17:13:54 2512  	 END;
17:13:54 2513  
17:13:54 2514  	 -- find if it was redeemed from a gift certificate
17:13:54 2515  	 BEGIN
17:13:54 2516  	   SELECT l.end_date INTO var_end_date
17:13:54 2517  	   FROM LICENSE l, GIFT_CERTIFICATE g
17:13:54 2518  	   WHERE l.invoice_id = g.finalized_invoice_id
17:13:54 2519  	   AND l.subscription_id = in_subscription_id;
17:13:54 2520  
17:13:54 2521  	   -- if the license end_date is bigger than today, we are in the
17:13:54 2522  	   -- first period, so we cannot cancel; otherwise can cancel
17:13:54 2523  	   IF var_end_date > today_date THEN
17:13:54 2524  	       RETURN GLOBAL_CONSTANTS_V23.FALSE;
17:13:54 2525  	   ELSE
17:13:54 2526  	       RETURN GLOBAL_CONSTANTS_V23.TRUE;
17:13:54 2527  	   END IF;
17:13:54 2528  
17:13:54 2529  	   EXCEPTION
17:13:54 2530  	       -- not coming from a gift certificate,
17:13:54 2531  	       -- use old logic
17:13:54 2532  	       WHEN NO_DATA_FOUND THEN
17:13:54 2533  		   SELECT
17:13:54 2534  		       PROCS_OFFER_CHAIN_V23.IS_OFFER_CHAIN_CANCELABLE(offer_id)
17:13:54 2535  		       INTO var_is_offer_chain_cancelable
17:13:54 2536  		   FROM DUAL;
17:13:54 2537  		   RETURN var_is_offer_chain_cancelable;
17:13:54 2538  	 END;
17:13:54 2539  
17:13:54 2540  EXCEPTION
17:13:54 2541  WHEN BAD_SUBSCRIPTION_ID THEN
17:13:54 2542  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 2543  	   SPROC_NAME, 'No such subscription');
17:13:54 2544  WHEN COULD_NOT_CHECK THEN
17:13:54 2545  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 2546  	   SPROC_NAME, 'Could not check if offer chain calcelable', EXCEPTION_MESSAGE);
17:13:54 2547  WHEN OTHERS THEN
17:13:54 2548  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 2549  	   SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 2550  
17:13:54 2551  END IS_SUBSCRIPTION_CANCELABLE;
