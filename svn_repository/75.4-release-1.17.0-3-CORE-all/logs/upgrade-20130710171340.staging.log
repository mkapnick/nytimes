17:13:54 707  FUNCTION GET_NEXT_OFFER_INDEX (
17:13:54 708  	in_offer_chain_id	     IN NUMBER,
17:13:54 709  	in_offer_chain_current_index IN NUMBER
17:13:54 710  ) RETURN NUMBER AS
17:13:54 711  -- VARIABLES
17:13:54 712  SPROC_NAME		     CONSTANT VARCHAR2(20) := 'GET_NEXT_OFFER_INDEX';
17:13:54 713  temp_offer_chain_id	     NUMBER;
17:13:54 714  temp_offer_chain_current_index NUMBER;
17:13:54 715  var_result		     NUMBER;
17:13:54 716  -- EXCEPTIONS
17:13:54 717  BAD_OFFER_CHAIN_ID      EXCEPTION;
17:13:54 718  BAD_CURRENT_INDEX_VALUE EXCEPTION;
17:13:54 719  BEGIN
17:13:54 720  
17:13:54 721  	-- Check that offer chain exists
17:13:54 722  	BEGIN
17:13:54 723  	  SELECT
17:13:54 724  	    OFFER_CHAIN.ID into temp_offer_chain_id
17:13:54 725  	  FROM
17:13:54 726  	    OFFER_CHAIN
17:13:54 727  	  WHERE
17:13:54 728  	    OFFER_CHAIN.ID = in_offer_chain_id;
17:13:54 729  	  EXCEPTION
17:13:54 730  	    WHEN NO_DATA_FOUND THEN
17:13:54 731  	      RAISE BAD_OFFER_CHAIN_ID;
17:13:54 732  	END;
17:13:54 733  
17:13:54 734  	-- Check that current offer index exists
17:13:54 735  	BEGIN
17:13:54 736  	  SELECT
17:13:54 737  	    OFFER_OFFER_CHAIN.INDEX_VALUE into temp_offer_chain_current_index
17:13:54 738  	  FROM
17:13:54 739  	    OFFER_OFFER_CHAIN
17:13:54 740  	  WHERE
17:13:54 741  	    OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
17:13:54 742  	    AND OFFER_OFFER_CHAIN.INDEX_VALUE = in_offer_chain_current_index
17:13:54 743  	    -- TODO: delete next line
17:13:54 744  	    AND ROWNUM <= 1;
17:13:54 745  	  EXCEPTION
17:13:54 746  	    WHEN NO_DATA_FOUND THEN
17:13:54 747  	      RAISE BAD_CURRENT_INDEX_VALUE;
17:13:54 748  	END;
17:13:54 749  
17:13:54 750  	-- Get next offer index
17:13:54 751  	BEGIN
17:13:54 752  	  SELECT
17:13:54 753  	    INDEX_VALUE into var_result
17:13:54 754  	  FROM (
17:13:54 755  	    SELECT
17:13:54 756  	      OFFER_OFFER_CHAIN.INDEX_VALUE
17:13:54 757  	    FROM
17:13:54 758  	      OFFER_OFFER_CHAIN
17:13:54 759  	    WHERE
17:13:54 760  	      OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
17:13:54 761  	      AND OFFER_OFFER_CHAIN.INDEX_VALUE > in_offer_chain_current_index
17:13:54 762  	    ORDER BY
17:13:54 763  	      OFFER_OFFER_CHAIN.INDEX_VALUE ASC
17:13:54 764  	  )
17:13:54 765  	  WHERE
17:13:54 766  	    ROWNUM <= 1;
17:13:54 767  	  EXCEPTION
17:13:54 768  	    WHEN NO_DATA_FOUND THEN
17:13:54 769  	      var_result := NULL;
17:13:54 770  	END;
17:13:54 771  
17:13:54 772  	RETURN var_result;
17:13:54 773  
17:13:54 774  EXCEPTION
17:13:54 775  WHEN BAD_OFFER_CHAIN_ID THEN
17:13:54 776  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 777  	  SPROC_NAME, 'No such offer chain');
17:13:54 778  WHEN BAD_CURRENT_INDEX_VALUE THEN
17:13:54 779  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 780  	  SPROC_NAME, 'Bad current index value');
17:13:54 781  WHEN OTHERS THEN
17:13:54 782  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 783  	  SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 784  END GET_NEXT_OFFER_INDEX;
