17:13:54 144  	IF var_account_status = GLOBAL_STATUSES_V23.ACCOUNT_DISABLED THEN
17:13:54 145  	  RAISE CAN_NOT_CREATE_FOR_DISABLE;
17:13:54 146  	END IF;
17:13:54 147  
17:13:54 148  	-- Get offer chain status
17:13:54 149  	BEGIN
17:13:54 150  	  SELECT
17:13:54 151  	    OFFER_CHAIN.OFFER_CHAIN_STATUS_ID,
17:13:54 152  	    OFFER_CHAIN.IS_GIFT_CERTIFICATE
17:13:54 153  	    into
17:13:54 154  	    var_offer_chain_status_id,
17:13:54 155  	    var_is_gift_certificate
17:13:54 156  	  FROM
17:13:54 157  	    OFFER_CHAIN
17:13:54 158  	  WHERE
17:13:54 159  	    OFFER_CHAIN.ID = in_offer_chain_id;
17:13:54 160  
17:13:54 161  	  EXCEPTION
17:13:54 162  	    WHEN NO_DATA_FOUND THEN
17:13:54 163  	      RAISE BAD_OFFER_CHAIN;
17:13:54 164  	END;
17:13:54 165  
17:13:54 166  	-- Could not subscribe to inactive/disabled offer chain
17:13:54 167  	IF var_offer_chain_status_id != GLOBAL_STATUSES_V23.OFFER_CHAIN_ACTIVE THEN
17:13:54 168  	  RAISE BAD_OFFER_CHAIN_STATUS;
17:13:54 169  	END IF;
17:13:54 170  
17:13:54 171  	-- Can not subscribe to Offer Chain for a Gift Certfiicate
17:13:54 172  	IF var_is_gift_certificate = GLOBAL_CONSTANTS_V23.TRUE THEN
17:13:54 173  	  RAISE CAN_NOT_SUBSCRIBE_TO_GC;
17:13:54 174  	END IF;
17:13:54 175  
17:13:54 176  	-- check if the OC is for Redemption:
17:13:54 177  	SELECT
17:13:54 178  	  COUNT(*) into var_is_for_redemption
17:13:54 179  	FROM
17:13:54 180  	  OFFER_CHAIN_ELIGIBILITY
17:13:54 181  	WHERE
17:13:54 182  	  OFFER_CHAIN_ELIGIBILITY.OFFER_CHAIN_ID = in_offer_chain_id
17:13:54 183  	  AND OFFER_CHAIN_ELIGIBILITY.NAME = GLOBAL_CONSTANTS_V23.GIFT_CERTIFICATE_REQUIRED
17:13:54 184  	  AND OFFER_CHAIN_ELIGIBILITY.VALUE = GLOBAL_CONSTANTS_V23.ELIGIBILITY_FLAG_SET;
17:13:54 185  
17:13:54 186  	IF var_is_for_redemption > 0 THEN
17:13:54 187  	  RAISE CAN_NOT_SUBSCRIBE_TO_RGC;
17:13:54 188  	END IF;
17:13:54 189  
17:13:54 190  	SELECT
17:13:54 191  	  COUNT(*) into var_same_offer_chains_num
17:13:54 192  	FROM
17:13:54 193  	  SUBSCRIPTION
17:13:54 194  	WHERE
17:13:54 195  	  SUBSCRIPTION.ACCOUNT_ID = var_account_id
17:13:54 196  	  AND SUBSCRIPTION.OFFER_CHAIN_ID = in_offer_chain_id
17:13:54 197  	  AND (SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V23.SUBSCRIPTION_ACTIVE
17:13:54 198  	       OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V23.SUBSCRIPTION_IN_GRACE_PERIOD);
17:13:54 199  
17:13:54 200  	-- ELIGIBILITY LOGIC CHANGED TO:
17:13:54 201  	-- FOR EACH offer chain eligibility rule in OC:
17:13:54 202  	--   IF offer chain eligibility rule fails:
17:13:54 203  	--     deny purchase;
17:13:54 204  	--   END IF
17:13:54 205  	-- END FOR
17:13:54 206  	-- FOR EACH product eligibility rule in OC:
17:13:54 207  	--   IF product eligibilty rule fails:
17:13:54 208  	--     deny purchase;
17:13:54 209  	--   END IF
17:13:54 210  	-- END FOR
17:13:54 211  	-- allow purchase;
17:13:54 212  
17:13:54 213  	-- if user have any active existing subscriptions to the offer chain
17:13:54 214  	-- and if MAX_CONCURRENT_SUBS <= [user's subscription count for the offer chain]
17:13:54 215  	-- then deny purchase
17:13:54 216  	var_max_concurrent_subscrs := PROCS_OFFER_CHAIN_V23.GET_OFFER_CHAIN_MAX_CONC_SUBSC(in_offer_chain_id);
17:13:54 217  	IF var_max_concurrent_subscrs != GLOBAL_CONSTANTS_V23.INFINITY
17:13:54 218  	  AND var_max_concurrent_subscrs <= var_same_offer_chains_num THEN
17:13:54 219  	  -- Find first concurrent subscription id:
17:13:54 220  	  SELECT
17:13:54 221  	    ID into var_concur_subscription_id
17:13:54 222  	  FROM (
17:13:54 223  	    SELECT
17:13:54 224  	      ID
17:13:54 225  	    FROM
17:13:54 226  	      SUBSCRIPTION
17:13:54 227  	    WHERE
17:13:54 228  	      SUBSCRIPTION.ACCOUNT_ID = var_account_id
17:13:54 229  	      AND SUBSCRIPTION.OFFER_CHAIN_ID = in_offer_chain_id
17:13:54 230  	      AND (SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V23.SUBSCRIPTION_ACTIVE
17:13:54 231  		   OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V23.SUBSCRIPTION_IN_GRACE_PERIOD)
17:13:54 232  	    ORDER BY
17:13:54 233  	      ID
17:13:54 234  	  )
17:13:54 235  	  WHERE
17:13:54 236  	    ROWNUM <= 1;
17:13:54 237  	  RAISE LIMIT_REACHED;
17:13:54 238  	END IF;
17:13:54 239  
17:13:54 240  	-- if user does not have any active existing subscriptions to the offer chain
17:13:54 241  	-- and if product from the offer chain is already owned from another offer chain
17:13:54 242  	-- then deny purchase
17:13:54 243  	IF (in_check_dupe_products != GLOBAL_CONSTANTS_V23.FALSE) THEN
17:13:54 244  	  FOR f_account_offer_chains IN (
17:13:54 245  	    SELECT DISTINCT
17:13:54 246  	      OFFER_CHAIN_ID
17:13:54 247  	    FROM
17:13:54 248  	      SUBSCRIPTION
17:13:54 249  	    WHERE
17:13:54 250  	      ACCOUNT_ID = var_account_id
17:13:54 251  	      AND (
17:13:54 252  		SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V23.SUBSCRIPTION_ACTIVE
17:13:54 253  		OR SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V23.SUBSCRIPTION_SUSPENDED
17:13:54 254  		OR SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V23.SUBSCRIPTION_IN_GRACE_PERIOD)
17:13:54 255  	  )
17:13:54 256  	  LOOP
17:13:54 257  	    IF PROCS_OFFER_CHAIN_V23.CHECK_FOR_SAME_PRODUCTS(
17:13:54 258  	      in_offer_chain_1	       => in_offer_chain_id,
17:13:54 259  	      in_offer_chain_2	       => f_account_offer_chains.OFFER_CHAIN_ID,
17:13:54 260  	      in_use_eligibility_rules => GLOBAL_CONSTANTS_V23.TRUE
17:13:54 261  	    ) = GLOBAL_CONSTANTS_V23.TRUE THEN
17:13:54 262  
17:13:54 263  	      -- Find first concurrent subscription id:
17:13:54 264  	      SELECT
17:13:54 265  		ID into var_concur_subscription_id
17:13:54 266  	      FROM (
17:13:54 267  		SELECT
17:13:54 268  		  ID
17:13:54 269  		FROM
17:13:54 270  		  SUBSCRIPTION
17:13:54 271  		WHERE
17:13:54 272  		  SUBSCRIPTION.ACCOUNT_ID = var_account_id
17:13:54 273  		  AND SUBSCRIPTION.OFFER_CHAIN_ID = f_account_offer_chains.OFFER_CHAIN_ID
17:13:54 274  		  AND (
17:13:54 275  		    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V23.SUBSCRIPTION_ACTIVE
17:13:54 276  		    OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V23.SUBSCRIPTION_SUSPENDED
17:13:54 277  		    OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V23.SUBSCRIPTION_IN_GRACE_PERIOD)
17:13:54 278  		ORDER BY
17:13:54 279  		  ID
17:13:54 280  	      )
17:13:54 281  	      WHERE
17:13:54 282  		ROWNUM <= 1;
17:13:54 283  
17:13:54 284  	      RAISE PRODUCT_ALREADY_PURCHASED;
17:13:54 285  	    END IF;
17:13:54 286  	  END LOOP;
17:13:54 287  	END IF;
17:13:54 288  
17:13:54 289  	BEGIN
17:13:54 290  	  PROCS_OFFER_CHAIN_V23.GET_FIRST_OFFER(in_offer_chain_id, var_first_offer_id);
17:13:54 291  	  EXCEPTION
17:13:54 292  	    WHEN OTHERS THEN
17:13:54 293  	      EXCEPTION_MESSAGE := SQLERRM;
17:13:54 294  	      RAISE CAN_NOT_GET_FIRST_OFFER_CHAIN;
17:13:54 295  	END;
17:13:54 296  
17:13:54 297  	BEGIN
17:13:54 298  	  PROCS_INVOICE_V23.CREATE_INVOICE(
17:13:54 299  	    in_invoice_status => GLOBAL_STATUSES_V23.INVOICE_OPEN,
17:13:54 300  	    in_created_by     => in_created_by,
17:13:54 301  	    in_tax_exempt_id  => var_account_tax_exempt_id,
17:13:54 302  	    out_invoice_id    => var_new_invoice_id
17:13:54 303  	  );
17:13:54 304  	  EXCEPTION
17:13:54 305  	    WHEN OTHERS THEN
17:13:54 306  	      EXCEPTION_MESSAGE := SQLERRM;
17:13:54 307  	      RAISE CAN_NOT_CREATE_INVOICE;
17:13:54 308  	END;
17:13:54 309  
17:13:54 310  	BEGIN
17:13:54 311  	  PROCS_LINE_ITEMS_V23.ADD_LINE_ITEMS(
17:13:54 312  	    in_invoice_id => var_new_invoice_id,
17:13:54 313  	    in_offer_id   => var_first_offer_id,
17:13:54 314  	    in_created_by => in_created_by
17:13:54 315  	  );
17:13:54 316  	  EXCEPTION
17:13:54 317  	    WHEN OTHERS THEN
17:13:54 318  	      EXCEPTION_MESSAGE := SQLERRM;
17:13:54 319  	      RAISE CAN_NOT_CREATE_LINE_ITEMS;
17:13:54 320  	END;
17:13:54 321  
17:13:54 322  	PROCS_SUBSCRIPTION_CRU_V23.CREATE_SUBSCRIPTION(
17:13:54 323  	  out_subscription_id	    => var_new_subscription_id,
17:13:54 324  	  in_account_id 	    => var_account_id,
17:13:54 325  	  in_purchase_date	    => var_date,
17:13:54 326  	  in_offer_chain_id	    => in_offer_chain_id,
17:13:54 327  	  in_created_by 	    => in_created_by,
17:13:54 328  	  in_instrument_type_id     => in_instrument_type_id,
17:13:54 329  	  in_instrument_id	    => in_instrument_id,
17:13:54 330  	  in_subscription_status_id => GLOBAL_STATUSES_V23.SUBSCRIPTION_ACTIVE
17:13:54 331  	);
17:13:54 332  
17:13:54 333  	BEGIN
17:13:54 334  	  PROCS_SUBSCRIPTION_V23.ANNOTATE_SUBSCRIPTION(
17:13:54 335  	    in_subscription_id => var_new_subscription_id,
17:13:54 336  	    in_agent_id        => in_agent_id,
17:13:54 337  	    in_note	       => in_note,
17:13:54 338  	    in_created_by      => in_created_by
17:13:54 339  	  );
17:13:54 340  	  EXCEPTION
17:13:54 341  	   WHEN OTHERS THEN
17:13:54 342  	     EXCEPTION_MESSAGE := SQLERRM;
17:13:54 343  	     RAISE CAN_NOT_CREATE_NOTE;
17:13:54 344  	END;
17:13:54 345  
17:13:54 346  	BEGIN
17:13:54 347  	  PROCS_LICENSE_V23.CREATE_LICENSE(
17:13:54 348  	    in_status_id		=> GLOBAL_STATUSES_V23.LICENSE_ACTIVE,
17:13:54 349  	    in_needs_entitlements	=> GLOBAL_CONSTANTS_V23.TRUE,
17:13:54 350  	    in_start_date		=> var_date,
17:13:54 351  	    in_end_date 		=> NULL, -- Will be calculated automatically
17:13:54 352  	    in_offer_id 		=> var_first_offer_id,
17:13:54 353  	    in_subscription_id		=> var_new_subscription_id,
17:13:54 354  	    in_invoice_id		=> var_new_invoice_id,
17:13:54 355  	    in_created_by		=> in_created_by,
17:13:54 356  	    in_is_extension		=> GLOBAL_CONSTANTS_V23.FALSE,
17:13:54 357  	    in_current_offer_index	=> PROCS_OFFER_CHAIN_V23.GET_FIRST_OFFER_INDEX(in_offer_chain_id),
17:13:54 358  	    in_current_offer_recurr_num => 1,
17:13:54 359  	    out_license_id		=> out_new_license_id
17:13:54 360  	  );
17:13:54 361  	  EXCEPTION
17:13:54 362  	    WHEN OTHERS THEN
17:13:54 363  	      EXCEPTION_MESSAGE := SQLERRM;
17:13:54 364  	      RAISE CAN_NOT_CREATE_LICENSE;
17:13:54 365  	END;
17:13:54 366  
17:13:54 367  	out_subscription_id := var_new_subscription_id;
17:13:54 368  	out_invoice_id := var_new_invoice_id;
17:13:54 369  
17:13:54 370  EXCEPTION
17:13:54 371  WHEN BAD_OFFER_CHAIN_STATUS THEN
17:13:54 372  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
17:13:54 373  	  SPROC_NAME, 'Offer chain is not active');
17:13:54 374  WHEN LIMIT_REACHED THEN
17:13:54 375  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.CONFLICT_ERROR,
17:13:54 376  	  SPROC_NAME, 'Limit reached for given offer chain. Concurrent subscription id: ' || var_concur_subscription_id);
17:13:54 377  WHEN CAN_NOT_CREATE_FOR_DISABLE THEN
17:13:54 378  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
17:13:54 379  	  SPROC_NAME, 'Can not create subsscription for disabled account');
17:13:54 380  WHEN CAN_NOT_SUBSCRIBE_TO_GC THEN
17:13:54 381  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
17:13:54 382  	  SPROC_NAME, 'Can not subscribe to Offer Chain for Gift Certificate');
17:13:54 383  WHEN CAN_NOT_SUBSCRIBE_TO_RGC THEN
17:13:54 384  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
17:13:54 385  	  SPROC_NAME, 'Can not subscribe to Offer Chain that is for redemption');
17:13:54 386  WHEN BAD_OFFER_CHAIN THEN
17:13:54 387  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 388  	  SPROC_NAME, 'No such offer chain');
17:13:54 389  WHEN PRODUCT_ALREADY_PURCHASED THEN
17:13:54 390  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.CONFLICT_ERROR,
17:13:54 391  	  SPROC_NAME, 'User already subscribed to some product in given offer chain. Concurrent subscription id: ' || var_concur_subscription_id);
17:13:54 392  WHEN CAN_NOT_GET_FIRST_OFFER_CHAIN THEN
17:13:54 393  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 394  	  SPROC_NAME, 'Could not find first offer in offer chain', EXCEPTION_MESSAGE);
17:13:54 395  WHEN CAN_NOT_CREATE_INVOICE THEN
17:13:54 396  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 397  	  SPROC_NAME, 'Could not create invoice', EXCEPTION_MESSAGE);
17:13:54 398  WHEN CAN_NOT_CREATE_LINE_ITEMS THEN
17:13:54 399  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 400  	  SPROC_NAME, 'Could not create line items', EXCEPTION_MESSAGE);
17:13:54 401  WHEN CAN_NOT_CREATE_LICENSE THEN
17:13:54 402  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 403  	  SPROC_NAME, 'Could not create new license', EXCEPTION_MESSAGE);
17:13:54 404  WHEN BAD_GROUP_ID THEN
17:13:54 405  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 406  	  SPROC_NAME, 'Bad group id');
17:13:54 407  WHEN CAN_NOT_CREATE_NOTE THEN
17:13:54 408  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 409  	  SPROC_NAME, 'Could not annotate subscription', EXCEPTION_MESSAGE);
17:13:54 410  WHEN OTHERS THEN
17:13:54 411  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 412  	  SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 413  END START_SUBSCRIPTION_CREATION;
