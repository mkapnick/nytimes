17:13:54 2051  
17:13:54 2052  PROCEDURE CANCEL_SUBSCRIPTION_INVOICE (
17:13:54 2053  	 in_invoice_id	      IN NUMBER,
17:13:54 2054  	 in_updated_by	      IN VARCHAR2,
17:13:54 2055  	 in_refundable	      IN NUMBER DEFAULT GLOBAL_CONSTANTS_V23.FALSE
17:13:54 2056  ) AS
17:13:54 2057  SPROC_NAME CONSTANT VARCHAR2(27) := 'CANCEL_SUBSCRIPTION_INVOICE';
17:13:54 2058  -- VARIABLES
17:13:54 2059  temp_invoice_id	      INVOICE.ID%TYPE;
17:13:54 2060  var_chargeback_amount  NUMBER(10,2);
17:13:54 2061  ver_refund_charge_id   NUMBER;
17:13:54 2062  -- EXCEPTIONS
17:13:54 2063  BAD_INVOICE_ID		    EXCEPTION;
17:13:54 2064  CAN_NOT_CALCULATE_CHARGEBACK EXCEPTION;
17:13:54 2065  CAN_NOT_APPLY_CHARGEBACK     EXCEPTION;
17:13:54 2066  EXCEPTION_MESSAGE	    VARCHAR2(1024);
17:13:54 2067  -- STUB
17:13:54 2068  var_now DATE;
17:13:54 2069  var_revoke NUMBER;
17:13:54 2070  var_refund NUMBER;
17:13:54 2071  var_billed NUMBER;
17:13:54 2072  var_subscription_in_grace NUMBER;
17:13:54 2073  BEGIN
17:13:54 2074  
17:13:54 2075  	 var_now := sysdate;
17:13:54 2076  
17:13:54 2077  	 -- Check that invoice exists
17:13:54 2078  	 BEGIN
17:13:54 2079  	   SELECT
17:13:54 2080  	     INVOICE.ID into temp_invoice_id
17:13:54 2081  	   FROM
17:13:54 2082  	     INVOICE
17:13:54 2083  	   WHERE
17:13:54 2084  	     INVOICE.ID = in_invoice_id;
17:13:54 2085  	   EXCEPTION
17:13:54 2086  	     WHEN NO_DATA_FOUND THEN
17:13:54 2087  	       RAISE BAD_INVOICE_ID;
17:13:54 2088  	 END;
17:13:54 2089  
17:13:54 2090  	 select decode(count(1), 0, GLOBAL_CONSTANTS_V23.FALSE, GLOBAL_CONSTANTS_V23.TRUE) into var_revoke
17:13:54 2091  	 from license l, subscription s, offer_chain oc
17:13:54 2092  	 where
17:13:54 2093  	   l.subscription_id = s.id and
17:13:54 2094  	   s.offer_chain_id = oc.id and
17:13:54 2095  	   l.invoice_id = in_invoice_id and
17:13:54 2096  	   oc.revoke_entitlements = GLOBAL_CONSTANTS_V23.TRUE and
17:13:54 2097  	   rownum < 2;
17:13:54 2098  
17:13:54 2099  	 select
17:13:54 2100  	   decode(s.subscription_status_id, GLOBAL_STATUSES_V23.SUBSCRIPTION_IN_GRACE_PERIOD, 1, 0)
17:13:54 2101  	 into
17:13:54 2102  	   var_subscription_in_grace
17:13:54 2103  	 from license l, subscription s
17:13:54 2104  	 where
17:13:54 2105  	   l.subscription_id = s.id and
17:13:54 2106  	   l.invoice_id = in_invoice_id and
17:13:54 2107  	   rownum < 2;
17:13:54 2108  
17:13:54 2109  	 var_billed := PROCS_INVOICE_V23.IS_INVOICE_PAYING_STARTED(in_invoice_id);
17:13:54 2110  	 var_refund := GLOBAL_CONSTANTS_V23.FALSE;
17:13:54 2111  
17:13:54 2112  	 -- Check that transaction for given invoice not started
17:13:54 2113  	 -- if refund enabled calculate and apply chargeback
17:13:54 2114  	 IF (
17:13:54 2115  	     var_billed = GLOBAL_CONSTANTS_V23.TRUE
17:13:54 2116  	   )THEN
17:13:54 2117  	   if (in_refundable = GLOBAL_CONSTANTS_V23.TRUE) then
17:13:54 2118  	     -- If started then we need to calculate refund
17:13:54 2119  	     BEGIN
17:13:54 2120  	       PROCS_INVOICE_V23.CALCULATE_INVOICE_CHARGEBACK(
17:13:54 2121  		 in_invoice_id,
17:13:54 2122  		 var_now,
