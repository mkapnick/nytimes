17:13:54 1138  /******************************************************************************/
17:13:54 1139  
17:13:54 1140  PROCEDURE REFUND_SUBSCRIPTION (
17:13:54 1141  	 in_subscription_id IN NUMBER,
17:13:54 1142  	 in_invoice_id	    IN NUMBER,
17:13:54 1143  	 in_refund_amount   IN NUMBER,
17:13:54 1144  	 in_note	    IN VARCHAR2,
17:13:54 1145  	 in_created_by	    IN VARCHAR2,
17:13:54 1146  	 out_charge_id	    OUT NUMBER
17:13:54 1147  ) AS
17:13:54 1148  -- VARIABLES
17:13:54 1149  SPROC_NAME	      CONSTANT VARCHAR2(19) := 'REFUND_SUBSCRIPTION';
17:13:54 1150  var_invoice_status_id  NUMBER;
17:13:54 1151  var_account_id	      NUMBER;
17:13:54 1152  var_account_status_id  NUMBER;
17:13:54 1153  var_new_transaction_id NUMBER;
17:13:54 1154  var_instrument_type_id NUMBER;
17:13:54 1155  var_instrument_id      NUMBER;
17:13:54 1156  var_new_charge_id      NUMBER;
17:13:54 1157  var_invoice_amount     NUMBER(10,2);
17:13:54 1158  var_refunds_before     NUMBER(10,2);
17:13:54 1159  var_charges_amount     NUMBER(10,2);
17:13:54 1160  -- EXCEPTIONS
17:13:54 1161  BAD_SUBSCRIPTION_ID	     EXCEPTION;
17:13:54 1162  ACCOUNT_IS_FROZEN	     EXCEPTION;
17:13:54 1163  BAD_INVOICE_ID		     EXCEPTION;
17:13:54 1164  CAN_NOT_CREATE_TRANSACTION    EXCEPTION;
17:13:54 1165  CAN_NOT_CREATE_CHARGE	     EXCEPTION;
17:13:54 1166  CAN_NOT_CALC_INVOICE_AMOUNT   EXCEPTION;
17:13:54 1167  REFUND_IS_GREATER_THAN_ANOUNT EXCEPTION;
17:13:54 1168  CAN_NOT_ANNOTATE_SUBSCRIPTION EXCEPTION;
17:13:54 1169  TOT_REF_IS_GREATER_THAN_ANOUNT EXCEPTION;
17:13:54 1170  INVOICE_IS_NOT_CLOSED	     EXCEPTION;
17:13:54 1171  TOT_REF_IS_GRATER_THAN_CHARGES EXCEPTION;
17:13:54 1172  EXCEPTION_MESSAGE	      VARCHAR2(1024);
17:13:54 1173  BEGIN
17:13:54 1174  
17:13:54 1175  	 BEGIN
17:13:54 1176  	   SELECT
17:13:54 1177  	     SUBSCRIPTION.INSTRUMENT_ID,
17:13:54 1178  	     SUBSCRIPTION.INSTRUMENT_TYPE_ID,
17:13:54 1179  	     SUBSCRIPTION.ACCOUNT_ID
17:13:54 1180  	     into
17:13:54 1181  	     var_instrument_id,
17:13:54 1182  	     var_instrument_type_id,
17:13:54 1183  	     var_account_id
17:13:54 1184  	   FROM
17:13:54 1185  	     SUBSCRIPTION
17:13:54 1186  	   WHERE
17:13:54 1187  	     SUBSCRIPTION.ID = in_subscription_id;
17:13:54 1188  	   EXCEPTION
17:13:54 1189  	     WHEN NO_DATA_FOUND THEN
17:13:54 1190  	       RAISE BAD_SUBSCRIPTION_ID;
17:13:54 1191  	 END;
17:13:54 1192  
17:13:54 1193  	 -- Check account status. It should not to be frozen
17:13:54 1194  	 SELECT
17:13:54 1195  	   ACCOUNT.ACCOUNT_STATUS_ID into var_account_status_id
17:13:54 1196  	 FROM
17:13:54 1197  	   ACCOUNT
17:13:54 1198  	 WHERE
17:13:54 1199  	   ACCOUNT.ID = var_account_id;
17:13:54 1200  
17:13:54 1201  	 IF var_account_status_id = GLOBAL_STATUSES_V23.ACCOUNT_FROZEN THEN
17:13:54 1202  	   RAISE ACCOUNT_IS_FROZEN;
17:13:54 1203  	 END IF;
17:13:54 1204  
17:13:54 1205  	 BEGIN
17:13:54 1206  	   SELECT
17:13:54 1207  	     INVOICE.INVOICE_STATUS_ID into var_invoice_status_id
17:13:54 1208  	   FROM
17:13:54 1209  	     INVOICE
17:13:54 1210  	   WHERE
17:13:54 1211  	     INVOICE.ID = in_invoice_id;
17:13:54 1212  	   EXCEPTION
17:13:54 1213  	     WHEN NO_DATA_FOUND THEN
17:13:54 1214  	       RAISE BAD_INVOICE_ID;
17:13:54 1215  	 END;
17:13:54 1216  
17:13:54 1217  	 IF var_invoice_status_id != GLOBAL_STATUSES_V23.INVOICE_CLOSED THEN
17:13:54 1218  	   RAISE INVOICE_IS_NOT_CLOSED;
17:13:54 1219  	 END IF;
17:13:54 1220  
17:13:54 1221  	 BEGIN
17:13:54 1222  	   PROCS_INVOICE_V23.CALCULATE_INVOICE_AMOUNT (
17:13:54 1223  	     in_invoice_id => in_invoice_id,
17:13:54 1224  	     out_amount    => var_invoice_amount
17:13:54 1225  	   );
17:13:54 1226  	   EXCEPTION
17:13:54 1227  	     WHEN OTHERS THEN
17:13:54 1228  	       EXCEPTION_MESSAGE := SQLERRM;
17:13:54 1229  	       RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
17:13:54 1230  	 END;
17:13:54 1231  
17:13:54 1232  	 IF ( in_refund_amount > var_invoice_amount ) THEN
17:13:54 1233  	   RAISE REFUND_IS_GREATER_THAN_ANOUNT;
17:13:54 1234  	 END IF;
17:13:54 1235  
17:13:54 1236  	 SELECT /*+ STAR_TRANSFORMATION */
17:13:54 1237  	   SUM(CHARGE.CHARGE_AMOUNT) into var_refunds_before
17:13:54 1238  	 FROM
17:13:54 1239  	   CHARGE
17:13:54 1240  	 WHERE
17:13:54 1241  	   CHARGE.INVOICE_ID = in_invoice_id
17:13:54 1242  	   AND (
17:13:54 1243  	     CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V23.CHARGE_OPENED
17:13:54 1244  	     OR CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V23.CHARGE_PROCESSED
17:13:54 1245  	   )
17:13:54 1246  	   AND CHARGE.CHARGE_AMOUNT < 0;
17:13:54 1247  
17:13:54 1248  	 -- Refunds are negative
17:13:54 1249  	 var_refunds_before := -var_refunds_before;
17:13:54 1250  
17:13:54 1251  	 var_charges_amount := 0;
17:13:54 1252  
17:13:54 1253  	 FOR f_processed_charges IN (
17:13:54 1254  	   SELECT
17:13:54 1255  	     CHARGE.CHARGE_AMOUNT
17:13:54 1256  	   FROM
17:13:54 1257  	     CHARGE
17:13:54 1258  	   WHERE
17:13:54 1259  	     CHARGE.INVOICE_ID = in_invoice_id
17:13:54 1260  	     AND CHARGE.CHARGE_STATUS_ID IN (SELECT GLOBAL_STATUSES_V23.CHARGE_PROCESSED FROM DUAL)
17:13:54 1261  	 )
17:13:54 1262  	 LOOP
17:13:54 1263  	   IF f_processed_charges.CHARGE_AMOUNT > 0 THEN
17:13:54 1264  	     var_charges_amount := var_charges_amount + f_processed_charges.CHARGE_AMOUNT;
17:13:54 1265  	   END IF;
17:13:54 1266  	 END LOOP;
17:13:54 1267  
17:13:54 1268  	 IF (in_refund_amount + var_refunds_before > var_invoice_amount) THEN
17:13:54 1269  	   RAISE TOT_REF_IS_GREATER_THAN_ANOUNT;
17:13:54 1270  	 END IF;
17:13:54 1271  
17:13:54 1272  	 IF (in_refund_amount + var_refunds_before > var_charges_amount) THEN
17:13:54 1273  	   RAISE TOT_REF_IS_GRATER_THAN_CHARGES;
17:13:54 1274  	 END IF;
17:13:54 1275  
17:13:54 1276  	 BEGIN
17:13:54 1277  	   PROCS_TRANSACTION_V23.CREATE_TRANSACTION(
17:13:54 1278  	     in_transaction_id	       => NULL,
17:13:54 1279  	     in_status_id	       => GLOBAL_STATUSES_V23.TRANSACTION_PREPARE,
17:13:54 1280  	     in_amount		       => -in_refund_amount,
17:13:54 1281  	     in_created_by	       => in_created_by,
17:13:54 1282  	     in_order_id	       => NULL,
17:13:54 1283  	     in_is_refund	       => GLOBAL_CONSTANTS_V23.TRUE,
17:13:54 1284  	     in_transaction_type_code  => 'REFUND',
17:13:54 1285  	     out_transaction_id        => var_new_transaction_id
17:13:54 1286  	   );
17:13:54 1287  	   EXCEPTION
17:13:54 1288  	     WHEN OTHERS THEN
17:13:54 1289  	       EXCEPTION_MESSAGE := SQLERRM;
17:13:54 1290  	       RAISE CAN_NOT_CREATE_TRANSACTION;
17:13:54 1291  	 END;
17:13:54 1292  
17:13:54 1293  	 BEGIN
17:13:54 1294  	   PROCS_CHARGE_V23.CREATE_CHARGE(
17:13:54 1295  	     in_invoice_id	   => in_invoice_id,
17:13:54 1296  	     in_transaction_id	   => var_new_transaction_id,
17:13:54 1297  	     in_instrument_type_id => var_instrument_type_id,
17:13:54 1298  	     in_instrument_id	   => var_instrument_id,
17:13:54 1299  	     in_charge_amount	   => -in_refund_amount,
17:13:54 1300  	     in_created_by	   => in_created_by,
17:13:54 1301  	     in_charge_status_id   => GLOBAL_STATUSES_V23.CHARGE_OPENED,
17:13:54 1302  	     out_charge_id	   => var_new_charge_id
17:13:54 1303  	   );
17:13:54 1304  	   EXCEPTION
17:13:54 1305  	     WHEN OTHERS THEN
17:13:54 1306  	       EXCEPTION_MESSAGE := SQLERRM;
17:13:54 1307  	       RAISE CAN_NOT_CREATE_CHARGE;
17:13:54 1308  	 END;
17:13:54 1309  
17:13:54 1310  	 IF in_note IS NOT NULL THEN
17:13:54 1311  	   BEGIN
17:13:54 1312  	     PROCS_SUBSCRIPTION_V23.ANNOTATE_SUBSCRIPTION(
17:13:54 1313  	       in_subscription_id => in_subscription_id,
17:13:54 1314  	       in_agent_id	  => 0, -- AGENT_ID??
17:13:54 1315  	       in_note		  => in_note,
17:13:54 1316  	       in_created_by	  => in_created_by
17:13:54 1317  	     );
17:13:54 1318  	     EXCEPTION
17:13:54 1319  	       WHEN OTHERS THEN
17:13:54 1320  		 EXCEPTION_MESSAGE := SQLERRM;
17:13:54 1321  		 RAISE CAN_NOT_ANNOTATE_SUBSCRIPTION;
17:13:54 1322  	   END;
17:13:54 1323  	 END IF;
17:13:54 1324  
17:13:54 1325  	 out_charge_id := var_new_charge_id;
17:13:54 1326  
17:13:54 1327  EXCEPTION
17:13:54 1328  WHEN INVOICE_IS_NOT_CLOSED THEN
17:13:54 1329  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
17:13:54 1330  	   SPROC_NAME, 'Invoice is not closed');
17:13:54 1331  WHEN BAD_SUBSCRIPTION_ID THEN
17:13:54 1332  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 1333  	   SPROC_NAME, 'No such subscription');
17:13:54 1334  WHEN ACCOUNT_IS_FROZEN THEN
17:13:54 1335  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
17:13:54 1336  	   SPROC_NAME, 'Could not refund subscription for frozen account');
17:13:54 1337  WHEN BAD_INVOICE_ID THEN
17:13:54 1338  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 1339  	   SPROC_NAME, 'No such invoice');
17:13:54 1340  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
17:13:54 1341  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 1342  	   SPROC_NAME, 'Could not calculate invoice amount', EXCEPTION_MESSAGE);
17:13:54 1343  WHEN REFUND_IS_GREATER_THAN_ANOUNT THEN
17:13:54 1344  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INVALID_PARAMETER,
17:13:54 1345  	   SPROC_NAME, 'Refund is greater than amount');
17:13:54 1346  WHEN TOT_REF_IS_GREATER_THAN_ANOUNT THEN
17:13:54 1347  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INVALID_PARAMETER,
17:13:54 1348  	   SPROC_NAME, 'There were refunds before and sum of all refunds and new refund more than invoice amount');
17:13:54 1349  WHEN TOT_REF_IS_GRATER_THAN_CHARGES THEN
17:13:54 1350  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INVALID_PARAMETER,
17:13:54 1351  	   SPROC_NAME, 'Total refund amount is greater than sum of processed charges');
17:13:54 1352  WHEN CAN_NOT_CREATE_TRANSACTION THEN
17:13:54 1353  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 1354  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
17:13:54 1355  WHEN CAN_NOT_CREATE_CHARGE THEN
17:13:54 1356  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 1357  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
17:13:54 1358  WHEN CAN_NOT_ANNOTATE_SUBSCRIPTION THEN
17:13:54 1359  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 1360  	   SPROC_NAME, 'Could not annotate subscription', EXCEPTION_MESSAGE);
17:13:54 1361  WHEN OTHERS THEN
17:13:54 1362  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 1363  	   SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 1364  END REFUND_SUBSCRIPTION;
