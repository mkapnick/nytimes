17:13:54 529  	  FROM
17:13:54 530  	    OFFER_OFFER_CHAIN
17:13:54 531  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
17:13:54 532  	  WHERE
17:13:54 533  	    OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
17:13:54 534  	    AND OFFER_OFFER_CHAIN.INDEX_VALUE = var_next_offer_index;
17:13:54 535  	END IF;
17:13:54 536  
17:13:54 537  EXCEPTION
