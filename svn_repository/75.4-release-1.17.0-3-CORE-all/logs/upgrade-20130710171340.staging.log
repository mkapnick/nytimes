17:13:54 327  
17:13:54 328  PROCEDURE GET_RECURRING_OFFER (
17:13:54 329  	in_license_id  IN NUMBER,
17:13:54 330  	out_result_set OUT SYS_REFCURSOR
17:13:54 331  ) AS
17:13:54 332  -- VARIABLES
17:13:54 333  SPROC_NAME		    CONSTANT VARCHAR2(19) := 'GET_RECURRING_OFFER';
17:13:54 334  var_offer_chain_id	    NUMBER;
17:13:54 335  var_offer_id		    NUMBER;
17:13:54 336  var_offer_num_recurrences     NUMBER;
17:13:54 337  var_license_cur_offer_rec_num NUMBER;
17:13:54 338  var_offer_index		    NUMBER;
17:13:54 339  -- EXCEPTIONS
17:13:54 340  BAD_LICENSE_ID	     EXCEPTION;
17:13:54 341  CAN_NOT_GET_OFFER_INFO EXCEPTION;
17:13:54 342  BEGIN
17:13:54 343  
17:13:54 344  	BEGIN
17:13:54 345  	  SELECT
17:13:54 346  	    SUBSCRIPTION.OFFER_CHAIN_ID,
17:13:54 347  	    LICENSE.OFFER_ID,
17:13:54 348  	    LICENSE.CURRENT_OFFER_RECURR_NUM
17:13:54 349  	    into
17:13:54 350  	    var_offer_chain_id,
17:13:54 351  	    var_offer_id,
17:13:54 352  	    var_license_cur_offer_rec_num
17:13:54 353  	  FROM
17:13:54 354  	    LICENSE
17:13:54 355  	    INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
17:13:54 356  	  WHERE
17:13:54 357  	    LICENSE.ID = in_license_id;
17:13:54 358  	  EXCEPTION
17:13:54 359  	    WHEN NO_DATA_FOUND THEN
17:13:54 360  	      RAISE BAD_LICENSE_ID;
17:13:54 361  	END;
17:13:54 362  
17:13:54 363  	BEGIN
17:13:54 364  	  SELECT
17:13:54 365  	    OFFER_OFFER_CHAIN.NUM_RECURRENCES,
17:13:54 366  	    OFFER_OFFER_CHAIN.INDEX_VALUE
17:13:54 367  	    into
17:13:54 368  	    var_offer_num_recurrences,
17:13:54 369  	    var_offer_index
17:13:54 370  	  FROM
17:13:54 371  	    OFFER_OFFER_CHAIN
17:13:54 372  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
17:13:54 373  	  WHERE
17:13:54 374  	    OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
17:13:54 375  	    AND OFFER_OFFER_CHAIN.OFFER_ID = var_offer_id;
17:13:54 376  	  EXCEPTION
17:13:54 377  	    WHEN NO_DATA_FOUND THEN
17:13:54 378  	      RAISE CAN_NOT_GET_OFFER_INFO;
17:13:54 379  	END;
17:13:54 380  
17:13:54 381  	IF var_offer_num_recurrences = 0 THEN
17:13:54 382  	  --out_result_set := NULL;
17:13:54 383  	  -- TODO: Remove this hardcode
17:13:54 384  	  OPEN out_result_set FOR
17:13:54 385  	  SELECT
17:13:54 386  	    OFFER.ID,
17:13:54 387  	    OFFER.OFFER_STATUS_ID,
17:13:54 388  	    OFFER.ENTITLEMENT_DURATION,
17:13:54 389  	    OFFER.CREATED_BY,
17:13:54 390  	    OFFER.CREATE_DATE,
17:13:54 391  	    OFFER.UPDATED_BY,
17:13:54 392  	    OFFER.UPDATE_DATE,
17:13:54 393  	    var_offer_num_recurrences as "RECURRENCE_NUMBER",
17:13:54 394  	    var_offer_index as "OFFER_INDEX"
17:13:54 395  	  FROM
17:13:54 396  	    OFFER_OFFER_CHAIN
17:13:54 397  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
17:13:54 398  	  WHERE ROWNUM = 0;
17:13:54 399  	ELSIF var_license_cur_offer_rec_num = var_offer_num_recurrences THEN
17:13:54 400  	  --out_result_set := NULL;
17:13:54 401  	  -- TODO: Remove this hardcode
17:13:54 402  	  OPEN out_result_set FOR
17:13:54 403  	  SELECT
17:13:54 404  	    OFFER.ID,
17:13:54 405  	    OFFER.OFFER_STATUS_ID,
17:13:54 406  	    OFFER.ENTITLEMENT_DURATION,
17:13:54 407  	    OFFER.CREATED_BY,
17:13:54 408  	    OFFER.CREATE_DATE,
17:13:54 409  	    OFFER.UPDATED_BY,
17:13:54 410  	    OFFER.UPDATE_DATE,
17:13:54 411  	    var_offer_num_recurrences as "RECURRENCE_NUMBER",
17:13:54 412  	    var_offer_index as "OFFER_INDEX"
17:13:54 413  	  FROM
17:13:54 414  	    OFFER_OFFER_CHAIN
17:13:54 415  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
17:13:54 416  	  WHERE ROWNUM = 0;
17:13:54 417  	ELSE
17:13:54 418  	  OPEN out_result_set FOR
17:13:54 419  	  SELECT
17:13:54 420  	    OFFER.ID,
17:13:54 421  	    OFFER.OFFER_STATUS_ID,
17:13:54 422  	    OFFER.ENTITLEMENT_DURATION,
17:13:54 423  	    OFFER.CREATED_BY,
17:13:54 424  	    OFFER.CREATE_DATE,
17:13:54 425  	    OFFER.UPDATED_BY,
17:13:54 426  	    OFFER.UPDATE_DATE,
17:13:54 427  	    var_offer_num_recurrences as "RECURRENCE_NUMBER",
17:13:54 428  	    var_offer_index as "OFFER_INDEX"
17:13:54 429  	  FROM
17:13:54 430  	    OFFER
17:13:54 431  	  WHERE
17:13:54 432  	    OFFER.ID = var_offer_id;
17:13:54 433  	END IF;
17:13:54 434  
17:13:54 435  EXCEPTION
17:13:54 436  WHEN BAD_LICENSE_ID THEN
17:13:54 437  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 438  	  SPROC_NAME, 'No such license');
17:13:54 439  WHEN CAN_NOT_GET_OFFER_INFO THEN
17:13:54 440  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 441  	  SPROC_NAME, 'Could not get offer information. Offer id = '||var_offer_id||', Offer chain id = '||var_offer_chain_id);
17:13:54 442  WHEN OTHERS THEN
17:13:54 443  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 444  	  SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 445  END GET_RECURRING_OFFER;
