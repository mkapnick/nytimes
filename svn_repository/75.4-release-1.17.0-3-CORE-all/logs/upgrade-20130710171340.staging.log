17:13:54  39  PROCEDURE GET_UNREDEEMED_GCS (
17:13:54  40  	out_result_set		OUT SYS_REFCURSOR,
17:13:54  41  	in_hours_number 	IN NUMBER DEFAULT 14*24,
17:13:54  42  	in_num_rows		IN NUMBER DEFAULT 10000,
17:13:54  43  	in_process_name IN PROCESS_RETRY_THROTTLE.PROCESS_NAME%TYPE
17:13:54  44  ) AS
17:13:54  45  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_UNREDEEMED_GCS';
17:13:54  46  BEGIN
17:13:54  47  	OPEN out_result_set FOR
17:13:54  48  SELECT * FROM
17:13:54  49  (
17:13:54  50  	SELECT
17:13:54  51  	  gc.EXPIRATION_DATE,
17:13:54  52  	  ch.name,
17:13:54  53  	  ch.id offer_chain_id,
17:13:54  54  	  gc.sender_email,
17:13:54  55  	  gc.sender_name,
17:13:54  56  	  gc.recipient_email,
17:13:54  57  	  gc.recipient_name,
17:13:54  58  	  gc.purchase_date,
17:13:54  59  	  gc.redemption_date,
17:13:54  60  	  gc.purchaser_group_id,
17:13:54  61  	  gc.redeemer_group_id,
17:13:54  62  	  gc.code,
17:13:54  63  	  gc.gift_message,
17:13:54  64  	  gc.recipient_notify_date,
17:13:54  65  	  gc.id
17:13:54  66  	FROM
17:13:54  67  	  GIFT_CERTIFICATE gc,
17:13:54  68  	  OFFER_CHAIN ch
17:13:54  69  	WHERE
17:13:54  70  	  ch.id = gc.offer_chain_id
17:13:54  71  	  AND gc.RECIPIENT_NOTIFY_DATE is not null
17:13:54  72  	  AND gc.RECIPIENT_NOTIFY_DATE >= (sysdate - in_hours_number/24)
17:13:54  73  	  AND gc.RECIPIENT_NOTIFY_DATE < (sysdate - (in_hours_number-72)/24)
17:13:54  74  	  AND gc.redeemer_group_id is null
17:13:54  75  	  AND NOT EXISTS(
17:13:54  76  	    SELECT NULL
17:13:54  77  	    FROM PROCESS_RETRY_THROTTLE
17:13:54  78  	    WHERE PROCESS_NAME = in_process_name
17:13:54  79  	      AND GENERIC_ID = gc.id
17:13:54  80  	  ) AND EXISTS(
17:13:54  81  	    SELECT NULL
17:13:54  82  	    FROM
17:13:54  83  	      charge c,
17:13:54  84  	      transaction_attempt ta,
17:13:54  85  	      transaction t
17:13:54  86  	    WHERE
17:13:54  87  	      c.invoice_id = gc.purchase_invoice_id and
17:13:54  88  	      c.transaction_id = t.id and
17:13:54  89  	      t.id = ta.transaction_id and
17:13:54  90  	      ta.transaction_attempt_status_id = GLOBAL_STATUSES_V23.TRANS_ATTEMPT_SUCCESS
17:13:54  91  	  ) AND NOT EXISTS (
17:13:54  92  	    SELECT NULL
17:13:54  93  	    FROM
17:13:54  94  	      charge c,
17:13:54  95  	      transaction t
17:13:54  96  	    WHERE
17:13:54  97  	      c.invoice_id = gc.purchase_invoice_id and
17:13:54  98  	      c.transaction_id = t.id and
17:13:54  99  	      t.is_refund = GLOBAL_CONSTANTS_V23.TRUE
17:13:54 100  	  )
17:13:54 101  	  AND ROWNUM <= in_num_rows*10
17:13:54 102  	  ORDER BY dbms_random.value
17:13:54 103  ) WHERE
17:13:54 104  	  ROWNUM <= in_num_rows;
17:13:54 105  EXCEPTION
17:13:54 106  WHEN OTHERS THEN
17:13:54 107  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 108  	  SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 109  END GET_UNREDEEMED_GCS;
