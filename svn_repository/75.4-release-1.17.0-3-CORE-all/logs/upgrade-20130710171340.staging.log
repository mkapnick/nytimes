17:13:54 1821  -- VARIABLES
17:13:54 1822  SPROC_NAME		CONSTANT VARCHAR2(28) := 'GET_GIFT_CERTIFICATE_BY_CODE';
17:13:54 1823  temp_gift_certificate_id NUMBER;
17:13:54 1824  -- EXCEPTIONS
17:13:54 1825  BAD_GIFT_CERTIFICATE_CODE EXCEPTION;
17:13:54 1826  BEGIN
17:13:54 1827  
17:13:54 1828  	 BEGIN
