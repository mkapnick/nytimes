17:13:54 416  
17:13:54 417  PROCEDURE FINALIZE_SUBSCRIPTION_CREATION (
17:13:54 418  	in_subscription_id    IN NUMBER,
17:13:54 419  	in_invoice_id	      IN NUMBER,
17:13:54 420  	in_instrument_type_id IN NUMBER,
17:13:54 421  	in_instrument_id      IN NUMBER,
17:13:54 422  	in_created_by	      IN VARCHAR2
17:13:54 423  ) AS
17:13:54 424  SPROC_NAME CONSTANT VARCHAR2(30) := 'FINALIZE_SUBSCRIPTION_CREATION';
17:13:54 425  -- VARIABLES
17:13:54 426  var_invoice_amount     NUMBER(10, 2);
17:13:54 427  var_new_transaction_id NUMBER;
17:13:54 428  var_new_charge_id      NUMBER;
17:13:54 429  -- EXCEPTIONS
17:13:54 430  CAN_NOT_USE_FCINSTR	  EXCEPTION;
17:13:54 431  CAN_NOT_CALC_INVOICE_AMOUNT EXCEPTION;
17:13:54 432  CAN_NOT_CREATE_TRANSACTION  EXCEPTION;
17:13:54 433  CAN_NOT_CREATE_CHARGE	  EXCEPTION;
17:13:54 434  
17:13:54 435  EXCEPTION_MESSAGE VARCHAR2(1024);
17:13:54 436  BEGIN
17:13:54 437  
17:13:54 438  	-- Calculate invoice amount ( + discounts, taxes)
17:13:54 439  	BEGIN
17:13:54 440  	  PROCS_INVOICE_V23.CALCULATE_INVOICE_AMOUNT(
17:13:54 441  	    in_invoice_id => in_invoice_id,
17:13:54 442  	    out_amount	  => var_invoice_amount
17:13:54 443  	  );
17:13:54 444  	  EXCEPTION
17:13:54 445  	   WHEN OTHERS THEN
17:13:54 446  	     EXCEPTION_MESSAGE := SQLERRM;
17:13:54 447  	     RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
17:13:54 448  	END;
17:13:54 449  
17:13:54 450  	IF in_instrument_type_id = GLOBAL_ENUMS_V23.INSTRUMENT_ZCI_INSTRUMENT
17:13:54 451  	  AND var_invoice_amount > 0 THEN
17:13:54 452  	  RAISE CAN_NOT_USE_FCINSTR;
17:13:54 453  	END IF;
17:13:54 454  
17:13:54 455  	IF var_invoice_amount = 0 THEN
17:13:54 456  	  -- UPDATE INVOICE. SET STATUS TO PROCESSED
17:13:54 457  	  PROCS_INVOICE_CRU_V23.UPDATE_INVOICE(
17:13:54 458  	    in_invoice_id		   => in_invoice_id,
17:13:54 459  	    in_updated_by		   => in_created_by,
17:13:54 460  	    in_invoice_status_id	   => GLOBAL_STATUSES_V23.INVOICE_CLOSED
17:13:54 461  	  );
17:13:54 462  	ELSE
17:13:54 463  	  -- Create transaction and charge
17:13:54 464  	  BEGIN
17:13:54 465  	    PROCS_TRANSACTION_V23.CREATE_TRANSACTION(
17:13:54 466  	      in_transaction_id 	=> NULL,
17:13:54 467  	      in_status_id		=> GLOBAL_STATUSES_V23.TRANSACTION_PENDING,
17:13:54 468  	      in_amount 		=> var_invoice_amount,
17:13:54 469  	      in_created_by		=> in_created_by,
17:13:54 470  	      in_order_id		=> NULL,
17:13:54 471  	      in_transaction_type_code	=> 'START_SUBSCRIPTION',
17:13:54 472  	      out_transaction_id	=> var_new_transaction_id
17:13:54 473  	    );
17:13:54 474  	    EXCEPTION
17:13:54 475  	      WHEN OTHERS THEN
17:13:54 476  		EXCEPTION_MESSAGE := SQLERRM;
17:13:54 477  		RAISE CAN_NOT_CREATE_TRANSACTION;
17:13:54 478  	  END;
17:13:54 479  
17:13:54 480  	  BEGIN
17:13:54 481  	    PROCS_CHARGE_V23.CREATE_CHARGE(
17:13:54 482  	      in_invoice_id	    => in_invoice_id,
17:13:54 483  	      in_transaction_id     => var_new_transaction_id,
17:13:54 484  	      in_instrument_type_id => in_instrument_type_id,
17:13:54 485  	      in_instrument_id	    => in_instrument_id,
17:13:54 486  	      in_charge_amount	    => var_invoice_amount,
17:13:54 487  	      in_created_by	    => in_created_by,
17:13:54 488  	      in_charge_status_id   => GLOBAL_STATUSES_V23.CHARGE_OPENED,
17:13:54 489  	      out_charge_id	    => var_new_charge_id
17:13:54 490  	    );
17:13:54 491  	    EXCEPTION
17:13:54 492  	      WHEN OTHERS THEN
17:13:54 493  		EXCEPTION_MESSAGE := SQLERRM;
17:13:54 494  		RAISE CAN_NOT_CREATE_CHARGE;
17:13:54 495  	  END;
17:13:54 496  	END IF;
17:13:54 497  
17:13:54 498  EXCEPTION
17:13:54 499  WHEN CAN_NOT_USE_FCINSTR THEN
17:13:54 500  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
17:13:54 501  	  SPROC_NAME, 'Could not use "free charge instrument" for non-zero invoice');
17:13:54 502  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
17:13:54 503  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 504  	  SPROC_NAME, 'Could not calculate invoice amount', EXCEPTION_MESSAGE);
17:13:54 505  WHEN CAN_NOT_CREATE_TRANSACTION THEN
17:13:54 506  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 507  	  SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
17:13:54 508  WHEN CAN_NOT_CREATE_CHARGE THEN
17:13:54 509  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 510  	  SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
17:13:54 511  WHEN OTHERS THEN
17:13:54 512  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 513  	  SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 514  END FINALIZE_SUBSCRIPTION_CREATION;
