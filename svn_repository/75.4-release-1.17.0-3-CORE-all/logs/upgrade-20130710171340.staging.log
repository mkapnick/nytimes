17:13:54 1097  
17:13:54 1098  PROCEDURE GET_INVOICE_DAYS_USED_NUMBER (
17:13:54 1099  	 in_invoice_id	     IN NUMBER,
17:13:54 1100  	 in_chargeback_date  IN DATE DEFAULT SYSDATE,
17:13:54 1101  	 out_days_num	     OUT NUMBER
17:13:54 1102  ) AS
17:13:54 1103  SPROC_NAME CONSTANT VARCHAR2(28) := 'GET_INVOICE_DAYS_USED_NUMBER';
17:13:54 1104  -- VARIABLES
17:13:54 1105  temp_invoice_id	      NUMBER;
17:13:54 1106  var_license_start_date DATE;
17:13:54 1107  var_license_end_date   DATE;
17:13:54 1108  var_chargeback_date    DATE;
17:13:54 1109  var_invoice_days_used  NUMBER;
17:13:54 1110  -- EXCEPTIONS
17:13:54 1111  BAD_INVOICE_ID EXCEPTION;
17:13:54 1112  BEGIN
17:13:54 1113  
17:13:54 1114  	 var_chargeback_date := NVL(in_chargeback_date, SYSDATE);
17:13:54 1115  
17:13:54 1116  	 BEGIN
17:13:54 1117  	   SELECT
17:13:54 1118  	     INVOICE.ID into temp_invoice_id
17:13:54 1119  	   FROM
17:13:54 1120  	     INVOICE
17:13:54 1121  	   WHERE
17:13:54 1122  	     INVOICE.ID = in_invoice_id;
17:13:54 1123  	   EXCEPTION
17:13:54 1124  	     WHEN NO_DATA_FOUND THEN
17:13:54 1125  	       RAISE BAD_INVOICE_ID;
17:13:54 1126  	 END;
17:13:54 1127  
17:13:54 1128  	 var_invoice_days_used := 0;
17:13:54 1129  
17:13:54 1130  	 FOR f_license IN (
17:13:54 1131  	   SELECT
17:13:54 1132  	     LICENSE.START_DATE,
17:13:54 1133  	     LICENSE.END_DATE
17:13:54 1134  	   FROM
17:13:54 1135  	     LICENSE
17:13:54 1136  	   WHERE
17:13:54 1137  	     LICENSE.INVOICE_ID = in_invoice_id
17:13:54 1138  	     AND LICENSE.IS_EXTENSION = GLOBAL_CONSTANTS_V23.FALSE
17:13:54 1139  	 )
17:13:54 1140  	 LOOP
17:13:54 1141  	   var_license_start_date := PROCS_COMMON_V23.NORMALIZE_DATE(f_license.START_DATE);
17:13:54 1142  	   var_license_end_date := PROCS_COMMON_V23.NORMALIZE_DATE(f_license.END_DATE);
17:13:54 1143  
17:13:54 1144  	   IF var_license_start_date <= var_chargeback_date THEN
17:13:54 1145  	     IF var_license_end_date <= var_chargeback_date THEN
17:13:54 1146  	       -- License is passed
17:13:54 1147  	       var_invoice_days_used := var_invoice_days_used + (var_license_end_date - var_license_start_date);
17:13:54 1148  	     ELSE
17:13:54 1149  	       -- This is current license
17:13:54 1150  	       var_invoice_days_used := var_invoice_days_used + (var_chargeback_date - var_license_start_date);
17:13:54 1151  	     END IF;
17:13:54 1152  	   ELSE
17:13:54 1153  	     -- if var_license_start_date > in_chargeback_date then do nothing
17:13:54 1154  	     NULL;
17:13:54 1155  	   END IF;
17:13:54 1156  	 END LOOP;
17:13:54 1157  
17:13:54 1158  	 out_days_num := var_invoice_days_used;
17:13:54 1159  
17:13:54 1160  EXCEPTION
17:13:54 1161  WHEN BAD_INVOICE_ID THEN
17:13:54 1162  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 1163  	   SPROC_NAME, 'No such invoice');
17:13:54 1164  WHEN OTHERS THEN
17:13:54 1165  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 1166  	   SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 1167  END GET_INVOICE_DAYS_USED_NUMBER;
