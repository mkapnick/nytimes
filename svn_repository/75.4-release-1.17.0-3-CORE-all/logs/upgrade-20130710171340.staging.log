17:13:54 1851  	   gc.redemption_date,
17:13:54 1852  	   gc.purchaser_group_id,
17:13:54 1853  	   gc.redeemer_group_id,
17:13:54 1854  	   gc.gift_message,
17:13:54 1855  	   ocmd.value redemption_offer_chain_ids,
17:13:54 1856  	   s.offer_chain_id redeemed_offer_chain_id,
17:13:54 1857  	   gc.recipient_notify_date,
17:13:54 1858  	   gc.gift_certificate_status_id,
17:13:54 1859  	   gc.purchase_invoice_id,
17:13:54 1860  	   gc.finalized_invoice_id,
17:13:54 1861  	   gccr.reference_code
17:13:54 1862  	 FROM
17:13:54 1863  	   GIFT_CERTIFICATE gc
17:13:54 1864  	 INNER JOIN OFFER_CHAIN ch ON ch.id = gc.offer_chain_id
17:13:54 1865  	 INNER JOIN OFFER_CHAIN_META_DATA ocmd ON gc.offer_chain_id = ocmd.offer_chain_id AND ocmd.name = 'redemption offer chain id'
17:13:54 1866  	 LEFT JOIN LICENSE l ON l.invoice_id = gc.finalized_invoice_id
17:13:54 1867  	 LEFT JOIN SUBSCRIPTION s ON l.subscription_id = s.id
17:13:54 1868  	 LEFT JOIN GC_CAMPAIGN_AND_REF gccr ON gc.id = gccr.GC_ID
17:13:54 1869  	 WHERE
