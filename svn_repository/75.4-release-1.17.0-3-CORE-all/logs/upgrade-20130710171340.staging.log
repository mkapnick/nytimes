17:13:54 1076  
17:13:54 1077  PROCEDURE PURCHASE_GIFT_CERTIFICATE (
17:13:54 1078  	 in_group_id		   IN NUMBER,
17:13:54 1079  	 in_offer_chain_id	   IN VARCHAR2,
17:13:54 1080  	 in_gift_certificate_code  IN VARCHAR2,
17:13:54 1081  	 in_created_by		   IN VARCHAR2,
17:13:54 1082  	 in_recipient_name	   IN VARCHAR2,
17:13:54 1083  	 in_recipient_email	   IN VARCHAR2,
17:13:54 1084  	 in_sender_name 	   IN VARCHAR2,
17:13:54 1085  	 in_sender_email	   IN VARCHAR2,
17:13:54 1086  	 in_gift_message	   IN VARCHAR2,
17:13:54 1087  	 in_instrument_id	   IN NUMBER,
17:13:54 1088  	 in_instrument_type_id	   IN NUMBER,
17:13:54 1089  	 in_expiration_date	   IN DATE,
17:13:54 1090  	 in_order_id		   IN VARCHAR2,
17:13:54 1091  	 in_transaction_id	   IN NUMBER
17:13:54 1092  ) AS
17:13:54 1093  SPROC_NAME	  CONSTANT VARCHAR2(25) := 'PURCHASE_GIFT_CERTIFICATE';
17:13:54 1094  -- VARIABLES
17:13:54 1095  var_gift_cert_id   NUMBER;
17:13:54 1096  var_account_id	  NUMBER;
17:13:54 1097  var_invoice_amount NUMBER (10,2);
17:13:54 1098  var_new_invoice_id NUMBER;
17:13:54 1099  var_new_charge_id  NUMBER;
17:13:54 1100  var_och_is_gc	  NUMBER;
17:13:54 1101  var_offer_chain_status_id NUMBER;
17:13:54 1102  var_is_for_redemption	 NUMBER;
17:13:54 1103  var_account_tax_exempt_id VARCHAR2(255);
17:13:54 1104  
17:13:54 1105  temp_transaction_id_count NUMBER;
17:13:54 1106  var_transaction_id	 NUMBER;
17:13:54 1107  temp_gc_code VARCHAR2(255);
17:13:54 1108  
17:13:54 1109  var_invoice_status_id NUMBER;
17:13:54 1110  -- EXCEPTIONS
17:13:54 1111  CAN_NOT_CREATE_INVOICE		   EXCEPTION;
17:13:54 1112  CAN_NOT_CREATE_TRANSACTION	   EXCEPTION;
17:13:54 1113  CAN_NOT_CREATE_CHARGE		   EXCEPTION;
17:13:54 1114  CAN_NOT_CREATE_LINE_ITEMS	   EXCEPTION;
17:13:54 1115  BAD_GROUP_ID			   EXCEPTION;
17:13:54 1116  BAD_OFFER_CHAIN_ID		   EXCEPTION;
17:13:54 1117  OCH_IS_NOT_GIFT_CERTIFICATE	   EXCEPTION;
17:13:54 1118  TRANSACTION_EXISTS		   EXCEPTION;
17:13:54 1119  GC_CODE_ALREADY_EXISTS		   EXCEPTION;
17:13:54 1120  BAD_INSTRUMENT_TYPE		   EXCEPTION;
17:13:54 1121  BAD_CREDIT_CARD_ID		   EXCEPTION;
17:13:54 1122  BAD_PAYPAL_ID			   EXCEPTION;
17:13:54 1123  CAN_NOT_CALCULATE_OCH_AMOUNT	   EXCEPTION;
17:13:54 1124  BAD_OFFER_CHAIN_STATUS		   EXCEPTION;
17:13:54 1125  OFFER_REC_NUM_LESS_THAN_ONE	   EXCEPTION;
17:13:54 1126  CAN_NOT_CALC_INVOICE_AMOUNT	   EXCEPTION;
17:13:54 1127  CAN_NOT_USE_FCINSTR		   EXCEPTION;
17:13:54 1128  CAN_NOT_PURCHASE_GC_FOR_RDMPN	   EXCEPTION;
17:13:54 1129  EXCEPTION_MESSAGE		   VARCHAR2(1024);
17:13:54 1130  BEGIN
17:13:54 1131  
17:13:54 1132  	 -- Get account id
17:13:54 1133  	 BEGIN
17:13:54 1134  	   SELECT
17:13:54 1135  	     ACCOUNT.ID,
17:13:54 1136  	     ACCOUNT.TAX_EXEMPT_ID
17:13:54 1137  	     into
17:13:54 1138  	     var_account_id,
17:13:54 1139  	     var_account_tax_exempt_id
17:13:54 1140  	   FROM
17:13:54 1141  	     ACCOUNT
17:13:54 1142  	   WHERE
17:13:54 1143  	     ACCOUNT.GROUP_ID = in_group_id;
17:13:54 1144  	   EXCEPTION
17:13:54 1145  	   WHEN NO_DATA_FOUND THEN
17:13:54 1146  	     RAISE BAD_GROUP_ID;
17:13:54 1147  	 END;
17:13:54 1148  
17:13:54 1149  	 -- Check that instrument exists
17:13:54 1150  	 IF in_instrument_type_id = GLOBAL_ENUMS_V23.INSTRUMENT_CREDIT_CARD THEN
17:13:54 1151  	   IF IS_CREDIT_CARD_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V23.FALSE THEN
17:13:54 1152  	     RAISE BAD_CREDIT_CARD_ID;
17:13:54 1153  	   END IF;
17:13:54 1154  	 ELSIF in_instrument_type_id = GLOBAL_ENUMS_V23.INSTRUMENT_PAYPAL THEN
17:13:54 1155  	   IF IS_PAYPAL_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V23.FALSE THEN
