17:13:54 436  -- norlov: #38796
17:13:54 437  PROCEDURE GET_TRANSACTION_INVOICE (
17:13:54 438  /*
17:13:54 439  Throws exceptions:
17:13:54 440  APP_EXCEPTION_CODES_V23.NOT_FOUND
17:13:54 441  APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR
17:13:54 442  */
17:13:54 443  	in_transaction_id  IN  NUMBER,
17:13:54 444  	out_result_set	      OUT SYS_REFCURSOR
17:13:54 445  ) AS
17:13:54 446  SPROC_NAME CONSTANT  VARCHAR2(23) := 'GET_TRANSACTION_INVOICE';
17:13:54 447  -- VARIABLES
17:13:54 448  temp_transaction_id  NUMBER;
17:13:54 449  var_invoice_id	   NUMBER;
17:13:54 450  var_subscription_id  NUMBER;
17:13:54 451  var_offer_chain_id   NUMBER;
17:13:54 452  var_offer_chain_name VARCHAR2(255);
17:13:54 453  -- EXCEPTIONS
17:13:54 454  BAD_TRANSACTION_ID     EXCEPTION;
17:13:54 455  EXCEPTION_MESSAGE      VARCHAR2(1024);
17:13:54 456  BEGIN
17:13:54 457   -- check if there is the transaction
17:13:54 458  	BEGIN
17:13:54 459  	  SELECT
17:13:54 460  	    TRANSACTION.ID into temp_transaction_id
17:13:54 461  	  FROM
17:13:54 462  	    TRANSACTION
17:13:54 463  	  WHERE
17:13:54 464  	    TRANSACTION.ID = in_transaction_id;
17:13:54 465  	  EXCEPTION
17:13:54 466  	    WHEN NO_DATA_FOUND THEN
17:13:54 467  	      RAISE BAD_TRANSACTION_ID;
17:13:54 468  	END;
17:13:54 469  
17:13:54 470  	-- Get invoice id
17:13:54 471  	SELECT DISTINCT
17:13:54 472  	  CHARGE.INVOICE_ID into var_invoice_id
17:13:54 473  	FROM
17:13:54 474  	  CHARGE
17:13:54 475  	WHERE
17:13:54 476  	  CHARGE.TRANSACTION_ID = in_transaction_id;
17:13:54 477  
17:13:54 478  	-- Get subscription id if exists
17:13:54 479  	BEGIN
17:13:54 480  	  SELECT DISTINCT
17:13:54 481  	    LICENSE.SUBSCRIPTION_ID into var_subscription_id
17:13:54 482  	  FROM
17:13:54 483  	    LICENSE
17:13:54 484  	  WHERE
17:13:54 485  	    LICENSE.INVOICE_ID = var_invoice_id;
17:13:54 486  	  EXCEPTION
17:13:54 487  	    WHEN NO_DATA_FOUND THEN
17:13:54 488  	      var_subscription_id := NULL;
17:13:54 489  	END;
17:13:54 490  
17:13:54 491  	IF var_subscription_id IS NOT NULL THEN
17:13:54 492  	  -- Fetch offer chain from subscription
17:13:54 493  	  SELECT
17:13:54 494  	    OFFER_CHAIN.ID,
17:13:54 495  	    OFFER_CHAIN.NAME
17:13:54 496  	    into
17:13:54 497  	    var_offer_chain_id,
17:13:54 498  	    var_offer_chain_name
17:13:54 499  	  FROM
17:13:54 500  	    OFFER_CHAIN
17:13:54 501  	    INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
17:13:54 502  	  WHERE
17:13:54 503  	    SUBSCRIPTION.ID = var_subscription_id;
17:13:54 504  	ELSE
17:13:54 505  	  -- Fetch offer chain from GC
17:13:54 506  	  SELECT
17:13:54 507  	    OFFER_CHAIN.ID,
17:13:54 508  	    OFFER_CHAIN.NAME
17:13:54 509  	    into
17:13:54 510  	    var_offer_chain_id,
17:13:54 511  	    var_offer_chain_name
17:13:54 512  	  FROM
17:13:54 513  	    OFFER_CHAIN
17:13:54 514  	    INNER JOIN GIFT_CERTIFICATE ON GIFT_CERTIFICATE.OFFER_CHAIN_ID = OFFER_CHAIN.ID
17:13:54 515  	  WHERE
17:13:54 516  	    GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = var_invoice_id;
17:13:54 517  	END IF;
17:13:54 518  
17:13:54 519  	OPEN out_result_set FOR
17:13:54 520  	SELECT DISTINCT
17:13:54 521  	  var_invoice_id       AS "INVOICE_ID",
17:13:54 522  	  var_subscription_id  AS "SUBSCRIPTION_ID",
17:13:54 523  	  var_offer_chain_id   AS "OFFER_CHAIN_ID",
17:13:54 524  	  var_offer_chain_name AS "OFFER_CHAIN_NAME"
17:13:54 525  	FROM
17:13:54 526  	  DUAL;
17:13:54 527  
17:13:54 528  EXCEPTION
17:13:54 529  WHEN BAD_TRANSACTION_ID THEN
17:13:54 530  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 531  	  SPROC_NAME, 'No such transaction');
17:13:54 532  WHEN OTHERS THEN
17:13:54 533  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 534  	  SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 535  END GET_TRANSACTION_INVOICE;
