17:13:54 854  	FROM
17:13:54 855  	    (
17:13:54 856  		SELECT
17:13:54 857  		    l.id
17:13:54 858  		FROM
17:13:54 859  		    license l
17:13:54 860  		JOIN
17:13:54 861  		    invoice i
