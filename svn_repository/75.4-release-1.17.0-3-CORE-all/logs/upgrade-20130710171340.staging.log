17:13:54 2235  BAD_GIFT_CERTIFICATE_ID	 EXCEPTION;
17:13:54 2236  CAN_NOT_FIND_SUBSCRIPTION EXCEPTION;
17:13:54 2237  BEGIN
17:13:54 2238  
17:13:54 2239  	 BEGIN
17:13:54 2240  	   SELECT
17:13:54 2241  	     CHARGE.INVOICE_ID into var_invoice_id
17:13:54 2242  	   FROM
17:13:54 2243  	     CHARGE
17:13:54 2244  	   WHERE
17:13:54 2245  	     CHARGE.ID = in_charge_id;
17:13:54 2246  	   EXCEPTION
17:13:54 2247  	     WHEN NO_DATA_FOUND THEN
17:13:54 2248  	       RAISE BAD_CHARGE_ID;
17:13:54 2249  	 END;
17:13:54 2250  
17:13:54 2251  	 BEGIN
17:13:54 2252  	   SELECT
17:13:54 2253  	     GIFT_CERTIFICATE.OFFER_CHAIN_ID into var_offer_chain_id
17:13:54 2254  	   FROM
17:13:54 2255  	     GIFT_CERTIFICATE
17:13:54 2256  	   WHERE
17:13:54 2257  	     GIFT_CERTIFICATE.ID = in_gift_certificate_id;
17:13:54 2258  	   EXCEPTION
17:13:54 2259  	     WHEN NO_DATA_FOUND THEN
17:13:54 2260  	       RAISE BAD_GIFT_CERTIFICATE_ID;
17:13:54 2261  	 END;
17:13:54 2262  
17:13:54 2263  	 BEGIN
17:13:54 2264  	   SELECT
17:13:54 2265  	     SUBSCRIPTION.OFFER_CHAIN_ID into var_offer_chain_s_id
17:13:54 2266  	   FROM
17:13:54 2267  	     SUBSCRIPTION
17:13:54 2268  	   WHERE
17:13:54 2269  	     SUBSCRIPTION.ID IN (
17:13:54 2270  	       SELECT DISTINCT
17:13:54 2271  		 LICENSE.SUBSCRIPTION_ID
17:13:54 2272  	       FROM
17:13:54 2273  		 LICENSE
17:13:54 2274  	       WHERE
17:13:54 2275  		 LICENSE.INVOICE_ID = var_invoice_id
17:13:54 2276  	     );
17:13:54 2277  	   EXCEPTION
17:13:54 2278  	     WHEN NO_DATA_FOUND THEN
17:13:54 2279  	       RAISE CAN_NOT_FIND_SUBSCRIPTION;
17:13:54 2280  	 END;
17:13:54 2281  
17:13:54 2282  	 IF var_offer_chain_s_id = var_offer_chain_id THEN
17:13:54 2283  	   out_result := GLOBAL_CONSTANTS_V23.TRUE;
