17:13:54 683  PROCEDURE FREEZE_ACCOUNT (
17:13:54 684  	in_group_id   IN NUMBER,
17:13:54 685  	in_updated_by IN VARCHAR2,
17:13:54 686  	in_note       IN VARCHAR2,
17:13:54 687  	in_agent_id   IN NUMBER
17:13:54 688  ) AS
17:13:54 689  SPROC_NAME	    CONSTANT VARCHAR2(14) := 'FREEZE_ACCOUNT';
17:13:54 690  -- VARIABLES
17:13:54 691  var_account_id	    NUMBER;
17:13:54 692  var_account_status_id NUMBER;
17:13:54 693  -- EXCEPTIONS
17:13:54 694  BAD_GROUP_ID	    EXCEPTION;
17:13:54 695  BAD_ACCOUNT_STATUS_ID EXCEPTION;
17:13:54 696  CAN_NOT_CREATE_NOTE   EXCEPTION;
17:13:54 697  EXCEPTION_MESSAGE     VARCHAR2(1024);
17:13:54 698  BEGIN
17:13:54 699  
17:13:54 700  	-- Get account status, account id
17:13:54 701  	BEGIN
17:13:54 702  	  SELECT
17:13:54 703  	    ACCOUNT.ID,
17:13:54 704  	    ACCOUNT.ACCOUNT_STATUS_ID
17:13:54 705  	    into
17:13:54 706  	    var_account_id,
17:13:54 707  	    var_account_status_id
17:13:54 708  	  FROM
17:13:54 709  	    ACCOUNT
17:13:54 710  	  WHERE
17:13:54 711  	    ACCOUNT.GROUP_ID = in_group_id;
17:13:54 712  	  EXCEPTION
17:13:54 713  	    WHEN NO_DATA_FOUND THEN
17:13:54 714  	      RAISE BAD_GROUP_ID;
17:13:54 715  	END;
17:13:54 716  
17:13:54 717  	-- We can freeze only ACTIVE accounts
17:13:54 718  	IF var_account_status_id != GLOBAL_STATUSES_V23.ACCOUNT_ACTIVE
17:13:54 719  	  AND var_account_status_id != GLOBAL_STATUSES_V23.ACCOUNT_FROZEN THEN
17:13:54 720  	  RAISE BAD_ACCOUNT_STATUS_ID;
17:13:54 721  	END IF;
17:13:54 722  
17:13:54 723  	-- Set account status
17:13:54 724  	PROCS_ACCOUNT_V23.UPDATE_ACCOUNT_STATUS(
17:13:54 725  	  in_account_id        => var_account_id,
17:13:54 726  	  in_updated_by        => in_updated_by,
17:13:54 727  	  in_account_status_id => GLOBAL_STATUSES_V23.ACCOUNT_FROZEN
17:13:54 728  	);
17:13:54 729  
17:13:54 730  	-- Annotate account
17:13:54 731  	BEGIN
17:13:54 732  	  PROCS_ACCOUNT_V23.ANNOTATE_ACCOUNT(
17:13:54 733  	    in_group_id   => in_group_id,
17:13:54 734  	    in_agent_id   => in_agent_id,
17:13:54 735  	    in_note	  => in_note,
17:13:54 736  	    in_created_by => in_updated_by
17:13:54 737  	  );
17:13:54 738  	  EXCEPTION
17:13:54 739  	    WHEN OTHERS THEN
17:13:54 740  	      EXCEPTION_MESSAGE := SQLERRM;
17:13:54 741  	      RAISE CAN_NOT_CREATE_NOTE;
17:13:54 742  	END;
17:13:54 743  
17:13:54 744  EXCEPTION
17:13:54 745  WHEN BAD_GROUP_ID THEN
17:13:54 746  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 747  	  SPROC_NAME, 'No such group id');
17:13:54 748  WHEN BAD_ACCOUNT_STATUS_ID THEN
17:13:54 749  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
17:13:54 750  	  SPROC_NAME, 'Could not update this account. Status should to be active or frozen');
17:13:54 751  WHEN CAN_NOT_CREATE_NOTE THEN
17:13:54 752  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 753  	  SPROC_NAME, 'Could not annotate account', EXCEPTION_MESSAGE);
17:13:54 754  WHEN OTHERS THEN
17:13:54 755  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 756  	  SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 757  END FREEZE_ACCOUNT;
