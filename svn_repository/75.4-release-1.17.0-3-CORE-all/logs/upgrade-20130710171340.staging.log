17:13:54 919  
17:13:54 920  FUNCTION GET_RENEWAL_DATE (
17:13:54 921  	in_subscription_id in NUMBER
17:13:54 922  ) RETURN DATE AS
17:13:54 923  -- VARIABLES
17:13:54 924  SPROC_NAME	      CONSTANT VARCHAR2(16) := 'GET_RENEWAL_DATE';
17:13:54 925  var_subscription_status NUMBER;
17:13:54 926  var_licenses_count      NUMBER;
17:13:54 927  var_license_end_date    DATE;
17:13:54 928  var_last_offer_id       NUMBER;
17:13:54 929  var_offer_chain_id      NUMBER;
17:13:54 930  var_last_license_id     NUMBER;
17:13:54 931  var_current_offer_index NUMBER;
17:13:54 932  var_current_offer_recurr_num NUMBER;
17:13:54 933  var_offer_recurr_num    NUMBER;
17:13:54 934  -- EXCEPTIONS
17:13:54 935  BAD_SUBSCRIPTION_ID EXCEPTION;
17:13:54 936  NO_LICENSES_FOUND EXCEPTION;
17:13:54 937  BEGIN
17:13:54 938  
17:13:54 939  	-- Get subscription id and offer chain id
17:13:54 940  	BEGIN
17:13:54 941  	  SELECT
17:13:54 942  	    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID,
17:13:54 943  	    SUBSCRIPTION.OFFER_CHAIN_ID
17:13:54 944  	    into
17:13:54 945  	    var_subscription_status,
17:13:54 946  	    var_offer_chain_id
17:13:54 947  	  FROM
17:13:54 948  	    SUBSCRIPTION
17:13:54 949  	  WHERE
17:13:54 950  	    SUBSCRIPTION.ID = in_subscription_id;
17:13:54 951  	  EXCEPTION
17:13:54 952  	    WHEN NO_DATA_FOUND THEN
17:13:54 953  	      RAISE BAD_SUBSCRIPTION_ID;
17:13:54 954  	END;
17:13:54 955  
17:13:54 956  	IF var_subscription_status != GLOBAL_STATUSES_V23.SUBSCRIPTION_ACTIVE THEN
17:13:54 957  	  -- TODO: Is suspended subscription has renewal date? (For the phase II)
17:13:54 958  	  -- AND var_subscription_status != GLOBAL_STATUSES_V23.SUBSCRIPTION_SUSPENDED THEN
17:13:54 959  	  RETURN NULL;
17:13:54 960  	END IF;
17:13:54 961  
17:13:54 962  	BEGIN
17:13:54 963  	  SELECT
17:13:54 964  	    LICENSE_ID,
17:13:54 965  	    END_DATE,
17:13:54 966  	    OFFER_ID,
17:13:54 967  	    CURRENT_OFFER_INDEX,
17:13:54 968  	    CURRENT_OFFER_RECURR_NUM
17:13:54 969  	    into
17:13:54 970  	    var_last_license_id,
17:13:54 971  	    var_license_end_date,
17:13:54 972  	    var_last_offer_id,
17:13:54 973  	    var_current_offer_index,
17:13:54 974  	    var_current_offer_recurr_num
17:13:54 975  	  FROM
17:13:54 976  	    (
17:13:54 977  	      SELECT
17:13:54 978  		LICENSE.ID as "LICENSE_ID",
17:13:54 979  		LICENSE.END_DATE,
17:13:54 980  		LICENSE.OFFER_ID,
17:13:54 981  		LICENSE.CURRENT_OFFER_INDEX,
17:13:54 982  		LICENSE.CURRENT_OFFER_RECURR_NUM
17:13:54 983  	      FROM
17:13:54 984  		LICENSE
17:13:54 985  	      WHERE
17:13:54 986  		LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V23.LICENSE_ACTIVE
17:13:54 987  		AND LICENSE.SUBSCRIPTION_ID = in_subscription_id
17:13:54 988  	      ORDER BY END_DATE DESC
17:13:54 989  	    )
17:13:54 990  	    INNER JOIN OFFER ON OFFER_ID = OFFER.ID
17:13:54 991  	  WHERE
17:13:54 992  	    ROWNUM <= 1;
17:13:54 993  
17:13:54 994  	  EXCEPTION
17:13:54 995  	    WHEN NO_DATA_FOUND THEN
17:13:54 996  	      -- RAISE NO_LICENSES_FOUND;
17:13:54 997  	      RETURN NULL;
17:13:54 998  	END;
17:13:54 999  
17:13:54 1000  	 SELECT
17:13:54 1001  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES into var_offer_recurr_num
17:13:54 1002  	 FROM
17:13:54 1003  	   OFFER_OFFER_CHAIN
17:13:54 1004  	 WHERE
17:13:54 1005  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
17:13:54 1006  	   AND OFFER_OFFER_CHAIN.OFFER_ID = var_last_offer_id;
17:13:54 1007  
17:13:54 1008  	 IF PROCS_OFFER_CHAIN_V23.GET_NEXT_OFFER_INDEX(var_offer_chain_id, var_current_offer_index) IS NULL
17:13:54 1009  	   AND var_offer_recurr_num = var_current_offer_recurr_num THEN
17:13:54 1010  	   -- There is no next offer for this subscription
17:13:54 1011  	   RETURN NULL;
17:13:54 1012  	 END IF;
17:13:54 1013  
17:13:54 1014  	 RETURN var_license_end_date;
17:13:54 1015  
17:13:54 1016  EXCEPTION
17:13:54 1017  WHEN BAD_SUBSCRIPTION_ID THEN
17:13:54 1018  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 1019  	   SPROC_NAME, 'No such subscription');
17:13:54 1020  WHEN NO_LICENSES_FOUND THEN
17:13:54 1021  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 1022  	   SPROC_NAME, 'No licenses found');
17:13:54 1023  WHEN OTHERS THEN
17:13:54 1024  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 1025  	   SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 1026  END GET_RENEWAL_DATE;
17:13:54 1027  
17:13:54 1028  /******************************************************************************/
17:13:54 1029  
17:13:54 1030  FUNCTION GET_RECENT_CHARGE (
17:13:54 1031  	 in_subscription_id IN NUMBER
17:13:54 1032  ) RETURN NUMBER AS
17:13:54 1033  -- VARIABLES
17:13:54 1034  SPROC_NAME	    CONSTANT VARCHAR2(17) := 'GET_RECENT_CHARGE';
17:13:54 1035  temp_subscription_id NUMBER;
17:13:54 1036  var_recent_charge    NUMBER(10,2);
17:13:54 1037  
17:13:54 1038  -- EXCEPTIONS
17:13:54 1039  BAD_SUBSCRIPTION_ID EXCEPTION;
17:13:54 1040  BEGIN
17:13:54 1041  
17:13:54 1042  	 BEGIN
17:13:54 1043  	   SELECT
17:13:54 1044  	     SUBSCRIPTION.ID into temp_subscription_id
17:13:54 1045  	   FROM
17:13:54 1046  	     SUBSCRIPTION
17:13:54 1047  	   WHERE
17:13:54 1048  	     SUBSCRIPTION.ID = in_subscription_id;
17:13:54 1049  	   EXCEPTION
17:13:54 1050  	     WHEN NO_DATA_FOUND THEN
17:13:54 1051  	       RAISE BAD_SUBSCRIPTION_ID;
17:13:54 1052  	 END;
17:13:54 1053  
17:13:54 1054  	 BEGIN
17:13:54 1055  	   SELECT
17:13:54 1056  	     CHARGE.CHARGE_AMOUNT into var_recent_charge
17:13:54 1057  	   FROM
17:13:54 1058  	     LICENSE
17:13:54 1059  	     INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
17:13:54 1060  	     INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
17:13:54 1061  	     INNER JOIN CHARGE ON CHARGE.INVOICE_ID = INVOICE.ID
17:13:54 1062  	   WHERE
17:13:54 1063  	     -- TODO: Review
17:13:54 1064  	     -- LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V23.LICENSE_PROCESSED
17:13:54 1065  	     -- AND
17:13:54 1066  	     SUBSCRIPTION.ID = in_subscription_id
17:13:54 1067  	     AND CHARGE.CHARGE_AMOUNT >= 0
17:13:54 1068  	     AND ROWNUM <= 1
17:13:54 1069  	   ORDER BY
17:13:54 1070  	     LICENSE.ID ASC, CHARGE.ID DESC;
17:13:54 1071  	   EXCEPTION
17:13:54 1072  	     WHEN NO_DATA_FOUND THEN
17:13:54 1073  	       var_recent_charge := 0;
17:13:54 1074  	 END;
17:13:54 1075  
17:13:54 1076  	 RETURN var_recent_charge;
17:13:54 1077  
17:13:54 1078  EXCEPTION
17:13:54 1079  WHEN BAD_SUBSCRIPTION_ID THEN
17:13:54 1080  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 1081  	   SPROC_NAME, 'No such subscription');
17:13:54 1082  WHEN OTHERS THEN
17:13:54 1083  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 1084  	   SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 1085  END GET_RECENT_CHARGE;
17:13:54 1086  
17:13:54 1087  /******************************************************************************/
