17:13:54 275  PROCEDURE REACTIVATE_ACCOUNT (
17:13:54 276  	in_group_id   IN NUMBER,
17:13:54 277  	in_updated_by IN VARCHAR2,
17:13:54 278  	in_note       IN VARCHAR2,
17:13:54 279  	in_agent_id   IN NUMBER
17:13:54 280  ) AS
17:13:54 281  -- VARIABLES
17:13:54 282  SPROC_NAME	      CONSTANT VARCHAR2(18) := 'REACTIVATE_ACCOUNT';
17:13:54 283  var_account_id	      NUMBER;
17:13:54 284  current_account_status  NUMBER;
17:13:54 285  
17:13:54 286  -- EXCEPTIONS
17:13:54 287  BAD_CURRENT_ACC_STATUS EXCEPTION;
17:13:54 288  CAN_NOT_CREATE_NOTE    EXCEPTION;
17:13:54 289  EXCEPTION_MESSAGE      VARCHAR2(1024);
17:13:54 290  BEGIN
17:13:54 291  	-- Get account id, status
17:13:54 292  	SELECT
17:13:54 293  	  ACCOUNT.ACCOUNT_STATUS_ID,
17:13:54 294  	  ACCOUNT.ID
17:13:54 295  	INTO
17:13:54 296  	  current_account_status,
17:13:54 297  	  var_account_id
17:13:54 298  	FROM ACCOUNT
17:13:54 299  	WHERE
17:13:54 300  	  ACCOUNT.GROUP_ID = in_group_id;
17:13:54 301  
17:13:54 302  	IF current_account_status != GLOBAL_STATUSES_V23.ACCOUNT_FROZEN THEN
17:13:54 303  	  RAISE BAD_CURRENT_ACC_STATUS;
17:13:54 304  	END IF;
17:13:54 305  
17:13:54 306  	-- Change account status
17:13:54 307  	PROCS_ACCOUNT_V23.UPDATE_ACCOUNT_STATUS(
17:13:54 308  	  in_account_id        => var_account_id,
17:13:54 309  	  in_updated_by        => in_updated_by,
17:13:54 310  	  in_account_status_id => GLOBAL_STATUSES_V23.ACCOUNT_ACTIVE
17:13:54 311  	);
17:13:54 312  
17:13:54 313  	-- Add note
17:13:54 314  	BEGIN
17:13:54 315  	  PROCS_ACCOUNT_V23.ANNOTATE_ACCOUNT(
17:13:54 316  	    in_group_id   => in_group_id,
17:13:54 317  	    in_agent_id   => in_agent_id,
17:13:54 318  	    in_note	  => in_note,
17:13:54 319  	    in_created_by => in_updated_by
17:13:54 320  	  );
17:13:54 321  	  EXCEPTION
17:13:54 322  	    WHEN OTHERS THEN
17:13:54 323  	      EXCEPTION_MESSAGE := SQLERRM;
17:13:54 324  	      RAISE CAN_NOT_CREATE_NOTE;
17:13:54 325  	END;
17:13:54 326  
17:13:54 327  EXCEPTION
17:13:54 328  WHEN BAD_CURRENT_ACC_STATUS THEN
17:13:54 329  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
17:13:54 330  	  SPROC_NAME, 'Current account status is not "frozen"');
17:13:54 331  WHEN NO_DATA_FOUND THEN
17:13:54 332  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 333  	  SPROC_NAME, 'Could not find account with given group ID');
17:13:54 334  WHEN CAN_NOT_CREATE_NOTE THEN
17:13:54 335  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 336  	  SPROC_NAME, 'Could not annotate account');
17:13:54 337  WHEN OTHERS THEN
17:13:54 338  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 339  	  SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 340  END REACTIVATE_ACCOUNT;
