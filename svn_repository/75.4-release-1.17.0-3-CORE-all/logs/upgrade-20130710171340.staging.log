17:13:54 1366  /******************************************************************************/
17:13:54 1367  
17:13:54 1368  PROCEDURE ADD_SUBSCRIPTION_EXTENSION (
17:13:54 1369  	 in_subscription_id	 IN NUMBER,
17:13:54 1370  	 in_effective_start_date IN DATE,
17:13:54 1371  	 in_effective_end_date	 IN DATE,
17:13:54 1372  	 in_note		 IN VARCHAR2,
17:13:54 1373  	 in_updated_by		 IN VARCHAR2
17:13:54 1374  ) AS
17:13:54 1375  -- VARIABLES
17:13:54 1376  SPROC_NAME		    CONSTANT VARCHAR2(26) := 'ADD_SUBSCRIPTION_EXTENSION';
17:13:54 1377  temp_subscription_id	    NUMBER;
17:13:54 1378  var_current_license_id	    NUMBER;
17:13:54 1379  var_current_license_start_date DATE;
17:13:54 1380  var_current_license_end_date DATE;
17:13:54 1381  var_current_offer_id	    NUMBER;
17:13:54 1382  var_current_invoice_id	    NUMBER;
17:13:54 1383  var_current_date 	    DATE;
17:13:54 1384  var_current_offer_index	    NUMBER;
17:13:54 1385  var_current_offer_recurr_num NUMBER;
17:13:54 1386  var_account_tax_exempt_id    VARCHAR2(255);
17:13:54 1387  
17:13:54 1388  var_free_invoice_id NUMBER;
17:13:54 1389  var_free_license_id NUMBER;
17:13:54 1390  var_new_license_id  NUMBER;
17:13:54 1391  var_ext_license_id  NUMBER;
17:13:54 1392  -- EXCEPTIONS
17:13:54 1393  BAD_SUBSCRIPTION_ID	     EXCEPTION;
17:13:54 1394  CAN_NOT_FIND_OFFER_OR_LICENSE EXCEPTION;
17:13:54 1395  CAN_NOT_CHANGE_LICENSE_STATUS EXCEPTION;
17:13:54 1396  CAN_NOT_CREATE_INVOICE	     EXCEPTION;
17:13:54 1397  CAN_NOT_CREATE_NEW_LICENSE    EXCEPTION;
17:13:54 1398  CAN_NOT_CREATE_END_LICENSE    EXCEPTION;
17:13:54 1399  CAN_NOT_ANNOTATE_SUBSCRIPTION EXCEPTION;
17:13:54 1400  EXTENS_START_DATE_IS_TOO_FAR  EXCEPTION;
17:13:54 1401  EXT_START_DATE_LATER_THEN_END EXCEPTION;
17:13:54 1402  EXTENS_START_DATE_IS_TOO_SMALL EXCEPTION;
17:13:54 1403  EXCEPTION_MESSAGE	     VARCHAR2(1024);
17:13:54 1404  BEGIN
17:13:54 1405  
17:13:54 1406  	 var_current_date := PROCS_COMMON_V23.NORMALIZE_DATE(SYSDATE);
17:13:54 1407  
17:13:54 1408  	 -- Check that subscription exists
17:13:54 1409  	 BEGIN
17:13:54 1410  	   SELECT
17:13:54 1411  	     SUBSCRIPTION.ID into temp_subscription_id
17:13:54 1412  	   FROM
17:13:54 1413  	     SUBSCRIPTION
17:13:54 1414  	   WHERE
17:13:54 1415  	     SUBSCRIPTION.ID = in_subscription_id;
17:13:54 1416  	   EXCEPTION
17:13:54 1417  	     WHEN NO_DATA_FOUND THEN
17:13:54 1418  	       RAISE BAD_SUBSCRIPTION_ID;
17:13:54 1419  	 END;
17:13:54 1420  
17:13:54 1421  	 -- Get account tax exempt id
17:13:54 1422  	 SELECT DISTINCT
17:13:54 1423  	   ACCOUNT.TAX_EXEMPT_ID into var_account_tax_exempt_id
17:13:54 1424  	 FROM
17:13:54 1425  	   ACCOUNT
17:13:54 1426  	   INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.ACCOUNT_ID = ACCOUNT.ID
17:13:54 1427  	 WHERE
17:13:54 1428  	   SUBSCRIPTION.ID = in_subscription_id;
17:13:54 1429  
17:13:54 1430  	 -- Select current data
17:13:54 1431  	 BEGIN
17:13:54 1432  	   SELECT
17:13:54 1433  	     LICENSE.ID,
17:13:54 1434  	     LICENSE.START_DATE,
17:13:54 1435  	     LICENSE.END_DATE,
17:13:54 1436  	     LICENSE.CURRENT_OFFER_INDEX,
17:13:54 1437  	     LICENSE.CURRENT_OFFER_RECURR_NUM,
17:13:54 1438  	     OFFER.ID,
17:13:54 1439  	     INVOICE.ID
17:13:54 1440  	   INTO
17:13:54 1441  	     var_current_license_id,
17:13:54 1442  	     var_current_license_start_date,
17:13:54 1443  	     var_current_license_end_date,
17:13:54 1444  	     var_current_offer_index,
17:13:54 1445  	     var_current_offer_recurr_num,
17:13:54 1446  	     var_current_offer_id,
17:13:54 1447  	     var_current_invoice_id
17:13:54 1448  	   FROM
17:13:54 1449  	     LICENSE
17:13:54 1450  	     INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
17:13:54 1451  	     INNER JOIN OFFER ON LICENSE.OFFER_ID = OFFER.ID
17:13:54 1452  	     INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
17:13:54 1453  	   WHERE
17:13:54 1454  	     SUBSCRIPTION.ID = in_subscription_id
17:13:54 1455  	     AND PROCS_COMMON_V23.NORMALIZE_DATE(LICENSE.END_DATE) > var_current_date
17:13:54 1456  	     AND PROCS_COMMON_V23.NORMALIZE_DATE(LICENSE.START_DATE) <= var_current_date
17:13:54 1457  	     AND ROWNUM <= 1
17:13:54 1458  	   ORDER BY
17:13:54 1459  	     LICENSE.ID DESC;
17:13:54 1460  	   EXCEPTION
17:13:54 1461  	     WHEN NO_DATA_FOUND THEN
17:13:54 1462  	       RAISE CAN_NOT_FIND_OFFER_OR_LICENSE;
17:13:54 1463  	 END;
17:13:54 1464  
17:13:54 1465  	 IF var_current_license_end_date < in_effective_start_date THEN
17:13:54 1466  	   RAISE EXTENS_START_DATE_IS_TOO_FAR;
17:13:54 1467  	 END IF;
17:13:54 1468  
17:13:54 1469  	 IF var_current_license_start_date > in_effective_start_date THEN
17:13:54 1470  	   RAISE EXTENS_START_DATE_IS_TOO_SMALL;
17:13:54 1471  	 END IF;
17:13:54 1472  
17:13:54 1473  	 IF in_effective_start_date > in_effective_end_date THEN
17:13:54 1474  	   RAISE EXT_START_DATE_LATER_THEN_END;
17:13:54 1475  	 END IF;
17:13:54 1476  
17:13:54 1477  	 -- Closing curent license
17:13:54 1478  	 BEGIN
17:13:54 1479  	   PROCS_LICENSE_CRU_V23.UPDATE_LICENSE(
17:13:54 1480  	     in_license_id	   => var_current_license_id,
17:13:54 1481  	     in_updated_by	   => in_updated_by,
17:13:54 1482  	     in_license_status_id  => GLOBAL_STATUSES_V23.LICENSE_CLOSED,
17:13:54 1483  	     in_end_date	   => in_effective_start_date,
17:13:54 1484  	     in_needs_entitlements => GLOBAL_CONSTANTS_V23.TRUE
17:13:54 1485  	   );
17:13:54 1486  	   EXCEPTION
17:13:54 1487  	     WHEN OTHERS THEN
17:13:54 1488  	       EXCEPTION_MESSAGE := SQLERRM;
17:13:54 1489  	       RAISE CAN_NOT_CHANGE_LICENSE_STATUS;
17:13:54 1490  	 END;
17:13:54 1491  
17:13:54 1492  	 -- Creating new "free" invoice
17:13:54 1493  	 BEGIN
17:13:54 1494  	   PROCS_INVOICE_V23.CREATE_INVOICE(
17:13:54 1495  	     in_invoice_status => GLOBAL_STATUSES_V23.INVOICE_CLOSED,
17:13:54 1496  	     in_created_by     => in_updated_by,
17:13:54 1497  	     in_tax_exempt_id  => var_account_tax_exempt_id,
17:13:54 1498  	     out_invoice_id    => var_free_invoice_id
17:13:54 1499  	   );
17:13:54 1500  	   EXCEPTION
17:13:54 1501  	     WHEN OTHERS THEN
17:13:54 1502  	       EXCEPTION_MESSAGE := SQLERRM;
17:13:54 1503  	       RAISE CAN_NOT_CREATE_INVOICE;
17:13:54 1504  	 END;
17:13:54 1505  
17:13:54 1506  	 -- Creating new "free" license
17:13:54 1507  	 BEGIN
17:13:54 1508  	   PROCS_LICENSE_V23.CREATE_LICENSE (
17:13:54 1509  	     in_status_id		 => GLOBAL_STATUSES_V23.LICENSE_ACTIVE,
17:13:54 1510  	     in_needs_entitlements	 => GLOBAL_CONSTANTS_V23.TRUE,
17:13:54 1511  	     in_start_date		 => in_effective_start_date,
17:13:54 1512  	     in_end_date		 => in_effective_end_date,
17:13:54 1513  	     in_offer_id		 => var_current_offer_id,
17:13:54 1514  	     in_subscription_id 	 => in_subscription_id,
17:13:54 1515  	     in_invoice_id		 => var_free_invoice_id,
17:13:54 1516  	     in_created_by		 => in_updated_by,
17:13:54 1517  	     in_is_extension		 => GLOBAL_CONSTANTS_V23.TRUE,
17:13:54 1518  	     in_current_offer_index	 => var_current_offer_index,
17:13:54 1519  	     in_current_offer_recurr_num => var_current_offer_recurr_num,
17:13:54 1520  	     out_license_id		 => var_free_license_id
17:13:54 1521  	   );
17:13:54 1522  	   EXCEPTION
17:13:54 1523  	     WHEN OTHERS THEN
17:13:54 1524  	       EXCEPTION_MESSAGE := SQLERRM;
17:13:54 1525  	       RAISE CAN_NOT_CREATE_NEW_LICENSE;
17:13:54 1526  	 END;
17:13:54 1527  
17:13:54 1528  	 -- Creating new license
17:13:54 1529  	 IF PROCS_COMMON_V23.NORMALIZE_DATE(var_current_license_end_date) >
17:13:54 1530  	    PROCS_COMMON_V23.NORMALIZE_DATE(in_effective_start_date) THEN
17:13:54 1531  	   BEGIN
17:13:54 1532  	     PROCS_LICENSE_V23.CREATE_LICENSE (
17:13:54 1533  	       in_status_id		   => GLOBAL_STATUSES_V23.LICENSE_ACTIVE,
17:13:54 1534  	       in_needs_entitlements	   => GLOBAL_CONSTANTS_V23.TRUE,
17:13:54 1535  	       in_start_date		   => in_effective_end_date,
17:13:54 1536  	       in_end_date		   => var_current_license_end_date + (in_effective_end_date - in_effective_start_date),
17:13:54 1537  	       in_offer_id		   => var_current_offer_id,
17:13:54 1538  	       in_subscription_id	   => in_subscription_id,
17:13:54 1539  	       in_invoice_id		   => var_current_invoice_id,
17:13:54 1540  	       in_created_by		   => in_updated_by,
17:13:54 1541  	       in_is_extension		   => GLOBAL_CONSTANTS_V23.FALSE,
17:13:54 1542  	       in_current_offer_index	   => var_current_offer_index,
17:13:54 1543  	       in_current_offer_recurr_num => var_current_offer_recurr_num,
17:13:54 1544  	       out_license_id		   => var_ext_license_id
17:13:54 1545  	     );
17:13:54 1546  	     EXCEPTION
17:13:54 1547  	       WHEN OTHERS THEN
17:13:54 1548  		 EXCEPTION_MESSAGE := SQLERRM;
17:13:54 1549  		 RAISE CAN_NOT_CREATE_END_LICENSE;
17:13:54 1550  	   END;
17:13:54 1551  	 END IF;
17:13:54 1552  
17:13:54 1553  	 -- Create new note for subscription
17:13:54 1554  	 BEGIN
17:13:54 1555  	   PROCS_SUBSCRIPTION_V23.ANNOTATE_SUBSCRIPTION (
17:13:54 1556  	     in_subscription_id => in_subscription_id,
17:13:54 1557  	     in_agent_id	=> 0, -- FIXME: What should to be here (agent id)?
17:13:54 1558  	     in_note		=> in_note,
17:13:54 1559  	     in_created_by	=> in_updated_by
17:13:54 1560  	   );
17:13:54 1561  	   EXCEPTION
17:13:54 1562  	     WHEN OTHERS THEN
17:13:54 1563  	       EXCEPTION_MESSAGE := SQLERRM;
17:13:54 1564  	       RAISE CAN_NOT_ANNOTATE_SUBSCRIPTION;
17:13:54 1565  	 END;
17:13:54 1566  
17:13:54 1567  EXCEPTION
17:13:54 1568  WHEN EXT_START_DATE_LATER_THEN_END THEN
17:13:54 1569  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INVALID_PARAMETER,
17:13:54 1570  	   SPROC_NAME, 'Extension start date is bigger then end date');
17:13:54 1571  WHEN EXTENS_START_DATE_IS_TOO_FAR THEN
17:13:54 1572  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INVALID_PARAMETER,
17:13:54 1573  	   SPROC_NAME, 'Extension start date is too far');
17:13:54 1574  WHEN EXTENS_START_DATE_IS_TOO_SMALL THEN
17:13:54 1575  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INVALID_PARAMETER,
17:13:54 1576  	   SPROC_NAME, 'Extension start date is too small');
17:13:54 1577  WHEN BAD_SUBSCRIPTION_ID THEN
17:13:54 1578  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 1579  	   SPROC_NAME, 'No such subscription');
17:13:54 1580  WHEN CAN_NOT_FIND_OFFER_OR_LICENSE THEN
17:13:54 1581  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 1582  	   SPROC_NAME, 'Could not find license and/or offer for given subscription');
17:13:54 1583  WHEN CAN_NOT_CHANGE_LICENSE_STATUS THEN
17:13:54 1584  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 1585  	   SPROC_NAME, 'Could not change license status', EXCEPTION_MESSAGE);
17:13:54 1586  WHEN CAN_NOT_CREATE_INVOICE THEN
17:13:54 1587  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 1588  	   SPROC_NAME, 'Could not create new invoice', EXCEPTION_MESSAGE);
17:13:54 1589  WHEN CAN_NOT_CREATE_NEW_LICENSE THEN
17:13:54 1590  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 1591  	   SPROC_NAME, 'Could not create new license', EXCEPTION_MESSAGE);
17:13:54 1592  WHEN CAN_NOT_CREATE_END_LICENSE THEN
17:13:54 1593  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 1594  	   SPROC_NAME, 'Could not create last license', EXCEPTION_MESSAGE);
17:13:54 1595  WHEN CAN_NOT_ANNOTATE_SUBSCRIPTION THEN
17:13:54 1596  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 1597  	   SPROC_NAME, 'Could not create new note for subscription', EXCEPTION_MESSAGE);
17:13:54 1598  WHEN OTHERS THEN
17:13:54 1599  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 1600  	   SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 1601  END ADD_SUBSCRIPTION_EXTENSION;
17:13:54 1602  
17:13:54 1603  /******************************************************************************/
17:13:54 1604  
17:13:54 1605  FUNCTION CALC_SUBSCRIPTION_END_DATE (
17:13:54 1606  	 in_subscription_id IN NUMBER
17:13:54 1607  ) RETURN DATE AS
17:13:54 1608  -- VARIABLES
17:13:54 1609  SPROC_NAME		     CONSTANT VARCHAR2(26) := 'CALC_SUBSCRIPTION_END_DATE';
17:13:54 1610  last_license_id		     NUMBER;
17:13:54 1611  last_license_end_date	     DATE;
17:13:54 1612  last_license_offer_id	     NUMBER;
17:13:54 1613  last_license_offer_index      NUMBER;
17:13:54 1614  last_license_offer_recurr_num NUMBER;
17:13:54 1615  var_last_license_id	     NUMBER;
17:13:54 1616  var_offer_chain_id	     NUMBER;
17:13:54 1617  var_current_offer_rec_number  NUMBER;
17:13:54 1618  var_next_offers_set	     SYS_REFCURSOR;
17:13:54 1619  var_next_offer_duration	     VARCHAR2(30);
17:13:54 1620  var_next_offer_recur	     NUMBER;
17:13:54 1621  var_infinity_offers_number    NUMBER;
17:13:54 1622  
17:13:54 1623  var_result_date DATE;
17:13:54 1624  
17:13:54 1625  var_ym_interval	INTERVAL YEAR TO MONTH;
17:13:54 1626  var_ds_interval	INTERVAL DAY(3) TO SECOND;
17:13:54 1627  var_offer_years	NUMBER;
17:13:54 1628  var_offer_months NUMBER;
17:13:54 1629  var_offer_days	NUMBER;
17:13:54 1630  
17:13:54 1631  -- EXCEPTIONS
17:13:54 1632  BAD_SUBSCRIPTION_ID	  EXCEPTION;
17:13:54 1633  CAN_NOT_FIND_LAST_LICENSE  EXCEPTION;
17:13:54 1634  CAN_NOT_CALC_OFFER_LENGTH  EXCEPTION;
17:13:54 1635  CAN_NOT_CALC_OFFER_LENGTH2 EXCEPTION;
17:13:54 1636  EXCEPTION_MESSAGE	  VARCHAR2(1024);
17:13:54 1637  BEGIN
17:13:54 1638  
17:13:54 1639  	 BEGIN
17:13:54 1640  	   SELECT
17:13:54 1641  	     SUBSCRIPTION.OFFER_CHAIN_ID into var_offer_chain_id
17:13:54 1642  	   FROM
17:13:54 1643  	     SUBSCRIPTION
17:13:54 1644  	   WHERE
17:13:54 1645  	     SUBSCRIPTION.ID = in_subscription_id;
17:13:54 1646  	   EXCEPTION
17:13:54 1647  	     WHEN NO_DATA_FOUND THEN
17:13:54 1648  	       RAISE BAD_SUBSCRIPTION_ID;
17:13:54 1649  	 END;
17:13:54 1650  
17:13:54 1651  	 BEGIN
17:13:54 1652  	   SELECT
17:13:54 1653  	     ID into var_last_license_id
17:13:54 1654  	   FROM
17:13:54 1655  	     (
17:13:54 1656  	       SELECT
17:13:54 1657  		 LICENSE.ID
17:13:54 1658  	       FROM
17:13:54 1659  		 LICENSE
17:13:54 1660  	       WHERE
17:13:54 1661  		 LICENSE.SUBSCRIPTION_ID = in_subscription_id
17:13:54 1662  	       ORDER BY
17:13:54 1663  		 LICENSE.END_DATE DESC
17:13:54 1664  	     )
17:13:54 1665  	   WHERE
17:13:54 1666  	     ROWNUM <= 1;
17:13:54 1667  	   EXCEPTION
17:13:54 1668  	     WHEN NO_DATA_FOUND THEN
17:13:54 1669  	       RAISE CAN_NOT_FIND_LAST_LICENSE;
17:13:54 1670  	 END;
17:13:54 1671  
17:13:54 1672  	 SELECT
17:13:54 1673  	   COUNT(*) into var_infinity_offers_number
17:13:54 1674  	 FROM
17:13:54 1675  	   OFFER_OFFER_CHAIN
17:13:54 1676  	 WHERE
17:13:54 1677  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
17:13:54 1678  	   AND OFFER_OFFER_CHAIN.NUM_RECURRENCES = GLOBAL_ENUMS_V23.OFFER_REC_INFINITY;
17:13:54 1679  
17:13:54 1680  	 IF var_infinity_offers_number > 0 THEN
17:13:54 1681  	   RETURN NULL;
17:13:54 1682  	 END IF;
17:13:54 1683  
17:13:54 1684  	 BEGIN
17:13:54 1685  	   SELECT
17:13:54 1686  	     LICENSE.ID,
17:13:54 1687  	     LICENSE.END_DATE,
17:13:54 1688  	     LICENSE.CURRENT_OFFER_INDEX,
17:13:54 1689  	     LICENSE.CURRENT_OFFER_RECURR_NUM,
17:13:54 1690  	     LICENSE.OFFER_ID
17:13:54 1691  	     into
17:13:54 1692  	     last_license_id,
17:13:54 1693  	     last_license_end_date,
17:13:54 1694  	     last_license_offer_index,
17:13:54 1695  	     last_license_offer_recurr_num,
17:13:54 1696  	     last_license_offer_id
17:13:54 1697  	   FROM
17:13:54 1698  	     LICENSE
17:13:54 1699  	   WHERE
17:13:54 1700  	     LICENSE.ID = var_last_license_id;
17:13:54 1701  	   EXCEPTION
17:13:54 1702  	     WHEN NO_DATA_FOUND THEN
17:13:54 1703  	       RAISE CAN_NOT_FIND_LAST_LICENSE;
17:13:54 1704  	 END;
17:13:54 1705  
17:13:54 1706  	 var_result_date := last_license_end_date;
17:13:54 1707  
17:13:54 1708  	 -- Find current recurrence number
17:13:54 1709  	 SELECT
17:13:54 1710  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES into var_current_offer_rec_number
17:13:54 1711  	 FROM
17:13:54 1712  	   OFFER_OFFER_CHAIN
17:13:54 1713  	 WHERE
17:13:54 1714  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
17:13:54 1715  	   AND OFFER_OFFER_CHAIN.OFFER_ID = last_license_offer_id
17:13:54 1716  	   AND OFFER_OFFER_CHAIN.INDEX_VALUE = last_license_offer_index;
17:13:54 1717  
17:13:54 1718  	 IF var_current_offer_rec_number > last_license_offer_recurr_num THEN
17:13:54 1719  	   BEGIN
17:13:54 1720  	     PROCS_OFFER_CHAIN_V23.GET_OFFER_LENGTH(
17:13:54 1721  	       last_license_offer_id,
17:13:54 1722  	       var_offer_years,
17:13:54 1723  	       var_offer_months,
17:13:54 1724  	       var_offer_days
17:13:54 1725  	     );
17:13:54 1726  
17:13:54 1727  	     var_ym_interval := var_offer_years||'-'||var_offer_months;
17:13:54 1728  	     var_ds_interval := var_offer_days||' 0:0:0';
17:13:54 1729  
17:13:54 1730  	     var_result_date := var_result_date
17:13:54 1731  	       + ( var_ym_interval * ( var_current_offer_rec_number - last_license_offer_recurr_num ) )
17:13:54 1732  	       + ( var_ds_interval * ( var_current_offer_rec_number - last_license_offer_recurr_num ) );
17:13:54 1733  	     EXCEPTION
17:13:54 1734  	       WHEN OTHERS THEN
17:13:54 1735  		 EXCEPTION_MESSAGE := SQLERRM;
17:13:54 1736  		 RAISE CAN_NOT_CALC_OFFER_LENGTH;
17:13:54 1737  	   END;
17:13:54 1738  	 END IF;
17:13:54 1739  
17:13:54 1740  	 OPEN var_next_offers_set FOR
17:13:54 1741  	 SELECT
17:13:54 1742  	   OFFER.ENTITLEMENT_DURATION,
17:13:54 1743  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES
17:13:54 1744  	 FROM
17:13:54 1745  	   OFFER_OFFER_CHAIN
17:13:54 1746  	   INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
17:13:54 1747  	 WHERE
17:13:54 1748  	   OFFER_OFFER_CHAIN.INDEX_VALUE > last_license_offer_index
17:13:54 1749  	   AND OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id;
17:13:54 1750  
17:13:54 1751  	 LOOP
17:13:54 1752  	   FETCH var_next_offers_set into var_next_offer_duration, var_next_offer_recur;
17:13:54 1753  	   EXIT WHEN var_next_offers_set%NOTFOUND;
17:13:54 1754  	   BEGIN
17:13:54 1755  	     PROCS_COMMON_V23.ISO8601DURATION_TO_INTERVALS(
17:13:54 1756  	       var_next_offer_duration,
17:13:54 1757  	       var_offer_years,
17:13:54 1758  	       var_offer_months,
17:13:54 1759  	       var_offer_days
17:13:54 1760  	     );
17:13:54 1761  
17:13:54 1762  	     var_ym_interval := var_offer_years||'-'||var_offer_months;
17:13:54 1763  	     var_ds_interval := var_offer_days||' 0:0:0';
17:13:54 1764  
17:13:54 1765  	     var_result_date := var_result_date
17:13:54 1766  	       + ( var_ym_interval * var_next_offer_recur )
17:13:54 1767  	       + ( var_ds_interval * var_next_offer_recur );
17:13:54 1768  	     EXCEPTION
17:13:54 1769  	       WHEN OTHERS THEN
17:13:54 1770  		 EXCEPTION_MESSAGE := SQLERRM;
17:13:54 1771  		 RAISE CAN_NOT_CALC_OFFER_LENGTH2;
17:13:54 1772  	   END;
17:13:54 1773  	 END LOOP;
17:13:54 1774  
17:13:54 1775  	 RETURN var_result_date;
17:13:54 1776  
17:13:54 1777  EXCEPTION
17:13:54 1778  WHEN BAD_SUBSCRIPTION_ID THEN
17:13:54 1779  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 1780  	   SPROC_NAME, 'No such subscription');
17:13:54 1781  WHEN CAN_NOT_FIND_LAST_LICENSE THEN
17:13:54 1782  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 1783  	   SPROC_NAME, 'Could not find last license for given subscription');
17:13:54 1784  WHEN CAN_NOT_CALC_OFFER_LENGTH THEN
17:13:54 1785  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 1786  	   SPROC_NAME, 'Could not calculate offer length', EXCEPTION_MESSAGE);
17:13:54 1787  WHEN CAN_NOT_CALC_OFFER_LENGTH2 THEN
17:13:54 1788  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 1789  	   SPROC_NAME, 'Could not calculate last offer length', EXCEPTION_MESSAGE);
17:13:54 1790  WHEN OTHERS THEN
17:13:54 1791  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 1792  	   SPROC_NAME, 'Unkown error', SQLERRM);
17:13:54 1793  END CALC_SUBSCRIPTION_END_DATE;
