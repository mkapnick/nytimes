17:13:54 426  		  ss.GROUP_ACCOUNT_ID = ga.ID AND
17:13:54 427  		  GA.SUBSCRIPTION_ID = l.SUBSCRIPTION_ID AND
17:13:54 428  		  SYSDATE BETWEEN l.START_DATE AND l.ENTITLEMENT_END_DATE AND
17:13:54 429  		  SYSDATE BETWEEN ss.START_DATE AND ss.END_DATE AND
17:13:54 430  		  ss.BORROWER_ACCOUNT_ID  = a.ID
17:13:54 431  	      ) Q
17:13:54 432  	    WHERE rownum <= l_end)
17:13:54 433  	  WHERE rnum >= l_start;
17:13:54 434  	END;
