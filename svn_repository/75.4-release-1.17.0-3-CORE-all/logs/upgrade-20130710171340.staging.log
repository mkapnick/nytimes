17:13:54 867  		AND SYSDATE + in_days_before_close >= l.grace_end_date
17:13:54 868  		AND NOT EXISTS
17:13:54 869  		    (
17:13:54 870  			SELECT
17:13:54 871  			    1
17:13:54 872  			FROM
17:13:54 873  			    charge c
