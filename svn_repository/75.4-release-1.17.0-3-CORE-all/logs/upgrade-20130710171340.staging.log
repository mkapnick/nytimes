17:13:53   5  	PROCEDURE POPULATE_REQUEST_INFO(
17:13:53   6  	  in_hours_prior    IN	NUMBER,
17:13:53   7  	  in_filename	    IN	CC_REQUEST_FILE.FILE_NAME%TYPE,
17:13:53   8  	  in_creator	    IN	CC_REQUEST_FILE.UPDATED_BY%TYPE
17:13:53   9  	) AS
17:13:53  10  	SPROC_NAME CONSTANT VARCHAR2(32) := 'POPULATE_REQUEST_INFO';
17:13:53  11  	var_start_date	    DATE := SYSDATE;
17:13:53  12  	var_end_date	    DATE := var_start_date + (in_hours_prior/24);
17:13:53  13  	var_request_file_id NUMBER := 0;
17:13:53  14  	var_license_count   NUMBER := 0;
17:13:53  15  	var_cc_update_count NUMBER := 0;
17:13:53  16  	BEGIN
17:13:53  17  	    SELECT CC_REQUEST_FILE_ID_SEQ.NEXTVAL INTO var_request_file_id  FROM DUAL;
17:13:53  18  	    INSERT INTO CC_REQUEST_FILE (ID,
17:13:53  19  					 FILE_NAME,
17:13:53  20  					 CC_REQUEST_FILE_STATUS,
17:13:53  21  					 CREATE_DATE,
17:13:53  22  					 CREATED_BY,
17:13:53  23  					 UPDATE_DATE,
17:13:53  24  					 UPDATED_BY)
17:13:53  25  					 VALUES (
17:13:53  26  					 var_request_file_id,
17:13:53  27  					 in_filename,
17:13:53  28  					 'NOT_CREATED',
17:13:53  29  					 var_start_date,
17:13:53  30  					 in_creator,
17:13:53  31  					 var_start_date,
17:13:53  32  					 in_creator);
17:13:53  33  
17:13:53  34  	   FOR record IN (SELECT
17:13:53  35  			    l.ID LICENSE_ID, cc.ID CREDIT_CARD_ID
17:13:53  36  			  FROM
17:13:53  37  			    LICENSE l INNER JOIN SUBSCRIPTION s ON L.SUBSCRIPTION_ID = s.ID
17:13:53  38  				      INNER JOIN CREDIT_CARD cc ON S.INSTRUMENT_ID   = cc.ID
17:13:53  39  			  WHERE
17:13:53  40  			    s.INSTRUMENT_TYPE_ID	 = 1
17:13:53  41  			    AND cc.CREDIT_CARD_STATUS_ID = 1
17:13:53  42  			    AND s.SUBSCRIPTION_STATUS_ID = 1
17:13:53  43  			    AND l.LICENSE_STATUS_ID	 = 2
17:13:53  44  			    AND cc.CREDIT_CARD_TYPE_ID IN (2,3)
17:13:53  45  			    AND l.END_DATE BETWEEN var_start_date AND var_end_date
17:13:53  46  			    AND l.ID NOT IN (SELECT LICENSE_ID FROM CC_UPDATE))
17:13:53  47  	   LOOP
17:13:53  48  	     var_license_count := 0;
17:13:53  49  	     SELECT COUNT(1) INTO  var_license_count FROM CC_UPDATE WHERE LICENSE_ID = record.LICENSE_ID;
17:13:53  50  
17:13:53  51  	     IF var_license_count = 0 THEN
17:13:53  52  		INSERT INTO CC_UPDATE (ID,
17:13:53  53  				       CREDIT_CARD_ID,
17:13:53  54  				       LICENSE_ID,
17:13:53  55  				       CC_UPDATE_STATUS,
17:13:53  56  				       CC_REQUEST_FILE_ID,
17:13:53  57  				       CREATE_DATE,
17:13:53  58  				       UPDATE_DATE,
17:13:53  59  				       CREATED_BY,
17:13:53  60  				       UPDATED_BY
17:13:53  61  				       ) VALUES (
17:13:53  62  				       CC_UPDATE_SEQ.NEXTVAL,
17:13:53  63  				       record.CREDIT_CARD_ID,
17:13:53  64  				       record.LICENSE_ID,
17:13:53  65  				       'NOT_ADDED_TO_FILE',
17:13:53  66  				       var_request_file_id,
17:13:53  67  				       var_start_date,
17:13:53  68  				       var_start_date,
17:13:53  69  				       in_creator,
17:13:53  70  				       in_creator
17:13:53  71  				       );
17:13:53  72  	     END IF;
17:13:53  73  	   END LOOP;
17:13:53  74  
17:13:53  75  	   SELECT COUNT(1) INTO var_cc_update_count
17:13:53  76  	   FROM CC_UPDATE
17:13:53  77  	   WHERE CC_REQUEST_FILE_ID = var_request_file_id;
17:13:53  78  	   IF var_cc_update_count <= 0 THEN
17:13:53  79  	     UPDATE CC_REQUEST_FILE
17:13:53  80  	     SET CC_REQUEST_FILE_STATUS = 'EMPTY'
17:13:53  81  	     WHERE ID = var_request_file_id;
17:13:53  82  	   END IF;
17:13:53  83  
17:13:53  84  	END POPULATE_REQUEST_INFO;
