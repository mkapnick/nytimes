17:13:54 1227  -- EXCEPTIONS
17:13:54 1228  BAD_INVOICE_ID EXCEPTION;
17:13:54 1229  BEGIN
17:13:54 1230  
17:13:54 1231  	 BEGIN
17:13:54 1232  	   SELECT
