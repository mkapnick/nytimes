17:13:54 695  
17:13:54 696  PROCEDURE GET_ENDED_GC_LICENSES (
17:13:54 697  	out_result_set		OUT SYS_REFCURSOR,
17:13:54 698  	in_hours_number 	IN NUMBER DEFAULT 14*24,
17:13:54 699  	in_num_rows		IN NUMBER DEFAULT 10000,
17:13:54 700  	in_process_name IN PROCESS_RETRY_THROTTLE.PROCESS_NAME%TYPE
17:13:54 701  ) AS
17:13:54 702  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_ENDED_GC_INVOICES';
17:13:54 703  var_days		NUMBER;
17:13:54 704  var_hours 	NUMBER;
17:13:54 705  var_time_interval INTERVAL DAY (3) TO SECOND;
17:13:54 706  BEGIN
17:13:54 707  	var_hours := mod(in_hours_number,24);
17:13:54 708  	var_days := (in_hours_number - var_hours) / 24;
17:13:54 709  	var_time_interval := var_days||' '||var_hours||':0:0';
17:13:54 710  	OPEN out_result_set FOR
17:13:54 711  SELECT * FROM
17:13:54 712  (
17:13:54 713  	SELECT
17:13:54 714  	  l.ID,
17:13:54 715  	  l.CREATE_DATE,
17:13:54 716  	  l.CREATED_BY,
17:13:54 717  	  l.CURRENT_OFFER_INDEX,
17:13:54 718  	  l.CURRENT_OFFER_RECURR_NUM,
17:13:54 719  	  l.END_DATE,
