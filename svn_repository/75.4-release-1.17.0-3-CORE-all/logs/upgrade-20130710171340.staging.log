17:13:54 384  ) AS
17:13:54 385  SPROC_NAME     CONSTANT VARCHAR2(29) := 'GET_ACCOUNT_GIFT_CERTIFICATES';
17:13:54 386  var_account_id NUMBER;
17:13:54 387  
17:13:54 388  -- Exceptions
17:13:54 389  WRONG_INSTR_EXCEPTION	 EXCEPTION;
17:13:54 390  BEGIN
17:13:54 391  
17:13:54 392  	-- Get account id
17:13:54 393  	SELECT
17:13:54 394  	  ACCOUNT.ID INTO var_account_id
17:13:54 395  	FROM
17:13:54 396  	  ACCOUNT
17:13:54 397  	WHERE
17:13:54 398  	  ACCOUNT.GROUP_ID = in_group_id;
17:13:54 399  
17:13:54 400  	-- Check that incoming data is correct
17:13:54 401  	IF in_instr_status != GLOBAL_CONSTANTS_V23.TRUE AND in_instr_status != GLOBAL_CONSTANTS_V23.FALSE THEN
17:13:54 402  	  RAISE WRONG_INSTR_EXCEPTION;
17:13:54 403  	END IF;
17:13:54 404  
17:13:54 405  	OPEN out_result_gc_set FOR
17:13:54 406  	SELECT
17:13:54 407  	  GIFT_CERTIFICATE.ID,
17:13:54 408  	  GIFT_CERTIFICATE.PURCHASER_GROUP_ID,
17:13:54 409  	  GIFT_CERTIFICATE.PURCHASE_INVOICE_ID,
17:13:54 410  	  GIFT_CERTIFICATE.PURCHASE_DATE,
17:13:54 411  	  GIFT_CERTIFICATE.OFFER_CHAIN_ID,
17:13:54 412  	  GIFT_CERTIFICATE.EXPIRATION_DATE,
17:13:54 413  	  GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID,
17:13:54 414  	  GIFT_CERTIFICATE.CODE,
17:13:54 415  	  GIFT_CERTIFICATE.CREATE_DATE,
17:13:54 416  	  GIFT_CERTIFICATE.CREATED_BY,
17:13:54 417  	  GIFT_CERTIFICATE.UPDATE_DATE,
17:13:54 418  	  GIFT_CERTIFICATE.UPDATED_BY,
17:13:54 419  	  GIFT_CERTIFICATE.RECIPIENT_NAME,
17:13:54 420  	  GIFT_CERTIFICATE.RECIPIENT_EMAIL,
17:13:54 421  	  GIFT_CERTIFICATE.SENDER_NAME,
17:13:54 422  	  GIFT_CERTIFICATE.SENDER_EMAIL,
17:13:54 423  	  GIFT_CERTIFICATE.REDEEMER_GROUP_ID,
17:13:54 424  	  GIFT_CERTIFICATE.REDEMPTION_DATE,
17:13:54 425  	  GIFT_CERTIFICATE.FINALIZED_INVOICE_ID,
17:13:54 426  	  GIFT_CERTIFICATE.GIFT_MESSAGE
17:13:54 427  	FROM
17:13:54 428  	  GIFT_CERTIFICATE
17:13:54 429  	WHERE
17:13:54 430  	  GIFT_CERTIFICATE.PURCHASER_GROUP_ID = in_group_id
17:13:54 431  	  AND (
17:13:54 432  		( in_instr_status = GLOBAL_CONSTANTS_V23.TRUE AND
17:13:54 433  		  (
17:13:54 434  		    GIFT_CERTIFICATE.EXPIRATION_DATE >= current_date
17:13:54 435  		    AND GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID = GLOBAL_STATUSES_V23.GIFT_CERTIFICATE_ACTIVE
17:13:54 436  		  )
17:13:54 437  		)
17:13:54 438  		OR
17:13:54 439  		(
17:13:54 440  		  in_instr_status = GLOBAL_CONSTANTS_V23.FALSE AND
17:13:54 441  		  (
17:13:54 442  		    GIFT_CERTIFICATE.EXPIRATION_DATE < current_date
17:13:54 443  		    OR GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID != GLOBAL_STATUSES_V23.GIFT_CERTIFICATE_ACTIVE
17:13:54 444  		  )
17:13:54 445  		)
17:13:54 446  	      );
17:13:54 447  
17:13:54 448  EXCEPTION
17:13:54 449  WHEN NO_DATA_FOUND THEN
17:13:54 450  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 451  	  SPROC_NAME, 'Can not find account with given group id');
17:13:54 452  WHEN WRONG_INSTR_EXCEPTION THEN
17:13:54 453  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INVALID_PARAMETER,
17:13:54 454  	  SPROC_NAME, 'Wrong gift certificate type');
17:13:54 455  WHEN OTHERS THEN
17:13:54 456  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 457  	  SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 458  END GET_ACCOUNT_GIFT_CERTIFICATES;
