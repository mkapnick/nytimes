17:13:54 886  
17:13:54 887  PROCEDURE APPLY_REFUND (
17:13:54 888  	in_invoice_id	     IN NUMBER,
17:13:54 889  	in_chargeback_amount IN NUMBER,
17:13:54 890  	in_created_by	     IN VARCHAR2,
17:13:54 891  	out_charge_id	     OUT NUMBER
17:13:54 892  ) AS
17:13:54 893  -- VARIABLES
17:13:54 894  SPROC_NAME		 CONSTANT VARCHAR2(16) := 'APPLY_CHARGEBACK';
17:13:54 895  temp_invoice_id		 NUMBER;
17:13:54 896  var_total_charges_amount	 NUMBER(10,2);
17:13:54 897  var_charge_amount_to_apply NUMBER(10,2);
17:13:54 898  var_transaction_id	 NUMBER;
17:13:54 899  var_instrument_type_id	 NUMBER;
17:13:54 900  var_instrument_id 	 NUMBER;
17:13:54 901  var_charge_id		 NUMBER;
17:13:54 902  -- EXCEPTIONS
17:13:54 903  BAD_INVOICE_ID		 EXCEPTION;
17:13:54 904  CAN_NOT_CREATE_TRANSACTION EXCEPTION;
17:13:54 905  CAN_NOT_FIND_INSTRUMENT	 EXCEPTION;
17:13:54 906  CAN_NOT_CREATE_CHARGE	 EXCEPTION;
17:13:54 907  EXCEPTION_MESSAGE 	 VARCHAR2(1024);
17:13:54 908  BEGIN
17:13:54 909  
17:13:54 910  	BEGIN
17:13:54 911  	  SELECT
17:13:54 912  	    INVOICE.ID into temp_invoice_id
17:13:54 913  	  FROM
17:13:54 914  	    INVOICE
17:13:54 915  	  WHERE
17:13:54 916  	    INVOICE.ID = in_invoice_id;
17:13:54 917  	  EXCEPTION
17:13:54 918  	    WHEN NO_DATA_FOUND THEN
17:13:54 919  	      RAISE BAD_INVOICE_ID;
17:13:54 920  	END;
17:13:54 921  
17:13:54 922  	SELECT
17:13:54 923  	  SUM (CHARGE.CHARGE_AMOUNT) into var_total_charges_amount
17:13:54 924  	FROM
17:13:54 925  	  CHARGE
17:13:54 926  	WHERE
17:13:54 927  	  CHARGE.INVOICE_ID = in_invoice_id
17:13:54 928  	  AND CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V23.CHARGE_PROCESSED
17:13:54 929  	  AND CHARGE.INSTRUMENT_TYPE_ID != GLOBAL_ENUMS_V23.INSTRUMENT_GIFT_CERTIFICATE;
17:13:54 930  
17:13:54 931  	-- FIXME: Maybe whe should throw exception here?
17:13:54 932  	IF var_total_charges_amount < in_chargeback_amount THEN
17:13:54 933  	  var_charge_amount_to_apply := var_total_charges_amount;
17:13:54 934  	ELSE
17:13:54 935  	  var_charge_amount_to_apply := in_chargeback_amount;
17:13:54 936  	END IF;
17:13:54 937  
17:13:54 938  	BEGIN
17:13:54 939  	  PROCS_TRANSACTION_V23.CREATE_TRANSACTION(
17:13:54 940  	    in_transaction_id	      => NULL,
17:13:54 941  	    in_status_id	      => GLOBAL_STATUSES_V23.TRANSACTION_PREPARE,
17:13:54 942  	    in_amount		      => -var_charge_amount_to_apply,
17:13:54 943  	    in_created_by	      => in_created_by,
17:13:54 944  	    in_order_id 	      => NULL,
17:13:54 945  	    in_is_refund	      => GLOBAL_CONSTANTS_V23.TRUE,
17:13:54 946  	    in_transaction_type_code  => 'REFUND',
17:13:54 947  	    out_transaction_id	      => var_transaction_id
17:13:54 948  	  );
17:13:54 949  	  EXCEPTION
17:13:54 950  	    WHEN OTHERS THEN
17:13:54 951  	      EXCEPTION_MESSAGE := SQLERRM;
17:13:54 952  	      RAISE CAN_NOT_CREATE_TRANSACTION;
17:13:54 953  	END;
17:13:54 954  
17:13:54 955  	BEGIN
17:13:54 956  	  SELECT
17:13:54 957  	    C.INSTRUMENT_TYPE_ID,
17:13:54 958  	    C.INSTRUMENT_ID
17:13:54 959  	    into
17:13:54 960  	    var_instrument_type_id,
17:13:54 961  	    var_instrument_id
17:13:54 962  	  FROM
17:13:54 963  	    CHARGE C,
17:13:54 964  	    TRANSACTION_ATTEMPT TA,
17:13:54 965  	    TRANSACTION T
17:13:54 966  	  WHERE
17:13:54 967  	    C.INVOICE_ID = in_invoice_id and
17:13:54 968  	    C.TRANSACTION_ID = T.ID and
17:13:54 969  	    TA.TRANSACTION_ID = T.ID and
17:13:54 970  	    TA.TRANSACTION_ATTEMPT_STATUS_ID = GLOBAL_STATUSES_V23.TRANS_ATTEMPT_SUCCESS and
17:13:54 971  	    T.IS_REFUND = GLOBAL_CONSTANTS_V23.FALSE and
17:13:54 972  	    T.TRANSACTION_AMOUNT >= 0 and
17:13:54 973  	    rownum < 2;
17:13:54 974  	EXCEPTION
17:13:54 975  	    WHEN NO_DATA_FOUND THEN
17:13:54 976  	      RAISE CAN_NOT_FIND_INSTRUMENT;
17:13:54 977  	END;
17:13:54 978  	BEGIN
17:13:54 979  	  PROCS_CHARGE_V23.CREATE_CHARGE(
17:13:54 980  	    in_invoice_id	  => in_invoice_id,
17:13:54 981  	    in_transaction_id	  => var_transaction_id,
17:13:54 982  	    in_instrument_type_id => var_instrument_type_id,
17:13:54 983  	    in_instrument_id	  => var_instrument_id,
17:13:54 984  	    in_charge_amount	  => -var_charge_amount_to_apply,
17:13:54 985  	    in_created_by	  => in_created_by,
17:13:54 986  	    in_charge_status_id   => GLOBAL_STATUSES_V23.CHARGE_OPENED,
17:13:54 987  	    out_charge_id	  => var_charge_id
17:13:54 988  	  );
17:13:54 989  	  EXCEPTION
17:13:54 990  	    WHEN OTHERS THEN
17:13:54 991  	      EXCEPTION_MESSAGE := SQLERRM;
17:13:54 992  	      RAISE CAN_NOT_CREATE_CHARGE;
17:13:54 993  	END;
17:13:54 994  
17:13:54 995  	out_charge_id := var_charge_id;
17:13:54 996  
17:13:54 997  EXCEPTION
17:13:54 998  WHEN BAD_INVOICE_ID THEN
17:13:54 999  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 1000  	   SPROC_NAME, 'No such invoice');
17:13:54 1001  WHEN CAN_NOT_CREATE_TRANSACTION THEN
17:13:54 1002  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 1003  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
17:13:54 1004  WHEN CAN_NOT_FIND_INSTRUMENT THEN
17:13:54 1005  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 1006  	   SPROC_NAME, 'Could not find financial instrument');
17:13:54 1007  WHEN CAN_NOT_CREATE_CHARGE THEN
17:13:54 1008  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 1009  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
17:13:54 1010  WHEN OTHERS THEN
17:13:54 1011  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 1012  	   SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 1013  END APPLY_REFUND;
