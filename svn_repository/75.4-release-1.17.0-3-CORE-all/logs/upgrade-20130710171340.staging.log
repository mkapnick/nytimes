17:13:54 1117  	   INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
17:13:54 1118  	   INNER JOIN OFFER ON LICENSE.OFFER_ID = OFFER.ID
17:13:54 1119  	 WHERE
17:13:54 1120  	   --LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V23.LICENSE_PROCESSED
17:13:54 1121  	   --AND
17:13:54 1122  	   SUBSCRIPTION.ID = in_subscription_id
17:13:54 1123  	   AND ROWNUM <= 1
17:13:54 1124  	 ORDER BY
