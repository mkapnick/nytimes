17:13:54 1211  PROCEDURE GET_GROUPS_ID_BY_INVOICE_ID (
17:13:54 1212  	 in_invoice_id IN NUMBER,
17:13:54 1213  	 out_group_ids OUT SYS_REFCURSOR
17:13:54 1214  ) AS
17:13:54 1215  SPROC_NAME CONSTANT VARCHAR2(27) := 'GET_GROUPS_ID_BY_INVOICE_ID';
17:13:54 1216  -- VARIABLES
17:13:54 1217  temp_invoice_id		 NUMBER;
17:13:54 1218  var_subscrib_group_id	 NUMBER;
17:13:54 1219  var_gc_purchaser_group_id NUMBER;
17:13:54 1220  var_gc_redeemer_group_id  NUMBER;
17:13:54 1221  -- EXCEPTIONS
17:13:54 1222  BAD_INVOICE_ID	    EXCEPTION;
17:13:54 1223  CAN_NOT_FIND_ACCOUNT EXCEPTION;
17:13:54 1224  BEGIN
17:13:54 1225  
17:13:54 1226  	 BEGIN
17:13:54 1227  	   SELECT
17:13:54 1228  	     INVOICE.ID into temp_invoice_id
17:13:54 1229  	   FROM
17:13:54 1230  	     INVOICE
17:13:54 1231  	   WHERE
17:13:54 1232  	     INVOICE.ID = in_invoice_id;
17:13:54 1233  	   EXCEPTION
17:13:54 1234  	     WHEN NO_DATA_FOUND THEN
17:13:54 1235  	       RAISE BAD_INVOICE_ID;
17:13:54 1236  	 END;
17:13:54 1237  
17:13:54 1238  	 BEGIN
17:13:54 1239  	   SELECT DISTINCT
17:13:54 1240  	     ACCOUNT.GROUP_ID into var_subscrib_group_id
17:13:54 1241  	   FROM
17:13:54 1242  	     ACCOUNT
17:13:54 1243  	     INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.ACCOUNT_ID = ACCOUNT.ID
17:13:54 1244  	     INNER JOIN LICENSE ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
17:13:54 1245  	   WHERE
17:13:54 1246  	     LICENSE.INVOICE_ID = in_invoice_id;
17:13:54 1247  	   EXCEPTION
17:13:54 1248  	     WHEN NO_DATA_FOUND THEN
17:13:54 1249  	       var_subscrib_group_id := NULL;
17:13:54 1250  	 END;
17:13:54 1251  
17:13:54 1252  	 IF var_subscrib_group_id IS NULL THEN
17:13:54 1253  	   BEGIN
17:13:54 1254  	     SELECT
17:13:54 1255  	       GIFT_CERTIFICATE.PURCHASER_GROUP_ID,
17:13:54 1256  	       GIFT_CERTIFICATE.REDEEMER_GROUP_ID
17:13:54 1257  	       into
17:13:54 1258  	       var_gc_purchaser_group_id,
17:13:54 1259  	       var_gc_redeemer_group_id
17:13:54 1260  	     FROM
17:13:54 1261  	       GIFT_CERTIFICATE
17:13:54 1262  	     WHERE
17:13:54 1263  	       GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = in_invoice_id
17:13:54 1264  	       OR GIFT_CERTIFICATE.FINALIZED_INVOICE_ID = in_invoice_id;
17:13:54 1265  	     EXCEPTION
17:13:54 1266  	       WHEN NO_DATA_FOUND THEN
17:13:54 1267  		 var_gc_purchaser_group_id := NULL;
17:13:54 1268  		 var_gc_redeemer_group_id  := NULL;
17:13:54 1269  	   END;
17:13:54 1270  	 END IF;
17:13:54 1271  
17:13:54 1272  	 IF var_subscrib_group_id IS NULL
17:13:54 1273  	   AND var_gc_purchaser_group_id IS NULL
17:13:54 1274  	   AND var_gc_redeemer_group_id IS NULL THEN
17:13:54 1275  	     RAISE CAN_NOT_FIND_ACCOUNT;
17:13:54 1276  	 END IF;
17:13:54 1277  
17:13:54 1278  	 OPEN out_group_ids FOR
17:13:54 1279  	 SELECT GROUP_ID FROM (
17:13:54 1280  	   SELECT
17:13:54 1281  	     var_subscrib_group_id as "GROUP_ID"
17:13:54 1282  	   FROM
17:13:54 1283  	     DUAL
17:13:54 1284  	   UNION
17:13:54 1285  	   SELECT
17:13:54 1286  	     var_gc_purchaser_group_id as "GROUP_ID"
17:13:54 1287  	   FROM
17:13:54 1288  	     DUAL
17:13:54 1289  	   UNION
17:13:54 1290  	   SELECT
17:13:54 1291  	     var_gc_redeemer_group_id as "GROUP_ID"
17:13:54 1292  	   FROM
17:13:54 1293  	     DUAL
17:13:54 1294  	 )
17:13:54 1295  	 WHERE
17:13:54 1296  	   GROUP_ID IS NOT NULL;
17:13:54 1297  
17:13:54 1298  EXCEPTION
17:13:54 1299  WHEN BAD_INVOICE_ID THEN
17:13:54 1300  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 1301  	   SPROC_NAME, 'No such invoice');
17:13:54 1302  WHEN CAN_NOT_FIND_ACCOUNT THEN
17:13:54 1303  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 1304  	   SPROC_NAME, 'Could not find account for given invoice');
17:13:54 1305  WHEN OTHERS THEN
17:13:54 1306  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 1307  	   SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 1308  END GET_GROUPS_ID_BY_INVOICE_ID;
