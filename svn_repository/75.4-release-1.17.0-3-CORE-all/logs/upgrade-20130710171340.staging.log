17:13:54 659  
17:13:54 660  PROCEDURE GET_SUBSCRIPTION_INFO (
17:13:54 661  	  in_subscription_id  IN  NUMBER,
17:13:54 662  	  out_result_set      OUT SYS_REFCURSOR
17:13:54 663  ) AS
17:13:54 664  SPROC_NAME CONSTANT VARCHAR2(21) := 'GET_SUBSCRIPTION_INFO';
17:13:54 665  -- VARIABLES
17:13:54 666  temp_subscription_id NUMBER;
17:13:54 667  -- EXCEPTIONS
17:13:54 668  BAD_SUBSCRIPTION_ID EXCEPTION;
17:13:54 669  BEGIN
17:13:54 670  	-- Find subscription by id
17:13:54 671  	-- Return its details
17:13:54 672  
17:13:54 673  	BEGIN
17:13:54 674  	  SELECT
17:13:54 675  	    SUBSCRIPTION.ID into temp_subscription_id
17:13:54 676  	  FROM
17:13:54 677  	    SUBSCRIPTION
17:13:54 678  	  WHERE
17:13:54 679  	    SUBSCRIPTION.ID = in_subscription_id;
17:13:54 680  	  EXCEPTION
17:13:54 681  	    WHEN NO_DATA_FOUND THEN
17:13:54 682  	      RAISE BAD_SUBSCRIPTION_ID;
17:13:54 683  	END;
17:13:54 684  
17:13:54 685  	OPEN out_result_set FOR
17:13:54 686  	SELECT
17:13:54 687  	  SUBSCRIPTION.ACCOUNT_ID,
17:13:54 688  	  SUBSCRIPTION.ID AS "SUBSCRIPTION_ID",
17:13:54 689  	  SUBSCRIPTION.SUBSCRIPTION_STATUS_ID,
17:13:54 690  	  SUBSCRIPTION.PURCHASE_DATE,
17:13:54 691  	  SUBSCRIPTION.SUSPEND_DATE,
17:13:54 692  	  SUBSCRIPTION.REACTIVATION_DATE,
17:13:54 693  	  SUBSCRIPTION.CANCELLATION_DATE,
17:13:54 694  	  SUBSCRIPTION_CANCEL_REASON.VALUE as "CANCEL_TYPE",
17:13:54 695  	  OFFER_CHAIN.ID AS "OFFER_CHAIN_ID",
17:13:54 696  	  OFFER_CHAIN.NAME,
17:13:54 697  	  OFFER_CHAIN.DESCRIPTION,
17:13:54 698  	  OFFER_CHAIN.PRODUCT_URI,
17:13:54 699  	  SUBSCRIPTION.INSTRUMENT_ID,
17:13:54 700  	  SUBSCRIPTION.INSTRUMENT_TYPE_ID,
17:13:54 701  	  PROCS_SUBSCRIPTION_V23.CALC_SUBSCRIPTION_END_DATE(SUBSCRIPTION.ID) as "END_DATE",
17:13:54 702  	  PROCS_SUBSCRIPTION_V23.GET_RECENT_CHARGE(SUBSCRIPTION.ID) AS "RECENT_CHARGE",
17:13:54 703  	  PROCS_SUBSCRIPTION_V23.GET_RENEWAL_DATE(SUBSCRIPTION.ID) AS "RENEWAL_DATE",
17:13:54 704  	  PROCS_SUBSCRIPTION_V23.GET_BILLING_CYCLE(SUBSCRIPTION.ID) AS "BILLING_CYCLE",
17:13:54 705  	  (
17:13:54 706  	    SELECT
17:13:54 707  	      ACCOUNT.GROUP_ID
17:13:54 708  	      FROM ACCOUNT
17:13:54 709  	      WHERE ACCOUNT.ID = SUBSCRIPTION.ACCOUNT_ID
17:13:54 710  	  ) as "GROUP_ID",
17:13:54 711  	  (
17:13:54 712  	    SELECT
17:13:54 713  	      MAX(ENTITLEMENT_END_DATE)
17:13:54 714  	      FROM LICENSE
17:13:54 715  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
17:13:54 716  	  )
17:13:54 717  	  as "ENT_END_DATE",
17:13:54 718  	  (
17:13:54 719  	    SELECT
17:13:54 720  	      MIN(START_DATE)
17:13:54 721  	      FROM LICENSE
17:13:54 722  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
17:13:54 723  	  )
17:13:54 724  	  as "ENT_START_DATE",
17:13:54 725  	  PROCS_SUBSCRIPTION_V23.IS_SUBSCRIPTION_CANCELABLE(SUBSCRIPTION.ID) AS "IS_CANCELABLE",
17:13:54 726  	  ITUNES_RECEIPT.ID AS "ITUNES_RECEIPT_ID",
17:13:54 727  	  GROUP_ACCOUNT.ID GA_ID,
17:13:54 728  	  GROUP_ACCOUNT.SUBSCRIPTION_ID GA_SUBSCRIPTION_ID,
17:13:54 729  	  GROUP_ACCOUNT.GROUP_NAME GA_GROUP_NAME,
17:13:54 730  	  GROUP_ACCOUNT.FIRST_NAME GA_FIRST_NAME,
17:13:54 731  	  GROUP_ACCOUNT.LAST_NAME GA_LAST_NAME,
17:13:54 732  	  GROUP_ACCOUNT.EMAIL GA_EMAIL,
17:13:54 733  	  GROUP_ACCOUNT.PHONE GA_PHONE,
17:13:54 734  	  GROUP_ACCOUNT.ORGANIZATION_TYPE GA_ORGANIZATION_TYPE,
17:13:54 735  	  GROUP_ACCOUNT.SEATS GA_SEATS,
17:13:54 736  	  PROCS_GROUP_ACCOUNT_V23.F_GET_NUM_OCCUPIED_GROUP_SEATS(GROUP_ACCOUNT.ID) GA_SEATS_USED,
17:13:54 737  	  GROUP_ACCOUNT.IP GA_IP,
17:13:54 738  	  PROCS_SUBSCRIPTION_V23.GET_GIFT_CERT_CODE_BY_SUB_ID(SUBSCRIPTION.ID) gift_certificate_code,
17:13:54 739  	  PROCS_ACCOUNT_V23.GET_GRACE_START_DATE(SUBSCRIPTION.ID) GRACE_START_DATE,
17:13:54 740  	  PROCS_ACCOUNT_V23.GET_GRACE_END_DATE(SUBSCRIPTION.ID) GRACE_END_DATE
17:13:54 741  	FROM
17:13:54 742  	  SUBSCRIPTION
17:13:54 743  	  INNER JOIN OFFER_CHAIN ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
17:13:54 744  	  LEFT JOIN SUBSCRIPTION_CANCEL_REASON ON SUBSCRIPTION.SCT_ID = SUBSCRIPTION_CANCEL_REASON.ID
17:13:54 745  	  LEFT JOIN ITUNES_RECEIPT ON SUBSCRIPTION.ID = ITUNES_RECEIPT.SUBSCRIPTION_ID
17:13:54 746  	  LEFT JOIN GROUP_ACCOUNT ON SUBSCRIPTION.ID = GROUP_ACCOUNT.SUBSCRIPTION_ID
17:13:54 747  	WHERE
17:13:54 748  	  SUBSCRIPTION.ID = in_subscription_id;
17:13:54 749  
17:13:54 750  EXCEPTION
17:13:54 751  WHEN BAD_SUBSCRIPTION_ID THEN
17:13:54 752  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 753  	  SPROC_NAME, 'No such subscription');
17:13:54 754  WHEN OTHERS THEN
17:13:54 755  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 756  	  SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 757  END GET_SUBSCRIPTION_INFO;
17:13:54 758  
17:13:54 759  /******************************************************************************/
17:13:54 760  
17:13:54 761  PROCEDURE GET_SUBSCRIPTION_INVOICES (
17:13:54 762  	in_subscription_id  IN	NUMBER,
17:13:54 763  	out_result_set	    OUT SYS_REFCURSOR
17:13:54 764  ) AS
17:13:54 765  SPROC_NAME	   CONSTANT VARCHAR2(25) := 'GET_SUBSCRIPTION_INVOICES';
17:13:54 766  temp_subscription_id NUMBER;
17:13:54 767  BEGIN
17:13:54 768  	-- Note: A subscription has one or more associated licenses, each of which has an associated invoice.
17:13:54 769  	-- Find associated LICENSES for the subscription by "LICENSE.subscription_id"
17:13:54 770  	--   for each license
17:13:54 771  	--     get associated invoice
17:13:54 772  	--     add to results list
17:13:54 773  	--   end loop
17:13:54 774  	-- end
17:13:54 775  
17:13:54 776  	SELECT
17:13:54 777  	  SUBSCRIPTION.ID into temp_subscription_id
17:13:54 778  	FROM
17:13:54 779  	  SUBSCRIPTION
17:13:54 780  	WHERE
17:13:54 781  	  SUBSCRIPTION.ID = in_subscription_id;
17:13:54 782  
17:13:54 783  	OPEN out_result_set FOR
17:13:54 784  	SELECT
17:13:54 785  	  INVOICE.ID,
17:13:54 786  	  INVOICE.INVOICE_STATUS_ID,
17:13:54 787  	  INVOICE.CREATE_DATE,
17:13:54 788  	  INVOICE.CREATED_BY,
17:13:54 789  	  INVOICE.UPDATE_DATE,
17:13:54 790  	  INVOICE.UPDATED_BY,
17:13:54 791  	  INVOICE.TAX_EXEMPT_ID
17:13:54 792  	FROM
17:13:54 793  	  LICENSE
17:13:54 794  	    INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
17:13:54 795  	WHERE
17:13:54 796  	  LICENSE.SUBSCRIPTION_ID = in_subscription_id;
17:13:54 797  
17:13:54 798  EXCEPTION
17:13:54 799  WHEN NO_DATA_FOUND THEN
17:13:54 800  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 801  	  SPROC_NAME, 'Could not find subscription with given ID');
17:13:54 802  WHEN OTHERS THEN
17:13:54 803  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 804  	  SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 805  END GET_SUBSCRIPTION_INVOICES;
17:13:54 806  
