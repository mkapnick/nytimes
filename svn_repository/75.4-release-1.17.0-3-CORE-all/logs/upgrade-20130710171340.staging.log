17:13:54 620  	  SUBSCRIPTION.INSTRUMENT_ID,
17:13:54 621  	  SUBSCRIPTION.INSTRUMENT_TYPE_ID,
17:13:54 622  	  OFFER_CHAIN.ID AS "OFFER_CHAIN_ID",
17:13:54 623  	  OFFER_CHAIN.NAME,
17:13:54 624  	  OFFER_CHAIN.DESCRIPTION,
17:13:54 625  	  OFFER_CHAIN.PRODUCT_URI,
17:13:54 626  	  PROCS_SUBSCRIPTION_V23.CALC_SUBSCRIPTION_END_DATE(SUBSCRIPTION.ID) as "END_DATE",
17:13:54 627  	  PROCS_SUBSCRIPTION_V23.GET_RECENT_CHARGE(SUBSCRIPTION.ID) AS "RECENT_CHARGE",
17:13:54 628  	  PROCS_SUBSCRIPTION_V23.GET_RENEWAL_DATE(SUBSCRIPTION.ID) AS "RENEWAL_DATE",
17:13:54 629  	  PROCS_SUBSCRIPTION_V23.GET_BILLING_CYCLE(SUBSCRIPTION.ID) AS "BILLING_CYCLE",
17:13:54 630  	  PROCS_SUBSCRIPTION_V23.IS_SUBSCRIPTION_CANCELABLE(SUBSCRIPTION.ID) AS "IS_CANCELABLE",
17:13:54 631  	  ITUNES_RECEIPT.ID AS "ITUNES_RECEIPT_ID",
17:13:54 632  	  (
17:13:54 633  	    SELECT
17:13:54 634  	      MAX(ENTITLEMENT_END_DATE)
17:13:54 635  	      FROM LICENSE
17:13:54 636  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
17:13:54 637  	  )
17:13:54 638  	  as "ENT_END_DATE",
17:13:54 639  	  (
17:13:54 640  	    SELECT
17:13:54 641  	      MIN(START_DATE)
17:13:54 642  	      FROM LICENSE
17:13:54 643  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
17:13:54 644  	  )
17:13:54 645  	  as "ENT_START_DATE",
17:13:54 646  	  GROUP_ACCOUNT.SUBSCRIPTION_ID GA_SUBSCRIPTION_ID,
17:13:54 647  	  GROUP_ACCOUNT.ID GA_ID,
17:13:54 648  	  GROUP_ACCOUNT.GROUP_NAME GA_GROUP_NAME,
17:13:54 649  	  GROUP_ACCOUNT.FIRST_NAME GA_FIRST_NAME,
17:13:54 650  	  GROUP_ACCOUNT.LAST_NAME GA_LAST_NAME,
17:13:54 651  	  GROUP_ACCOUNT.EMAIL GA_EMAIL,
17:13:54 652  	  GROUP_ACCOUNT.PHONE GA_PHONE,
17:13:54 653  	  GROUP_ACCOUNT.ORGANIZATION_TYPE GA_ORGANIZATION_TYPE,
17:13:54 654  	  GROUP_ACCOUNT.SEATS GA_SEATS,
17:13:54 655  	  PROCS_GROUP_ACCOUNT_V23.F_GET_NUM_OCCUPIED_GROUP_SEATS(GROUP_ACCOUNT.ID) GA_SEATS_USED,
17:13:54 656  	  GROUP_ACCOUNT.IP GA_IP,
17:13:54 657  	  PROCS_SUBSCRIPTION_V23.GET_GIFT_CERT_CODE_BY_SUB_ID(SUBSCRIPTION.ID) GIFT_CERTIFICATE_CODE,
17:13:54 658  	  PROCS_ACCOUNT_V23.GET_GRACE_START_DATE(SUBSCRIPTION.ID) GRACE_START_DATE,
17:13:54 659  	  PROCS_ACCOUNT_V23.GET_GRACE_END_DATE(SUBSCRIPTION.ID) GRACE_END_DATE
17:13:54 660  	FROM
17:13:54 661  	  SUBSCRIPTION
17:13:54 662  	  INNER JOIN OFFER_CHAIN ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
17:13:54 663  	  LEFT JOIN SUBSCRIPTION_CANCEL_REASON ON SUBSCRIPTION.SCT_ID = SUBSCRIPTION_CANCEL_REASON.ID
17:13:54 664  	  LEFT JOIN ITUNES_RECEIPT ON SUBSCRIPTION.ID = ITUNES_RECEIPT.SUBSCRIPTION_ID
17:13:54 665  	  LEFT JOIN GROUP_ACCOUNT ON SUBSCRIPTION.ID = GROUP_ACCOUNT.SUBSCRIPTION_ID
17:13:54 666  	WHERE
17:13:54 667  	  SUBSCRIPTION.ACCOUNT_ID = var_account_id
17:13:54 668  	  AND (SUBSCRIPTION.SCT_ID IS NULL OR SUBSCRIPTION.SCT_ID != GLOBAL_STATUSES_V23.REAL_TIME_CANCEL_REASON)
17:13:54 669  	  AND SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = NVL(in_status, SUBSCRIPTION.SUBSCRIPTION_STATUS_ID)
17:13:54 670  	  AND PROCS_COMMON_V23.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE) >= NVL(in_start_date, PROCS_COMMON_V23.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE))
17:13:54 671  	  AND PROCS_COMMON_V23.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE) <= NVL(in_end_date, PROCS_COMMON_V23.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE))
17:13:54 672  	  AND (in_group_account_type IS NULL OR OFFER_CHAIN.GROUP_ACCOUNT_TYPE_ID = in_group_account_type);
17:13:54 673  
17:13:54 674  EXCEPTION
17:13:54 675  WHEN BAD_GROUP_ID THEN
17:13:54 676  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 677  	  SPROC_NAME, 'Can not find account with given group id');
17:13:54 678  WHEN OTHERS THEN
17:13:54 679  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 680  	  SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 681  END GET_ACCOUNT_SUBSCRIPTIONS;
