17:13:54  58  	    OFFER.ID = in_offer_id;
17:13:54  59  
17:13:54  60  	  EXCEPTION
17:13:54  61  	    WHEN NO_DATA_FOUND THEN
17:13:54  62  	      RAISE BAD_OFFER_ID;
17:13:54  63  	END;
17:13:54  64  
17:13:54  65  	-- Get product_offering data
17:13:54  66  	OPEN var_line_item_data FOR
17:13:54  67  	SELECT
17:13:54  68  	  PRODUCT_OFFERING.ID,
17:13:54  69  	  PRODUCT_OFFERING.UNIT_PRICE,
17:13:54  70  	  PRODUCT_OFFERING.QUANTITY
17:13:54  71  	FROM
17:13:54  72  	  OFFER_PRODUCT_OFFERING
17:13:54  73  	  INNER JOIN PRODUCT_OFFERING ON OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = PRODUCT_OFFERING.ID
17:13:54  74  	WHERE
17:13:54  75  	  OFFER_PRODUCT_OFFERING.OFFER_ID = in_offer_id;
17:13:54  76  
17:13:54  77  	-- insert line items
17:13:54  78  	-- add discounts to line items
17:13:54  79  	LOOP
17:13:54  80  	  FETCH var_line_item_data INTO
17:13:54  81  	    var_product_offering_id,
17:13:54  82  	    var_product_unit_price,
17:13:54  83  	    var_product_quantity;
17:13:54  84  	  EXIT WHEN var_line_item_data%NOTFOUND;
17:13:54  85  
17:13:54  86  	  var_product_offering_oprice := var_product_unit_price * var_product_quantity;
17:13:54  87  	  var_product_offering_price := var_product_offering_oprice;
17:13:54  88  
17:13:54  89  	  -- Apply discounts to line_item
17:13:54  90  	  BEGIN
17:13:54  91  	    SELECT
17:13:54  92  	      SUM (DISCOUNT.FIXED_AMOUNT) into var_discount_fixed_amount
17:13:54  93  	    FROM
17:13:54  94  	      DISCOUNT_PRODUCT_OFFERING
17:13:54  95  	      INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
17:13:54  96  	    WHERE
17:13:54  97  	      DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
17:13:54  98  	      AND DISCOUNT.FIXED_AMOUNT IS NOT NULL;
17:13:54  99  	    EXCEPTION
17:13:54 100  	    WHEN NO_DATA_FOUND THEN
17:13:54 101  	      var_discount_fixed_amount := NULL;
17:13:54 102  	  END;
17:13:54 103  
17:13:54 104  	  BEGIN
17:13:54 105  	    SELECT
17:13:54 106  	      SUM (DISCOUNT.PERCENT_AMOUNT) into var_discount_percent_amount
17:13:54 107  	    FROM
17:13:54 108  	      DISCOUNT_PRODUCT_OFFERING
17:13:54 109  	      INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
17:13:54 110  	    WHERE
17:13:54 111  	      DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
17:13:54 112  	      AND DISCOUNT.PERCENT_AMOUNT IS NOT NULL;
17:13:54 113  	    EXCEPTION
17:13:54 114  	    WHEN NO_DATA_FOUND THEN
17:13:54 115  	      var_discount_percent_amount := NULL;
17:13:54 116  	  END;
17:13:54 117  
17:13:54 118  	  IF (var_discount_percent_amount IS NOT NULL) THEN
17:13:54 119  	    var_product_offering_price := var_product_offering_price * ( 1 - var_discount_percent_amount / 100 );
17:13:54 120  	  END IF;
17:13:54 121  
17:13:54 122  	  IF (var_discount_fixed_amount IS NOT NULL) THEN
17:13:54 123  	    var_product_offering_price := var_product_offering_price - var_discount_fixed_amount;
17:13:54 124  	  END IF;
17:13:54 125  
17:13:54 126  	  var_line_item_price := PROCS_COMMON_V23.ROUND_10_6_TO_10_2(var_product_offering_price);
17:13:54 127  
17:13:54 128  	  IF (var_line_item_price < 0) THEN
17:13:54 129  	      RAISE BAD_DISCOUNT;
17:13:54 130  	  END IF;
17:13:54 131  
17:13:54 132  	  var_new_line_item_id := NULL;
17:13:54 133  	  PROCS_LINE_ITEMS_CRU_V23.CREATE_LINE_ITEM(
17:13:54 134  	    inout_line_item_id	=> var_new_line_item_id,
17:13:54 135  	    in_product_offer_id => var_product_offering_id,
17:13:54 136  	    in_invoice_id	=> in_invoice_id,
17:13:54 137  	    in_amount		=> var_line_item_price,
17:13:54 138  	    in_created_by	=> in_created_by,
17:13:54 139  	    in_discount_amount	=> var_product_offering_oprice - var_line_item_price,
17:13:54 140  	    in_taxes_amount	=> NULL
17:13:54 141  	  );
17:13:54 142  
17:13:54 143  	  FOR f_discount IN (
17:13:54 144  	    SELECT
17:13:54 145  	      DISCOUNT.ID
17:13:54 146  	    FROM
17:13:54 147  	      DISCOUNT_PRODUCT_OFFERING
17:13:54 148  	      INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
17:13:54 149  	    WHERE
17:13:54 150  	      DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
17:13:54 151  	  )
17:13:54 152  	  LOOP
17:13:54 153  	    PROCS_LINE_ITEMS_CRU_V23.CREATE_DISCOUNT_LINE_ITEM(
17:13:54 154  	      in_discount_id =>  f_discount.ID,
17:13:54 155  	      in_line_item_id => var_new_line_item_id
17:13:54 156  	    );
17:13:54 157  	  END LOOP;
17:13:54 158  	END LOOP;
17:13:54 159  
17:13:54 160  EXCEPTION
17:13:54 161  WHEN BAD_INVOICE_ID THEN
17:13:54 162  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 163  	  SPROC_NAME, 'No such license');
17:13:54 164  WHEN BAD_OFFER_ID THEN
17:13:54 165  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 166  	  SPROC_NAME, 'No such offer');
17:13:54 167  WHEN BAD_DISCOUNT THEN
