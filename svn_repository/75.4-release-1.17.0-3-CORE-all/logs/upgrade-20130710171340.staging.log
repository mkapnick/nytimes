17:13:54 1133  WHEN OTHERS THEN
17:13:54 1134  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 1135  	   SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 1136  END GET_BILLING_CYCLE;
