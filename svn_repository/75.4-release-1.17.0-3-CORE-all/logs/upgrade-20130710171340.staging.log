17:13:54 912  					     SPROC_NAME, 'No such group id');
17:13:54 913  	    WHEN BAD_OFFER_CHAIN_ID THEN
17:13:54 914  	    PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 915  					     SPROC_NAME, 'No such offer chain');
17:13:54 916  	    WHEN OFFER_REC_NUM_LESS_THAN_ONE THEN
17:13:54 917  	    PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 918  					     SPROC_NAME, 'Some offer has recurrences number less than 1');
17:13:54 919  	    WHEN REF_CODE_ALREADY_EXISTS THEN
17:13:54 920  	    PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
