17:13:53  15  	BEGIN
17:13:53  16  	  OPEN out_result_set FOR
17:13:53  17  
17:13:53  18  	SELECT
17:13:53  19  	  P.ENTITLEMENT,
17:13:53  20  	  P.NAME,
17:13:53  21  	  0 INHERITED,
17:13:53  22  	  P.IS_SHAREABLE,
17:13:53  23  	  MAX(LIC.ENTITLEMENT_END_DATE) EXPIRES
17:13:53  24  	FROM
17:13:53  25  	  SUBSCRIPTION SB
17:13:53  26  	  INNER JOIN ACCOUNT AC ON AC.ID = SB.ACCOUNT_ID
17:13:53  27  	  INNER JOIN LICENSE LIC ON LIC.SUBSCRIPTION_ID = SB.ID
17:13:53  28  	  INNER JOIN OFFER_PRODUCT_OFFERING OPO ON OPO.OFFER_ID = LIC.OFFER_ID
17:13:53  29  	  INNER JOIN PRODUCT_OFFERING PO ON PO.ID = OPO.PRODUCT_OFFERING_ID
17:13:53  30  	  INNER JOIN PRODUCT P ON P.ID = PO.PRODUCT_ID
17:13:53  31  	WHERE
17:13:53  32  	  LIC.ENTITLEMENT_END_DATE >= TRUNC(SYSDATE)
17:13:53  33  	  AND LIC.START_DATE <= SYSDATE
17:13:53  34  	  AND AC.GROUP_ID = in_group_id
17:13:53  35  	GROUP BY
17:13:53  36  	  P.ENTITLEMENT, 0, P.IS_SHAREABLE, P.NAME
17:13:53  37  UNION ALL
17:13:53  38  	SELECT
17:13:53  39  	  P.ENTITLEMENT,
17:13:53  40  	  P.NAME,
17:13:53  41  	  1 INHERITED,
17:13:53  42  	  P.IS_SHAREABLE,
17:13:53  43  	  MAX(LEAST(SS.END_DATE, LIC.ENTITLEMENT_END_DATE)) EXPIRES
17:13:53  44  	FROM
17:13:53  45  	  ACCOUNT BORROWER,
17:13:53  46  	  SUBSCRIPTION S,
17:13:53  47  	  LICENSE LIC,
17:13:53  48  	  OFFER_PRODUCT_OFFERING OPO,
17:13:53  49  	  PRODUCT_OFFERING PO,
17:13:53  50  	  PRODUCT P,
17:13:53  51  	  GROUP_ACCOUNT GA,
17:13:53  52  	  SUBSCRIPTION_SHARE SS
17:13:53  53  	WHERE
17:13:53  54  	  BORROWER.GROUP_ID = in_group_id
17:13:53  55  	  AND LIC.SUBSCRIPTION_ID = S.ID
17:13:53  56  	  AND OPO.OFFER_ID = LIC.OFFER_ID
17:13:53  57  	  AND PO.ID = OPO.PRODUCT_OFFERING_ID
17:13:53  58  	  AND PO.PRODUCT_ID = P.ID
17:13:53  59  	  AND GA.SUBSCRIPTION_ID = S.ID
17:13:53  60  	  AND SS.BORROWER_ACCOUNT_ID = BORROWER.ID
17:13:53  61  	  AND SS.GROUP_ACCOUNT_ID = GA.ID
17:13:53  62  	  AND SYSDATE BETWEEN SS.START_DATE AND SS.END_DATE
17:13:53  63  	  AND SYSDATE BETWEEN LIC.START_DATE AND LIC.ENTITLEMENT_END_DATE
17:13:53  64  	  AND P.IS_SHAREABLE = 1
17:13:53  65  	GROUP BY
17:13:53  66  	  P.ENTITLEMENT, 0, P.IS_SHAREABLE, P.NAME;
17:13:53  67  
17:13:53  68  	EXCEPTION
17:13:53  69  	  WHEN OTHERS THEN
17:13:53  70  	    PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:53  71  	      SPROC_NAME, 'Unknown error', SQLERRM);
17:13:53  72  	END GET_ALL_ENTITLEMENTS;
