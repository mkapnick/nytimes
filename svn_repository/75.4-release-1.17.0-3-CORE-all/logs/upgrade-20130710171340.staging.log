17:13:54 209  PROCEDURE GET_ENDING_LICENSES (
17:13:54 210  	in_hours_number IN NUMBER,
17:13:54 211  	out_result_set	OUT SYS_REFCURSOR
17:13:54 212  ) AS
17:13:54 213  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_ENDING_LICENSES';
17:13:54 214  -- VARIABLES
17:13:54 215  var_days		NUMBER;
17:13:54 216  var_hours 	NUMBER;
17:13:54 217  var_time_interval INTERVAL DAY (3) TO SECOND;
17:13:54 218  BEGIN
17:13:54 219  
17:13:54 220  	var_hours := mod(in_hours_number,24);
17:13:54 221  	var_days := (in_hours_number - var_hours) / 24;
17:13:54 222  	var_time_interval := var_days||' '||var_hours||':0:0';
17:13:54 223  
17:13:54 224  	OPEN out_result_set FOR
17:13:54 225  SELECT * FROM
17:13:54 226  (
