17:13:54 1016  -- norlov: #38796
17:13:54 1017  PROCEDURE GET_TRANSACTIONS (
17:13:54 1018  /*
17:13:54 1019  Throws exceptions:
17:13:54 1020  APP_EXCEPTION_CODES_V23.NOT_FOUND
17:13:54 1021  APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR
17:13:54 1022  */
17:13:54 1023  	 in_group_id	       IN  NUMBER,
17:13:54 1024  	 in_invoice_id	       IN NUMBER DEFAULT NULL,
17:13:54 1025  	 in_subscription_id    IN NUMBER DEFAULT NULL,
17:13:54 1026  	 in_start_date	       IN TRANSACTION.CREATE_DATE%TYPE DEFAULT NULL,
17:13:54 1027  	 in_end_date	       IN TRANSACTION.CREATE_DATE%TYPE DEFAULT NULL,
17:13:54 1028  	 in_transaction_status IN NUMBER DEFAULT NULL,
17:13:54 1029  	 out_result_set        OUT SYS_REFCURSOR
17:13:54 1030  ) AS
17:13:54 1031  SPROC_NAME CONSTANT VARCHAR2(16) := 'GET_TRANSACTIONS';
17:13:54 1032  -- VARIABLES
17:13:54 1033  var_account_id ACCOUNT.ID%TYPE;
17:13:54 1034  statement VARCHAR2(2000);
17:13:54 1035  -- EXCEPTIONS
17:13:54 1036  BAD_GROUP_ID   EXCEPTION;
17:13:54 1037  BEGIN
17:13:54 1038  	-- check group id
17:13:54 1039  	BEGIN
17:13:54 1040  	   SELECT
17:13:54 1041  	     ACCOUNT.ID into var_account_id
17:13:54 1042  	   FROM
17:13:54 1043  	     ACCOUNT
17:13:54 1044  	   WHERE
17:13:54 1045  	     ACCOUNT.GROUP_ID = in_group_id;
17:13:54 1046  	   EXCEPTION
17:13:54 1047  	     WHEN NO_DATA_FOUND THEN
17:13:54 1048  	       RAISE BAD_GROUP_ID;
17:13:54 1049  	 END;
17:13:54 1050  
17:13:54 1051  	 OPEN out_result_set FOR
17:13:54 1052  	 SELECT
17:13:54 1053  	   TRANSACTION.ID,
17:13:54 1054  	   TRANSACTION.TRANSACTION_STATUS_ID,
17:13:54 1055  	   TRANSACTION.TRANSACTION_AMOUNT,
17:13:54 1056  	   TRANSACTION.CREATE_DATE,
17:13:54 1057  	   TRANSACTION.CREATED_BY,
17:13:54 1058  	   TRANSACTION.IS_REFUND,
17:13:54 1059  	   GET_TRANSACTION_TAX_AMOUNT(TRANSACTION.ID) as TRANSACTION_TAX_AMOUNT,
17:13:54 1060  	   GET_TRANSACTION_INTRL_TAXES(TRANSACTION.ID) as INTERNATIONAL_TOTAL
17:13:54 1061  	 FROM
17:13:54 1062  	   TRANSACTION
17:13:54 1063  	   INNER JOIN CHARGE ON TRANSACTION.ID = CHARGE.TRANSACTION_ID
17:13:54 1064  	   INNER JOIN INVOICE ON INVOICE.ID = CHARGE.INVOICE_ID
17:13:54 1065  	 WHERE
17:13:54 1066  	   -- Filter by invoice ID
17:13:54 1067  	   (
17:13:54 1068  	     INVOICE.ID IN (
17:13:54 1069  	       -- Gift certificate invoices
17:13:54 1070  	       SELECT
17:13:54 1071  		 GIFT_CERTIFICATE.PURCHASE_INVOICE_ID
17:13:54 1072  	       FROM
17:13:54 1073  		 GIFT_CERTIFICATE
17:13:54 1074  	       WHERE
17:13:54 1075  		 GIFT_CERTIFICATE.PURCHASER_GROUP_ID = in_group_id
17:13:54 1076  		 -- If subscription_id is set then return nothing
17:13:54 1077  		 AND EXISTS (SELECT 1 FROM DUAL WHERE in_subscription_id IS NULL)
17:13:54 1078  	     )
17:13:54 1079  	     OR
17:13:54 1080  	     INVOICE.ID IN (
17:13:54 1081  	       SELECT
17:13:54 1082  		 LICENSE.INVOICE_ID
17:13:54 1083  	       FROM
17:13:54 1084  		 LICENSE
17:13:54 1085  		 INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
17:13:54 1086  	       WHERE
17:13:54 1087  		 SUBSCRIPTION.ACCOUNT_ID = var_account_id
17:13:54 1088  		 -- Filter by subscription id
17:13:54 1089  		 AND SUBSCRIPTION.ID = NVL(in_subscription_id, SUBSCRIPTION.ID)
17:13:54 1090  	     )
17:13:54 1091  	   )
17:13:54 1092  	   -- Filter by invoice id
17:13:54 1093  	   AND INVOICE.ID = NVL(in_invoice_id, INVOICE.ID)
17:13:54 1094  	   -- Filter by start date
17:13:54 1095  	   AND TRANSACTION.CREATE_DATE >= NVL(in_start_date, TRANSACTION.CREATE_DATE)
17:13:54 1096  	   -- Filter by end date
17:13:54 1097  	   AND TRANSACTION.CREATE_DATE <= NVL(in_end_date, TRANSACTION.CREATE_DATE)
17:13:54 1098  	   -- Filter by transaction status
17:13:54 1099  	   AND TRANSACTION.TRANSACTION_STATUS_ID IN ( SELECT NVL(in_transaction_status, TRANSACTION.TRANSACTION_STATUS_ID) FROM DUAL);
17:13:54 1100  
17:13:54 1101  /*
17:13:54 1102  
17:13:54 1103  statement :=  'select distinct '||CHR(10)
17:13:54 1104  || ' TRANSACTION.ID,'||CHR(10)
17:13:54 1105  || ' TRANSACTION.TRANSACTION_STATUS_ID, '||CHR(10)
17:13:54 1106  || ' TRANSACTION.TRANSACTION_AMOUNT, '||CHR(10)
17:13:54 1107  || ' TRANSACTION.CREATE_DATE, '||CHR(10)
17:13:54 1108  || ' TRANSACTION.CREATED_BY, '||CHR(10)
17:13:54 1109  || ' TRANSACTION.IS_REFUND from TRANSACTION '||CHR(10)
17:13:54 1110  || ' inner join CHARGE on (CHARGE.TRANSACTION_ID = TRANSACTION.ID)'||CHR(10)
17:13:54 1111  || ' inner join INVOICE on (INVOICE.ID = CHARGE.INVOICE_ID)'||CHR(10)
17:13:54 1112  || ' inner join LICENSE on (LICENSE.INVOICE_ID = INVOICE.ID)'||CHR(10)
17:13:54 1113  || ' inner join SUBSCRIPTION on (SUBSCRIPTION.ID = LICENSE.SUBSCRIPTION_ID)'||CHR(10)
17:13:54 1114  || ' inner join ACCOUNT on (ACCOUNT.ID = SUBSCRIPTION.ACCOUNT_ID)'||CHR(10)
17:13:54 1115  || ' where ACCOUNT.GROUP_ID = '||in_group_id;
17:13:54 1116  
17:13:54 1117  IF (in_transaction_status IS NOT NULL) THEN
17:13:54 1118  	 statement := statement || CHR(10) || 'AND TRANSACTION.TRANSACTION_STATUS_ID=' || in_transaction_status;
17:13:54 1119  END IF;
17:13:54 1120  
17:13:54 1121  IF (in_invoice_id IS NOT NULL) THEN
17:13:54 1122  	 statement := statement || CHR(10) || 'AND INVOICE.ID=' || in_invoice_id;
17:13:54 1123  END IF;
17:13:54 1124  
17:13:54 1125  IF (in_subscription_id IS NOT NULL) THEN
17:13:54 1126  	 statement := statement || CHR(10) || 'AND SUBSCRIPTION.ID=' || in_subscription_id;
