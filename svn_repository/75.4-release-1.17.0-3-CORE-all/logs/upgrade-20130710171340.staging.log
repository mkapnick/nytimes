17:13:54 703  
17:13:54 704  	PROCEDURE START_GC_PURCHASING (
17:13:54 705  	  in_group_id		    IN NUMBER,
17:13:54 706  	  in_offer_chain_id	    IN VARCHAR2,
17:13:54 707  	  in_gift_certificate_code  IN	VARCHAR2,
17:13:54 708  	  in_created_by 	    IN	VARCHAR2,
17:13:54 709  	  in_recipient_name	    IN	VARCHAR2,
17:13:54 710  	  in_recipient_email	    IN	VARCHAR2,
17:13:54 711  	  in_recipient_address_id   IN NUMBER,
17:13:54 712  	  in_recipient_notify_date  IN DATE,
17:13:54 713  	  in_sender_name	    IN VARCHAR2,
17:13:54 714  	  in_sender_email	    IN VARCHAR2,
17:13:54 715  	  in_gift_message	    IN	VARCHAR2,
17:13:54 716  	  in_expiration_date	    IN DATE,
17:13:54 717  	  in_campaign		    IN VARCHAR2,
17:13:54 718  	  in_reference_code	    IN VARCHAR2,
17:13:54 719  	  out_gift_certificate_id   OUT NUMBER,
17:13:54 720  	  out_invoice_id	    OUT NUMBER
17:13:54 721  	) AS
17:13:54 722  	  SPROC_NAME CONSTANT VARCHAR2(19) := 'START_GC_PURCHASING';
17:13:54 723  -- VARIABLES
17:13:54 724  	  var_account_id	    NUMBER;
17:13:54 725  	  temp_gc_code		    GIFT_CERTIFICATE.CODE%TYPE;
17:13:54 726  	  var_och_is_gc 	    NUMBER;
17:13:54 727  	  var_offer_chain_status_id NUMBER;
17:13:54 728  	  var_is_for_redemption     NUMBER;
17:13:54 729  	  var_new_invoice_id	    NUMBER;
17:13:54 730  	  var_gift_cert_id	    NUMBER;
17:13:54 731  	  var_account_tax_exempt_id VARCHAR2(255);
17:13:54 732  -- EXCEPTIONS
17:13:54 733  	    BAD_GROUP_ID		  EXCEPTION;
17:13:54 734  	    GC_CODE_ALREADY_EXISTS	  EXCEPTION;
17:13:54 735  	    BAD_OFFER_CHAIN_ID		  EXCEPTION;
17:13:54 736  	    OCH_IS_NOT_GIFT_CERTIFICATE   EXCEPTION;
17:13:54 737  	    BAD_OFFER_CHAIN_STATUS	  EXCEPTION;
17:13:54 738  	    CAN_NOT_PURCHASE_GC_FOR_RDMPN EXCEPTION;
17:13:54 739  	    CAN_NOT_CREATE_INVOICE	  EXCEPTION;
17:13:54 740  	    OFFER_REC_NUM_LESS_THAN_ONE   EXCEPTION;
17:13:54 741  	    CAN_NOT_CREATE_LINE_ITEMS	  EXCEPTION;
17:13:54 742  	    REF_CODE_ALREADY_EXISTS	  EXCEPTION;
17:13:54 743  
17:13:54 744  	  EXCEPTION_MESSAGE VARCHAR2(1024);
17:13:54 745  	  BEGIN
17:13:54 746  -- Get account id
17:13:54 747  	    BEGIN
17:13:54 748  	      SELECT
17:13:54 749  		ACCOUNT.ID,
17:13:54 750  		ACCOUNT.TAX_EXEMPT_ID
17:13:54 751  	      into
17:13:54 752  		var_account_id,
17:13:54 753  		var_account_tax_exempt_id
17:13:54 754  	      FROM
17:13:54 755  		ACCOUNT
17:13:54 756  	      WHERE
17:13:54 757  		ACCOUNT.GROUP_ID = in_group_id;
17:13:54 758  	      EXCEPTION
17:13:54 759  	      WHEN NO_DATA_FOUND THEN
17:13:54 760  	      RAISE BAD_GROUP_ID;
17:13:54 761  	    END;
17:13:54 762  
17:13:54 763  -- Check for the same code
17:13:54 764  	    BEGIN
17:13:54 765  	      SELECT
17:13:54 766  		GIFT_CERTIFICATE.CODE into temp_gc_code
17:13:54 767  	      FROM
17:13:54 768  		GIFT_CERTIFICATE
17:13:54 769  	      WHERE
17:13:54 770  		GIFT_CERTIFICATE.CODE = in_gift_certificate_code;
17:13:54 771  
17:13:54 772  	      RAISE GC_CODE_ALREADY_EXISTS;
17:13:54 773  
17:13:54 774  	      EXCEPTION
17:13:54 775  	      WHEN NO_DATA_FOUND THEN
17:13:54 776  	      NULL;
17:13:54 777  	    END;
17:13:54 778  
17:13:54 779  -- Get offer chain flag "is_gift_certificate"
17:13:54 780  	    BEGIN
17:13:54 781  	      SELECT
17:13:54 782  		OFFER_CHAIN.IS_GIFT_CERTIFICATE,
17:13:54 783  		OFFER_CHAIN.OFFER_CHAIN_STATUS_ID
17:13:54 784  	      into
17:13:54 785  		var_och_is_gc,
17:13:54 786  		var_offer_chain_status_id
17:13:54 787  	      FROM
17:13:54 788  		OFFER_CHAIN
17:13:54 789  	      WHERE
17:13:54 790  		OFFER_CHAIN.ID = in_offer_chain_id;
17:13:54 791  	      EXCEPTION
17:13:54 792  	      WHEN NO_DATA_FOUND THEN
17:13:54 793  	      RAISE BAD_OFFER_CHAIN_ID;
17:13:54 794  	    END;
17:13:54 795  
17:13:54 796  	    IF var_och_is_gc != GLOBAL_CONSTANTS_V23.TRUE
17:13:54 797  	       OR var_och_is_gc IS NULL THEN
17:13:54 798  	      RAISE OCH_IS_NOT_GIFT_CERTIFICATE;
17:13:54 799  	    END IF;
17:13:54 800  
17:13:54 801  	    IF var_offer_chain_status_id != GLOBAL_STATUSES_V23.OFFER_CHAIN_ACTIVE THEN
17:13:54 802  	      RAISE BAD_OFFER_CHAIN_STATUS;
17:13:54 803  	    END IF;
17:13:54 804  
17:13:54 805  -- norlov: #38151 check if the OC is for Redemption:
17:13:54 806  	    SELECT
17:13:54 807  	      COUNT(*) into var_is_for_redemption
17:13:54 808  	    FROM
17:13:54 809  	      OFFER_CHAIN_ELIGIBILITY
17:13:54 810  	    WHERE
17:13:54 811  	      OFFER_CHAIN_ELIGIBILITY.OFFER_CHAIN_ID = in_offer_chain_id
17:13:54 812  	      AND OFFER_CHAIN_ELIGIBILITY.NAME = GLOBAL_CONSTANTS_V23.GIFT_CERTIFICATE_REQUIRED
17:13:54 813  	      AND OFFER_CHAIN_ELIGIBILITY.VALUE = GLOBAL_CONSTANTS_V23.ELIGIBILITY_FLAG_SET;
17:13:54 814  
17:13:54 815  	    IF var_is_for_redemption > 0 THEN
17:13:54 816  	      RAISE CAN_NOT_PURCHASE_GC_FOR_RDMPN;
17:13:54 817  	    END IF;
17:13:54 818  
17:13:54 819  -- Create new invoice
17:13:54 820  	    BEGIN
17:13:54 821  	      PROCS_INVOICE_V23.CREATE_INVOICE(
17:13:54 822  		  in_invoice_status => GLOBAL_STATUSES_V23.INVOICE_OPEN,
17:13:54 823  		  in_created_by     => in_created_by,
17:13:54 824  		  in_tax_exempt_id  => var_account_tax_exempt_id,
17:13:54 825  		  out_invoice_id    => var_new_invoice_id
17:13:54 826  	      );
17:13:54 827  	      EXCEPTION
17:13:54 828  	      WHEN OTHERS THEN
