17:13:54 759  PROCEDURE GET_ACCOUNT_SUBSCR_INVOICES (
17:13:54 760  	in_group_id	   IN  NUMBER,
17:13:54 761  	in_subscription_id IN NUMBER,
17:13:54 762  	out_result_set	   OUT SYS_REFCURSOR
17:13:54 763  ) AS
17:13:54 764  SPROC_NAME	   CONSTANT VARCHAR2(27) := 'GET_ACCOUNT_SUBSCR_INVOICES';
17:13:54 765  -- VARIABLES
17:13:54 766  var_account_id	   NUMBER;
17:13:54 767  temp_subscription_id NUMBER;
17:13:54 768  -- EXCEPTIONS
17:13:54 769  BAD_GROUP_ID	  EXCEPTION;
17:13:54 770  BAD_SUBSCRIPTION_ID EXCEPTION;
17:13:54 771  BEGIN
17:13:54 772  	-- Get account id
17:13:54 773  	BEGIN
17:13:54 774  	  SELECT
17:13:54 775  	    ACCOUNT.ID into var_account_id
17:13:54 776  	  FROM
17:13:54 777  	    ACCOUNT
17:13:54 778  	  WHERE
17:13:54 779  	    ACCOUNT.GROUP_ID = in_group_id;
17:13:54 780  	  EXCEPTION
17:13:54 781  	    WHEN NO_DATA_FOUND THEN
17:13:54 782  	      RAISE BAD_GROUP_ID;
17:13:54 783  	END;
17:13:54 784  
17:13:54 785  	-- Check that subscription exists
17:13:54 786  	BEGIN
17:13:54 787  	  IF in_subscription_id IS NOT NULL THEN
17:13:54 788  	    SELECT
17:13:54 789  	      SUBSCRIPTION.ID into temp_subscription_id
17:13:54 790  	    FROM
17:13:54 791  	      SUBSCRIPTION
17:13:54 792  	    WHERE
17:13:54 793  	      SUBSCRIPTION.ID = in_subscription_id;
17:13:54 794  	  END IF;
17:13:54 795  	  EXCEPTION
17:13:54 796  	    WHEN NO_DATA_FOUND THEN
17:13:54 797  	      RAISE BAD_SUBSCRIPTION_ID;
17:13:54 798  	END;
17:13:54 799  
17:13:54 800  	OPEN out_result_set FOR
17:13:54 801  	SELECT DISTINCT
17:13:54 802  	  INVOICE.ID as "INVOICE_ID",
17:13:54 803  	  INVOICE.CREATE_DATE,
17:13:54 804  	  INVOICE.INVOICE_STATUS_ID,
17:13:54 805  	  PROCS_INVOICE_V23.F_CALCULATE_INVOICE_AMOUNT(INVOICE.ID) as "AMOUNT",
17:13:54 806  	  OFFER_CHAIN.ID as "OFFER_CHAIN_ID",
17:13:54 807  	  OFFER_CHAIN.NAME as "OFFER_CHAIN_NAME",
17:13:54 808  	  SUBSCRIPTION.ID as "SUBSCRIPTION_ID",
17:13:54 809  	  NULL as "GC_CODE",
17:13:54 810  	  NULL as "GC_ID"
17:13:54 811  	FROM
17:13:54 812  	  LICENSE
17:13:54 813  	  INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
17:13:54 814  	  INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
17:13:54 815  	  INNER JOIN OFFER_CHAIN ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
17:13:54 816  	WHERE
17:13:54 817  	  SUBSCRIPTION.ACCOUNT_ID = var_account_id
17:13:54 818  	  AND (SUBSCRIPTION.SCT_ID IS NULL OR SUBSCRIPTION.SCT_ID != GLOBAL_STATUSES_V23.REAL_TIME_CANCEL_REASON)
17:13:54 819  	  AND SUBSCRIPTION.ID = NVL(in_subscription_id, SUBSCRIPTION.ID);
17:13:54 820  
17:13:54 821  EXCEPTION
17:13:54 822  WHEN BAD_GROUP_ID THEN
17:13:54 823  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 824  	  SPROC_NAME, 'No such account');
17:13:54 825  WHEN BAD_SUBSCRIPTION_ID THEN
17:13:54 826  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 827  	  SPROC_NAME, 'No such subscription');
17:13:54 828  WHEN OTHERS THEN
17:13:54 829  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 830  	  SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 831  END GET_ACCOUNT_SUBSCR_INVOICES;
