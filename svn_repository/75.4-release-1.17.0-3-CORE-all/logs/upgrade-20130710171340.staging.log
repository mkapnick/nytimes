17:13:54 121  PROCEDURE LOCK_ACCOUNT (
17:13:54 122  	in_group_id    IN NUMBER,
17:13:54 123  	in_lock_key    IN VARCHAR2,
17:13:54 124  	in_seconds_num IN NUMBER,
17:13:54 125  	in_created_by  IN VARCHAR2,
17:13:54 126  	in_reason      IN VARCHAR2
17:13:54 127  ) AS
17:13:54 128  SPROC_NAME CONSTANT VARCHAR2(12) := 'LOCK_ACCOUNT';
17:13:54 129  -- CONSTANTS
17:13:54 130  one_second_interval CONSTANT INTERVAL DAY TO SECOND := INTERVAL '0 00:00:01' DAY TO SECOND;
17:13:54 131  -- VARIABLES
17:13:54 132  var_account_id NUMBER;
17:13:54 133  -- arosolovskiy: using data variable instead of direct get data calls (#38860)
17:13:54 134  var_date		DATE := SYSDATE;
17:13:54 135  var_lock_end_date DATE;
17:13:54 136  -- EXCEPTIONS
17:13:54 137  BAD_GROUP_ID   EXCEPTION;
17:13:54 138  ALREADY_LOCKED EXCEPTION;
17:13:54 139  BEGIN
17:13:54 140  
17:13:54 141  	BEGIN
17:13:54 142  	  SELECT
17:13:54 143  	    ACCOUNT.ID into var_account_id
17:13:54 144  	  FROM
17:13:54 145  	    ACCOUNT
17:13:54 146  	  WHERE
17:13:54 147  	    ACCOUNT.GROUP_ID = in_group_id;
17:13:54 148  	  EXCEPTION
17:13:54 149  	    WHEN NO_DATA_FOUND THEN
17:13:54 150  	      RAISE BAD_GROUP_ID;
17:13:54 151  	END;
17:13:54 152  
17:13:54 153  	var_lock_end_date := var_date + ( in_seconds_num * one_second_interval );
17:13:54 154  
17:13:54 155  	BEGIN
17:13:54 156  
17:13:54 157  	  INSERT INTO ACCOUNT_LOCK (
17:13:54 158  	    ACCOUNT_ID,
17:13:54 159  	    LOCK_KEY,
17:13:54 160  	    END_DATE,
17:13:54 161  	    CREATED_BY,
17:13:54 162  	    REASON
17:13:54 163  	  ) VALUES (
17:13:54 164  	    var_account_id,
17:13:54 165  	    in_lock_key,
17:13:54 166  	    var_lock_end_date,
17:13:54 167  	    in_created_by,
17:13:54 168  	    in_reason
17:13:54 169  	  );
17:13:54 170  
17:13:54 171  	  EXCEPTION
17:13:54 172  	    WHEN DUP_VAL_ON_INDEX THEN
17:13:54 173  	      -- This rows was created before... I will try to update it
17:13:54 174  	      BEGIN
17:13:54 175  
17:13:54 176  		UPDATE
17:13:54 177  		  ACCOUNT_LOCK
17:13:54 178  		SET
17:13:54 179  		  ACCOUNT_LOCK.LOCK_KEY = in_lock_key,
17:13:54 180  		  ACCOUNT_LOCK.END_DATE = var_lock_end_date,
17:13:54 181  		  ACCOUNT_LOCK.CREATED_BY = in_created_by,
17:13:54 182  		  ACCOUNT_LOCK.REASON = in_reason
17:13:54 183  		WHERE
17:13:54 184  		  ACCOUNT_LOCK.ACCOUNT_ID = var_account_id
17:13:54 185  		  AND ACCOUNT_LOCK.END_DATE <= var_date;
17:13:54 186  
17:13:54 187  		IF SQL%ROWCOUNT = 0 THEN
17:13:54 188  		  RAISE ALREADY_LOCKED;
17:13:54 189  		END IF;
17:13:54 190  
17:13:54 191  	      END;
17:13:54 192  	END;
17:13:54 193  
17:13:54 194  EXCEPTION
17:13:54 195  WHEN BAD_GROUP_ID THEN
17:13:54 196  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 197  	  SPROC_NAME, 'No such group id');
17:13:54 198  WHEN ALREADY_LOCKED THEN
17:13:54 199  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
17:13:54 200  	  SPROC_NAME, 'Account already locked');
17:13:54 201  WHEN OTHERS THEN
17:13:54 202  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 203  	  SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 204  END LOCK_ACCOUNT;
