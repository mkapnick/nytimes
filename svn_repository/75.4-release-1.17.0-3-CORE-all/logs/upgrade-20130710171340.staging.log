17:13:54 1016  
17:13:54 1017  PROCEDURE GET_MAX_REFUND (
17:13:54 1018  	 in_invoice_id IN NUMBER,
17:13:54 1019  	 out_amount    OUT NUMBER
17:13:54 1020  ) AS
17:13:54 1021  SPROC_NAME CONSTANT VARCHAR2(14) := 'GET_MAX_REFUND';
17:13:54 1022  -- VARIABLES
17:13:54 1023  temp_invoice_id	       NUMBER;
17:13:54 1024  var_invoice_refunds_sum NUMBER(10,2);
17:13:54 1025  var_invoice_charges_sum NUMBER(10,2);
17:13:54 1026  -- EXCEPTIONS
17:13:54 1027  BAD_INVOICE_ID EXCEPTION;
17:13:54 1028  BEGIN
17:13:54 1029  
17:13:54 1030  	 BEGIN
17:13:54 1031  	   SELECT
17:13:54 1032  	     INVOICE.ID into temp_invoice_id
17:13:54 1033  	   FROM
17:13:54 1034  	     INVOICE
17:13:54 1035  	   WHERE
17:13:54 1036  	     INVOICE.ID = in_invoice_id;
17:13:54 1037  	   EXCEPTION
17:13:54 1038  	     WHEN NO_DATA_FOUND THEN
17:13:54 1039  	       RAISE BAD_INVOICE_ID;
17:13:54 1040  	 END;
17:13:54 1041  
17:13:54 1042  	 var_invoice_refunds_sum := 0;
17:13:54 1043  	 var_invoice_charges_sum := 0;
17:13:54 1044  
17:13:54 1045  	 FOR f_charge IN (
17:13:54 1046  	   SELECT
17:13:54 1047  	     CHARGE.ID,
17:13:54 1048  	     CHARGE.CHARGE_STATUS_ID,
17:13:54 1049  	     CHARGE.CHARGE_AMOUNT,
17:13:54 1050  	     CHARGE.TRANSACTION_ID
17:13:54 1051  	   FROM
17:13:54 1052  	     CHARGE
17:13:54 1053  	   WHERE
17:13:54 1054  	     CHARGE.INVOICE_ID = in_invoice_id
17:13:54 1055  	 )
17:13:54 1056  	 LOOP
17:13:54 1057  	   -- If charge.status = canceled then continue
17:13:54 1058  	   IF f_charge.CHARGE_STATUS_ID != GLOBAL_STATUSES_V23.CHARGE_CANCELED THEN
17:13:54 1059  
17:13:54 1060  	     IF f_charge.CHARGE_AMOUNT > 0 THEN
17:13:54 1061  	       IF f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V23.CHARGE_PROCESSED
17:13:54 1062  		  AND PROCS_TRANSACTION_V23.IS_TRANSACTION_COLLECTED(f_charge.TRANSACTION_ID) = GLOBAL_CONSTANTS_V23.TRUE THEN
17:13:54 1063  		 -- Transaction collected
17:13:54 1064  		 var_invoice_charges_sum := var_invoice_charges_sum + f_charge.CHARGE_AMOUNT;
17:13:54 1065  	       ELSE
17:13:54 1066  		 -- Transaction is not collected. Do nothing
17:13:54 1067  		 NULL;
17:13:54 1068  	       END IF;
17:13:54 1069  	     ELSE
17:13:54 1070  	       IF f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V23.CHARGE_PROCESSED
17:13:54 1071  		  AND PROCS_TRANSACTION_V23.IS_TRANSACTION_COLLECTED(f_charge.TRANSACTION_ID) = GLOBAL_CONSTANTS_V23.FALSE THEN
17:13:54 1072  		 -- If charge is processed transaction is not collected then do nothing
17:13:54 1073  		 NULL;
17:13:54 1074  	       ELSE
17:13:54 1075  		 var_invoice_refunds_sum := var_invoice_refunds_sum + f_charge.CHARGE_AMOUNT;
17:13:54 1076  	       END IF;
17:13:54 1077  	     END IF;
17:13:54 1078  
17:13:54 1079  	   END IF;
17:13:54 1080  	 END LOOP;
17:13:54 1081  
17:13:54 1082  	 -- Refunds are negative
17:13:54 1083  	 var_invoice_refunds_sum := 0 - var_invoice_refunds_sum;
17:13:54 1084  
17:13:54 1085  	 out_amount := var_invoice_charges_sum - var_invoice_refunds_sum;
17:13:54 1086  
17:13:54 1087  EXCEPTION
17:13:54 1088  WHEN BAD_INVOICE_ID THEN
17:13:54 1089  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 1090  	   SPROC_NAME, 'No such invoice');
17:13:54 1091  WHEN OTHERS THEN
17:13:54 1092  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 1093  	   SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 1094  END GET_MAX_REFUND;
17:13:54 1095  
