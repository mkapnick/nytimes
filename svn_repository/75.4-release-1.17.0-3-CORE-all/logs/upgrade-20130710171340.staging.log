17:13:54 370  ) AS
17:13:54 371  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_SUBSCRIPTION_SHARES';
17:13:54 372  range_diff NUMBER := 0;
17:13:54 373  upper_bond_diff NUMBER := 0;
17:13:54 374  l_start NUMBER := 0;
17:13:54 375  l_end   NUMBER := 0;
17:13:54 376  BEGIN
17:13:54 377  	-- Normalize the end points [START]
17:13:54 378  	IF (in_start IS NULL OR in_start < 0) Then
17:13:54 379  	  l_start := 0;
17:13:54 380  	ELSE
17:13:54 381  	  l_start := in_start;
17:13:54 382  	END IF;
17:13:54 383  
17:13:54 384  	IF (in_end IS NULL) Then
17:13:54 385  	  l_end := 11;
17:13:54 386  	ELSE
17:13:54 387  	  l_end := in_end;
17:13:54 388  	END IF;
17:13:54 389  
17:13:54 390  	l_start := l_start + 1;
17:13:54 391  	l_end	:= l_end   + 1;
17:13:54 392  
17:13:54 393  	range_diff := l_end - l_start;
17:13:54 394  	upper_bond_diff :=  range_diff - GLOBAL_CONSTANTS_V23.MAX_RETURN_COUNT;
17:13:54 395  
17:13:54 396  	IF (upper_bond_diff > 0) Then
17:13:54 397  	  l_end := l_end - upper_bond_diff;
17:13:54 398  	END IF;
17:13:54 399  	-- Normalize the end points [END]
17:13:54 400  
17:13:54 401  	BEGIN
17:13:54 402  	   OPEN out_result_set FOR
17:13:54 403  	   SELECT *
17:13:54 404  	   FROM
17:13:54 405  	     (SELECT rownum rnum, Q.*
17:13:54 406  	      FROM
17:13:54 407  	       (SELECT
17:13:54 408  		  ss.ID,
17:13:54 409  		  ss.GROUP_ACCOUNT_ID,
17:13:54 410  		  ss.BORROWER_ACCOUNT_ID,
17:13:54 411  		  ss.IP_ADDRESS,
17:13:54 412  		  ss.START_DATE,
