17:13:54 451  
17:13:54 452  PROCEDURE DISABLE_PAYPAL (
17:13:54 453  /*
17:13:54 454  Throws exceptions:
17:13:54 455  APP_EXCEPTION_CODES_V23.NOT_FOUND
17:13:54 456  APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR
17:13:54 457  APP_EXCEPTION_CODES_V23.INTERNAL_ERROR
17:13:54 458  */
17:13:54 459  	in_paypal_id  IN NUMBER,
17:13:54 460  	in_updated_by IN VARCHAR2
17:13:54 461  ) AS
17:13:54 462  SPROC_NAME CONSTANT VARCHAR2(14) := 'DISABLE_PAYPAL';
17:13:54 463  -- VARIABLES
17:13:54 464  var_paypal_status_id NUMBER;
17:13:54 465  var_account_id	   NUMBER;
17:13:54 466  var_group_id	   NUMBER;
17:13:54 467  var_pending_invoices_num NUMBER;
17:13:54 468  current_def_instrument_type_id NUMBER;
17:13:54 469  current_def_instrument_id NUMBER;
17:13:54 470  -- EXCEPTIONS
17:13:54 471  BAD_PAYPAL_ID EXCEPTION;
17:13:54 472  PAYPAL_ALREADY_INACTIVE EXCEPTION;
17:13:54 473  PENDING_TRANSACTIONS_FOUND EXCEPTION;
17:13:54 474  CAN_NOT_GET_DEF_FINANCIAL EXCEPTION;
17:13:54 475  CAN_NOT_DEL_DEF_FINANCIAL EXCEPTION;
17:13:54 476  EXCEPTION_MESSAGE VARCHAR2(1024);
17:13:54 477  BEGIN
17:13:54 478  
17:13:54 479  	-- Get Paypal status
17:13:54 480  	-- Get account
17:13:54 481  	BEGIN
17:13:54 482  	  SELECT
17:13:54 483  	    PAYPAL.PAYPAL_STATUS_ID,
17:13:54 484  	    PAYPAL.ACCOUNT_ID
17:13:54 485  	    into
17:13:54 486  	    var_paypal_status_id,
17:13:54 487  	    var_account_id
17:13:54 488  	  FROM
17:13:54 489  	    PAYPAL
17:13:54 490  	  WHERE
17:13:54 491  	    PAYPAL.ID = in_paypal_id;
17:13:54 492  	  EXCEPTION
17:13:54 493  	    WHEN NO_DATA_FOUND THEN
17:13:54 494  	      RAISE BAD_PAYPAL_ID;
17:13:54 495  	END;
17:13:54 496  
17:13:54 497  	-- Get group id
17:13:54 498  	SELECT
17:13:54 499  	  ACCOUNT.GROUP_ID into var_group_id
17:13:54 500  	FROM
17:13:54 501  	  ACCOUNT
17:13:54 502  	WHERE
17:13:54 503  	  ACCOUNT.ID = var_account_id;
17:13:54 504  
17:13:54 505  	-- Card should not be disabled
17:13:54 506  	IF var_paypal_status_id = GLOBAL_STATUSES_V23.PAYPAL_INACTIVE THEN
17:13:54 507  	  RAISE PAYPAL_ALREADY_INACTIVE;
17:13:54 508  	END IF;
17:13:54 509  
17:13:54 510  	-- Looking for pending transactions associated with given credit card
17:13:54 511  	SELECT
17:13:54 512  	  COUNT(*) into var_pending_invoices_num
17:13:54 513  	FROM
17:13:54 514  	  CHARGE
17:13:54 515  	  INNER JOIN TRANSACTION ON CHARGE.TRANSACTION_ID = TRANSACTION.ID
17:13:54 516  	WHERE
17:13:54 517  	  CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V23.INSTRUMENT_PAYPAL
17:13:54 518  	  AND CHARGE.INSTRUMENT_ID = in_paypal_id
17:13:54 519  	  AND TRANSACTION.TRANSACTION_STATUS_ID = GLOBAL_STATUSES_V23.TRANSACTION_PENDING;
17:13:54 520  
17:13:54 521  	IF var_pending_invoices_num > 0 THEN
17:13:54 522  	  RAISE PENDING_TRANSACTIONS_FOUND;
17:13:54 523  	END IF;
17:13:54 524  
17:13:54 525  	-- Getting current default financial instrument
17:13:54 526  	BEGIN
17:13:54 527  	  GET_DEF_FINANCIAL_INSTRUMENT(
17:13:54 528  	    in_group_id 	   => var_group_id,
17:13:54 529  	    out_instrument_type_id => current_def_instrument_type_id,
17:13:54 530  	    out_instrument_id	   => current_def_instrument_id
17:13:54 531  	  );
17:13:54 532  	  EXCEPTION
17:13:54 533  	    WHEN OTHERS THEN
17:13:54 534  	      EXCEPTION_MESSAGE := SQLERRM;
17:13:54 535  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
17:13:54 536  	END;
17:13:54 537  
17:13:54 538  	-- Checking that credit card is not default
17:13:54 539  	IF current_def_instrument_type_id = GLOBAL_ENUMS_V23.INSTRUMENT_PAYPAL
17:13:54 540  	  AND current_def_instrument_id = in_paypal_id THEN
17:13:54 541  	  BEGIN
17:13:54 542  	    DEL_DEF_FINANCIAL_INSTRUMENT(
17:13:54 543  	      in_group_id => var_group_id
17:13:54 544  	    );
17:13:54 545  	  EXCEPTION
17:13:54 546  	    WHEN OTHERS THEN
17:13:54 547  	      EXCEPTION_MESSAGE := SQLERRM;
17:13:54 548  	      RAISE CAN_NOT_DEL_DEF_FINANCIAL;
17:13:54 549  	  END;
17:13:54 550  	END IF;
17:13:54 551  
17:13:54 552  	PROCS_FIN_INSTRUMENTS_V23.UPDATE_PAYPAL_STATUS(
17:13:54 553  	  in_paypal_id	      => in_paypal_id,
17:13:54 554  	  in_updated_by       => in_updated_by,
17:13:54 555  	  in_paypal_status_id => GLOBAL_STATUSES_V23.PAYPAL_INACTIVE
17:13:54 556  	);
17:13:54 557  
17:13:54 558  EXCEPTION
17:13:54 559  WHEN BAD_PAYPAL_ID THEN
17:13:54 560  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 561  	  SPROC_NAME, 'No such paypal');
17:13:54 562  WHEN PAYPAL_ALREADY_INACTIVE THEN
17:13:54 563  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
17:13:54 564  	  SPROC_NAME, 'Paypal already inactive');
17:13:54 565  WHEN PENDING_TRANSACTIONS_FOUND THEN
17:13:54 566  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.STATE_ERROR,
17:13:54 567  	  SPROC_NAME, 'Account has pending charge which are using this paypal');
17:13:54 568  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
17:13:54 569  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 570  	  SPROC_NAME, 'Could not get current default financial instrument', EXCEPTION_MESSAGE);
17:13:54 571  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
17:13:54 572  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 573  	  SPROC_NAME, 'Could not delete information about default financial instrument from account', EXCEPTION_MESSAGE);
17:13:54 574  WHEN OTHERS THEN
17:13:54 575  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 576  	  SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 577  END DISABLE_PAYPAL;
17:13:54 578  
17:13:54 579  /******************************************************************************/
17:13:54 580  
17:13:54 581  PROCEDURE UPDATE_CREDIT_CARD (
17:13:54 582  /*
17:13:54 583  Throws exceptions:
17:13:54 584  APP_EXCEPTION_CODES_V23.NOT_FOUND
17:13:54 585  APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR
17:13:54 586  */
17:13:54 587  	in_credit_card_id	  IN NUMBER,
17:13:54 588  	in_updated_by		  IN VARCHAR2,
17:13:54 589  	in_instrument_name	  IN VARCHAR2,
17:13:54 590  	in_is_default		  IN NUMBER
17:13:54 591  ) AS
17:13:54 592  SPROC_NAME CONSTANT VARCHAR2(18) := 'UPDATE_CREDIT_CARD';
17:13:54 593  -- VARIABLES
17:13:54 594  var_account_id NUMBER;
17:13:54 595  var_group_id   NUMBER;
17:13:54 596  temp_cc_rownum NUMBER;
17:13:54 597  current_def_instrument_type_id NUMBER;
17:13:54 598  current_def_instrument_id      NUMBER;
17:13:54 599  -- EXCEPTION
17:13:54 600  CAN_NOT_SET_DEF_FINANCIAL  EXCEPTION;
17:13:54 601  BAD_IS_DEFAULT_VALUE	 EXCEPTION;
17:13:54 602  CAN_NOT_GET_DEF_FINANCIAL  EXCEPTION;
17:13:54 603  CAN_NOT_DEL_DEF_FINANCIAL  EXCEPTION;
17:13:54 604  EXCEPTION_MESSAGE 	 VARCHAR2(1024);
17:13:54 605  BEGIN
17:13:54 606  
17:13:54 607  	-- Get account id
17:13:54 608  	SELECT
17:13:54 609  	  CREDIT_CARD.ACCOUNT_ID
17:13:54 610  	  into
17:13:54 611  	  var_account_id
17:13:54 612  	FROM
17:13:54 613  	  CREDIT_CARD
17:13:54 614  	WHERE
17:13:54 615  	  CREDIT_CARD.ID = in_credit_card_id;
17:13:54 616  
17:13:54 617  	-- Get group id
17:13:54 618  	SELECT
17:13:54 619  	  ACCOUNT.GROUP_ID into var_group_id
17:13:54 620  	FROM
17:13:54 621  	  ACCOUNT
17:13:54 622  	WHERE
17:13:54 623  	  ACCOUNT.ID = var_account_id;
17:13:54 624  
17:13:54 625  	-- Check that passed data is correct
17:13:54 626  	IF in_is_default != GLOBAL_CONSTANTS_V23.TRUE
17:13:54 627  	  AND in_is_default != GLOBAL_CONSTANTS_V23.FALSE
17:13:54 628  	  AND in_is_default IS NOT NULL THEN
17:13:54 629  	  RAISE BAD_IS_DEFAULT_VALUE;
17:13:54 630  	END IF;
17:13:54 631  
17:13:54 632  	-- Update credit card
17:13:54 633  	IF in_instrument_name IS NOT NULL THEN
17:13:54 634  	  PROCS_FIN_INSTRUMENTS_CRU_V23.UPDATE_CREDIT_CARD(
17:13:54 635  	    in_credit_card_id  => in_credit_card_id,
17:13:54 636  	    in_updated_by      => in_updated_by,
17:13:54 637  	    in_instrument_name => in_instrument_name
17:13:54 638  	  );
17:13:54 639  	END IF;
17:13:54 640  
17:13:54 641  	-- Set default financial instrument
17:13:54 642  	IF in_is_default = GLOBAL_CONSTANTS_V23.TRUE THEN
17:13:54 643  	  BEGIN
17:13:54 644  	    PROCS_FIN_INSTRUMENTS_V23.SET_DEF_FINANCIAL_INSTRUMENT(
17:13:54 645  	      in_group_id	    => var_group_id,
17:13:54 646  	      in_instrument_type_id => GLOBAL_ENUMS_V23.INSTRUMENT_CREDIT_CARD,
17:13:54 647  	      in_instrument_id	    => in_credit_card_id,
17:13:54 648  	      in_updated_by	    => in_updated_by
17:13:54 649  	    );
17:13:54 650  	    EXCEPTION
17:13:54 651  	      WHEN OTHERS THEN
17:13:54 652  		EXCEPTION_MESSAGE := SQLERRM;
17:13:54 653  		RAISE CAN_NOT_SET_DEF_FINANCIAL;
17:13:54 654  	  END;
17:13:54 655  	END IF;
17:13:54 656  
17:13:54 657  	-- Set default financial instrument
17:13:54 658  	IF in_is_default = GLOBAL_CONSTANTS_V23.FALSE THEN
17:13:54 659  	  BEGIN
17:13:54 660  	    GET_DEF_FINANCIAL_INSTRUMENT(
17:13:54 661  	      in_group_id	     => var_group_id,
17:13:54 662  	      out_instrument_type_id => current_def_instrument_type_id,
17:13:54 663  	      out_instrument_id      => current_def_instrument_id
17:13:54 664  	    );
17:13:54 665  	  EXCEPTION
17:13:54 666  	    WHEN OTHERS THEN
17:13:54 667  	      EXCEPTION_MESSAGE := SQLERRM;
17:13:54 668  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
17:13:54 669  	  END;
17:13:54 670  	  IF current_def_instrument_type_id = GLOBAL_ENUMS_V23.INSTRUMENT_CREDIT_CARD
17:13:54 671  	    AND current_def_instrument_id = in_credit_card_id THEN
17:13:54 672  	    BEGIN
17:13:54 673  	      DEL_DEF_FINANCIAL_INSTRUMENT(
17:13:54 674  		in_group_id => var_group_id
17:13:54 675  	      );
17:13:54 676  	      EXCEPTION
17:13:54 677  		WHEN OTHERS THEN
17:13:54 678  		  EXCEPTION_MESSAGE := SQLERRM;
17:13:54 679  		  RAISE CAN_NOT_DEL_DEF_FINANCIAL;
17:13:54 680  	    END;
17:13:54 681  	  END IF;
17:13:54 682  	END IF;
17:13:54 683  
17:13:54 684  EXCEPTION
17:13:54 685  WHEN NO_DATA_FOUND THEN
17:13:54 686  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 687  	  SPROC_NAME, 'No such credit card');
17:13:54 688  WHEN CAN_NOT_SET_DEF_FINANCIAL THEN
17:13:54 689  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 690  	  SPROC_NAME, 'Could not set default financial instrument for account', EXCEPTION_MESSAGE);
17:13:54 691  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
17:13:54 692  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 693  	  SPROC_NAME, 'Could not get default financial instrument for account', EXCEPTION_MESSAGE);
17:13:54 694  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
17:13:54 695  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.INTERNAL_ERROR,
17:13:54 696  	  SPROC_NAME, 'Could not delete information about default financial instrument', EXCEPTION_MESSAGE);
17:13:54 697  WHEN OTHERS THEN
17:13:54 698  	PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 699  	  SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 700  END UPDATE_CREDIT_CARD;
