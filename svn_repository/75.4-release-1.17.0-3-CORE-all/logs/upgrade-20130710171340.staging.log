17:13:54 844  PROCEDURE GET_GRACE_LICE_FOR_FINAL_TRANS (
17:13:54 845  	in_days_before_close	 IN NUMBER,
17:13:54 846  	in_num_licenses_to_fetch IN NUMBER,
17:13:54 847  	out_result_set		 OUT SYS_REFCURSOR
17:13:54 848  ) AS
17:13:54 849  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_GRACE_LICE_FOR_FINAL_TRANS';
