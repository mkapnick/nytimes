17:13:54 1653  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 1654  	   SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 1655  END SET_IS_TAX_CALCULATION_NEEDED;
