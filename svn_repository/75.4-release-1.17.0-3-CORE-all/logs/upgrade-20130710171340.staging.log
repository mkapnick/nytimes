17:13:54 417  		  ss.UPDATE_DATE,
17:13:54 418  		  a.GROUP_ID AS BORROWER_GROUP_ID
17:13:54 419  		FROM
17:13:54 420  		  SUBSCRIPTION_SHARE ss,
17:13:54 421  		  ACCOUNT a,
17:13:54 422  		  LICENSE l,
17:13:54 423  		  GROUP_ACCOUNT ga
17:13:54 424  		WHERE
