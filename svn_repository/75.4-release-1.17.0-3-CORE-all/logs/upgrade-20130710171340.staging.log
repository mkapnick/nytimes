17:13:54 985  /******************************************************************************/
17:13:54 986  
17:13:54 987  FUNCTION GET_TRANSACTION_INTRL_TAXES (
17:13:54 988  	in_transaction_id IN NUMBER
17:13:54 989  ) RETURN NUMBER AS
17:13:54 990  SPROC_NAME CONSTANT VARCHAR2(27) := 'GET_TRANSACTION_INTRL_TAXES';
17:13:54 991  -- Variables
17:13:54 992  var_intrl_tax_amount NUMBER(10, 2);
17:13:54 993  BEGIN
17:13:54 994  
17:13:54 995  	SELECT SUM(T.CALCULATED_AMOUNT) into var_intrl_tax_amount
17:13:54 996  	FROM
17:13:54 997  	  LINE_ITEM LI
17:13:54 998  	  INNER JOIN INVOICE I ON LI.INVOICE_ID = I.ID
17:13:54 999  	  INNER JOIN CHARGE CH ON CH.INVOICE_ID=  I.ID
17:13:54 1000  	   INNER JOIN TAX T ON T.LINE_ITEM_ID = LI.ID
17:13:54 1001  	 WHERE
17:13:54 1002  	   CH.TRANSACTION_ID = in_transaction_id
17:13:54 1003  	   AND T.TAX_TYPE_ID IN (
17:13:54 1004  	     SELECT GLOBAL_ENUMS_V23.TAX_TYPE_VAT FROM DUAL
17:13:54 1005  	   );
17:13:54 1006  
17:13:54 1007  	 IF var_intrl_tax_amount IS NULL THEN
17:13:54 1008  	   var_intrl_tax_amount := 0;
17:13:54 1009  	 END IF;
17:13:54 1010  
17:13:54 1011  	 RETURN var_intrl_tax_amount;
17:13:54 1012  
17:13:54 1013  END GET_TRANSACTION_INTRL_TAXES;
