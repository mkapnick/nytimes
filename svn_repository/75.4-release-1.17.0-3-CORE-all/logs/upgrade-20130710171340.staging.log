17:13:54 1163  	     IF var_product_eligibility_limit < same_product_count THEN
17:13:54 1164  	       RETURN GLOBAL_CONSTANTS_V23.TRUE;
17:13:54 1165  	     END IF;
17:13:54 1166  
17:13:54 1167  	   END IF;
17:13:54 1168  	 END LOOP;
17:13:54 1169  
17:13:54 1170  	 RETURN GLOBAL_CONSTANTS_V23.FALSE;
17:13:54 1171  
17:13:54 1172  EXCEPTION
17:13:54 1173  WHEN BAD_FIRST_OFFER_CHAIN THEN
17:13:54 1174  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 1175  	   SPROC_NAME, 'First offer chain not found');
17:13:54 1176  WHEN BAD_SECOND_OFFER_CHAIN THEN
17:13:54 1177  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 1178  	   SPROC_NAME, 'Second offer chain not found');
17:13:54 1179  WHEN OTHERS THEN
17:13:54 1180  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 1181  	   SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 1182  END CHECK_FOR_SAME_PRODUCTS;
