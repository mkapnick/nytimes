17:13:54 126  	    rownum <= in_set_maximum
17:13:54 127  	  ;
17:13:54 128  	  COMMIT;
17:13:54 129  	  RETURN v_set_id;
17:13:54 130  END;
17:13:54 131  --------------------------------------------------------------------------------
17:13:54 132  FUNCTION RETREIVE_TRANSFER_SET
17:13:54 133  	  (
17:13:54 134  	      in_set_id core_owner.polling_sync.set_id%type)
17:13:54 135  	  RETURN sys_refcursor
17:13:54 136  IS
17:13:54 137  	  v_refcursor sys_refcursor;
17:13:54 138  BEGIN
17:13:54 139  	  UPDATE
17:13:54 140  	      core_owner.polling_sync ps
17:13:54 141  	  SET
17:13:54 142  	      ps.last_send_date = sysdate,
17:13:54 143  	      ps.num_calls = ps.num_calls + 1
17:13:54 144  	  WHERE
17:13:54 145  	      ps.set_id = in_set_id ;
17:13:54 146  	  OPEN v_refcursor FOR
17:13:54 147  	  SELECT
17:13:54 148  	      ps.set_id,
17:13:54 149  	      ps.group_id,
17:13:54 150  	      ps.event_type,
17:13:54 151  	      ps.event_date
17:13:54 152  	  FROM
17:13:54 153  	      core_owner.polling_sync ps
17:13:54 154  	  WHERE
17:13:54 155  	      ps.set_id = in_set_id
17:13:54 156  	  AND ps.group_id IS NOT NULL ;
17:13:54 157  	  RETURN v_refcursor;
17:13:54 158  END;
17:13:54 159  --------------------------------------------------------------------------------
17:13:54 160  PROCEDURE GET_TRANSFER_SET
17:13:54 161  	  (
17:13:54 162  	      in_set_maximum NUMBER,
17:13:54 163  	      in_max_retries NUMBER,
17:13:54 164  	      out_refcursor OUT sys_refcursor)
17:13:54 165  IS
17:13:54 166  	  v_set_id core_owner.polling_sync.set_id%type;
17:13:54 167  BEGIN
17:13:54 168  	  /* Look for previously sent but unconfirmed sets and
17:13:54 169  	  send again until max_retries calls */
17:13:54 170  	  FOR x IN
17:13:54 171  	  (
17:13:54 172  	      SELECT
17:13:54 173  		  ps.set_id,
17:13:54 174  		  COUNT( *) set_size
17:13:54 175  	      FROM
17:13:54 176  		  core_owner.polling_sync ps
17:13:54 177  	      WHERE
17:13:54 178  		  ps.event_date > sysdate - 14
17:13:54 179  	      AND ps.confirm_date IS NULL
17:13:54 180  	      AND ps.last_send_date IS NOT NULL
17:13:54 181  	      AND ps.num_calls < in_max_retries
17:13:54 182  	      GROUP BY
17:13:54 183  		  ps.set_id
17:13:54 184  	      ORDER BY
17:13:54 185  		  ps.set_id
17:13:54 186  	  )
17:13:54 187  	  LOOP
17:13:54 188  	      out_refcursor := Retreive_Transfer_Set(x.set_id) ;
17:13:54 189  	      RETURN;
17:13:54 190  	  END LOOP;
17:13:54 191  	  v_set_id := Create_New_Transfer_Set(in_set_maximum) ;
17:13:54 192  	  out_refcursor := Retreive_Transfer_Set(v_set_id) ;
17:13:54 193  	  RETURN;
17:13:54 194  END;
17:13:54 195  --------------------------------------------------------------------------------
17:13:54 196  PROCEDURE CONFIRM_TRANSFER_SET
17:13:54 197  	  (
17:13:54 198  	      in_set_id core_owner.polling_sync.set_id%type)
17:13:54 199  IS
17:13:54 200  	  v_unconfirmable EXCEPTION;
17:13:54 201  BEGIN
17:13:54 202  	  UPDATE
17:13:54 203  	      core_owner.polling_sync ps
17:13:54 204  	  SET
17:13:54 205  	      ps.confirm_date = systimestamp
17:13:54 206  	  WHERE
17:13:54 207  	      ps.set_id = in_set_id
17:13:54 208  	  AND ps.confirm_date IS NULL ;
17:13:54 209  	  IF(sql%rowcount < 1) THEN
17:13:54 210  	      raise v_unconfirmable;
17:13:54 211  	  END IF;
17:13:54 212  END;
17:13:54 213  PROCEDURE SET_LAST_RUN(ts in timestamp)
17:13:54 214  IS
17:13:54 215  BEGIN
17:13:54 216  	  UPDATE POLLING_SYNC_LASTRUN
17:13:54 217  	  SET last_run = current_timestamp;
17:13:54 218  	  IF ( sql%rowcount = 0 )
17:13:54 219  	  THEN
17:13:54 220  	    INSERT INTO POLLING_SYNC_LASTRUN VALUES (ts);
17:13:54 221  	  END if;
17:13:54 222  	  COMMIT;
17:13:54 223  END;
17:13:54 224  PROCEDURE GET_LAST_RUN(ts out timestamp)
17:13:54 225  IS
17:13:54 226  BEGIN
17:13:54 227  	  SELECT LAST_RUN INTO ts
17:13:54 228  	  FROM POLLING_SYNC_LASTRUN
