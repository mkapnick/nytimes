17:13:54 2731  PROCEDURE ARE_REFUNDS_PENDING_FOR_SUBSCR (
17:13:54 2732  	 in_subscription_id IN NUMBER,
17:13:54 2733  	 out_result	    OUT NUMBER
17:13:54 2734  ) AS
17:13:54 2735  SPROC_NAME CONSTANT VARCHAR2(30) := 'ARE_REFUNDS_PENDING_FOR_SUBSCR';
17:13:54 2736  -- VARIABLES
17:13:54 2737  temp_subscription_id NUMBER;
17:13:54 2738  var_local_result     NUMBER;
17:13:54 2739  -- EXCEPTIONS
17:13:54 2740  BAD_SUBSCRIPTION_ID EXCEPTION;
17:13:54 2741  BEGIN
17:13:54 2742  
17:13:54 2743  	 -- Check that subscription exists
17:13:54 2744  	 BEGIN
17:13:54 2745  	   SELECT
17:13:54 2746  	     SUBSCRIPTION.ID into temp_subscription_id
17:13:54 2747  	   FROM
17:13:54 2748  	     SUBSCRIPTION
17:13:54 2749  	   WHERE
17:13:54 2750  	     SUBSCRIPTION.ID = in_subscription_id;
17:13:54 2751  	   EXCEPTION
17:13:54 2752  	     WHEN NO_DATA_FOUND THEN
17:13:54 2753  	       RAISE BAD_SUBSCRIPTION_ID;
17:13:54 2754  	 END;
17:13:54 2755  
17:13:54 2756  	 var_local_result := NULL;
17:13:54 2757  
17:13:54 2758  	 -- Check charges for each invoice associated with gived subscription
17:13:54 2759  	 FOR f_invoice IN (
17:13:54 2760  	   SELECT DISTINCT
17:13:54 2761  	     LICENSE.INVOICE_ID as "ID"
17:13:54 2762  	   FROM
17:13:54 2763  	     LICENSE
17:13:54 2764  	   WHERE
17:13:54 2765  	     LICENSE.SUBSCRIPTION_ID = in_subscription_id
17:13:54 2766  	     AND LICENSE.LICENSE_STATUS_ID IN ( SELECT GLOBAL_STATUSES_V23.LICENSE_ACTIVE FROM DUAL )
17:13:54 2767  	 )
17:13:54 2768  	 LOOP
17:13:54 2769  
17:13:54 2770  	   -- Check each charge in invoice
17:13:54 2771  	   FOR f_charge IN (
17:13:54 2772  	     SELECT
17:13:54 2773  	       CHARGE.ID,
17:13:54 2774  	       CHARGE.CHARGE_STATUS_ID,
17:13:54 2775  	       CHARGE.CHARGE_AMOUNT
17:13:54 2776  	     FROM
17:13:54 2777  	       CHARGE
17:13:54 2778  	     WHERE
17:13:54 2779  	       CHARGE.INVOICE_ID = f_invoice.ID
17:13:54 2780  	   )
17:13:54 2781  	   LOOP
17:13:54 2782  
17:13:54 2783  	     -- Charge amount < 0     => it is a refund
17:13:54 2784  	     -- Charge status is OPEN => means that it is not processed yet
17:13:54 2785  	     IF f_charge.CHARGE_AMOUNT < 0
17:13:54 2786  		AND f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V23.CHARGE_OPENED THEN
17:13:54 2787  	       var_local_result := GLOBAL_CONSTANTS_V23.TRUE;
17:13:54 2788  	     END IF;
17:13:54 2789  
17:13:54 2790  	   END LOOP;
17:13:54 2791  
17:13:54 2792  	 END LOOP;
17:13:54 2793  
17:13:54 2794  	 IF var_local_result IS NULL THEN
17:13:54 2795  	   out_result := GLOBAL_CONSTANTS_V23.FALSE;
17:13:54 2796  	 ELSE
17:13:54 2797  	   out_result := GLOBAL_CONSTANTS_V23.TRUE;
17:13:54 2798  	 END IF;
17:13:54 2799  
17:13:54 2800  EXCEPTION
17:13:54 2801  WHEN BAD_SUBSCRIPTION_ID THEN
17:13:54 2802  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.NOT_FOUND,
17:13:54 2803  	   SPROC_NAME, 'No such transaction id');
17:13:54 2804  WHEN OTHERS THEN
17:13:54 2805  	 PROCS_COMMON_V23.THROW_EXCEPTION(APP_EXCEPTION_CODES_V23.UNKNOWN_ERROR,
17:13:54 2806  	   SPROC_NAME, 'Unknown error', SQLERRM);
17:13:54 2807  END ARE_REFUNDS_PENDING_FOR_SUBSCR;
