15:47:48 439  /******************************************************************************/
15:47:48 440  
15:47:48 441  FUNCTION CALCULATE_OFFER_CHAIN_END_DATE (
15:47:48 442  /*
15:47:48 443  Throws exceptions (codes):
15:47:48 444  APP_EXCEPTION_CODES_V17.NOT_FOUND
15:47:48 445  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:48 446  */
15:47:48 447  	in_offer_chain_id	  IN NUMBER,
15:47:48 448  	in_offer_chain_start_date IN DATE
15:47:48 449  ) RETURN DATE AS
15:47:48 450  -- VARIABLES
15:47:48 451  SPROC_NAME	     CONSTANT VARCHAR2(30) := 'CALCULATE_OFFER_CHAIN_END_DATE';
15:47:48 452  temp_offer_chain_id    NUMBER;
15:47:48 453  var_offer_chain_length NUMBER;
15:47:48 454  var_offer_duration     VARCHAR2(30);
15:47:48 455  var_offer_recurrences  NUMBER;
15:47:48 456  var_end_date	     DATE;
15:47:48 457  
15:47:48 458  var_offer_ym_interval INTERVAL YEAR TO MONTH;
15:47:48 459  var_offer_ds_interval INTERVAL DAY(3) TO SECOND;
15:47:48 460  var_offer_years	    NUMBER;
15:47:48 461  var_offer_months	    NUMBER;
15:47:48 462  var_offer_days	    NUMBER;
15:47:48 463  var_infinity_offers_count NUMBER;
15:47:48 464  
15:47:48 465  var_offers_set SYS_REFCURSOR;
15:47:48 466  
15:47:48 467  -- EXCEPTIONS
15:47:48 468  BAD_OFFER_CHAIN_ID EXCEPTION;
15:47:48 469  BEGIN
15:47:48 470  
15:47:48 471  	var_end_date := in_offer_chain_start_date;
15:47:48 472  
15:47:48 473  	-- Check that offer chain exists
15:47:48 474  	BEGIN
15:47:48 475  	  SELECT
15:47:48 476  	    OFFER_CHAIN.ID into temp_offer_chain_id
15:47:48 477  	  FROM
15:47:48 478  	    OFFER_CHAIN
15:47:48 479  	  WHERE
15:47:48 480  	    OFFER_CHAIN.ID = in_offer_chain_id;
15:47:48 481  	  EXCEPTION
15:47:48 482  	    WHEN NO_DATA_FOUND
15:47:48 483  	      THEN RAISE BAD_OFFER_CHAIN_ID;
15:47:48 484  	END;
15:47:48 485  
15:47:48 486  	SELECT
15:47:48 487  	  COUNT(*) into var_infinity_offers_count
15:47:48 488  	FROM
15:47:48 489  	  OFFER_OFFER_CHAIN
15:47:48 490  	WHERE
15:47:48 491  	  OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
15:47:48 492  	  AND OFFER_OFFER_CHAIN.NUM_RECURRENCES = GLOBAL_ENUMS_V17.OFFER_REC_INFINITY;
15:47:48 493  
15:47:48 494  	IF var_infinity_offers_count > 0 THEN
15:47:48 495  	  -- Offer chain contains offers with infinity num of recurrences
15:47:48 496  	  RETURN NULL;
15:47:48 497  	END IF;
15:47:48 498  
15:47:48 499  	-- Select offers durations
15:47:48 500  	OPEN var_offers_set FOR
15:47:48 501  	SELECT
15:47:48 502  	  OFFER.ENTITLEMENT_DURATION,
15:47:48 503  	  OFFER_OFFER_CHAIN.NUM_RECURRENCES
15:47:48 504  	FROM
15:47:48 505  	  OFFER_OFFER_CHAIN
15:47:48 506  	  INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
15:47:48 507  	WHERE
15:47:48 508  	  OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id;
15:47:48 509  
15:47:48 510  	-- Calculate sum of offers durations
15:47:48 511  	LOOP
15:47:48 512  	  FETCH var_offers_set INTO var_offer_duration, var_offer_recurrences;
15:47:48 513  	  EXIT WHEN var_offers_set%NOTFOUND;
15:47:48 514  	  PROCS_COMMON_V17.ISO8601DURATION_TO_INTERVALS(var_offer_duration, var_offer_years, var_offer_months, var_offer_days);
15:47:48 515  	  var_offer_ym_interval := var_offer_years||'-'||var_offer_months;
15:47:48 516  	  var_offer_ds_interval := var_offer_days||' 0:0:0';
15:47:48 517  	  var_end_date := var_end_date + ( var_offer_ym_interval * ( var_offer_recurrences + 1) ) + ( var_offer_ds_interval * ( var_offer_recurrences + 1) );
15:47:48 518  	END LOOP;
15:47:48 519  
15:47:48 520  	RETURN var_end_date;
15:47:48 521  
15:47:48 522  EXCEPTION
15:47:48 523  WHEN BAD_OFFER_CHAIN_ID THEN
15:47:48 524  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:48 525  	  SPROC_NAME, 'No such offer chain');
15:47:48 526  WHEN OTHERS THEN
15:47:48 527  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:48 528  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:48 529  END CALCULATE_OFFER_CHAIN_END_DATE;
15:47:48 530  
15:47:48 531  /******************************************************************************/
15:47:48 532  
15:47:48 533  FUNCTION CALCULATE_OFFER_AMOUNT (
15:47:48 534  	in_offer_id IN NUMBER
15:47:48 535  ) RETURN NUMBER AS
15:47:48 536  -- VARIABLES
15:47:48 537  SPROC_NAME    CONSTANT VARCHAR2(22) := 'CALCULATE_OFFER_AMOUNT';
15:47:48 538  temp_offer_id NUMBER;
15:47:48 539  
15:47:48 540  var_product_offering_set	    SYS_REFCURSOR;
15:47:48 541  var_product_offering_id	    NUMBER;
15:47:48 542  var_product_offering_price    NUMBER(10,6);
15:47:48 543  var_product_offering_t_amount NUMBER(10,6);
15:47:48 544  var_product_offering_quantity NUMBER;
15:47:48 545  
15:47:48 546  var_total_amount NUMBER(10,6);
15:47:48 547  var_final_amount NUMBER(10,2);
15:47:48 548  
15:47:48 549  var_percent_discount NUMBER(10,2);
15:47:48 550  var_fixed_discount NUMBER(10,6);
15:47:48 551  
15:47:48 552  -- EXCEPTIONS
15:47:48 553  BAD_OFFER_ID EXCEPTION;
15:47:48 554  BEGIN
15:47:48 555  
15:47:48 556  	BEGIN
15:47:48 557  	  SELECT
15:47:48 558  	    OFFER.ID into temp_offer_id
15:47:48 559  	  FROM
15:47:48 560  	    OFFER
15:47:48 561  	  WHERE
15:47:48 562  	    OFFER.ID = in_offer_id;
15:47:48 563  	  EXCEPTION
15:47:48 564  	    WHEN NO_DATA_FOUND THEN
15:47:48 565  	      RAISE BAD_OFFER_ID;
15:47:48 566  	END;
15:47:48 567  
15:47:48 568  	OPEN var_product_offering_set FOR
15:47:48 569  	SELECT
15:47:48 570  	  OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID,
15:47:48 571  	  PRODUCT_OFFERING.UNIT_PRICE,
15:47:48 572  	  PRODUCT_OFFERING.QUANTITY
15:47:48 573  	FROM
15:47:48 574  	  OFFER_PRODUCT_OFFERING
15:47:48 575  	  INNER JOIN PRODUCT_OFFERING ON OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = PRODUCT_OFFERING.ID
15:47:48 576  	WHERE
15:47:48 577  	  OFFER_PRODUCT_OFFERING.OFFER_ID = in_offer_id;
15:47:48 578  
15:47:48 579  	var_total_amount := 0;
15:47:48 580  
15:47:48 581  	LOOP
15:47:48 582  	  FETCH var_product_offering_set into
15:47:48 583  	    var_product_offering_id,
15:47:48 584  	    var_product_offering_price,
15:47:48 585  	    var_product_offering_quantity;
15:47:48 586  	  EXIT WHEN var_product_offering_set%NOTFOUND;
15:47:48 587  
15:47:48 588  	  SELECT
15:47:48 589  	    SUM(DISCOUNT.FIXED_AMOUNT) into var_fixed_discount
15:47:48 590  	  FROM
15:47:48 591  	    DISCOUNT_PRODUCT_OFFERING
15:47:48 592  	    INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
15:47:48 593  	  WHERE
15:47:48 594  	    DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
15:47:48 595  	    AND DISCOUNT.FIXED_AMOUNT IS NOT NULL;
15:47:48 596  
15:47:48 597  	  SELECT
15:47:48 598  	    SUM(DISCOUNT.PERCENT_AMOUNT) into var_percent_discount
15:47:48 599  	  FROM
15:47:48 600  	    DISCOUNT_PRODUCT_OFFERING
15:47:48 601  	    INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
15:47:48 602  	  WHERE
15:47:48 603  	    DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
15:47:48 604  	    AND DISCOUNT.PERCENT_AMOUNT IS NOT NULL;
15:47:48 605  
15:47:48 606  	  var_product_offering_t_amount := var_product_offering_price * var_product_offering_quantity;
15:47:48 607  
15:47:48 608  	  IF var_percent_discount IS NOT NULL THEN
15:47:48 609  	    var_product_offering_t_amount := var_product_offering_t_amount - ( var_product_offering_t_amount * var_percent_discount / 100 );
15:47:48 610  	  END IF;
15:47:48 611  
15:47:48 612  	  IF var_fixed_discount IS NOT NULL THEN
15:47:48 613  	    var_product_offering_t_amount := var_product_offering_t_amount - var_fixed_discount;
15:47:48 614  	  END IF;
15:47:48 615  
15:47:48 616  	  var_total_amount := var_total_amount + var_product_offering_t_amount;
15:47:48 617  	END LOOP;
15:47:48 618  	var_final_amount := var_total_amount;
15:47:48 619  	RETURN var_final_amount;
15:47:48 620  
15:47:48 621  EXCEPTION
15:47:48 622  WHEN BAD_OFFER_ID THEN
15:47:48 623  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:48 624  	  SPROC_NAME, 'No such offer');
15:47:48 625  WHEN OTHERS THEN
15:47:48 626  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:48 627  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:48 628  END CALCULATE_OFFER_AMOUNT;
15:47:48 629  
15:47:48 630  /******************************************************************************/
15:47:48 631  
15:47:48 632  FUNCTION CALCULATE_OFFER_CHAIN_AMOUNT (
15:47:48 633  	in_offer_chain_id IN NUMBER
15:47:48 634  ) RETURN NUMBER AS
15:47:48 635  -- VARIABLES
15:47:48 636  SPROC_NAME	     CONSTANT VARCHAR2(28) := 'CALCULATE_OFFER_CHAIN_AMOUNT';
15:47:48 637  temp_offer_chain_id    NUMBER;
15:47:48 638  var_first_offer_id     NUMBER;
15:47:48 639  -- EXCEPTIONS
15:47:48 640  BAD_OFFER_CHAIN_ID      EXCEPTION;
15:47:48 641  CAN_NOT_GET_FIRST_OFFER EXCEPTION;
15:47:48 642  EXCEPTION_MESSAGE       VARCHAR2(1024);
15:47:48 643  BEGIN
15:47:48 644  
15:47:48 645  	BEGIN
15:47:48 646  	  SELECT
15:47:48 647  	    OFFER_CHAIN.ID into temp_offer_chain_id
15:47:48 648  	  FROM
15:47:48 649  	    OFFER_CHAIN
15:47:48 650  	  WHERE
15:47:48 651  	    OFFER_CHAIN.ID = in_offer_chain_id;
15:47:48 652  	  EXCEPTION
15:47:48 653  	    WHEN NO_DATA_FOUND THEN
15:47:48 654  	      RAISE BAD_OFFER_CHAIN_ID;
15:47:48 655  	END;
15:47:48 656  
15:47:48 657  	BEGIN
15:47:48 658  	  PROCS_OFFER_CHAIN_V17.GET_FIRST_OFFER(
15:47:48 659  	    in_offer_chain_id => in_offer_chain_id,
15:47:48 660  	    out_offer_id      => var_first_offer_id
15:47:48 661  	  );
15:47:48 662  	  EXCEPTION
15:47:48 663  	    WHEN OTHERS THEN
15:47:48 664  	      EXCEPTION_MESSAGE := SQLERRM;
15:47:48 665  	      RAISE CAN_NOT_GET_FIRST_OFFER;
15:47:48 666  	END;
15:47:48 667  
15:47:48 668  	RETURN CALCULATE_OFFER_AMOUNT(var_first_offer_id);
15:47:48 669  
15:47:48 670  EXCEPTION
15:47:48 671  WHEN BAD_OFFER_CHAIN_ID THEN
15:47:48 672  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:48 673  	  SPROC_NAME, 'No such offer chain');
15:47:48 674  WHEN CAN_NOT_GET_FIRST_OFFER THEN
15:47:48 675  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:48 676  	  SPROC_NAME, 'Could not find first offer', EXCEPTION_MESSAGE);
15:47:48 677  WHEN OTHERS THEN
15:47:48 678  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:48 679  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:48 680  END CALCULATE_OFFER_CHAIN_AMOUNT;
