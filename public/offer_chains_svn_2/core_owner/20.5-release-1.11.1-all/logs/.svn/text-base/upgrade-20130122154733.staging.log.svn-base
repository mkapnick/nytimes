15:47:50 1403  /******************************************************************************/
15:47:50 1404  
15:47:50 1405  PROCEDURE REDEEM_GIFT_CERTIFICATE (
15:47:50 1406  	 in_group_id			 IN NUMBER,
15:47:50 1407  	 in_gift_certificate_code	 IN VARCHAR2,
15:47:50 1408  	 in_created_by			 IN VARCHAR2,
15:47:50 1409  	 in_redeemer_address_id 	 IN NUMBER,
15:47:50 1410  	 in_fin_instrument_id		 IN NUMBER,
15:47:50 1411  	 in_fin_instrument_type_id	 IN NUMBER,
15:47:50 1412  	 in_redemption_offer_chain_id	 IN NUMBER,
15:47:50 1413  	 out_subscription_id		 OUT NUMBER,
15:47:50 1414  	 out_license_id 		 OUT NUMBER
15:47:50 1415  ) AS
15:47:50 1416  SPROC_NAME	       CONSTANT VARCHAR2(23) := 'REDEEM_GIFT_CERTIFICATE';
15:47:50 1417  -- VARIABLES
15:47:50 1418  var_gift_certificate_id NUMBER;
15:47:50 1419  -- norlov: #38151 var_offer_chain_id replaced by var_purchased_oc_id and var_oc_id_to_redeem
15:47:50 1420  var_purchased_oc_id     NUMBER;
15:47:50 1421  var_oc_id_to_redeem     NUMBER := in_redemption_offer_chain_id;
15:47:50 1422  var_offer_duration      VARCHAR2(30);
15:47:50 1423  var_invoice_id	       NUMBER;
15:47:50 1424  var_succ_purch_attempts_num NUMBER;
15:47:50 1425  var_subscription_id     NUMBER;
15:47:50 1426  var_license_id	       NUMBER;
15:47:50 1427  var_account_id	       NUMBER;
15:47:50 1428  var_gc_status_id        NUMBER;
15:47:50 1429  var_gc_charges_amount   NUMBER;
15:47:50 1430  var_gc_expiration_date  DATE;
15:47:50 1431  var_gc_redeemer_group_id NUMBER;
15:47:50 1432  var_gc_purchase_invoice_id NUMBER;
15:47:50 1433  var_gc_purchase_inv_status_id NUMBER;
15:47:50 1434  var_offer_index		     NUMBER;
15:47:50 1435  var_purchaser_group_id	     NUMBER;
15:47:50 1436  temp_license_id		     NUMBER;
15:47:50 1437  var_same_offer_chains_num     NUMBER;
15:47:50 1438  var_max_concurrent_subscrs    NUMBER;
15:47:50 1439  var_account_tax_exempt_id     VARCHAR2(255);
15:47:50 1440  var_fin_instrument_type_id    NUMBER := in_fin_instrument_type_id;
15:47:50 1441  var_fin_instrument_id	     NUMBER := in_fin_instrument_id;
15:47:50 1442  var_first_offer_id	  NUMBER;
15:47:50 1443  var_date 	     DATE := SYSDATE;
15:47:50 1444  
15:47:50 1445  var_offers SYS_REFCURSOR;
15:47:50 1446  
15:47:50 1447  -- EXCEPTIONS
15:47:50 1448  BAD_GIFT_CERTIFICATE_CODE      EXCEPTION;
15:47:50 1449  BAD_GROUP_ID		      EXCEPTION;
15:47:50 1450  CAN_NOT_CREATE_LICENSE	      EXCEPTION;
15:47:50 1451  GIFT_CERT_IS_FINALIZED	      EXCEPTION;
15:47:50 1452  GIFT_CERT_IS_REFUNDED	      EXCEPTION;
15:47:50 1453  CAN_NOT_UPDATE_CERTIFICATE     EXCEPTION;
15:47:50 1454  GIFT_CERTIFICATE_EXPIRED       EXCEPTION;
15:47:50 1455  GIFT_CERTIFICATE_REDEEMED      EXCEPTION;
15:47:50 1456  USER_ALREADY_SUBSCRIBED_TO_PRD EXCEPTION;
15:47:50 1457  LIMIT_REACHED		      EXCEPTION;
15:47:50 1458  GC_PURCHASE_INVOICE_NOT_CLOSED EXCEPTION;
15:47:50 1459  PURCHASE_INVOICES_NOT_PAID     EXCEPTION;
15:47:50 1460  OC_TO_REDEEM_NOT_FOUND	      EXCEPTION;
15:47:50 1461  CAN_NOT_GET_FIRST_OFFER_CHAIN  EXCEPTION;
15:47:50 1462  EXCEPTION_MESSAGE	      VARCHAR2(1024);
15:47:50 1463  BEGIN
15:47:50 1464  
15:47:50 1465  	 -- Get account id
15:47:50 1466  	 BEGIN
15:47:50 1467  	   SELECT
15:47:50 1468  	     ACCOUNT.ID,
15:47:50 1469  	     ACCOUNT.TAX_EXEMPT_ID
15:47:50 1470  	     into
15:47:50 1471  	     var_account_id,
15:47:50 1472  	     var_account_tax_exempt_id
15:47:50 1473  	   FROM
15:47:50 1474  	     ACCOUNT
15:47:50 1475  	   WHERE
15:47:50 1476  	     ACCOUNT.GROUP_ID = in_group_id
15:47:50 1477  	     AND ROWNUM <= 1;
15:47:50 1478  
15:47:50 1479  	   EXCEPTION
15:47:50 1480  	   WHEN NO_DATA_FOUND THEN
15:47:50 1481  	     RAISE BAD_GROUP_ID;
15:47:50 1482  	 END;
15:47:50 1483  
15:47:50 1484  	 -- Get gift certificate data
15:47:50 1485  	 BEGIN
15:47:50 1486  	   SELECT
15:47:50 1487  	     GIFT_CERTIFICATE.ID,
15:47:50 1488  	     GIFT_CERTIFICATE.OFFER_CHAIN_ID,
15:47:50 1489  	     GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID,
15:47:50 1490  	     GIFT_CERTIFICATE.PURCHASER_GROUP_ID,
15:47:50 1491  	     GIFT_CERTIFICATE.EXPIRATION_DATE,
15:47:50 1492  	     GIFT_CERTIFICATE.REDEEMER_GROUP_ID,
15:47:50 1493  	     GIFT_CERTIFICATE.PURCHASE_INVOICE_ID
15:47:50 1494  	     into
15:47:50 1495  	     var_gift_certificate_id,
15:47:50 1496  	     var_purchased_oc_id,
15:47:50 1497  	     var_gc_status_id,
15:47:50 1498  	     var_purchaser_group_id,
15:47:50 1499  	     var_gc_expiration_date,
15:47:50 1500  	     var_gc_redeemer_group_id,
15:47:50 1501  	     var_gc_purchase_invoice_id
15:47:50 1502  	   FROM
15:47:50 1503  	     GIFT_CERTIFICATE
15:47:50 1504  	   WHERE
15:47:50 1505  	     GIFT_CERTIFICATE.CODE = in_gift_certificate_code
15:47:50 1506  	     AND ROWNUM <= 1;
15:47:50 1507  
15:47:50 1508  	   EXCEPTION
15:47:50 1509  	   WHEN NO_DATA_FOUND THEN
15:47:50 1510  	     RAISE BAD_GIFT_CERTIFICATE_CODE;
15:47:50 1511  	 END;
15:47:50 1512  
15:47:50 1513  	 -- get redemption oc id from meta data if it wasn't passed in, parsing will fail for gcs with multiple redemption offer chains,
15:47:50 1514  	 -- but in that case a redemption offer chain id should always be passed in
15:47:50 1515  	 IF var_oc_id_to_redeem IS NULL THEN
15:47:50 1516  	   BEGIN
15:47:50 1517  	     SELECT
15:47:50 1518  	       to_number(OFFER_CHAIN_META_DATA.VALUE)
15:47:50 1519  	       into
15:47:50 1520  	       var_oc_id_to_redeem
15:47:50 1521  	     FROM
15:47:50 1522  	       OFFER_CHAIN_META_DATA
15:47:50 1523  	     WHERE
15:47:50 1524  	       OFFER_CHAIN_META_DATA.OFFER_CHAIN_ID = var_purchased_oc_id
15:47:50 1525  	       AND OFFER_CHAIN_META_DATA.NAME = GLOBAL_CONSTANTS_V17.REDEMPTION_OC_ID
15:47:50 1526  	       AND ROWNUM = 1;
15:47:50 1527  -- requested by ticket so (but above is correct for the actual migrated data):
15:47:50 1528  --  SELECT
15:47:50 1529  --      OFFER_CHAIN.ID
15:47:50 1530  --      into
15:47:50 1531  --      var_oc_id_to_redeem
15:47:50 1532  --    FROM
15:47:50 1533  --      OFFER_CHAIN
15:47:50 1534  --	 INNER JOIN ELIGIBILITY ON OFFER_CHAIN.ID = ELIGIBILITY.OFFER_CHAIN_ID
15:47:50 1535  --	 INNER JOIN OFFER_CHAIN_META_DATA ON OFFER_CHAIN.ID = OFFER_CHAIN_META_DATA.OFFER_CHAIN_ID
15:47:50 1536  --    WHERE
15:47:50 1537  --      ELIGIBILITY.OFFER_CHAIN_ID = OFFER_CHAIN.ID
15:47:50 1538  --      AND ELIGIBILITY.NAME = GLOBAL_CONSTANTS_V17.GIFT_CERTIFICATE_REQUIRED
15:47:50 1539  --      AND ELIGIBILITY.VALUE = GLOBAL_CONSTANTS_V17.ELIGIBILITY_FLAG_SET
15:47:50 1540  --      AND OFFER_CHAIN_META_DATA.OFFER_CHAIN_ID = OFFER_CHAIN.ID
15:47:50 1541  --      AND OFFER_CHAIN_META_DATA.NAME = GLOBAL_CONSTANTS_V17.REDEMPTION_OC_ID
15:47:50 1542  --      AND to_number(OFFER_CHAIN_META_DATA.VALUE) = var_purchased_oc_id
15:47:50 1543  --      AND ROWNUM = 1;
15:47:50 1544  
15:47:50 1545  	     EXCEPTION
15:47:50 1546  	     WHEN NO_DATA_FOUND THEN
15:47:50 1547  	       RAISE OC_TO_REDEEM_NOT_FOUND;
15:47:50 1548  	   END;
15:47:50 1549  	 END IF;
15:47:50 1550  
15:47:50 1551  	 -- Check that purchase invoice for this GC was closed
15:47:50 1552  	 SELECT
15:47:50 1553  	   INVOICE.INVOICE_STATUS_ID into var_gc_purchase_inv_status_id
15:47:50 1554  	 FROM
15:47:50 1555  	   INVOICE
15:47:50 1556  	 WHERE
15:47:50 1557  	   INVOICE.ID = var_gc_purchase_invoice_id;
15:47:50 1558  
15:47:50 1559  	 IF var_gc_purchase_inv_status_id != GLOBAL_STATUSES_V17.INVOICE_CLOSED THEN
15:47:50 1560  	   RAISE GC_PURCHASE_INVOICE_NOT_CLOSED;
15:47:50 1561  	 END IF;
15:47:50 1562  
15:47:50 1563  	 -- Check that this invoice was successfully processed by billing
15:47:50 1564  	 SELECT
15:47:50 1565  	   COUNT(1) into var_succ_purch_attempts_num
15:47:50 1566  	 FROM
15:47:50 1567  	   TRANSACTION_ATTEMPT TA
15:47:50 1568  	   INNER JOIN TRANSACTION T ON T.ID = TA.TRANSACTION_ID
15:47:50 1569  	   INNER JOIN CHARGE CH ON CH.TRANSACTION_ID = T.ID
15:47:50 1570  	 WHERE
15:47:50 1571  	   CH.INVOICE_ID = var_gc_purchase_invoice_id
15:47:50 1572  	   AND TA.TRANSACTION_ATTEMPT_STATUS_ID = GLOBAL_STATUSES_V17.TRANS_ATTEMPT_SUCCESS;
15:47:50 1573  
15:47:50 1574  	 IF var_succ_purch_attempts_num = 0 THEN
15:47:50 1575  	   SELECT
15:47:50 1576  	     COUNT(1) into var_succ_purch_attempts_num
15:47:50 1577  	   FROM
15:47:50 1578  	     DUAL
15:47:50 1579  	   WHERE
15:47:50 1580  	     PROCS_INVOICE_V17.F_CALCULATE_INVOICE_AMOUNT(var_gc_purchase_invoice_id) = 0;
15:47:50 1581  	 END IF;
15:47:50 1582  
15:47:50 1583  	 IF var_succ_purch_attempts_num = 0 THEN
15:47:50 1584  	   RAISE PURCHASE_INVOICES_NOT_PAID;
15:47:50 1585  	 END IF;
15:47:50 1586  
15:47:50 1587  	 -- Check limit for gc's offer chain
15:47:50 1588  	 SELECT
15:47:50 1589  	   COUNT(*) into var_same_offer_chains_num
15:47:50 1590  	 FROM
15:47:50 1591  	   SUBSCRIPTION
15:47:50 1592  	 WHERE
15:47:50 1593  	   SUBSCRIPTION.ACCOUNT_ID = var_account_id
15:47:50 1594  	   AND SUBSCRIPTION.OFFER_CHAIN_ID = var_oc_id_to_redeem
15:47:50 1595  	   AND (
15:47:50 1596  	     SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V17.SUBSCRIPTION_ACTIVE
15:47:50 1597  	     -- TODO: Does we need next line?
15:47:50 1598  	     -- OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V17.SUBSCRIPTION_SUSPENDED
15:47:50 1599  	   );
15:47:50 1600  -- norlov: #38414 the same checking as on OC purchase (just copypasted from CREATE_SUBSCRIPTION - TODO: refactor it not to keep the same code twice)
15:47:50 1601  --  IF PROCS_OFFER_CHAIN_V17.GET_OFFER_CHAIN_MAX_CONC_SUBSC(var_oc_id_to_redeem) = var_same_offer_chains_num THEN
15:47:50 1602  --    RAISE LIMIT_REACHED;
15:47:50 1603  --  END IF;
15:47:50 1604  IF var_same_offer_chains_num = 0 THEN
15:47:50 1605  	   -- if user does not have any active existing subscriptions to the offer chain
15:47:50 1606  	   -- and if product from the offer chain is already owned from another offer chain
15:47:50 1607  	   -- then deny purchase
15:47:50 1608  	   FOR f_account_offer_chains IN (
15:47:50 1609  	     SELECT DISTINCT
15:47:50 1610  	       OFFER_CHAIN_ID
15:47:50 1611  	     FROM
15:47:50 1612  	       SUBSCRIPTION
15:47:50 1613  	     WHERE
15:47:50 1614  	       ACCOUNT_ID = var_account_id
15:47:50 1615  	       AND (
15:47:50 1616  		 SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V17.SUBSCRIPTION_ACTIVE
15:47:50 1617  		 OR SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V17.SUBSCRIPTION_SUSPENDED)
15:47:50 1618  	   )
15:47:50 1619  	   LOOP
15:47:50 1620  	     IF PROCS_OFFER_CHAIN_V17.CHECK_FOR_SAME_PRODUCTS(var_oc_id_to_redeem, f_account_offer_chains.OFFER_CHAIN_ID) = GLOBAL_CONSTANTS_V17.TRUE THEN
15:47:50 1621  	       RAISE USER_ALREADY_SUBSCRIBED_TO_PRD;
15:47:50 1622  	     END IF;
15:47:50 1623  	   END LOOP;
15:47:50 1624  	 ELSE
15:47:50 1625  
15:47:50 1626  	   -- if user have any active existing subscriptions to the offer chain
15:47:50 1627  	   -- and if MAX_CONCURRENT_SUBS <= [user's subscription count for the offer chain]
15:47:50 1628  	   -- then deny purchase
15:47:50 1629  	   var_max_concurrent_subscrs := PROCS_OFFER_CHAIN_V17.GET_OFFER_CHAIN_MAX_CONC_SUBSC(var_oc_id_to_redeem);
15:47:50 1630  	   IF var_max_concurrent_subscrs != GLOBAL_CONSTANTS_V17.INFINITY
15:47:50 1631  	     AND var_max_concurrent_subscrs <= var_same_offer_chains_num THEN
15:47:50 1632  	     RAISE LIMIT_REACHED;
15:47:50 1633  	   END IF;
15:47:50 1634  	 END IF;
15:47:50 1635  -- norlov: END OF TODO
15:47:50 1636  
15:47:50 1637  
15:47:50 1638  	 IF var_gc_redeemer_group_id IS NOT NULL THEN
15:47:50 1639  	   RAISE GIFT_CERTIFICATE_REDEEMED;
15:47:50 1640  	 END IF;
15:47:50 1641  
15:47:50 1642  	 IF var_gc_expiration_date < sysdate THEN
15:47:50 1643  	   RAISE GIFT_CERTIFICATE_EXPIRED;
15:47:50 1644  	 END IF;
15:47:50 1645  
15:47:50 1646  	  IF var_gc_status_id = GLOBAL_STATUSES_V17.GIFT_CERTIFICATE_REFUNDED THEN
15:47:50 1647  	   RAISE GIFT_CERT_IS_REFUNDED;
15:47:50 1648  	 END IF;
15:47:50 1649  
15:47:50 1650  	 IF var_gc_status_id = GLOBAL_STATUSES_V17.GIFT_CERTIFICATE_FINALIZED THEN
15:47:50 1651  	   RAISE GIFT_CERT_IS_FINALIZED;
15:47:50 1652  	 END IF;
15:47:50 1653  
15:47:50 1654  	 -- Check that user did not subscribed to same product already
15:47:50 1655  	 -- norlov: get rid of this since there is already the check?
15:47:50 1656  	 FOR f_user_offer_chain IN (
15:47:50 1657  	   SELECT DISTINCT
15:47:50 1658  	     OFFER_CHAIN_ID
15:47:50 1659  	   FROM
15:47:50 1660  	     SUBSCRIPTION
15:47:50 1661  	   WHERE
15:47:50 1662  	     ACCOUNT_ID=var_account_id
15:47:50 1663  	     AND (
15:47:50 1664  	       SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V17.SUBSCRIPTION_ACTIVE
15:47:50 1665  	       OR SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V17.SUBSCRIPTION_SUSPENDED
15:47:50 1666  	     )
15:47:50 1667  	 )
15:47:50 1668  	 LOOP
15:47:50 1669  	   IF PROCS_OFFER_CHAIN_V17.CHECK_FOR_SAME_PRODUCTS(var_oc_id_to_redeem, f_user_offer_chain.OFFER_CHAIN_ID)=GLOBAL_CONSTANTS_V17.TRUE THEN
15:47:50 1670  	     RAISE USER_ALREADY_SUBSCRIBED_TO_PRD;
15:47:50 1671  	   END IF;
15:47:50 1672  	 END LOOP;
15:47:50 1673  
15:47:50 1674  	 -- Check for gift certificate amount
15:47:50 1675  	 SELECT
15:47:50 1676  	   SUM(CHARGE.CHARGE_AMOUNT) into var_gc_charges_amount
15:47:50 1677  	 FROM
15:47:50 1678  	   CHARGE
15:47:50 1679  	 WHERE
15:47:50 1680  	   CHARGE.INSTRUMENT_ID = var_gift_certificate_id
15:47:50 1681  	   AND CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V17.INSTRUMENT_GIFT_CERTIFICATE;
15:47:50 1682  
15:47:50 1683  	 -- Create new invoice
15:47:50 1684  	 PROCS_INVOICE_V17.CREATE_INVOICE(
15:47:50 1685  	   out_invoice_id    => var_invoice_id,
15:47:50 1686  	   in_invoice_status => GLOBAL_STATUSES_V17.INVOICE_CLOSED,
15:47:50 1687  	   in_tax_exempt_id  => var_account_tax_exempt_id,
15:47:50 1688  	   in_created_by     => in_created_by
15:47:50 1689  	 );
15:47:50 1690  
15:47:50 1691  	 -- If a financial instrument wasn't passed in, use the gift certificate id
15:47:50 1692  	 -- Real financial instrument is required for upsell/till forbid gift subscriptions
15:47:50 1693  	 IF var_fin_instrument_id is null THEN
15:47:50 1694  	   var_fin_instrument_id := var_gift_certificate_id;
15:47:50 1695  	   var_fin_instrument_type_id := GLOBAL_ENUMS_V17.INSTRUMENT_GIFT_CERTIFICATE;
15:47:50 1696  	 END IF;
15:47:50 1697  
15:47:50 1698  	 -- Insert new row into subscription table
15:47:50 1699  	 PROCS_SUBSCRIPTION_CRU_V17.CREATE_SUBSCRIPTION(
15:47:50 1700  	   out_subscription_id	     => var_subscription_id,
15:47:50 1701  	   in_account_id	     => var_account_id,
15:47:50 1702  	   in_purchase_date	     => var_date,
15:47:50 1703  	   in_offer_chain_id	     => var_oc_id_to_redeem,
15:47:50 1704  	   in_created_by	     => in_created_by,
15:47:50 1705  	   in_instrument_type_id     => var_fin_instrument_type_id,
15:47:50 1706  	   in_instrument_id	     => var_fin_instrument_id,
15:47:50 1707  	   in_subscription_status_id => GLOBAL_STATUSES_V17.SUBSCRIPTION_ACTIVE
15:47:50 1708  	 );
15:47:50 1709  
15:47:50 1710  	 out_subscription_id := var_subscription_id;
15:47:50 1711  
15:47:50 1712  	 BEGIN
15:47:50 1713  	   PROCS_OFFER_CHAIN_V17.GET_FIRST_OFFER(var_oc_id_to_redeem, var_first_offer_id);
15:47:50 1714  	   EXCEPTION
15:47:50 1715  	     WHEN OTHERS THEN
15:47:50 1716  	       EXCEPTION_MESSAGE := SQLERRM;
15:47:50 1717  	       RAISE CAN_NOT_GET_FIRST_OFFER_CHAIN;
15:47:50 1718  	 END;
15:47:50 1719  
15:47:50 1720  	 BEGIN
15:47:50 1721  	   PROCS_LICENSE_V17.CREATE_LICENSE (
15:47:50 1722  	     out_license_id		 => out_license_id,
15:47:50 1723  	     in_status_id		 => GLOBAL_STATUSES_V17.LICENSE_ACTIVE,
15:47:50 1724  	     in_needs_entitlements	 => GLOBAL_CONSTANTS_V17.TRUE,
15:47:50 1725  	     in_start_date		 => var_date,
15:47:50 1726  	     in_offer_id		 => var_first_offer_id,
15:47:50 1727  	     in_subscription_id 	 => var_subscription_id,
15:47:50 1728  	     in_invoice_id		 => var_invoice_id,
