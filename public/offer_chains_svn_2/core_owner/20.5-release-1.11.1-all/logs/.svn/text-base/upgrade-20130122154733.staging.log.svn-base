15:47:48 1898  PROCEDURE CHECK_PRODUCT_ELIGIBILITY (
15:47:48 1899  	 in_group_id	   IN NUMBER,
15:47:48 1900  	 in_offer_chain_id IN NUMBER,
15:47:48 1901  	 out_is_eligible   OUT NUMBER,
15:47:48 1902  	 out_concurrent_subscription_id OUT NUMBER
15:47:48 1903  ) AS
15:47:48 1904  SPROC_NAME CONSTANT VARCHAR2(25) := 'CHECK_PRODUCT_ELIGIBILITY';
15:47:48 1905  -- Variables
15:47:48 1906  var_account_id  NUMBER;
15:47:48 1907  var_is_eligible NUMBER;
15:47:48 1908  var_is_gc       NUMBER;
15:47:48 1909  -- Exceptions
15:47:48 1910  BAD_GROUP_ID		   EXCEPTION;
15:47:48 1911  CAN_NOT_CHECK_SAME_PRODUCTS EXCEPTION;
15:47:48 1912  BAD_OC_ID		   EXCEPTION;
15:47:48 1913  EXCEPTION_MESSAGE	   VARCHAR(1024);
15:47:48 1914  BEGIN
15:47:48 1915  
15:47:48 1916  	 var_is_eligible := GLOBAL_CONSTANTS_V17.TRUE;
15:47:48 1917  
15:47:48 1918  	 out_concurrent_subscription_id := NULL;
15:47:48 1919  
15:47:48 1920  	 BEGIN
15:47:48 1921  	   SELECT
15:47:48 1922  	     OC.IS_GIFT_CERTIFICATE into var_is_gc
15:47:48 1923  	   FROM
15:47:48 1924  	     OFFER_CHAIN OC
15:47:48 1925  	   WHERE
15:47:48 1926  	     OC.ID = in_offer_chain_id;
15:47:48 1927  	   EXCEPTION
15:47:48 1928  	     WHEN NO_DATA_FOUND THEN
15:47:48 1929  	       RAISE BAD_OC_ID;
15:47:48 1930  	 END;
15:47:48 1931  
15:47:48 1932  	 -- only check eligibility if this is not a gift certificate
15:47:48 1933  	 IF (var_is_gc IS NULL OR var_is_gc != 1) THEN
15:47:48 1934  	   BEGIN
15:47:48 1935  	     SELECT
15:47:48 1936  	       A.ID into var_account_id
15:47:48 1937  	     FROM
15:47:48 1938  	       ACCOUNT A
15:47:48 1939  	     WHERE
15:47:48 1940  	       A.GROUP_ID = in_group_id;
15:47:48 1941  	     EXCEPTION
15:47:48 1942  	       WHEN NO_DATA_FOUND THEN
15:47:48 1943  		 RAISE BAD_GROUP_ID;
15:47:48 1944  	   END;
15:47:48 1945  
15:47:48 1946  
15:47:48 1947  	   FOR f_offer_chain IN (
15:47:48 1948  	       SELECT
15:47:48 1949  		 S.ID as SUBSCRIPTION_ID,
15:47:48 1950  		 S.OFFER_CHAIN_ID
15:47:48 1951  	       FROM
15:47:48 1952  		 SUBSCRIPTION S
15:47:48 1953  	       WHERE
15:47:48 1954  		 S.ACCOUNT_ID = var_account_id
15:47:48 1955  		 AND S.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V17.SUBSCRIPTION_ACTIVE
15:47:48 1956  	   )
15:47:48 1957  	   LOOP
15:47:48 1958  	     BEGIN
15:47:48 1959  	       IF (
15:47:48 1960  		 PROCS_OFFER_CHAIN_V17.CHECK_FOR_SAME_PRODUCTS(
15:47:48 1961  		   in_offer_chain_id,
15:47:48 1962  		   f_offer_chain.offer_chain_id,
15:47:48 1963  		   GLOBAL_CONSTANTS_V17.TRUE
15:47:48 1964  		 ) = GLOBAL_CONSTANTS_V17.TRUE
15:47:48 1965  	       ) THEN
15:47:48 1966  		 var_is_eligible := GLOBAL_CONSTANTS_V17.FALSE;
15:47:48 1967  		 out_concurrent_subscription_id := f_offer_chain.SUBSCRIPTION_ID;
15:47:48 1968  	       END IF;
15:47:48 1969  	       EXCEPTION
15:47:48 1970  		 WHEN OTHERS THEN
15:47:48 1971  		   EXCEPTION_MESSAGE := SQLERRM;
15:47:48 1972  		   RAISE CAN_NOT_CHECK_SAME_PRODUCTS;
15:47:48 1973  	     END;
15:47:48 1974  	   END LOOP;
15:47:48 1975  	 END IF;
15:47:48 1976  	 out_is_eligible := var_is_eligible;
15:47:48 1977  
15:47:48 1978  EXCEPTION
15:47:48 1979  WHEN BAD_GROUP_ID THEN
15:47:48 1980  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:48 1981  	   SPROC_NAME, 'No such offer chain', SQLERRM);
15:47:48 1982  WHEN BAD_OC_ID THEN
15:47:48 1983  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:48 1984  	   SPROC_NAME, 'No such offer chain', SQLERRM);
15:47:48 1985  WHEN CAN_NOT_CHECK_SAME_PRODUCTS THEN
15:47:48 1986  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:48 1987  	   SPROC_NAME, 'Could not check offers for same products', EXCEPTION_MESSAGE);
15:47:48 1988  WHEN OTHERS THEN
15:47:48 1989  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:48 1990  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:47:48 1991  END CHECK_PRODUCT_ELIGIBILITY;
