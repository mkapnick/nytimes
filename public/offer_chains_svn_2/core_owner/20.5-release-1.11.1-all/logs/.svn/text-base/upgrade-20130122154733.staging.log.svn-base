15:47:49  50  	    INVOICE
15:47:49  51  	  WHERE
15:47:49  52  	    INVOICE.ID = in_invoice_id;
15:47:49  53  	  EXCEPTION
15:47:49  54  	    WHEN NO_DATA_FOUND THEN
15:47:49  55  	      RAISE BAD_INVOICE_ID;
15:47:49  56  	END;
15:47:49  57  
15:47:49  58  	-- Check that transaction exists
15:47:49  59  	BEGIN
15:47:49  60  	  SELECT
15:47:49  61  	    TRANSACTION.ID into temp_transaction_id
15:47:49  62  	  FROM
15:47:49  63  	    TRANSACTION
15:47:49  64  	  WHERE
15:47:49  65  	    TRANSACTION.ID = in_transaction_id;
15:47:49  66  	  EXCEPTION
15:47:49  67  	    WHEN NO_DATA_FOUND THEN
15:47:49  68  	      RAISE BAD_TRANSACTION_ID;
15:47:49  69  	END;
15:47:49  70  
15:47:49  71  	-- Check that instrument exists
15:47:49  72  	IF in_instrument_type_id = GLOBAL_ENUMS_V17.INSTRUMENT_CREDIT_CARD THEN
15:47:49  73  	  IF PROCS_FIN_INSTRUMENTS_V17.IS_CREDIT_CARD_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V17.FALSE THEN
15:47:49  74  	    RAISE BAD_CREDIT_CARD_ID;
15:47:49  75  	  END IF;
15:47:49  76  	ELSIF in_instrument_type_id = GLOBAL_ENUMS_V17.INSTRUMENT_PAYPAL THEN
15:47:49  77  	  IF PROCS_FIN_INSTRUMENTS_V17.IS_PAYPAL_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V17.FALSE THEN
15:47:49  78  	    RAISE BAD_PAYPAL_ID;
15:47:49  79  	  END IF;
15:47:49  80  	END IF;
15:47:49  81  
15:47:49  82  	-- Create new charge
15:47:49  83  	PROCS_CHARGE_CRU_V17.CREATE_CHARGE(
15:47:49  84  	  out_charge_id 	=> var_new_charge_id,
15:47:49  85  	  in_invoice_id 	=> in_invoice_id,
15:47:49  86  	  in_transaction_id	=> in_transaction_id,
15:47:49  87  	  in_instrument_type_id => in_instrument_type_id,
15:47:49  88  	  in_instrument_id	=> in_instrument_id,
15:47:49  89  	  in_charge_amount	=> in_charge_amount,
15:47:49  90  	  in_charge_status_id	=> in_charge_status_id,
15:47:49  91  	  in_created_by 	=> in_created_by
15:47:49  92  	);
15:47:49  93  
15:47:49  94  	out_charge_id := var_new_charge_id;
15:47:49  95  
15:47:49  96  EXCEPTION
15:47:49  97  WHEN BAD_CHARGE_STATUS_ID THEN
15:47:49  98  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INVALID_PARAMETER,
15:47:49  99  	  PROCS_NAME, 'Bad charge status: '||in_charge_status_id);
15:47:49 100  WHEN BAD_INSTRUMENT_TYPE THEN
15:47:49 101  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INVALID_PARAMETER,
15:47:49 102  	  PROCS_NAME, 'Bad instrument type id');
15:47:49 103  WHEN BAD_INVOICE_ID THEN
15:47:49 104  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 105  	  PROCS_NAME, 'No such invoice');
15:47:49 106  WHEN BAD_TRANSACTION_ID THEN
15:47:49 107  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 108  	  PROCS_NAME, 'No such transaction');
15:47:49 109  WHEN BAD_PAYPAL_ID THEN
15:47:49 110  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 111  	  PROCS_NAME, 'No such paypal');
15:47:49 112  WHEN BAD_CREDIT_CARD_ID THEN
15:47:49 113  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 114  	  PROCS_NAME, 'No such credit card');
15:47:49 115  WHEN OTHERS THEN
15:47:49 116  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 117  	  PROCS_NAME, 'Unknown error', SQLERRM);
15:47:49 118  END CREATE_CHARGE;
