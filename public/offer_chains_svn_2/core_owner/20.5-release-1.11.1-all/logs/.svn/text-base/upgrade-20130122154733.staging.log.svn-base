15:47:49 448  
15:47:49 449  PROCEDURE GET_NEXT_OFFER (
15:47:49 450  /*
15:47:49 451  Throws exceptions:
15:47:49 452  APP_EXCEPTION_CODES_V17.NOT_FOUND
15:47:49 453  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:49 454  */
15:47:49 455  	in_license_id  IN NUMBER,
15:47:49 456  	out_result_set OUT SYS_REFCURSOR
15:47:49 457  ) AS
15:47:49 458  -- VARIABLES
15:47:49 459  SPROC_NAME		CONSTANT VARCHAR2(14) := 'GET_NEXT_OFFER';
15:47:49 460  var_offer_chain_id	NUMBER;
15:47:49 461  var_offer_id		NUMBER;
15:47:49 462  var_license_current_index NUMBER;
15:47:49 463  var_next_offer_index	NUMBER;
15:47:49 464  -- EXCEPTIONS
15:47:49 465  BAD_LICENSE_ID	      EXCEPTION;
15:47:49 466  CAN_NOT_FIND_NEXT_OFFER EXCEPTION;
15:47:49 467  EXCEPTION_MESSAGE       VARCHAR2(1024);
15:47:49 468  BEGIN
15:47:49 469  
15:47:49 470  	BEGIN
15:47:49 471  	  SELECT
15:47:49 472  	    SUBSCRIPTION.OFFER_CHAIN_ID,
15:47:49 473  	    LICENSE.OFFER_ID,
15:47:49 474  	    LICENSE.CURRENT_OFFER_INDEX
15:47:49 475  	    into
