15:47:49 1771  
15:47:49 1772  PROCEDURE HAS_FUTURE_LICENSE (
15:47:49 1773  /*
15:47:49 1774  APP_EXCEPTION_CODES_V17.NOT_FOUND
15:47:49 1775  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:49 1776  --
15:47:49 1777  RETURNS:
15:47:49 1778  GLOBAL_CONSTANTS_V17.TRUE - if has,
15:47:49 1779  GLOBAL_CONSTANTS_V17.FALSE - else
15:47:49 1780  */
15:47:49 1781  	 in_license_id IN  NUMBER,
15:47:49 1782  	 out_result    OUT NUMBER
15:47:49 1783  ) AS
15:47:49 1784  -- VARIABLES
15:47:49 1785  SPROC_NAME		 CONSTANT VARCHAR2(18) := 'HAS_FUTURE_LICENSE';
15:47:49 1786  var_subscription_id	 NUMBER;
15:47:49 1787  var_future_licenses_count NUMBER;
15:47:49 1788  -- EXCEPTIONS
15:47:49 1789  BAD_LICENSE_ID	    EXCEPTION;
15:47:49 1790  BEGIN
15:47:49 1791  
15:47:49 1792  	 BEGIN
15:47:49 1793  	   SELECT
15:47:49 1794  	     LICENSE.SUBSCRIPTION_ID into var_subscription_id
15:47:49 1795  	   FROM
15:47:49 1796  	     LICENSE
15:47:49 1797  	   WHERE
15:47:49 1798  	     LICENSE.ID = in_license_id;
15:47:49 1799  	   EXCEPTION
15:47:49 1800  	     WHEN NO_DATA_FOUND THEN
15:47:49 1801  	       RAISE BAD_LICENSE_ID;
15:47:49 1802  	 END;
15:47:49 1803  
15:47:49 1804  	 SELECT
15:47:49 1805  	   COUNT(*) into var_future_licenses_count
15:47:49 1806  	 FROM
15:47:49 1807  	   LICENSE
15:47:49 1808  	 WHERE
15:47:49 1809  	   LICENSE.ID != in_license_id
15:47:49 1810  	   AND LICENSE.SUBSCRIPTION_ID = var_subscription_id
15:47:49 1811  	   AND LICENSE.END_DATE > sysdate;
15:47:49 1812  
15:47:49 1813  	 IF var_future_licenses_count > 0 THEN
15:47:49 1814  	   out_result := GLOBAL_CONSTANTS_V17.TRUE;
15:47:49 1815  	 ELSE
15:47:49 1816  	   out_result := GLOBAL_CONSTANTS_V17.FALSE;
15:47:49 1817  	 END IF;
15:47:49 1818  
15:47:49 1819  EXCEPTION
15:47:49 1820  WHEN BAD_LICENSE_ID THEN
