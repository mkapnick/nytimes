15:47:50  78  	    WHERE PROCESS_NAME = in_process_name
15:47:50  79  	      AND GENERIC_ID = gc.id
15:47:50  80  	  ) AND EXISTS(
15:47:50  81  	    SELECT NULL
15:47:50  82  	    FROM
