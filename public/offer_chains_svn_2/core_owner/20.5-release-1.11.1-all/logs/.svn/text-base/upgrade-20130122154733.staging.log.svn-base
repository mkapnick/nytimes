15:47:48 964  
15:47:48 965  PROCEDURE GET_OFFER_LENGTH_IN_DAYS (
15:47:48 966  	in_offer_id   IN NUMBER,
15:47:48 967  	in_start_date IN DATE DEFAULT SYSDATE,
15:47:48 968  	out_days      OUT NUMBER
15:47:48 969  ) AS
15:47:48 970  SPROC_NAME CONSTANT VARCHAR2(24) := 'GET_OFFER_LENGTH_IN_DAYS';
15:47:48 971  -- VARIABLES
15:47:48 972  var_offer_duration VARCHAR2(30);
15:47:48 973  var_offer_years	 NUMBER;
15:47:48 974  var_offer_months	 NUMBER;
15:47:48 975  var_offer_days	 NUMBER;
15:47:48 976  var_offer_end_date DATE;
15:47:48 977  -- EXCEPTIONS
15:47:48 978  BAD_OFFER_ID EXCEPTION;
15:47:48 979  BEGIN
15:47:48 980  
15:47:48 981  	BEGIN
15:47:48 982  	  SELECT
15:47:48 983  	    OFFER.ENTITLEMENT_DURATION into var_offer_duration
15:47:48 984  	  FROM
15:47:48 985  	    OFFER
15:47:48 986  	  WHERE
15:47:48 987  	    OFFER.ID = in_offer_id;
15:47:48 988  	  EXCEPTION
15:47:48 989  	    WHEN NO_DATA_FOUND THEN
15:47:48 990  	      RAISE BAD_OFFER_ID;
15:47:48 991  	END;
15:47:48 992  
15:47:48 993  	PROCS_COMMON_V17.ISO8601DURATION_TO_INTERVALS (
15:47:48 994  	  var_offer_duration,
15:47:48 995  	  var_offer_years,
15:47:48 996  	  var_offer_months,
15:47:48 997  	  var_offer_days
15:47:48 998  	);
15:47:48 999  
15:47:48 1000  	 var_offer_end_date := ( ( in_start_date
15:47:48 1001  	   + GLOBAL_CONSTANTS_V17.ONE_DAY_INTERVAL * var_offer_days )
15:47:48 1002  	   + GLOBAL_CONSTANTS_V17.ONE_MONTH_INTERVAL * var_offer_months )
15:47:48 1003  	   + GLOBAL_CONSTANTS_V17.ONE_YEAR_INTERVAL * var_offer_years;
15:47:48 1004  
15:47:48 1005  	 out_days := var_offer_end_date - in_start_date;
15:47:48 1006  
15:47:48 1007  EXCEPTION
15:47:48 1008  WHEN BAD_OFFER_ID THEN
15:47:48 1009  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:48 1010  	   SPROC_NAME, 'No such offer');
15:47:48 1011  WHEN OTHERS THEN
15:47:48 1012  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:48 1013  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:47:48 1014  END GET_OFFER_LENGTH_IN_DAYS;
