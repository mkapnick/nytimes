15:47:50 309  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50 310  	  SPROC_NAME, 'Could not create paypal', EXCEPTION_MESSAGE);
15:47:50 311  WHEN OTHERS THEN
15:47:50 312  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:50 313  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:50 314  END ADD_PAYPAL;
