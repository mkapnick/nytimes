15:47:49 1911  
15:47:49 1912  PROCEDURE GET_SUBSCRIPTION_PRODUCTS (
15:47:49 1913  /*
15:47:49 1914  APP_EXCEPTION_CODES_V17.NOT_FOUND
15:47:49 1915  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:49 1916  */
15:47:49 1917  	 in_subscription_id IN NUMBER,
15:47:49 1918  	 out_result_set     OUT SYS_REFCURSOR
15:47:49 1919  ) AS
15:47:49 1920  SPROC_NAME CONSTANT VARCHAR2(25) := 'GET_SUBSCRIPTION_PRODUCTS';
15:47:49 1921  -- VARIABLES
15:47:49 1922  var_offer_chain NUMBER;
15:47:49 1923  -- EXCEPTIONS
15:47:49 1924  BAD_SUBSCRIPTION_ID EXCEPTION;
15:47:49 1925  BEGIN
15:47:49 1926  	 BEGIN
15:47:49 1927  	   SELECT
15:47:49 1928  	     SUBSCRIPTION.OFFER_CHAIN_ID into var_offer_chain
15:47:49 1929  	   FROM
15:47:49 1930  	     SUBSCRIPTION
15:47:49 1931  	   WHERE
15:47:49 1932  	     SUBSCRIPTION.ID = in_subscription_id;
15:47:49 1933  	   EXCEPTION
15:47:49 1934  	     WHEN NO_DATA_FOUND THEN
15:47:49 1935  	       RAISE BAD_SUBSCRIPTION_ID;
15:47:49 1936  	 END;
15:47:49 1937  
15:47:49 1938  	 OPEN out_result_set FOR
15:47:49 1939  	 SELECT DISTINCT
15:47:49 1940  	   PRODUCT.ID,
15:47:49 1941  	   PRODUCT.NAME
15:47:49 1942  	 FROM
15:47:49 1943  	   PRODUCT
15:47:49 1944  	 WHERE
15:47:49 1945  	   PRODUCT.ID IN (
15:47:49 1946  	     SELECT DISTINCT
15:47:49 1947  	       PRODUCT_OFFERING.PRODUCT_ID
15:47:49 1948  	     FROM
