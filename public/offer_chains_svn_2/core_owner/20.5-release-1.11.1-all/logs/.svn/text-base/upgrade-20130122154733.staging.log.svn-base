15:47:49 1399  	     LICENSE.ID,
15:47:49 1400  	     LICENSE.START_DATE,
15:47:49 1401  	     LICENSE.END_DATE,
15:47:49 1402  	     LICENSE.CURRENT_OFFER_INDEX,
