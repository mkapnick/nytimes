15:47:48  49  	END;
15:47:48  50  
15:47:48  51  	-- Check that given offer exists
15:47:48  52  	BEGIN
15:47:48  53  	  SELECT
15:47:48  54  	    OFFER.ID into temp_offer_id
15:47:48  55  	  FROM
15:47:48  56  	    OFFER
15:47:48  57  	  WHERE
15:47:48  58  	    OFFER.ID = in_offer_id;
15:47:48  59  
15:47:48  60  	  EXCEPTION
15:47:48  61  	    WHEN NO_DATA_FOUND THEN
15:47:48  62  	      RAISE BAD_OFFER_ID;
15:47:48  63  	END;
15:47:48  64  
15:47:48  65  	-- Get product_offering data
15:47:48  66  	OPEN var_line_item_data FOR
15:47:48  67  	SELECT
15:47:48  68  	  PRODUCT_OFFERING.ID,
