15:47:50 291  PROCEDURE TEST_DELETE_USER_ACCOUNTS  (
15:47:50 292  	in_start_group_id IN NUMBER,
15:47:50 293  	in_end_group_id   IN NUMBER
15:47:50 294  ) IS
15:47:50 295   gid ACCOUNT.GROUP_ID%TYPE;
15:47:50 296   CURSOR c (v_from NUMBER, v_to NUMBER) IS SELECT ACCOUNT.GROUP_ID FROM ACCOUNT WHERE GROUP_ID BETWEEN v_from AND v_to;
15:47:50 297  BEGIN
15:47:50 298  -- arosolovskiy refactoring: call delete_user_account only "COUNT(group_id) WHERE ...." times instead of "in_end_group_id - in_start_group_id" times;
15:47:50 299  	/*
15:47:50 300  	FOR var_group_id IN in_start_group_id..in_end_group_id
15:47:50 301  	LOOP
15:47:50 302  	  TEST_DELETE_USER_ACCOUNT(var_group_id);
15:47:50 303  	END LOOP;*/
15:47:50 304  	OPEN c(in_start_group_id, in_end_group_id);
15:47:50 305  	WHILE c%ISOPEN LOOP
15:47:50 306  	  FETCH c INTO gid;
15:47:50 307  	  IF c%NOTFOUND THEN
15:47:50 308  	   CLOSE c;
15:47:50 309  	  END IF;
15:47:50 310  	  TEST_DELETE_USER_ACCOUNT(gid);
15:47:50 311  	END LOOP;
15:47:50 312  END;
