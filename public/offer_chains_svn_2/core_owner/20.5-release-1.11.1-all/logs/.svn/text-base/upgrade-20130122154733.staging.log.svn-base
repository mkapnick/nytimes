15:47:48 682  /******************************************************************************/
15:47:48 683  
15:47:48 684  FUNCTION GET_FIRST_OFFER_INDEX (
15:47:48 685  	in_offer_chain_id IN NUMBER
15:47:48 686  ) RETURN NUMBER AS
15:47:48 687  -- VARIABLES
15:47:48 688  SPROC_NAME	    CONSTANT VARCHAR2(21) := 'GET_FIRST_OFFER_INDEX';
15:47:48 689  var_first_offer_index NUMBER;
15:47:48 690  BEGIN
15:47:48 691  
15:47:48 692  	SELECT
15:47:48 693  	  INDEX_VALUE into var_first_offer_index
15:47:48 694  	FROM (
15:47:48 695  	  SELECT
15:47:48 696  	    OFFER_OFFER_CHAIN.INDEX_VALUE
15:47:48 697  	  FROM
15:47:48 698  	    OFFER_OFFER_CHAIN
15:47:48 699  	  WHERE
15:47:48 700  	    OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
15:47:48 701  	  ORDER BY
15:47:48 702  	    OFFER_OFFER_CHAIN.INDEX_VALUE ASC
15:47:48 703  	)
15:47:48 704  	WHERE
15:47:48 705  	  ROWNUM <= 1;
15:47:48 706  
15:47:48 707  	RETURN var_first_offer_index;
15:47:48 708  
15:47:48 709  EXCEPTION
15:47:48 710  WHEN NO_DATA_FOUND THEN
15:47:48 711  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:48 712  	  SPROC_NAME, 'No such offer chain');
15:47:48 713  WHEN OTHERS THEN
15:47:48 714  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:48 715  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:48 716  END GET_FIRST_OFFER_INDEX;
15:47:48 717  
15:47:48 718  /******************************************************************************/
15:47:48 719  
15:47:48 720  FUNCTION GET_NEXT_OFFER_INDEX (
15:47:48 721  /*
15:47:48 722  NULL, if not exists
15:47:48 723  */
15:47:48 724  	in_offer_chain_id	     IN NUMBER,
15:47:48 725  	in_offer_chain_current_index IN NUMBER
15:47:48 726  ) RETURN NUMBER AS
15:47:48 727  -- VARIABLES
15:47:48 728  SPROC_NAME		     CONSTANT VARCHAR2(20) := 'GET_NEXT_OFFER_INDEX';
15:47:48 729  temp_offer_chain_id	     NUMBER;
15:47:48 730  temp_offer_chain_current_index NUMBER;
15:47:48 731  var_result		     NUMBER;
15:47:48 732  -- EXCEPTIONS
15:47:48 733  BAD_OFFER_CHAIN_ID      EXCEPTION;
15:47:48 734  BAD_CURRENT_INDEX_VALUE EXCEPTION;
15:47:48 735  BEGIN
15:47:48 736  
15:47:48 737  	-- Check that offer chain exists
15:47:48 738  	BEGIN
15:47:48 739  	  SELECT
15:47:48 740  	    OFFER_CHAIN.ID into temp_offer_chain_id
15:47:48 741  	  FROM
15:47:48 742  	    OFFER_CHAIN
15:47:48 743  	  WHERE
15:47:48 744  	    OFFER_CHAIN.ID = in_offer_chain_id;
15:47:48 745  	  EXCEPTION
15:47:48 746  	    WHEN NO_DATA_FOUND THEN
15:47:48 747  	      RAISE BAD_OFFER_CHAIN_ID;
15:47:48 748  	END;
15:47:48 749  
15:47:48 750  	-- Check that current offer index exists
15:47:48 751  	BEGIN
15:47:48 752  	  SELECT
15:47:48 753  	    OFFER_OFFER_CHAIN.INDEX_VALUE into temp_offer_chain_current_index
15:47:48 754  	  FROM
15:47:48 755  	    OFFER_OFFER_CHAIN
15:47:48 756  	  WHERE
15:47:48 757  	    OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
15:47:48 758  	    AND OFFER_OFFER_CHAIN.INDEX_VALUE = in_offer_chain_current_index
15:47:48 759  	    -- TODO: delete next line
15:47:48 760  	    AND ROWNUM <= 1;
15:47:48 761  	  EXCEPTION
15:47:48 762  	    WHEN NO_DATA_FOUND THEN
15:47:48 763  	      RAISE BAD_CURRENT_INDEX_VALUE;
15:47:48 764  	END;
15:47:48 765  
15:47:48 766  	-- Get next offer index
15:47:48 767  	BEGIN
15:47:48 768  	  SELECT
15:47:48 769  	    INDEX_VALUE into var_result
15:47:48 770  	  FROM (
15:47:48 771  	    SELECT
15:47:48 772  	      OFFER_OFFER_CHAIN.INDEX_VALUE
15:47:48 773  	    FROM
15:47:48 774  	      OFFER_OFFER_CHAIN
15:47:48 775  	    WHERE
15:47:48 776  	      OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
15:47:48 777  	      AND OFFER_OFFER_CHAIN.INDEX_VALUE > in_offer_chain_current_index
15:47:48 778  	    ORDER BY
15:47:48 779  	      OFFER_OFFER_CHAIN.INDEX_VALUE ASC
15:47:48 780  	  )
15:47:48 781  	  WHERE
15:47:48 782  	    ROWNUM <= 1;
15:47:48 783  	  EXCEPTION
15:47:48 784  	    WHEN NO_DATA_FOUND THEN
15:47:48 785  	      var_result := NULL;
15:47:48 786  	END;
15:47:48 787  
15:47:48 788  	RETURN var_result;
15:47:48 789  
15:47:48 790  EXCEPTION
15:47:48 791  WHEN BAD_OFFER_CHAIN_ID THEN
15:47:48 792  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:48 793  	  SPROC_NAME, 'No such offer chain');
15:47:48 794  WHEN BAD_CURRENT_INDEX_VALUE THEN
15:47:48 795  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:48 796  	  SPROC_NAME, 'Bad current index value');
15:47:48 797  WHEN OTHERS THEN
15:47:48 798  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:48 799  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:48 800  END GET_NEXT_OFFER_INDEX;
15:47:48 801  
15:47:48 802  /******************************************************************************/
15:47:48 803  
15:47:48 804  PROCEDURE P_GET_NEXT_OFFER_INDEX (
15:47:48 805  	in_offer_chain_id	     IN NUMBER,
15:47:48 806  	in_offer_chain_current_index IN NUMBER,
15:47:48 807  	out_next_offer_index	     OUT NUMBER
15:47:48 808  ) AS
15:47:48 809  BEGIN
15:47:48 810  	out_next_offer_index := GET_NEXT_OFFER_INDEX(
15:47:48 811  	  in_offer_chain_id,
15:47:48 812  	  in_offer_chain_current_index
15:47:48 813  	);
15:47:48 814  END P_GET_NEXT_OFFER_INDEX;
15:47:48 815  
15:47:48 816  /******************************************************************************/
15:47:48 817  
15:47:48 818  PROCEDURE GET_NEXT_OFFER_INDEX_BY_LCNS (
15:47:48 819  	in_license_id		     IN NUMBER,
15:47:48 820  	in_offer_chain_current_index IN NUMBER,
15:47:48 821  	out_next_offer_index	     OUT NUMBER
15:47:48 822  ) AS
15:47:48 823  -- VARIABLES
15:47:48 824  SPROC_NAME	 CONSTANT VARCHAR2(28) := 'GET_NEXT_OFFER_INDEX_BY_LCNS';
15:47:48 825  var_offer_chain_id NUMBER;
15:47:48 826  -- EXCEPTIONS
15:47:48 827  BAD_LICENSE_ID		   EXCEPTION;
15:47:48 828  CAN_NOT_GET_NEXT_OFFER_INDEX EXCEPTION;
15:47:48 829  EXCEPTION_MESSAGE 	   VARCHAR2(1024);
15:47:48 830  BEGIN
15:47:48 831  
15:47:48 832  	BEGIN
15:47:48 833  	  SELECT
15:47:48 834  	    SUBSCRIPTION.OFFER_CHAIN_ID into var_offer_chain_id
15:47:48 835  	  FROM
15:47:48 836  	    SUBSCRIPTION
15:47:48 837  	  WHERE
15:47:48 838  	    SUBSCRIPTION.ID = (
15:47:48 839  	      SELECT
15:47:48 840  		LICENSE.SUBSCRIPTION_ID
15:47:48 841  	      FROM
15:47:48 842  		LICENSE
15:47:48 843  	      WHERE
15:47:48 844  		LICENSE.ID = in_license_id
15:47:48 845  	    );
15:47:48 846  	  EXCEPTION
15:47:48 847  	    WHEN NO_DATA_FOUND THEN
15:47:48 848  	      RAISE BAD_LICENSE_ID;
