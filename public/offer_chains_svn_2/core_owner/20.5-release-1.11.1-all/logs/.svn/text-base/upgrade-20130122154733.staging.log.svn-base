15:47:49 618  
15:47:49 619  FUNCTION IS_INVOICE_PAYING_STARTED (
15:47:49 620  /*
15:47:49 621  Throws exceptions:
15:47:49 622  APP_EXCEPTION_CODES_V17.NOT_FOUND
15:47:49 623  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:49 624  */
15:47:49 625  	in_invoice_id IN NUMBER
15:47:49 626  ) RETURN NUMBER AS
15:47:49 627  -- VARIABLES
15:47:49 628  SPROC_NAME		 CONSTANT VARCHAR2(30) := 'IS_INVOICE_PAYING_STARTED';
15:47:49 629  temp_invoice_id		 NUMBER;
15:47:49 630  var_processed_charges_num  NUMBER;
15:47:49 631  var_processed_transac_num  NUMBER;
15:47:49 632  var_success_attempts_num	 NUMBER;
15:47:49 633  var_is_gc 		 NUMBER;
15:47:49 634  -- EXCEPTIONS
15:47:49 635  BAD_INVOICE_ID EXCEPTION;
15:47:49 636  BEGIN
15:47:49 637  
15:47:49 638  	-- Check that invoice exists
15:47:49 639  	BEGIN
15:47:49 640  	  SELECT
15:47:49 641  	    INVOICE.ID into temp_invoice_id
15:47:49 642  	  FROM
15:47:49 643  	    INVOICE
15:47:49 644  	  WHERE
15:47:49 645  	    INVOICE.ID = in_invoice_id;
15:47:49 646  	  EXCEPTION
15:47:49 647  	   WHEN NO_DATA_FOUND THEN
15:47:49 648  	     RAISE BAD_INVOICE_ID;
15:47:49 649  	END;
15:47:49 650  
15:47:49 651  	-- Check that there are tansaction attempts with status success
15:47:49 652  	SELECT
15:47:49 653  	  COUNT(1) into var_success_attempts_num
15:47:49 654  	FROM
15:47:49 655  	  TRANSACTION_ATTEMPT ta,
15:47:49 656  	  TRANSACTION t,
15:47:49 657  	  CHARGE c
15:47:49 658  	WHERE
15:47:49 659  	  ta.TRANSACTION_ATTEMPT_STATUS_ID = GLOBAL_STATUSES_V17.TRANS_ATTEMPT_SUCCESS and
15:47:49 660  	  ta.transaction_id = t.id and
15:47:49 661  	  c.transaction_id = t.id and
15:47:49 662  	  t.is_refund = GLOBAL_CONSTANTS_V17.FALSE and
15:47:49 663  	  c.invoice_id = in_invoice_id
15:47:49 664  	;
15:47:49 665  
15:47:49 666  	IF var_success_attempts_num > 0 THEN
15:47:49 667  	  RETURN GLOBAL_CONSTANTS_V17.TRUE;
15:47:49 668  	END IF;
15:47:49 669  
15:47:49 670  	SELECT
15:47:49 671  	  COUNT(1) into var_success_attempts_num
15:47:49 672  	FROM
15:47:49 673  	  TRANSACTION t,
15:47:49 674  	  CHARGE c
15:47:49 675  	WHERE
15:47:49 676  	  c.transaction_id = t.id and
15:47:49 677  	  t.is_refund = GLOBAL_CONSTANTS_V17.FALSE and
15:47:49 678  	  t.is_settled = GLOBAL_CONSTANTS_V17.TRUE and
15:47:49 679  	  c.invoice_id = in_invoice_id
15:47:49 680  	;
15:47:49 681  
15:47:49 682  	IF var_success_attempts_num > 0 THEN
15:47:49 683  	  RETURN GLOBAL_CONSTANTS_V17.TRUE;
15:47:49 684  	END IF;
15:47:49 685  
15:47:49 686  	SELECT
15:47:49 687  	  COUNT(1) into var_is_gc
15:47:49 688  	FROM
15:47:49 689  	  gift_certificate gc
15:47:49 690  	WHERE
15:47:49 691  	  gc.finalized_invoice_id = in_invoice_id
15:47:49 692  	;
15:47:49 693  
15:47:49 694  	IF var_is_gc > 0 THEN
15:47:49 695  	  RETURN GLOBAL_CONSTANTS_V17.TRUE;
15:47:49 696  	END IF;
15:47:49 697  
15:47:49 698  	RETURN GLOBAL_CONSTANTS_V17.FALSE;
15:47:49 699  
15:47:49 700  EXCEPTION
15:47:49 701  WHEN BAD_INVOICE_ID THEN
15:47:49 702  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 703  	  SPROC_NAME, 'No such invoice');
15:47:49 704  WHEN OTHERS THEN
15:47:49 705  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 706  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:49 707  END IS_INVOICE_PAYING_STARTED;
