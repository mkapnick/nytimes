15:47:48  73  	  INNER JOIN PRODUCT_OFFERING ON OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = PRODUCT_OFFERING.ID
15:47:48  74  	WHERE
15:47:48  75  	  OFFER_PRODUCT_OFFERING.OFFER_ID = in_offer_id;
15:47:48  76  
15:47:48  77  	-- insert line items
15:47:48  78  	-- add discounts to line items
15:47:48  79  	LOOP
15:47:48  80  	  FETCH var_line_item_data INTO
15:47:48  81  	    var_product_offering_id,
15:47:48  82  	    var_product_unit_price,
15:47:48  83  	    var_product_quantity;
15:47:48  84  	  EXIT WHEN var_line_item_data%NOTFOUND;
15:47:48  85  
15:47:48  86  	  var_product_offering_oprice := var_product_unit_price * var_product_quantity;
15:47:48  87  	  var_product_offering_price := var_product_offering_oprice;
15:47:48  88  
15:47:48  89  	  -- Apply discounts to line_item
15:47:48  90  	  BEGIN
15:47:48  91  	    SELECT
15:47:48  92  	      SUM (DISCOUNT.FIXED_AMOUNT) into var_discount_fixed_amount
15:47:48  93  	    FROM
15:47:48  94  	      DISCOUNT_PRODUCT_OFFERING
15:47:48  95  	      INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
15:47:48  96  	    WHERE
15:47:48  97  	      DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
15:47:48  98  	      AND DISCOUNT.FIXED_AMOUNT IS NOT NULL;
15:47:48  99  	    EXCEPTION
15:47:48 100  	    WHEN NO_DATA_FOUND THEN
15:47:48 101  	      var_discount_fixed_amount := NULL;
15:47:48 102  	  END;
15:47:48 103  
15:47:48 104  	  BEGIN
15:47:48 105  	    SELECT
15:47:48 106  	      SUM (DISCOUNT.PERCENT_AMOUNT) into var_discount_percent_amount
15:47:48 107  	    FROM
15:47:48 108  	      DISCOUNT_PRODUCT_OFFERING
15:47:48 109  	      INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
15:47:48 110  	    WHERE
15:47:48 111  	      DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
15:47:48 112  	      AND DISCOUNT.PERCENT_AMOUNT IS NOT NULL;
15:47:48 113  	    EXCEPTION
15:47:48 114  	    WHEN NO_DATA_FOUND THEN
15:47:48 115  	      var_discount_percent_amount := NULL;
15:47:48 116  	  END;
15:47:48 117  
15:47:48 118  	  IF (var_discount_percent_amount IS NOT NULL) THEN
15:47:48 119  	    var_product_offering_price := var_product_offering_price * ( 1 - var_discount_percent_amount / 100 );
15:47:48 120  	  END IF;
15:47:48 121  
15:47:48 122  	  IF (var_discount_fixed_amount IS NOT NULL) THEN
15:47:48 123  	    var_product_offering_price := var_product_offering_price - var_discount_fixed_amount;
15:47:48 124  	  END IF;
15:47:48 125  
15:47:48 126  	  var_line_item_price := PROCS_COMMON_V17.ROUND_10_6_TO_10_2(var_product_offering_price);
15:47:48 127  
15:47:48 128  	  IF (var_line_item_price < 0) THEN
15:47:48 129  	      RAISE BAD_DISCOUNT;
15:47:48 130  	  END IF;
15:47:48 131  
15:47:48 132  	  var_new_line_item_id := NULL;
15:47:48 133  	  PROCS_LINE_ITEMS_CRU_V17.CREATE_LINE_ITEM(
