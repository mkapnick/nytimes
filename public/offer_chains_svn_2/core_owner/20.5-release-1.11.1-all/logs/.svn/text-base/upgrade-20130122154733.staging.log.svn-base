15:47:50 702  /******************************************************************************/
15:47:50 703  
15:47:50 704  PROCEDURE START_GC_PURCHASING (
15:47:50 705  	in_group_id		  IN NUMBER,
15:47:50 706  	in_offer_chain_id	  IN VARCHAR2,
15:47:50 707  	in_gift_certificate_code  IN  VARCHAR2,
15:47:50 708  	in_created_by		  IN  VARCHAR2,
15:47:50 709  	in_recipient_name	  IN  VARCHAR2,
15:47:50 710  	in_recipient_email	  IN  VARCHAR2,
15:47:50 711  	in_recipient_address_id   IN NUMBER,
15:47:50 712  	in_recipient_notify_date  IN DATE,
15:47:50 713  	in_sender_name		  IN VARCHAR2,
15:47:50 714  	in_sender_email 	  IN VARCHAR2,
15:47:50 715  	in_gift_message 	  IN  VARCHAR2,
15:47:50 716  	in_expiration_date	  IN DATE,
15:47:50 717  	out_gift_certificate_id   OUT NUMBER,
15:47:50 718  	out_invoice_id		  OUT NUMBER
15:47:50 719  ) AS
15:47:50 720  SPROC_NAME CONSTANT VARCHAR2(19) := 'START_GC_PURCHASING';
15:47:50 721  -- VARIABLES
15:47:50 722  var_account_id		NUMBER;
15:47:50 723  temp_gc_code		GIFT_CERTIFICATE.CODE%TYPE;
15:47:50 724  var_och_is_gc		NUMBER;
15:47:50 725  var_offer_chain_status_id NUMBER;
15:47:50 726  var_is_for_redemption	NUMBER;
15:47:50 727  var_new_invoice_id	NUMBER;
15:47:50 728  var_gift_cert_id		NUMBER;
15:47:50 729  var_account_tax_exempt_id VARCHAR2(255);
15:47:50 730  -- EXCEPTIONS
15:47:50 731  BAD_GROUP_ID		    EXCEPTION;
15:47:50 732  GC_CODE_ALREADY_EXISTS	    EXCEPTION;
15:47:50 733  BAD_OFFER_CHAIN_ID	    EXCEPTION;
15:47:50 734  OCH_IS_NOT_GIFT_CERTIFICATE   EXCEPTION;
15:47:50 735  BAD_OFFER_CHAIN_STATUS	    EXCEPTION;
15:47:50 736  CAN_NOT_PURCHASE_GC_FOR_RDMPN EXCEPTION;
15:47:50 737  CAN_NOT_CREATE_INVOICE	    EXCEPTION;
15:47:50 738  OFFER_REC_NUM_LESS_THAN_ONE   EXCEPTION;
15:47:50 739  CAN_NOT_CREATE_LINE_ITEMS     EXCEPTION;
15:47:50 740  
15:47:50 741  EXCEPTION_MESSAGE VARCHAR2(1024);
15:47:50 742  BEGIN
15:47:50 743  	-- Get account id
15:47:50 744  	BEGIN
15:47:50 745  	  SELECT
15:47:50 746  	    ACCOUNT.ID,
15:47:50 747  	    ACCOUNT.TAX_EXEMPT_ID
15:47:50 748  	    into
15:47:50 749  	    var_account_id,
15:47:50 750  	    var_account_tax_exempt_id
15:47:50 751  	  FROM
15:47:50 752  	    ACCOUNT
15:47:50 753  	  WHERE
15:47:50 754  	    ACCOUNT.GROUP_ID = in_group_id;
15:47:50 755  	  EXCEPTION
15:47:50 756  	  WHEN NO_DATA_FOUND THEN
15:47:50 757  	    RAISE BAD_GROUP_ID;
15:47:50 758  	END;
15:47:50 759  
15:47:50 760  	-- Check for the same code
15:47:50 761  	BEGIN
15:47:50 762  	  SELECT
15:47:50 763  	    GIFT_CERTIFICATE.CODE into temp_gc_code
15:47:50 764  	  FROM
15:47:50 765  	    GIFT_CERTIFICATE
15:47:50 766  	  WHERE
15:47:50 767  	    GIFT_CERTIFICATE.CODE = in_gift_certificate_code;
15:47:50 768  
15:47:50 769  	  RAISE GC_CODE_ALREADY_EXISTS;
15:47:50 770  
15:47:50 771  	  EXCEPTION
15:47:50 772  	    WHEN NO_DATA_FOUND THEN
15:47:50 773  	      NULL;
15:47:50 774  	END;
15:47:50 775  
15:47:50 776  	-- Get offer chain flag "is_gift_certificate"
15:47:50 777  	BEGIN
15:47:50 778  	  SELECT
15:47:50 779  	    OFFER_CHAIN.IS_GIFT_CERTIFICATE,
15:47:50 780  	    OFFER_CHAIN.OFFER_CHAIN_STATUS_ID
15:47:50 781  	    into
15:47:50 782  	    var_och_is_gc,
15:47:50 783  	    var_offer_chain_status_id
15:47:50 784  	  FROM
15:47:50 785  	    OFFER_CHAIN
15:47:50 786  	  WHERE
15:47:50 787  	    OFFER_CHAIN.ID = in_offer_chain_id;
15:47:50 788  	  EXCEPTION
15:47:50 789  	  WHEN NO_DATA_FOUND THEN
15:47:50 790  	    RAISE BAD_OFFER_CHAIN_ID;
15:47:50 791  	END;
15:47:50 792  
15:47:50 793  	IF var_och_is_gc != GLOBAL_CONSTANTS_V17.TRUE
15:47:50 794  	  OR var_och_is_gc IS NULL THEN
15:47:50 795  	  RAISE OCH_IS_NOT_GIFT_CERTIFICATE;
15:47:50 796  	END IF;
15:47:50 797  
15:47:50 798  	IF var_offer_chain_status_id != GLOBAL_STATUSES_V17.OFFER_CHAIN_ACTIVE THEN
15:47:50 799  	  RAISE BAD_OFFER_CHAIN_STATUS;
15:47:50 800  	END IF;
15:47:50 801  
15:47:50 802  	-- norlov: #38151 check if the OC is for Redemption:
15:47:50 803  	SELECT
15:47:50 804  	  COUNT(*) into var_is_for_redemption
15:47:50 805  	FROM
15:47:50 806  	  OFFER_CHAIN_ELIGIBILITY
15:47:50 807  	WHERE
15:47:50 808  	  OFFER_CHAIN_ELIGIBILITY.OFFER_CHAIN_ID = in_offer_chain_id
15:47:50 809  	  AND OFFER_CHAIN_ELIGIBILITY.NAME = GLOBAL_CONSTANTS_V17.GIFT_CERTIFICATE_REQUIRED
15:47:50 810  	  AND OFFER_CHAIN_ELIGIBILITY.VALUE = GLOBAL_CONSTANTS_V17.ELIGIBILITY_FLAG_SET;
15:47:50 811  
15:47:50 812  	IF var_is_for_redemption > 0 THEN
15:47:50 813  	  RAISE CAN_NOT_PURCHASE_GC_FOR_RDMPN;
15:47:50 814  	END IF;
15:47:50 815  
15:47:50 816  	-- Create new invoice
15:47:50 817  	BEGIN
15:47:50 818  	  PROCS_INVOICE_V17.CREATE_INVOICE(
15:47:50 819  	    in_invoice_status => GLOBAL_STATUSES_V17.INVOICE_OPEN,
15:47:50 820  	    in_created_by     => in_created_by,
15:47:50 821  	    in_tax_exempt_id  => var_account_tax_exempt_id,
15:47:50 822  	    out_invoice_id    => var_new_invoice_id
15:47:50 823  	  );
15:47:50 824  	  EXCEPTION
15:47:50 825  	    WHEN OTHERS THEN
15:47:50 826  	      EXCEPTION_MESSAGE := SQLERRM;
15:47:50 827  	      RAISE CAN_NOT_CREATE_INVOICE;
15:47:50 828  	END;
15:47:50 829  
15:47:50 830  	-- Add line items for new invoice
15:47:50 831  	BEGIN
15:47:50 832  	  FOR f_offer_data IN (
15:47:50 833  	    SELECT
15:47:50 834  	      OFFER_ID,
15:47:50 835  	      NUM_RECURRENCES
15:47:50 836  	    FROM
15:47:50 837  	      OFFER_OFFER_CHAIN
15:47:50 838  	    WHERE
15:47:50 839  	      OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
15:47:50 840  	  )
15:47:50 841  	  LOOP
15:47:50 842  	    IF f_offer_data.NUM_RECURRENCES < 1 THEN
15:47:50 843  	      RAISE OFFER_REC_NUM_LESS_THAN_ONE;
15:47:50 844  	    END IF;
15:47:50 845  	    FOR i_offer_recurrences_iterator IN 1..f_offer_data.NUM_RECURRENCES
15:47:50 846  	    LOOP
15:47:50 847  	      PROCS_LINE_ITEMS_V17.ADD_LINE_ITEMS(
15:47:50 848  		in_invoice_id => var_new_invoice_id,
15:47:50 849  		in_offer_id   => f_offer_data.OFFER_ID,
15:47:50 850  		in_created_by => in_created_by
15:47:50 851  	      );
15:47:50 852  	    END LOOP;
15:47:50 853  	  END LOOP;
15:47:50 854  
15:47:50 855  	  EXCEPTION
15:47:50 856  	    WHEN OTHERS THEN
15:47:50 857  	      EXCEPTION_MESSAGE := SQLERRM;
15:47:50 858  	      RAISE CAN_NOT_CREATE_LINE_ITEMS;
15:47:50 859  	END;
15:47:50 860  
15:47:50 861  	PROCS_FIN_INSTRUMENTS_CRU_V17.CREATE_GIFT_CERTIFICATE(
15:47:50 862  	  out_gift_certificate_id	=> var_gift_cert_id,
15:47:50 863  	  in_purchaser_group_id 	=> in_group_id,
15:47:50 864  	  in_purchaser_invoice_id	=> var_new_invoice_id,
15:47:50 865  	  in_offer_chain_id		=> in_offer_chain_id,
15:47:50 866  	  in_expiration_date		=> in_expiration_date,
15:47:50 867  	  in_purchase_date		=> SYSDATE,
15:47:50 868  	  in_gift_certificate_status_id => GLOBAL_STATUSES_V17.GIFT_CERTIFICATE_ACTIVE,
15:47:50 869  	  in_code			=> in_gift_certificate_code,
15:47:50 870  	  in_created_by 		=> in_created_by,
15:47:50 871  	  in_recipient_name		=> in_recipient_name,
15:47:50 872  	  in_gift_message		=> in_gift_message,
15:47:50 873  	  in_recipient_email		=> in_recipient_email,
15:47:50 874  	  in_sender_email		=> in_sender_email,
15:47:50 875  	  in_sender_name		=> in_sender_name,
15:47:50 876  	  in_recipient_address_id	=> in_recipient_address_id,
15:47:50 877  	  in_recipient_notify_date	=> in_recipient_notify_date
15:47:50 878  	);
15:47:50 879  
15:47:50 880  	out_gift_certificate_id := var_gift_cert_id;
15:47:50 881  	out_invoice_id := var_new_invoice_id;
15:47:50 882  
15:47:50 883  EXCEPTION
15:47:50 884  WHEN BAD_OFFER_CHAIN_STATUS THEN
15:47:50 885  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.STATE_ERROR,
15:47:50 886  	  SPROC_NAME, 'Offer chain is not active');
15:47:50 887  WHEN GC_CODE_ALREADY_EXISTS THEN
15:47:50 888  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.DUPLICATE_ERROR,
15:47:50 889  	  SPROC_NAME, 'Gift certificate with same code already exists');
15:47:50 890  WHEN OCH_IS_NOT_GIFT_CERTIFICATE THEN
15:47:50 891  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INVALID_PARAMETER,
15:47:50 892  	  SPROC_NAME, 'This offer chain can not be used for gift certificate');
15:47:50 893  WHEN CAN_NOT_PURCHASE_GC_FOR_RDMPN THEN
15:47:50 894  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INVALID_PARAMETER,
15:47:50 895  	  SPROC_NAME, 'This offer chain can not be purchased for gift certificate since it is for redemption');
15:47:50 896  WHEN CAN_NOT_CREATE_INVOICE THEN
15:47:50 897  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50 898  	  SPROC_NAME, 'Could not create new invoice', EXCEPTION_MESSAGE);
15:47:50 899  WHEN CAN_NOT_CREATE_LINE_ITEMS THEN
15:47:50 900  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50 901  	  SPROC_NAME, 'Could not create line items', EXCEPTION_MESSAGE);
15:47:50 902  WHEN BAD_GROUP_ID THEN
15:47:50 903  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 904  	  SPROC_NAME, 'No such group id');
15:47:50 905  WHEN BAD_OFFER_CHAIN_ID THEN
15:47:50 906  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 907  	  SPROC_NAME, 'No such offer chain');
15:47:50 908  WHEN OFFER_REC_NUM_LESS_THAN_ONE THEN
15:47:50 909  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50 910  	  SPROC_NAME, 'Some offer has recurrences number less than 1');
15:47:50 911  WHEN OTHERS THEN
15:47:50 912  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:50 913  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:50 914  END START_GC_PURCHASING;
15:47:50 915  
15:47:50 916  /******************************************************************************/
15:47:50 917  
15:47:50 918  PROCEDURE FINALIZE_GC_PURCHASING (
15:47:50 919  	in_invoice_id	      IN NUMBER,
15:47:50 920  	in_created_by	      IN VARCHAR2,
15:47:50 921  	in_instrument_id      IN NUMBER,
15:47:50 922  	in_instrument_type_id IN NUMBER,
15:47:50 923  	in_order_id	      IN VARCHAR2,
15:47:50 924  	in_transaction_id     IN NUMBER,
15:47:50 925  	out_charge_amount     OUT NUMBER
15:47:50 926  ) AS
15:47:50 927  SPROC_NAME CONSTANT VARCHAR2(22) := 'FINALIZE_GC_PURCHASING';
15:47:50 928  -- VARIABLES
15:47:50 929  temp_transaction_id_count NUMBER;
15:47:50 930  var_invoice_amount	NUMBER(10,2);
15:47:50 931  var_transaction_id	NUMBER;
15:47:50 932  var_new_charge_id 	NUMBER;
15:47:50 933  -- EXCEPTIONS
15:47:50 934  BAD_CREDIT_CARD_ID	  EXCEPTION;
15:47:50 935  BAD_PAYPAL_ID		  EXCEPTION;
15:47:50 936  BAD_INSTRUMENT_TYPE	  EXCEPTION;
15:47:50 937  TRANSACTION_EXISTS	  EXCEPTION;
15:47:50 938  CAN_NOT_CALC_INVOICE_AMOUNT EXCEPTION;
15:47:50 939  CAN_NOT_USE_FCINSTR	  EXCEPTION;
15:47:50 940  CAN_NOT_CREATE_TRANSACTION  EXCEPTION;
15:47:50 941  CAN_NOT_CREATE_CHARGE	  EXCEPTION;
15:47:50 942  EXCEPTION_MESSAGE   VARCHAR2(1024);
15:47:50 943  BEGIN
15:47:50 944  
15:47:50 945  	-- Check that instrument exists
15:47:50 946  	IF in_instrument_type_id = GLOBAL_ENUMS_V17.INSTRUMENT_CREDIT_CARD THEN
15:47:50 947  	  IF IS_CREDIT_CARD_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V17.FALSE THEN
15:47:50 948  	    RAISE BAD_CREDIT_CARD_ID;
15:47:50 949  	  END IF;
15:47:50 950  	ELSIF in_instrument_type_id = GLOBAL_ENUMS_V17.INSTRUMENT_PAYPAL THEN
15:47:50 951  	  IF IS_PAYPAL_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V17.FALSE THEN
15:47:50 952  	    RAISE BAD_PAYPAL_ID;
15:47:50 953  	  END IF;
15:47:50 954  	ELSIF in_instrument_type_id = GLOBAL_ENUMS_V17.INSTRUMENT_ZCI_INSTRUMENT THEN
15:47:50 955  	  NULL;
15:47:50 956  	ELSE
15:47:50 957  	  RAISE BAD_INSTRUMENT_TYPE;
15:47:50 958  	END IF;
15:47:50 959  
15:47:50 960  	-- Check that transaction with given id do not exists
15:47:50 961  	SELECT
15:47:50 962  	  COUNT(*) into temp_transaction_id_count
15:47:50 963  	FROM
15:47:50 964  	  TRANSACTION
15:47:50 965  	WHERE
15:47:50 966  	  TRANSACTION.ID = in_transaction_id;
15:47:50 967  
15:47:50 968  	IF temp_transaction_id_count > 0 THEN
15:47:50 969  	  RAISE TRANSACTION_EXISTS;
15:47:50 970  	END IF;
15:47:50 971  
15:47:50 972  	-- Calculate new invoice amount
15:47:50 973  	BEGIN
15:47:50 974  	  PROCS_INVOICE_V17.CALCULATE_INVOICE_AMOUNT(in_invoice_id, var_invoice_amount);
15:47:50 975  	  EXCEPTION
15:47:50 976  	    WHEN OTHERS THEN
15:47:50 977  	      EXCEPTION_MESSAGE := SQLERRM;
15:47:50 978  	      RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
15:47:50 979  	END;
15:47:50 980  
15:47:50 981  	IF var_invoice_amount > 0
15:47:50 982  	  AND in_instrument_type_id = GLOBAL_ENUMS_V17.INSTRUMENT_ZCI_INSTRUMENT THEN
15:47:50 983  	  RAISE CAN_NOT_USE_FCINSTR;
15:47:50 984  	END IF;
15:47:50 985  
15:47:50 986  	-- If invoice amount iz 0 then we need to set status for this invoice to PROCCESSED
15:47:50 987  	IF var_invoice_amount = 0 THEN
15:47:50 988  	  PROCS_INVOICE_CRU_V17.UPDATE_INVOICE(
15:47:50 989  	    in_invoice_id		   => in_invoice_id,
15:47:50 990  	    in_updated_by		   => in_created_by,
15:47:50 991  	    in_invoice_status_id	   => GLOBAL_STATUSES_V17.INVOICE_CLOSED
15:47:50 992  	  );
15:47:50 993  	END IF;
15:47:50 994  
15:47:50 995  	IF var_invoice_amount > 0 THEN
15:47:50 996  	  -- Create transaction
15:47:50 997  	  BEGIN
15:47:50 998  	    PROCS_TRANSACTION_V17.CREATE_TRANSACTION(
15:47:50 999  	      in_transaction_id  => in_transaction_id,
15:47:50 1000  	       in_status_id	  => GLOBAL_STATUSES_V17.TRANSACTION_PENDING,
15:47:50 1001  	       in_amount	  => var_invoice_amount,
15:47:50 1002  	       in_created_by	  => in_created_by,
15:47:50 1003  	       in_order_id	  => in_order_id,
15:47:50 1004  	       out_transaction_id => var_transaction_id
15:47:50 1005  	     );
15:47:50 1006  	     EXCEPTION
15:47:50 1007  	       WHEN OTHERS THEN
15:47:50 1008  		 EXCEPTION_MESSAGE := SQLERRM;
15:47:50 1009  		 RAISE CAN_NOT_CREATE_TRANSACTION;
15:47:50 1010  	   END;
15:47:50 1011  
15:47:50 1012  	   -- Create charge
15:47:50 1013  	   BEGIN
15:47:50 1014  	     PROCS_CHARGE_V17.CREATE_CHARGE(
15:47:50 1015  	       in_invoice_id	     => in_invoice_id,
15:47:50 1016  	       in_transaction_id     => var_transaction_id,
15:47:50 1017  	       in_instrument_type_id => in_instrument_type_id,
15:47:50 1018  	       in_instrument_id      => in_instrument_id,
15:47:50 1019  	       in_charge_amount      => var_invoice_amount,
15:47:50 1020  	       in_created_by	     => in_created_by,
15:47:50 1021  	       in_charge_status_id   => GLOBAL_STATUSES_V17.CHARGE_OPENED,
15:47:50 1022  	       out_charge_id	     => var_new_charge_id
15:47:50 1023  	     );
15:47:50 1024  	     out_charge_amount := var_invoice_amount;
15:47:50 1025  	     EXCEPTION
15:47:50 1026  	       WHEN OTHERS THEN
15:47:50 1027  		 EXCEPTION_MESSAGE := SQLERRM;
15:47:50 1028  		 RAISE CAN_NOT_CREATE_CHARGE;
15:47:50 1029  	   END;
15:47:50 1030  	 ELSE
15:47:50 1031  	   out_charge_amount := 0;
15:47:50 1032  	 END IF;
15:47:50 1033  
15:47:50 1034  EXCEPTION
15:47:50 1035  WHEN CAN_NOT_USE_FCINSTR THEN
15:47:50 1036  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.STATE_ERROR,
15:47:50 1037  	   SPROC_NAME, 'Could not use "free charge instrument" for non-zero invoice');
15:47:50 1038  WHEN BAD_CREDIT_CARD_ID THEN
15:47:50 1039  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 1040  	   SPROC_NAME, 'Bad credit card id');
15:47:50 1041  WHEN BAD_PAYPAL_ID THEN
15:47:50 1042  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 1043  	   SPROC_NAME, 'Bad paypal id');
15:47:50 1044  WHEN BAD_INSTRUMENT_TYPE THEN
15:47:50 1045  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INVALID_PARAMETER,
15:47:50 1046  	   SPROC_NAME, 'Bad instrument type');
15:47:50 1047  WHEN TRANSACTION_EXISTS THEN
15:47:50 1048  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.DUPLICATE_ERROR,
15:47:50 1049  	   SPROC_NAME, 'Transaction with given id already exists');
15:47:50 1050  WHEN CAN_NOT_CREATE_TRANSACTION THEN
15:47:50 1051  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50 1052  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
15:47:50 1053  WHEN CAN_NOT_CREATE_CHARGE THEN
