15:47:49 1879  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.STATE_ERROR,
15:47:49 1880  	   SPROC_NAME, 'Active licenses found');
15:47:49 1881  WHEN OTHERS THEN
15:47:49 1882  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 1883  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:47:49 1884  END CLOSE_SUBSCRIPTION;
