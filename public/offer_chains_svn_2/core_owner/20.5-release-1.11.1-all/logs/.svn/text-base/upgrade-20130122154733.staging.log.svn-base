15:47:49 2385  --  in_cancelation_date	 IN SUBSCRIPTION.CANCELLATION_DATE%TYPE,
15:47:49 2386  --  in_note		 IN SUBSCRIPTION_NOTE.NOTE%TYPE,
15:47:49 2387  --  in_agent_id		 IN SUBSCRIPTION_NOTE.AGENT_ID%TYPE,
15:47:49 2388  --  in_updated_by	 IN SUBSCRIPTION.UPDATED_BY%TYPE
15:47:49 2389  	 in_subscription_id    IN NUMBER,
15:47:49 2390  	 in_cancelation_date   IN DATE,
15:47:49 2391  	 in_note	       IN VARCHAR2,
15:47:49 2392  	 in_agent_id	       IN NUMBER,
15:47:49 2393  	 in_updated_by	       IN VARCHAR2
