15:47:49 2746  PROCEDURE ARE_REFUNDS_PENDING_FOR_SUBSCR (
15:47:49 2747  /*
15:47:49 2748  APP_EXCEPTION_CODES_V17.NOT_FOUND
15:47:49 2749  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:49 2750  */
15:47:49 2751  	 in_subscription_id IN NUMBER,
15:47:49 2752  	 out_result	    OUT NUMBER
15:47:49 2753  ) AS
15:47:49 2754  SPROC_NAME CONSTANT VARCHAR2(30) := 'ARE_REFUNDS_PENDING_FOR_SUBSCR';
15:47:49 2755  -- VARIABLES
15:47:49 2756  temp_subscription_id NUMBER;
15:47:49 2757  var_local_result     NUMBER;
15:47:49 2758  -- EXCEPTIONS
15:47:49 2759  BAD_SUBSCRIPTION_ID EXCEPTION;
15:47:49 2760  BEGIN
15:47:49 2761  
15:47:49 2762  	 -- Check that subscription exists
15:47:49 2763  	 BEGIN
15:47:49 2764  	   SELECT
15:47:49 2765  	     SUBSCRIPTION.ID into temp_subscription_id
15:47:49 2766  	   FROM
15:47:49 2767  	     SUBSCRIPTION
15:47:49 2768  	   WHERE
15:47:49 2769  	     SUBSCRIPTION.ID = in_subscription_id;
15:47:49 2770  	   EXCEPTION
15:47:49 2771  	     WHEN NO_DATA_FOUND THEN
15:47:49 2772  	       RAISE BAD_SUBSCRIPTION_ID;
15:47:49 2773  	 END;
15:47:49 2774  
15:47:49 2775  	 var_local_result := NULL;
15:47:49 2776  
15:47:49 2777  	 -- Check charges for each invoice associated with gived subscription
15:47:49 2778  	 FOR f_invoice IN (
15:47:49 2779  	   SELECT DISTINCT
15:47:49 2780  	     LICENSE.INVOICE_ID as "ID"
15:47:49 2781  	   FROM
15:47:49 2782  	     LICENSE
15:47:49 2783  	   WHERE
15:47:49 2784  	     LICENSE.SUBSCRIPTION_ID = in_subscription_id
15:47:49 2785  	     AND LICENSE.LICENSE_STATUS_ID IN ( SELECT GLOBAL_STATUSES_V17.LICENSE_ACTIVE FROM DUAL )
15:47:49 2786  	 )
15:47:49 2787  	 LOOP
15:47:49 2788  
15:47:49 2789  	   -- Check each charge in invoice
15:47:49 2790  	   FOR f_charge IN (
15:47:49 2791  	     SELECT
15:47:49 2792  	       CHARGE.ID,
15:47:49 2793  	       CHARGE.CHARGE_STATUS_ID,
15:47:49 2794  	       CHARGE.CHARGE_AMOUNT
15:47:49 2795  	     FROM
15:47:49 2796  	       CHARGE
15:47:49 2797  	     WHERE
15:47:49 2798  	       CHARGE.INVOICE_ID = f_invoice.ID
15:47:49 2799  	   )
15:47:49 2800  	   LOOP
15:47:49 2801  
15:47:49 2802  	     -- Charge amount < 0     => it is a refund
15:47:49 2803  	     -- Charge status is OPEN => means that it is not processed yet
15:47:49 2804  	     IF f_charge.CHARGE_AMOUNT < 0
15:47:49 2805  		AND f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V17.CHARGE_OPENED THEN
15:47:49 2806  	       var_local_result := GLOBAL_CONSTANTS_V17.TRUE;
15:47:49 2807  	     END IF;
15:47:49 2808  
15:47:49 2809  	   END LOOP;
15:47:49 2810  
15:47:49 2811  	 END LOOP;
15:47:49 2812  
15:47:49 2813  	 IF var_local_result IS NULL THEN
15:47:49 2814  	   out_result := GLOBAL_CONSTANTS_V17.FALSE;
15:47:49 2815  	 ELSE
15:47:49 2816  	   out_result := GLOBAL_CONSTANTS_V17.TRUE;
15:47:49 2817  	 END IF;
15:47:49 2818  
15:47:49 2819  EXCEPTION
15:47:49 2820  WHEN BAD_SUBSCRIPTION_ID THEN
15:47:49 2821  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 2822  	   SPROC_NAME, 'No such transaction id');
15:47:49 2823  WHEN OTHERS THEN
15:47:49 2824  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 2825  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:47:49 2826  END ARE_REFUNDS_PENDING_FOR_SUBSCR;
