15:47:49 187  -- VARIABLES
15:47:49 188  SPROC_NAME	     CONSTANT VARCHAR2(24) := 'CALCULATE_INVOICE_AMOUNT';
15:47:49 189  temp_invoice_id	     NUMBER;
15:47:49 190  var_total_amount	     NUMBER(10,6);
15:47:49 191  var_final_amount	     NUMBER(10,2);
15:47:49 192  var_line_item_amount   NUMBER(10,6);
15:47:49 193  
15:47:49 194  var_line_items_set     SYS_REFCURSOR;
15:47:49 195  var_line_item_id	     NUMBER;
15:47:49 196  var_line_item_quantity NUMBER;
15:47:49 197  var_line_item_price    NUMBER (10,2);
