15:47:49 1096  
15:47:49 1097  PROCEDURE REFUND_SUBSCRIPTION (
15:47:49 1098  /*
15:47:49 1099  Throws exceptions:
15:47:49 1100  APP_EXCEPTION_CODES_V17.NOT_FOUND
15:47:49 1101  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:49 1102  */
15:47:49 1103  	 in_subscription_id IN NUMBER,
15:47:49 1104  	 in_invoice_id	    IN NUMBER,
15:47:49 1105  	 in_refund_amount   IN NUMBER,
15:47:49 1106  	 in_note	    IN VARCHAR2,
15:47:49 1107  	 in_created_by	    IN VARCHAR2,
15:47:49 1108  	 out_charge_id	    OUT NUMBER
15:47:49 1109  ) AS
15:47:49 1110  -- VARIABLES
15:47:49 1111  SPROC_NAME	      CONSTANT VARCHAR2(19) := 'REFUND_SUBSCRIPTION';
15:47:49 1112  var_invoice_status_id  NUMBER;
15:47:49 1113  var_account_id	      NUMBER;
15:47:49 1114  var_account_status_id  NUMBER;
15:47:49 1115  var_new_transaction_id NUMBER;
15:47:49 1116  var_instrument_type_id NUMBER;
15:47:49 1117  var_instrument_id      NUMBER;
15:47:49 1118  var_new_charge_id      NUMBER;
15:47:49 1119  var_invoice_amount     NUMBER(10,2);
15:47:49 1120  var_refunds_before     NUMBER(10,2);
15:47:49 1121  var_charges_amount     NUMBER(10,2);
15:47:49 1122  -- EXCEPTIONS
15:47:49 1123  BAD_SUBSCRIPTION_ID	     EXCEPTION;
15:47:49 1124  ACCOUNT_IS_FROZEN	     EXCEPTION;
15:47:49 1125  BAD_INVOICE_ID		     EXCEPTION;
15:47:49 1126  CAN_NOT_CREATE_TRANSACTION    EXCEPTION;
15:47:49 1127  CAN_NOT_CREATE_CHARGE	     EXCEPTION;
15:47:49 1128  CAN_NOT_CALC_INVOICE_AMOUNT   EXCEPTION;
15:47:49 1129  REFUND_IS_GREATER_THAN_ANOUNT EXCEPTION;
15:47:49 1130  CAN_NOT_ANNOTATE_SUBSCRIPTION EXCEPTION;
15:47:49 1131  TOT_REF_IS_GREATER_THAN_ANOUNT EXCEPTION;
15:47:49 1132  INVOICE_IS_NOT_CLOSED	     EXCEPTION;
15:47:49 1133  TOT_REF_IS_GRATER_THAN_CHARGES EXCEPTION;
15:47:49 1134  EXCEPTION_MESSAGE	      VARCHAR2(1024);
15:47:49 1135  BEGIN
15:47:49 1136  
15:47:49 1137  	 BEGIN
15:47:49 1138  	   SELECT
15:47:49 1139  	     SUBSCRIPTION.INSTRUMENT_ID,
15:47:49 1140  	     SUBSCRIPTION.INSTRUMENT_TYPE_ID,
15:47:49 1141  	     SUBSCRIPTION.ACCOUNT_ID
15:47:49 1142  	     into
15:47:49 1143  	     var_instrument_id,
15:47:49 1144  	     var_instrument_type_id,
15:47:49 1145  	     var_account_id
15:47:49 1146  	   FROM
15:47:49 1147  	     SUBSCRIPTION
15:47:49 1148  	   WHERE
15:47:49 1149  	     SUBSCRIPTION.ID = in_subscription_id;
15:47:49 1150  	   EXCEPTION
15:47:49 1151  	     WHEN NO_DATA_FOUND THEN
15:47:49 1152  	       RAISE BAD_SUBSCRIPTION_ID;
15:47:49 1153  	 END;
15:47:49 1154  
15:47:49 1155  	 -- Check account status. It should not to be frozen
15:47:49 1156  	 SELECT
15:47:49 1157  	   ACCOUNT.ACCOUNT_STATUS_ID into var_account_status_id
15:47:49 1158  	 FROM
15:47:49 1159  	   ACCOUNT
15:47:49 1160  	 WHERE
15:47:49 1161  	   ACCOUNT.ID = var_account_id;
15:47:49 1162  
15:47:49 1163  	 IF var_account_status_id = GLOBAL_STATUSES_V17.ACCOUNT_FROZEN THEN
15:47:49 1164  	   RAISE ACCOUNT_IS_FROZEN;
15:47:49 1165  	 END IF;
15:47:49 1166  
15:47:49 1167  	 BEGIN
15:47:49 1168  	   SELECT
15:47:49 1169  	     INVOICE.INVOICE_STATUS_ID into var_invoice_status_id
15:47:49 1170  	   FROM
15:47:49 1171  	     INVOICE
15:47:49 1172  	   WHERE
15:47:49 1173  	     INVOICE.ID = in_invoice_id;
15:47:49 1174  	   EXCEPTION
15:47:49 1175  	     WHEN NO_DATA_FOUND THEN
15:47:49 1176  	       RAISE BAD_INVOICE_ID;
15:47:49 1177  	 END;
15:47:49 1178  
15:47:49 1179  	 IF var_invoice_status_id != GLOBAL_STATUSES_V17.INVOICE_CLOSED THEN
15:47:49 1180  	   RAISE INVOICE_IS_NOT_CLOSED;
15:47:49 1181  	 END IF;
15:47:49 1182  
15:47:49 1183  	 BEGIN
15:47:49 1184  	   PROCS_INVOICE_V17.CALCULATE_INVOICE_AMOUNT (
15:47:49 1185  	     in_invoice_id => in_invoice_id,
15:47:49 1186  	     out_amount    => var_invoice_amount
15:47:49 1187  	   );
