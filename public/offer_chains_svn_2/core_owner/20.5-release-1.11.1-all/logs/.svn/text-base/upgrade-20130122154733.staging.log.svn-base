15:47:49 223  
15:47:49 224  	-- Calculate amount for each line item in invoice
15:47:49 225  	FOR f_line_item IN (
15:47:49 226  	  SELECT
15:47:49 227  	    LINE_ITEM.ID
15:47:49 228  	  FROM
15:47:49 229  	    LINE_ITEM
15:47:49 230  	  WHERE
