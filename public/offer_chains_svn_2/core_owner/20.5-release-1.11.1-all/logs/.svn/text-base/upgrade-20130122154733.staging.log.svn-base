15:47:49 202  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 203  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:49 204  END UPDATE_LICENSE_STATUS;
