15:47:49 866  
15:47:49 867  FUNCTION GET_RENEWAL_DATE (
15:47:49 868  /*
15:47:49 869  Throws exceptions:
15:47:49 870  APP_EXCEPTION_CODES_V17.NOT_FOUND
15:47:49 871  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:49 872  */
15:47:49 873  	in_subscription_id in NUMBER
15:47:49 874  ) RETURN DATE AS
15:47:49 875  -- VARIABLES
15:47:49 876  SPROC_NAME	      CONSTANT VARCHAR2(16) := 'GET_RENEWAL_DATE';
15:47:49 877  var_subscription_status NUMBER;
15:47:49 878  var_licenses_count      NUMBER;
15:47:49 879  var_license_end_date    DATE;
15:47:49 880  var_last_offer_id       NUMBER;
15:47:49 881  var_offer_chain_id      NUMBER;
15:47:49 882  var_last_license_id     NUMBER;
15:47:49 883  var_current_offer_index NUMBER;
15:47:49 884  var_current_offer_recurr_num NUMBER;
15:47:49 885  var_offer_recurr_num    NUMBER;
15:47:49 886  -- EXCEPTIONS
15:47:49 887  BAD_SUBSCRIPTION_ID EXCEPTION;
15:47:49 888  NO_LICENSES_FOUND EXCEPTION;
15:47:49 889  BEGIN
15:47:49 890  
15:47:49 891  	-- Get subscription id and offer chain id
15:47:49 892  	BEGIN
15:47:49 893  	  SELECT
15:47:49 894  	    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID,
15:47:49 895  	    SUBSCRIPTION.OFFER_CHAIN_ID
15:47:49 896  	    into
15:47:49 897  	    var_subscription_status,
15:47:49 898  	    var_offer_chain_id
15:47:49 899  	  FROM
15:47:49 900  	    SUBSCRIPTION
15:47:49 901  	  WHERE
15:47:49 902  	    SUBSCRIPTION.ID = in_subscription_id;
15:47:49 903  	  EXCEPTION
15:47:49 904  	    WHEN NO_DATA_FOUND THEN
15:47:49 905  	      RAISE BAD_SUBSCRIPTION_ID;
15:47:49 906  	END;
15:47:49 907  
15:47:49 908  	IF var_subscription_status != GLOBAL_STATUSES_V17.SUBSCRIPTION_ACTIVE THEN
15:47:49 909  	  -- TODO: Is suspended subscription has renewal date? (For the phase II)
15:47:49 910  	  -- AND var_subscription_status != GLOBAL_STATUSES_V17.SUBSCRIPTION_SUSPENDED THEN
15:47:49 911  	  RETURN NULL;
15:47:49 912  	END IF;
15:47:49 913  
15:47:49 914  	BEGIN
15:47:49 915  	  SELECT
15:47:49 916  	    LICENSE_ID,
15:47:49 917  	    END_DATE,
15:47:49 918  	    OFFER_ID,
15:47:49 919  	    CURRENT_OFFER_INDEX,
15:47:49 920  	    CURRENT_OFFER_RECURR_NUM
15:47:49 921  	    into
15:47:49 922  	    var_last_license_id,
15:47:49 923  	    var_license_end_date,
15:47:49 924  	    var_last_offer_id,
15:47:49 925  	    var_current_offer_index,
15:47:49 926  	    var_current_offer_recurr_num
15:47:49 927  	  FROM
15:47:49 928  	    (
15:47:49 929  	      SELECT
15:47:49 930  		LICENSE.ID as "LICENSE_ID",
15:47:49 931  		LICENSE.END_DATE,
15:47:49 932  		LICENSE.OFFER_ID,
15:47:49 933  		LICENSE.CURRENT_OFFER_INDEX,
15:47:49 934  		LICENSE.CURRENT_OFFER_RECURR_NUM
15:47:49 935  	      FROM
15:47:49 936  		LICENSE
15:47:49 937  	      WHERE
15:47:49 938  		LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V17.LICENSE_ACTIVE
15:47:49 939  		AND LICENSE.SUBSCRIPTION_ID = in_subscription_id
15:47:49 940  	      ORDER BY END_DATE DESC
15:47:49 941  	    )
15:47:49 942  	    INNER JOIN OFFER ON OFFER_ID = OFFER.ID
15:47:49 943  	  WHERE
15:47:49 944  	    ROWNUM <= 1;
15:47:49 945  
15:47:49 946  	  EXCEPTION
15:47:49 947  	    WHEN NO_DATA_FOUND THEN
15:47:49 948  	      -- RAISE NO_LICENSES_FOUND;
15:47:49 949  	      RETURN NULL;
15:47:49 950  	END;
15:47:49 951  
15:47:49 952  	SELECT
15:47:49 953  	  OFFER_OFFER_CHAIN.NUM_RECURRENCES into var_offer_recurr_num
15:47:49 954  	FROM
15:47:49 955  	  OFFER_OFFER_CHAIN
15:47:49 956  	WHERE
15:47:49 957  	  OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
15:47:49 958  	  AND OFFER_OFFER_CHAIN.OFFER_ID = var_last_offer_id;
15:47:49 959  
15:47:49 960  	IF PROCS_OFFER_CHAIN_V17.GET_NEXT_OFFER_INDEX(var_offer_chain_id, var_current_offer_index) IS NULL
15:47:49 961  	  AND var_offer_recurr_num = var_current_offer_recurr_num THEN
15:47:49 962  	  -- There is no next offer for this subscription
15:47:49 963  	  RETURN NULL;
15:47:49 964  	END IF;
15:47:49 965  
15:47:49 966  	RETURN var_license_end_date;
15:47:49 967  
15:47:49 968  EXCEPTION
15:47:49 969  WHEN BAD_SUBSCRIPTION_ID THEN
15:47:49 970  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 971  	  SPROC_NAME, 'No such subscription');
15:47:49 972  WHEN NO_LICENSES_FOUND THEN
15:47:49 973  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 974  	  SPROC_NAME, 'No licenses found');
15:47:49 975  WHEN OTHERS THEN
15:47:49 976  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 977  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:49 978  END GET_RENEWAL_DATE;
15:47:49 979  
