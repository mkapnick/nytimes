15:47:47  88  	PROCEDURE CHASE_PROFILE_BY_REQ_FILE_ID(
15:47:47  89  	  in_request_file_id IN CC_UPDATE.CC_REQUEST_FILE_ID%TYPE,
15:47:47  90  	  in_start	     IN NUMBER,
15:47:47  91  	  in_end	     IN NUMBER,
15:47:47  92  	  out_result_set     OUT SYS_REFCURSOR
15:47:47  93  	) AS
15:47:47  94  	SPROC_NAME CONSTANT VARCHAR2(32) := 'CHASE_PROFILE_BY_REQ_FILE_ID';
15:47:47  95  	var_range_diff	    NUMBER := 0;
15:47:47  96  	var_upper_bond_diff NUMBER := 0;
15:47:47  97  	var_l_start	    NUMBER := 0;
15:47:47  98  	var_l_end	    NUMBER := 0;
15:47:47  99  	BEGIN
15:47:47 100  	  --Normalize the end points [START]
15:47:47 101  	  IF (in_start IS NULL OR in_start < 0) Then
15:47:47 102  	    var_l_start := 0;
15:47:47 103  	  ELSE
15:47:47 104  	    var_l_start := in_start;
15:47:47 105  	  END IF;
15:47:47 106  
15:47:47 107  	  IF (in_end IS NULL) Then
15:47:47 108  	    var_l_end := 500;
15:47:47 109  	  ELSE
15:47:47 110  	    var_l_end := in_end;
15:47:47 111  	  END IF;
15:47:47 112  
15:47:47 113  	  var_l_start := var_l_start + 1;
15:47:47 114  	  var_l_end   := var_l_end   + 1;
15:47:47 115  
15:47:47 116  	  var_range_diff  := var_l_end - var_l_start;
15:47:47 117  	  var_upper_bond_diff :=  var_range_diff - 1000;
15:47:47 118  
15:47:47 119  	  IF (var_upper_bond_diff > 0) Then
15:47:47 120  	    var_l_end := var_l_end - var_upper_bond_diff;
15:47:47 121  	  END IF;
15:47:47 122  	  --Normalize the end points [END]
15:47:47 123  
15:47:47 124  	  OPEN out_result_set FOR
15:47:47 125  	    SELECT CHASE_PROFILE_ID FROM
15:47:47 126  	      (SELECT rownum rnum, q.* FROM
15:47:47 127  		 (SELECT
15:47:47 128  		    cc.CHASE_PROFILE_ID
15:47:47 129  		  FROM
15:47:47 130  		    CREDIT_CARD cc,
15:47:47 131  		    CC_UPDATE ccu
15:47:47 132  		  WHERE
15:47:47 133  		    ccu.CC_REQUEST_FILE_ID = in_request_file_id
15:47:47 134  		    AND ccu.CREDIT_CARD_ID = cc.id
15:47:47 135  		) Q
15:47:47 136  	      WHERE rownum <= var_l_end)
15:47:47 137  	    WHERE rnum >= var_l_Start;
15:47:47 138  	END CHASE_PROFILE_BY_REQ_FILE_ID;
