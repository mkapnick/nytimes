15:47:49 147  PROCEDURE UPDATE_LICENSE_STATUS(
15:47:49 148  	  in_license_id     IN NUMBER,
15:47:49 149  	  in_license_status IN NUMBER,
15:47:49 150  	  in_updated_by     IN VARCHAR2,
15:47:49 151  	  in_ent_end	    IN NUMBER DEFAULT GLOBAL_CONSTANTS_V17.FALSE
15:47:49 152  ) AS
15:47:49 153  SPROC_NAME CONSTANT VARCHAR2(21) := 'UPDATE_LICENSE_STATUS';
15:47:49 154  -- VARIABLES
15:47:49 155  temp_license_id NUMBER;
15:47:49 156  -- EXCEPTIONS
15:47:49 157  BAD_LICENSE_ID	     EXCEPTION;
15:47:49 158  BAD_LICENSE_STATUS     EXCEPTION;
15:47:49 159  EXCEPTION_MESSAGE      VARCHAR2(1024);
15:47:49 160  BEGIN
15:47:49 161  
15:47:49 162  	BEGIN
15:47:49 163  	  SELECT
15:47:49 164  	    ID into temp_license_id
15:47:49 165  	  FROM
15:47:49 166  	    LICENSE
15:47:49 167  	  WHERE
15:47:49 168  	    LICENSE.ID = in_license_id;
15:47:49 169  	  EXCEPTION
15:47:49 170  	    WHEN NO_DATA_FOUND THEN
15:47:49 171  	      RAISE BAD_LICENSE_ID;
15:47:49 172  	END;
15:47:49 173  
15:47:49 174  	IF in_license_status != GLOBAL_STATUSES_V17.LICENSE_CLOSED
15:47:49 175  	   AND in_license_status != GLOBAL_STATUSES_V17.LICENSE_ACTIVE THEN
15:47:49 176  	  RAISE BAD_LICENSE_STATUS;
15:47:49 177  	END IF;
15:47:49 178  
15:47:49 179  	IF (in_ent_end is not null and in_ent_end = GLOBAL_CONSTANTS_V17.TRUE) then
15:47:49 180  	  PROCS_LICENSE_CRU_V17.UPDATE_LICENSE(
15:47:49 181  	    in_license_id	 => in_license_id,
15:47:49 182  	    in_updated_by	 => in_updated_by,
15:47:49 183  	    in_license_status_id => in_license_status,
15:47:49 184  	    in_entitlement_end_date	 => sysdate
15:47:49 185  	  );
15:47:49 186  	ELSE
15:47:49 187  	  PROCS_LICENSE_CRU_V17.UPDATE_LICENSE(
15:47:49 188  	    in_license_id	 => in_license_id,
15:47:49 189  	    in_updated_by	 => in_updated_by,
15:47:49 190  	    in_license_status_id => in_license_status
15:47:49 191  	  );
15:47:49 192  	END IF;
15:47:49 193  
15:47:49 194  EXCEPTION
15:47:49 195  WHEN BAD_LICENSE_STATUS THEN
15:47:49 196  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INVALID_PARAMETER,
15:47:49 197  	  SPROC_NAME, 'Bad status id');
15:47:49 198  WHEN BAD_LICENSE_ID THEN
15:47:49 199  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 200  	  SPROC_NAME, 'No such license');
