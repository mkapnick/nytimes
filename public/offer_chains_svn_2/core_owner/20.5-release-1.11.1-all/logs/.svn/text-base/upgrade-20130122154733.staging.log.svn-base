15:47:49 261  PROCEDURE GET_ACCOUNT_BY_INVOICE_ID (
15:47:49 262  /*
15:47:49 263  Throws exceptions:
15:47:49 264  APP_EXCEPTION_CODES_V17.NOT_FOUND
15:47:49 265  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:49 266  */
15:47:49 267  	in_invoice_id  IN  NUMBER,
15:47:49 268  	out_account_id OUT NUMBER
15:47:49 269  ) AS
15:47:49 270  -- VARIABLES
15:47:49 271  SPROC_NAME	 CONSTANT VARCHAR2(25) := 'GET_ACCOUNT_BY_INVOICE_ID';
15:47:49 272  temp_gc_account_id NUMBER;
15:47:49 273  temp_ss_account_id NUMBER;
15:47:49 274  temp_invoice_id	 NUMBER;
15:47:49 275  -- EXCEPTIONS
15:47:49 276  BAD_INVOICE_ID	   EXCEPTION;
15:47:49 277  CAN_NOT_FIND_ACCOUNT EXCEPTION;
15:47:49 278  BEGIN
15:47:49 279  
15:47:49 280  	-- Check that given invoice exists
15:47:49 281  	BEGIN
15:47:49 282  	  SELECT
15:47:49 283  	    INVOICE.ID into temp_invoice_id
15:47:49 284  	  FROM
15:47:49 285  	    INVOICE
15:47:49 286  	  WHERE
15:47:49 287  	    INVOICE.ID = in_invoice_id;
15:47:49 288  	  EXCEPTION
15:47:49 289  	    WHEN NO_DATA_FOUND THEN
15:47:49 290  	      RAISE BAD_INVOICE_ID;
15:47:49 291  	END;
15:47:49 292  
15:47:49 293  	-- Try to find gift certificate with given invoice
15:47:49 294  	BEGIN
15:47:49 295  	  SELECT
15:47:49 296  	    ACCOUNT.GROUP_ID into temp_gc_account_id
15:47:49 297  	  FROM
15:47:49 298  	    GIFT_CERTIFICATE
15:47:49 299  	    INNER JOIN ACCOUNT ON GIFT_CERTIFICATE.PURCHASER_GROUP_ID = ACCOUNT.GROUP_ID
15:47:49 300  	  WHERE
15:47:49 301  	    GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = in_invoice_id
15:47:49 302  	    AND ROWNUM <= 1;
15:47:49 303  	  EXCEPTION
15:47:49 304  	    WHEN NO_DATA_FOUND THEN
15:47:49 305  	      temp_gc_account_id := NULL;
15:47:49 306  	END;
15:47:49 307  
15:47:49 308  	-- check subscriptions for given invoice
15:47:49 309  	IF temp_gc_account_id IS NOT NULL THEN
15:47:49 310  	  out_account_id := temp_gc_account_id;
15:47:49 311  	ELSE
15:47:49 312  	  BEGIN
15:47:49 313  	    SELECT
15:47:49 314  	      ACCOUNT.GROUP_ID into temp_ss_account_id
15:47:49 315  	    FROM
15:47:49 316  	      LICENSE
15:47:49 317  	      INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:47:49 318  	      INNER JOIN ACCOUNT ON SUBSCRIPTION.ACCOUNT_ID = ACCOUNT.ID
15:47:49 319  	    WHERE
15:47:49 320  	      LICENSE.INVOICE_ID = in_invoice_id
15:47:49 321  	      AND ROWNUM <= 1;
15:47:49 322  	    EXCEPTION
15:47:49 323  	      WHEN NO_DATA_FOUND THEN
15:47:49 324  		temp_ss_account_id := NULL;
15:47:49 325  	  END;
15:47:49 326  	  IF temp_ss_account_id IS NULL THEN
15:47:49 327  	    RAISE CAN_NOT_FIND_ACCOUNT;
15:47:49 328  	  ELSE
15:47:49 329  	    out_account_id := temp_ss_account_id;
15:47:49 330  	  END IF;
15:47:49 331  	END IF;
15:47:49 332  
15:47:49 333  EXCEPTION
15:47:49 334  WHEN BAD_INVOICE_ID THEN
15:47:49 335  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 336  	  SPROC_NAME, 'No such invoice');
15:47:49 337  WHEN CAN_NOT_FIND_ACCOUNT THEN
15:47:49 338  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 339  	  SPROC_NAME, 'Could not find account by given invoice id');
15:47:49 340  WHEN OTHERS THEN
15:47:49 341  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 342  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:49 343  END GET_ACCOUNT_BY_INVOICE_ID;
