15:47:50 451  
15:47:50 452  PROCEDURE DISABLE_PAYPAL (
15:47:50 453  /*
15:47:50 454  Throws exceptions:
15:47:50 455  APP_EXCEPTION_CODES_V17.NOT_FOUND
15:47:50 456  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:50 457  APP_EXCEPTION_CODES_V17.INTERNAL_ERROR
15:47:50 458  */
15:47:50 459  	in_paypal_id  IN NUMBER,
15:47:50 460  	in_updated_by IN VARCHAR2
15:47:50 461  ) AS
15:47:50 462  SPROC_NAME CONSTANT VARCHAR2(14) := 'DISABLE_PAYPAL';
15:47:50 463  -- VARIABLES
15:47:50 464  var_paypal_status_id NUMBER;
15:47:50 465  var_account_id	   NUMBER;
15:47:50 466  var_group_id	   NUMBER;
15:47:50 467  var_pending_invoices_num NUMBER;
15:47:50 468  current_def_instrument_type_id NUMBER;
15:47:50 469  current_def_instrument_id NUMBER;
15:47:50 470  -- EXCEPTIONS
15:47:50 471  BAD_PAYPAL_ID EXCEPTION;
15:47:50 472  PAYPAL_ALREADY_INACTIVE EXCEPTION;
15:47:50 473  PENDING_TRANSACTIONS_FOUND EXCEPTION;
15:47:50 474  CAN_NOT_GET_DEF_FINANCIAL EXCEPTION;
15:47:50 475  CAN_NOT_DEL_DEF_FINANCIAL EXCEPTION;
15:47:50 476  EXCEPTION_MESSAGE VARCHAR2(1024);
15:47:50 477  BEGIN
15:47:50 478  
15:47:50 479  	-- Get Paypal status
15:47:50 480  	-- Get account
15:47:50 481  	BEGIN
15:47:50 482  	  SELECT
15:47:50 483  	    PAYPAL.PAYPAL_STATUS_ID,
15:47:50 484  	    PAYPAL.ACCOUNT_ID
15:47:50 485  	    into
15:47:50 486  	    var_paypal_status_id,
15:47:50 487  	    var_account_id
15:47:50 488  	  FROM
15:47:50 489  	    PAYPAL
15:47:50 490  	  WHERE
15:47:50 491  	    PAYPAL.ID = in_paypal_id;
15:47:50 492  	  EXCEPTION
15:47:50 493  	    WHEN NO_DATA_FOUND THEN
15:47:50 494  	      RAISE BAD_PAYPAL_ID;
15:47:50 495  	END;
15:47:50 496  
15:47:50 497  	-- Get group id
15:47:50 498  	SELECT
15:47:50 499  	  ACCOUNT.GROUP_ID into var_group_id
15:47:50 500  	FROM
15:47:50 501  	  ACCOUNT
15:47:50 502  	WHERE
15:47:50 503  	  ACCOUNT.ID = var_account_id;
15:47:50 504  
15:47:50 505  	-- Card should not be disabled
15:47:50 506  	IF var_paypal_status_id = GLOBAL_STATUSES_V17.PAYPAL_INACTIVE THEN
15:47:50 507  	  RAISE PAYPAL_ALREADY_INACTIVE;
15:47:50 508  	END IF;
15:47:50 509  
15:47:50 510  	-- Looking for pending transactions associated with given credit card
15:47:50 511  	SELECT
15:47:50 512  	  COUNT(*) into var_pending_invoices_num
15:47:50 513  	FROM
15:47:50 514  	  CHARGE
15:47:50 515  	  INNER JOIN TRANSACTION ON CHARGE.TRANSACTION_ID = TRANSACTION.ID
15:47:50 516  	WHERE
15:47:50 517  	  CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V17.INSTRUMENT_PAYPAL
15:47:50 518  	  AND CHARGE.INSTRUMENT_ID = in_paypal_id
15:47:50 519  	  AND TRANSACTION.TRANSACTION_STATUS_ID = GLOBAL_STATUSES_V17.TRANSACTION_PENDING;
15:47:50 520  
15:47:50 521  	IF var_pending_invoices_num > 0 THEN
15:47:50 522  	  RAISE PENDING_TRANSACTIONS_FOUND;
15:47:50 523  	END IF;
15:47:50 524  
15:47:50 525  	-- Getting current default financial instrument
15:47:50 526  	BEGIN
15:47:50 527  	  GET_DEF_FINANCIAL_INSTRUMENT(
15:47:50 528  	    in_group_id 	   => var_group_id,
15:47:50 529  	    out_instrument_type_id => current_def_instrument_type_id,
15:47:50 530  	    out_instrument_id	   => current_def_instrument_id
15:47:50 531  	  );
15:47:50 532  	  EXCEPTION
15:47:50 533  	    WHEN OTHERS THEN
15:47:50 534  	      EXCEPTION_MESSAGE := SQLERRM;
15:47:50 535  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
15:47:50 536  	END;
15:47:50 537  
15:47:50 538  	-- Checking that credit card is not default
15:47:50 539  	IF current_def_instrument_type_id = GLOBAL_ENUMS_V17.INSTRUMENT_PAYPAL
15:47:50 540  	  AND current_def_instrument_id = in_paypal_id THEN
15:47:50 541  	  BEGIN
15:47:50 542  	    DEL_DEF_FINANCIAL_INSTRUMENT(
15:47:50 543  	      in_group_id => var_group_id
15:47:50 544  	    );
15:47:50 545  	  EXCEPTION
15:47:50 546  	    WHEN OTHERS THEN
15:47:50 547  	      EXCEPTION_MESSAGE := SQLERRM;
15:47:50 548  	      RAISE CAN_NOT_DEL_DEF_FINANCIAL;
15:47:50 549  	  END;
15:47:50 550  	END IF;
15:47:50 551  
15:47:50 552  	PROCS_FIN_INSTRUMENTS_V17.UPDATE_PAYPAL_STATUS(
15:47:50 553  	  in_paypal_id	      => in_paypal_id,
15:47:50 554  	  in_updated_by       => in_updated_by,
15:47:50 555  	  in_paypal_status_id => GLOBAL_STATUSES_V17.PAYPAL_INACTIVE
15:47:50 556  	);
15:47:50 557  
15:47:50 558  EXCEPTION
15:47:50 559  WHEN BAD_PAYPAL_ID THEN
15:47:50 560  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 561  	  SPROC_NAME, 'No such paypal');
15:47:50 562  WHEN PAYPAL_ALREADY_INACTIVE THEN
15:47:50 563  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.STATE_ERROR,
15:47:50 564  	  SPROC_NAME, 'Paypal already inactive');
15:47:50 565  WHEN PENDING_TRANSACTIONS_FOUND THEN
15:47:50 566  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.STATE_ERROR,
15:47:50 567  	  SPROC_NAME, 'Account has pending charge which are using this paypal');
15:47:50 568  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
15:47:50 569  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50 570  	  SPROC_NAME, 'Could not get current default financial instrument', EXCEPTION_MESSAGE);
15:47:50 571  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
15:47:50 572  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50 573  	  SPROC_NAME, 'Could not delete information about default financial instrument from account', EXCEPTION_MESSAGE);
15:47:50 574  WHEN OTHERS THEN
15:47:50 575  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:50 576  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:50 577  END DISABLE_PAYPAL;
