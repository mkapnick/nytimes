15:47:49 1404  	     OFFER.ID,
15:47:49 1405  	     INVOICE.ID
15:47:49 1406  	   INTO
15:47:49 1407  	     var_current_license_id,
15:47:49 1408  	     var_current_license_start_date,
15:47:49 1409  	     var_current_license_end_date,
15:47:49 1410  	     var_current_offer_index,
15:47:49 1411  	     var_current_offer_recurr_num,
15:47:49 1412  	     var_current_offer_id,
15:47:49 1413  	     var_current_invoice_id
15:47:49 1414  	   FROM
15:47:49 1415  	     LICENSE
15:47:49 1416  	     INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:47:49 1417  	     INNER JOIN OFFER ON LICENSE.OFFER_ID = OFFER.ID
15:47:49 1418  	     INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
15:47:49 1419  	   WHERE
15:47:49 1420  	     SUBSCRIPTION.ID = in_subscription_id
