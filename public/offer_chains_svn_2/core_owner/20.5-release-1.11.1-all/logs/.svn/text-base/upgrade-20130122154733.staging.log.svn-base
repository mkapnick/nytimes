15:47:48  13  -- VARIABLES
15:47:48  14  SPROC_NAME      CONSTANT VARCHAR2(14) := 'ADD_LINE_ITEMS';
15:47:48  15  temp_invoice_id NUMBER;
15:47:48  16  temp_offer_id   NUMBER;
15:47:48  17  
15:47:48  18  var_line_item_data SYS_REFCURSOR;
15:47:48  19  var_new_line_item_id NUMBER;
15:47:48  20  var_product_unit_price NUMBER (10,6);
15:47:48  21  var_product_offering_price NUMBER(10,6);
15:47:48  22  var_product_offering_oprice NUMBER(10,6);
