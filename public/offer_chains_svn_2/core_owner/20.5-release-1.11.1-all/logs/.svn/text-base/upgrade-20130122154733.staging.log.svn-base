15:47:49 204  CAN_NOT_CALC_LINE_ITEM_AMOUNT EXCEPTION;
15:47:49 205  EXCEPTION_MESSAGE VARCHAR2(1024);
15:47:49 206  BEGIN
15:47:49 207  
15:47:49 208  	var_total_amount := 0;
15:47:49 209  
15:47:49 210  	-- Check that given invoice exists
15:47:49 211  	BEGIN
15:47:49 212  	  SELECT
15:47:49 213  	    INVOICE.ID into temp_invoice_id
15:47:49 214  	  FROM
15:47:49 215  	    INVOICE
15:47:49 216  	  WHERE
15:47:49 217  	    INVOICE.ID = in_invoice_id
15:47:49 218  	    AND ROWNUM <= 1;
