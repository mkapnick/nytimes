15:47:49 407  
15:47:49 408  	-- get all pp charges for given invoice
15:47:49 409  	OPEN out_pp_charges_set FOR
15:47:49 410  	SELECT
15:47:49 411  	  CHARGE.ID as "CHARGE_ID",
15:47:49 412  	  CHARGE_AMOUNT,
15:47:49 413  	  CHARGE.INSTRUMENT_ID,
15:47:49 414  	  CHARGE.CHARGE_STATUS_ID
