15:47:49 981  
15:47:49 982  FUNCTION GET_RECENT_CHARGE (
15:47:49 983  /*
15:47:49 984  Throws exceptions:
15:47:49 985  APP_EXCEPTION_CODES_V17.NOT_FOUND
15:47:49 986  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:49 987  */
15:47:49 988  	in_subscription_id IN NUMBER
15:47:49 989  ) RETURN NUMBER AS
15:47:49 990  -- VARIABLES
15:47:49 991  SPROC_NAME	   CONSTANT VARCHAR2(17) := 'GET_RECENT_CHARGE';
15:47:49 992  temp_subscription_id NUMBER;
15:47:49 993  var_recent_charge    NUMBER(10,2);
15:47:49 994  
15:47:49 995  -- EXCEPTIONS
15:47:49 996  BAD_SUBSCRIPTION_ID EXCEPTION;
15:47:49 997  BEGIN
15:47:49 998  
15:47:49 999  	BEGIN
15:47:49 1000  	   SELECT
15:47:49 1001  	     SUBSCRIPTION.ID into temp_subscription_id
15:47:49 1002  	   FROM
15:47:49 1003  	     SUBSCRIPTION
15:47:49 1004  	   WHERE
15:47:49 1005  	     SUBSCRIPTION.ID = in_subscription_id;
15:47:49 1006  	   EXCEPTION
15:47:49 1007  	     WHEN NO_DATA_FOUND THEN
15:47:49 1008  	       RAISE BAD_SUBSCRIPTION_ID;
15:47:49 1009  	 END;
15:47:49 1010  
15:47:49 1011  	 BEGIN
15:47:49 1012  	   SELECT
15:47:49 1013  	     CHARGE.CHARGE_AMOUNT into var_recent_charge
15:47:49 1014  	   FROM
15:47:49 1015  	     LICENSE
15:47:49 1016  	     INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:47:49 1017  	     INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
15:47:49 1018  	     INNER JOIN CHARGE ON CHARGE.INVOICE_ID = INVOICE.ID
15:47:49 1019  	   WHERE
15:47:49 1020  	     -- TODO: Review
15:47:49 1021  	     -- LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V17.LICENSE_PROCESSED
15:47:49 1022  	     -- AND
15:47:49 1023  	     SUBSCRIPTION.ID = in_subscription_id
15:47:49 1024  	     AND CHARGE.CHARGE_AMOUNT >= 0
15:47:49 1025  	     AND ROWNUM <= 1
15:47:49 1026  	   ORDER BY
15:47:49 1027  	     LICENSE.ID ASC, CHARGE.ID DESC;
15:47:49 1028  	   EXCEPTION
15:47:49 1029  	     WHEN NO_DATA_FOUND THEN
15:47:49 1030  	       var_recent_charge := 0;
15:47:49 1031  	 END;
15:47:49 1032  
15:47:49 1033  	 RETURN var_recent_charge;
15:47:49 1034  
15:47:49 1035  EXCEPTION
15:47:49 1036  WHEN BAD_SUBSCRIPTION_ID THEN
15:47:49 1037  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 1038  	   SPROC_NAME, 'No such subscription');
15:47:49 1039  WHEN OTHERS THEN
15:47:49 1040  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 1041  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:47:49 1042  END GET_RECENT_CHARGE;
