15:47:49 1527  	   EXCEPTION
15:47:49 1528  	     WHEN OTHERS THEN
15:47:49 1529  	       EXCEPTION_MESSAGE := SQLERRM;
15:47:49 1530  	       RAISE CAN_NOT_ANNOTATE_SUBSCRIPTION;
15:47:49 1531  	 END;
15:47:49 1532  
15:47:49 1533  EXCEPTION
