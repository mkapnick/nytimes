15:47:49 1496  	    PROCS_COMMON_V17.NORMALIZE_DATE(in_effective_start_date) THEN
15:47:49 1497  	   BEGIN
15:47:49 1498  	     PROCS_LICENSE_V17.CREATE_LICENSE (
15:47:49 1499  	       in_status_id		   => GLOBAL_STATUSES_V17.LICENSE_ACTIVE,
15:47:49 1500  	       in_needs_entitlements	   => GLOBAL_CONSTANTS_V17.TRUE,
15:47:49 1501  	       in_start_date		   => in_effective_end_date,
15:47:49 1502  	       in_end_date		   => var_current_license_end_date + (in_effective_end_date - in_effective_start_date),
15:47:49 1503  	       in_offer_id		   => var_current_offer_id,
15:47:49 1504  	       in_subscription_id	   => in_subscription_id,
15:47:49 1505  	       in_invoice_id		   => var_current_invoice_id,
15:47:49 1506  	       in_created_by		   => in_updated_by,
15:47:49 1507  	       in_is_extension		   => GLOBAL_CONSTANTS_V17.FALSE,
15:47:49 1508  	       in_current_offer_index	   => var_current_offer_index,
15:47:49 1509  	       in_current_offer_recurr_num => var_current_offer_recurr_num,
15:47:49 1510  	       out_license_id		   => var_ext_license_id
15:47:49 1511  	     );
15:47:49 1512  	     EXCEPTION
15:47:49 1513  	       WHEN OTHERS THEN
15:47:49 1514  		 EXCEPTION_MESSAGE := SQLERRM;
15:47:49 1515  		 RAISE CAN_NOT_CREATE_END_LICENSE;
15:47:49 1516  	   END;
15:47:49 1517  	 END IF;
15:47:49 1518  
15:47:49 1519  	 -- Create new note for subscription
15:47:49 1520  	 BEGIN
