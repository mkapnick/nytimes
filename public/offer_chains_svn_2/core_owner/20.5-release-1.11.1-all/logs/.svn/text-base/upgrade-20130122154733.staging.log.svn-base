15:47:50 148  		   ACCOUNT a2
15:47:50 149  		 WHERE
15:47:50 150  		   a.GROUP_ID	       = in_group_id AND
15:47:50 151  		   a.ID 	       = ss.BORROWER_ACCOUNT_ID AND
15:47:50 152  		   ss.GROUP_ACCOUNT_ID = ga.ID	AND
15:47:50 153  		   ga.SUBSCRIPTION_ID  = s.ID AND
15:47:50 154  		   s.ACCOUNT_ID        = a2.ID
15:47:50 155  		) Q
15:47:50 156  	      WHERE rownum <= l_end)
15:47:50 157  	    WHERE rnum >= l_Start;
15:47:50 158  	END;
15:47:50 159  	ELSE
15:47:50 160  	BEGIN
15:47:50 161  	    OPEN out_result_set FOR
15:47:50 162  	    SELECT
15:47:50 163  	      *
15:47:50 164  	    FROM
15:47:50 165  	      (SELECT rownum rnum, q.*
15:47:50 166  	       FROM
15:47:50 167  		(SELECT
15:47:50 168  		   ga.SUBSCRIPTION_ID,
15:47:50 169  		   ss.START_DATE,
15:47:50 170  		   ss.END_DATE,
15:47:50 171  		   a2.GROUP_ID AS PARENT_GROUP_ID
15:47:50 172  		 FROM
15:47:50 173  		   GROUP_ACCOUNT ga,
15:47:50 174  		   SUBSCRIPTION_SHARE ss,
15:47:50 175  		   ACCOUNT a,
15:47:50 176  		   SUBSCRIPTION s,
15:47:50 177  		   ACCOUNT a2
15:47:50 178  		 WHERE
15:47:50 179  		   a.GROUP_ID	       = in_group_id AND
15:47:50 180  		   a.ID 	       = ss.BORROWER_ACCOUNT_ID AND
15:47:50 181  		   SYSDATE BETWEEN START_DATE AND END_DATE AND
15:47:50 182  		   ss.GROUP_ACCOUNT_ID = ga.ID AND
15:47:50 183  		   ga.SUBSCRIPTION_ID  = s.ID  AND
15:47:50 184  		   s.ACCOUNT_ID        = a2.ID
15:47:50 185  		) Q
15:47:50 186  	      WHERE rownum <= l_end)
15:47:50 187  	    WHERE rnum >= l_start;
15:47:50 188  	  END;
15:47:50 189  	END IF;
