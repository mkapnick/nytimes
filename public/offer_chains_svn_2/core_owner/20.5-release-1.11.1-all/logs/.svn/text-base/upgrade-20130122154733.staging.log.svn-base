15:47:50 958  	  -- Check for overlapping ip address range after insert.  Note that if another
15:47:50 959  	  -- call to add_ip_range has not completed, overlapping ip entries can occur.
15:47:50 960  	  SELECT count(1) into var_is_dupe
15:47:50 961  	  FROM
15:47:50 962  	    GROUP_ACCOUNT_IP_RANGE
15:47:50 963  	  WHERE
15:47:50 964  	    GROUP_ACC_IP_RNG_STATUS_ID = GLOBAL_STATUSES_V17.GROUP_ACC_IP_RNG_ACTIVE AND
15:47:50 965  		((
15:47:50 966  		  (in_minimum_ip_high > minimum_ip_high and in_minimum_ip_high < maximum_ip_high) or
15:47:50 967  		  (in_minimum_ip_high = minimum_ip_high and (in_minimum_ip_low >= minimum_ip_low and in_minimum_ip_low <= maximum_ip_low)) or
15:47:50 968  		  (in_minimum_ip_high = maximum_ip_high and (in_minimum_ip_low >= minimum_ip_low and in_minimum_ip_low <= maximum_ip_low))
15:47:50 969  		) OR
15:47:50 970  
15:47:50 971  		(
15:47:50 972  		  (in_maximum_ip_high > minimum_ip_high and in_maximum_ip_high < maximum_ip_high) or
15:47:50 973  		  (in_maximum_ip_high = minimum_ip_high and (in_maximum_ip_low >= minimum_ip_low and in_maximum_ip_low <= maximum_ip_low)) or
