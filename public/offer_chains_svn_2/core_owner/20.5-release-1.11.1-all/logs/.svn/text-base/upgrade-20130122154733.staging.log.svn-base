15:47:49 417  	WHERE
15:47:49 418  	  CHARGE.INVOICE_ID = in_invoice_id
15:47:49 419  	  AND CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V17.INSTRUMENT_PAYPAL;
15:47:49 420  
15:47:49 421  	-- get all credir cards for given invoice
