15:47:49  58  Throws exceptions:
15:47:49  59  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:49  60  */
15:47:49  61  	out_result_set1      OUT SYS_REFCURSOR,
15:47:49  62  	out_result_set2      OUT SYS_REFCURSOR,
15:47:49  63  	out_result_set3      OUT SYS_REFCURSOR,
15:47:49  64  	in_row_number	     IN NUMBER DEFAULT NULL
15:47:49  65  ) AS
15:47:49  66  SPROC_NAME CONSTANT VARCHAR2(20) := 'GET_PENDING_INVOICES';
15:47:49  67  -- COMSTANTS
15:47:49  68  DEFAULT_ROW_NUMBER CONSTANT NUMBER := 1;
15:47:49  69  -- VARIABLES
15:47:49  70  var_row_number NUMBER;
15:47:49  71  BEGIN
15:47:49  72  
15:47:49  73  	IF in_row_number IS NULL THEN
15:47:49  74  	  var_row_number := DEFAULT_ROW_NUMBER;
15:47:49  75  	ELSE
15:47:49  76  	  var_row_number := in_row_number;
15:47:49  77  	END IF;
15:47:49  78  
15:47:49  79  	-- Invoices with one or more payments(charges) with transaction status PENDING
15:47:49  80  	OPEN out_result_set1 FOR
15:47:49  81  SELECT * FROM
15:47:49  82  (
15:47:49  83  	SELECT
15:47:49  84  	  INVOICE.ID
15:47:49  85  	FROM
15:47:49  86  	  CHARGE
15:47:49  87  	  INNER JOIN INVOICE ON CHARGE.INVOICE_ID = INVOICE.ID
15:47:49  88  	WHERE
15:47:49  89  	  EXISTS(
15:47:49  90  	    SELECT NULL
15:47:49  91  	    FROM TRANSACTION
15:47:49  92  	    WHERE
15:47:49  93  	      TRANSACTION.TRANSACTION_STATUS_ID = GLOBAL_STATUSES_V17.TRANSACTION_PENDING
15:47:49  94  	      AND TRANSACTION.ID = CHARGE.TRANSACTION_ID
15:47:49  95  	      AND TRANSACTION.IS_REFUND != GLOBAL_CONSTANTS_V17.TRUE
15:47:49  96  	      AND TRANSACTION.TRANSACTION_AMOUNT >= 0
15:47:49  97  	  )
15:47:49  98  	  AND
15:47:49  99  	  NOT EXISTS(
15:47:49 100  	    SELECT NULL
15:47:49 101  	    FROM PROCESS_RETRY_THROTTLE
15:47:49 102  	    WHERE PROCESS_NAME = SPROC_NAME
15:47:49 103  	      AND GENERIC_ID = INVOICE.ID
15:47:49 104  	  )
15:47:49 105  	  AND ROWNUM <= var_row_number*10
15:47:49 106  	  ORDER BY dbms_random.value
15:47:49 107  ) WHERE
15:47:49 108  	  ROWNUM <= var_row_number;
15:47:49 109  
15:47:49 110  	-- Invoices not marked as CLOSED but are fully paid (shouldn't happen).
15:47:49 111  	OPEN out_result_set2 FOR
15:47:49 112  	SELECT
15:47:49 113  	  INVOICE.ID
15:47:49 114  	FROM
15:47:49 115  	  INVOICE
15:47:49 116  	WHERE
15:47:49 117  	  1 = 2 AND
15:47:49 118  	  (
15:47:49 119  	    INVOICE.INVOICE_STATUS_ID = GLOBAL_STATUSES_V17.INVOICE_OPEN
15:47:49 120  	  )
15:47:49 121  	  AND NOT EXISTS(
15:47:49 122  	    SELECT 1 FROM CHARGE WHERE INVOICE_ID=INVOICE.ID AND CHARGE_STATUS_ID = GLOBAL_STATUSES_V17.CHARGE_OPENED AND ROWNUM <= 1
15:47:49 123  	  )
15:47:49 124  	  AND EXISTS (
15:47:49 125  	    SELECT 1 FROM CHARGE WHERE INVOICE_ID=INVOICE.ID AND CHARGE_STATUS_ID != GLOBAL_STATUSES_V17.CHARGE_OPENED AND ROWNUM <= 1
15:47:49 126  	  )
15:47:49 127  	  AND INVOICE.ID NOT IN
15:47:49 128  	    (
15:47:49 129  	      SELECT GENERIC_ID
15:47:49 130  	      FROM PROCESS_RETRY_THROTTLE
15:47:49 131  	      WHERE
15:47:49 132  		PROCESS_NAME = SPROC_NAME
15:47:49 133  	    )
15:47:49 134  	  AND ROWNUM <= var_row_number;
15:47:49 135  
15:47:49 136  	-- Invoices not marked as CLOSED with no payments(charges).
15:47:49 137  	OPEN out_result_set3 FOR
15:47:49 138  	SELECT
15:47:49 139  	  INVOICE.ID
15:47:49 140  	FROM
15:47:49 141  	  INVOICE
15:47:49 142  	WHERE
15:47:49 143  	  1 = 2 AND
15:47:49 144  	  (
15:47:49 145  	    INVOICE.INVOICE_STATUS_ID = GLOBAL_STATUSES_V17.INVOICE_OPEN
15:47:49 146  	  )
15:47:49 147  	  AND NOT EXISTS (
15:47:49 148  	    SELECT 1 FROM CHARGE WHERE CHARGE.INVOICE_ID = INVOICE.ID AND ROWNUM <= 1
15:47:49 149  	  )
15:47:49 150  	  AND INVOICE.ID NOT IN
15:47:49 151  	    (
15:47:49 152  	      SELECT GENERIC_ID
15:47:49 153  	      FROM PROCESS_RETRY_THROTTLE
15:47:49 154  	      WHERE
15:47:49 155  		PROCESS_NAME = SPROC_NAME
15:47:49 156  	    )
15:47:49 157  	  AND ROWNUM <= var_row_number;
