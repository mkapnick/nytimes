15:47:48 106  FUNCTION CREATE_NEW_TRANSFER_SET
15:47:48 107  	  (
15:47:48 108  	      in_set_maximum NUMBER)
15:47:48 109  	  RETURN core_owner.polling_sync.set_id%type
15:47:48 110  IS
15:47:48 111  	  pragma autonomous_transaction;
15:47:48 112  	  v_set_id core_owner.polling_sync.set_id%type;
15:47:48 113  BEGIN
15:47:48 114  	  SELECT
15:47:48 115  	      pollsync_setid_seq.nextval
15:47:48 116  	  INTO
15:47:48 117  	      v_set_id
15:47:48 118  	  FROM
15:47:48 119  	      dual;
15:47:48 120  	  update
15:47:48 121  	    core_owner.polling_sync ps
15:47:48 122  	  set
15:47:48 123  	    ps.set_id = v_set_id
15:47:48 124  	  where
15:47:48 125  	    ps.set_id IS NULL and
15:47:48 126  	    rownum <= in_set_maximum
15:47:48 127  	  ;
15:47:48 128  	  COMMIT;
15:47:48 129  	  RETURN v_set_id;
15:47:48 130  END;
15:47:48 131  --------------------------------------------------------------------------------
15:47:48 132  FUNCTION RETREIVE_TRANSFER_SET
15:47:48 133  	  (
15:47:48 134  	      in_set_id core_owner.polling_sync.set_id%type)
15:47:48 135  	  RETURN sys_refcursor
15:47:48 136  IS
15:47:48 137  	  v_refcursor sys_refcursor;
15:47:48 138  BEGIN
15:47:48 139  	  UPDATE
15:47:48 140  	      core_owner.polling_sync ps
15:47:48 141  	  SET
15:47:48 142  	      ps.last_send_date = sysdate,
15:47:48 143  	      ps.num_calls = ps.num_calls + 1
15:47:48 144  	  WHERE
15:47:48 145  	      ps.set_id = in_set_id ;
15:47:48 146  	  OPEN v_refcursor FOR
15:47:48 147  	  SELECT
15:47:48 148  	      ps.set_id,
15:47:48 149  	      ps.group_id,
15:47:48 150  	      ps.event_type,
15:47:48 151  	      ps.event_date
15:47:48 152  	  FROM
15:47:48 153  	      core_owner.polling_sync ps
15:47:48 154  	  WHERE
15:47:48 155  	      ps.set_id = in_set_id
15:47:48 156  	  AND ps.group_id IS NOT NULL ;
15:47:48 157  	  RETURN v_refcursor;
15:47:48 158  END;
15:47:48 159  --------------------------------------------------------------------------------
15:47:48 160  PROCEDURE GET_TRANSFER_SET
15:47:48 161  	  (
15:47:48 162  	      in_set_maximum NUMBER,
15:47:48 163  	      in_max_retries NUMBER,
15:47:48 164  	      out_refcursor OUT sys_refcursor)
15:47:48 165  IS
15:47:48 166  	  v_set_id core_owner.polling_sync.set_id%type;
15:47:48 167  BEGIN
15:47:48 168  	  /* Look for previously sent but unconfirmed sets and
15:47:48 169  	  send again until max_retries calls */
15:47:48 170  	  FOR x IN
15:47:48 171  	  (
15:47:48 172  	      SELECT
15:47:48 173  		  ps.set_id,
15:47:48 174  		  COUNT( *) set_size
15:47:48 175  	      FROM
15:47:48 176  		  core_owner.polling_sync ps
15:47:48 177  	      WHERE
15:47:48 178  		  ps.event_date > sysdate - 14
15:47:48 179  	      AND ps.confirm_date IS NULL
15:47:48 180  	      AND ps.last_send_date IS NOT NULL
15:47:48 181  	      AND ps.num_calls < in_max_retries
15:47:48 182  	      GROUP BY
15:47:48 183  		  ps.set_id
15:47:48 184  	      ORDER BY
15:47:48 185  		  ps.set_id
15:47:48 186  	  )
15:47:48 187  	  LOOP
15:47:48 188  	      out_refcursor := Retreive_Transfer_Set(x.set_id) ;
15:47:48 189  	      RETURN;
15:47:48 190  	  END LOOP;
15:47:48 191  	  v_set_id := Create_New_Transfer_Set(in_set_maximum) ;
15:47:48 192  	  out_refcursor := Retreive_Transfer_Set(v_set_id) ;
15:47:48 193  	  RETURN;
15:47:48 194  END;
15:47:48 195  --------------------------------------------------------------------------------
15:47:48 196  PROCEDURE CONFIRM_TRANSFER_SET
15:47:48 197  	  (
15:47:48 198  	      in_set_id core_owner.polling_sync.set_id%type)
15:47:48 199  IS
15:47:48 200  	  v_unconfirmable EXCEPTION;
15:47:48 201  BEGIN
15:47:48 202  	  UPDATE
15:47:48 203  	      core_owner.polling_sync ps
15:47:48 204  	  SET
15:47:48 205  	      ps.confirm_date = systimestamp
15:47:48 206  	  WHERE
15:47:48 207  	      ps.set_id = in_set_id
15:47:48 208  	  AND ps.confirm_date IS NULL ;
15:47:48 209  	  IF(sql%rowcount < 1) THEN
15:47:48 210  	      raise v_unconfirmable;
15:47:48 211  	  END IF;
15:47:48 212  END;
15:47:48 213  PROCEDURE SET_LAST_RUN(ts in timestamp)
15:47:48 214  IS
15:47:48 215  BEGIN
15:47:48 216  	  UPDATE POLLING_SYNC_LASTRUN
15:47:48 217  	  SET last_run = current_timestamp;
15:47:48 218  	  IF ( sql%rowcount = 0 )
15:47:48 219  	  THEN
15:47:48 220  	    INSERT INTO POLLING_SYNC_LASTRUN VALUES (ts);
15:47:48 221  	  END if;
15:47:48 222  	  COMMIT;
15:47:48 223  END;
15:47:48 224  PROCEDURE GET_LAST_RUN(ts out timestamp)
15:47:48 225  IS
15:47:48 226  BEGIN
15:47:48 227  	  SELECT LAST_RUN INTO ts
15:47:48 228  	  FROM POLLING_SYNC_LASTRUN
15:47:48 229  	  WHERE ROWNUM < 2;
15:47:48 230  EXCEPTION
15:47:48 231  	WHEN NO_DATA_FOUND
15:47:48 232  	THEN
15:47:48 233  	  ts := current_timestamp;
15:47:48 234  END;
15:47:48 235  END PROCS_POLLING_SYNC;
