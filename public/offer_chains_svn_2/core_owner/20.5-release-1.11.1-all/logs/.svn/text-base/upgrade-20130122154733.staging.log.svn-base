15:47:49 460  PROCEDURE GET_TRANSACTION_INVOICE (
15:47:49 461  /*
15:47:49 462  Throws exceptions:
15:47:49 463  APP_EXCEPTION_CODES_V17.NOT_FOUND
15:47:49 464  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:49 465  */
15:47:49 466  	in_transaction_id  IN  NUMBER,
15:47:49 467  	out_result_set	      OUT SYS_REFCURSOR
15:47:49 468  ) AS
15:47:49 469  SPROC_NAME CONSTANT  VARCHAR2(23) := 'GET_TRANSACTION_INVOICE';
15:47:49 470  -- VARIABLES
15:47:49 471  temp_transaction_id  NUMBER;
15:47:49 472  var_invoice_id	   NUMBER;
15:47:49 473  var_subscription_id  NUMBER;
15:47:49 474  var_offer_chain_id   NUMBER;
15:47:49 475  var_offer_chain_name VARCHAR2(255);
15:47:49 476  -- EXCEPTIONS
15:47:49 477  BAD_TRANSACTION_ID     EXCEPTION;
15:47:49 478  EXCEPTION_MESSAGE      VARCHAR2(1024);
15:47:49 479  BEGIN
15:47:49 480   -- check if there is the transaction
15:47:49 481  	BEGIN
15:47:49 482  	  SELECT
15:47:49 483  	    TRANSACTION.ID into temp_transaction_id
15:47:49 484  	  FROM
15:47:49 485  	    TRANSACTION
15:47:49 486  	  WHERE
15:47:49 487  	    TRANSACTION.ID = in_transaction_id;
15:47:49 488  	  EXCEPTION
15:47:49 489  	    WHEN NO_DATA_FOUND THEN
15:47:49 490  	      RAISE BAD_TRANSACTION_ID;
15:47:49 491  	END;
15:47:49 492  
15:47:49 493  	-- Get invoice id
15:47:49 494  	SELECT DISTINCT
15:47:49 495  	  CHARGE.INVOICE_ID into var_invoice_id
15:47:49 496  	FROM
15:47:49 497  	  CHARGE
15:47:49 498  	WHERE
15:47:49 499  	  CHARGE.TRANSACTION_ID = in_transaction_id;
15:47:49 500  
15:47:49 501  	-- Get subscription id if exists
15:47:49 502  	BEGIN
15:47:49 503  	  SELECT DISTINCT
15:47:49 504  	    LICENSE.SUBSCRIPTION_ID into var_subscription_id
15:47:49 505  	  FROM
15:47:49 506  	    LICENSE
15:47:49 507  	  WHERE
15:47:49 508  	    LICENSE.INVOICE_ID = var_invoice_id;
15:47:49 509  	  EXCEPTION
15:47:49 510  	    WHEN NO_DATA_FOUND THEN
15:47:49 511  	      var_subscription_id := NULL;
15:47:49 512  	END;
15:47:49 513  
15:47:49 514  	IF var_subscription_id IS NOT NULL THEN
15:47:49 515  	  -- Fetch offer chain from subscription
15:47:49 516  	  SELECT
15:47:49 517  	    OFFER_CHAIN.ID,
15:47:49 518  	    OFFER_CHAIN.NAME
15:47:49 519  	    into
15:47:49 520  	    var_offer_chain_id,
15:47:49 521  	    var_offer_chain_name
15:47:49 522  	  FROM
15:47:49 523  	    OFFER_CHAIN
15:47:49 524  	    INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
15:47:49 525  	  WHERE
15:47:49 526  	    SUBSCRIPTION.ID = var_subscription_id;
15:47:49 527  	ELSE
15:47:49 528  	  -- Fetch offer chain from GC
15:47:49 529  	  SELECT
15:47:49 530  	    OFFER_CHAIN.ID,
15:47:49 531  	    OFFER_CHAIN.NAME
15:47:49 532  	    into
15:47:49 533  	    var_offer_chain_id,
15:47:49 534  	    var_offer_chain_name
15:47:49 535  	  FROM
15:47:49 536  	    OFFER_CHAIN
15:47:49 537  	    INNER JOIN GIFT_CERTIFICATE ON GIFT_CERTIFICATE.OFFER_CHAIN_ID = OFFER_CHAIN.ID
15:47:49 538  	  WHERE
15:47:49 539  	    GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = var_invoice_id;
15:47:49 540  	END IF;
15:47:49 541  
15:47:49 542  	OPEN out_result_set FOR
15:47:49 543  	SELECT DISTINCT
15:47:49 544  	  var_invoice_id       AS "INVOICE_ID",
15:47:49 545  	  var_subscription_id  AS "SUBSCRIPTION_ID",
15:47:49 546  	  var_offer_chain_id   AS "OFFER_CHAIN_ID",
15:47:49 547  	  var_offer_chain_name AS "OFFER_CHAIN_NAME"
15:47:49 548  	FROM
15:47:49 549  	  DUAL;
15:47:49 550  
15:47:49 551  EXCEPTION
15:47:49 552  WHEN BAD_TRANSACTION_ID THEN
15:47:49 553  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 554  	  SPROC_NAME, 'No such transaction');
15:47:49 555  WHEN OTHERS THEN
15:47:49 556  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 557  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:49 558  END GET_TRANSACTION_INVOICE;
