15:47:49 343  	in_instrument_id      IN NUMBER,
15:47:49 344  	in_created_by	      IN VARCHAR2
15:47:49 345  ) AS
15:47:49 346  SPROC_NAME CONSTANT VARCHAR2(30) := 'FINALIZE_SUBSCRIPTION_CREATION';
15:47:49 347  -- VARIABLES
15:47:49 348  var_invoice_amount     NUMBER(10, 2);
15:47:49 349  var_new_transaction_id NUMBER;
15:47:49 350  var_new_charge_id      NUMBER;
15:47:49 351  -- EXCEPTIONS
15:47:49 352  CAN_NOT_USE_FCINSTR	  EXCEPTION;
15:47:49 353  CAN_NOT_CALC_INVOICE_AMOUNT EXCEPTION;
15:47:49 354  CAN_NOT_CREATE_TRANSACTION  EXCEPTION;
15:47:49 355  CAN_NOT_CREATE_CHARGE	  EXCEPTION;
15:47:49 356  
15:47:49 357  EXCEPTION_MESSAGE VARCHAR2(1024);
15:47:49 358  BEGIN
15:47:49 359  
15:47:49 360  	-- Calculate invoice amount ( + discounts, taxes)
15:47:49 361  	BEGIN
15:47:49 362  	  PROCS_INVOICE_V17.CALCULATE_INVOICE_AMOUNT(
15:47:49 363  	    in_invoice_id => in_invoice_id,
15:47:49 364  	    out_amount	  => var_invoice_amount
15:47:49 365  	  );
15:47:49 366  	  EXCEPTION
15:47:49 367  	   WHEN OTHERS THEN
15:47:49 368  	     EXCEPTION_MESSAGE := SQLERRM;
15:47:49 369  	     RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
15:47:49 370  	END;
15:47:49 371  
15:47:49 372  	IF in_instrument_type_id = GLOBAL_ENUMS_V17.INSTRUMENT_ZCI_INSTRUMENT
15:47:49 373  	  AND var_invoice_amount > 0 THEN
15:47:49 374  	  RAISE CAN_NOT_USE_FCINSTR;
15:47:49 375  	END IF;
15:47:49 376  
15:47:49 377  	IF var_invoice_amount = 0 THEN
15:47:49 378  	  -- UPDATE INVOICE. SET STATUS TO PROCESSED
15:47:49 379  	  PROCS_INVOICE_CRU_V17.UPDATE_INVOICE(
15:47:49 380  	    in_invoice_id		   => in_invoice_id,
15:47:49 381  	    in_updated_by		   => in_created_by,
15:47:49 382  	    in_invoice_status_id	   => GLOBAL_STATUSES_V17.INVOICE_CLOSED
15:47:49 383  	  );
15:47:49 384  	ELSE
15:47:49 385  	  -- Create transaction and charge
15:47:49 386  	  BEGIN
15:47:49 387  	    PROCS_TRANSACTION_V17.CREATE_TRANSACTION(
15:47:49 388  	      in_transaction_id  => NULL,
15:47:49 389  	      in_status_id	 => GLOBAL_STATUSES_V17.TRANSACTION_PENDING,
15:47:49 390  	      in_amount 	 => var_invoice_amount,
15:47:49 391  	      in_created_by	 => in_created_by,
15:47:49 392  	      in_order_id	 => NULL,
15:47:49 393  	      out_transaction_id => var_new_transaction_id
15:47:49 394  	    );
15:47:49 395  	    EXCEPTION
15:47:49 396  	      WHEN OTHERS THEN
15:47:49 397  		EXCEPTION_MESSAGE := SQLERRM;
15:47:49 398  		RAISE CAN_NOT_CREATE_TRANSACTION;
15:47:49 399  	  END;
15:47:49 400  
15:47:49 401  	  BEGIN
15:47:49 402  	    PROCS_CHARGE_V17.CREATE_CHARGE(
15:47:49 403  	      in_invoice_id	    => in_invoice_id,
15:47:49 404  	      in_transaction_id     => var_new_transaction_id,
15:47:49 405  	      in_instrument_type_id => in_instrument_type_id,
15:47:49 406  	      in_instrument_id	    => in_instrument_id,
15:47:49 407  	      in_charge_amount	    => var_invoice_amount,
15:47:49 408  	      in_created_by	    => in_created_by,
15:47:49 409  	      in_charge_status_id   => GLOBAL_STATUSES_V17.CHARGE_OPENED,
15:47:49 410  	      out_charge_id	    => var_new_charge_id
15:47:49 411  	    );
15:47:49 412  	    EXCEPTION
15:47:49 413  	      WHEN OTHERS THEN
15:47:49 414  		EXCEPTION_MESSAGE := SQLERRM;
15:47:49 415  		RAISE CAN_NOT_CREATE_CHARGE;
15:47:49 416  	  END;
15:47:49 417  	END IF;
15:47:49 418  
15:47:49 419  EXCEPTION
15:47:49 420  WHEN CAN_NOT_USE_FCINSTR THEN
15:47:49 421  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.STATE_ERROR,
15:47:49 422  	  SPROC_NAME, 'Could not use "free charge instrument" for non-zero invoice');
15:47:49 423  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
15:47:49 424  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:49 425  	  SPROC_NAME, 'Could not calculate invoice amount', EXCEPTION_MESSAGE);
15:47:49 426  WHEN CAN_NOT_CREATE_TRANSACTION THEN
15:47:49 427  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:49 428  	  SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
15:47:49 429  WHEN CAN_NOT_CREATE_CHARGE THEN
15:47:49 430  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:49 431  	  SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
15:47:49 432  WHEN OTHERS THEN
15:47:49 433  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 434  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:49 435  END FINALIZE_SUBSCRIPTION_CREATION;
