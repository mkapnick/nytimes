15:47:49 1422  	     AND PROCS_COMMON_V17.NORMALIZE_DATE(LICENSE.START_DATE) <= var_current_date
15:47:49 1423  	     AND ROWNUM <= 1
15:47:49 1424  	   ORDER BY
15:47:49 1425  	     LICENSE.ID DESC;
15:47:49 1426  	   EXCEPTION
15:47:49 1427  	     WHEN NO_DATA_FOUND THEN
15:47:49 1428  	       RAISE CAN_NOT_FIND_OFFER_OR_LICENSE;
15:47:49 1429  	 END;
15:47:49 1430  
15:47:49 1431  	 IF var_current_license_end_date < in_effective_start_date THEN
15:47:49 1432  	   RAISE EXTENS_START_DATE_IS_TOO_FAR;
15:47:49 1433  	 END IF;
15:47:49 1434  
