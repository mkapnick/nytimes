15:47:48 154  	      in_discount_id =>  f_discount.ID,
15:47:48 155  	      in_line_item_id => var_new_line_item_id
15:47:48 156  	    );
15:47:48 157  	  END LOOP;
15:47:48 158  	END LOOP;
15:47:48 159  
15:47:48 160  EXCEPTION
