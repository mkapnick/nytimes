15:47:49 400  FUNCTION IS_CHARGE_COLLECTED (
15:47:49 401  /*
15:47:49 402  Throws:
15:47:49 403  APP_EXCEPTION_CODES_V17.NOT_FOUND
15:47:49 404  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:49 405  Returns:
15:47:49 406  GLOBAL_CONST.TRUE if transaction collected,
15:47:49 407  GLOBAL_CONST.FALSE else
15:47:49 408  */
15:47:49 409  	in_charge_id IN NUMBER
15:47:49 410  ) RETURN NUMBER AS
15:47:49 411  SPROC_NAME CONSTANT VARCHAR2(19) := 'IS_CHARGE_COLLECTED';
15:47:49 412  -- VARIABLES
15:47:49 413  var_transaction_id NUMBER;
15:47:49 414  is_transaction_collected NUMBER;
15:47:49 415  -- EXCEPTIONS
15:47:49 416  BAD_CHARGE_ID	       EXCEPTION;
15:47:49 417  CAN_NOT_CHECK_TRANSACTION EXCEPTION;
15:47:49 418  EXCEPTION_MESSAGE        VARCHAR2(1024);
15:47:49 419  BEGIN
15:47:49 420  
15:47:49 421  	BEGIN
15:47:49 422  	  SELECT
15:47:49 423  	    CHARGE.TRANSACTION_ID into var_transaction_id
15:47:49 424  	  FROM
15:47:49 425  	    CHARGE
15:47:49 426  	  WHERE
15:47:49 427  	    CHARGE.ID = in_charge_id;
15:47:49 428  	  EXCEPTION
15:47:49 429  	    WHEN NO_DATA_FOUND THEN
15:47:49 430  	      RAISE BAD_CHARGE_ID;
15:47:49 431  	END;
15:47:49 432  
15:47:49 433  	BEGIN
15:47:49 434  	  is_transaction_collected := PROCS_TRANSACTION_V17.IS_TRANSACTION_COLLECTED(var_transaction_id);
15:47:49 435  	  EXCEPTION
15:47:49 436  	    WHEN OTHERS THEN
15:47:49 437  	      EXCEPTION_MESSAGE := SQLERRM;
15:47:49 438  	      RAISE CAN_NOT_CHECK_TRANSACTION;
15:47:49 439  	END;
15:47:49 440  
15:47:49 441  	RETURN is_transaction_collected;
15:47:49 442  
15:47:49 443  EXCEPTION
15:47:49 444  WHEN BAD_CHARGE_ID THEN
15:47:49 445  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 446  	  SPROC_NAME, 'No such charge');
15:47:49 447  WHEN CAN_NOT_CHECK_TRANSACTION THEN
15:47:49 448  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:49 449  	  SPROC_NAME, 'Could not check if transaction was collected', EXCEPTION_MESSAGE);
15:47:49 450  WHEN OTHERS THEN
15:47:49 451  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 452  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:49 453  END;
