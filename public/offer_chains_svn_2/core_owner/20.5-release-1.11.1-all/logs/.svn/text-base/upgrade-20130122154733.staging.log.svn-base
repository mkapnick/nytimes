15:47:49 1830  PROCEDURE CLOSE_SUBSCRIPTION (
15:47:49 1831  	 in_subscription_id IN NUMBER,
15:47:49 1832  	 in_updated_by	    IN VARCHAR2
15:47:49 1833  ) AS
15:47:49 1834  -- VARIABLES
15:47:49 1835  SPROC_NAME	    CONSTANT VARCHAR2(18) := 'CLOSE_SUBSCRIPTION';
15:47:49 1836  temp_subscription_id NUMBER;
15:47:49 1837  var_licenses_count   NUMBER;
15:47:49 1838  -- EXCEPTIONS
15:47:49 1839  BAD_SUBSCRIPTION_ID    EXCEPTION;
15:47:49 1840  ACTIVE_LICENSES_FOUND  EXCEPTION;
15:47:49 1841  EXCEPTION_MESSAGE      VARCHAR2(1024);
15:47:49 1842  BEGIN
15:47:49 1843  
15:47:49 1844  	 BEGIN
15:47:49 1845  	   SELECT
15:47:49 1846  	     SUBSCRIPTION.ID into temp_subscription_id
15:47:49 1847  	   FROM
15:47:49 1848  	     SUBSCRIPTION
15:47:49 1849  	   WHERE
15:47:49 1850  	     SUBSCRIPTION.ID = in_subscription_id;
15:47:49 1851  	   EXCEPTION
15:47:49 1852  	     WHEN NO_DATA_FOUND THEN
15:47:49 1853  	       RAISE BAD_SUBSCRIPTION_ID;
15:47:49 1854  	 END;
15:47:49 1855  
15:47:49 1856  	 SELECT
15:47:49 1857  	   COUNT(*) into var_licenses_count
15:47:49 1858  	 FROM
15:47:49 1859  	   LICENSE
15:47:49 1860  	 WHERE
15:47:49 1861  	   LICENSE.SUBSCRIPTION_ID = in_subscription_id
15:47:49 1862  	   AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V17.LICENSE_ACTIVE;
15:47:49 1863  
15:47:49 1864  	 IF var_licenses_count > 0 THEN
15:47:49 1865  	   RAISE ACTIVE_LICENSES_FOUND;
15:47:49 1866  	 END IF;
15:47:49 1867  
15:47:49 1868  	 PROCS_SUBSCRIPTION_V17.UPDATE_SUBSCRIPTION_STATUS(
15:47:49 1869  	   in_subscription_id	     => in_subscription_id,
15:47:49 1870  	   in_updated_by	     => in_updated_by,
15:47:49 1871  	   in_subscription_status_id => GLOBAL_STATUSES_V17.SUBSCRIPTION_CLOSED
15:47:49 1872  	 );
15:47:49 1873  
15:47:49 1874  EXCEPTION
15:47:49 1875  WHEN BAD_SUBSCRIPTION_ID THEN
15:47:49 1876  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 1877  	   SPROC_NAME, 'No such subscription');
