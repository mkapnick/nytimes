15:47:50 975  		)) AND
15:47:50 976  	    ROWNUM < 3;
15:47:50 977  
15:47:50 978  	  If(var_is_dupe > 1) THEN
15:47:50 979  	    RAISE DUPE;
15:47:50 980  	  END IF;
