15:47:50 1388  	   SPROC_NAME, 'No such group id');
15:47:50 1389  WHEN BAD_OFFER_CHAIN_ID THEN
15:47:50 1390  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 1391  	   SPROC_NAME, 'No such offer chain');
15:47:50 1392  WHEN OFFER_REC_NUM_LESS_THAN_ONE THEN
15:47:50 1393  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50 1394  	   SPROC_NAME, 'Some offer has recurrences number less than 1');
15:47:50 1395  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
15:47:50 1396  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50 1397  	   SPROC_NAME, 'COuold not calculate amount for new invoice', EXCEPTION_MESSAGE);
15:47:50 1398  WHEN OTHERS THEN
15:47:50 1399  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:50 1400  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:47:50 1401  END PURCHASE_GIFT_CERTIFICATE;
