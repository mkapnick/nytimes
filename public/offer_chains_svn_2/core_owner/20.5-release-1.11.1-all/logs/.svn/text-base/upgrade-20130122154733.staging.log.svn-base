15:47:49 1570  
15:47:49 1571  FUNCTION CALC_SUBSCRIPTION_END_DATE (
15:47:49 1572  /*
15:47:49 1573  APP_EXCEPTION_CODES_V17.NOT_FOUND
15:47:49 1574  APP_EXCEPTION_CODES_V17.INTERNAL_ERROR
15:47:49 1575  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:49 1576  Returns:
15:47:49 1577  NULL if it is impossible to calculate end date (for example,
15:47:49 1578  	 offer chain includes offer with infinity recurrences number)
15:47:49 1579  DATE else
15:47:49 1580  */
15:47:49 1581  	 in_subscription_id IN NUMBER
15:47:49 1582  ) RETURN DATE AS
15:47:49 1583  -- VARIABLES
15:47:49 1584  SPROC_NAME		     CONSTANT VARCHAR2(26) := 'CALC_SUBSCRIPTION_END_DATE';
15:47:49 1585  last_license_id		     NUMBER;
15:47:49 1586  last_license_end_date	     DATE;
15:47:49 1587  last_license_offer_id	     NUMBER;
15:47:49 1588  last_license_offer_index      NUMBER;
15:47:49 1589  last_license_offer_recurr_num NUMBER;
15:47:49 1590  var_last_license_id	     NUMBER;
15:47:49 1591  var_offer_chain_id	     NUMBER;
15:47:49 1592  var_current_offer_rec_number  NUMBER;
15:47:49 1593  var_next_offers_set	     SYS_REFCURSOR;
15:47:49 1594  var_next_offer_duration	     VARCHAR2(30);
15:47:49 1595  var_next_offer_recur	     NUMBER;
15:47:49 1596  var_infinity_offers_number    NUMBER;
15:47:49 1597  
15:47:49 1598  var_result_date DATE;
15:47:49 1599  
15:47:49 1600  var_ym_interval	INTERVAL YEAR TO MONTH;
15:47:49 1601  var_ds_interval	INTERVAL DAY(3) TO SECOND;
15:47:49 1602  var_offer_years	NUMBER;
15:47:49 1603  var_offer_months NUMBER;
15:47:49 1604  var_offer_days	NUMBER;
15:47:49 1605  
15:47:49 1606  -- EXCEPTIONS
15:47:49 1607  BAD_SUBSCRIPTION_ID	  EXCEPTION;
15:47:49 1608  CAN_NOT_FIND_LAST_LICENSE  EXCEPTION;
15:47:49 1609  CAN_NOT_CALC_OFFER_LENGTH  EXCEPTION;
15:47:49 1610  CAN_NOT_CALC_OFFER_LENGTH2 EXCEPTION;
15:47:49 1611  EXCEPTION_MESSAGE	  VARCHAR2(1024);
15:47:49 1612  BEGIN
15:47:49 1613  
15:47:49 1614  	 BEGIN
15:47:49 1615  	   SELECT
15:47:49 1616  	     SUBSCRIPTION.OFFER_CHAIN_ID into var_offer_chain_id
15:47:49 1617  	   FROM
15:47:49 1618  	     SUBSCRIPTION
15:47:49 1619  	   WHERE
15:47:49 1620  	     SUBSCRIPTION.ID = in_subscription_id;
15:47:49 1621  	   EXCEPTION
15:47:49 1622  	     WHEN NO_DATA_FOUND THEN
15:47:49 1623  	       RAISE BAD_SUBSCRIPTION_ID;
15:47:49 1624  	 END;
15:47:49 1625  
15:47:49 1626  	 BEGIN
15:47:49 1627  	   SELECT
15:47:49 1628  	     ID into var_last_license_id
15:47:49 1629  	   FROM
15:47:49 1630  	     (
15:47:49 1631  	       SELECT
15:47:49 1632  		 LICENSE.ID
15:47:49 1633  	       FROM
15:47:49 1634  		 LICENSE
15:47:49 1635  	       WHERE
15:47:49 1636  		 LICENSE.SUBSCRIPTION_ID = in_subscription_id
15:47:49 1637  	       ORDER BY
15:47:49 1638  		 LICENSE.END_DATE DESC
15:47:49 1639  	     )
15:47:49 1640  	   WHERE
15:47:49 1641  	     ROWNUM <= 1;
15:47:49 1642  	   EXCEPTION
15:47:49 1643  	     WHEN NO_DATA_FOUND THEN
15:47:49 1644  	       RAISE CAN_NOT_FIND_LAST_LICENSE;
15:47:49 1645  	 END;
15:47:49 1646  
15:47:49 1647  	 SELECT
15:47:49 1648  	   COUNT(*) into var_infinity_offers_number
15:47:49 1649  	 FROM
15:47:49 1650  	   OFFER_OFFER_CHAIN
15:47:49 1651  	 WHERE
15:47:49 1652  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
15:47:49 1653  	   AND OFFER_OFFER_CHAIN.NUM_RECURRENCES = GLOBAL_ENUMS_V17.OFFER_REC_INFINITY;
15:47:49 1654  
15:47:49 1655  	 IF var_infinity_offers_number > 0 THEN
15:47:49 1656  	   RETURN NULL;
15:47:49 1657  	 END IF;
15:47:49 1658  
15:47:49 1659  	 BEGIN
15:47:49 1660  	   SELECT
15:47:49 1661  	     LICENSE.ID,
15:47:49 1662  	     LICENSE.END_DATE,
15:47:49 1663  	     LICENSE.CURRENT_OFFER_INDEX,
15:47:49 1664  	     LICENSE.CURRENT_OFFER_RECURR_NUM,
15:47:49 1665  	     LICENSE.OFFER_ID
15:47:49 1666  	     into
15:47:49 1667  	     last_license_id,
15:47:49 1668  	     last_license_end_date,
15:47:49 1669  	     last_license_offer_index,
15:47:49 1670  	     last_license_offer_recurr_num,
15:47:49 1671  	     last_license_offer_id
15:47:49 1672  	   FROM
15:47:49 1673  	     LICENSE
15:47:49 1674  	   WHERE
15:47:49 1675  	     LICENSE.ID = var_last_license_id;
15:47:49 1676  	   EXCEPTION
15:47:49 1677  	     WHEN NO_DATA_FOUND THEN
15:47:49 1678  	       RAISE CAN_NOT_FIND_LAST_LICENSE;
15:47:49 1679  	 END;
15:47:49 1680  
15:47:49 1681  	 var_result_date := last_license_end_date;
15:47:49 1682  
15:47:49 1683  	 -- Find current recurrence number
15:47:49 1684  	 SELECT
15:47:49 1685  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES into var_current_offer_rec_number
15:47:49 1686  	 FROM
15:47:49 1687  	   OFFER_OFFER_CHAIN
15:47:49 1688  	 WHERE
15:47:49 1689  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
15:47:49 1690  	   AND OFFER_OFFER_CHAIN.OFFER_ID = last_license_offer_id
15:47:49 1691  	   AND OFFER_OFFER_CHAIN.INDEX_VALUE = last_license_offer_index;
15:47:49 1692  
15:47:49 1693  	 IF var_current_offer_rec_number > last_license_offer_recurr_num THEN
15:47:49 1694  	   BEGIN
15:47:49 1695  	     PROCS_OFFER_CHAIN_V17.GET_OFFER_LENGTH(
15:47:49 1696  	       last_license_offer_id,
15:47:49 1697  	       var_offer_years,
15:47:49 1698  	       var_offer_months,
15:47:49 1699  	       var_offer_days
15:47:49 1700  	     );
15:47:49 1701  
15:47:49 1702  	     var_ym_interval := var_offer_years||'-'||var_offer_months;
15:47:49 1703  	     var_ds_interval := var_offer_days||' 0:0:0';
15:47:49 1704  
15:47:49 1705  	     var_result_date := var_result_date
15:47:49 1706  	       + ( var_ym_interval * ( var_current_offer_rec_number - last_license_offer_recurr_num ) )
15:47:49 1707  	       + ( var_ds_interval * ( var_current_offer_rec_number - last_license_offer_recurr_num ) );
15:47:49 1708  	     EXCEPTION
15:47:49 1709  	       WHEN OTHERS THEN
15:47:49 1710  		 EXCEPTION_MESSAGE := SQLERRM;
15:47:49 1711  		 RAISE CAN_NOT_CALC_OFFER_LENGTH;
15:47:49 1712  	   END;
15:47:49 1713  	 END IF;
15:47:49 1714  
15:47:49 1715  	 OPEN var_next_offers_set FOR
15:47:49 1716  	 SELECT
15:47:49 1717  	   OFFER.ENTITLEMENT_DURATION,
15:47:49 1718  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES
15:47:49 1719  	 FROM
15:47:49 1720  	   OFFER_OFFER_CHAIN
15:47:49 1721  	   INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
15:47:49 1722  	 WHERE
15:47:49 1723  	   OFFER_OFFER_CHAIN.INDEX_VALUE > last_license_offer_index
15:47:49 1724  	   AND OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id;
15:47:49 1725  
15:47:49 1726  	 LOOP
15:47:49 1727  	   FETCH var_next_offers_set into var_next_offer_duration, var_next_offer_recur;
15:47:49 1728  	   EXIT WHEN var_next_offers_set%NOTFOUND;
15:47:49 1729  	   BEGIN
15:47:49 1730  	     PROCS_COMMON_V17.ISO8601DURATION_TO_INTERVALS(
15:47:49 1731  	       var_next_offer_duration,
15:47:49 1732  	       var_offer_years,
15:47:49 1733  	       var_offer_months,
15:47:49 1734  	       var_offer_days
15:47:49 1735  	     );
15:47:49 1736  
15:47:49 1737  	     var_ym_interval := var_offer_years||'-'||var_offer_months;
15:47:49 1738  	     var_ds_interval := var_offer_days||' 0:0:0';
15:47:49 1739  
15:47:49 1740  	     var_result_date := var_result_date
15:47:49 1741  	       + ( var_ym_interval * var_next_offer_recur )
15:47:49 1742  	       + ( var_ds_interval * var_next_offer_recur );
15:47:49 1743  	     EXCEPTION
15:47:49 1744  	       WHEN OTHERS THEN
15:47:49 1745  		 EXCEPTION_MESSAGE := SQLERRM;
15:47:49 1746  		 RAISE CAN_NOT_CALC_OFFER_LENGTH2;
15:47:49 1747  	   END;
15:47:49 1748  	 END LOOP;
15:47:49 1749  
15:47:49 1750  	 RETURN var_result_date;
15:47:49 1751  
15:47:49 1752  EXCEPTION
15:47:49 1753  WHEN BAD_SUBSCRIPTION_ID THEN
15:47:49 1754  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 1755  	   SPROC_NAME, 'No such subscription');
15:47:49 1756  WHEN CAN_NOT_FIND_LAST_LICENSE THEN
15:47:49 1757  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 1758  	   SPROC_NAME, 'Could not find last license for given subscription');
15:47:49 1759  WHEN CAN_NOT_CALC_OFFER_LENGTH THEN
15:47:49 1760  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:49 1761  	   SPROC_NAME, 'Could not calculate offer length', EXCEPTION_MESSAGE);
15:47:49 1762  WHEN CAN_NOT_CALC_OFFER_LENGTH2 THEN
15:47:49 1763  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:49 1764  	   SPROC_NAME, 'Could not calculate last offer length', EXCEPTION_MESSAGE);
15:47:49 1765  WHEN OTHERS THEN
15:47:49 1766  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 1767  	   SPROC_NAME, 'Unkown error', SQLERRM);
15:47:49 1768  END CALC_SUBSCRIPTION_END_DATE;
15:47:49 1769  
