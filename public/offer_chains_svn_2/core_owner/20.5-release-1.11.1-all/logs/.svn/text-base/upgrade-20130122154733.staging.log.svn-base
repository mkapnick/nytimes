15:47:49 1950  PROCEDURE GET_PAYMENT_INFO_BY_INVOICE_ID (
15:47:49 1951  	 in_invoice_id		     IN NUMBER,
15:47:49 1952  	 out_order_id		     OUT VARCHAR2,
15:47:49 1953  	 out_external_transaction_id OUT VARCHAR2
15:47:49 1954  ) AS
15:47:49 1955  SPROC_NAME CONSTANT VARCHAR2(30) := 'GET_PEYMENT_INFO_BY_INVOICE_ID';
15:47:49 1956  -- VARIABLES
15:47:49 1957  temp_invoice_id number;
15:47:49 1958  cnt_matched_instr number := 0;
15:47:49 1959  -- EXCEPTIONS
15:47:49 1960  BAD_INVOICE_ID EXCEPTION;
15:47:49 1961  BEGIN
15:47:49 1962  
15:47:49 1963  	 BEGIN
15:47:49 1964  	   SELECT
15:47:49 1965  	     i.id into temp_invoice_id
15:47:49 1966  	   from
15:47:49 1967  	     invoice i
15:47:49 1968  	   where
15:47:49 1969  	     i.id = in_invoice_id;
15:47:49 1970  	   EXCEPTION
15:47:49 1971  	     WHEN NO_DATA_FOUND THEN
15:47:49 1972  	       RAISE BAD_INVOICE_ID;
15:47:49 1973  	 END;
15:47:49 1974  
15:47:49 1975  	 select
15:47:49 1976  	   count(1) into cnt_matched_instr
15:47:49 1977  	 from
15:47:49 1978  	   charge ch
15:47:49 1979  	 inner join
15:47:49 1980  	   subscription s
15:47:49 1981  	 on
15:47:49 1982  	   s.instrument_id = ch.instrument_id
15:47:49 1983  	 where
15:47:49 1984  	   ch.invoice_id = in_invoice_id;
15:47:49 1985  
15:47:49 1986  	 if cnt_matched_instr = 0 then
15:47:49 1987  	   out_external_transaction_id := null;
15:47:49 1988  	   out_order_id := null;
15:47:49 1989  	   return;
15:47:49 1990  	 end if;
15:47:49 1991  
15:47:49 1992  	 SELECT
15:47:49 1993  	   t.order_id,
15:47:49 1994  	   ta.external_transaction_id
15:47:49 1995  	   into
15:47:49 1996  	   out_order_id,
15:47:49 1997  	   out_external_transaction_id
15:47:49 1998  	 from
15:47:49 1999  	   charge ch
15:47:49 2000  	   inner join transaction t on ch.transaction_id = t.id
15:47:49 2001  	   inner join transaction_attempt ta on ta.transaction_id = t.id
15:47:49 2002  	 where
15:47:49 2003  	   ch.invoice_id = in_invoice_id
15:47:49 2004  	   and ta.transaction_attempt_status_id = GLOBAL_STATUSES_V17.TRANS_ATTEMPT_SUCCESS
15:47:49 2005  	   and ch.charge_amount > 0; -- We are not creating charges for the 0-amount invoices
15:47:49 2006  
15:47:49 2007  EXCEPTION
15:47:49 2008  WHEN BAD_INVOICE_ID THEN
15:47:49 2009  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 2010  	   SPROC_NAME, 'No such invoice');
15:47:49 2011  WHEN NO_DATA_FOUND THEN
15:47:49 2012  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 2013  	   SPROC_NAME, 'No payment data found');
15:47:49 2014  WHEN OTHERS THEN
15:47:49 2015  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 2016  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:47:49 2017  END GET_PAYMENT_INFO_BY_INVOICE_ID;
