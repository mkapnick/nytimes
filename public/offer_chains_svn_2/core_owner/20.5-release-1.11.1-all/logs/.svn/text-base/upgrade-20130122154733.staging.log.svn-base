15:47:50  64  	  gc.recipient_notify_date,
15:47:50  65  	  gc.id
15:47:50  66  	FROM
15:47:50  67  	  GIFT_CERTIFICATE gc,
15:47:50  68  	  OFFER_CHAIN ch
15:47:50  69  	WHERE
15:47:50  70  	  ch.id = gc.offer_chain_id
15:47:50  71  	  AND gc.RECIPIENT_NOTIFY_DATE is not null
15:47:50  72  	  AND gc.RECIPIENT_NOTIFY_DATE >= (sysdate - in_hours_number/24)
15:47:50  73  	  AND gc.RECIPIENT_NOTIFY_DATE < (sysdate - (in_hours_number-72)/24)
15:47:50  74  	  AND gc.redeemer_group_id is null
15:47:50  75  	  AND NOT EXISTS(
