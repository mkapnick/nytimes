15:47:50 742  
15:47:50 743  PROCEDURE GET_ACCOUNT_SUBSCRIPTIONS (
15:47:50 744  /*
15:47:50 745  Throws exceptions:
15:47:50 746  APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 747  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:50 748  */
15:47:50 749  	  in_group_id	 IN  NUMBER,
15:47:50 750  	  in_start_date  IN DATE,
15:47:50 751  	  in_end_date	 IN DATE,
15:47:50 752  	  in_status	 IN NUMBER,
15:47:50 753  	  in_group_account_type IN VARCHAR2,
15:47:50 754  	  out_result_set OUT SYS_REFCURSOR
15:47:50 755  ) AS
15:47:50 756  -- VARIABLES
15:47:50 757  SPROC_NAME     CONSTANT VARCHAR2(25) := 'GET_ACCOUNT_SUBSCRIPTIONS';
15:47:50 758  var_account_id NUMBER;
15:47:50 759  -- EXCEPTIONS
15:47:50 760  BAD_GROUP_ID	      EXCEPTION;
15:47:50 761  BAD_SUBSCRIPTION_STATUS EXCEPTION;
15:47:50 762  BEGIN
15:47:50 763  	-- Check that all passed data is correct
15:47:50 764  	IF in_status != GLOBAL_STATUSES_V17.SUBSCRIPTION_ACTIVE
15:47:50 765  	  AND in_status != GLOBAL_STATUSES_V17.SUBSCRIPTION_SUSPENDED
15:47:50 766  	  AND in_status != GLOBAL_STATUSES_V17.SUBSCRIPTION_CANCELED
15:47:50 767  	  AND in_status != GLOBAL_STATUSES_V17.SUBSCRIPTION_CLOSED
15:47:50 768  	THEN
15:47:50 769  	  RAISE BAD_SUBSCRIPTION_STATUS;
15:47:50 770  	END IF;
15:47:50 771  
15:47:50 772  	-- Get account id
15:47:50 773  	BEGIN
15:47:50 774  	  SELECT
15:47:50 775  	    ACCOUNT.ID INTO var_account_id
15:47:50 776  	  FROM
15:47:50 777  	    ACCOUNT
15:47:50 778  	  WHERE
15:47:50 779  	    ACCOUNT.GROUP_ID = in_group_id;
15:47:50 780  	  EXCEPTION
15:47:50 781  	    WHEN NO_DATA_FOUND THEN
15:47:50 782  	      RAISE BAD_GROUP_ID;
15:47:50 783  	END;
15:47:50 784  
15:47:50 785  	-- Get information about account subscriptions
15:47:50 786  	OPEN out_result_set FOR
15:47:50 787  	SELECT
15:47:50 788  	  in_group_id AS "GROUP_ID",
15:47:50 789  	  SUBSCRIPTION.ID AS "SUBSCRIPTION_ID",
15:47:50 790  	  SUBSCRIPTION.SUBSCRIPTION_STATUS_ID,
15:47:50 791  	  SUBSCRIPTION.PURCHASE_DATE,
15:47:50 792  	  SUBSCRIPTION.SUSPEND_DATE,
15:47:50 793  	  SUBSCRIPTION.REACTIVATION_DATE,
15:47:50 794  	  SUBSCRIPTION.CANCELLATION_DATE,
15:47:50 795  	  SUBSCRIPTION_CANCEL_REASON.VALUE as "CANCEL_TYPE",
15:47:50 796  	  SUBSCRIPTION.INSTRUMENT_ID,
15:47:50 797  	  SUBSCRIPTION.INSTRUMENT_TYPE_ID,
15:47:50 798  	  OFFER_CHAIN.ID AS "OFFER_CHAIN_ID",
15:47:50 799  	  OFFER_CHAIN.NAME,
15:47:50 800  	  OFFER_CHAIN.DESCRIPTION,
15:47:50 801  	  OFFER_CHAIN.PRODUCT_URI,
15:47:50 802  	  PROCS_SUBSCRIPTION_V17.CALC_SUBSCRIPTION_END_DATE(SUBSCRIPTION.ID) as "END_DATE",
15:47:50 803  	  PROCS_SUBSCRIPTION_V17.GET_RECENT_CHARGE(SUBSCRIPTION.ID) AS "RECENT_CHARGE",
15:47:50 804  	  PROCS_SUBSCRIPTION_V17.GET_RENEWAL_DATE(SUBSCRIPTION.ID) AS "RENEWAL_DATE",
15:47:50 805  	  PROCS_SUBSCRIPTION_V17.GET_BILLING_CYCLE(SUBSCRIPTION.ID) AS "BILLING_CYCLE",
15:47:50 806  	  PROCS_SUBSCRIPTION_V17.IS_SUBSCRIPTION_CANCELABLE(SUBSCRIPTION.ID) AS "IS_CANCELABLE",
15:47:50 807  	  ITUNES_RECEIPT.ID AS "ITUNES_RECEIPT_ID",
15:47:50 808  	  (
15:47:50 809  	    SELECT
15:47:50 810  	      MAX(ENTITLEMENT_END_DATE)
15:47:50 811  	      FROM LICENSE
15:47:50 812  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:47:50 813  	  )
15:47:50 814  	  as "ENT_END_DATE",
15:47:50 815  	  (
15:47:50 816  	    SELECT
15:47:50 817  	      MIN(START_DATE)
15:47:50 818  	      FROM LICENSE
15:47:50 819  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:47:50 820  	  )
15:47:50 821  	  as "ENT_START_DATE",
15:47:50 822  	  GROUP_ACCOUNT.SUBSCRIPTION_ID GA_SUBSCRIPTION_ID,
15:47:50 823  	  GROUP_ACCOUNT.ID GA_ID,
15:47:50 824  	  GROUP_ACCOUNT.GROUP_NAME GA_GROUP_NAME,
15:47:50 825  	  GROUP_ACCOUNT.FIRST_NAME GA_FIRST_NAME,
15:47:50 826  	  GROUP_ACCOUNT.LAST_NAME GA_LAST_NAME,
15:47:50 827  	  GROUP_ACCOUNT.EMAIL GA_EMAIL,
15:47:50 828  	  GROUP_ACCOUNT.PHONE GA_PHONE,
15:47:50 829  	  GROUP_ACCOUNT.ORGANIZATION_TYPE GA_ORGANIZATION_TYPE,
15:47:50 830  	  GROUP_ACCOUNT.SEATS GA_SEATS,
15:47:50 831  	  PROCS_GROUP_ACCOUNT_V17.F_GET_NUM_OCCUPIED_GROUP_SEATS(GROUP_ACCOUNT.ID) GA_SEATS_USED,
15:47:50 832  	  GROUP_ACCOUNT.IP GA_IP,
15:47:50 833  	  PROCS_SUBSCRIPTION_V17.GET_GIFT_CERT_CODE_BY_SUB_ID(SUBSCRIPTION.ID) GIFT_CERTIFICATE_CODE
15:47:50 834  	FROM
15:47:50 835  	  SUBSCRIPTION
15:47:50 836  	  INNER JOIN OFFER_CHAIN ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
15:47:50 837  	  LEFT JOIN SUBSCRIPTION_CANCEL_REASON ON SUBSCRIPTION.SCT_ID = SUBSCRIPTION_CANCEL_REASON.ID
15:47:50 838  	  LEFT JOIN ITUNES_RECEIPT ON SUBSCRIPTION.ID = ITUNES_RECEIPT.SUBSCRIPTION_ID
15:47:50 839  	  LEFT JOIN GROUP_ACCOUNT ON SUBSCRIPTION.ID = GROUP_ACCOUNT.SUBSCRIPTION_ID
15:47:50 840  	WHERE
15:47:50 841  	  SUBSCRIPTION.ACCOUNT_ID = var_account_id
15:47:50 842  	  AND (SUBSCRIPTION.SCT_ID IS NULL OR SUBSCRIPTION.SCT_ID != REAL_TIME_CANCEL_REASON)
15:47:50 843  	  AND SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = NVL(in_status, SUBSCRIPTION.SUBSCRIPTION_STATUS_ID)
15:47:50 844  	  AND PROCS_COMMON_V17.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE) >= NVL(in_start_date, PROCS_COMMON_V17.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE))
15:47:50 845  	  AND PROCS_COMMON_V17.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE) <= NVL(in_end_date, PROCS_COMMON_V17.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE))
15:47:50 846  	  AND (in_group_account_type IS NULL OR OFFER_CHAIN.GROUP_ACCOUNT_TYPE_ID = in_group_account_type);
15:47:50 847  
15:47:50 848  EXCEPTION
15:47:50 849  WHEN BAD_SUBSCRIPTION_STATUS THEN
15:47:50 850  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INVALID_PARAMETER,
15:47:50 851  	  SPROC_NAME, 'Bad subscription status');
15:47:50 852  WHEN BAD_GROUP_ID THEN
15:47:50 853  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 854  	  SPROC_NAME, 'Can not find account with given group id');
15:47:50 855  WHEN OTHERS THEN
15:47:50 856  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:50 857  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:50 858  END GET_ACCOUNT_SUBSCRIPTIONS;
15:47:50 859  
15:47:50 860  /******************************************************************************/
15:47:50 861  
15:47:50 862  PROCEDURE FREEZE_ACCOUNT (
15:47:50 863  /*
15:47:50 864  Throws exceptions:
15:47:50 865  APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 866  APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50 867  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:50 868  */
15:47:50 869  	in_group_id   IN NUMBER,
15:47:50 870  	in_updated_by IN VARCHAR2,
15:47:50 871  	in_note       IN VARCHAR2,
15:47:50 872  	in_agent_id   IN NUMBER
15:47:50 873  ) AS
15:47:50 874  SPROC_NAME	    CONSTANT VARCHAR2(14) := 'FREEZE_ACCOUNT';
15:47:50 875  -- VARIABLES
15:47:50 876  var_account_id	    NUMBER;
15:47:50 877  var_account_status_id NUMBER;
15:47:50 878  -- EXCEPTIONS
15:47:50 879  BAD_GROUP_ID	    EXCEPTION;
15:47:50 880  BAD_ACCOUNT_STATUS_ID EXCEPTION;
15:47:50 881  CAN_NOT_CREATE_NOTE   EXCEPTION;
15:47:50 882  EXCEPTION_MESSAGE     VARCHAR2(1024);
15:47:50 883  BEGIN
15:47:50 884  
15:47:50 885  	-- Get account status, account id
15:47:50 886  	BEGIN
15:47:50 887  	  SELECT
15:47:50 888  	    ACCOUNT.ID,
15:47:50 889  	    ACCOUNT.ACCOUNT_STATUS_ID
15:47:50 890  	    into
15:47:50 891  	    var_account_id,
15:47:50 892  	    var_account_status_id
15:47:50 893  	  FROM
15:47:50 894  	    ACCOUNT
15:47:50 895  	  WHERE
15:47:50 896  	    ACCOUNT.GROUP_ID = in_group_id;
15:47:50 897  	  EXCEPTION
15:47:50 898  	    WHEN NO_DATA_FOUND THEN
15:47:50 899  	      RAISE BAD_GROUP_ID;
15:47:50 900  	END;
15:47:50 901  
15:47:50 902  	-- We can freeze only ACTIVE accounts
15:47:50 903  	IF var_account_status_id != GLOBAL_STATUSES_V17.ACCOUNT_ACTIVE
15:47:50 904  	  AND var_account_status_id != GLOBAL_STATUSES_V17.ACCOUNT_FROZEN THEN
15:47:50 905  	  RAISE BAD_ACCOUNT_STATUS_ID;
15:47:50 906  	END IF;
15:47:50 907  
15:47:50 908  	-- Set account status
15:47:50 909  	PROCS_ACCOUNT_V17.UPDATE_ACCOUNT_STATUS(
15:47:50 910  	  in_account_id        => var_account_id,
15:47:50 911  	  in_updated_by        => in_updated_by,
15:47:50 912  	  in_account_status_id => GLOBAL_STATUSES_V17.ACCOUNT_FROZEN
15:47:50 913  	);
15:47:50 914  
15:47:50 915  	-- Annotate account
15:47:50 916  	BEGIN
15:47:50 917  	  PROCS_ACCOUNT_V17.ANNOTATE_ACCOUNT(
15:47:50 918  	    in_group_id   => in_group_id,
15:47:50 919  	    in_agent_id   => in_agent_id,
15:47:50 920  	    in_note	  => in_note,
15:47:50 921  	    in_created_by => in_updated_by
15:47:50 922  	  );
15:47:50 923  	  EXCEPTION
15:47:50 924  	    WHEN OTHERS THEN
15:47:50 925  	      EXCEPTION_MESSAGE := SQLERRM;
15:47:50 926  	      RAISE CAN_NOT_CREATE_NOTE;
15:47:50 927  	END;
15:47:50 928  
15:47:50 929  EXCEPTION
15:47:50 930  WHEN BAD_GROUP_ID THEN
15:47:50 931  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 932  	  SPROC_NAME, 'No such group id');
15:47:50 933  WHEN BAD_ACCOUNT_STATUS_ID THEN
15:47:50 934  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.STATE_ERROR,
15:47:50 935  	  SPROC_NAME, 'Could not update this account. Status should to be active or frozen');
15:47:50 936  WHEN CAN_NOT_CREATE_NOTE THEN
15:47:50 937  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50 938  	  SPROC_NAME, 'Could not annotate account', EXCEPTION_MESSAGE);
15:47:50 939  WHEN OTHERS THEN
15:47:50 940  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:50 941  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:50 942  END FREEZE_ACCOUNT;
