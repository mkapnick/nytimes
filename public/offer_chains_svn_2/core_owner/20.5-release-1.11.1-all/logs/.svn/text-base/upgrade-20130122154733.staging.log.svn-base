15:47:50 356  		RAISE CAN_NOT_CREATE_HISTORY;
15:47:50 357  	  END;
15:47:50 358  
15:47:50 359  	  DELETE FROM NOTIFICATION WHERE ID = in_notification_id;
15:47:50 360  
15:47:50 361  	END IF;
15:47:50 362  	commit;
