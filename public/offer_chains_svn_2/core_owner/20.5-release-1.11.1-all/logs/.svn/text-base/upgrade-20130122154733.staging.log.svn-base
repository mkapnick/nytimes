15:47:50 238  	in_paypal_prvt_street_address2	IN VARCHAR2,
15:47:50 239  	in_state			IN VARCHAR2,
15:47:50 240  	in_city 			IN VARCHAR2,
15:47:50 241  	in_postal_code			IN VARCHAR2,
15:47:50 242  	in_country			IN CHAR,
15:47:50 243  	in_expiration_date		IN DATE,
15:47:50 244  	in_secret_token 		IN VARCHAR2,
15:47:50 245  	out_paypal_id			OUT NUMBER
15:47:50 246  ) AS
15:47:50 247  SPROC_NAME CONSTANT VARCHAR2(10) := 'ADD_PAYPAL';
15:47:50 248  var_paypal_id NUMBER;
15:47:50 249  var_account_id  NUMBER;
15:47:50 250  -- EXCEPTIONS
15:47:50 251  BAD_GROUP_ID	    EXCEPTION;
15:47:50 252  CAN_NOT_CREATE_PAYPAL EXCEPTION;
15:47:50 253  BAD_PAYPAL_STATUS     EXCEPTION;
15:47:50 254  EXCEPTION_MESSAGE VARCHAR2(1024);
15:47:50 255  BEGIN
15:47:50 256  
15:47:50 257  	IF in_paypal_status_id != GLOBAL_STATUSES_V17.PAYPAL_ACTIVE
15:47:50 258  	  AND in_paypal_status_id != GLOBAL_STATUSES_V17.PAYPAL_INACTIVE
15:47:50 259  	  AND in_paypal_status_id != GLOBAL_STATUSES_V17.PAYPAL_FROZEN THEN
15:47:50 260  	  RAISE BAD_PAYPAL_STATUS;
15:47:50 261  	END IF;
15:47:50 262  
15:47:50 263  	BEGIN
15:47:50 264  	  SELECT
15:47:50 265  	    a.id into var_account_id
15:47:50 266  	  from
15:47:50 267  	    account a
15:47:50 268  	  where
15:47:50 269  	    a.group_id = in_group_id;
15:47:50 270  	  EXCEPTION
15:47:50 271  	    WHEN NO_DATA_FOUND THEN
15:47:50 272  	      RAISE BAD_GROUP_ID;
15:47:50 273  	END;
15:47:50 274  
15:47:50 275  	BEGIN
15:47:50 276  	  PROCS_FIN_INSTRUMENTS_CRU_V17.CREATE_PAYPAL(
15:47:50 277  	    out_paypal_id		   => var_paypal_id,
15:47:50 278  	    in_paypal_id		   => NULL,
15:47:50 279  	    in_account_id		   => var_account_id,
15:47:50 280  	    in_instrument_name		   => in_instrument_name,
15:47:50 281  	    in_private_email_address	   => in_private_email_address,
15:47:50 282  	    in_created_by		   => in_created_by,
15:47:50 283  	    in_paypal_status_id 	   => in_paypal_status_id,
15:47:50 284  	    in_paypal_prvt_street_address  => in_paypal_prvt_street_address,
15:47:50 285  	    in_paypal_prvt_street_address2 => in_paypal_prvt_street_address2,
15:47:50 286  	    in_state			   => in_state,
15:47:50 287  	    in_city			   => in_city,
15:47:50 288  	    in_postal_code		   => in_postal_code,
15:47:50 289  	    in_country			   => in_country,
15:47:50 290  	    in_expiration_date		   => in_expiration_date,
15:47:50 291  	    in_secret_token		   => in_secret_token
15:47:50 292  	  );
15:47:50 293  	  EXCEPTION
15:47:50 294  	    WHEN OTHERS THEN
15:47:50 295  	      EXCEPTION_MESSAGE := SQLERRM;
15:47:50 296  	      RAISE CAN_NOT_CREATE_PAYPAL;
15:47:50 297  	END;
15:47:50 298  
15:47:50 299  	out_paypal_id := var_paypal_id;
15:47:50 300  
15:47:50 301  EXCEPTION
15:47:50 302  WHEN BAD_GROUP_ID THEN
15:47:50 303  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 304  	  SPROC_NAME, 'No such group id');
15:47:50 305  WHEN BAD_PAYPAL_STATUS THEN
15:47:50 306  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 307  	  SPROC_NAME, 'Bad paypal status');
