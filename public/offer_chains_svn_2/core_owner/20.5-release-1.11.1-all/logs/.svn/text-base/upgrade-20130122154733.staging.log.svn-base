15:47:49 1461  	     in_invoice_status => GLOBAL_STATUSES_V17.INVOICE_CLOSED,
15:47:49 1462  	     in_created_by     => in_updated_by,
15:47:49 1463  	     in_tax_exempt_id  => var_account_tax_exempt_id,
15:47:49 1464  	     out_invoice_id    => var_free_invoice_id
15:47:49 1465  	   );
15:47:49 1466  	   EXCEPTION
15:47:49 1467  	     WHEN OTHERS THEN
15:47:49 1468  	       EXCEPTION_MESSAGE := SQLERRM;
15:47:49 1469  	       RAISE CAN_NOT_CREATE_INVOICE;
15:47:49 1470  	 END;
15:47:49 1471  
15:47:49 1472  	 -- Creating new "free" license
15:47:49 1473  	 BEGIN
15:47:49 1474  	   PROCS_LICENSE_V17.CREATE_LICENSE (
15:47:49 1475  	     in_status_id		 => GLOBAL_STATUSES_V17.LICENSE_ACTIVE,
15:47:49 1476  	     in_needs_entitlements	 => GLOBAL_CONSTANTS_V17.TRUE,
15:47:49 1477  	     in_start_date		 => in_effective_start_date,
15:47:49 1478  	     in_end_date		 => in_effective_end_date,
15:47:49 1479  	     in_offer_id		 => var_current_offer_id,
15:47:49 1480  	     in_subscription_id 	 => in_subscription_id,
15:47:49 1481  	     in_invoice_id		 => var_free_invoice_id,
15:47:49 1482  	     in_created_by		 => in_updated_by,
15:47:49 1483  	     in_is_extension		 => GLOBAL_CONSTANTS_V17.TRUE,
15:47:49 1484  	     in_current_offer_index	 => var_current_offer_index,
15:47:49 1485  	     in_current_offer_recurr_num => var_current_offer_recurr_num,
15:47:49 1486  	     out_license_id		 => var_free_license_id
15:47:49 1487  	   );
15:47:49 1488  	   EXCEPTION
15:47:49 1489  	     WHEN OTHERS THEN
