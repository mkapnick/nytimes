15:47:50 335  
15:47:50 336  PROCEDURE GET_ACCOUNT_CREDIT_CARDS (
15:47:50 337  /*
15:47:50 338  IN:
15:47:50 339  instr_status:
15:47:50 340  1 - get active instruments only (default)
15:47:50 341  0 - get inactive instruments only
15:47:50 342  
15:47:50 343  Throws exceptions:
15:47:50 344  APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 345  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:50 346  */
15:47:50 347  	in_group_id	  IN NUMBER,
15:47:50 348  	out_result_cc_set OUT SYS_REFCURSOR,
15:47:50 349  	out_default_id	  OUT NUMBER,
15:47:50 350  	in_instr_status   IN NUMBER DEFAULT GLOBAL_STATUSES_V17.CREDIT_CARD_ACTIVE
15:47:50 351  ) AS
15:47:50 352  SPROC_NAME		 CONSTANT VARCHAR2(24) := 'GET_ACCOUNT_CREDIT_CARDS';
15:47:50 353  var_account_id		 NUMBER;
15:47:50 354  var_def_instrument_type_id NUMBER;
15:47:50 355  var_def_instrument_id	 NUMBER;
15:47:50 356  temp_cc_status_id 	 NUMBER;
15:47:50 357  
15:47:50 358  -- Exceptions
15:47:50 359  WRONG_INSTR_EXCEPTION	     EXCEPTION;
15:47:50 360  CAN_NOT_GET_DEF_FIN_INSTRUMENT EXCEPTION;
15:47:50 361  EXCEPTION_MESSAGE 	     VARCHAR2(1024);
15:47:50 362  BEGIN
15:47:50 363  
15:47:50 364  	-- Get group id
15:47:50 365  	SELECT
15:47:50 366  	  ACCOUNT.ID INTO var_account_id
15:47:50 367  	FROM
15:47:50 368  	  ACCOUNT
15:47:50 369  	WHERE
15:47:50 370  	  ACCOUNT.GROUP_ID = in_group_id;
15:47:50 371  
15:47:50 372  	-- Check that incoming parameter is correct
15:47:50 373  	BEGIN
15:47:50 374  	  SELECT
15:47:50 375  	    CREDIT_CARD_STATUS.ID into temp_cc_status_id
15:47:50 376  	  FROM
15:47:50 377  	    CREDIT_CARD_STATUS
15:47:50 378  	  WHERE
15:47:50 379  	    CREDIT_CARD_STATUS.ID = in_instr_status;
15:47:50 380  	  EXCEPTION
15:47:50 381  	    WHEN NO_DATA_FOUND THEN
15:47:50 382  	      RAISE WRONG_INSTR_EXCEPTION;
15:47:50 383  	END;
15:47:50 384  
15:47:50 385  	-- Select all crdit cards for given account
15:47:50 386  	OPEN out_result_cc_set FOR
15:47:50 387  	SELECT
15:47:50 388  	  CREDIT_CARD.ID,
15:47:50 389  	  CREDIT_CARD.ACCOUNT_ID,
15:47:50 390  	  CREDIT_CARD.INSTRUMENT_NAME,
15:47:50 391  	  CREDIT_CARD.PRIVATE_CARD_HOLDER_NAME,
15:47:50 392  	  CREDIT_CARD.PRIVATE_STREET_ADDRESS,
15:47:50 393  	  CREDIT_CARD.PRIVATE_STREET_ADDRESS2,
15:47:50 394  	  CREDIT_CARD.STATE,
15:47:50 395  	  CREDIT_CARD.CITY,
15:47:50 396  	  CREDIT_CARD.POSTAL_CODE,
15:47:50 397  	  CREDIT_CARD.COUNTRY,
15:47:50 398  	  CREDIT_CARD.LAST_FOUR_CC,
15:47:50 399  	  CREDIT_CARD.EXPIRATION_DATE,
15:47:50 400  	  CREDIT_CARD.CREDIT_CARD_TYPE_ID,
15:47:50 401  	  CREDIT_CARD.SECRET_TOKEN,
15:47:50 402  	  CREDIT_CARD.CREATE_DATE,
15:47:50 403  	  CREDIT_CARD.CREATED_BY,
15:47:50 404  	  CREDIT_CARD.UPDATE_DATE,
15:47:50 405  	  CREDIT_CARD.UPDATED_BY,
15:47:50 406  	  CREDIT_CARD.CREDIT_CARD_STATUS_ID,
15:47:50 407  	  CREDIT_CARD.PRIVATE_FIRST_NAME,
15:47:50 408  	  CREDIT_CARD.PRIVATE_LAST_NAME
15:47:50 409  	FROM
15:47:50 410  	  CREDIT_CARD
15:47:50 411  	WHERE
15:47:50 412  	  CREDIT_CARD.ACCOUNT_ID = var_account_id
15:47:50 413  	  AND CREDIT_CARD.CREDIT_CARD_STATUS_ID = in_instr_status;
15:47:50 414  
15:47:50 415  	-- Get default credit card if exists
15:47:50 416  	BEGIN
15:47:50 417  	  PROCS_FIN_INSTRUMENTS_V17.GET_DEF_FINANCIAL_INSTRUMENT(
15:47:50 418  	    in_group_id 	   => in_group_id,
15:47:50 419  	    out_instrument_type_id => var_def_instrument_type_id,
15:47:50 420  	    out_instrument_id	   => var_def_instrument_id
15:47:50 421  	  );
15:47:50 422  	  EXCEPTION
15:47:50 423  	    WHEN OTHERS THEN
15:47:50 424  	      EXCEPTION_MESSAGE := SQLERRM;
15:47:50 425  	      RAISE CAN_NOT_GET_DEF_FIN_INSTRUMENT;
15:47:50 426  	END;
15:47:50 427  
15:47:50 428  	IF var_def_instrument_type_id = GLOBAL_ENUMS_V17.INSTRUMENT_CREDIT_CARD THEN
15:47:50 429  	  out_default_id := var_def_instrument_id;
15:47:50 430  	ELSE
15:47:50 431  	  out_default_id := 0;
15:47:50 432  	END IF;
15:47:50 433  
15:47:50 434  EXCEPTION
15:47:50 435  WHEN NO_DATA_FOUND THEN
15:47:50 436  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 437  	  SPROC_NAME, 'No such account');
15:47:50 438  WHEN WRONG_INSTR_EXCEPTION THEN
15:47:50 439  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.STATE_ERROR,
15:47:50 440  	  SPROC_NAME, 'Wrong instrument status');
15:47:50 441  WHEN CAN_NOT_GET_DEF_FIN_INSTRUMENT THEN
15:47:50 442  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50 443  	  SPROC_NAME, 'Could not get default financial instrument', EXCEPTION_MESSAGE);
15:47:50 444  WHEN OTHERS THEN
15:47:50 445  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:50 446  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:50 447  END GET_ACCOUNT_CREDIT_CARDS;
15:47:50 448  
15:47:50 449  /******************************************************************************/
15:47:50 450  
15:47:50 451  PROCEDURE GET_ACCOUNT_GIFT_CERTIFICATES (
15:47:50 452  /*
15:47:50 453  IN:
15:47:50 454  instr_status:
15:47:50 455  GLOBAL_CONSTANTS_V17.TRUE - get active instruments only (default)
15:47:50 456  GLOBAL_CONSTANTS_V17.FALSE - get inactive instruments only
15:47:50 457  
15:47:50 458  Throws exceptions:
15:47:50 459  APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 460  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:50 461  */
15:47:50 462  	in_group_id	  IN NUMBER,
15:47:50 463  	out_result_gc_set OUT SYS_REFCURSOR,
15:47:50 464  	in_instr_status   IN NUMBER DEFAULT GLOBAL_CONSTANTS_V17.TRUE
15:47:50 465  ) AS
15:47:50 466  SPROC_NAME     CONSTANT VARCHAR2(29) := 'GET_ACCOUNT_GIFT_CERTIFICATES';
15:47:50 467  var_account_id NUMBER;
15:47:50 468  
15:47:50 469  -- Exceptions
15:47:50 470  WRONG_INSTR_EXCEPTION	 EXCEPTION;
15:47:50 471  BEGIN
15:47:50 472  
15:47:50 473  	-- Get account id
15:47:50 474  	SELECT
15:47:50 475  	  ACCOUNT.ID INTO var_account_id
15:47:50 476  	FROM
15:47:50 477  	  ACCOUNT
15:47:50 478  	WHERE
15:47:50 479  	  ACCOUNT.GROUP_ID = in_group_id;
15:47:50 480  
15:47:50 481  	-- Check that incoming data is correct
15:47:50 482  	IF in_instr_status != GLOBAL_CONSTANTS_V17.TRUE AND in_instr_status != GLOBAL_CONSTANTS_V17.FALSE THEN
15:47:50 483  	  RAISE WRONG_INSTR_EXCEPTION;
15:47:50 484  	END IF;
15:47:50 485  
15:47:50 486  	OPEN out_result_gc_set FOR
15:47:50 487  	SELECT
15:47:50 488  	  GIFT_CERTIFICATE.ID,
15:47:50 489  	  GIFT_CERTIFICATE.PURCHASER_GROUP_ID,
15:47:50 490  	  GIFT_CERTIFICATE.PURCHASE_INVOICE_ID,
15:47:50 491  	  GIFT_CERTIFICATE.PURCHASE_DATE,
15:47:50 492  	  GIFT_CERTIFICATE.OFFER_CHAIN_ID,
15:47:50 493  	  GIFT_CERTIFICATE.EXPIRATION_DATE,
15:47:50 494  	  GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID,
15:47:50 495  	  GIFT_CERTIFICATE.CODE,
15:47:50 496  	  GIFT_CERTIFICATE.CREATE_DATE,
15:47:50 497  	  GIFT_CERTIFICATE.CREATED_BY,
15:47:50 498  	  GIFT_CERTIFICATE.UPDATE_DATE,
15:47:50 499  	  GIFT_CERTIFICATE.UPDATED_BY,
15:47:50 500  	  GIFT_CERTIFICATE.RECIPIENT_NAME,
15:47:50 501  	  GIFT_CERTIFICATE.RECIPIENT_EMAIL,
15:47:50 502  	  GIFT_CERTIFICATE.SENDER_NAME,
15:47:50 503  	  GIFT_CERTIFICATE.SENDER_EMAIL,
15:47:50 504  	  GIFT_CERTIFICATE.REDEEMER_GROUP_ID,
15:47:50 505  	  GIFT_CERTIFICATE.REDEMPTION_DATE,
15:47:50 506  	  GIFT_CERTIFICATE.FINALIZED_INVOICE_ID,
15:47:50 507  	  GIFT_CERTIFICATE.GIFT_MESSAGE
15:47:50 508  	FROM
15:47:50 509  	  GIFT_CERTIFICATE
15:47:50 510  	WHERE
15:47:50 511  	  GIFT_CERTIFICATE.PURCHASER_GROUP_ID = in_group_id
15:47:50 512  	  AND (
15:47:50 513  		( in_instr_status = GLOBAL_CONSTANTS_V17.TRUE AND
15:47:50 514  		  (
15:47:50 515  		    GIFT_CERTIFICATE.EXPIRATION_DATE >= current_date
15:47:50 516  		    AND GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID = GLOBAL_STATUSES_V17.GIFT_CERTIFICATE_ACTIVE
15:47:50 517  		  )
15:47:50 518  		)
15:47:50 519  		OR
15:47:50 520  		(
15:47:50 521  		  in_instr_status = GLOBAL_CONSTANTS_V17.FALSE AND
15:47:50 522  		  (
15:47:50 523  		    GIFT_CERTIFICATE.EXPIRATION_DATE < current_date
15:47:50 524  		    OR GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID != GLOBAL_STATUSES_V17.GIFT_CERTIFICATE_ACTIVE
15:47:50 525  		  )
15:47:50 526  		)
15:47:50 527  	      );
15:47:50 528  
15:47:50 529  EXCEPTION
15:47:50 530  WHEN NO_DATA_FOUND THEN
15:47:50 531  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 532  	  SPROC_NAME, 'Can not find account with given group id');
15:47:50 533  WHEN WRONG_INSTR_EXCEPTION THEN
15:47:50 534  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INVALID_PARAMETER,
15:47:50 535  	  SPROC_NAME, 'Wrong gift certificate type');
15:47:50 536  WHEN OTHERS THEN
15:47:50 537  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:50 538  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:50 539  END GET_ACCOUNT_GIFT_CERTIFICATES;
