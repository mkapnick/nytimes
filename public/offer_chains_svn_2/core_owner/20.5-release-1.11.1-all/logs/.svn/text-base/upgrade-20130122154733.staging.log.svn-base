15:47:50 2706  
15:47:50 2707  PROCEDURE SWITCH_FINANCIAL_INSTRUMENT (
15:47:50 2708  	 /*in_group_id		      IN NUMBER  -- TODO: should we pass group_id here?*/
15:47:50 2709  	 in_old_fin_instrument_id   IN NUMBER,
15:47:50 2710  	 in_old_fin_instrument_type IN NUMBER,
15:47:50 2711  	 in_new_fin_instrument_id   IN NUMBER,
15:47:50 2712  	 in_new_fin_instrument_type IN NUMBER,
15:47:50 2713  	 in_updated_by		    IN VARCHAR2
15:47:50 2714  ) AS
15:47:50 2715  SPROC_NAME CONSTANT VARCHAR2(27) := 'SWITCH_FINANCIAL_INSTRUMENT';
15:47:50 2716  -- variables
15:47:50 2717  temp_out_transaction_id NUMBER;
15:47:50 2718  temp_out_charge_id      NUMBER;
15:47:50 2719  var_accounts_count      NUMBER;
15:47:50 2720  -- EXCEPTIONS
15:47:50 2721  BAD_OLD_CC   EXCEPTION;
15:47:50 2722  BAD_OLD_PP   EXCEPTION;
15:47:50 2723  BAD_OLD_TYPE EXCEPTION;
15:47:50 2724  BAD_NEW_CC   EXCEPTION;
15:47:50 2725  BAD_NEW_PP   EXCEPTION;
15:47:50 2726  BAD_NEW_TYPE EXCEPTION;
15:47:50 2727  DIFFERENT_OWNERS EXCEPTION;
15:47:50 2728  BEGIN
15:47:50 2729  
15:47:50 2730  	 IF in_old_fin_instrument_type = GLOBAL_ENUMS_V17.INSTRUMENT_CREDIT_CARD THEN
15:47:50 2731  	   IF PROCS_FIN_INSTRUMENTS_V17.IS_CREDIT_CARD_EXISTS(in_old_fin_instrument_id) = GLOBAL_CONSTANTS_V17.FALSE THEN
15:47:50 2732  	     -- throw exception: bad old credit card
15:47:50 2733  	     RAISE BAD_OLD_CC;
15:47:50 2734  	   END IF;
15:47:50 2735  	 ELSIF in_old_fin_instrument_type = GLOBAL_ENUMS_V17.INSTRUMENT_PAYPAL THEN
15:47:50 2736  	   IF PROCS_FIN_INSTRUMENTS_V17.IS_PAYPAL_EXISTS(in_old_fin_instrument_id) = GLOBAL_CONSTANTS_V17.FALSE THEN
15:47:50 2737  	     -- throw exception: bad old paypal
15:47:50 2738  	     RAISE BAD_OLD_PP;
15:47:50 2739  	   END IF;
15:47:50 2740  	 ELSE
15:47:50 2741  	   -- throw exception: bad instrument type
15:47:50 2742  	   RAISE BAD_OLD_TYPE;
15:47:50 2743  	 END IF;
15:47:50 2744  
15:47:50 2745  	 IF in_new_fin_instrument_type = GLOBAL_ENUMS_V17.INSTRUMENT_CREDIT_CARD THEN
15:47:50 2746  	   IF PROCS_FIN_INSTRUMENTS_V17.IS_CREDIT_CARD_EXISTS(in_new_fin_instrument_id) = GLOBAL_CONSTANTS_V17.FALSE THEN
15:47:50 2747  	     -- throw exception: bad new credit card
15:47:50 2748  	     RAISE BAD_NEW_CC;
15:47:50 2749  	   END IF;
15:47:50 2750  	 ELSIF in_new_fin_instrument_type = GLOBAL_ENUMS_V17.INSTRUMENT_PAYPAL THEN
15:47:50 2751  	   IF PROCS_FIN_INSTRUMENTS_V17.IS_PAYPAL_EXISTS(in_new_fin_instrument_id) = GLOBAL_CONSTANTS_V17.FALSE THEN
15:47:50 2752  	     -- throw exception: bad new paypal
15:47:50 2753  	     RAISE BAD_NEW_PP;
15:47:50 2754  	   END IF;
15:47:50 2755  	 ELSE
15:47:50 2756  	   -- throw exception: bad new instrument type
15:47:50 2757  	   RAISE BAD_NEW_TYPE;
15:47:50 2758  	 END IF;
15:47:50 2759  
15:47:50 2760  	 -- Check that owner of both instruments - same man
15:47:50 2761  
15:47:50 2762  	 SELECT count(1) into var_accounts_count FROM (
15:47:50 2763  	   SELECT
15:47:50 2764  	     CC.ACCOUNT_ID
15:47:50 2765  	   FROM
15:47:50 2766  	     CREDIT_CARD CC
15:47:50 2767  	   WHERE
15:47:50 2768  	     (
15:47:50 2769  	       CC.ID = in_old_fin_instrument_id
15:47:50 2770  	       AND in_old_fin_instrument_type = GLOBAL_ENUMS_V17.INSTRUMENT_CREDIT_CARD
15:47:50 2771  	     )
15:47:50 2772  	     OR
15:47:50 2773  	     (
15:47:50 2774  	       CC.ID = in_new_fin_instrument_id
15:47:50 2775  	       AND in_new_fin_instrument_type = GLOBAL_ENUMS_V17.INSTRUMENT_CREDIT_CARD
15:47:50 2776  	     )
15:47:50 2777  	   UNION
15:47:50 2778  	   SELECT
15:47:50 2779  	     PP.ACCOUNT_ID
15:47:50 2780  	   FROM
15:47:50 2781  	     PAYPAL PP
15:47:50 2782  	   WHERE
15:47:50 2783  	     (
15:47:50 2784  	       PP.ID = in_old_fin_instrument_id
15:47:50 2785  	       AND in_old_fin_instrument_type = GLOBAL_ENUMS_V17.INSTRUMENT_PAYPAL
15:47:50 2786  	     )
15:47:50 2787  	     OR
15:47:50 2788  	     (
15:47:50 2789  	       PP.ID = in_new_fin_instrument_id
15:47:50 2790  	       AND in_new_fin_instrument_type = GLOBAL_ENUMS_V17.INSTRUMENT_PAYPAL
15:47:50 2791  	     )
15:47:50 2792  	 )
15:47:50 2793  	 WHERE
15:47:50 2794  	   account_id IS NOT NULL;
15:47:50 2795  
15:47:50 2796  	 IF (var_accounts_count > 1) THEN
15:47:50 2797  	   -- Throw exception: different owners of instruments
15:47:50 2798  	   RAISE DIFFERENT_OWNERS;
15:47:50 2799  	 END IF;
15:47:50 2800  
15:47:50 2801  	 FOR f_sub IN (
15:47:50 2802  	   select
15:47:50 2803  	     s.id
15:47:50 2804  	   FROM
15:47:50 2805  	     subscription s
15:47:50 2806  	   WHERE
15:47:50 2807  	     (
15:47:50 2808  	       s.subscription_status_id = GLOBAL_STATUSES_V17.SUBSCRIPTION_ACTIVE OR
15:47:50 2809  	       s.subscription_status_id = GLOBAL_STATUSES_V17.SUBSCRIPTION_SUSPENDED
15:47:50 2810  	     )
15:47:50 2811  	     AND
15:47:50 2812  	     s.instrument_type_id = in_old_fin_instrument_type AND
15:47:50 2813  	     s.instrument_id = in_old_fin_instrument_id
15:47:50 2814  	 ) LOOP
15:47:50 2815  	   PROCS_SUBSCRIPTION_CRU_V17.UPDATE_SUBSCRIPTION(
15:47:50 2816  	     in_subscription_id => f_sub.id,
15:47:50 2817  	     in_instrument_type_id => in_new_fin_instrument_type,
15:47:50 2818  	     in_instrument_id => in_new_fin_instrument_id,
15:47:50 2819  	     in_updated_by => in_updated_by
15:47:50 2820  	   );
15:47:50 2821  	 END LOOP;
15:47:50 2822  
15:47:50 2823  	 FOR f_open_charge IN (
15:47:50 2824  	   select
15:47:50 2825  	     ch.id,
15:47:50 2826  	     ch.invoice_id,
15:47:50 2827  	     ch.transaction_id,
15:47:50 2828  	     ch.charge_amount
15:47:50 2829  	   FROM
15:47:50 2830  	     charge ch
15:47:50 2831  	   WHERE
15:47:50 2832  	     ch.instrument_type_id = in_old_fin_instrument_type
15:47:50 2833  	     AND ch.instrument_id = in_old_fin_instrument_id
15:47:50 2834  	     AND ch.charge_status_id = GLOBAL_STATUSES_V17.CHARGE_OPENED
15:47:50 2835  	 ) LOOP
15:47:50 2836  
15:47:50 2837  	   FOR f_pending_transaction IN (
15:47:50 2838  	     select
15:47:50 2839  	       id, transaction_amount, order_id, is_refund
15:47:50 2840  	     from
15:47:50 2841  	       transaction
15:47:50 2842  	     where
15:47:50 2843  	       id = f_open_charge.transaction_id
15:47:50 2844  	       and transaction_status_id = GLOBAL_STATUSES_V17.TRANSACTION_PENDING
15:47:50 2845  	   ) LOOP
15:47:50 2846  	     PROCS_TRANSACTION_V17.CREATE_TRANSACTION (
15:47:50 2847  	       in_transaction_id => NULL,
15:47:50 2848  	       in_status_id  => GLOBAL_STATUSES_V17.TRANSACTION_PENDING,
15:47:50 2849  	       in_amount     => f_pending_transaction.transaction_amount,
15:47:50 2850  	       in_created_by => in_updated_by,
15:47:50 2851  	       in_order_id   => null,
15:47:50 2852  	       in_is_refund  => f_pending_transaction.is_refund,
15:47:50 2853  	       out_transaction_id => temp_out_transaction_id
15:47:50 2854  	     );
15:47:50 2855  
15:47:50 2856  	     PROCS_TRANSACTION_V17.UPDATE_TRANSACTION_STATUS(
15:47:50 2857  	       in_transaction_id => f_pending_transaction.id,
15:47:50 2858  	       in_updated_by	 => in_updated_by,
15:47:50 2859  	       in_transaction_status_id  => GLOBAL_STATUSES_V17.TRANSACTION_CLOSED
15:47:50 2860  	     );
15:47:50 2861  
15:47:50 2862  	     -- Create new charge
15:47:50 2863  	     PROCS_CHARGE_V17.CREATE_CHARGE (
15:47:50 2864  	       in_invoice_id	     => f_open_charge.invoice_id,
15:47:50 2865  	       in_transaction_id     => temp_out_transaction_id,
15:47:50 2866  	       in_instrument_type_id => in_new_fin_instrument_type,
15:47:50 2867  	       in_instrument_id      => in_new_fin_instrument_id,
15:47:50 2868  	       in_charge_amount      => f_open_charge.charge_amount,
15:47:50 2869  	       in_created_by	     => in_updated_by,
15:47:50 2870  	       in_charge_status_id   => GLOBAL_STATUSES_V17.CHARGE_OPENED,
15:47:50 2871  	       out_charge_id	     => temp_out_charge_id
15:47:50 2872  	     );
15:47:50 2873  	     -- Cancel old charge
15:47:50 2874  	     PROCS_CHARGE_V17.UPDATE_CHARGE_STATUS(
15:47:50 2875  	       in_charge_id	   => f_open_charge.id,
15:47:50 2876  	       in_updated_by	   => in_updated_by,
15:47:50 2877  	       in_charge_status_id => GLOBAL_STATUSES_V17.CHARGE_CANCELED
15:47:50 2878  	     );
15:47:50 2879  
15:47:50 2880  	     PROCS_ADJUSTMENTS_V17.UPDATE_INVOICE_ADJUSTMENT(
15:47:50 2881  	       IN_INVOICE_ID => f_open_charge.invoice_id,
15:47:50 2882  	       IN_ORIGINAL_CHARGE_ID => f_open_charge.id,
15:47:50 2883  	       IN_CHARGE_ID => temp_out_charge_id,
15:47:50 2884  	       IN_UPDATED_BY => in_updated_by
15:47:50 2885  	     );
15:47:50 2886  
15:47:50 2887  	   END LOOP;
15:47:50 2888  	 END LOOP;
15:47:50 2889  
15:47:50 2890  EXCEPTION
15:47:50 2891  WHEN BAD_OLD_CC THEN
15:47:50 2892  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 2893  	   SPROC_NAME, 'Trying to switch from non existing credit card');
15:47:50 2894  WHEN BAD_OLD_PP THEN
15:47:50 2895  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 2896  	   SPROC_NAME, 'Trying to switch from non existing paypal');
15:47:50 2897  WHEN BAD_OLD_TYPE THEN
15:47:50 2898  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 2899  	   SPROC_NAME, 'Trying to switch from unknown/unsupported financial instrument');
15:47:50 2900  WHEN BAD_NEW_CC THEN
15:47:50 2901  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 2902  	   SPROC_NAME, 'Trying to switch to non existing credit card');
15:47:50 2903  WHEN BAD_NEW_PP THEN
15:47:50 2904  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 2905  	   SPROC_NAME, 'Trying to switch to non existing paypal');
