15:47:50  61  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:50  62  */
15:47:50  63  	in_group_id IN	NUMBER,
15:47:50  64  	out_exists  OUT NUMBER
15:47:50  65  ) AS
15:47:50  66  -- VARIABLES
15:47:50  67  var_found_id  NUMBER;
15:47:50  68  SPROC_NAME    CONSTANT VARCHAR2(21) := 'ASSERT_ACCOUNT_EXISTS';
15:47:50  69  BEGIN
15:47:50  70  	SELECT ACCOUNT.ID INTO var_found_id FROM ACCOUNT WHERE ACCOUNT.GROUP_ID = in_group_id;
15:47:50  71  	out_exists := GLOBAL_CONSTANTS_V17.TRUE;
15:47:50  72  EXCEPTION
15:47:50  73  WHEN NO_DATA_FOUND THEN
15:47:50  74  	out_exists := GLOBAL_CONSTANTS_V17.FALSE;
15:47:50  75  WHEN OTHERS THEN
15:47:50  76  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:50  77  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:50  78  END ASSERT_ACCOUNT_EXISTS;
