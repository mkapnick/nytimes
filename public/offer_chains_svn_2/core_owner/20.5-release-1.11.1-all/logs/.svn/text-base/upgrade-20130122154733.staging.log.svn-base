15:47:49 427  	  CHARGE.CHARGE_STATUS_ID
15:47:49 428  	FROM
15:47:49 429  	  CHARGE
15:47:49 430  	WHERE
15:47:49 431  	  CHARGE.INVOICE_ID = in_invoice_id
15:47:49 432  	  AND CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V17.INSTRUMENT_CREDIT_CARD;
15:47:49 433  
15:47:49 434  	OPEN out_gc_charges_set FOR
15:47:49 435  	SELECT
15:47:49 436  	  CHARGE.ID as "CHARGE_ID",
15:47:49 437  	  CHARGE.CHARGE_AMOUNT,
15:47:49 438  	  CHARGE.INSTRUMENT_ID,
15:47:49 439  	  CHARGE.CHARGE_STATUS_ID
15:47:49 440  	FROM
15:47:49 441  	  CHARGE
15:47:49 442  	WHERE
15:47:49 443  	  CHARGE.INVOICE_ID = in_invoice_id
15:47:49 444  	  AND CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V17.INSTRUMENT_GIFT_CERTIFICATE;
15:47:49 445  
15:47:49 446  EXCEPTION
15:47:49 447  WHEN BAD_INVOICE_ID THEN
15:47:49 448  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 449  	  SPROC_NAME, 'No such invoice');
15:47:49 450  WHEN CAN_NOT_FIND_ACCOUNT THEN
15:47:49 451  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 452  	  SPROC_NAME, 'Could not find account for given invoice id', EXCEPTION_MESSAGE);
15:47:49 453  WHEN OTHERS THEN
15:47:49 454  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 455  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:49 456  END GET_INVOICE_DETAILS;
