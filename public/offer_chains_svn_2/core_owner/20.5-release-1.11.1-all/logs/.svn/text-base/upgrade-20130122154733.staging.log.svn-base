15:47:49 720  /******************************************************************************/
15:47:49 721  
15:47:49 722  PROCEDURE CALCULATE_INVOICE_CHARGEBACK (
15:47:49 723  	in_invoice_id	      IN NUMBER,
15:47:49 724  	in_chargeback_date    IN DATE,
15:47:49 725  	out_chargeback_amount OUT NUMBER
15:47:49 726  ) AS
15:47:49 727  -- VARIABLES
15:47:49 728  SPROC_NAME	     CONSTANT VARCHAR2(28) := 'CALCULATE_INVOICE_CHARGEBACK';
15:47:49 729  var_chargeback_date    DATE;
15:47:49 730  temp_invoice_id	     NUMBER;
15:47:49 731  var_licenses_number    NUMBER;
15:47:49 732  var_invoice_start_date DATE;
15:47:49 733  var_invoice_end_date   DATE;
15:47:49 734  var_offer_id	     NUMBER;
15:47:49 735  var_offer_days_interval NUMBER;
15:47:49 736  var_license_days_used  NUMBER;
15:47:49 737  var_invoice_amount     NUMBER(10,2);
15:47:49 738  var_offer_chain_id     NUMBER;
15:47:49 739  var_offer_chain_meta_data_val VARCHAR2(1024);
15:47:49 740  var_offer_chain_full_refund   NUMBER;
15:47:49 741  var_offer_chain_prorated_ref  NUMBER;
15:47:49 742  var_chargeback_calculated NUMBER;
15:47:49 743  var_max_invoice_refund	NUMBER;
15:47:49 744  -- EXCEPTIONS
15:47:49 745  BAD_INVOICE_ID		  EXCEPTION;
15:47:49 746  NO_LICENSES_FOUND_EXCEPTION EXCEPTION;
15:47:49 747  OFFER_LENGTH_IS_ZERO	  EXCEPTION;
15:47:49 748  BEGIN
15:47:49 749  
15:47:49 750  	IF in_chargeback_date IS NULL THEN
15:47:49 751  	  var_chargeback_date := PROCS_COMMON_V17.NORMALIZE_DATE(current_date);
15:47:49 752  	ELSE
15:47:49 753  	  var_chargeback_date := PROCS_COMMON_V17.NORMALIZE_DATE(in_chargeback_date);
15:47:49 754  	END IF;
15:47:49 755  
15:47:49 756  	-- Check that invoice exists
15:47:49 757  	BEGIN
15:47:49 758  	  SELECT
15:47:49 759  	    INVOICE.ID into temp_invoice_id
15:47:49 760  	  FROM
15:47:49 761  	    INVOICE
15:47:49 762  	  WHERE
15:47:49 763  	    INVOICE.ID = in_invoice_id;
15:47:49 764  	  EXCEPTION
15:47:49 765  	    WHEN NO_DATA_FOUND THEN
15:47:49 766  	      RAISE BAD_INVOICE_ID;
15:47:49 767  	END;
15:47:49 768  
15:47:49 769  	-- Check that invoice has at least one license
15:47:49 770  	SELECT
15:47:49 771  	  COUNT(*) into var_licenses_number
15:47:49 772  	FROM
15:47:49 773  	  LICENSE
15:47:49 774  	WHERE
15:47:49 775  	  LICENSE.INVOICE_ID = in_invoice_id;
15:47:49 776  
15:47:49 777  	IF var_licenses_number = 0 THEN
15:47:49 778  	  RAISE NO_LICENSES_FOUND_EXCEPTION;
15:47:49 779  	END IF;
15:47:49 780  
15:47:49 781  	SELECT
15:47:49 782  	  MIN(LICENSE.START_DATE) into var_invoice_start_date
15:47:49 783  	FROM
15:47:49 784  	  LICENSE
15:47:49 785  	WHERE
15:47:49 786  	  LICENSE.INVOICE_ID = in_invoice_id;
15:47:49 787  
15:47:49 788  	var_invoice_start_date := PROCS_COMMON_V17.NORMALIZE_DATE(var_invoice_start_date);
15:47:49 789  
15:47:49 790  	SELECT DISTINCT
15:47:49 791  	  LICENSE.OFFER_ID into var_offer_id
15:47:49 792  	FROM
15:47:49 793  	  LICENSE
15:47:49 794  	WHERE
15:47:49 795  	  LICENSE.INVOICE_ID = in_invoice_id;
15:47:49 796  
15:47:49 797  	SELECT
15:47:49 798  	  MAX (LICENSE.END_DATE) into var_invoice_end_date
15:47:49 799  	FROM
15:47:49 800  	  LICENSE
15:47:49 801  	WHERE
15:47:49 802  	  LICENSE.INVOICE_ID = in_invoice_id;
15:47:49 803  
15:47:49 804  	-- All licenses for given invoice should point into the same offer
15:47:49 805  
15:47:49 806  	PROCS_OFFER_CHAIN_V17.GET_OFFER_LENGTH_IN_DAYS(
15:47:49 807  	  in_offer_id	=> var_offer_id,
15:47:49 808  	  in_start_date => var_invoice_start_date,
15:47:49 809  	  out_days	=> var_offer_days_interval
15:47:49 810  	);
15:47:49 811  
15:47:49 812  	IF var_offer_days_interval = 0 THEN
15:47:49 813  	  RAISE OFFER_LENGTH_IS_ZERO;
15:47:49 814  	END IF;
15:47:49 815  
15:47:49 816  	PROCS_INVOICE_V17.GET_INVOICE_DAYS_USED_NUMBER(
15:47:49 817  	  in_invoice_id      => in_invoice_id,
15:47:49 818  	  in_chargeback_date => var_chargeback_date,
15:47:49 819  	  out_days_num	     => var_license_days_used
15:47:49 820  	);
15:47:49 821  
15:47:49 822  	PROCS_INVOICE_V17.CALCULATE_INVOICE_AMOUNT(
15:47:49 823  	  in_invoice_id => in_invoice_id,
15:47:49 824  	  out_amount	=> var_invoice_amount
15:47:49 825  	);
15:47:49 826  
15:47:49 827  	-- 39437
15:47:49 828  	-- Get offer chain id by invoice id
15:47:49 829  	SELECT DISTINCT
15:47:49 830  	  SUBSCRIPTION.OFFER_CHAIN_ID into var_offer_chain_id
15:47:49 831  	FROM
15:47:49 832  	  SUBSCRIPTION
15:47:49 833  	  INNER JOIN LICENSE ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:47:49 834  	WHERE
15:47:49 835  	  LICENSE.INVOICE_ID = in_invoice_id;
15:47:49 836  
15:47:49 837  	-- Get offer chain's meta data full amount value
15:47:49 838  	PROCS_OFFER_CHAIN_V17.GET_OFFER_CHAIN_MD_VALUE(
15:47:49 839  	  in_offer_chain_id => var_offer_chain_id,
15:47:49 840  	  in_meta_data_name => GLOBAL_CONSTANTS_V17.OFFER_CHAIN_FULL_REFUND,
15:47:49 841  	  out_value	    => var_offer_chain_meta_data_val
15:47:49 842  	);
15:47:49 843  	IF var_offer_chain_meta_data_val IS NULL THEN
15:47:49 844  	  var_offer_chain_full_refund := NULL;
15:47:49 845  	ELSE
15:47:49 846  	  var_offer_chain_full_refund := TO_NUMBER(var_offer_chain_meta_data_val);
15:47:49 847  	END IF;
15:47:49 848  
15:47:49 849  	-- Get offer chain's meta data prorated amount value
15:47:49 850  	PROCS_OFFER_CHAIN_V17.GET_OFFER_CHAIN_MD_VALUE(
15:47:49 851  	  in_offer_chain_id => var_offer_chain_id,
15:47:49 852  	  in_meta_data_name => GLOBAL_CONSTANTS_V17.OFFER_CHAIN_PRORATED_REFUND,
15:47:49 853  	  out_value	    => var_offer_chain_meta_data_val
15:47:49 854  	);
15:47:49 855  	IF var_offer_chain_meta_data_val IS NULL THEN
15:47:49 856  	  var_offer_chain_prorated_ref := NULL;
15:47:49 857  	ELSE
15:47:49 858  	  var_offer_chain_prorated_ref := TO_NUMBER(var_offer_chain_meta_data_val);
15:47:49 859  	END IF;
15:47:49 860  
15:47:49 861  	var_chargeback_calculated := GLOBAL_CONSTANTS_V17.FALSE;
15:47:49 862  
15:47:49 863  	IF var_offer_chain_full_refund IS NOT NULL
15:47:49 864  	   AND var_chargeback_calculated = GLOBAL_CONSTANTS_V17.FALSE THEN
15:47:49 865  	  IF var_license_days_used < var_offer_chain_full_refund THEN
15:47:49 866  	    out_chargeback_amount := var_invoice_amount;
15:47:49 867  	    var_chargeback_calculated := GLOBAL_CONSTANTS_V17.TRUE;
15:47:49 868  	  END IF;
15:47:49 869  	END IF;
15:47:49 870  
15:47:49 871  	IF var_offer_chain_prorated_ref IS NOT NULL
15:47:49 872  	   AND var_chargeback_calculated = GLOBAL_CONSTANTS_V17.FALSE THEN
15:47:49 873  	  IF var_license_days_used < var_offer_chain_prorated_ref THEN
15:47:49 874  	    out_chargeback_amount := ( var_invoice_amount * (var_offer_days_interval - var_license_days_used) ) / var_offer_days_interval;
15:47:49 875  	    var_chargeback_calculated := GLOBAL_CONSTANTS_V17.TRUE;
15:47:49 876  	  END IF;
15:47:49 877  	END IF;
15:47:49 878  
15:47:49 879  	IF var_chargeback_calculated = GLOBAL_CONSTANTS_V17.TRUE THEN
15:47:49 880  	  PROCS_INVOICE_V17.GET_MAX_REFUND(
15:47:49 881  	    in_invoice_id => in_invoice_id,
15:47:49 882  	    out_amount	  => var_max_invoice_refund
15:47:49 883  	  );
15:47:49 884  	  IF var_max_invoice_refund < out_chargeback_amount THEN
15:47:49 885  	    out_chargeback_amount := var_max_invoice_refund;
15:47:49 886  	  END IF;
15:47:49 887  	END IF;
15:47:49 888  
15:47:49 889  	IF var_chargeback_calculated = GLOBAL_CONSTANTS_V17.FALSE THEN
15:47:49 890  	  out_chargeback_amount := 0;
15:47:49 891  	END IF;
15:47:49 892  
15:47:49 893  EXCEPTION
15:47:49 894  WHEN BAD_INVOICE_ID THEN
15:47:49 895  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 896  	  SPROC_NAME, 'No such invoice');
15:47:49 897  WHEN NO_LICENSES_FOUND_EXCEPTION THEN
15:47:49 898  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 899  	  SPROC_NAME, 'No licenses found');
15:47:49 900  WHEN OFFER_LENGTH_IS_ZERO THEN
15:47:49 901  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:49 902  	  SPROC_NAME, 'Offer length is zero');
15:47:49 903  WHEN OTHERS THEN
15:47:49 904  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 905  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:49 906  END CALCULATE_INVOICE_CHARGEBACK;
15:47:49 907  
15:47:49 908  /********************************************************************/
15:47:49 909  
15:47:49 910  PROCEDURE APPLY_REFUND (
15:47:49 911  	in_invoice_id	     IN NUMBER,
15:47:49 912  	in_chargeback_amount IN NUMBER,
15:47:49 913  	in_created_by	     IN VARCHAR2,
15:47:49 914  	out_charge_id	     OUT NUMBER
15:47:49 915  ) AS
15:47:49 916  -- VARIABLES
15:47:49 917  SPROC_NAME		 CONSTANT VARCHAR2(16) := 'APPLY_CHARGEBACK';
15:47:49 918  temp_invoice_id		 NUMBER;
15:47:49 919  var_total_charges_amount	 NUMBER(10,2);
15:47:49 920  var_charge_amount_to_apply NUMBER(10,2);
15:47:49 921  var_transaction_id	 NUMBER;
15:47:49 922  var_instrument_type_id	 NUMBER;
15:47:49 923  var_instrument_id 	 NUMBER;
15:47:49 924  var_charge_id		 NUMBER;
15:47:49 925  -- EXCEPTIONS
15:47:49 926  BAD_INVOICE_ID		 EXCEPTION;
15:47:49 927  CAN_NOT_CREATE_TRANSACTION EXCEPTION;
15:47:49 928  CAN_NOT_FIND_INSTRUMENT	 EXCEPTION;
15:47:49 929  CAN_NOT_CREATE_CHARGE	 EXCEPTION;
15:47:49 930  EXCEPTION_MESSAGE 	 VARCHAR2(1024);
15:47:49 931  BEGIN
15:47:49 932  
15:47:49 933  	BEGIN
15:47:49 934  	  SELECT
15:47:49 935  	    INVOICE.ID into temp_invoice_id
15:47:49 936  	  FROM
15:47:49 937  	    INVOICE
15:47:49 938  	  WHERE
15:47:49 939  	    INVOICE.ID = in_invoice_id;
15:47:49 940  	  EXCEPTION
15:47:49 941  	    WHEN NO_DATA_FOUND THEN
15:47:49 942  	      RAISE BAD_INVOICE_ID;
15:47:49 943  	END;
15:47:49 944  
15:47:49 945  	SELECT
15:47:49 946  	  SUM (CHARGE.CHARGE_AMOUNT) into var_total_charges_amount
15:47:49 947  	FROM
15:47:49 948  	  CHARGE
15:47:49 949  	WHERE
15:47:49 950  	  CHARGE.INVOICE_ID = in_invoice_id
15:47:49 951  	  AND CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V17.CHARGE_PROCESSED
15:47:49 952  	  AND CHARGE.INSTRUMENT_TYPE_ID != GLOBAL_ENUMS_V17.INSTRUMENT_GIFT_CERTIFICATE;
15:47:49 953  
15:47:49 954  	-- FIXME: Maybe whe should throw exception here?
15:47:49 955  	IF var_total_charges_amount < in_chargeback_amount THEN
15:47:49 956  	  var_charge_amount_to_apply := var_total_charges_amount;
15:47:49 957  	ELSE
15:47:49 958  	  var_charge_amount_to_apply := in_chargeback_amount;
15:47:49 959  	END IF;
15:47:49 960  
15:47:49 961  	BEGIN
15:47:49 962  	  PROCS_TRANSACTION_V17.CREATE_TRANSACTION(
15:47:49 963  	    in_transaction_id  => NULL,
15:47:49 964  	    in_status_id       => GLOBAL_STATUSES_V17.TRANSACTION_PREPARE,
15:47:49 965  	    in_amount	       => -var_charge_amount_to_apply,
15:47:49 966  	    in_created_by      => in_created_by,
15:47:49 967  	    in_order_id        => NULL,
15:47:49 968  	    in_is_refund       => GLOBAL_CONSTANTS_V17.TRUE,
15:47:49 969  	    out_transaction_id => var_transaction_id
15:47:49 970  	  );
15:47:49 971  	  EXCEPTION
15:47:49 972  	    WHEN OTHERS THEN
15:47:49 973  	      EXCEPTION_MESSAGE := SQLERRM;
15:47:49 974  	      RAISE CAN_NOT_CREATE_TRANSACTION;
15:47:49 975  	END;
15:47:49 976  
15:47:49 977  	BEGIN
15:47:49 978  	  SELECT
15:47:49 979  	    C.INSTRUMENT_TYPE_ID,
15:47:49 980  	    C.INSTRUMENT_ID
15:47:49 981  	    into
15:47:49 982  	    var_instrument_type_id,
15:47:49 983  	    var_instrument_id
15:47:49 984  	  FROM
15:47:49 985  	    CHARGE C,
15:47:49 986  	    TRANSACTION_ATTEMPT TA,
15:47:49 987  	    TRANSACTION T
15:47:49 988  	  WHERE
15:47:49 989  	    C.INVOICE_ID = in_invoice_id and
15:47:49 990  	    C.TRANSACTION_ID = T.ID and
15:47:49 991  	    TA.TRANSACTION_ID = T.ID and
15:47:49 992  	    TA.TRANSACTION_ATTEMPT_STATUS_ID = GLOBAL_STATUSES_V17.TRANS_ATTEMPT_SUCCESS and
15:47:49 993  	    T.IS_REFUND = GLOBAL_CONSTANTS_V17.FALSE and
15:47:49 994  	    T.TRANSACTION_AMOUNT >= 0 and
15:47:49 995  	    rownum < 2;
15:47:49 996  	EXCEPTION
15:47:49 997  	    WHEN NO_DATA_FOUND THEN
15:47:49 998  	      RAISE CAN_NOT_FIND_INSTRUMENT;
15:47:49 999  	END;
15:47:49 1000  	 BEGIN
15:47:49 1001  	   PROCS_CHARGE_V17.CREATE_CHARGE(
15:47:49 1002  	     in_invoice_id	   => in_invoice_id,
15:47:49 1003  	     in_transaction_id	   => var_transaction_id,
15:47:49 1004  	     in_instrument_type_id => var_instrument_type_id,
15:47:49 1005  	     in_instrument_id	   => var_instrument_id,
15:47:49 1006  	     in_charge_amount	   => -var_charge_amount_to_apply,
15:47:49 1007  	     in_created_by	   => in_created_by,
15:47:49 1008  	     in_charge_status_id   => GLOBAL_STATUSES_V17.CHARGE_OPENED,
15:47:49 1009  	     out_charge_id	   => var_charge_id
15:47:49 1010  	   );
15:47:49 1011  	   EXCEPTION
15:47:49 1012  	     WHEN OTHERS THEN
15:47:49 1013  	       EXCEPTION_MESSAGE := SQLERRM;
15:47:49 1014  	       RAISE CAN_NOT_CREATE_CHARGE;
15:47:49 1015  	 END;
15:47:49 1016  
15:47:49 1017  	 out_charge_id := var_charge_id;
15:47:49 1018  
15:47:49 1019  EXCEPTION
15:47:49 1020  WHEN BAD_INVOICE_ID THEN
15:47:49 1021  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 1022  	   SPROC_NAME, 'No such invoice');
15:47:49 1023  WHEN CAN_NOT_CREATE_TRANSACTION THEN
15:47:49 1024  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:49 1025  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
15:47:49 1026  WHEN CAN_NOT_FIND_INSTRUMENT THEN
15:47:49 1027  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 1028  	   SPROC_NAME, 'Could not find financial instrument');
15:47:49 1029  WHEN CAN_NOT_CREATE_CHARGE THEN
15:47:49 1030  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:49 1031  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
15:47:49 1032  WHEN OTHERS THEN
15:47:49 1033  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 1034  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:47:49 1035  END APPLY_REFUND;
15:47:49 1036  
15:47:49 1037  /******************************************************************************/
15:47:49 1038  
15:47:49 1039  PROCEDURE GET_MAX_REFUND (
15:47:49 1040  	 in_invoice_id IN NUMBER,
15:47:49 1041  	 out_amount    OUT NUMBER
15:47:49 1042  ) AS
15:47:49 1043  SPROC_NAME CONSTANT VARCHAR2(14) := 'GET_MAX_REFUND';
15:47:49 1044  -- VARIABLES
15:47:49 1045  temp_invoice_id	       NUMBER;
15:47:49 1046  var_invoice_refunds_sum NUMBER(10,2);
15:47:49 1047  var_invoice_charges_sum NUMBER(10,2);
15:47:49 1048  -- EXCEPTIONS
15:47:49 1049  BAD_INVOICE_ID EXCEPTION;
15:47:49 1050  BEGIN
15:47:49 1051  
15:47:49 1052  	 BEGIN
15:47:49 1053  	   SELECT
15:47:49 1054  	     INVOICE.ID into temp_invoice_id
15:47:49 1055  	   FROM
15:47:49 1056  	     INVOICE
15:47:49 1057  	   WHERE
15:47:49 1058  	     INVOICE.ID = in_invoice_id;
15:47:49 1059  	   EXCEPTION
15:47:49 1060  	     WHEN NO_DATA_FOUND THEN
15:47:49 1061  	       RAISE BAD_INVOICE_ID;
15:47:49 1062  	 END;
15:47:49 1063  
15:47:49 1064  	 var_invoice_refunds_sum := 0;
15:47:49 1065  	 var_invoice_charges_sum := 0;
15:47:49 1066  
15:47:49 1067  	 FOR f_charge IN (
15:47:49 1068  	   SELECT
15:47:49 1069  	     CHARGE.ID,
15:47:49 1070  	     CHARGE.CHARGE_STATUS_ID,
15:47:49 1071  	     CHARGE.CHARGE_AMOUNT,
15:47:49 1072  	     CHARGE.TRANSACTION_ID
15:47:49 1073  	   FROM
15:47:49 1074  	     CHARGE
15:47:49 1075  	   WHERE
15:47:49 1076  	     CHARGE.INVOICE_ID = in_invoice_id
15:47:49 1077  	 )
15:47:49 1078  	 LOOP
15:47:49 1079  	   -- If charge.status = canceled then continue
15:47:49 1080  	   IF f_charge.CHARGE_STATUS_ID != GLOBAL_STATUSES_V17.CHARGE_CANCELED THEN
15:47:49 1081  
15:47:49 1082  	     IF f_charge.CHARGE_AMOUNT > 0 THEN
15:47:49 1083  	       IF f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V17.CHARGE_PROCESSED
15:47:49 1084  		  AND PROCS_TRANSACTION_V17.IS_TRANSACTION_COLLECTED(f_charge.TRANSACTION_ID) = GLOBAL_CONSTANTS_V17.TRUE THEN
15:47:49 1085  		 -- Transaction collected
15:47:49 1086  		 var_invoice_charges_sum := var_invoice_charges_sum + f_charge.CHARGE_AMOUNT;
15:47:49 1087  	       ELSE
15:47:49 1088  		 -- Transaction is not collected. Do nothing
15:47:49 1089  		 NULL;
15:47:49 1090  	       END IF;
15:47:49 1091  	     ELSE
15:47:49 1092  	       IF f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V17.CHARGE_PROCESSED
15:47:49 1093  		  AND PROCS_TRANSACTION_V17.IS_TRANSACTION_COLLECTED(f_charge.TRANSACTION_ID) = GLOBAL_CONSTANTS_V17.FALSE THEN
15:47:49 1094  		 -- If charge is processed transaction is not collected then do nothing
15:47:49 1095  		 NULL;
15:47:49 1096  	       ELSE
15:47:49 1097  		 var_invoice_refunds_sum := var_invoice_refunds_sum + f_charge.CHARGE_AMOUNT;
15:47:49 1098  	       END IF;
15:47:49 1099  	     END IF;
15:47:49 1100  
15:47:49 1101  	   END IF;
15:47:49 1102  	 END LOOP;
15:47:49 1103  
15:47:49 1104  	 -- Refunds are negative
15:47:49 1105  	 var_invoice_refunds_sum := 0 - var_invoice_refunds_sum;
15:47:49 1106  
15:47:49 1107  	 out_amount := var_invoice_charges_sum - var_invoice_refunds_sum;
15:47:49 1108  
15:47:49 1109  EXCEPTION
15:47:49 1110  WHEN BAD_INVOICE_ID THEN
15:47:49 1111  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 1112  	   SPROC_NAME, 'No such invoice');
15:47:49 1113  WHEN OTHERS THEN
15:47:49 1114  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 1115  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:47:49 1116  END GET_MAX_REFUND;
