15:47:50 1065  
15:47:50 1066  PROCEDURE PURCHASE_GIFT_CERTIFICATE (
15:47:50 1067  	 in_group_id		   IN NUMBER,
15:47:50 1068  	 in_offer_chain_id	   IN VARCHAR2,
15:47:50 1069  	 in_gift_certificate_code  IN VARCHAR2,
15:47:50 1070  	 in_created_by		   IN VARCHAR2,
15:47:50 1071  	 in_recipient_name	   IN VARCHAR2,
15:47:50 1072  	 in_recipient_email	   IN VARCHAR2,
15:47:50 1073  	 in_sender_name 	   IN VARCHAR2,
15:47:50 1074  	 in_sender_email	   IN VARCHAR2,
15:47:50 1075  	 in_gift_message	   IN VARCHAR2,
15:47:50 1076  	 in_instrument_id	   IN NUMBER,
15:47:50 1077  	 in_instrument_type_id	   IN NUMBER,
15:47:50 1078  	 in_expiration_date	   IN DATE,
15:47:50 1079  	 in_order_id		   IN VARCHAR2,
15:47:50 1080  	 in_transaction_id	   IN NUMBER
15:47:50 1081  ) AS
15:47:50 1082  SPROC_NAME	  CONSTANT VARCHAR2(25) := 'PURCHASE_GIFT_CERTIFICATE';
15:47:50 1083  -- VARIABLES
15:47:50 1084  var_gift_cert_id   NUMBER;
15:47:50 1085  var_account_id	  NUMBER;
15:47:50 1086  var_invoice_amount NUMBER (10,2);
15:47:50 1087  var_new_invoice_id NUMBER;
15:47:50 1088  var_new_charge_id  NUMBER;
15:47:50 1089  var_och_is_gc	  NUMBER;
15:47:50 1090  var_offer_chain_status_id NUMBER;
15:47:50 1091  var_is_for_redemption	 NUMBER;
15:47:50 1092  var_account_tax_exempt_id VARCHAR2(255);
15:47:50 1093  
15:47:50 1094  temp_transaction_id_count NUMBER;
15:47:50 1095  var_transaction_id	 NUMBER;
15:47:50 1096  temp_gc_code VARCHAR2(255);
15:47:50 1097  
15:47:50 1098  var_invoice_status_id NUMBER;
15:47:50 1099  -- EXCEPTIONS
15:47:50 1100  CAN_NOT_CREATE_INVOICE		   EXCEPTION;
15:47:50 1101  CAN_NOT_CREATE_TRANSACTION	   EXCEPTION;
15:47:50 1102  CAN_NOT_CREATE_CHARGE		   EXCEPTION;
15:47:50 1103  CAN_NOT_CREATE_LINE_ITEMS	   EXCEPTION;
15:47:50 1104  BAD_GROUP_ID			   EXCEPTION;
15:47:50 1105  BAD_OFFER_CHAIN_ID		   EXCEPTION;
15:47:50 1106  OCH_IS_NOT_GIFT_CERTIFICATE	   EXCEPTION;
15:47:50 1107  TRANSACTION_EXISTS		   EXCEPTION;
15:47:50 1108  GC_CODE_ALREADY_EXISTS		   EXCEPTION;
15:47:50 1109  BAD_INSTRUMENT_TYPE		   EXCEPTION;
15:47:50 1110  BAD_CREDIT_CARD_ID		   EXCEPTION;
15:47:50 1111  BAD_PAYPAL_ID			   EXCEPTION;
15:47:50 1112  CAN_NOT_CALCULATE_OCH_AMOUNT	   EXCEPTION;
15:47:50 1113  BAD_OFFER_CHAIN_STATUS		   EXCEPTION;
15:47:50 1114  OFFER_REC_NUM_LESS_THAN_ONE	   EXCEPTION;
15:47:50 1115  CAN_NOT_CALC_INVOICE_AMOUNT	   EXCEPTION;
15:47:50 1116  CAN_NOT_USE_FCINSTR		   EXCEPTION;
15:47:50 1117  CAN_NOT_PURCHASE_GC_FOR_RDMPN	   EXCEPTION;
15:47:50 1118  EXCEPTION_MESSAGE		   VARCHAR2(1024);
15:47:50 1119  BEGIN
15:47:50 1120  
15:47:50 1121  	 -- Get account id
15:47:50 1122  	 BEGIN
15:47:50 1123  	   SELECT
15:47:50 1124  	     ACCOUNT.ID,
15:47:50 1125  	     ACCOUNT.TAX_EXEMPT_ID
15:47:50 1126  	     into
15:47:50 1127  	     var_account_id,
15:47:50 1128  	     var_account_tax_exempt_id
15:47:50 1129  	   FROM
15:47:50 1130  	     ACCOUNT
15:47:50 1131  	   WHERE
15:47:50 1132  	     ACCOUNT.GROUP_ID = in_group_id;
15:47:50 1133  	   EXCEPTION
15:47:50 1134  	   WHEN NO_DATA_FOUND THEN
15:47:50 1135  	     RAISE BAD_GROUP_ID;
15:47:50 1136  	 END;
15:47:50 1137  
15:47:50 1138  	 -- Check that instrument exists
15:47:50 1139  	 IF in_instrument_type_id = GLOBAL_ENUMS_V17.INSTRUMENT_CREDIT_CARD THEN
15:47:50 1140  	   IF IS_CREDIT_CARD_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V17.FALSE THEN
15:47:50 1141  	     RAISE BAD_CREDIT_CARD_ID;
15:47:50 1142  	   END IF;
15:47:50 1143  	 ELSIF in_instrument_type_id = GLOBAL_ENUMS_V17.INSTRUMENT_PAYPAL THEN
15:47:50 1144  	   IF IS_PAYPAL_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V17.FALSE THEN
15:47:50 1145  	     RAISE BAD_PAYPAL_ID;
15:47:50 1146  	   END IF;
15:47:50 1147  	 ELSIF in_instrument_type_id = GLOBAL_ENUMS_V17.INSTRUMENT_ZCI_INSTRUMENT THEN
15:47:50 1148  	   NULL;
15:47:50 1149  	 ELSE
15:47:50 1150  	   RAISE BAD_INSTRUMENT_TYPE;
15:47:50 1151  	 END IF;
15:47:50 1152  
15:47:50 1153  	 -- Check for the same code
15:47:50 1154  	 BEGIN
15:47:50 1155  	   SELECT
15:47:50 1156  	     GIFT_CERTIFICATE.CODE into temp_gc_code
15:47:50 1157  	   FROM
15:47:50 1158  	     GIFT_CERTIFICATE
15:47:50 1159  	   WHERE
15:47:50 1160  	     GIFT_CERTIFICATE.CODE = in_gift_certificate_code;
15:47:50 1161  
15:47:50 1162  	   RAISE GC_CODE_ALREADY_EXISTS;
15:47:50 1163  
15:47:50 1164  	   EXCEPTION
15:47:50 1165  	     WHEN NO_DATA_FOUND THEN
15:47:50 1166  	       NULL;
15:47:50 1167  	 END;
15:47:50 1168  
15:47:50 1169  	 -- Get offer chain flag "is_gift_certificate"
15:47:50 1170  	 BEGIN
15:47:50 1171  	   SELECT
15:47:50 1172  	     OFFER_CHAIN.IS_GIFT_CERTIFICATE,
15:47:50 1173  	     OFFER_CHAIN.OFFER_CHAIN_STATUS_ID
15:47:50 1174  	     into
15:47:50 1175  	     var_och_is_gc,
15:47:50 1176  	     var_offer_chain_status_id
15:47:50 1177  	   FROM
15:47:50 1178  	     OFFER_CHAIN
15:47:50 1179  	   WHERE
15:47:50 1180  	     OFFER_CHAIN.ID = in_offer_chain_id;
15:47:50 1181  	   EXCEPTION
15:47:50 1182  	   WHEN NO_DATA_FOUND THEN
15:47:50 1183  	     RAISE BAD_OFFER_CHAIN_ID;
15:47:50 1184  	 END;
15:47:50 1185  
15:47:50 1186  	 IF var_och_is_gc != GLOBAL_CONSTANTS_V17.TRUE
15:47:50 1187  	   OR var_och_is_gc IS NULL THEN
15:47:50 1188  	   RAISE OCH_IS_NOT_GIFT_CERTIFICATE;
15:47:50 1189  	 END IF;
15:47:50 1190  
15:47:50 1191  	 IF var_offer_chain_status_id != GLOBAL_STATUSES_V17.OFFER_CHAIN_ACTIVE THEN
15:47:50 1192  	   RAISE BAD_OFFER_CHAIN_STATUS;
15:47:50 1193  	 END IF;
15:47:50 1194  
15:47:50 1195  	 -- norlov: #38151 check if the OC is for Redemption:
15:47:50 1196  	 SELECT
15:47:50 1197  	   COUNT(*) into var_is_for_redemption
15:47:50 1198  	 FROM
15:47:50 1199  	   OFFER_CHAIN_ELIGIBILITY
15:47:50 1200  	 WHERE
15:47:50 1201  	   OFFER_CHAIN_ELIGIBILITY.OFFER_CHAIN_ID = in_offer_chain_id
15:47:50 1202  	   AND OFFER_CHAIN_ELIGIBILITY.NAME = GLOBAL_CONSTANTS_V17.GIFT_CERTIFICATE_REQUIRED
15:47:50 1203  	   AND OFFER_CHAIN_ELIGIBILITY.VALUE = GLOBAL_CONSTANTS_V17.ELIGIBILITY_FLAG_SET;
15:47:50 1204  
15:47:50 1205  	 IF var_is_for_redemption > 0 THEN
15:47:50 1206  	   RAISE CAN_NOT_PURCHASE_GC_FOR_RDMPN;
15:47:50 1207  	 END IF;
15:47:50 1208  
15:47:50 1209  	 -- Check that transaction with given id do not exists
15:47:50 1210  	 SELECT
15:47:50 1211  	   COUNT(*) into temp_transaction_id_count
15:47:50 1212  	 FROM
15:47:50 1213  	   TRANSACTION
15:47:50 1214  	 WHERE
15:47:50 1215  	   TRANSACTION.ID = in_transaction_id;
15:47:50 1216  
15:47:50 1217  	 IF temp_transaction_id_count > 0 THEN
15:47:50 1218  	   RAISE TRANSACTION_EXISTS;
15:47:50 1219  	 END IF;
15:47:50 1220  
15:47:50 1221  	 -- Create new invoice
15:47:50 1222  	 BEGIN
15:47:50 1223  	   PROCS_INVOICE_V17.CREATE_INVOICE(
15:47:50 1224  	     in_invoice_status => GLOBAL_STATUSES_V17.INVOICE_OPEN,
15:47:50 1225  	     in_created_by     => in_created_by,
15:47:50 1226  	     in_tax_exempt_id  => var_account_tax_exempt_id,
15:47:50 1227  	     out_invoice_id    => var_new_invoice_id
15:47:50 1228  	   );
15:47:50 1229  	   EXCEPTION
15:47:50 1230  	     WHEN OTHERS THEN
15:47:50 1231  	       EXCEPTION_MESSAGE := SQLERRM;
15:47:50 1232  	       RAISE CAN_NOT_CREATE_INVOICE;
15:47:50 1233  	 END;
15:47:50 1234  
15:47:50 1235  	 -- Add line items for new invoice
15:47:50 1236  	 BEGIN
15:47:50 1237  	   FOR f_offer_data IN (
15:47:50 1238  	     SELECT
15:47:50 1239  	       OFFER_ID,
15:47:50 1240  	       NUM_RECURRENCES
15:47:50 1241  	     FROM
15:47:50 1242  	       OFFER_OFFER_CHAIN
15:47:50 1243  	     WHERE
15:47:50 1244  	       OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
15:47:50 1245  	   )
15:47:50 1246  	   LOOP
15:47:50 1247  	     IF f_offer_data.NUM_RECURRENCES < 1 THEN
15:47:50 1248  	       RAISE OFFER_REC_NUM_LESS_THAN_ONE;
15:47:50 1249  	     END IF;
15:47:50 1250  	     FOR i_offer_recurrences_iterator IN 1..f_offer_data.NUM_RECURRENCES
15:47:50 1251  	     LOOP
15:47:50 1252  	       PROCS_LINE_ITEMS_V17.ADD_LINE_ITEMS(
15:47:50 1253  		 in_invoice_id => var_new_invoice_id,
15:47:50 1254  		 in_offer_id   => f_offer_data.OFFER_ID,
15:47:50 1255  		 in_created_by => in_created_by
15:47:50 1256  	       );
15:47:50 1257  	     END LOOP;
15:47:50 1258  	   END LOOP;
15:47:50 1259  
15:47:50 1260  	   EXCEPTION
15:47:50 1261  	     WHEN OTHERS THEN
15:47:50 1262  	       EXCEPTION_MESSAGE := SQLERRM;
15:47:50 1263  	       RAISE CAN_NOT_CREATE_LINE_ITEMS;
15:47:50 1264  	 END;
15:47:50 1265  
15:47:50 1266  	 -- Calculate new invoice amount
15:47:50 1267  	 BEGIN
15:47:50 1268  	   PROCS_INVOICE_V17.CALCULATE_INVOICE_AMOUNT(var_new_invoice_id, var_invoice_amount);
15:47:50 1269  	   EXCEPTION
15:47:50 1270  	     WHEN OTHERS THEN
15:47:50 1271  	       EXCEPTION_MESSAGE := SQLERRM;
15:47:50 1272  	       RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
15:47:50 1273  	 END;
15:47:50 1274  
15:47:50 1275  	 IF var_invoice_amount > 0
15:47:50 1276  	   AND in_instrument_type_id = GLOBAL_ENUMS_V17.INSTRUMENT_ZCI_INSTRUMENT THEN
15:47:50 1277  	   RAISE CAN_NOT_USE_FCINSTR;
15:47:50 1278  	 END IF;
15:47:50 1279  
15:47:50 1280  	 -- If invoice amount iz 0 then we need to set status for this invoice to PROCCESSED
15:47:50 1281  	 IF var_invoice_amount = 0 THEN
15:47:50 1282  	   PROCS_INVOICE_CRU_V17.UPDATE_INVOICE(
15:47:50 1283  	     in_invoice_id		    => var_new_invoice_id,
15:47:50 1284  	     in_updated_by		    => in_created_by,
15:47:50 1285  	     in_invoice_status_id	    => GLOBAL_STATUSES_V17.INVOICE_CLOSED
15:47:50 1286  	   );
15:47:50 1287  	 END IF;
15:47:50 1288  
15:47:50 1289  	 IF var_invoice_amount > 0 THEN
15:47:50 1290  	   -- Create transaction
15:47:50 1291  	   BEGIN
15:47:50 1292  	     PROCS_TRANSACTION_V17.CREATE_TRANSACTION(
15:47:50 1293  	       in_transaction_id  => in_transaction_id,
15:47:50 1294  	       in_status_id	  => GLOBAL_STATUSES_V17.TRANSACTION_PENDING,
15:47:50 1295  	       in_amount	  => var_invoice_amount,
15:47:50 1296  	       in_created_by	  => in_created_by,
15:47:50 1297  	       in_order_id	  => in_order_id,
15:47:50 1298  	       out_transaction_id => var_transaction_id
15:47:50 1299  	     );
15:47:50 1300  	     EXCEPTION
15:47:50 1301  	       WHEN OTHERS THEN
15:47:50 1302  		 EXCEPTION_MESSAGE := SQLERRM;
15:47:50 1303  		 RAISE CAN_NOT_CREATE_TRANSACTION;
15:47:50 1304  	   END;
15:47:50 1305  
15:47:50 1306  	   -- Create charge
15:47:50 1307  	   BEGIN
15:47:50 1308  	     PROCS_CHARGE_V17.CREATE_CHARGE(
15:47:50 1309  	       in_invoice_id	     => var_new_invoice_id,
15:47:50 1310  	       in_transaction_id     => var_transaction_id,
15:47:50 1311  	       in_instrument_type_id => in_instrument_type_id,
15:47:50 1312  	       in_instrument_id      => in_instrument_id,
15:47:50 1313  	       in_charge_amount      => var_invoice_amount,
15:47:50 1314  	       in_created_by	     => in_created_by,
15:47:50 1315  	       in_charge_status_id   => GLOBAL_STATUSES_V17.CHARGE_OPENED,
15:47:50 1316  	       out_charge_id	     => var_new_charge_id
15:47:50 1317  	     );
15:47:50 1318  	     EXCEPTION
15:47:50 1319  	       WHEN OTHERS THEN
15:47:50 1320  		 EXCEPTION_MESSAGE := SQLERRM;
15:47:50 1321  		 RAISE CAN_NOT_CREATE_CHARGE;
15:47:50 1322  	   END;
15:47:50 1323  	 END IF;
15:47:50 1324  
15:47:50 1325  	 -- Create new row in GIFT_CERTIFICATE table
15:47:50 1326  	 PROCS_FIN_INSTRUMENTS_CRU_V17.CREATE_GIFT_CERTIFICATE(
15:47:50 1327  	   out_gift_certificate_id	 => var_gift_cert_id,
15:47:50 1328  	   in_purchaser_group_id	 => in_group_id,
15:47:50 1329  	   in_purchaser_invoice_id	 => var_new_invoice_id,
15:47:50 1330  	   in_offer_chain_id		 => in_offer_chain_id,
15:47:50 1331  	   in_expiration_date		 => in_expiration_date,
15:47:50 1332  	   in_purchase_date		 => SYSDATE,
15:47:50 1333  	   in_gift_certificate_status_id => GLOBAL_STATUSES_V17.GIFT_CERTIFICATE_ACTIVE,
15:47:50 1334  	   in_code			 => in_gift_certificate_code,
15:47:50 1335  	   in_created_by		 => in_created_by,
15:47:50 1336  	   in_recipient_name		 => in_recipient_name,
15:47:50 1337  	   in_gift_message		 => in_gift_message,
15:47:50 1338  	   in_recipient_email		 => in_recipient_email,
15:47:50 1339  	   in_sender_email		 => in_sender_email,
15:47:50 1340  	   in_sender_name		 => in_sender_name
15:47:50 1341  	 );
15:47:50 1342  
15:47:50 1343  EXCEPTION
15:47:50 1344  WHEN CAN_NOT_USE_FCINSTR THEN
15:47:50 1345  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.STATE_ERROR,
15:47:50 1346  	   SPROC_NAME, 'Could not use "free charge instrument" for non-zero invoice');
15:47:50 1347  WHEN BAD_OFFER_CHAIN_STATUS THEN
15:47:50 1348  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.STATE_ERROR,
15:47:50 1349  	   SPROC_NAME, 'Offer chain is not active');
15:47:50 1350  WHEN CAN_NOT_CALCULATE_OCH_AMOUNT THEN
15:47:50 1351  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50 1352  	   SPROC_NAME, 'Could not calculate offer chain amount', EXCEPTION_MESSAGE);
15:47:50 1353  WHEN BAD_CREDIT_CARD_ID THEN
15:47:50 1354  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 1355  	   SPROC_NAME, 'Bad credit card id');
15:47:50 1356  WHEN BAD_PAYPAL_ID THEN
15:47:50 1357  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 1358  	   SPROC_NAME, 'Bad paypal id');
15:47:50 1359  WHEN BAD_INSTRUMENT_TYPE THEN
15:47:50 1360  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INVALID_PARAMETER,
15:47:50 1361  	   SPROC_NAME, 'Bad instrument type');
15:47:50 1362  WHEN GC_CODE_ALREADY_EXISTS THEN
15:47:50 1363  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.DUPLICATE_ERROR,
15:47:50 1364  	   SPROC_NAME, 'Gift certificate with same code already exists');
15:47:50 1365  WHEN OCH_IS_NOT_GIFT_CERTIFICATE THEN
15:47:50 1366  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INVALID_PARAMETER,
15:47:50 1367  	   SPROC_NAME, 'This offer chain can not be used for gift certificate');
15:47:50 1368  WHEN CAN_NOT_PURCHASE_GC_FOR_RDMPN THEN
15:47:50 1369  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INVALID_PARAMETER,
15:47:50 1370  	   SPROC_NAME, 'This offer chain can not be purchased for gift certificate since it is for redemption');
15:47:50 1371  WHEN TRANSACTION_EXISTS THEN
15:47:50 1372  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.DUPLICATE_ERROR,
15:47:50 1373  	   SPROC_NAME, 'Transaction with given id already exists');
15:47:50 1374  WHEN CAN_NOT_CREATE_INVOICE THEN
15:47:50 1375  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50 1376  	   SPROC_NAME, 'Could not create new invoice', EXCEPTION_MESSAGE);
15:47:50 1377  WHEN CAN_NOT_CREATE_TRANSACTION THEN
15:47:50 1378  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50 1379  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
15:47:50 1380  WHEN CAN_NOT_CREATE_CHARGE THEN
