15:47:50 317  
15:47:50 318  PROCEDURE DISABLE_CREDIT_CARD (
15:47:50 319  /*
15:47:50 320  Throws exceptions:
15:47:50 321  APP_EXCEPTION_CODES_V17.NOT_FOUND
15:47:50 322  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:50 323  APP_EXCEPTION_CODES_V17.INTERNAL_ERROR
15:47:50 324  */
15:47:50 325  	in_credit_card_id IN NUMBER,
15:47:50 326  	in_updated_by	  IN VARCHAR2
15:47:50 327  ) AS
15:47:50 328  SPROC_NAME		   CONSTANT VARCHAR2(19) := 'DISABLE_CREDIT_CARD';
15:47:50 329  -- VARIBLES
15:47:50 330  var_account_id		     NUMBER;
15:47:50 331  var_group_id		     NUMBER;
15:47:50 332  var_credit_card_status	     NUMBER;
15:47:50 333  var_pending_transactions_num   NUMBER;
15:47:50 334  var_pending_invoices_num	     NUMBER;
15:47:50 335  current_def_instrument_type_id NUMBER;
15:47:50 336  current_def_instrument_id      NUMBER;
15:47:50 337  -- EXCEPTIONS
15:47:50 338  BAD_CC_STATUS		   EXCEPTION;
15:47:50 339  PENDING_TRANSACTIONS_FOUNDED EXCEPTION;
15:47:50 340  CAN_NOT_GET_DEF_FINANCIAL    EXCEPTION;
15:47:50 341  CAN_NOT_DEL_DEF_FINANCIAL    EXCEPTION;
15:47:50 342  CAN_NOT_DISABLE_CREDIT_CARD  EXCEPTION;
15:47:50 343  EXCEPTION_MESSAGE 	   VARCHAR2(1024);
15:47:50 344  BEGIN
15:47:50 345  
15:47:50 346  	-- Get credit card status
15:47:50 347  	-- Get account id
15:47:50 348  	SELECT
15:47:50 349  	  CREDIT_CARD.CREDIT_CARD_STATUS_ID,
15:47:50 350  	  CREDIT_CARD.ACCOUNT_ID
15:47:50 351  	  into
15:47:50 352  	  var_credit_card_status,
15:47:50 353  	  var_account_id
15:47:50 354  	FROM
15:47:50 355  	  CREDIT_CARD
15:47:50 356  	WHERE
15:47:50 357  	  CREDIT_CARD.ID = in_credit_card_id;
15:47:50 358  
15:47:50 359  	-- Check that we can disable this credit card (STUB)
15:47:50 360  	IF F_CAN_DISABLE_CREDIT_CARD(in_credit_card_id) = GLOBAL_CONSTANTS_V17.FALSE THEN
15:47:50 361  	  RAISE CAN_NOT_DISABLE_CREDIT_CARD;
15:47:50 362  	END IF;
15:47:50 363  
15:47:50 364  	-- Get account id
15:47:50 365  	SELECT
15:47:50 366  	  ACCOUNT.GROUP_ID into var_group_id
15:47:50 367  	FROM
15:47:50 368  	  ACCOUNT
15:47:50 369  	WHERE
15:47:50 370  	  ACCOUNT.ID = var_account_id;
15:47:50 371  
15:47:50 372  	-- Card should to be active
15:47:50 373  	IF var_credit_card_status != GLOBAL_STATUSES_V17.CREDIT_CARD_ACTIVE THEN
15:47:50 374  	  RAISE BAD_CC_STATUS;
15:47:50 375  	END IF;
15:47:50 376  
15:47:50 377  	-- Looking for pending transactions associated with given credit card
15:47:50 378  	SELECT
15:47:50 379  	  COUNT(*) into var_pending_invoices_num
15:47:50 380  	FROM
15:47:50 381  	  CHARGE
15:47:50 382  	  INNER JOIN TRANSACTION ON CHARGE.TRANSACTION_ID = TRANSACTION.ID
15:47:50 383  	WHERE
15:47:50 384  	  CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V17.INSTRUMENT_CREDIT_CARD
15:47:50 385  	  AND CHARGE.INSTRUMENT_ID = in_credit_card_id
15:47:50 386  	  AND TRANSACTION.TRANSACTION_STATUS_ID = GLOBAL_STATUSES_V17.TRANSACTION_PENDING;
15:47:50 387  
15:47:50 388  	IF var_pending_invoices_num > 0 THEN
15:47:50 389  	  RAISE PENDING_TRANSACTIONS_FOUNDED;
15:47:50 390  	END IF;
15:47:50 391  
15:47:50 392  	-- Getting current default financial instrument
15:47:50 393  	BEGIN
15:47:50 394  	  GET_DEF_FINANCIAL_INSTRUMENT(
15:47:50 395  	    in_group_id 	   => var_group_id,
15:47:50 396  	    out_instrument_type_id => current_def_instrument_type_id,
15:47:50 397  	    out_instrument_id	   => current_def_instrument_id
15:47:50 398  	  );
15:47:50 399  	  EXCEPTION
15:47:50 400  	    WHEN OTHERS THEN
15:47:50 401  	      EXCEPTION_MESSAGE := SQLERRM;
15:47:50 402  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
15:47:50 403  	END;
15:47:50 404  
15:47:50 405  	-- Checking that credit card is not default
15:47:50 406  	IF current_def_instrument_type_id = GLOBAL_ENUMS_V17.INSTRUMENT_CREDIT_CARD
15:47:50 407  	  AND current_def_instrument_id = in_credit_card_id THEN
15:47:50 408  	  BEGIN
15:47:50 409  	    DEL_DEF_FINANCIAL_INSTRUMENT(
15:47:50 410  	      in_group_id => var_group_id
15:47:50 411  	    );
15:47:50 412  	  EXCEPTION
15:47:50 413  	    WHEN OTHERS THEN
15:47:50 414  	      EXCEPTION_MESSAGE := SQLERRM;
15:47:50 415  	      RAISE CAN_NOT_DEL_DEF_FINANCIAL;
15:47:50 416  	  END;
15:47:50 417  	END IF;
15:47:50 418  
15:47:50 419  	-- Update credit card status
15:47:50 420  	PROCS_FIN_INSTRUMENTS_V17.UPDATE_CREDIT_CARD_STATUS(
15:47:50 421  	  in_credit_card_id	   => in_credit_card_id,
15:47:50 422  	  in_updated_by 	   => in_updated_by,
15:47:50 423  	  in_credit_card_status_id => GLOBAL_STATUSES_V17.CREDIT_CARD_DISABLED
15:47:50 424  	);
15:47:50 425  
15:47:50 426  EXCEPTION
15:47:50 427  WHEN NO_DATA_FOUND THEN
15:47:50 428  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 429  	  SPROC_NAME, 'No such credit card');
15:47:50 430  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
15:47:50 431  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50 432  	  SPROC_NAME, 'Could not get current default financial instrument', EXCEPTION_MESSAGE);
15:47:50 433  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
15:47:50 434  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50 435  	  SPROC_NAME, 'Could not delete information about default financial instrument from account', EXCEPTION_MESSAGE);
15:47:50 436  WHEN BAD_CC_STATUS THEN
15:47:50 437  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.STATE_ERROR,
15:47:50 438  	  SPROC_NAME, 'Credit card is not active');
15:47:50 439  WHEN PENDING_TRANSACTIONS_FOUNDED THEN
15:47:50 440  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.STATE_ERROR,
15:47:50 441  	  SPROC_NAME, 'Account has pending charge which is using this card');
15:47:50 442  WHEN CAN_NOT_DISABLE_CREDIT_CARD THEN
15:47:50 443  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50 444  	  SPROC_NAME, 'Could not disable this credit card', EXCEPTION_MESSAGE);
15:47:50 445  WHEN OTHERS THEN
15:47:50 446  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:50 447  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:50 448  END DISABLE_CREDIT_CARD;
