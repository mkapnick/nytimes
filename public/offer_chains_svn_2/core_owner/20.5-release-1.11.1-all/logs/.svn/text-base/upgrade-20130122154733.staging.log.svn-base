15:47:49 327  
15:47:49 328  PROCEDURE GET_RECURRING_OFFER (
15:47:49 329  	in_license_id  IN NUMBER,
15:47:49 330  	out_result_set OUT SYS_REFCURSOR
15:47:49 331  ) AS
15:47:49 332  -- VARIABLES
15:47:49 333  SPROC_NAME		    CONSTANT VARCHAR2(19) := 'GET_RECURRING_OFFER';
15:47:49 334  var_offer_chain_id	    NUMBER;
15:47:49 335  var_offer_id		    NUMBER;
15:47:49 336  var_offer_num_recurrences     NUMBER;
15:47:49 337  var_license_cur_offer_rec_num NUMBER;
15:47:49 338  var_offer_index		    NUMBER;
15:47:49 339  -- EXCEPTIONS
15:47:49 340  BAD_LICENSE_ID	     EXCEPTION;
15:47:49 341  CAN_NOT_GET_OFFER_INFO EXCEPTION;
15:47:49 342  BEGIN
15:47:49 343  
15:47:49 344  	BEGIN
15:47:49 345  	  SELECT
15:47:49 346  	    SUBSCRIPTION.OFFER_CHAIN_ID,
15:47:49 347  	    LICENSE.OFFER_ID,
15:47:49 348  	    LICENSE.CURRENT_OFFER_RECURR_NUM
15:47:49 349  	    into
15:47:49 350  	    var_offer_chain_id,
15:47:49 351  	    var_offer_id,
15:47:49 352  	    var_license_cur_offer_rec_num
15:47:49 353  	  FROM
15:47:49 354  	    LICENSE
15:47:49 355  	    INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:47:49 356  	  WHERE
15:47:49 357  	    LICENSE.ID = in_license_id;
15:47:49 358  	  EXCEPTION
15:47:49 359  	    WHEN NO_DATA_FOUND THEN
15:47:49 360  	      RAISE BAD_LICENSE_ID;
15:47:49 361  	END;
15:47:49 362  
15:47:49 363  	BEGIN
15:47:49 364  	  SELECT
15:47:49 365  	    OFFER_OFFER_CHAIN.NUM_RECURRENCES,
15:47:49 366  	    OFFER_OFFER_CHAIN.INDEX_VALUE
15:47:49 367  	    into
15:47:49 368  	    var_offer_num_recurrences,
15:47:49 369  	    var_offer_index
15:47:49 370  	  FROM
15:47:49 371  	    OFFER_OFFER_CHAIN
15:47:49 372  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
15:47:49 373  	  WHERE
15:47:49 374  	    OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
15:47:49 375  	    AND OFFER_OFFER_CHAIN.OFFER_ID = var_offer_id;
15:47:49 376  	  EXCEPTION
15:47:49 377  	    WHEN NO_DATA_FOUND THEN
15:47:49 378  	      RAISE CAN_NOT_GET_OFFER_INFO;
15:47:49 379  	END;
15:47:49 380  
15:47:49 381  	IF var_offer_num_recurrences = 0 THEN
15:47:49 382  	  --out_result_set := NULL;
15:47:49 383  	  -- TODO: Remove this hardcode
15:47:49 384  	  OPEN out_result_set FOR
15:47:49 385  	  SELECT
15:47:49 386  	    OFFER.ID,
15:47:49 387  	    OFFER.OFFER_STATUS_ID,
15:47:49 388  	    OFFER.ENTITLEMENT_DURATION,
15:47:49 389  	    OFFER.CREATED_BY,
15:47:49 390  	    OFFER.CREATE_DATE,
15:47:49 391  	    OFFER.UPDATED_BY,
15:47:49 392  	    OFFER.UPDATE_DATE,
15:47:49 393  	    var_offer_num_recurrences as "RECURRENCE_NUMBER",
15:47:49 394  	    var_offer_index as "OFFER_INDEX"
15:47:49 395  	  FROM
15:47:49 396  	    OFFER_OFFER_CHAIN
15:47:49 397  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
15:47:49 398  	  WHERE ROWNUM = 0;
15:47:49 399  	ELSIF var_license_cur_offer_rec_num = var_offer_num_recurrences THEN
15:47:49 400  	  --out_result_set := NULL;
15:47:49 401  	  -- TODO: Remove this hardcode
15:47:49 402  	  OPEN out_result_set FOR
15:47:49 403  	  SELECT
15:47:49 404  	    OFFER.ID,
15:47:49 405  	    OFFER.OFFER_STATUS_ID,
15:47:49 406  	    OFFER.ENTITLEMENT_DURATION,
15:47:49 407  	    OFFER.CREATED_BY,
15:47:49 408  	    OFFER.CREATE_DATE,
15:47:49 409  	    OFFER.UPDATED_BY,
15:47:49 410  	    OFFER.UPDATE_DATE,
15:47:49 411  	    var_offer_num_recurrences as "RECURRENCE_NUMBER",
15:47:49 412  	    var_offer_index as "OFFER_INDEX"
15:47:49 413  	  FROM
15:47:49 414  	    OFFER_OFFER_CHAIN
15:47:49 415  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
15:47:49 416  	  WHERE ROWNUM = 0;
15:47:49 417  	ELSE
15:47:49 418  	  OPEN out_result_set FOR
15:47:49 419  	  SELECT
15:47:49 420  	    OFFER.ID,
15:47:49 421  	    OFFER.OFFER_STATUS_ID,
15:47:49 422  	    OFFER.ENTITLEMENT_DURATION,
15:47:49 423  	    OFFER.CREATED_BY,
15:47:49 424  	    OFFER.CREATE_DATE,
15:47:49 425  	    OFFER.UPDATED_BY,
15:47:49 426  	    OFFER.UPDATE_DATE,
15:47:49 427  	    var_offer_num_recurrences as "RECURRENCE_NUMBER",
15:47:49 428  	    var_offer_index as "OFFER_INDEX"
15:47:49 429  	  FROM
15:47:49 430  	    OFFER
15:47:49 431  	  WHERE
15:47:49 432  	    OFFER.ID = var_offer_id;
15:47:49 433  	END IF;
15:47:49 434  
15:47:49 435  EXCEPTION
15:47:49 436  WHEN BAD_LICENSE_ID THEN
15:47:49 437  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 438  	  SPROC_NAME, 'No such license');
15:47:49 439  WHEN CAN_NOT_GET_OFFER_INFO THEN
15:47:49 440  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 441  	  SPROC_NAME, 'Could not get offer information. Offer id = '||var_offer_id||', Offer chain id = '||var_offer_chain_id);
15:47:49 442  WHEN OTHERS THEN
15:47:49 443  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 444  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:49 445  END GET_RECURRING_OFFER;
