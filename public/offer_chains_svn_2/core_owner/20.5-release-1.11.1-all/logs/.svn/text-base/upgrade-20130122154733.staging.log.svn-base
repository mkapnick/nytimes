15:47:49 696  PROCEDURE GET_ENDED_GC_LICENSES (
15:47:49 697  	out_result_set		OUT SYS_REFCURSOR,
15:47:49 698  	in_hours_number 	IN NUMBER DEFAULT 14*24,
15:47:49 699  	in_num_rows		IN NUMBER DEFAULT 10000,
15:47:49 700  	in_process_name IN PROCESS_RETRY_THROTTLE.PROCESS_NAME%TYPE
15:47:49 701  ) AS
15:47:49 702  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_ENDED_GC_INVOICES';
15:47:49 703  var_days		NUMBER;
15:47:49 704  var_hours 	NUMBER;
15:47:49 705  var_time_interval INTERVAL DAY (3) TO SECOND;
15:47:49 706  BEGIN
15:47:49 707  	var_hours := mod(in_hours_number,24);
15:47:49 708  	var_days := (in_hours_number - var_hours) / 24;
15:47:49 709  	var_time_interval := var_days||' '||var_hours||':0:0';
15:47:49 710  	OPEN out_result_set FOR
15:47:49 711  SELECT * FROM
15:47:49 712  (
15:47:49 713  	SELECT
15:47:49 714  	  l.ID,
15:47:49 715  	  l.CREATE_DATE,
15:47:49 716  	  l.CREATED_BY,
15:47:49 717  	  l.CURRENT_OFFER_INDEX,
15:47:49 718  	  l.CURRENT_OFFER_RECURR_NUM,
15:47:49 719  	  l.END_DATE,
15:47:49 720  	  l.ENTITLEMENT_END_DATE,
15:47:49 721  	  l.INVOICE_ID,
15:47:49 722  	  l.IS_EXTENSION,
15:47:49 723  	  l.LICENSE_STATUS_ID,
15:47:49 724  	  l.OFFER_ID,
15:47:49 725  	  l.START_DATE,
15:47:49 726  	  l.SUBSCRIPTION_ID,
15:47:49 727  	  l.UPDATE_DATE,
15:47:49 728  	  l.UPDATED_BY
15:47:49 729  	FROM
15:47:49 730  	  GIFT_CERTIFICATE gc
15:47:49 731  	  INNER JOIN INVOICE i ON i.id = gc.PURCHASE_INVOICE_ID
15:47:49 732  	  INNER JOIN LICENSE l ON l.invoice_id = i.id
15:47:49 733  	  LEFT JOIN SUBSCRIPTION s ON s.id = l.subscription_id
15:47:49 734  	WHERE
15:47:49 735  	  l.LICENSE_STATUS_ID != GLOBAL_STATUSES_V17.LICENSE_ACTIVE
15:47:49 736  	  AND l.ENTITLEMENT_END_DATE <= (current_timestamp)
15:47:49 737  	  AND l.ENTITLEMENT_END_DATE > (current_timestamp - var_time_interval)
15:47:49 738  	  AND s.subscription_status_id = GLOBAL_STATUSES_V17.SUBSCRIPTION_CLOSED
15:47:49 739  	  AND NOT EXISTS (
15:47:49 740  	    SELECT NULL
15:47:49 741  	    FROM PROCESS_RETRY_THROTTLE
15:47:49 742  	    WHERE PROCESS_NAME = in_process_name
15:47:49 743  	      AND GENERIC_ID = l.ID
15:47:49 744  	  )
15:47:49 745  	  AND ROWNUM <= in_num_rows*10
15:47:49 746  	  ORDER BY dbms_random.value
15:47:49 747  ) WHERE
15:47:49 748  	  ROWNUM <= in_num_rows
15:47:49 749  	  GROUP BY SUBSCRIPTION_ID;
15:47:49 750  EXCEPTION
15:47:49 751  WHEN OTHERS THEN
15:47:49 752  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 753  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:49 754  END GET_ENDED_GC_LICENSES;
