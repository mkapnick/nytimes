15:47:50 1540  PROCEDURE GET_GROUPS_ID_BY_INVOICE_ID (
15:47:50 1541  /*
15:47:50 1542  This procedure is using for LOCKING only
15:47:50 1543  
15:47:50 1544  Throws exceptions:
15:47:50 1545  APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 1546  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:50 1547  */
15:47:50 1548  	 in_invoice_id IN NUMBER,
15:47:50 1549  	 out_group_ids OUT SYS_REFCURSOR
15:47:50 1550  ) AS
15:47:50 1551  SPROC_NAME CONSTANT VARCHAR2(27) := 'GET_GROUPS_ID_BY_INVOICE_ID';
15:47:50 1552  -- VARIABLES
15:47:50 1553  temp_invoice_id		 NUMBER;
15:47:50 1554  var_subscrib_group_id	 NUMBER;
15:47:50 1555  var_gc_purchaser_group_id NUMBER;
15:47:50 1556  var_gc_redeemer_group_id  NUMBER;
15:47:50 1557  -- EXCEPTIONS
15:47:50 1558  BAD_INVOICE_ID	    EXCEPTION;
15:47:50 1559  CAN_NOT_FIND_ACCOUNT EXCEPTION;
15:47:50 1560  BEGIN
15:47:50 1561  
15:47:50 1562  	 BEGIN
15:47:50 1563  	   SELECT
15:47:50 1564  	     INVOICE.ID into temp_invoice_id
15:47:50 1565  	   FROM
15:47:50 1566  	     INVOICE
15:47:50 1567  	   WHERE
15:47:50 1568  	     INVOICE.ID = in_invoice_id;
15:47:50 1569  	   EXCEPTION
15:47:50 1570  	     WHEN NO_DATA_FOUND THEN
15:47:50 1571  	       RAISE BAD_INVOICE_ID;
15:47:50 1572  	 END;
15:47:50 1573  
15:47:50 1574  	 BEGIN
15:47:50 1575  	   SELECT DISTINCT
15:47:50 1576  	     ACCOUNT.GROUP_ID into var_subscrib_group_id
15:47:50 1577  	   FROM
15:47:50 1578  	     ACCOUNT
15:47:50 1579  	     INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.ACCOUNT_ID = ACCOUNT.ID
15:47:50 1580  	     INNER JOIN LICENSE ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:47:50 1581  	   WHERE
15:47:50 1582  	     LICENSE.INVOICE_ID = in_invoice_id;
15:47:50 1583  	   EXCEPTION
15:47:50 1584  	     WHEN NO_DATA_FOUND THEN
15:47:50 1585  	       var_subscrib_group_id := NULL;
15:47:50 1586  	 END;
15:47:50 1587  
15:47:50 1588  	 IF var_subscrib_group_id IS NULL THEN
15:47:50 1589  	   BEGIN
15:47:50 1590  	     SELECT
15:47:50 1591  	       GIFT_CERTIFICATE.PURCHASER_GROUP_ID,
15:47:50 1592  	       GIFT_CERTIFICATE.REDEEMER_GROUP_ID
15:47:50 1593  	       into
15:47:50 1594  	       var_gc_purchaser_group_id,
15:47:50 1595  	       var_gc_redeemer_group_id
15:47:50 1596  	     FROM
15:47:50 1597  	       GIFT_CERTIFICATE
15:47:50 1598  	     WHERE
15:47:50 1599  	       GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = in_invoice_id
15:47:50 1600  	       OR GIFT_CERTIFICATE.FINALIZED_INVOICE_ID = in_invoice_id;
15:47:50 1601  	     EXCEPTION
15:47:50 1602  	       WHEN NO_DATA_FOUND THEN
15:47:50 1603  		 var_gc_purchaser_group_id := NULL;
15:47:50 1604  		 var_gc_redeemer_group_id  := NULL;
15:47:50 1605  	   END;
15:47:50 1606  	 END IF;
15:47:50 1607  
15:47:50 1608  	 IF var_subscrib_group_id IS NULL
15:47:50 1609  	   AND var_gc_purchaser_group_id IS NULL
15:47:50 1610  	   AND var_gc_redeemer_group_id IS NULL THEN
15:47:50 1611  	     RAISE CAN_NOT_FIND_ACCOUNT;
15:47:50 1612  	 END IF;
15:47:50 1613  
15:47:50 1614  	 OPEN out_group_ids FOR
15:47:50 1615  	 SELECT GROUP_ID FROM (
15:47:50 1616  	   SELECT
15:47:50 1617  	     var_subscrib_group_id as "GROUP_ID"
15:47:50 1618  	   FROM
15:47:50 1619  	     DUAL
15:47:50 1620  	   UNION
15:47:50 1621  	   SELECT
15:47:50 1622  	     var_gc_purchaser_group_id as "GROUP_ID"
15:47:50 1623  	   FROM
15:47:50 1624  	     DUAL
15:47:50 1625  	   UNION
15:47:50 1626  	   SELECT
15:47:50 1627  	     var_gc_redeemer_group_id as "GROUP_ID"
15:47:50 1628  	   FROM
15:47:50 1629  	     DUAL
15:47:50 1630  	 )
15:47:50 1631  	 WHERE
15:47:50 1632  	   GROUP_ID IS NOT NULL;
15:47:50 1633  
15:47:50 1634  EXCEPTION
15:47:50 1635  WHEN BAD_INVOICE_ID THEN
15:47:50 1636  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 1637  	   SPROC_NAME, 'No such invoice');
15:47:50 1638  WHEN CAN_NOT_FIND_ACCOUNT THEN
15:47:50 1639  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 1640  	   SPROC_NAME, 'Could not find account for given invoice');
15:47:50 1641  WHEN OTHERS THEN
15:47:50 1642  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:50 1643  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:47:50 1644  END GET_GROUPS_ID_BY_INVOICE_ID;
