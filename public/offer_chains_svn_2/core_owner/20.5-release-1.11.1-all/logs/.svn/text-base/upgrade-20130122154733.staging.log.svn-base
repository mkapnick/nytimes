15:47:49 1680  
15:47:49 1681  PROCEDURE REFUND_INVOICE (
15:47:49 1682  	 in_invoice_id	    IN NUMBER,
15:47:49 1683  	 in_refund_amount   IN NUMBER,
15:47:49 1684  	 in_note	    IN VARCHAR2,
15:47:49 1685  	 in_created_by	    IN VARCHAR2,
15:47:49 1686  	 out_charge_id	    OUT NUMBER
15:47:49 1687  ) AS
15:47:49 1688  SPROC_NAME CONSTANT VARCHAR2(14) := 'REFUND_INVOICE';
15:47:49 1689  -- VARIABLES
15:47:49 1690  var_invoice_status_id  NUMBER;
15:47:49 1691  var_subscription_id    NUMBER;
15:47:49 1692  var_account_id	      NUMBER;
15:47:49 1693  var_group_id	      NUMBER;
15:47:49 1694  var_account_status_id  NUMBER;
15:47:49 1695  var_new_transaction_id NUMBER;
15:47:49 1696  var_instrument_type_id NUMBER;
15:47:49 1697  var_instrument_id      NUMBER;
15:47:49 1698  var_new_charge_id      NUMBER;
15:47:49 1699  var_invoice_amount     NUMBER(10,2);
15:47:49 1700  var_refunds_before     NUMBER(10,2);
15:47:49 1701  var_charges_amount     NUMBER(10,2);
15:47:49 1702  -- EXCEPTIONS
15:47:49 1703  CAN_NOT_FIND_SUBSCR_OR_GC     EXCEPTION;
15:47:49 1704  ACCOUNT_IS_FROZEN	     EXCEPTION;
15:47:49 1705  BAD_INVOICE_ID		     EXCEPTION;
15:47:49 1706  CAN_NOT_CREATE_TRANSACTION    EXCEPTION;
15:47:49 1707  CAN_NOT_CREATE_CHARGE	     EXCEPTION;
15:47:49 1708  CAN_NOT_CALC_INVOICE_AMOUNT   EXCEPTION;
15:47:49 1709  REFUND_IS_GREATER_THAN_ANOUNT EXCEPTION;
15:47:49 1710  CAN_NOT_ANNOTATE_SUBSCRIPTION EXCEPTION;
15:47:49 1711  TOT_REF_IS_GREATER_THAN_ANOUNT EXCEPTION;
15:47:49 1712  INVOICE_IS_NOT_CLOSED	     EXCEPTION;
15:47:49 1713  TOT_REF_IS_GRATER_THAN_CHARGES EXCEPTION;
15:47:49 1714  EXCEPTION_MESSAGE	      VARCHAR2(1024);
15:47:49 1715  BEGIN
15:47:49 1716  
15:47:49 1717  	 BEGIN
15:47:49 1718  	   SELECT
15:47:49 1719  	     INVOICE.INVOICE_STATUS_ID into var_invoice_status_id
15:47:49 1720  	   FROM
15:47:49 1721  	     INVOICE
15:47:49 1722  	   WHERE
15:47:49 1723  	     INVOICE.ID = in_invoice_id;
15:47:49 1724  	   EXCEPTION
15:47:49 1725  	     WHEN NO_DATA_FOUND THEN
15:47:49 1726  	       RAISE BAD_INVOICE_ID;
15:47:49 1727  	 END;
15:47:49 1728  
15:47:49 1729  	 -- Get instrument and subscription id if exists
15:47:49 1730  	 BEGIN
15:47:49 1731  	   SELECT
15:47:49 1732  	     SUBSCRIPTION.INSTRUMENT_ID,
15:47:49 1733  	     SUBSCRIPTION.INSTRUMENT_TYPE_ID,
15:47:49 1734  	     SUBSCRIPTION.ACCOUNT_ID,
15:47:49 1735  	     SUBSCRIPTION.ID
15:47:49 1736  	     into
15:47:49 1737  	     var_instrument_id,
15:47:49 1738  	     var_instrument_type_id,
15:47:49 1739  	     var_account_id,
15:47:49 1740  	     var_subscription_id
15:47:49 1741  	   FROM
15:47:49 1742  	     SUBSCRIPTION
15:47:49 1743  	     INNER JOIN LICENSE ON SUBSCRIPTION.ID = LICENSE.SUBSCRIPTION_ID
15:47:49 1744  	   WHERE
15:47:49 1745  	     LICENSE.INVOICE_ID = in_invoice_id
15:47:49 1746  	     AND ROWNUM <= 1;
15:47:49 1747  	   EXCEPTION
15:47:49 1748  	     WHEN NO_DATA_FOUND THEN
15:47:49 1749  	       BEGIN
15:47:49 1750  
15:47:49 1751  		 var_subscription_id := NULL;
15:47:49 1752  
15:47:49 1753  		 SELECT
15:47:49 1754  		   CHARGE.INSTRUMENT_ID,
15:47:49 1755  		   CHARGE.INSTRUMENT_TYPE_ID,
15:47:49 1756  		   GIFT_CERTIFICATE.PURCHASER_GROUP_ID
15:47:49 1757  		   into
15:47:49 1758  		   var_instrument_id,
15:47:49 1759  		   var_instrument_type_id,
15:47:49 1760  		   var_group_id
15:47:49 1761  		 FROM
15:47:49 1762  		   INVOICE
15:47:49 1763  		   INNER JOIN CHARGE ON INVOICE.ID = CHARGE.INVOICE_ID
15:47:49 1764  		   INNER JOIN GIFT_CERTIFICATE ON GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = INVOICE.ID
15:47:49 1765  		 WHERE
15:47:49 1766  		   INVOICE.ID = in_invoice_id
15:47:49 1767  		   AND ROWNUM <= 1;
15:47:49 1768  
15:47:49 1769  		 SELECT
15:47:49 1770  		   ACCOUNT.ID into var_account_id
15:47:49 1771  		 FROM
15:47:49 1772  		   ACCOUNT
15:47:49 1773  		 WHERE
15:47:49 1774  		   ACCOUNT.GROUP_ID = var_group_id;
15:47:49 1775  
15:47:49 1776  		 EXCEPTION
15:47:49 1777  		   WHEN NO_DATA_FOUND THEN
15:47:49 1778  		     RAISE CAN_NOT_FIND_SUBSCR_OR_GC;
15:47:49 1779  	       END;
15:47:49 1780  	 END;
15:47:49 1781  
15:47:49 1782  	 -- Check account status. It should not to be frozen
15:47:49 1783  	 SELECT
15:47:49 1784  	   ACCOUNT.ACCOUNT_STATUS_ID into var_account_status_id
15:47:49 1785  	 FROM
15:47:49 1786  	   ACCOUNT
15:47:49 1787  	 WHERE
15:47:49 1788  	   ACCOUNT.ID = var_account_id;
15:47:49 1789  
15:47:49 1790  	 IF var_account_status_id = GLOBAL_STATUSES_V17.ACCOUNT_FROZEN THEN
15:47:49 1791  	   RAISE ACCOUNT_IS_FROZEN;
15:47:49 1792  	 END IF;
15:47:49 1793  
15:47:49 1794  	 IF var_invoice_status_id != GLOBAL_STATUSES_V17.INVOICE_CLOSED THEN
15:47:49 1795  	   RAISE INVOICE_IS_NOT_CLOSED;
15:47:49 1796  	 END IF;
15:47:49 1797  
15:47:49 1798  	 BEGIN
15:47:49 1799  	   PROCS_INVOICE_V17.CALCULATE_INVOICE_AMOUNT (
15:47:49 1800  	     in_invoice_id => in_invoice_id,
15:47:49 1801  	     out_amount    => var_invoice_amount
15:47:49 1802  	   );
15:47:49 1803  	   EXCEPTION
15:47:49 1804  	     WHEN OTHERS THEN
15:47:49 1805  	       EXCEPTION_MESSAGE := SQLERRM;
15:47:49 1806  	       RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
15:47:49 1807  	 END;
15:47:49 1808  
15:47:49 1809  	 IF ( in_refund_amount > var_invoice_amount ) THEN
15:47:49 1810  	   RAISE REFUND_IS_GREATER_THAN_ANOUNT;
15:47:49 1811  	 END IF;
15:47:49 1812  
15:47:49 1813  	 SELECT /*+ STAR_TRANSFORMATION */
15:47:49 1814  	   SUM(CHARGE.CHARGE_AMOUNT) into var_refunds_before
15:47:49 1815  	 FROM
15:47:49 1816  	   CHARGE
15:47:49 1817  	 WHERE
15:47:49 1818  	   CHARGE.INVOICE_ID = in_invoice_id
15:47:49 1819  	   AND (
15:47:49 1820  	     CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V17.CHARGE_OPENED
15:47:49 1821  	     OR (
15:47:49 1822  	       CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V17.CHARGE_PROCESSED
15:47:49 1823  	       AND EXISTS (
15:47:49 1824  		 SELECT 1 FROM TRANSACTION_ATTEMPT ta where ta.transaction_id = CHARGE.TRANSACTION_ID and ta.transaction_attempt_status_id = GLOBAL_STATUSES_V17.TRANS_ATTEMPT_SUCCESS
15:47:49 1825  	       )
15:47:49 1826  	     )
15:47:49 1827  	   )
15:47:49 1828  	   AND CHARGE.CHARGE_AMOUNT < 0;
15:47:49 1829  
15:47:49 1830  	 -- Refunds are negative
15:47:49 1831  	 IF var_refunds_before IS NULL THEN var_refunds_before := 0; END IF;
15:47:49 1832  	 var_refunds_before := 0 - var_refunds_before;
15:47:49 1833  
15:47:49 1834  	 var_charges_amount := 0;
15:47:49 1835  
15:47:49 1836  	 FOR f_processed_charges IN (
15:47:49 1837  	   SELECT
15:47:49 1838  	     CHARGE.CHARGE_AMOUNT
15:47:49 1839  	   FROM
15:47:49 1840  	     CHARGE
15:47:49 1841  	   WHERE
15:47:49 1842  	     CHARGE.INVOICE_ID = in_invoice_id
15:47:49 1843  	     AND CHARGE.CHARGE_AMOUNT > 0
15:47:49 1844  	     AND CHARGE.CHARGE_STATUS_ID IN (SELECT GLOBAL_STATUSES_V17.CHARGE_PROCESSED FROM DUAL)
15:47:49 1845  	     AND EXISTS (SELECT 1 FROM TRANSACTION_ATTEMPT ta where ta.transaction_id = CHARGE.TRANSACTION_ID and ta.transaction_attempt_status_id = GLOBAL_STATUSES_V17.TRANS_ATTEMPT_SUCCESS)
15:47:49 1846  	 )
15:47:49 1847  	 LOOP
15:47:49 1848  	   var_charges_amount := var_charges_amount + f_processed_charges.CHARGE_AMOUNT;
15:47:49 1849  	 END LOOP;
15:47:49 1850  
15:47:49 1851  	 IF (in_refund_amount + var_refunds_before > var_invoice_amount) THEN
15:47:49 1852  	   RAISE TOT_REF_IS_GREATER_THAN_ANOUNT;
15:47:49 1853  	 END IF;
15:47:49 1854  
15:47:49 1855  	 IF (in_refund_amount + var_refunds_before > var_charges_amount) THEN
15:47:49 1856  	   RAISE TOT_REF_IS_GRATER_THAN_CHARGES;
15:47:49 1857  	 END IF;
15:47:49 1858  
15:47:49 1859  	 BEGIN
15:47:49 1860  	   PROCS_TRANSACTION_V17.CREATE_TRANSACTION(
15:47:49 1861  	     in_transaction_id	=> NULL,
15:47:49 1862  	     in_status_id	=> GLOBAL_STATUSES_V17.TRANSACTION_PREPARE,
15:47:49 1863  	     in_amount		=> -in_refund_amount,
15:47:49 1864  	     in_created_by	=> in_created_by,
15:47:49 1865  	     in_order_id	=> NULL,
15:47:49 1866  	     in_is_refund	=> GLOBAL_CONSTANTS_V17.TRUE,
15:47:49 1867  	     out_transaction_id => var_new_transaction_id
15:47:49 1868  	   );
15:47:49 1869  	   EXCEPTION
15:47:49 1870  	     WHEN OTHERS THEN
15:47:49 1871  	       EXCEPTION_MESSAGE := SQLERRM;
15:47:49 1872  	       RAISE CAN_NOT_CREATE_TRANSACTION;
15:47:49 1873  	 END;
15:47:49 1874  
15:47:49 1875  	 BEGIN
15:47:49 1876  	   PROCS_CHARGE_V17.CREATE_CHARGE(
15:47:49 1877  	     in_invoice_id	   => in_invoice_id,
15:47:49 1878  	     in_transaction_id	   => var_new_transaction_id,
15:47:49 1879  	     in_instrument_type_id => var_instrument_type_id,
15:47:49 1880  	     in_instrument_id	   => var_instrument_id,
15:47:49 1881  	     in_charge_amount	   => -in_refund_amount,
15:47:49 1882  	     in_created_by	   => in_created_by,
15:47:49 1883  	     in_charge_status_id   => GLOBAL_STATUSES_V17.CHARGE_OPENED,
15:47:49 1884  	     out_charge_id	   => var_new_charge_id
15:47:49 1885  	   );
15:47:49 1886  	   EXCEPTION
15:47:49 1887  	     WHEN OTHERS THEN
15:47:49 1888  	       EXCEPTION_MESSAGE := SQLERRM;
15:47:49 1889  	       RAISE CAN_NOT_CREATE_CHARGE;
15:47:49 1890  	 END;
15:47:49 1891  
15:47:49 1892  	 out_charge_id := var_new_charge_id;
15:47:49 1893  
15:47:49 1894  	 IF in_note IS NOT NULL AND var_subscription_id IS NOT NULL THEN
15:47:49 1895  	   BEGIN
