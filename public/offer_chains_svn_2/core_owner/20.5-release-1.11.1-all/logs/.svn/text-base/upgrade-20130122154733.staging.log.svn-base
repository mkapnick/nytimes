15:47:50 580  
15:47:50 581  PROCEDURE UPDATE_CREDIT_CARD (
15:47:50 582  /*
15:47:50 583  Throws exceptions:
15:47:50 584  APP_EXCEPTION_CODES_V17.NOT_FOUND
15:47:50 585  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:50 586  */
15:47:50 587  	in_credit_card_id	  IN NUMBER,
15:47:50 588  	in_updated_by		  IN VARCHAR2,
15:47:50 589  	in_instrument_name	  IN VARCHAR2,
15:47:50 590  	in_is_default		  IN NUMBER
15:47:50 591  ) AS
15:47:50 592  SPROC_NAME CONSTANT VARCHAR2(18) := 'UPDATE_CREDIT_CARD';
15:47:50 593  -- VARIABLES
15:47:50 594  var_account_id NUMBER;
15:47:50 595  var_group_id   NUMBER;
15:47:50 596  temp_cc_rownum NUMBER;
15:47:50 597  current_def_instrument_type_id NUMBER;
15:47:50 598  current_def_instrument_id      NUMBER;
15:47:50 599  -- EXCEPTION
15:47:50 600  CAN_NOT_SET_DEF_FINANCIAL  EXCEPTION;
15:47:50 601  BAD_IS_DEFAULT_VALUE	 EXCEPTION;
15:47:50 602  CAN_NOT_GET_DEF_FINANCIAL  EXCEPTION;
15:47:50 603  CAN_NOT_DEL_DEF_FINANCIAL  EXCEPTION;
15:47:50 604  EXCEPTION_MESSAGE 	 VARCHAR2(1024);
15:47:50 605  BEGIN
15:47:50 606  
15:47:50 607  	-- Get account id
15:47:50 608  	SELECT
15:47:50 609  	  CREDIT_CARD.ACCOUNT_ID
15:47:50 610  	  into
15:47:50 611  	  var_account_id
15:47:50 612  	FROM
15:47:50 613  	  CREDIT_CARD
15:47:50 614  	WHERE
15:47:50 615  	  CREDIT_CARD.ID = in_credit_card_id;
15:47:50 616  
15:47:50 617  	-- Get group id
15:47:50 618  	SELECT
15:47:50 619  	  ACCOUNT.GROUP_ID into var_group_id
15:47:50 620  	FROM
15:47:50 621  	  ACCOUNT
15:47:50 622  	WHERE
15:47:50 623  	  ACCOUNT.ID = var_account_id;
15:47:50 624  
15:47:50 625  	-- Check that passed data is correct
15:47:50 626  	IF in_is_default != GLOBAL_CONSTANTS_V17.TRUE
15:47:50 627  	  AND in_is_default != GLOBAL_CONSTANTS_V17.FALSE
15:47:50 628  	  AND in_is_default IS NOT NULL THEN
15:47:50 629  	  RAISE BAD_IS_DEFAULT_VALUE;
15:47:50 630  	END IF;
15:47:50 631  
15:47:50 632  	-- Update credit card
15:47:50 633  	IF in_instrument_name IS NOT NULL THEN
15:47:50 634  	  PROCS_FIN_INSTRUMENTS_CRU_V17.UPDATE_CREDIT_CARD(
15:47:50 635  	    in_credit_card_id  => in_credit_card_id,
15:47:50 636  	    in_updated_by      => in_updated_by,
15:47:50 637  	    in_instrument_name => in_instrument_name
15:47:50 638  	  );
15:47:50 639  	END IF;
15:47:50 640  
15:47:50 641  	-- Set default financial instrument
15:47:50 642  	IF in_is_default = GLOBAL_CONSTANTS_V17.TRUE THEN
15:47:50 643  	  BEGIN
15:47:50 644  	    PROCS_FIN_INSTRUMENTS_V17.SET_DEF_FINANCIAL_INSTRUMENT(
15:47:50 645  	      in_group_id	    => var_group_id,
15:47:50 646  	      in_instrument_type_id => GLOBAL_ENUMS_V17.INSTRUMENT_CREDIT_CARD,
15:47:50 647  	      in_instrument_id	    => in_credit_card_id,
15:47:50 648  	      in_updated_by	    => in_updated_by
15:47:50 649  	    );
15:47:50 650  	    EXCEPTION
15:47:50 651  	      WHEN OTHERS THEN
15:47:50 652  		EXCEPTION_MESSAGE := SQLERRM;
15:47:50 653  		RAISE CAN_NOT_SET_DEF_FINANCIAL;
15:47:50 654  	  END;
15:47:50 655  	END IF;
15:47:50 656  
15:47:50 657  	-- Set default financial instrument
15:47:50 658  	IF in_is_default = GLOBAL_CONSTANTS_V17.FALSE THEN
15:47:50 659  	  BEGIN
15:47:50 660  	    GET_DEF_FINANCIAL_INSTRUMENT(
15:47:50 661  	      in_group_id	     => var_group_id,
15:47:50 662  	      out_instrument_type_id => current_def_instrument_type_id,
15:47:50 663  	      out_instrument_id      => current_def_instrument_id
15:47:50 664  	    );
15:47:50 665  	  EXCEPTION
15:47:50 666  	    WHEN OTHERS THEN
15:47:50 667  	      EXCEPTION_MESSAGE := SQLERRM;
15:47:50 668  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
15:47:50 669  	  END;
15:47:50 670  	  IF current_def_instrument_type_id = GLOBAL_ENUMS_V17.INSTRUMENT_CREDIT_CARD
15:47:50 671  	    AND current_def_instrument_id = in_credit_card_id THEN
15:47:50 672  	    BEGIN
15:47:50 673  	      DEL_DEF_FINANCIAL_INSTRUMENT(
15:47:50 674  		in_group_id => var_group_id
15:47:50 675  	      );
15:47:50 676  	      EXCEPTION
15:47:50 677  		WHEN OTHERS THEN
15:47:50 678  		  EXCEPTION_MESSAGE := SQLERRM;
15:47:50 679  		  RAISE CAN_NOT_DEL_DEF_FINANCIAL;
15:47:50 680  	    END;
15:47:50 681  	  END IF;
15:47:50 682  	END IF;
15:47:50 683  
15:47:50 684  EXCEPTION
15:47:50 685  WHEN NO_DATA_FOUND THEN
15:47:50 686  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 687  	  SPROC_NAME, 'No such credit card');
15:47:50 688  WHEN CAN_NOT_SET_DEF_FINANCIAL THEN
15:47:50 689  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50 690  	  SPROC_NAME, 'Could not set default financial instrument for account', EXCEPTION_MESSAGE);
15:47:50 691  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
15:47:50 692  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50 693  	  SPROC_NAME, 'Could not get default financial instrument for account', EXCEPTION_MESSAGE);
15:47:50 694  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
15:47:50 695  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50 696  	  SPROC_NAME, 'Could not delete information about default financial instrument', EXCEPTION_MESSAGE);
15:47:50 697  WHEN OTHERS THEN
15:47:50 698  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:50 699  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:50 700  END UPDATE_CREDIT_CARD;
