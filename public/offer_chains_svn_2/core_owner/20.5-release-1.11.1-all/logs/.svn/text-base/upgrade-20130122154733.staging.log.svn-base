15:47:49 438  
15:47:49 439  PROCEDURE SUSPEND_SUBSCRIPTION(
15:47:49 440  /*
15:47:49 441  Throws exceptions:
15:47:49 442  APP_EXCEPTION_CODES_V17.NOT_FOUND
15:47:49 443  APP_EXCEPTION_CODES_V17.INTERNAL_ERROR
15:47:49 444  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:49 445  */
15:47:49 446  	  in_subs_id	IN NUMBER,
15:47:49 447  	  in_updated_by IN VARCHAR2
15:47:49 448  ) AS
15:47:49 449  SPROC_NAME		  CONSTANT VARCHAR2(20) := 'SUSPEND_SUBSCRIPTION';
15:47:49 450  var_subscription_status_id  NUMBER;
15:47:49 451  var_license_id		  NUMBER;
15:47:49 452  var_offer_id		  NUMBER;
15:47:49 453  var_license_start_date	  DATE;
15:47:49 454  var_license_end_date	  DATE;
15:47:49 455  
15:47:49 456  var_entitlement_dupration   VARCHAR2(30);
15:47:49 457  var_d_entitlement_dupration NUMBER;
15:47:49 458  
15:47:49 459  var_ym_interval INTERVAL YEAR TO MONTH;
15:47:49 460  var_ds_interval INTERVAL DAY(3) TO SECOND;
15:47:49 461  
15:47:49 462  -- EXCEPTIONS
15:47:49 463  BAD_SUBSCRIPTION_ID     EXCEPTION;
15:47:49 464  BAD_SUBSCRIPTION_STATUS EXCEPTION;
15:47:49 465  NO_LICENSE_FOUND	      EXCEPTION;
15:47:49 466  NO_OFFER_FOUND	      EXCEPTION;
15:47:49 467  EXCEPTION_MESSAGE       VARCHAR2(1024);
15:47:49 468  BEGIN
15:47:49 469  	-- TODO: Finish this prcedure (in Phase II)
15:47:49 470  
15:47:49 471  	-- Get subscription by id. FAULT if no such subscription.
15:47:49 472  	-- begin TX
15:47:49 473  	--   Get for update associated license (subscription.license_id). FAULT if not found.
15:47:49 474  	--   Set status to PROCESSED.
15:47:49 475  	--   updated record.
15:47:49 476  	--   compute days remaining in the subscription: original end_date - today = days_remaining_adjustment
15:47:49 477  	--   new subscription status is SUSPENDED.
15:47:49 478  	--   suspend_date is now.
15:47:49 479  	--   update subscription record.
15:47:49 480  	-- end TX
15:47:49 481  
15:47:49 482  	BEGIN
15:47:49 483  	  SELECT
15:47:49 484  	    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID into var_subscription_status_id
15:47:49 485  	  FROM
15:47:49 486  	    SUBSCRIPTION
15:47:49 487  	  WHERE
15:47:49 488  	    SUBSCRIPTION.ID = in_subs_id;
15:47:49 489  	  EXCEPTION
15:47:49 490  	  WHEN NO_DATA_FOUND THEN
15:47:49 491  	    RAISE BAD_SUBSCRIPTION_ID;
15:47:49 492  	END;
15:47:49 493  
15:47:49 494  	IF var_subscription_status_id != GLOBAL_STATUSES_V17.SUBSCRIPTION_ACTIVE THEN
15:47:49 495  	  RAISE BAD_SUBSCRIPTION_STATUS;
15:47:49 496  	END IF;
15:47:49 497  
15:47:49 498  	BEGIN
15:47:49 499  	  SELECT
15:47:49 500  	    LICENSE.ID,
15:47:49 501  	    LICENSE.OFFER_ID,
15:47:49 502  	    LICENSE.START_DATE
15:47:49 503  	    into
15:47:49 504  	    var_license_id,
15:47:49 505  	    var_offer_id,
15:47:49 506  	    var_license_start_date
15:47:49 507  	  FROM
15:47:49 508  	    LICENSE
15:47:49 509  	  WHERE
15:47:49 510  	    LICENSE.SUBSCRIPTION_ID = in_subs_id
15:47:49 511  	      AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V17.LICENSE_ACTIVE;
15:47:49 512  	  EXCEPTION
15:47:49 513  	  WHEN NO_DATA_FOUND THEN
15:47:49 514  	    RAISE NO_LICENSE_FOUND;
15:47:49 515  	END;
15:47:49 516  
15:47:49 517  	PROCS_LICENSE_CRU_V17.UPDATE_LICENSE(
15:47:49 518  	  in_license_id        => var_license_id,
15:47:49 519  	  in_updated_by        => in_updated_by,
15:47:49 520  	  in_license_status_id => GLOBAL_STATUSES_V17.LICENSE_CLOSED
15:47:49 521  	);
15:47:49 522  
15:47:49 523  	BEGIN
15:47:49 524  	  SELECT
15:47:49 525  	    OFFER.ENTITLEMENT_DURATION into var_entitlement_dupration
15:47:49 526  	  FROM
15:47:49 527  	    OFFER
15:47:49 528  	  WHERE
15:47:49 529  	    OFFER.ID = var_offer_id;
15:47:49 530  	  EXCEPTION
15:47:49 531  	  WHEN NO_DATA_FOUND THEN
15:47:49 532  	    RAISE NO_OFFER_FOUND;
15:47:49 533  	END;
15:47:49 534  
15:47:49 535  	var_ym_interval := substr(var_entitlement_dupration, 0, 4);
15:47:49 536  	var_ds_interval := substr(var_entitlement_dupration, 4);
15:47:49 537  
15:47:49 538  	var_license_end_date := var_license_start_date + var_ym_interval + var_ds_interval;
15:47:49 539  
15:47:49 540  	var_d_entitlement_dupration := var_license_end_date - current_date;
15:47:49 541  
15:47:49 542  	PROCS_SUBSCRIPTION_CRU_V17.UPDATE_SUBSCRIPTION(
15:47:49 543  	  in_subscription_id	       => in_subs_id,
15:47:49 544  	  in_updated_by 	       => in_updated_by,
15:47:49 545  	  in_suspend_date	       => SYSDATE,
15:47:49 546  	  in_subscription_status_id    => GLOBAL_STATUSES_V17.SUBSCRIPTION_SUSPENDED,
15:47:49 547  	  in_days_remainning_ajustment => var_d_entitlement_dupration
15:47:49 548  	);
15:47:49 549  
15:47:49 550  EXCEPTION
15:47:49 551  WHEN BAD_SUBSCRIPTION_ID THEN
15:47:49 552  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 553  	  SPROC_NAME, 'No such subscription');
15:47:49 554  WHEN BAD_SUBSCRIPTION_STATUS THEN
15:47:49 555  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.STATE_ERROR,
15:47:49 556  	  SPROC_NAME, 'Subscription is not active');
15:47:49 557  WHEN NO_LICENSE_FOUND THEN
15:47:49 558  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 559  	  SPROC_NAME, 'Can not find license associated with given subscription ID');
15:47:49 560  WHEN NO_OFFER_FOUND THEN
15:47:49 561  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 562  	  SPROC_NAME, 'Can not find offer associated with given subscription ID');
15:47:49 563  WHEN OTHERS THEN
15:47:49 564  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 565  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:49 566  END SUSPEND_SUBSCRIPTION;
