15:47:50 639  PROCEDURE GET_ACCOUNT_PAYPALS(
15:47:50 640  /*
15:47:50 641  IN:
15:47:50 642  instr_status:
15:47:50 643  1 - get active instruments only (default)
15:47:50 644  0 - get inactive instruments only
15:47:50 645  
15:47:50 646  Throws exceptions:
15:47:50 647  APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 648  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:50 649  */
15:47:50 650  	in_group_id	  IN  NUMBER,
15:47:50 651  	out_result_pp_set OUT SYS_REFCURSOR,
15:47:50 652  	out_default_id	  OUT NUMBER,
15:47:50 653  	in_instr_status   IN  NUMBER DEFAULT GLOBAL_STATUSES_V17.PAYPAL_ACTIVE
15:47:50 654  ) AS
15:47:50 655  SPROC_NAME     CONSTANT VARCHAR2(19) := 'GET_ACCOUNT_PAYPALS';
15:47:50 656  -- VARIABLES
15:47:50 657  var_account_id NUMBER;
15:47:50 658  var_def_instrument_type_id NUMBER;
15:47:50 659  var_def_instrument_id	 NUMBER;
15:47:50 660  -- EXCEPTIONS
15:47:50 661  WRONG_INSTR_EXCEPTION	 EXCEPTION;
15:47:50 662  CAN_NOT_GET_DEF_FIN_INSTRUMENT EXCEPTION;
15:47:50 663  EXCEPTION_MESSAGE 	     VARCHAR2(1024);
15:47:50 664  BEGIN
15:47:50 665  
15:47:50 666  	-- Get account id
15:47:50 667  	SELECT
15:47:50 668  	  ACCOUNT.ID INTO var_account_id
15:47:50 669  	FROM
15:47:50 670  	  ACCOUNT
15:47:50 671  	WHERE
15:47:50 672  	  ACCOUNT.GROUP_ID = in_group_id;
15:47:50 673  
15:47:50 674  	-- Check that all passed agruments are valid
15:47:50 675  	IF in_instr_status != GLOBAL_STATUSES_V17.PAYPAL_ACTIVE
15:47:50 676  	  AND in_instr_status != GLOBAL_STATUSES_V17.PAYPAL_INACTIVE
15:47:50 677  	  AND in_instr_status != GLOBAL_STATUSES_V17.PAYPAL_FROZEN THEN
15:47:50 678  	  RAISE WRONG_INSTR_EXCEPTION;
15:47:50 679  	END IF;
15:47:50 680  
15:47:50 681  	-- Retreiving information about paypals
15:47:50 682  	OPEN out_result_pp_set FOR
15:47:50 683  	SELECT
15:47:50 684  	  PAYPAL.ID,
15:47:50 685  	  PAYPAL.ACCOUNT_ID,
15:47:50 686  	  PAYPAL.INSTRUMENT_NAME,
15:47:50 687  	  PAYPAL.PRIVATE_EMAIL_ADDRESS,
15:47:50 688  	  PAYPAL.CREATE_DATE,
15:47:50 689  	  PAYPAL.CREATED_BY,
15:47:50 690  	  PAYPAL.UPDATE_DATE,
15:47:50 691  	  PAYPAL.UPDATED_BY,
15:47:50 692  	  PAYPAL.PAYPAL_STATUS_ID,
15:47:50 693  	  PAYPAL.PRIVATE_STREET_ADDRESS,
15:47:50 694  	  PAYPAL.PRIVATE_STREET_ADDRESS2,
15:47:50 695  	  PAYPAL.STATE,
15:47:50 696  	  PAYPAL.CITY,
15:47:50 697  	  PAYPAL.POSTAL_CODE,
15:47:50 698  	  PAYPAL.COUNTRY,
15:47:50 699  	  PAYPAL.EXPIRATION_DATE,
15:47:50 700  	  PAYPAL.SECRET_TOKEN
15:47:50 701  	FROM
15:47:50 702  	  PAYPAL
15:47:50 703  	WHERE
15:47:50 704  	  PAYPAL.ACCOUNT_ID = var_account_id
15:47:50 705  	  AND PAYPAL.PAYPAL_STATUS_ID = in_instr_status;
15:47:50 706  
15:47:50 707  	-- Get default credit card if exists
15:47:50 708  	BEGIN
15:47:50 709  	  PROCS_FIN_INSTRUMENTS_V17.GET_DEF_FINANCIAL_INSTRUMENT(
15:47:50 710  	    in_group_id 	   => in_group_id,
15:47:50 711  	    out_instrument_type_id => var_def_instrument_type_id,
15:47:50 712  	    out_instrument_id	   => var_def_instrument_id
15:47:50 713  	  );
