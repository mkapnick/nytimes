15:47:49 1119  
15:47:49 1120  PROCEDURE GET_INVOICE_DAYS_USED_NUMBER (
15:47:49 1121  	 in_invoice_id	     IN NUMBER,
15:47:49 1122  	 in_chargeback_date  IN DATE DEFAULT SYSDATE,
15:47:49 1123  	 out_days_num	     OUT NUMBER
15:47:49 1124  ) AS
15:47:49 1125  SPROC_NAME CONSTANT VARCHAR2(28) := 'GET_INVOICE_DAYS_USED_NUMBER';
15:47:49 1126  -- VARIABLES
15:47:49 1127  temp_invoice_id	      NUMBER;
15:47:49 1128  var_license_start_date DATE;
15:47:49 1129  var_license_end_date   DATE;
15:47:49 1130  var_chargeback_date    DATE;
15:47:49 1131  var_invoice_days_used  NUMBER;
15:47:49 1132  -- EXCEPTIONS
15:47:49 1133  BAD_INVOICE_ID EXCEPTION;
15:47:49 1134  BEGIN
15:47:49 1135  
15:47:49 1136  	 var_chargeback_date := NVL(in_chargeback_date, SYSDATE);
15:47:49 1137  
15:47:49 1138  	 BEGIN
15:47:49 1139  	   SELECT
15:47:49 1140  	     INVOICE.ID into temp_invoice_id
15:47:49 1141  	   FROM
15:47:49 1142  	     INVOICE
15:47:49 1143  	   WHERE
15:47:49 1144  	     INVOICE.ID = in_invoice_id;
15:47:49 1145  	   EXCEPTION
15:47:49 1146  	     WHEN NO_DATA_FOUND THEN
15:47:49 1147  	       RAISE BAD_INVOICE_ID;
15:47:49 1148  	 END;
15:47:49 1149  
15:47:49 1150  	 var_invoice_days_used := 0;
15:47:49 1151  
15:47:49 1152  	 FOR f_license IN (
15:47:49 1153  	   SELECT
15:47:49 1154  	     LICENSE.START_DATE,
15:47:49 1155  	     LICENSE.END_DATE
15:47:49 1156  	   FROM
15:47:49 1157  	     LICENSE
15:47:49 1158  	   WHERE
15:47:49 1159  	     LICENSE.INVOICE_ID = in_invoice_id
15:47:49 1160  	     AND LICENSE.IS_EXTENSION = GLOBAL_CONSTANTS_V17.FALSE
15:47:49 1161  	 )
15:47:49 1162  	 LOOP
15:47:49 1163  	   var_license_start_date := PROCS_COMMON_V17.NORMALIZE_DATE(f_license.START_DATE);
15:47:49 1164  	   var_license_end_date := PROCS_COMMON_V17.NORMALIZE_DATE(f_license.END_DATE);
15:47:49 1165  
15:47:49 1166  	   IF var_license_start_date <= var_chargeback_date THEN
15:47:49 1167  	     IF var_license_end_date <= var_chargeback_date THEN
15:47:49 1168  	       -- License is passed
15:47:49 1169  	       var_invoice_days_used := var_invoice_days_used + (var_license_end_date - var_license_start_date);
15:47:49 1170  	     ELSE
15:47:49 1171  	       -- This is current license
15:47:49 1172  	       var_invoice_days_used := var_invoice_days_used + (var_chargeback_date - var_license_start_date);
15:47:49 1173  	     END IF;
15:47:49 1174  	   ELSE
15:47:49 1175  	     -- if var_license_start_date > in_chargeback_date then do nothing
15:47:49 1176  	     NULL;
15:47:49 1177  	   END IF;
15:47:49 1178  	 END LOOP;
15:47:49 1179  
15:47:49 1180  	 out_days_num := var_invoice_days_used;
15:47:49 1181  
15:47:49 1182  EXCEPTION
15:47:49 1183  WHEN BAD_INVOICE_ID THEN
15:47:49 1184  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 1185  	   SPROC_NAME, 'No such invoice');
15:47:49 1186  WHEN OTHERS THEN
15:47:49 1187  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 1188  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:47:49 1189  END GET_INVOICE_DAYS_USED_NUMBER;
