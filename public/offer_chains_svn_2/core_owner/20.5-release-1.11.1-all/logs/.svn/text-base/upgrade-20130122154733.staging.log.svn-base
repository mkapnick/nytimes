15:47:49 184  
15:47:49 185  	      -- Find first concurrent subscription id:
15:47:49 186  	      SELECT
15:47:49 187  		ID into var_concur_subscription_id
15:47:49 188  	      FROM (
15:47:49 189  		SELECT
15:47:49 190  		  ID
15:47:49 191  		FROM
15:47:49 192  		  SUBSCRIPTION
15:47:49 193  		WHERE
