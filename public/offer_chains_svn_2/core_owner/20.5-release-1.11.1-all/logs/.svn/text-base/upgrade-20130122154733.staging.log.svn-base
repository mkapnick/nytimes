15:47:50  80  /******************************************************************************/
15:47:50  81  
15:47:50  82  PROCEDURE DISABLE_ACCOUNT (
15:47:50  83  /*
15:47:50  84  Throws exceptions:
15:47:50  85  APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50  86  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:50  87  */
15:47:50  88  	in_group_id   IN NUMBER,
15:47:50  89  	in_updated_by IN VARCHAR2,
15:47:50  90  	in_note       IN VARCHAR2,
15:47:50  91  	in_agent_id   IN NUMBER
15:47:50  92  ) AS
15:47:50  93  SPROC_NAME	      CONSTANT VARCHAR2(15) := 'DISABLE_ACCOUNT';
15:47:50  94  var_account_id	      NUMBER;
15:47:50  95  current_account_status  NUMBER;
15:47:50  96  
15:47:50  97  var_active_subscriptions_num NUMBER;
15:47:50  98  var_pending_invoices_num	   NUMBER;
15:47:50  99  
15:47:50 100  -- EXCEPTIONS
15:47:50 101  BAD_ACOUNT_ID		EXCEPTION;
15:47:50 102  BAD_CURRENT_ACC_STATUS	EXCEPTION;
15:47:50 103  PENDING_INVOICES_FOUND	EXCEPTION;
15:47:50 104  ACCOUNT_HAS_ACIVE_SUBSCRS EXCEPTION;
15:47:50 105  CAN_NOT_ANNOTATE_ACCOUNT	EXCEPTION;
15:47:50 106  EXCEPTION_MESSAGE 	VARCHAR2(1024);
15:47:50 107  BEGIN
15:47:50 108  
15:47:50 109  	-- Get account's status and id
15:47:50 110  	BEGIN
15:47:50 111  	  SELECT
15:47:50 112  	    ACCOUNT.ACCOUNT_STATUS_ID,
15:47:50 113  	    ACCOUNT.ID
15:47:50 114  	  INTO
15:47:50 115  	    current_account_status,
15:47:50 116  	    var_account_id
15:47:50 117  	  FROM ACCOUNT
15:47:50 118  	  WHERE
15:47:50 119  	    ACCOUNT.GROUP_ID = in_group_id;
15:47:50 120  	  EXCEPTION
15:47:50 121  	    WHEN NO_DATA_FOUND THEN
15:47:50 122  	      RAISE BAD_ACOUNT_ID;
15:47:50 123  	END;
15:47:50 124  
15:47:50 125  	-- For now, we can disable account whenever
15:47:50 126  	IF current_account_status = GLOBAL_STATUSES_V17.ACCOUNT_DISABLED THEN
15:47:50 127  	  RAISE BAD_CURRENT_ACC_STATUS;
15:47:50 128  	END IF;
15:47:50 129  
15:47:50 130  	-- Checks for out outstanding balances
15:47:50 131  	-- CHECK: No outstanding balances. If monies are due, then we can not cancel account. Return ERROR.
15:47:50 132  	SELECT
15:47:50 133  	  COUNT(*) into var_pending_invoices_num
15:47:50 134  	FROM
15:47:50 135  	  LICENSE
15:47:50 136  	    INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:47:50 137  	    INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
15:47:50 138  	WHERE
15:47:50 139  	  SUBSCRIPTION.ACCOUNT_ID = var_account_id
15:47:50 140  	  AND INVOICE.INVOICE_STATUS_ID IN ( SELECT GLOBAL_STATUSES_V17.INVOICE_OPEN FROM DUAL );
15:47:50 141  
15:47:50 142  	IF var_pending_invoices_num > 0 THEN
15:47:50 143  	  RAISE PENDING_INVOICES_FOUND;
15:47:50 144  	END IF;
15:47:50 145  
15:47:50 146  	SELECT
15:47:50 147  	  COUNT(*) into var_active_subscriptions_num
15:47:50 148  	FROM
15:47:50 149  	  SUBSCRIPTION
15:47:50 150  	WHERE
15:47:50 151  	  SUBSCRIPTION.ACCOUNT_ID = var_account_id
15:47:50 152  	  AND (
15:47:50 153  	    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V17.SUBSCRIPTION_ACTIVE
15:47:50 154  	    OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V17.SUBSCRIPTION_SUSPENDED
15:47:50 155  	  );
15:47:50 156  
15:47:50 157  	IF var_active_subscriptions_num > 0 THEN
15:47:50 158  	  RAISE ACCOUNT_HAS_ACIVE_SUBSCRS;
15:47:50 159  	END IF;
15:47:50 160  
15:47:50 161  	PROCS_ACCOUNT_V17.UPDATE_ACCOUNT_STATUS(
15:47:50 162  	  in_account_id        => var_account_id,
15:47:50 163  	  in_account_status_id => GLOBAL_STATUSES_V17.ACCOUNT_DISABLED,
15:47:50 164  	  in_updated_by        => in_updated_by
15:47:50 165  	);
15:47:50 166  
15:47:50 167  	-- Annotate account
15:47:50 168  	IF in_note IS NOT NULL THEN
15:47:50 169  	  BEGIN
15:47:50 170  	    PROCS_ACCOUNT_V17.ANNOTATE_ACCOUNT(
15:47:50 171  	      in_group_id   => in_group_id,
15:47:50 172  	      in_agent_id   => in_agent_id,
15:47:50 173  	      in_note	    => in_note,
15:47:50 174  	      in_created_by => in_updated_by
15:47:50 175  	    );
15:47:50 176  	    EXCEPTION
15:47:50 177  	      WHEN OTHERS THEN
15:47:50 178  		EXCEPTION_MESSAGE := SQLERRM;
15:47:50 179  		RAISE CAN_NOT_ANNOTATE_ACCOUNT;
15:47:50 180  	  END;
15:47:50 181  	END IF;
15:47:50 182  
15:47:50 183  EXCEPTION
15:47:50 184  WHEN ACCOUNT_HAS_ACIVE_SUBSCRS THEN
15:47:50 185  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50 186  	  SPROC_NAME, 'Account has active or suspended subsciptions');
15:47:50 187  WHEN BAD_CURRENT_ACC_STATUS THEN
15:47:50 188  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.STATE_ERROR,
15:47:50 189  	  SPROC_NAME, 'Current account status is "disabled". Can not disable it one more time.');
15:47:50 190  WHEN PENDING_INVOICES_FOUND THEN
15:47:50 191  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50 192  	  SPROC_NAME, 'Opened/Pending invoices founded');
15:47:50 193  WHEN BAD_ACOUNT_ID THEN
15:47:50 194  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 195  	  SPROC_NAME, 'No such account');
15:47:50 196  WHEN CAN_NOT_ANNOTATE_ACCOUNT THEN
15:47:50 197  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50 198  	  SPROC_NAME, 'Could not annotate account', EXCEPTION_MESSAGE);
15:47:50 199  WHEN OTHERS THEN
15:47:50 200  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:50 201  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:50 202  END DISABLE_ACCOUNT;
15:47:50 203  
15:47:50 204  /******************************************************************************/
15:47:50 205  
15:47:50 206  PROCEDURE CREATE_ACTIVE_ACCOUNT(
15:47:50 207  /*
15:47:50 208  Throws exceptions:
15:47:50 209  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:50 210  APP_EXCEPTION_CODES_V17.DUPLICATE_ERROR
15:47:50 211  */
15:47:50 212  	  in_created_by IN VARCHAR2,
15:47:50 213  	  in_group_id	IN NUMBER
15:47:50 214  ) AS
15:47:50 215  -- VARIABLES
15:47:50 216  SPROC_NAME      CONSTANT VARCHAR2(21) := 'CREATE_ACTIVE_ACCOUNT';
15:47:50 217  new_account_id  NUMBER;
15:47:50 218  temp_group_id   NUMBER;
15:47:50 219  -- EXCEPTIONS
15:47:50 220  GROUP_EXISTS_EXCEPTION EXCEPTION;
15:47:50 221  EXCEPTION_MESSAGE      VARCHAR2(1024);
15:47:50 222  BEGIN
15:47:50 223  	-- Check if account already exists
15:47:50 224  	BEGIN
15:47:50 225  	  SELECT
15:47:50 226  	    ACCOUNT.GROUP_ID into temp_group_id
15:47:50 227  	  FROM
15:47:50 228  	    ACCOUNT
15:47:50 229  	  WHERE
15:47:50 230  	    ACCOUNT.GROUP_ID = in_group_id
15:47:50 231  	    AND ROWNUM <= 1;
15:47:50 232  
15:47:50 233  	  IF SQL%ROWCOUNT = 1 THEN
15:47:50 234  	    RAISE GROUP_EXISTS_EXCEPTION;
15:47:50 235  	  END IF;
15:47:50 236  
15:47:50 237  	  EXCEPTION
15:47:50 238  	    WHEN NO_DATA_FOUND THEN
15:47:50 239  	      NULL;
15:47:50 240  	END;
15:47:50 241  
15:47:50 242  	-- Insert new row into ACCOUNT table
15:47:50 243  	PROCS_ACCOUNT_CRU_V17.CREATE_ACCOUNT(
15:47:50 244  	  out_account_id	=> new_account_id,
15:47:50 245  	  in_account_status_id	=> GLOBAL_STATUSES_V17.ACCOUNT_ACTIVE,
15:47:50 246  	  in_group_id		=> in_group_id,
15:47:50 247  	  in_created_by 	=> in_created_by,
15:47:50 248  	  in_system_category_id => GLOBAL_ENUMS_V17.SYSTEM_CATEGORY_LIVE
15:47:50 249  	);
15:47:50 250  
15:47:50 251  EXCEPTION
15:47:50 252  WHEN GROUP_EXISTS_EXCEPTION THEN
15:47:50 253  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.DUPLICATE_ERROR,
15:47:50 254  	  SPROC_NAME, 'Group already exists');
15:47:50 255  WHEN OTHERS THEN
15:47:50 256  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:50 257  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:50 258  END CREATE_ACTIVE_ACCOUNT;
15:47:50 259  
15:47:50 260  /******************************************************************************/
15:47:50 261  
15:47:50 262  PROCEDURE REACTIVATE_ACCOUNT (
15:47:50 263  /*
15:47:50 264  Throws exceptions:
15:47:50 265  APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50 266  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:50 267  */
15:47:50 268  	in_group_id   IN NUMBER,
15:47:50 269  	in_updated_by IN VARCHAR2,
15:47:50 270  	in_note       IN VARCHAR2,
15:47:50 271  	in_agent_id   IN NUMBER
15:47:50 272  ) AS
15:47:50 273  -- VARIABLES
15:47:50 274  SPROC_NAME	      CONSTANT VARCHAR2(18) := 'REACTIVATE_ACCOUNT';
15:47:50 275  var_account_id	      NUMBER;
15:47:50 276  current_account_status  NUMBER;
15:47:50 277  
15:47:50 278  -- EXCEPTIONS
15:47:50 279  BAD_CURRENT_ACC_STATUS EXCEPTION;
15:47:50 280  CAN_NOT_CREATE_NOTE    EXCEPTION;
15:47:50 281  EXCEPTION_MESSAGE      VARCHAR2(1024);
15:47:50 282  BEGIN
15:47:50 283  	-- Get account id, status
15:47:50 284  	SELECT
15:47:50 285  	  ACCOUNT.ACCOUNT_STATUS_ID,
15:47:50 286  	  ACCOUNT.ID
15:47:50 287  	INTO
15:47:50 288  	  current_account_status,
15:47:50 289  	  var_account_id
15:47:50 290  	FROM ACCOUNT
15:47:50 291  	WHERE
15:47:50 292  	  ACCOUNT.GROUP_ID = in_group_id;
15:47:50 293  
15:47:50 294  	IF current_account_status != GLOBAL_STATUSES_V17.ACCOUNT_FROZEN THEN
15:47:50 295  	  RAISE BAD_CURRENT_ACC_STATUS;
15:47:50 296  	END IF;
15:47:50 297  
15:47:50 298  	-- Change account status
15:47:50 299  	PROCS_ACCOUNT_V17.UPDATE_ACCOUNT_STATUS(
15:47:50 300  	  in_account_id        => var_account_id,
15:47:50 301  	  in_updated_by        => in_updated_by,
15:47:50 302  	  in_account_status_id => GLOBAL_STATUSES_V17.ACCOUNT_ACTIVE
15:47:50 303  	);
15:47:50 304  
15:47:50 305  	-- Add note
15:47:50 306  	BEGIN
15:47:50 307  	  PROCS_ACCOUNT_V17.ANNOTATE_ACCOUNT(
15:47:50 308  	    in_group_id   => in_group_id,
15:47:50 309  	    in_agent_id   => in_agent_id,
15:47:50 310  	    in_note	  => in_note,
15:47:50 311  	    in_created_by => in_updated_by
15:47:50 312  	  );
15:47:50 313  	  EXCEPTION
15:47:50 314  	    WHEN OTHERS THEN
15:47:50 315  	      EXCEPTION_MESSAGE := SQLERRM;
15:47:50 316  	      RAISE CAN_NOT_CREATE_NOTE;
15:47:50 317  	END;
15:47:50 318  
15:47:50 319  EXCEPTION
15:47:50 320  WHEN BAD_CURRENT_ACC_STATUS THEN
15:47:50 321  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.STATE_ERROR,
15:47:50 322  	  SPROC_NAME, 'Current account status is not "frozen"');
15:47:50 323  WHEN NO_DATA_FOUND THEN
15:47:50 324  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 325  	  SPROC_NAME, 'Could not find account with given group ID');
15:47:50 326  WHEN CAN_NOT_CREATE_NOTE THEN
15:47:50 327  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50 328  	  SPROC_NAME, 'Could not annotate account');
15:47:50 329  WHEN OTHERS THEN
15:47:50 330  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:50 331  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:50 332  END REACTIVATE_ACCOUNT;
