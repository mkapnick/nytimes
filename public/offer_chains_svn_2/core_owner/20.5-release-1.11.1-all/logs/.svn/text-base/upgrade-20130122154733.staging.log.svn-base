15:47:49 2405  CAN_NOT_CREATE_NOTE	   EXCEPTION;
15:47:49 2406  EXCEPTION_MESSAGE	   VARCHAR2(1024);
15:47:49 2407  BEGIN
15:47:49 2408  
15:47:49 2409  	 -- Get current subscription status
15:47:49 2410  	 BEGIN
15:47:49 2411  	   SELECT
15:47:49 2412  	     SUBSCRIPTION.SUBSCRIPTION_STATUS_ID into var_current_subscr_status
15:47:49 2413  	   FROM
15:47:49 2414  	     SUBSCRIPTION
15:47:49 2415  	   WHERE
15:47:49 2416  	     SUBSCRIPTION.ID = in_subscription_id;
15:47:49 2417  	   EXCEPTION
15:47:49 2418  	     WHEN NO_DATA_FOUND THEN
15:47:49 2419  	       RAISE BAD_SUBSCRIPTION_ID;
15:47:49 2420  	 END;
15:47:49 2421  
15:47:49 2422  	 -- Check for invoices with active licenses
15:47:49 2423  	 SELECT
15:47:49 2424  	   COUNT(*) into var_active_invoices_count
15:47:49 2425  	 FROM
15:47:49 2426  	   LICENSE
15:47:49 2427  	   INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
15:47:49 2428  	 WHERE
15:47:49 2429  	   LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V17.LICENSE_ACTIVE
15:47:49 2430  	   AND LICENSE.SUBSCRIPTION_ID = in_subscription_id;
15:47:49 2431  
15:47:49 2432  	 IF var_active_invoices_count > 0 THEN
15:47:49 2433  	   RAISE ACTIVE_INVOICES_FOUND;
15:47:49 2434  	 END IF;
15:47:49 2435  
15:47:49 2436  	 -- Check that subscription is active
15:47:49 2437  	 IF var_current_subscr_status != GLOBAL_STATUSES_V17.SUBSCRIPTION_ACTIVE
15:47:49 2438  	    AND var_current_subscr_status != GLOBAL_STATUSES_V17.SUBSCRIPTION_SUSPENDED THEN
15:47:49 2439  	   RAISE BAD_SUBSCRIPTION_STATUS;
15:47:49 2440  	 END IF;
15:47:49 2441  
15:47:49 2442  	 -- Update subscription data
15:47:49 2443  	 BEGIN
15:47:49 2444  	   PROCS_SUBSCRIPTION_CRU_V17.UPDATE_SUBSCRIPTION(
15:47:49 2445  	     in_subscription_id        => in_subscription_id,
15:47:49 2446  	     in_subscription_status_id => GLOBAL_STATUSES_V17.SUBSCRIPTION_FALSE_START,
15:47:49 2447  	     in_cancelation_date       => in_cancelation_date,
15:47:49 2448  	     in_updated_by	       => in_updated_by,
15:47:49 2449  	     in_sct_id		       => FALSE_START_REASON
15:47:49 2450  	   );
15:47:49 2451  	   EXCEPTION
15:47:49 2452  	     WHEN OTHERS THEN
15:47:49 2453  	       EXCEPTION_MESSAGE := SQLERRM;
15:47:49 2454  	       RAISE CAN_NOT_UPDATE_SUBSCRIPTION;
15:47:49 2455  	 END;
15:47:49 2456  
15:47:49 2457  	 -- Annotate subscription
15:47:49 2458  	 IF in_note IS NOT NULL THEN
15:47:49 2459  	   BEGIN
15:47:49 2460  	     PROCS_SUBSCRIPTION_V17.ANNOTATE_SUBSCRIPTION(
15:47:49 2461  	       in_subscription_id,
15:47:49 2462  	       in_agent_id,
15:47:49 2463  	       in_note,
15:47:49 2464  	       in_updated_by
15:47:49 2465  	     );
15:47:49 2466  	     EXCEPTION
15:47:49 2467  	      WHEN OTHERS THEN
15:47:49 2468  		EXCEPTION_MESSAGE := SQLERRM;
15:47:49 2469  		RAISE CAN_NOT_CREATE_NOTE;
15:47:49 2470  	   END;
15:47:49 2471  	 END IF;
15:47:49 2472  
15:47:49 2473  EXCEPTION
15:47:49 2474  WHEN BAD_SUBSCRIPTION_ID THEN
15:47:49 2475  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 2476  	   SPROC_NAME, 'No such subscription');
15:47:49 2477  WHEN BAD_SUBSCRIPTION_STATUS THEN
15:47:49 2478  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INVALID_PARAMETER,
15:47:49 2479  	   SPROC_NAME, 'Bad current subscription status');
15:47:49 2480  WHEN CAN_NOT_UPDATE_SUBSCRIPTION THEN
15:47:49 2481  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:49 2482  	   SPROC_NAME, 'Could not update subscription data', EXCEPTION_MESSAGE);
15:47:49 2483  WHEN ACTIVE_INVOICES_FOUND THEN
15:47:49 2484  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.STATE_ERROR,
15:47:49 2485  	   SPROC_NAME, 'Invoices with active licenses found');
15:47:49 2486  WHEN CAN_NOT_CREATE_NOTE THEN
15:47:49 2487  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:49 2488  	   SPROC_NAME, 'Could not annotate subscription', EXCEPTION_MESSAGE);
15:47:49 2489  WHEN OTHERS THEN
15:47:49 2490  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 2491  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:47:49 2492  END FINALIZE_FALSE_START;
