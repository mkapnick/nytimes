15:47:49 580  
15:47:49 581  PROCEDURE GET_SUBSCRIPTION_INFO (
15:47:49 582  /*
15:47:49 583  Throws exceptions:
15:47:49 584  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:49 585  */
15:47:49 586  	  in_subscription_id  IN  NUMBER,
15:47:49 587  	  out_result_set      OUT SYS_REFCURSOR
15:47:49 588  ) AS
15:47:49 589  SPROC_NAME CONSTANT VARCHAR2(21) := 'GET_SUBSCRIPTION_INFO';
15:47:49 590  -- VARIABLES
15:47:49 591  temp_subscription_id NUMBER;
15:47:49 592  -- EXCEPTIONS
15:47:49 593  BAD_SUBSCRIPTION_ID EXCEPTION;
15:47:49 594  BEGIN
15:47:49 595  	-- Find subscription by id
15:47:49 596  	-- Return its details
15:47:49 597  
15:47:49 598  	BEGIN
15:47:49 599  	  SELECT
15:47:49 600  	    SUBSCRIPTION.ID into temp_subscription_id
15:47:49 601  	  FROM
15:47:49 602  	    SUBSCRIPTION
15:47:49 603  	  WHERE
15:47:49 604  	    SUBSCRIPTION.ID = in_subscription_id;
15:47:49 605  	  EXCEPTION
15:47:49 606  	    WHEN NO_DATA_FOUND THEN
15:47:49 607  	      RAISE BAD_SUBSCRIPTION_ID;
15:47:49 608  	END;
15:47:49 609  
15:47:49 610  	OPEN out_result_set FOR
15:47:49 611  	SELECT
15:47:49 612  	  SUBSCRIPTION.ID AS "SUBSCRIPTION_ID",
15:47:49 613  	  SUBSCRIPTION.SUBSCRIPTION_STATUS_ID,
15:47:49 614  	  SUBSCRIPTION.PURCHASE_DATE,
15:47:49 615  	  SUBSCRIPTION.SUSPEND_DATE,
15:47:49 616  	  SUBSCRIPTION.REACTIVATION_DATE,
15:47:49 617  	  SUBSCRIPTION.CANCELLATION_DATE,
15:47:49 618  	  SUBSCRIPTION_CANCEL_REASON.VALUE as "CANCEL_TYPE",
15:47:49 619  	  OFFER_CHAIN.ID AS "OFFER_CHAIN_ID",
15:47:49 620  	  OFFER_CHAIN.NAME,
15:47:49 621  	  OFFER_CHAIN.DESCRIPTION,
15:47:49 622  	  OFFER_CHAIN.PRODUCT_URI,
15:47:49 623  	  SUBSCRIPTION.INSTRUMENT_ID,
15:47:49 624  	  SUBSCRIPTION.INSTRUMENT_TYPE_ID,
15:47:49 625  	  --PROCS_OFFER_CHAIN_V17.CALCULATE_OFFER_CHAIN_END_DATE(OFFER_CHAIN.ID, SUBSCRIPTION.PURCHASE_DATE) as "END_DATE",
15:47:49 626  	  PROCS_SUBSCRIPTION_V17.CALC_SUBSCRIPTION_END_DATE(SUBSCRIPTION.ID) as "END_DATE",
15:47:49 627  	  PROCS_SUBSCRIPTION_V17.GET_RECENT_CHARGE(SUBSCRIPTION.ID) AS "RECENT_CHARGE",
15:47:49 628  	  PROCS_SUBSCRIPTION_V17.GET_RENEWAL_DATE(SUBSCRIPTION.ID) AS "RENEWAL_DATE",
15:47:49 629  	  PROCS_SUBSCRIPTION_V17.GET_BILLING_CYCLE(SUBSCRIPTION.ID) AS "BILLING_CYCLE",
15:47:49 630  	  (
15:47:49 631  	    SELECT
15:47:49 632  	      ACCOUNT.GROUP_ID
15:47:49 633  	      FROM ACCOUNT
15:47:49 634  	      WHERE ACCOUNT.ID = SUBSCRIPTION.ACCOUNT_ID
15:47:49 635  	  ) as "GROUP_ID",
15:47:49 636  	  (
15:47:49 637  	    SELECT
15:47:49 638  	      MAX(ENTITLEMENT_END_DATE)
15:47:49 639  	      FROM LICENSE
15:47:49 640  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:47:49 641  	  )
15:47:49 642  	  as "ENT_END_DATE",
15:47:49 643  	  (
15:47:49 644  	    SELECT
15:47:49 645  	      MIN(START_DATE)
15:47:49 646  	      FROM LICENSE
15:47:49 647  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:47:49 648  	  )
15:47:49 649  	  as "ENT_START_DATE",
15:47:49 650  	  PROCS_SUBSCRIPTION_V17.IS_SUBSCRIPTION_CANCELABLE(SUBSCRIPTION.ID) AS "IS_CANCELABLE",
15:47:49 651  	  ITUNES_RECEIPT.ID AS "ITUNES_RECEIPT_ID",
15:47:49 652  	  GROUP_ACCOUNT.ID GA_ID,
15:47:49 653  	  GROUP_ACCOUNT.SUBSCRIPTION_ID GA_SUBSCRIPTION_ID,
15:47:49 654  	  GROUP_ACCOUNT.GROUP_NAME GA_GROUP_NAME,
15:47:49 655  	  GROUP_ACCOUNT.FIRST_NAME GA_FIRST_NAME,
15:47:49 656  	  GROUP_ACCOUNT.LAST_NAME GA_LAST_NAME,
15:47:49 657  	  GROUP_ACCOUNT.EMAIL GA_EMAIL,
15:47:49 658  	  GROUP_ACCOUNT.PHONE GA_PHONE,
15:47:49 659  	  GROUP_ACCOUNT.ORGANIZATION_TYPE GA_ORGANIZATION_TYPE,
15:47:49 660  	  GROUP_ACCOUNT.SEATS GA_SEATS,
15:47:49 661  	  PROCS_GROUP_ACCOUNT_V17.F_GET_NUM_OCCUPIED_GROUP_SEATS(GROUP_ACCOUNT.ID) GA_SEATS_USED,
15:47:49 662  	  GROUP_ACCOUNT.IP GA_IP,
15:47:49 663  	  PROCS_SUBSCRIPTION_V17.GET_GIFT_CERT_CODE_BY_SUB_ID(SUBSCRIPTION.ID) gift_certificate_code
15:47:49 664  	FROM
15:47:49 665  	  SUBSCRIPTION
15:47:49 666  	  INNER JOIN OFFER_CHAIN ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
15:47:49 667  	  LEFT JOIN SUBSCRIPTION_CANCEL_REASON ON SUBSCRIPTION.SCT_ID = SUBSCRIPTION_CANCEL_REASON.ID
15:47:49 668  	  LEFT JOIN ITUNES_RECEIPT ON SUBSCRIPTION.ID = ITUNES_RECEIPT.SUBSCRIPTION_ID
15:47:49 669  	  LEFT JOIN GROUP_ACCOUNT ON SUBSCRIPTION.ID = GROUP_ACCOUNT.SUBSCRIPTION_ID
15:47:49 670  	WHERE
15:47:49 671  	  SUBSCRIPTION.ID = in_subscription_id;
15:47:49 672  
15:47:49 673  EXCEPTION
15:47:49 674  WHEN BAD_SUBSCRIPTION_ID THEN
15:47:49 675  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 676  	  SPROC_NAME, 'No such subscription');
15:47:49 677  WHEN OTHERS THEN
15:47:49 678  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 679  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:49 680  END GET_SUBSCRIPTION_INFO;
15:47:49 681  
15:47:49 682  /******************************************************************************/
15:47:49 683  
15:47:49 684  PROCEDURE GET_SUBSCRIPTION_INVOICES (
15:47:49 685  /*
15:47:49 686  Throws exceptions:
15:47:49 687  APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 688  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:49 689  */
15:47:49 690  	in_subscription_id  IN	NUMBER,
15:47:49 691  	out_result_set	    OUT SYS_REFCURSOR
15:47:49 692  ) AS
15:47:49 693  SPROC_NAME	   CONSTANT VARCHAR2(25) := 'GET_SUBSCRIPTION_INVOICES';
15:47:49 694  temp_subscription_id NUMBER;
15:47:49 695  BEGIN
15:47:49 696  	-- Note: A subscription has one or more associated licenses, each of which has an associated invoice.
15:47:49 697  	-- Find associated LICENSES for the subscription by "LICENSE.subscription_id"
15:47:49 698  	--   for each license
15:47:49 699  	--     get associated invoice
15:47:49 700  	--     add to results list
15:47:49 701  	--   end loop
15:47:49 702  	-- end
15:47:49 703  
15:47:49 704  	SELECT
15:47:49 705  	  SUBSCRIPTION.ID into temp_subscription_id
15:47:49 706  	FROM
15:47:49 707  	  SUBSCRIPTION
15:47:49 708  	WHERE
15:47:49 709  	  SUBSCRIPTION.ID = in_subscription_id;
15:47:49 710  
15:47:49 711  	OPEN out_result_set FOR
15:47:49 712  	SELECT
15:47:49 713  	  INVOICE.ID,
15:47:49 714  	  INVOICE.INVOICE_STATUS_ID,
15:47:49 715  	  INVOICE.CREATE_DATE,
15:47:49 716  	  INVOICE.CREATED_BY,
15:47:49 717  	  INVOICE.UPDATE_DATE,
15:47:49 718  	  INVOICE.UPDATED_BY,
15:47:49 719  	  INVOICE.TAX_EXEMPT_ID
15:47:49 720  	FROM
15:47:49 721  	  LICENSE
15:47:49 722  	    INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
15:47:49 723  	WHERE
15:47:49 724  	  LICENSE.SUBSCRIPTION_ID = in_subscription_id;
15:47:49 725  
15:47:49 726  EXCEPTION
15:47:49 727  WHEN NO_DATA_FOUND THEN
15:47:49 728  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 729  	  SPROC_NAME, 'Could not find subscription with given ID');
15:47:49 730  WHEN OTHERS THEN
15:47:49 731  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 732  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:49 733  END GET_SUBSCRIPTION_INVOICES;
15:47:49 734  
15:47:49 735  /******************************************************************************/
15:47:49 736  
15:47:49 737  PROCEDURE GET_SUBSCRIPTION_NOTES (
15:47:49 738  /*
15:47:49 739  Throws exceptions:
15:47:49 740  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:49 741  */
15:47:49 742  	in_subscription_id  IN	NUMBER,
15:47:49 743  	out_result_set	    OUT SYS_REFCURSOR
15:47:49 744  ) AS
