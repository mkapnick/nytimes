15:47:49 1209  
15:47:49 1210  	 -- Refunds are negative
15:47:49 1211  	 var_refunds_before := -var_refunds_before;
15:47:49 1212  
15:47:49 1213  	 var_charges_amount := 0;
15:47:49 1214  
15:47:49 1215  	 FOR f_processed_charges IN (
15:47:49 1216  	   SELECT
15:47:49 1217  	     CHARGE.CHARGE_AMOUNT
15:47:49 1218  	   FROM
15:47:49 1219  	     CHARGE
15:47:49 1220  	   WHERE
15:47:49 1221  	     CHARGE.INVOICE_ID = in_invoice_id
15:47:49 1222  	     AND CHARGE.CHARGE_STATUS_ID IN (SELECT GLOBAL_STATUSES_V17.CHARGE_PROCESSED FROM DUAL)
15:47:49 1223  	 )
15:47:49 1224  	 LOOP
15:47:49 1225  	   IF f_processed_charges.CHARGE_AMOUNT > 0 THEN
15:47:49 1226  	     var_charges_amount := var_charges_amount + f_processed_charges.CHARGE_AMOUNT;
15:47:49 1227  	   END IF;
15:47:49 1228  	 END LOOP;
15:47:49 1229  
15:47:49 1230  	 IF (in_refund_amount + var_refunds_before > var_invoice_amount) THEN
15:47:49 1231  	   RAISE TOT_REF_IS_GREATER_THAN_ANOUNT;
15:47:49 1232  	 END IF;
15:47:49 1233  
15:47:49 1234  	 IF (in_refund_amount + var_refunds_before > var_charges_amount) THEN
15:47:49 1235  	   RAISE TOT_REF_IS_GRATER_THAN_CHARGES;
15:47:49 1236  	 END IF;
15:47:49 1237  
15:47:49 1238  	 BEGIN
15:47:49 1239  	   PROCS_TRANSACTION_V17.CREATE_TRANSACTION(
15:47:49 1240  	     in_transaction_id	=> NULL,
15:47:49 1241  	     in_status_id	=> GLOBAL_STATUSES_V17.TRANSACTION_PREPARE,
15:47:49 1242  	     in_amount		=> -in_refund_amount,
15:47:49 1243  	     in_created_by	=> in_created_by,
15:47:49 1244  	     in_order_id	=> NULL,
15:47:49 1245  	     in_is_refund	=> GLOBAL_CONSTANTS_V17.TRUE,
15:47:49 1246  	     out_transaction_id => var_new_transaction_id
15:47:49 1247  	   );
15:47:49 1248  	   EXCEPTION
15:47:49 1249  	     WHEN OTHERS THEN
15:47:49 1250  	       EXCEPTION_MESSAGE := SQLERRM;
15:47:49 1251  	       RAISE CAN_NOT_CREATE_TRANSACTION;
15:47:49 1252  	 END;
15:47:49 1253  
15:47:49 1254  	 BEGIN
15:47:49 1255  	   PROCS_CHARGE_V17.CREATE_CHARGE(
15:47:49 1256  	     in_invoice_id	   => in_invoice_id,
15:47:49 1257  	     in_transaction_id	   => var_new_transaction_id,
15:47:49 1258  	     in_instrument_type_id => var_instrument_type_id,
15:47:49 1259  	     in_instrument_id	   => var_instrument_id,
15:47:49 1260  	     in_charge_amount	   => -in_refund_amount,
15:47:49 1261  	     in_created_by	   => in_created_by,
15:47:49 1262  	     in_charge_status_id   => GLOBAL_STATUSES_V17.CHARGE_OPENED,
15:47:49 1263  	     out_charge_id	   => var_new_charge_id
15:47:49 1264  	   );
15:47:49 1265  	   EXCEPTION
15:47:49 1266  	     WHEN OTHERS THEN
15:47:49 1267  	       EXCEPTION_MESSAGE := SQLERRM;
15:47:49 1268  	       RAISE CAN_NOT_CREATE_CHARGE;
15:47:49 1269  	 END;
15:47:49 1270  
15:47:49 1271  	 IF in_note IS NOT NULL THEN
15:47:49 1272  	   BEGIN
15:47:49 1273  	     PROCS_SUBSCRIPTION_V17.ANNOTATE_SUBSCRIPTION(
15:47:49 1274  	       in_subscription_id => in_subscription_id,
15:47:49 1275  	       in_agent_id	  => 0, -- AGENT_ID??
15:47:49 1276  	       in_note		  => in_note,
15:47:49 1277  	       in_created_by	  => in_created_by
15:47:49 1278  	     );
15:47:49 1279  	     EXCEPTION
15:47:49 1280  	       WHEN OTHERS THEN
15:47:49 1281  		 EXCEPTION_MESSAGE := SQLERRM;
15:47:49 1282  		 RAISE CAN_NOT_ANNOTATE_SUBSCRIPTION;
15:47:49 1283  	   END;
15:47:49 1284  	 END IF;
15:47:49 1285  
15:47:49 1286  	 out_charge_id := var_new_charge_id;
15:47:49 1287  
15:47:49 1288  EXCEPTION
15:47:49 1289  WHEN INVOICE_IS_NOT_CLOSED THEN
15:47:49 1290  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.STATE_ERROR,
15:47:49 1291  	   SPROC_NAME, 'Invoice is not closed');
15:47:49 1292  WHEN BAD_SUBSCRIPTION_ID THEN
15:47:49 1293  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 1294  	   SPROC_NAME, 'No such subscription');
15:47:49 1295  WHEN ACCOUNT_IS_FROZEN THEN
15:47:49 1296  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.STATE_ERROR,
15:47:49 1297  	   SPROC_NAME, 'Could not refund subscription for frozen account');
15:47:49 1298  WHEN BAD_INVOICE_ID THEN
15:47:49 1299  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 1300  	   SPROC_NAME, 'No such invoice');
15:47:49 1301  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
15:47:49 1302  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:49 1303  	   SPROC_NAME, 'Could not calculate invoice amount', EXCEPTION_MESSAGE);
15:47:49 1304  WHEN REFUND_IS_GREATER_THAN_ANOUNT THEN
15:47:49 1305  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INVALID_PARAMETER,
15:47:49 1306  	   SPROC_NAME, 'Refund is greater than amount');
15:47:49 1307  WHEN TOT_REF_IS_GREATER_THAN_ANOUNT THEN
15:47:49 1308  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INVALID_PARAMETER,
15:47:49 1309  	   SPROC_NAME, 'There were refunds before and sum of all refunds and new refund more than invoice amount');
15:47:49 1310  WHEN TOT_REF_IS_GRATER_THAN_CHARGES THEN
15:47:49 1311  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INVALID_PARAMETER,
15:47:49 1312  	   SPROC_NAME, 'Total refund amount is greater than sum of processed charges');
15:47:49 1313  WHEN CAN_NOT_CREATE_TRANSACTION THEN
15:47:49 1314  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:49 1315  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
15:47:49 1316  WHEN CAN_NOT_CREATE_CHARGE THEN
15:47:49 1317  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:49 1318  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
