15:47:48 1147  /******************************************************************************/
15:47:48 1148  
15:47:48 1149  FUNCTION CHECK_FOR_SAME_PRODUCTS (
15:47:48 1150  /*
15:47:48 1151  Throws exceptions (codes):
15:47:48 1152  APP_EXCEPTION_CODES_V17.NOT_FOUND
15:47:48 1153  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:48 1154  Returns:
15:47:48 1155  GLOBAL_CONSTANTS_V17.TRUE if there are at least one same product
15:47:48 1156  GLOBAL_CONSTANTS_V17.FALSE else
15:47:48 1157  */
15:47:48 1158  	 in_offer_chain_1	  IN OFFER_CHAIN.ID%TYPE,
15:47:48 1159  	 in_offer_chain_2	  IN OFFER_CHAIN.ID%TYPE,
15:47:48 1160  	 in_use_eligibility_rules IN NUMBER DEFAULT GLOBAL_CONSTANTS_V17.FALSE
15:47:48 1161  ) RETURN NUMBER AS
15:47:48 1162  SPROC_NAME CONSTANT VARCHAR2(23) := 'CHECK_FOR_SAME_PRODUCTS';
15:47:48 1163  -- CONSTANTS
15:47:48 1164  PRODUCT_ELIGIBILITY_NAME CONSTANT VARCHAR2(19) := 'MAX_CONCURRENT_SUBS';
15:47:48 1165  -- VARIABLES
15:47:48 1166  temp_offer_chain_id	OFFER_CHAIN.ID%TYPE;
15:47:48 1167  var_same_products	SYS_REFCURSOR;
15:47:48 1168  var_same_product_id	NUMBER;
15:47:48 1169  same_product_count	NUMBER;
15:47:48 1170  var_product_eligibility_limit NUMBER;
15:47:48 1171  s_product_eligibility_limit   VARCHAR2(100);
15:47:48 1172  -- EXCEPTIONS
15:47:48 1173  BAD_FIRST_OFFER_CHAIN	      EXCEPTION;
15:47:48 1174  BAD_SECOND_OFFER_CHAIN	      EXCEPTION;
15:47:48 1175  BEGIN
15:47:48 1176  
15:47:48 1177  	 -- Check that first offer chain exists
15:47:48 1178  	 BEGIN
15:47:48 1179  	   SELECT
15:47:48 1180  	     OFFER_CHAIN.ID into temp_offer_chain_id
15:47:48 1181  	   FROM
15:47:48 1182  	     OFFER_CHAIN
15:47:48 1183  	   WHERE
15:47:48 1184  	     OFFER_CHAIN.ID = in_offer_chain_1;
15:47:48 1185  	   EXCEPTION
15:47:48 1186  	     WHEN NO_DATA_FOUND THEN
15:47:48 1187  	       RAISE BAD_FIRST_OFFER_CHAIN;
15:47:48 1188  	 END;
15:47:48 1189  
15:47:48 1190  	 -- Check that second offer chain exists
15:47:48 1191  	 BEGIN
15:47:48 1192  	   SELECT
15:47:48 1193  	     OFFER_CHAIN.ID into temp_offer_chain_id
15:47:48 1194  	   FROM
15:47:48 1195  	     OFFER_CHAIN
15:47:48 1196  	   WHERE
15:47:48 1197  	     OFFER_CHAIN.ID = in_offer_chain_2;
15:47:48 1198  	   EXCEPTION
15:47:48 1199  	     WHEN NO_DATA_FOUND THEN
15:47:48 1200  	       RAISE BAD_SECOND_OFFER_CHAIN;
15:47:48 1201  	 END;
15:47:48 1202  
15:47:48 1203  	 PROCS_OFFER_CHAIN_V17.GET_OFF_CHAINS_SAME_PRODUCTS(
15:47:48 1204  	   in_offer_chain_1 => in_offer_chain_1,
15:47:48 1205  	   in_offer_chain_2 => in_offer_chain_2,
15:47:48 1206  	   out_result_set   => var_same_products
15:47:48 1207  	 );
15:47:48 1208  
15:47:48 1209  	 LOOP
15:47:48 1210  	   FETCH var_same_products INTO var_same_product_id, same_product_count;
15:47:48 1211  	   EXIT WHEN var_same_products%NOTFOUND;
15:47:48 1212  
15:47:48 1213  	   IF in_use_eligibility_rules = GLOBAL_CONSTANTS_V17.FALSE THEN
15:47:48 1214  	     -- Return false because this offer chains having same products
15:47:48 1215  	     RETURN GLOBAL_CONSTANTS_V17.TRUE;
15:47:48 1216  	   ELSE
15:47:48 1217  
15:47:48 1218  	     -- Get eligibility rule for given product
15:47:48 1219  	     BEGIN
15:47:48 1220  	       SELECT
15:47:48 1221  		 PRODUCT_ELIGIBILITY.VALUE into s_product_eligibility_limit
15:47:48 1222  	       FROM
15:47:48 1223  		 PRODUCT_ELIGIBILITY
15:47:48 1224  	       WHERE
15:47:48 1225  		 PRODUCT_ELIGIBILITY.PRODUCT_ID = var_same_product_id
15:47:48 1226  		 AND PRODUCT_ELIGIBILITY.NAME = PRODUCT_ELIGIBILITY_NAME;
15:47:48 1227  
15:47:48 1228  	       -- REVU: What should to be here? 1?
15:47:48 1229  	       EXCEPTION
15:47:48 1230  		 WHEN NO_DATA_FOUND THEN
15:47:48 1231  		   s_product_eligibility_limit := '1';
15:47:48 1232  	     END;
15:47:48 1233  
15:47:48 1234  	     IF UPPER(s_product_eligibility_limit) = GLOBAL_CONSTANTS_V17.MAX_CONSURRENT_PRD_UNLIM THEN
15:47:48 1235  	       RETURN GLOBAL_CONSTANTS_V17.FALSE;
15:47:48 1236  	     END IF;
15:47:48 1237  
15:47:48 1238  	     var_product_eligibility_limit := TO_NUMBER(s_product_eligibility_limit);
15:47:48 1239  
15:47:48 1240  	     -- Check for limit
15:47:48 1241  	     IF var_product_eligibility_limit < same_product_count THEN
15:47:48 1242  	       RETURN GLOBAL_CONSTANTS_V17.TRUE;
15:47:48 1243  	     END IF;
15:47:48 1244  
15:47:48 1245  	   END IF;
15:47:48 1246  	 END LOOP;
15:47:48 1247  
15:47:48 1248  	 RETURN GLOBAL_CONSTANTS_V17.FALSE;
15:47:48 1249  
15:47:48 1250  EXCEPTION
15:47:48 1251  WHEN BAD_FIRST_OFFER_CHAIN THEN
15:47:48 1252  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:48 1253  	   SPROC_NAME, 'First offer chain not found');
15:47:48 1254  WHEN BAD_SECOND_OFFER_CHAIN THEN
15:47:48 1255  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:48 1256  	   SPROC_NAME, 'Second offer chain not found');
15:47:48 1257  WHEN OTHERS THEN
15:47:48 1258  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:48 1259  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:47:48 1260  END CHECK_FOR_SAME_PRODUCTS;
15:47:48 1261  
15:47:48 1262  /******************************************************************************/
15:47:48 1263  
15:47:48 1264  FUNCTION IS_OFFER_CHAIN_CANCELABLE (
15:47:48 1265  /*
