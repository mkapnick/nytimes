15:47:49 2395  SPROC_NAME CONSTANT VARCHAR2(20) := 'FINALIZE_FALSE_START';
15:47:49 2396  FALSE_START_REASON CONSTANT NUMBER := 41;
15:47:49 2397  -- VARIABLES
15:47:49 2398  var_current_subscr_status SUBSCRIPTION.SUBSCRIPTION_STATUS_ID%TYPE;
15:47:49 2399  var_active_invoices_count NUMBER;
15:47:49 2400  -- EXCEPTIONS
