15:47:49 1822  	   SPROC_NAME, 'No such license');
15:47:49 1823  WHEN OTHERS THEN
15:47:49 1824  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 1825  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:47:49 1826  END HAS_FUTURE_LICENSE;
