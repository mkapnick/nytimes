15:47:49 2075  
15:47:49 2076  PROCEDURE CANCEL_SUBSCRIPTION_INVOICE (
15:47:49 2077  /*
15:47:49 2078  APP_EXCEPTION_CODES_V17.NOT_FOUND
15:47:49 2079  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:49 2080  APP_EXCEPTION_CODES_V17.INTERNAL_ERROR
15:47:49 2081  */
15:47:49 2082  	 in_invoice_id	      IN NUMBER,
15:47:49 2083  	 in_updated_by	      IN VARCHAR2,
15:47:49 2084  	 in_refundable	      IN NUMBER DEFAULT GLOBAL_CONSTANTS_V17.FALSE
15:47:49 2085  ) AS
15:47:49 2086  SPROC_NAME CONSTANT VARCHAR2(27) := 'CANCEL_SUBSCRIPTION_INVOICE';
15:47:49 2087  -- VARIABLES
15:47:49 2088  temp_invoice_id	      INVOICE.ID%TYPE;
15:47:49 2089  var_chargeback_amount  NUMBER(10,2);
15:47:49 2090  ver_refund_charge_id   NUMBER;
15:47:49 2091  -- EXCEPTIONS
15:47:49 2092  BAD_INVOICE_ID		    EXCEPTION;
15:47:49 2093  CAN_NOT_CALCULATE_CHARGEBACK EXCEPTION;
15:47:49 2094  CAN_NOT_APPLY_CHARGEBACK     EXCEPTION;
15:47:49 2095  EXCEPTION_MESSAGE	    VARCHAR2(1024);
15:47:49 2096  -- STUB
15:47:49 2097  var_now DATE;
15:47:49 2098  var_revoke NUMBER;
15:47:49 2099  var_refund NUMBER;
15:47:49 2100  var_billed NUMBER;
15:47:49 2101  BEGIN
15:47:49 2102  
15:47:49 2103  	 var_now := sysdate;
15:47:49 2104  
15:47:49 2105  	 -- Check that invoice exists
15:47:49 2106  	 BEGIN
15:47:49 2107  	   SELECT
15:47:49 2108  	     INVOICE.ID into temp_invoice_id
15:47:49 2109  	   FROM
15:47:49 2110  	     INVOICE
15:47:49 2111  	   WHERE
15:47:49 2112  	     INVOICE.ID = in_invoice_id;
15:47:49 2113  	   EXCEPTION
15:47:49 2114  	     WHEN NO_DATA_FOUND THEN
15:47:49 2115  	       RAISE BAD_INVOICE_ID;
15:47:49 2116  	 END;
15:47:49 2117  
15:47:49 2118  	 select decode(count(1), 0, GLOBAL_CONSTANTS_V17.FALSE, GLOBAL_CONSTANTS_V17.TRUE) into var_revoke
15:47:49 2119  	 from license l, subscription s, offer_chain oc
15:47:49 2120  	 where
15:47:49 2121  	   l.subscription_id = s.id and
15:47:49 2122  	   s.offer_chain_id = oc.id and
15:47:49 2123  	   l.invoice_id = in_invoice_id and
15:47:49 2124  	   oc.revoke_entitlements = GLOBAL_CONSTANTS_V17.TRUE and
15:47:49 2125  	   rownum < 2;
15:47:49 2126  
15:47:49 2127  	 var_billed := PROCS_INVOICE_V17.IS_INVOICE_PAYING_STARTED(in_invoice_id);
15:47:49 2128  	 var_refund := GLOBAL_CONSTANTS_V17.FALSE;
15:47:49 2129  
15:47:49 2130  	 -- Check that transaction for given invoice not started
15:47:49 2131  	 -- if refund enabled calculate and apply chargeback
15:47:49 2132  	 IF (
15:47:49 2133  	     var_billed = GLOBAL_CONSTANTS_V17.TRUE
15:47:49 2134  	   )THEN
15:47:49 2135  	   if (in_refundable = GLOBAL_CONSTANTS_V17.TRUE) then
15:47:49 2136  	     -- If started then we need to calculate refund
15:47:49 2137  	     BEGIN
15:47:49 2138  	       PROCS_INVOICE_V17.CALCULATE_INVOICE_CHARGEBACK(
15:47:49 2139  		 in_invoice_id,
15:47:49 2140  		 var_now,
15:47:49 2141  		 var_chargeback_amount
15:47:49 2142  	       );
15:47:49 2143  	       EXCEPTION
15:47:49 2144  		 WHEN OTHERS THEN
15:47:49 2145  		   EXCEPTION_MESSAGE := SQLERRM;
15:47:49 2146  		   RAISE CAN_NOT_CALCULATE_CHARGEBACK;
15:47:49 2147  	     END;
15:47:49 2148  	     IF var_chargeback_amount > 0 THEN
15:47:49 2149  	       BEGIN
15:47:49 2150  		 PROCS_INVOICE_V17.APPLY_REFUND(
15:47:49 2151  		   in_invoice_id,
15:47:49 2152  		   var_chargeback_amount,
15:47:49 2153  		   in_updated_by,
15:47:49 2154  		   ver_refund_charge_id
15:47:49 2155  		 );
15:47:49 2156  		 EXCEPTION
15:47:49 2157  		   WHEN OTHERS THEN
15:47:49 2158  		     EXCEPTION_MESSAGE := SQLERRM;
15:47:49 2159  		     RAISE CAN_NOT_APPLY_CHARGEBACK;
15:47:49 2160  	       END;
15:47:49 2161  	       var_refund := GLOBAL_CONSTANTS_V17.TRUE;
15:47:49 2162  	     END IF;
15:47:49 2163  	   end if;
15:47:49 2164  	 ELSE
15:47:49 2165  
15:47:49 2166  	   FOR f_transaction_to_close IN (
15:47:49 2167  	     SELECT DISTINCT CHARGE.TRANSACTION_ID AS "ID" FROM CHARGE WHERE CHARGE.INVOICE_ID = in_invoice_id and CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V17.CHARGE_OPENED
15:47:49 2168  	   )
15:47:49 2169  	   LOOP
15:47:49 2170  	     PROCS_TRANSACTION_V17.UPDATE_TRANSACTION_STATUS(
15:47:49 2171  	       in_transaction_id	=> f_transaction_to_close.ID,
15:47:49 2172  	       in_updated_by		=> in_updated_by,
15:47:49 2173  	       in_transaction_status_id => GLOBAL_STATUSES_V17.TRANSACTION_CLOSED
15:47:49 2174  	     );
15:47:49 2175  	   END LOOP;
15:47:49 2176  	   -- Needs to close charges. No refund.
15:47:49 2177  	   FOR f_charge_to_close IN (
15:47:49 2178  	     SELECT CHARGE.ID FROM CHARGE WHERE CHARGE.INVOICE_ID = in_invoice_id and CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V17.CHARGE_OPENED
15:47:49 2179  	   )
15:47:49 2180  	   LOOP
15:47:49 2181  	     PROCS_CHARGE_V17.UPDATE_CHARGE_STATUS(
15:47:49 2182  	       in_charge_id	   => f_charge_to_close.ID,
15:47:49 2183  	       in_updated_by	   => in_updated_by,
15:47:49 2184  	       in_charge_status_id => GLOBAL_STATUSES_V17.CHARGE_CANCELED
15:47:49 2185  	     );
15:47:49 2186  	   END LOOP;
15:47:49 2187  
15:47:49 2188  	   PROCS_INVOICE_V17.UPDATE_INVOICE_STATUS(
15:47:49 2189  	     in_invoice_id		    => in_invoice_id,
15:47:49 2190  	     in_updated_by		    => in_updated_by,
15:47:49 2191  	     in_invoice_status_id	    => GLOBAL_STATUSES_V17.INVOICE_CLOSED
15:47:49 2192  	   );
15:47:49 2193  
15:47:49 2194  	 END IF;
15:47:49 2195  	 -- update licenses
15:47:49 2196  	 IF(var_revoke = GLOBAL_CONSTANTS_V17.TRUE OR var_chargeback_amount > 0 OR var_billed = GLOBAL_CONSTANTS_V17.FALSE) THEN
15:47:49 2197  	   FOR f_license_to_cancel IN (
15:47:49 2198  	     SELECT LICENSE.ID FROM LICENSE WHERE LICENSE.INVOICE_ID = in_invoice_id AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V17.LICENSE_ACTIVE
15:47:49 2199  	   )
15:47:49 2200  	   LOOP
15:47:49 2201  	     PROCS_LICENSE_CRU_V17.UPDATE_LICENSE(
15:47:49 2202  	       in_license_id	     => f_license_to_cancel.ID,
15:47:49 2203  	       in_license_status_id  => GLOBAL_STATUSES_V17.LICENSE_CLOSED,
15:47:49 2204  	       in_needs_entitlements => GLOBAL_CONSTANTS_V17.TRUE,
15:47:49 2205  	       in_updated_by	     => in_updated_by,
15:47:49 2206  	       in_entitlement_end_date => var_now
15:47:49 2207  	     );
15:47:49 2208  	   END LOOP;
15:47:49 2209  	 ELSE
15:47:49 2210  	   FOR f_license_to_cancel IN (
15:47:49 2211  	     SELECT LICENSE.ID FROM LICENSE WHERE LICENSE.INVOICE_ID = in_invoice_id AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V17.LICENSE_ACTIVE
15:47:49 2212  	   )
15:47:49 2213  	   LOOP
15:47:49 2214  	     PROCS_LICENSE_CRU_V17.UPDATE_LICENSE(
15:47:49 2215  	       in_license_id	     => f_license_to_cancel.ID,
15:47:49 2216  	       in_license_status_id  => GLOBAL_STATUSES_V17.LICENSE_CLOSED,
15:47:49 2217  	       in_updated_by	     => in_updated_by
15:47:49 2218  	     );
15:47:49 2219  	   END LOOP;
15:47:49 2220  	 END IF;
15:47:49 2221  
15:47:49 2222  
15:47:49 2223  EXCEPTION
15:47:49 2224  WHEN BAD_INVOICE_ID THEN
15:47:49 2225  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 2226  	   SPROC_NAME, 'No such invoice');
15:47:49 2227  WHEN CAN_NOT_CALCULATE_CHARGEBACK THEN
15:47:49 2228  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:49 2229  	   SPROC_NAME, 'Could not calculate invoice refund', EXCEPTION_MESSAGE);
15:47:49 2230  WHEN CAN_NOT_APPLY_CHARGEBACK THEN
15:47:49 2231  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:49 2232  	   SPROC_NAME, 'Could not apply chargeback', EXCEPTION_MESSAGE);
15:47:49 2233  WHEN OTHERS THEN
15:47:49 2234  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 2235  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:47:49 2236  END CANCEL_SUBSCRIPTION_INVOICE;
15:47:49 2237  
15:47:49 2238  /******************************************************************************/
15:47:49 2239  
15:47:49 2240  PROCEDURE FINALIZE_CANCELATION (
15:47:49 2241  /*
15:47:49 2242  APP_EXCEPTION_CODES_V17.NOT_FOUND
15:47:49 2243  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:49 2244  APP_EXCEPTION_CODES_V17.INTERNAL_ERROR
15:47:49 2245  */
15:47:49 2246  --  in_subscription_id	 IN SUBSCRIPTION.ID%TYPE,
15:47:49 2247  --  in_cancelation_reason IN SUBSCRIPTION_CANCEL_REASON.VALUE%TYPE,
15:47:49 2248  --  in_cancelation_date	 IN SUBSCRIPTION.CANCELLATION_DATE%TYPE,
15:47:49 2249  --  in_note		 IN SUBSCRIPTION_NOTE.NOTE%TYPE,
15:47:49 2250  --  in_agent_id		 IN SUBSCRIPTION_NOTE.AGENT_ID%TYPE,
15:47:49 2251  --  in_updated_by	 IN SUBSCRIPTION.UPDATED_BY%TYPE
15:47:49 2252  	 in_subscription_id    IN NUMBER,
15:47:49 2253  	 in_cancelation_reason IN VARCHAR2,
15:47:49 2254  	 in_cancelation_date   IN DATE,
15:47:49 2255  	 in_note	       IN VARCHAR2,
15:47:49 2256  	 in_agent_id	       IN NUMBER,
15:47:49 2257  	 in_updated_by	       IN VARCHAR2
15:47:49 2258  ) AS
15:47:49 2259  SPROC_NAME CONSTANT VARCHAR2(20) := 'FINALIZE_CANCELATION';
15:47:49 2260  -- VARIABLES
15:47:49 2261  var_current_subscr_status SUBSCRIPTION.SUBSCRIPTION_STATUS_ID%TYPE;
15:47:49 2262  var_sct_id		 SUBSCRIPTION.SCT_ID%TYPE;
15:47:49 2263  var_active_invoices_count NUMBER;
15:47:49 2264  -- EXCEPTIONS
15:47:49 2265  BAD_SUBSCRIPTION_ID	   EXCEPTION;
15:47:49 2266  BAD_SUBSCRIPTION_STATUS	   EXCEPTION;
15:47:49 2267  BAD_CANCELATION_REASON	   EXCEPTION;
15:47:49 2268  CAN_NOT_UPDATE_SUBSCRIPTION EXCEPTION;
15:47:49 2269  ACTIVE_INVOICES_FOUND	   EXCEPTION;
15:47:49 2270  CAN_NOT_CREATE_NOTE	   EXCEPTION;
15:47:49 2271  EXCEPTION_MESSAGE	   VARCHAR2(1024);
15:47:49 2272  BEGIN
15:47:49 2273  
15:47:49 2274  	 -- Get current subscription status
15:47:49 2275  	 BEGIN
15:47:49 2276  	   SELECT
15:47:49 2277  	     SUBSCRIPTION.SUBSCRIPTION_STATUS_ID into var_current_subscr_status
15:47:49 2278  	   FROM
15:47:49 2279  	     SUBSCRIPTION
15:47:49 2280  	   WHERE
15:47:49 2281  	     SUBSCRIPTION.ID = in_subscription_id;
15:47:49 2282  	   EXCEPTION
15:47:49 2283  	     WHEN NO_DATA_FOUND THEN
15:47:49 2284  	       RAISE BAD_SUBSCRIPTION_ID;
15:47:49 2285  	 END;
15:47:49 2286  
15:47:49 2287  	 -- Check that subscription reason is correct
15:47:49 2288  	 BEGIN
15:47:49 2289  	   SELECT
15:47:49 2290  	     SUBSCRIPTION_CANCEL_REASON.ID into var_sct_id
15:47:49 2291  	   FROM
15:47:49 2292  	     SUBSCRIPTION_CANCEL_REASON
15:47:49 2293  	   WHERE
15:47:49 2294  	     SUBSCRIPTION_CANCEL_REASON.VALUE LIKE in_cancelation_reason
15:47:49 2295  	     AND ROWNUM <= 1;
15:47:49 2296  	   EXCEPTION
15:47:49 2297  	     WHEN NO_DATA_FOUND THEN
15:47:49 2298  	       RAISE BAD_CANCELATION_REASON;
15:47:49 2299  	 END;
15:47:49 2300  
15:47:49 2301  	 -- Check for invoices with active licenses
15:47:49 2302  	 SELECT
15:47:49 2303  	   COUNT(*) into var_active_invoices_count
15:47:49 2304  	 FROM
15:47:49 2305  	   LICENSE
15:47:49 2306  	   INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
15:47:49 2307  	 WHERE
15:47:49 2308  	   LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V17.LICENSE_ACTIVE
15:47:49 2309  	   AND LICENSE.SUBSCRIPTION_ID = in_subscription_id;
15:47:49 2310  
15:47:49 2311  	 IF var_active_invoices_count > 0 THEN
15:47:49 2312  	   RAISE ACTIVE_INVOICES_FOUND;
15:47:49 2313  	 END IF;
15:47:49 2314  
15:47:49 2315  	 -- Check that subscription is active
15:47:49 2316  	 IF var_current_subscr_status != GLOBAL_STATUSES_V17.SUBSCRIPTION_ACTIVE
15:47:49 2317  	    AND var_current_subscr_status != GLOBAL_STATUSES_V17.SUBSCRIPTION_SUSPENDED THEN
15:47:49 2318  	   RAISE BAD_SUBSCRIPTION_STATUS;
15:47:49 2319  	 END IF;
15:47:49 2320  
15:47:49 2321  	 -- Update subscription data
15:47:49 2322  	 BEGIN
15:47:49 2323  	   PROCS_SUBSCRIPTION_CRU_V17.UPDATE_SUBSCRIPTION(
15:47:49 2324  	     in_subscription_id        => in_subscription_id,
15:47:49 2325  	     in_subscription_status_id => GLOBAL_STATUSES_V17.SUBSCRIPTION_CANCELED,
15:47:49 2326  	     in_cancelation_date       => in_cancelation_date,
15:47:49 2327  	     in_updated_by	       => in_updated_by,
15:47:49 2328  	     in_sct_id		       => var_sct_id
15:47:49 2329  	   );
15:47:49 2330  	   EXCEPTION
15:47:49 2331  	     WHEN OTHERS THEN
15:47:49 2332  	       EXCEPTION_MESSAGE := SQLERRM;
15:47:49 2333  	       RAISE CAN_NOT_UPDATE_SUBSCRIPTION;
15:47:49 2334  	 END;
15:47:49 2335  
15:47:49 2336  	 -- Annotate subscription
15:47:49 2337  	 IF in_note IS NOT NULL THEN
15:47:49 2338  	   BEGIN
