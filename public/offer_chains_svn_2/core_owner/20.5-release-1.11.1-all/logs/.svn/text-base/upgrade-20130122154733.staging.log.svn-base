15:47:50 946  PROCEDURE GET_ACCOUNT_SUBSCR_INVOICES (
15:47:50 947  /*
15:47:50 948  Throws exceptions:
15:47:50 949  APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 950  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:50 951  */
15:47:50 952  	in_group_id	   IN  NUMBER,
15:47:50 953  	in_subscription_id IN NUMBER,
15:47:50 954  	out_result_set	   OUT SYS_REFCURSOR
15:47:50 955  ) AS
15:47:50 956  SPROC_NAME	   CONSTANT VARCHAR2(27) := 'GET_ACCOUNT_SUBSCR_INVOICES';
15:47:50 957  -- VARIABLES
15:47:50 958  var_account_id	   NUMBER;
15:47:50 959  temp_subscription_id NUMBER;
15:47:50 960  -- EXCEPTIONS
15:47:50 961  BAD_GROUP_ID	  EXCEPTION;
15:47:50 962  BAD_SUBSCRIPTION_ID EXCEPTION;
15:47:50 963  BEGIN
15:47:50 964  	-- Get account id
15:47:50 965  	BEGIN
15:47:50 966  	  SELECT
15:47:50 967  	    ACCOUNT.ID into var_account_id
15:47:50 968  	  FROM
15:47:50 969  	    ACCOUNT
15:47:50 970  	  WHERE
15:47:50 971  	    ACCOUNT.GROUP_ID = in_group_id;
15:47:50 972  	  EXCEPTION
15:47:50 973  	    WHEN NO_DATA_FOUND THEN
15:47:50 974  	      RAISE BAD_GROUP_ID;
15:47:50 975  	END;
15:47:50 976  
15:47:50 977  	-- Check that subscription exists
15:47:50 978  	BEGIN
15:47:50 979  	  IF in_subscription_id IS NOT NULL THEN
15:47:50 980  	    SELECT
15:47:50 981  	      SUBSCRIPTION.ID into temp_subscription_id
15:47:50 982  	    FROM
15:47:50 983  	      SUBSCRIPTION
15:47:50 984  	    WHERE
15:47:50 985  	      SUBSCRIPTION.ID = in_subscription_id;
15:47:50 986  	  END IF;
15:47:50 987  	  EXCEPTION
15:47:50 988  	    WHEN NO_DATA_FOUND THEN
15:47:50 989  	      RAISE BAD_SUBSCRIPTION_ID;
15:47:50 990  	END;
15:47:50 991  
15:47:50 992  	OPEN out_result_set FOR
15:47:50 993  	SELECT DISTINCT
15:47:50 994  	  INVOICE.ID as "INVOICE_ID",
15:47:50 995  	  INVOICE.CREATE_DATE,
15:47:50 996  	  INVOICE.INVOICE_STATUS_ID,
15:47:50 997  	  PROCS_INVOICE_V17.F_CALCULATE_INVOICE_AMOUNT(INVOICE.ID) as "AMOUNT",
15:47:50 998  	  OFFER_CHAIN.ID as "OFFER_CHAIN_ID",
15:47:50 999  	  OFFER_CHAIN.NAME as "OFFER_CHAIN_NAME",
15:47:50 1000  	   SUBSCRIPTION.ID as "SUBSCRIPTION_ID",
15:47:50 1001  	   NULL as "GC_CODE",
15:47:50 1002  	   NULL as "GC_ID"
15:47:50 1003  	 FROM
15:47:50 1004  	   LICENSE
15:47:50 1005  	   INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
15:47:50 1006  	   INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
15:47:50 1007  	   INNER JOIN OFFER_CHAIN ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
15:47:50 1008  	 WHERE
15:47:50 1009  	   SUBSCRIPTION.ACCOUNT_ID = var_account_id
15:47:50 1010  	   AND (SUBSCRIPTION.SCT_ID IS NULL OR SUBSCRIPTION.SCT_ID != REAL_TIME_CANCEL_REASON)
15:47:50 1011  	   AND SUBSCRIPTION.ID = NVL(in_subscription_id, SUBSCRIPTION.ID);
15:47:50 1012  
15:47:50 1013  EXCEPTION
15:47:50 1014  WHEN BAD_GROUP_ID THEN
15:47:50 1015  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 1016  	   SPROC_NAME, 'No such account');
15:47:50 1017  WHEN BAD_SUBSCRIPTION_ID THEN
15:47:50 1018  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 1019  	   SPROC_NAME, 'No such subscription');
15:47:50 1020  WHEN OTHERS THEN
15:47:50 1021  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:50 1022  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:47:50 1023  END GET_ACCOUNT_SUBSCR_INVOICES;
