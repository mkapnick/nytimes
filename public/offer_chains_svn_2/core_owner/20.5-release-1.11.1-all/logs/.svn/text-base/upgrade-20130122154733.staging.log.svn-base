15:47:49 388  	      EXCEPTION_MESSAGE := SQLERRM;
15:47:49 389  	      RAISE CAN_NOT_FIND_ACCOUNT;
15:47:49 390  	END;
15:47:49 391  
15:47:49 392  	-- get all line items for given invoice
