15:47:49 2496  FUNCTION IS_SUBSCRIPTION_CANCELABLE (
15:47:49 2497  	 in_subscription_id IN NUMBER
15:47:49 2498  ) RETURN NUMBER AS
15:47:49 2499  SPROC_NAME CONSTANT VARCHAR2(26) := 'IS_SUBSCRIPTION_CANCELABLE';
15:47:49 2500  -- VARIABLES
15:47:49 2501  var_is_offer_chain_cancelable NUMBER;
15:47:49 2502  --64603
15:47:49 2503  var_end_date date;
15:47:49 2504  today_date date := current_date;
15:47:49 2505  offer_id number;
15:47:49 2506  -- EXCEPTIONS
15:47:49 2507  COULD_NOT_CHECK	   EXCEPTION;
15:47:49 2508  BAD_SUBSCRIPTION_ID EXCEPTION;
15:47:49 2509  EXCEPTION_MESSAGE   VARCHAR2(1024);
15:47:49 2510  BEGIN
15:47:49 2511  
15:47:49 2512  	 BEGIN
15:47:49 2513  	   -- find offer_chain_id for given in_subscription_id
15:47:49 2514  	   SELECT OFFER_CHAIN_ID into offer_id
15:47:49 2515  	   FROM SUBSCRIPTION
15:47:49 2516  	   WHERE ID = in_subscription_id;
15:47:49 2517  	   EXCEPTION
15:47:49 2518  	     WHEN NO_DATA_FOUND THEN
15:47:49 2519  	       RAISE BAD_SUBSCRIPTION_ID;
15:47:49 2520  	     WHEN OTHERS THEN
15:47:49 2521  	       EXCEPTION_MESSAGE := SQLERRM;
15:47:49 2522  	       RAISE COULD_NOT_CHECK;
15:47:49 2523  	 END;
15:47:49 2524  
15:47:49 2525  	 -- find if it was redeemed from a gift certificate
15:47:49 2526  	 BEGIN
15:47:49 2527  	   SELECT l.end_date INTO var_end_date
15:47:49 2528  	   FROM LICENSE l, GIFT_CERTIFICATE g
15:47:49 2529  	   WHERE l.invoice_id = g.finalized_invoice_id
15:47:49 2530  	   AND l.subscription_id = in_subscription_id;
15:47:49 2531  
15:47:49 2532  	   -- if the license end_date is bigger than today, we are in the
15:47:49 2533  	   -- first period, so we cannot cancel; otherwise can cancel
15:47:49 2534  	   IF var_end_date > today_date THEN
15:47:49 2535  	       RETURN GLOBAL_CONSTANTS_V17.FALSE;
15:47:49 2536  	   ELSE
15:47:49 2537  	       RETURN GLOBAL_CONSTANTS_V17.TRUE;
15:47:49 2538  	   END IF;
15:47:49 2539  
15:47:49 2540  	   EXCEPTION
15:47:49 2541  	       -- not coming from a gift certificate,
15:47:49 2542  	       -- use old logic
15:47:49 2543  	       WHEN NO_DATA_FOUND THEN
15:47:49 2544  		   SELECT
15:47:49 2545  		       PROCS_OFFER_CHAIN_V17.IS_OFFER_CHAIN_CANCELABLE(offer_id)
15:47:49 2546  		       INTO var_is_offer_chain_cancelable
15:47:49 2547  		   FROM DUAL;
15:47:49 2548  		   RETURN var_is_offer_chain_cancelable;
15:47:49 2549  	 END;
15:47:49 2550  
15:47:49 2551  EXCEPTION
15:47:49 2552  WHEN BAD_SUBSCRIPTION_ID THEN
15:47:49 2553  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 2554  	   SPROC_NAME, 'No such subscription');
15:47:49 2555  WHEN COULD_NOT_CHECK THEN
15:47:49 2556  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:49 2557  	   SPROC_NAME, 'Could not check if offer chain calcelable', EXCEPTION_MESSAGE);
15:47:49 2558  WHEN OTHERS THEN
15:47:49 2559  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 2560  	   SPROC_NAME, 'Unknown error', SQLERRM);
15:47:49 2561  
15:47:49 2562  END IS_SUBSCRIPTION_CANCELABLE;
