15:47:49 1436  	   RAISE EXTENS_START_DATE_IS_TOO_SMALL;
15:47:49 1437  	 END IF;
15:47:49 1438  
15:47:49 1439  	 IF in_effective_start_date > in_effective_end_date THEN
15:47:49 1440  	   RAISE EXT_START_DATE_LATER_THEN_END;
15:47:49 1441  	 END IF;
15:47:49 1442  
15:47:49 1443  	 -- Closing curent license
15:47:49 1444  	 BEGIN
15:47:49 1445  	   PROCS_LICENSE_CRU_V17.UPDATE_LICENSE(
15:47:49 1446  	     in_license_id	   => var_current_license_id,
15:47:49 1447  	     in_updated_by	   => in_updated_by,
15:47:49 1448  	     in_license_status_id  => GLOBAL_STATUSES_V17.LICENSE_CLOSED,
15:47:49 1449  	     in_end_date	   => in_effective_start_date,
15:47:49 1450  	     in_needs_entitlements => GLOBAL_CONSTANTS_V17.TRUE
15:47:49 1451  	   );
15:47:49 1452  	   EXCEPTION
15:47:49 1453  	     WHEN OTHERS THEN
15:47:49 1454  	       EXCEPTION_MESSAGE := SQLERRM;
15:47:49 1455  	       RAISE CAN_NOT_CHANGE_LICENSE_STATUS;
15:47:49 1456  	 END;
15:47:49 1457  
15:47:49 1458  	 -- Creating new "free" invoice
15:47:49 1459  	 BEGIN
