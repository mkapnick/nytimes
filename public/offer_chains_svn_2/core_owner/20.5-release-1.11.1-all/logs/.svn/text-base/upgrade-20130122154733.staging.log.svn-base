15:47:49 122  PROCEDURE GET_PENDING_REFUND_CHARGES (
15:47:49 123  /*
15:47:49 124  Throws exceptions:
15:47:49 125  APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR
15:47:49 126  */
15:47:49 127  	out_result_set	    OUT SYS_REFCURSOR,
15:47:49 128  	in_row_number	    IN NUMBER DEFAULT NULL
15:47:49 129  ) AS
15:47:49 130  SPROC_NAME CONSTANT VARCHAR2(26) := 'GET_PENDING_REFUND_CHARGES';
15:47:49 131  -- COMSTANTS
15:47:49 132  DEFAULT_ROW_NUMBER CONSTANT NUMBER := 1;
15:47:49 133  -- VARIABLES
15:47:49 134  var_row_number NUMBER;
15:47:49 135  BEGIN
15:47:49 136  	IF in_row_number IS NULL THEN
15:47:49 137  	  var_row_number := DEFAULT_ROW_NUMBER;
15:47:49 138  	ELSE
15:47:49 139  	  var_row_number := in_row_number;
15:47:49 140  	END IF;
15:47:49 141  
15:47:49 142  	-- Select charges
15:47:49 143  	OPEN out_result_set FOR
15:47:49 144  SELECT * FROM
15:47:49 145  (
15:47:49 146  	SELECT
15:47:49 147  	  CHARGE.ID,
15:47:49 148  	  CHARGE.TRANSACTION_ID,
15:47:49 149  	  CHARGE.INSTRUMENT_ID,
15:47:49 150  	  CHARGE.INSTRUMENT_TYPE_ID,
15:47:49 151  	  CHARGE.CHARGE_AMOUNT,
15:47:49 152  	  CHARGE.CREATE_DATE,
15:47:49 153  	  CHARGE.CREATED_BY,
15:47:49 154  	  CHARGE.INVOICE_ID
15:47:49 155  	FROM
15:47:49 156  	  CHARGE
15:47:49 157  	  INNER JOIN TRANSACTION ON CHARGE.TRANSACTION_ID = TRANSACTION.ID
15:47:49 158  	WHERE
15:47:49 159  	  TRANSACTION.TRANSACTION_STATUS_ID = GLOBAL_STATUSES_V17.TRANSACTION_PENDING
15:47:49 160  	  AND TRANSACTION.IS_REFUND = GLOBAL_CONSTANTS_V17.TRUE
15:47:49 161  	  AND TRANSACTION.TRANSACTION_AMOUNT < 0
15:47:49 162  	  AND NOT EXISTS
15:47:49 163  	  (
15:47:49 164  	    SELECT NULL
15:47:49 165  	    FROM PROCESS_RETRY_THROTTLE
15:47:49 166  	    WHERE PROCESS_NAME = SPROC_NAME
15:47:49 167  	      AND GENERIC_ID = CHARGE.ID
15:47:49 168  	  )
15:47:49 169  	  AND ROWNUM <= var_row_number*10
15:47:49 170  	  ORDER BY dbms_random.value
15:47:49 171  ) WHERE
15:47:49 172  	  ROWNUM <= var_row_number;
15:47:49 173  
15:47:49 174  END GET_PENDING_REFUND_CHARGES;
