15:47:49  48  
15:47:49  49  	out_license_id := NULL;
15:47:49  50  
15:47:49  51  	-- get offer id and offer entitlement duration
15:47:49  52  	BEGIN
15:47:49  53  	  SELECT
15:47:49  54  	    OFFER.ID,
15:47:49  55  	    OFFER.ENTITLEMENT_DURATION
15:47:49  56  	    into
15:47:49  57  	    temp_offer_id,
15:47:49  58  	    var_offer_duration
15:47:49  59  	  FROM
15:47:49  60  	    OFFER
15:47:49  61  	  WHERE
15:47:49  62  	    OFFER.ID = in_offer_id;
15:47:49  63  	  EXCEPTION
15:47:49  64  	  WHEN NO_DATA_FOUND THEN
15:47:49  65  	    RAISE BAD_OFFER_ID;
15:47:49  66  	END;
15:47:49  67  
15:47:49  68  	-- Check that subscription exists
15:47:49  69  	BEGIN
15:47:49  70  	  SELECT
15:47:49  71  	    SUBSCRIPTION.ID into temp_subscription_id
15:47:49  72  	  FROM
15:47:49  73  	    SUBSCRIPTION
15:47:49  74  	  WHERE
15:47:49  75  	    SUBSCRIPTION.ID = in_subscription_id;
15:47:49  76  	  EXCEPTION
15:47:49  77  	  WHEN NO_DATA_FOUND THEN
15:47:49  78  	    RAISE BAD_SUBSCRIPTION_ID;
15:47:49  79  	END;
15:47:49  80  
15:47:49  81  	-- Check that invoice exists
15:47:49  82  	BEGIN
15:47:49  83  	  SELECT
15:47:49  84  	    INVOICE.ID into temp_invoice_id
15:47:49  85  	  FROM
15:47:49  86  	    INVOICE
15:47:49  87  	  WHERE
15:47:49  88  	    INVOICE.ID = in_invoice_id;
15:47:49  89  	  EXCEPTION
15:47:49  90  	  WHEN NO_DATA_FOUND THEN
15:47:49  91  	    RAISE BAD_INVOICE_ID;
15:47:49  92  	END;
15:47:49  93  
15:47:49  94  	-- convert offer duration into intervals
15:47:49  95  	BEGIN
15:47:49  96  	  PROCS_COMMON_V17.ISO8601DURATION_TO_INTERVALS(
15:47:49  97  	    var_offer_duration,
15:47:49  98  	    var_offer_years,
15:47:49  99  	    var_offer_months,
15:47:49 100  	    var_offer_days);
15:47:49 101  	  var_offer_ym_interval := var_offer_years||'-'||var_offer_months;
15:47:49 102  	  var_offer_ds_interval := var_offer_days||' 0:0:0';
15:47:49 103  	  EXCEPTION
15:47:49 104  	    WHEN OTHERS THEN
15:47:49 105  	      EXCEPTION_MESSAGE := SQLERRM;
15:47:49 106  	      RAISE BAD_OFFER_DURATION;
15:47:49 107  	END;
15:47:49 108  
15:47:49 109  	-- insert new row into license table
15:47:49 110  	PROCS_LICENSE_CRU_V17.CREATE_LICENSE(
15:47:49 111  	  out_license_id	      => var_new_license_id,
15:47:49 112  	  in_license_status_id	      => in_status_id,
15:47:49 113  	  in_needs_entitlements       => in_needs_entitlements,
15:47:49 114  	  in_start_date 	      => in_start_date,
15:47:49 115  	  in_offer_id		      => in_offer_id,
15:47:49 116  	  in_subscription_id	      => in_subscription_id,
15:47:49 117  	  in_invoice_id 	      => in_invoice_id,
15:47:49 118  	  in_end_date		      => NVL(in_end_date, in_start_date + var_offer_ym_interval + var_offer_ds_interval),
15:47:49 119  	  in_created_by 	      => in_created_by,
15:47:49 120  	  in_is_extension	      => in_is_extension,
15:47:49 121  	  in_current_offer_index      => in_current_offer_index,
15:47:49 122  	  in_current_offer_recurr_num => in_current_offer_recurr_num
15:47:49 123  	);
15:47:49 124  
15:47:49 125  	out_license_id := var_new_license_id;
15:47:49 126  
15:47:49 127  EXCEPTION
15:47:49 128  WHEN BAD_OFFER_DURATION THEN
15:47:49 129  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:49 130  	  SPROC_NAME, 'Bad offer duration format', EXCEPTION_MESSAGE);
15:47:49 131  WHEN BAD_OFFER_ID THEN
15:47:49 132  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 133  	  SPROC_NAME, 'No such offer');
15:47:49 134  WHEN BAD_SUBSCRIPTION_ID THEN
15:47:49 135  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 136  	  SPROC_NAME, 'No such subscription');
15:47:49 137  WHEN BAD_INVOICE_ID THEN
15:47:49 138  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:49 139  	  SPROC_NAME, 'No such invoice');
15:47:49 140  WHEN OTHERS THEN
15:47:49 141  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 142  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:49 143  END;
