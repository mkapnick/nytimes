15:47:49 207  
15:47:49 208  PROCEDURE GET_ENDING_LICENSES (
15:47:49 209  	in_hours_number IN NUMBER,
15:47:49 210  	out_result_set	OUT SYS_REFCURSOR
15:47:49 211  ) AS
15:47:49 212  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_ENDING_LICENSES';
15:47:49 213  -- VARIABLES
15:47:49 214  var_days		NUMBER;
15:47:49 215  var_hours 	NUMBER;
15:47:49 216  var_time_interval INTERVAL DAY (3) TO SECOND;
15:47:49 217  BEGIN
15:47:49 218  
15:47:49 219  	var_hours := mod(in_hours_number,24);
15:47:49 220  	var_days := (in_hours_number - var_hours) / 24;
15:47:49 221  	var_time_interval := var_days||' '||var_hours||':0:0';
15:47:49 222  
15:47:49 223  	OPEN out_result_set FOR
15:47:49 224  SELECT * FROM
15:47:49 225  (
15:47:49 226  	SELECT
15:47:49 227  	  LICENSE.ID,
15:47:49 228  	  LICENSE.CREATE_DATE,
15:47:49 229  	  LICENSE.CREATED_BY,
15:47:49 230  	  LICENSE.CURRENT_OFFER_INDEX,
15:47:49 231  	  LICENSE.CURRENT_OFFER_RECURR_NUM,
15:47:49 232  	  LICENSE.END_DATE,
15:47:49 233  	  LICENSE.ENTITLEMENT_END_DATE,
15:47:49 234  	  LICENSE.INVOICE_ID,
15:47:49 235  	  LICENSE.IS_EXTENSION,
15:47:49 236  	  LICENSE.LICENSE_STATUS_ID,
15:47:49 237  	  LICENSE.OFFER_ID,
15:47:49 238  	  LICENSE.START_DATE,
15:47:49 239  	  LICENSE.SUBSCRIPTION_ID,
15:47:49 240  	  LICENSE.UPDATE_DATE,
15:47:49 241  	  LICENSE.UPDATED_BY
15:47:49 242  	FROM
15:47:49 243  	  LICENSE
15:47:49 244  	WHERE
15:47:49 245  	  TO_DATE(LICENSE.END_DATE) <= (current_timestamp + var_time_interval)
15:47:49 246  	  AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V17.LICENSE_ACTIVE
15:47:49 247  	  AND NOT EXISTS
15:47:49 248  	  (
15:47:49 249  	    SELECT NULL
15:47:49 250  	    FROM PROCESS_RETRY_THROTTLE
15:47:49 251  	    WHERE PROCESS_NAME = SPROC_NAME
15:47:49 252  	      AND GENERIC_ID = LICENSE.ID
15:47:49 253  	  )
15:47:49 254  	  AND ROWNUM <= 10000
15:47:49 255  	  ORDER BY dbms_random.value
15:47:49 256  ) WHERE
15:47:49 257  	  ROWNUM <= 1000
15:47:49 258  	  ;
15:47:49 259  EXCEPTION
15:47:49 260  WHEN OTHERS THEN
15:47:49 261  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:49 262  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:49 263  END GET_ENDING_LICENSES;
