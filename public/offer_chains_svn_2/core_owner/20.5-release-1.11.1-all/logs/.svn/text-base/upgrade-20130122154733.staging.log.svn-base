15:47:50 1829  	   EXCEPTION
15:47:50 1830  	     WHEN NO_DATA_FOUND THEN
15:47:50 1831  	       RAISE BAD_GIFT_CERTIFICATE_CODE;
15:47:50 1832  	 END;
15:47:50 1833  
15:47:50 1834  	 -- Select all gift certificates with given code
15:47:50 1835  	 OPEN out_result_set FOR
15:47:50 1836  	 SELECT
15:47:50 1837  	   gc.EXPIRATION_DATE,
15:47:50 1838  	   ch.name,
15:47:50 1839  	   ch.id,
15:47:50 1840  	   gc.sender_email,
15:47:50 1841  	   gc.sender_name,
15:47:50 1842  	   gc.recipient_email,
15:47:50 1843  	   gc.recipient_name,
15:47:50 1844  	   gc.purchase_date,
15:47:50 1845  	   gc.redemption_date,
15:47:50 1846  	   gc.purchaser_group_id,
15:47:50 1847  	   gc.redeemer_group_id,
15:47:50 1848  	   gc.gift_message,
15:47:50 1849  	   ocmd.value redemption_offer_chain_ids,
