15:47:50 1815  -- VARIABLES
15:47:50 1816  SPROC_NAME		CONSTANT VARCHAR2(28) := 'GET_GIFT_CERTIFICATE_BY_CODE';
15:47:50 1817  temp_gift_certificate_id NUMBER;
15:47:50 1818  -- EXCEPTIONS
15:47:50 1819  BAD_GIFT_CERTIFICATE_CODE EXCEPTION;
15:47:50 1820  BEGIN
15:47:50 1821  
15:47:50 1822  	 BEGIN
15:47:50 1823  	   SELECT
15:47:50 1824  	     GIFT_CERTIFICATE.ID into temp_gift_certificate_id
15:47:50 1825  	   FROM
15:47:50 1826  	     GIFT_CERTIFICATE
15:47:50 1827  	   WHERE
