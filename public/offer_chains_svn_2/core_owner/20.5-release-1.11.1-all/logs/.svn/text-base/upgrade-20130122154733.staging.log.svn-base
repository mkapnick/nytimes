15:47:49 1190  	       EXCEPTION_MESSAGE := SQLERRM;
15:47:49 1191  	       RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
15:47:49 1192  	 END;
15:47:49 1193  
15:47:49 1194  	 IF ( in_refund_amount > var_invoice_amount ) THEN
15:47:49 1195  	   RAISE REFUND_IS_GREATER_THAN_ANOUNT;
15:47:49 1196  	 END IF;
15:47:49 1197  
15:47:49 1198  	 SELECT /*+ STAR_TRANSFORMATION */
15:47:49 1199  	   SUM(CHARGE.CHARGE_AMOUNT) into var_refunds_before
15:47:49 1200  	 FROM
15:47:49 1201  	   CHARGE
15:47:49 1202  	 WHERE
15:47:49 1203  	   CHARGE.INVOICE_ID = in_invoice_id
15:47:49 1204  	   AND (
15:47:49 1205  	     CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V17.CHARGE_OPENED
15:47:49 1206  	     OR CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V17.CHARGE_PROCESSED
15:47:49 1207  	   )
