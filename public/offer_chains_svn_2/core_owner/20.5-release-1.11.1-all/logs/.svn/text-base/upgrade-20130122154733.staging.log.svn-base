15:47:50 1860  	 LEFT JOIN SUBSCRIPTION s ON l.subscription_id = s.id
15:47:50 1861  	 WHERE
15:47:50 1862  	   gc.code = in_code;
15:47:50 1863  
15:47:50 1864  EXCEPTION
15:47:50 1865  WHEN BAD_GIFT_CERTIFICATE_CODE THEN
15:47:50 1866  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 1867  	   SPROC_NAME, 'No such gift certificate');
15:47:50 1868  WHEN OTHERS THEN
15:47:50 1869  	 PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:50 1870  	   SPROC_NAME, 'Unknown Error', SQLERRM);
15:47:50 1871  END;
