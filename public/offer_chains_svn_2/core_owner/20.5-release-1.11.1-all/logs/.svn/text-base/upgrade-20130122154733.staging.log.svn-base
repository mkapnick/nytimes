15:47:50 716  	      EXCEPTION_MESSAGE := SQLERRM;
15:47:50 717  	      RAISE CAN_NOT_GET_DEF_FIN_INSTRUMENT;
15:47:50 718  	END;
15:47:50 719  
15:47:50 720  	IF var_def_instrument_type_id = GLOBAL_ENUMS_V17.INSTRUMENT_PAYPAL THEN
15:47:50 721  	  out_default_id := var_def_instrument_id;
15:47:50 722  	ELSE
15:47:50 723  	  out_default_id := 0;
15:47:50 724  	END IF;
15:47:50 725  
15:47:50 726  EXCEPTION
15:47:50 727  WHEN NO_DATA_FOUND THEN
15:47:50 728  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.NOT_FOUND,
15:47:50 729  	  SPROC_NAME, 'Could not find account with given group id');
15:47:50 730  WHEN WRONG_INSTR_EXCEPTION THEN
15:47:50 731  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INVALID_PARAMETER,
15:47:50 732  	  SPROC_NAME, 'Bad paypal status id');
15:47:50 733  WHEN CAN_NOT_GET_DEF_FIN_INSTRUMENT THEN
15:47:50 734  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.INTERNAL_ERROR,
15:47:50 735  	  SPROC_NAME, 'Could not get default financial instrument', EXCEPTION_MESSAGE);
15:47:50 736  WHEN OTHERS THEN
15:47:50 737  	PROCS_COMMON_V17.THROW_EXCEPTION(APP_EXCEPTION_CODES_V17.UNKNOWN_ERROR,
15:47:50 738  	  SPROC_NAME, 'Unknown error', SQLERRM);
15:47:50 739  END GET_ACCOUNT_PAYPALS;
