14:50:31 954  		  (in_maximum_ip_high = minimum_ip_high and (in_maximum_ip_low >= minimum_ip_low and in_maximum_ip_low <= maximum_ip_low)) or
14:50:31 955  		  (in_maximum_ip_high = maximum_ip_high and (in_maximum_ip_low >= minimum_ip_low and in_maximum_ip_low <= maximum_ip_low))
14:50:31 956  		)) AND
14:50:31 957  	    ROWNUM < 3;
14:50:31 958  
14:50:31 959  	  If(var_is_dupe > 1) THEN
14:50:31 960  	    RAISE DUPE;
14:50:31 961  	  END IF;
14:50:31 962  EXCEPTION
14:50:31 963  WHEN NOT_GL THEN
14:50:31 964  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INVALID_PARAMETER,
