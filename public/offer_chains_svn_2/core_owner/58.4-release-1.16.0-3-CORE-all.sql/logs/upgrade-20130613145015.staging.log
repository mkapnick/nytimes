14:50:31 824  PROCEDURE SHOULD_MOVE_TO_GRACE(
14:50:31 825  	in_invoice_id  IN INVOICE.ID%TYPE,
14:50:31 826  	out_result     OUT NUMBER
14:50:31 827  ) AS
14:50:31 828  BEGIN
14:50:31 829  	-- if the invoice preceding the given invoice has no transaction attempts, then
14:50:31 830  	-- it is not billed out of Sartre. if so, then the associated subscription
14:50:31 831  	-- should be canceled after a final failed billing attempt--not moved to grace.
14:50:31 832  	SELECT DECODE(COUNT(1), 0, 0, 1) INTO out_result
14:50:31 833  	FROM CHARGE c
14:50:31 834  	INNER JOIN TRANSACTION t ON c.TRANSACTION_ID = t.ID
14:50:31 835  	INNER JOIN TRANSACTION_ATTEMPT ta ON ta.TRANSACTION_ID = t.ID
14:50:31 836  	WHERE c.INVOICE_ID = (
14:50:31 837  	  -- select previous invoice_id, or -1 if there is none
14:50:31 838  	  SELECT PREV_INVOICE_ID FROM (
14:50:31 839  	    SELECT i.ID, LAG(i.ID, 1, -1) OVER (ORDER BY i.CREATE_DATE) AS PREV_INVOICE_ID
14:50:31 840  	    FROM INVOICE i
14:50:31 841  	    INNER JOIN LICENSE l ON i.ID = l.INVOICE_ID
14:50:31 842  	    WHERE l.SUBSCRIPTION_ID = (
14:50:31 843  	      SELECT SUBSCRIPTION_ID FROM LICENSE WHERE INVOICE_ID = in_invoice_id
14:50:31 844  	    )
14:50:31 845  	  ) WHERE ID = in_invoice_id
14:50:31 846  	);
14:50:31 847  END SHOULD_MOVE_TO_GRACE;
