14:50:31 631  
14:50:31 632  	-- Update credit card
14:50:31 633  	IF in_instrument_name IS NOT NULL THEN
14:50:31 634  	  PROCS_FIN_INSTRUMENTS_CRU_V22.UPDATE_CREDIT_CARD(
14:50:31 635  	    in_credit_card_id  => in_credit_card_id,
14:50:31 636  	    in_updated_by      => in_updated_by,
14:50:31 637  	    in_instrument_name => in_instrument_name
14:50:31 638  	  );
14:50:31 639  	END IF;
14:50:31 640  
14:50:31 641  	-- Set default financial instrument
14:50:31 642  	IF in_is_default = GLOBAL_CONSTANTS_V22.TRUE THEN
14:50:31 643  	  BEGIN
14:50:31 644  	    PROCS_FIN_INSTRUMENTS_V22.SET_DEF_FINANCIAL_INSTRUMENT(
14:50:31 645  	      in_group_id	    => var_group_id,
