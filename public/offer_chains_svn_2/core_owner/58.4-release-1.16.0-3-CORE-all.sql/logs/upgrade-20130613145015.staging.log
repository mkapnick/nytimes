14:50:30 160  PROCEDURE GET_TRANSFER_SET
14:50:30 161  	  (
14:50:30 162  	      in_set_maximum NUMBER,
14:50:30 163  	      in_max_retries NUMBER,
14:50:30 164  	      out_refcursor OUT sys_refcursor)
14:50:30 165  IS
14:50:30 166  	  v_set_id core_owner.polling_sync.set_id%type;
14:50:30 167  BEGIN
14:50:30 168  	  /* Look for previously sent but unconfirmed sets and
14:50:30 169  	  send again until max_retries calls */
14:50:30 170  	  FOR x IN
14:50:30 171  	  (
14:50:30 172  	      SELECT
14:50:30 173  		  ps.set_id,
14:50:30 174  		  COUNT( *) set_size
14:50:30 175  	      FROM
14:50:30 176  		  core_owner.polling_sync ps
14:50:30 177  	      WHERE
14:50:30 178  		  ps.event_date > sysdate - 14
14:50:30 179  	      AND ps.confirm_date IS NULL
14:50:30 180  	      AND ps.last_send_date IS NOT NULL
14:50:30 181  	      AND ps.num_calls < in_max_retries
14:50:30 182  	      GROUP BY
14:50:30 183  		  ps.set_id
14:50:30 184  	      ORDER BY
14:50:30 185  		  ps.set_id
14:50:30 186  	  )
14:50:30 187  	  LOOP
14:50:30 188  	      out_refcursor := Retreive_Transfer_Set(x.set_id) ;
14:50:30 189  	      RETURN;
14:50:30 190  	  END LOOP;
14:50:30 191  	  v_set_id := Create_New_Transfer_Set(in_set_maximum) ;
14:50:30 192  	  out_refcursor := Retreive_Transfer_Set(v_set_id) ;
