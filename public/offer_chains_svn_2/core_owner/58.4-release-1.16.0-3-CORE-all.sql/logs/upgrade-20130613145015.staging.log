14:50:30 499  FUNCTION CALCULATE_OFFER_AMOUNT (
14:50:30 500  	in_offer_id IN NUMBER
14:50:30 501  ) RETURN NUMBER AS
14:50:30 502  -- VARIABLES
14:50:30 503  SPROC_NAME    CONSTANT VARCHAR2(22) := 'CALCULATE_OFFER_AMOUNT';
14:50:30 504  temp_offer_id NUMBER;
14:50:30 505  
14:50:30 506  var_product_offering_set	    SYS_REFCURSOR;
14:50:30 507  var_product_offering_id	    NUMBER;
14:50:30 508  var_product_offering_price    NUMBER(10,6);
14:50:30 509  var_product_offering_t_amount NUMBER(10,6);
14:50:30 510  var_product_offering_quantity NUMBER;
14:50:30 511  
14:50:30 512  var_total_amount NUMBER(10,6);
14:50:30 513  var_final_amount NUMBER(10,2);
14:50:30 514  
14:50:30 515  var_percent_discount NUMBER(10,2);
14:50:30 516  var_fixed_discount NUMBER(10,6);
14:50:30 517  
14:50:30 518  -- EXCEPTIONS
14:50:30 519  BAD_OFFER_ID EXCEPTION;
14:50:30 520  BEGIN
14:50:30 521  
14:50:30 522  	BEGIN
14:50:30 523  	  SELECT
14:50:30 524  	    OFFER.ID into temp_offer_id
14:50:30 525  	  FROM
14:50:30 526  	    OFFER
14:50:30 527  	  WHERE
14:50:30 528  	    OFFER.ID = in_offer_id;
14:50:30 529  	  EXCEPTION
14:50:30 530  	    WHEN NO_DATA_FOUND THEN
14:50:30 531  	      RAISE BAD_OFFER_ID;
14:50:30 532  	END;
14:50:30 533  
14:50:30 534  	OPEN var_product_offering_set FOR
14:50:30 535  	SELECT
14:50:30 536  	  OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID,
14:50:30 537  	  PRODUCT_OFFERING.UNIT_PRICE,
14:50:30 538  	  PRODUCT_OFFERING.QUANTITY
14:50:30 539  	FROM
14:50:30 540  	  OFFER_PRODUCT_OFFERING
14:50:30 541  	  INNER JOIN PRODUCT_OFFERING ON OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = PRODUCT_OFFERING.ID
14:50:30 542  	WHERE
14:50:30 543  	  OFFER_PRODUCT_OFFERING.OFFER_ID = in_offer_id;
14:50:30 544  
14:50:30 545  	var_total_amount := 0;
14:50:30 546  
14:50:30 547  	LOOP
14:50:30 548  	  FETCH var_product_offering_set into
14:50:30 549  	    var_product_offering_id,
14:50:30 550  	    var_product_offering_price,
14:50:30 551  	    var_product_offering_quantity;
14:50:30 552  	  EXIT WHEN var_product_offering_set%NOTFOUND;
14:50:30 553  
14:50:30 554  	  SELECT
14:50:30 555  	    SUM(DISCOUNT.FIXED_AMOUNT) into var_fixed_discount
14:50:30 556  	  FROM
14:50:30 557  	    DISCOUNT_PRODUCT_OFFERING
14:50:30 558  	    INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
14:50:30 559  	  WHERE
14:50:30 560  	    DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
14:50:30 561  	    AND DISCOUNT.FIXED_AMOUNT IS NOT NULL;
14:50:30 562  
14:50:30 563  	  SELECT
14:50:30 564  	    SUM(DISCOUNT.PERCENT_AMOUNT) into var_percent_discount
14:50:30 565  	  FROM
14:50:30 566  	    DISCOUNT_PRODUCT_OFFERING
14:50:30 567  	    INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
14:50:30 568  	  WHERE
14:50:30 569  	    DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
14:50:30 570  	    AND DISCOUNT.PERCENT_AMOUNT IS NOT NULL;
14:50:30 571  
14:50:30 572  	  var_product_offering_t_amount := var_product_offering_price * var_product_offering_quantity;
14:50:30 573  
14:50:30 574  	  IF var_percent_discount IS NOT NULL THEN
14:50:30 575  	    var_product_offering_t_amount := var_product_offering_t_amount - ( var_product_offering_t_amount * var_percent_discount / 100 );
14:50:30 576  	  END IF;
14:50:30 577  
14:50:30 578  	  IF var_fixed_discount IS NOT NULL THEN
14:50:30 579  	    var_product_offering_t_amount := var_product_offering_t_amount - var_fixed_discount;
14:50:30 580  	  END IF;
14:50:30 581  
14:50:30 582  	  var_total_amount := var_total_amount + var_product_offering_t_amount;
14:50:30 583  	END LOOP;
14:50:30 584  	var_final_amount := var_total_amount;
14:50:30 585  	RETURN var_final_amount;
14:50:30 586  
14:50:30 587  EXCEPTION
14:50:30 588  WHEN BAD_OFFER_ID THEN
14:50:30 589  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:30 590  	  SPROC_NAME, 'No such offer');
14:50:30 591  WHEN OTHERS THEN
14:50:30 592  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:30 593  	  SPROC_NAME, 'Unknown error', SQLERRM);
14:50:30 594  END CALCULATE_OFFER_AMOUNT;
