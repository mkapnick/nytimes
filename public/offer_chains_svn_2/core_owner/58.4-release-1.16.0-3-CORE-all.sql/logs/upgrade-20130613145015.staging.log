14:50:31 238  PROCEDURE GET_ACCOUNT_BY_INVOICE_ID (
14:50:31 239  /*
14:50:31 240  Throws exceptions:
14:50:31 241  APP_EXCEPTION_CODES_V22.NOT_FOUND
14:50:31 242  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
14:50:31 243  */
14:50:31 244  	in_invoice_id  IN  NUMBER,
14:50:31 245  	out_account_id OUT NUMBER
14:50:31 246  ) AS
14:50:31 247  -- VARIABLES
14:50:31 248  SPROC_NAME	 CONSTANT VARCHAR2(25) := 'GET_ACCOUNT_BY_INVOICE_ID';
14:50:31 249  temp_gc_account_id NUMBER;
14:50:31 250  temp_ss_account_id NUMBER;
14:50:31 251  temp_invoice_id	 NUMBER;
14:50:31 252  -- EXCEPTIONS
14:50:31 253  BAD_INVOICE_ID	   EXCEPTION;
14:50:31 254  CAN_NOT_FIND_ACCOUNT EXCEPTION;
14:50:31 255  BEGIN
14:50:31 256  
14:50:31 257  	-- Check that given invoice exists
14:50:31 258  	BEGIN
14:50:31 259  	  SELECT
14:50:31 260  	    INVOICE.ID into temp_invoice_id
14:50:31 261  	  FROM
14:50:31 262  	    INVOICE
14:50:31 263  	  WHERE
14:50:31 264  	    INVOICE.ID = in_invoice_id;
14:50:31 265  	  EXCEPTION
14:50:31 266  	    WHEN NO_DATA_FOUND THEN
14:50:31 267  	      RAISE BAD_INVOICE_ID;
14:50:31 268  	END;
14:50:31 269  
14:50:31 270  	-- Try to find gift certificate with given invoice
14:50:31 271  	BEGIN
14:50:31 272  	  SELECT
14:50:31 273  	    ACCOUNT.GROUP_ID into temp_gc_account_id
14:50:31 274  	  FROM
14:50:31 275  	    GIFT_CERTIFICATE
14:50:31 276  	    INNER JOIN ACCOUNT ON GIFT_CERTIFICATE.PURCHASER_GROUP_ID = ACCOUNT.GROUP_ID
14:50:31 277  	  WHERE
14:50:31 278  	    GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = in_invoice_id
14:50:31 279  	    AND ROWNUM <= 1;
14:50:31 280  	  EXCEPTION
14:50:31 281  	    WHEN NO_DATA_FOUND THEN
14:50:31 282  	      temp_gc_account_id := NULL;
14:50:31 283  	END;
14:50:31 284  
14:50:31 285  	-- check subscriptions for given invoice
14:50:31 286  	IF temp_gc_account_id IS NOT NULL THEN
14:50:31 287  	  out_account_id := temp_gc_account_id;
14:50:31 288  	ELSE
14:50:31 289  	  BEGIN
14:50:31 290  	    SELECT
14:50:31 291  	      ACCOUNT.GROUP_ID into temp_ss_account_id
14:50:31 292  	    FROM
14:50:31 293  	      LICENSE
14:50:31 294  	      INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
14:50:31 295  	      INNER JOIN ACCOUNT ON SUBSCRIPTION.ACCOUNT_ID = ACCOUNT.ID
14:50:31 296  	    WHERE
14:50:31 297  	      LICENSE.INVOICE_ID = in_invoice_id
14:50:31 298  	      AND ROWNUM <= 1;
14:50:31 299  	    EXCEPTION
14:50:31 300  	      WHEN NO_DATA_FOUND THEN
14:50:31 301  		temp_ss_account_id := NULL;
14:50:31 302  	  END;
14:50:31 303  	  IF temp_ss_account_id IS NULL THEN
14:50:31 304  	    RAISE CAN_NOT_FIND_ACCOUNT;
14:50:31 305  	  ELSE
14:50:31 306  	    out_account_id := temp_ss_account_id;
14:50:31 307  	  END IF;
14:50:31 308  	END IF;
14:50:31 309  
14:50:31 310  EXCEPTION
14:50:31 311  WHEN BAD_INVOICE_ID THEN
14:50:31 312  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 313  	  SPROC_NAME, 'No such invoice');
14:50:31 314  WHEN CAN_NOT_FIND_ACCOUNT THEN
14:50:31 315  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 316  	  SPROC_NAME, 'Could not find account by given invoice id');
14:50:31 317  WHEN OTHERS THEN
14:50:31 318  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:31 319  	  SPROC_NAME, 'Unknown error', SQLERRM);
14:50:31 320  END GET_ACCOUNT_BY_INVOICE_ID;
