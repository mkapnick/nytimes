14:50:30 994  
14:50:30 995  	SELECT SUM(T.CALCULATED_AMOUNT) into var_intrl_tax_amount
14:50:30 996  	FROM
14:50:30 997  	  LINE_ITEM LI
14:50:30 998  	  INNER JOIN INVOICE I ON LI.INVOICE_ID = I.ID
14:50:30 999  	  INNER JOIN CHARGE CH ON CH.INVOICE_ID=  I.ID
14:50:30 1000  	   INNER JOIN TAX T ON T.LINE_ITEM_ID = LI.ID
14:50:30 1001  	 WHERE
14:50:30 1002  	   CH.TRANSACTION_ID = in_transaction_id
14:50:30 1003  	   AND T.TAX_TYPE_ID IN (
14:50:30 1004  	     SELECT GLOBAL_ENUMS_V22.TAX_TYPE_VAT FROM DUAL
14:50:30 1005  	   );
14:50:30 1006  
14:50:30 1007  	 IF var_intrl_tax_amount IS NULL THEN
14:50:30 1008  	   var_intrl_tax_amount := 0;
14:50:30 1009  	 END IF;
14:50:30 1010  
14:50:30 1011  	 RETURN var_intrl_tax_amount;
14:50:30 1012  
14:50:30 1013  END GET_TRANSACTION_INTRL_TAXES;
