14:50:30  48  	      RAISE BAD_INVOICE_ID;
14:50:30  49  	END;
14:50:30  50  
14:50:30  51  	-- Check that given offer exists
14:50:30  52  	BEGIN
14:50:30  53  	  SELECT
14:50:30  54  	    OFFER.ID into temp_offer_id
14:50:30  55  	  FROM
14:50:30  56  	    OFFER
14:50:30  57  	  WHERE
14:50:30  58  	    OFFER.ID = in_offer_id;
14:50:30  59  
14:50:30  60  	  EXCEPTION
14:50:30  61  	    WHEN NO_DATA_FOUND THEN
14:50:30  62  	      RAISE BAD_OFFER_ID;
14:50:30  63  	END;
14:50:30  64  
14:50:30  65  	-- Get product_offering data
14:50:30  66  	OPEN var_line_item_data FOR
14:50:30  67  	SELECT
14:50:30  68  	  PRODUCT_OFFERING.ID,
14:50:30  69  	  PRODUCT_OFFERING.UNIT_PRICE,
