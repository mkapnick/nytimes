14:50:31 682  PROCEDURE FREEZE_ACCOUNT (
14:50:31 683  	in_group_id   IN NUMBER,
14:50:31 684  	in_updated_by IN VARCHAR2,
14:50:31 685  	in_note       IN VARCHAR2,
14:50:31 686  	in_agent_id   IN NUMBER
14:50:31 687  ) AS
14:50:31 688  SPROC_NAME	    CONSTANT VARCHAR2(14) := 'FREEZE_ACCOUNT';
14:50:31 689  -- VARIABLES
14:50:31 690  var_account_id	    NUMBER;
14:50:31 691  var_account_status_id NUMBER;
14:50:31 692  -- EXCEPTIONS
14:50:31 693  BAD_GROUP_ID	    EXCEPTION;
14:50:31 694  BAD_ACCOUNT_STATUS_ID EXCEPTION;
14:50:31 695  CAN_NOT_CREATE_NOTE   EXCEPTION;
14:50:31 696  EXCEPTION_MESSAGE     VARCHAR2(1024);
14:50:31 697  BEGIN
14:50:31 698  
14:50:31 699  	-- Get account status, account id
14:50:31 700  	BEGIN
14:50:31 701  	  SELECT
14:50:31 702  	    ACCOUNT.ID,
14:50:31 703  	    ACCOUNT.ACCOUNT_STATUS_ID
14:50:31 704  	    into
14:50:31 705  	    var_account_id,
14:50:31 706  	    var_account_status_id
14:50:31 707  	  FROM
14:50:31 708  	    ACCOUNT
14:50:31 709  	  WHERE
14:50:31 710  	    ACCOUNT.GROUP_ID = in_group_id;
14:50:31 711  	  EXCEPTION
14:50:31 712  	    WHEN NO_DATA_FOUND THEN
14:50:31 713  	      RAISE BAD_GROUP_ID;
14:50:31 714  	END;
14:50:31 715  
14:50:31 716  	-- We can freeze only ACTIVE accounts
14:50:31 717  	IF var_account_status_id != GLOBAL_STATUSES_V22.ACCOUNT_ACTIVE
14:50:31 718  	  AND var_account_status_id != GLOBAL_STATUSES_V22.ACCOUNT_FROZEN THEN
14:50:31 719  	  RAISE BAD_ACCOUNT_STATUS_ID;
14:50:31 720  	END IF;
14:50:31 721  
14:50:31 722  	-- Set account status
14:50:31 723  	PROCS_ACCOUNT_V22.UPDATE_ACCOUNT_STATUS(
14:50:31 724  	  in_account_id        => var_account_id,
14:50:31 725  	  in_updated_by        => in_updated_by,
14:50:31 726  	  in_account_status_id => GLOBAL_STATUSES_V22.ACCOUNT_FROZEN
14:50:31 727  	);
14:50:31 728  
14:50:31 729  	-- Annotate account
14:50:31 730  	BEGIN
14:50:31 731  	  PROCS_ACCOUNT_V22.ANNOTATE_ACCOUNT(
14:50:31 732  	    in_group_id   => in_group_id,
14:50:31 733  	    in_agent_id   => in_agent_id,
14:50:31 734  	    in_note	  => in_note,
14:50:31 735  	    in_created_by => in_updated_by
14:50:31 736  	  );
14:50:31 737  	  EXCEPTION
14:50:31 738  	    WHEN OTHERS THEN
14:50:31 739  	      EXCEPTION_MESSAGE := SQLERRM;
