14:50:31 1935  -- VARIABLES
14:50:31 1936  temp_invoice_id number;
14:50:31 1937  cnt_matched_instr number := 0;
14:50:31 1938  -- EXCEPTIONS
14:50:31 1939  BAD_INVOICE_ID EXCEPTION;
14:50:31 1940  BEGIN
14:50:31 1941  
14:50:31 1942  	 BEGIN
14:50:31 1943  	   SELECT
14:50:31 1944  	     i.id into temp_invoice_id
14:50:31 1945  	   from
14:50:31 1946  	     invoice i
14:50:31 1947  	   where
14:50:31 1948  	     i.id = in_invoice_id;
14:50:31 1949  	   EXCEPTION
14:50:31 1950  	     WHEN NO_DATA_FOUND THEN
14:50:31 1951  	       RAISE BAD_INVOICE_ID;
14:50:31 1952  	 END;
14:50:31 1953  
14:50:31 1954  	 select
14:50:31 1955  	   count(1) into cnt_matched_instr
14:50:31 1956  	 from
14:50:31 1957  	   charge ch
14:50:31 1958  	 inner join
14:50:31 1959  	   subscription s
14:50:31 1960  	 on
14:50:31 1961  	   s.instrument_id = ch.instrument_id
14:50:31 1962  	 where
14:50:31 1963  	   ch.invoice_id = in_invoice_id;
14:50:31 1964  
14:50:31 1965  	 if cnt_matched_instr = 0 then
14:50:31 1966  	   out_external_transaction_id := null;
