14:50:31 936  
14:50:31 937  FUNCTION GET_RENEWAL_DATE (
14:50:31 938  /*
14:50:31 939  Throws exceptions:
14:50:31 940  APP_EXCEPTION_CODES_V22.NOT_FOUND
14:50:31 941  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
14:50:31 942  */
14:50:31 943  	in_subscription_id in NUMBER
14:50:31 944  ) RETURN DATE AS
14:50:31 945  -- VARIABLES
14:50:31 946  SPROC_NAME	      CONSTANT VARCHAR2(16) := 'GET_RENEWAL_DATE';
14:50:31 947  var_subscription_status NUMBER;
14:50:31 948  var_licenses_count      NUMBER;
14:50:31 949  var_license_end_date    DATE;
14:50:31 950  var_last_offer_id       NUMBER;
14:50:31 951  var_offer_chain_id      NUMBER;
14:50:31 952  var_last_license_id     NUMBER;
14:50:31 953  var_current_offer_index NUMBER;
14:50:31 954  var_current_offer_recurr_num NUMBER;
14:50:31 955  var_offer_recurr_num    NUMBER;
14:50:31 956  -- EXCEPTIONS
14:50:31 957  BAD_SUBSCRIPTION_ID EXCEPTION;
14:50:31 958  NO_LICENSES_FOUND EXCEPTION;
14:50:31 959  BEGIN
14:50:31 960  
14:50:31 961  	-- Get subscription id and offer chain id
14:50:31 962  	BEGIN
14:50:31 963  	  SELECT
14:50:31 964  	    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID,
14:50:31 965  	    SUBSCRIPTION.OFFER_CHAIN_ID
14:50:31 966  	    into
14:50:31 967  	    var_subscription_status,
14:50:31 968  	    var_offer_chain_id
14:50:31 969  	  FROM
14:50:31 970  	    SUBSCRIPTION
14:50:31 971  	  WHERE
14:50:31 972  	    SUBSCRIPTION.ID = in_subscription_id;
14:50:31 973  	  EXCEPTION
14:50:31 974  	    WHEN NO_DATA_FOUND THEN
14:50:31 975  	      RAISE BAD_SUBSCRIPTION_ID;
14:50:31 976  	END;
14:50:31 977  
14:50:31 978  	IF var_subscription_status != GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE THEN
14:50:31 979  	  -- TODO: Is suspended subscription has renewal date? (For the phase II)
14:50:31 980  	  -- AND var_subscription_status != GLOBAL_STATUSES_V22.SUBSCRIPTION_SUSPENDED THEN
14:50:31 981  	  RETURN NULL;
14:50:31 982  	END IF;
14:50:31 983  
14:50:31 984  	BEGIN
14:50:31 985  	  SELECT
14:50:31 986  	    LICENSE_ID,
14:50:31 987  	    END_DATE,
14:50:31 988  	    OFFER_ID,
14:50:31 989  	    CURRENT_OFFER_INDEX,
14:50:31 990  	    CURRENT_OFFER_RECURR_NUM
14:50:31 991  	    into
14:50:31 992  	    var_last_license_id,
14:50:31 993  	    var_license_end_date,
14:50:31 994  	    var_last_offer_id,
14:50:31 995  	    var_current_offer_index,
14:50:31 996  	    var_current_offer_recurr_num
14:50:31 997  	  FROM
14:50:31 998  	    (
14:50:31 999  	      SELECT
14:50:31 1000  		 LICENSE.ID as "LICENSE_ID",
14:50:31 1001  		 LICENSE.END_DATE,
14:50:31 1002  		 LICENSE.OFFER_ID,
14:50:31 1003  		 LICENSE.CURRENT_OFFER_INDEX,
14:50:31 1004  		 LICENSE.CURRENT_OFFER_RECURR_NUM
14:50:31 1005  	       FROM
14:50:31 1006  		 LICENSE
14:50:31 1007  	       WHERE
14:50:31 1008  		 LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V22.LICENSE_ACTIVE
14:50:31 1009  		 AND LICENSE.SUBSCRIPTION_ID = in_subscription_id
14:50:31 1010  	       ORDER BY END_DATE DESC
14:50:31 1011  	     )
14:50:31 1012  	     INNER JOIN OFFER ON OFFER_ID = OFFER.ID
14:50:31 1013  	   WHERE
14:50:31 1014  	     ROWNUM <= 1;
14:50:31 1015  
14:50:31 1016  	   EXCEPTION
14:50:31 1017  	     WHEN NO_DATA_FOUND THEN
14:50:31 1018  	       -- RAISE NO_LICENSES_FOUND;
14:50:31 1019  	       RETURN NULL;
14:50:31 1020  	 END;
14:50:31 1021  
14:50:31 1022  	 SELECT
14:50:31 1023  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES into var_offer_recurr_num
14:50:31 1024  	 FROM
14:50:31 1025  	   OFFER_OFFER_CHAIN
14:50:31 1026  	 WHERE
14:50:31 1027  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
14:50:31 1028  	   AND OFFER_OFFER_CHAIN.OFFER_ID = var_last_offer_id;
14:50:31 1029  
14:50:31 1030  	 IF PROCS_OFFER_CHAIN_V22.GET_NEXT_OFFER_INDEX(var_offer_chain_id, var_current_offer_index) IS NULL
14:50:31 1031  	   AND var_offer_recurr_num = var_current_offer_recurr_num THEN
14:50:31 1032  	   -- There is no next offer for this subscription
14:50:31 1033  	   RETURN NULL;
14:50:31 1034  	 END IF;
14:50:31 1035  
14:50:31 1036  	 RETURN var_license_end_date;
14:50:31 1037  
14:50:31 1038  EXCEPTION
14:50:31 1039  WHEN BAD_SUBSCRIPTION_ID THEN
14:50:31 1040  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 1041  	   SPROC_NAME, 'No such subscription');
14:50:31 1042  WHEN NO_LICENSES_FOUND THEN
14:50:31 1043  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 1044  	   SPROC_NAME, 'No licenses found');
14:50:31 1045  WHEN OTHERS THEN
14:50:31 1046  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:31 1047  	   SPROC_NAME, 'Unknown error', SQLERRM);
14:50:31 1048  END GET_RENEWAL_DATE;
14:50:31 1049  
