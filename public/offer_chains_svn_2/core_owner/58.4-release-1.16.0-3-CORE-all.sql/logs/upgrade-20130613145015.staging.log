14:50:31 517  
14:50:31 518  PROCEDURE SUSPEND_SUBSCRIPTION(
14:50:31 519  /*
14:50:31 520  Throws exceptions:
14:50:31 521  APP_EXCEPTION_CODES_V22.NOT_FOUND
14:50:31 522  APP_EXCEPTION_CODES_V22.INTERNAL_ERROR
14:50:31 523  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
14:50:31 524  */
14:50:31 525  	  in_subs_id	IN NUMBER,
14:50:31 526  	  in_updated_by IN VARCHAR2
14:50:31 527  ) AS
14:50:31 528  SPROC_NAME		  CONSTANT VARCHAR2(20) := 'SUSPEND_SUBSCRIPTION';
14:50:31 529  var_subscription_status_id  NUMBER;
14:50:31 530  var_license_id		  NUMBER;
14:50:31 531  var_offer_id		  NUMBER;
14:50:31 532  var_license_start_date	  DATE;
14:50:31 533  var_license_end_date	  DATE;
14:50:31 534  
14:50:31 535  var_entitlement_dupration   VARCHAR2(30);
14:50:31 536  var_d_entitlement_dupration NUMBER;
14:50:31 537  
14:50:31 538  var_ym_interval INTERVAL YEAR TO MONTH;
14:50:31 539  var_ds_interval INTERVAL DAY(3) TO SECOND;
14:50:31 540  
14:50:31 541  -- EXCEPTIONS
14:50:31 542  BAD_SUBSCRIPTION_ID     EXCEPTION;
14:50:31 543  BAD_SUBSCRIPTION_STATUS EXCEPTION;
14:50:31 544  NO_LICENSE_FOUND	      EXCEPTION;
14:50:31 545  NO_OFFER_FOUND	      EXCEPTION;
14:50:31 546  EXCEPTION_MESSAGE       VARCHAR2(1024);
14:50:31 547  BEGIN
14:50:31 548  	-- TODO: Finish this prcedure (in Phase II)
14:50:31 549  
14:50:31 550  	-- Get subscription by id. FAULT if no such subscription.
14:50:31 551  	-- begin TX
14:50:31 552  	--   Get for update associated license (subscription.license_id). FAULT if not found.
14:50:31 553  	--   Set status to PROCESSED.
14:50:31 554  	--   updated record.
14:50:31 555  	--   compute days remaining in the subscription: original end_date - today = days_remaining_adjustment
14:50:31 556  	--   new subscription status is SUSPENDED.
14:50:31 557  	--   suspend_date is now.
14:50:31 558  	--   update subscription record.
14:50:31 559  	-- end TX
14:50:31 560  
14:50:31 561  	BEGIN
14:50:31 562  	  SELECT
14:50:31 563  	    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID into var_subscription_status_id
14:50:31 564  	  FROM
14:50:31 565  	    SUBSCRIPTION
14:50:31 566  	  WHERE
14:50:31 567  	    SUBSCRIPTION.ID = in_subs_id;
14:50:31 568  	  EXCEPTION
14:50:31 569  	  WHEN NO_DATA_FOUND THEN
14:50:31 570  	    RAISE BAD_SUBSCRIPTION_ID;
14:50:31 571  	END;
14:50:31 572  
14:50:31 573  	IF var_subscription_status_id != GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE THEN
14:50:31 574  	  RAISE BAD_SUBSCRIPTION_STATUS;
14:50:31 575  	END IF;
14:50:31 576  
14:50:31 577  	BEGIN
14:50:31 578  	  SELECT
14:50:31 579  	    LICENSE.ID,
14:50:31 580  	    LICENSE.OFFER_ID,
14:50:31 581  	    LICENSE.START_DATE
14:50:31 582  	    into
14:50:31 583  	    var_license_id,
14:50:31 584  	    var_offer_id,
14:50:31 585  	    var_license_start_date
14:50:31 586  	  FROM
14:50:31 587  	    LICENSE
14:50:31 588  	  WHERE
14:50:31 589  	    LICENSE.SUBSCRIPTION_ID = in_subs_id
14:50:31 590  	      AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V22.LICENSE_ACTIVE;
14:50:31 591  	  EXCEPTION
14:50:31 592  	  WHEN NO_DATA_FOUND THEN
14:50:31 593  	    RAISE NO_LICENSE_FOUND;
14:50:31 594  	END;
14:50:31 595  
14:50:31 596  	PROCS_LICENSE_CRU_V22.UPDATE_LICENSE(
14:50:31 597  	  in_license_id        => var_license_id,
14:50:31 598  	  in_updated_by        => in_updated_by,
14:50:31 599  	  in_license_status_id => GLOBAL_STATUSES_V22.LICENSE_CLOSED
14:50:31 600  	);
14:50:31 601  
14:50:31 602  	BEGIN
14:50:31 603  	  SELECT
14:50:31 604  	    OFFER.ENTITLEMENT_DURATION into var_entitlement_dupration
14:50:31 605  	  FROM
14:50:31 606  	    OFFER
14:50:31 607  	  WHERE
14:50:31 608  	    OFFER.ID = var_offer_id;
14:50:31 609  	  EXCEPTION
14:50:31 610  	  WHEN NO_DATA_FOUND THEN
14:50:31 611  	    RAISE NO_OFFER_FOUND;
14:50:31 612  	END;
14:50:31 613  
14:50:31 614  	var_ym_interval := substr(var_entitlement_dupration, 0, 4);
14:50:31 615  	var_ds_interval := substr(var_entitlement_dupration, 4);
14:50:31 616  
14:50:31 617  	var_license_end_date := var_license_start_date + var_ym_interval + var_ds_interval;
14:50:31 618  
14:50:31 619  	var_d_entitlement_dupration := var_license_end_date - current_date;
14:50:31 620  
14:50:31 621  	PROCS_SUBSCRIPTION_CRU_V22.UPDATE_SUBSCRIPTION(
14:50:31 622  	  in_subscription_id	       => in_subs_id,
14:50:31 623  	  in_updated_by 	       => in_updated_by,
14:50:31 624  	  in_suspend_date	       => SYSDATE,
14:50:31 625  	  in_subscription_status_id    => GLOBAL_STATUSES_V22.SUBSCRIPTION_SUSPENDED,
14:50:31 626  	  in_days_remainning_ajustment => var_d_entitlement_dupration
14:50:31 627  	);
14:50:31 628  
14:50:31 629  EXCEPTION
14:50:31 630  WHEN BAD_SUBSCRIPTION_ID THEN
14:50:31 631  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 632  	  SPROC_NAME, 'No such subscription');
14:50:31 633  WHEN BAD_SUBSCRIPTION_STATUS THEN
14:50:31 634  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.STATE_ERROR,
14:50:31 635  	  SPROC_NAME, 'Subscription is not active');
14:50:31 636  WHEN NO_LICENSE_FOUND THEN
14:50:31 637  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 638  	  SPROC_NAME, 'Can not find license associated with given subscription ID');
14:50:31 639  WHEN NO_OFFER_FOUND THEN
14:50:31 640  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 641  	  SPROC_NAME, 'Can not find offer associated with given subscription ID');
14:50:31 642  WHEN OTHERS THEN
14:50:31 643  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:31 644  	  SPROC_NAME, 'Unknown error', SQLERRM);
14:50:31 645  END SUSPEND_SUBSCRIPTION;
