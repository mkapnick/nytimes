14:50:30 414  FUNCTION CALCULATE_OFFER_CHAIN_END_DATE (
14:50:30 415  	in_offer_chain_id	  IN NUMBER,
14:50:30 416  	in_offer_chain_start_date IN DATE
14:50:30 417  ) RETURN DATE AS
14:50:30 418  -- VARIABLES
14:50:30 419  SPROC_NAME	     CONSTANT VARCHAR2(30) := 'CALCULATE_OFFER_CHAIN_END_DATE';
14:50:30 420  temp_offer_chain_id    NUMBER;
14:50:30 421  var_offer_chain_length NUMBER;
14:50:30 422  var_offer_duration     VARCHAR2(30);
14:50:30 423  var_offer_recurrences  NUMBER;
14:50:30 424  var_end_date	     DATE;
14:50:30 425  
14:50:30 426  var_offer_ym_interval INTERVAL YEAR TO MONTH;
14:50:30 427  var_offer_ds_interval INTERVAL DAY(3) TO SECOND;
14:50:30 428  var_offer_years	    NUMBER;
14:50:30 429  var_offer_months	    NUMBER;
14:50:30 430  var_offer_days	    NUMBER;
14:50:30 431  var_infinity_offers_count NUMBER;
14:50:30 432  
14:50:30 433  var_offers_set SYS_REFCURSOR;
14:50:30 434  
14:50:30 435  -- EXCEPTIONS
14:50:30 436  BAD_OFFER_CHAIN_ID EXCEPTION;
14:50:30 437  BEGIN
14:50:30 438  
14:50:30 439  	var_end_date := in_offer_chain_start_date;
14:50:30 440  
14:50:30 441  	-- Check that offer chain exists
14:50:30 442  	BEGIN
14:50:30 443  	  SELECT
14:50:30 444  	    OFFER_CHAIN.ID into temp_offer_chain_id
14:50:30 445  	  FROM
14:50:30 446  	    OFFER_CHAIN
14:50:30 447  	  WHERE
14:50:30 448  	    OFFER_CHAIN.ID = in_offer_chain_id;
14:50:30 449  	  EXCEPTION
14:50:30 450  	    WHEN NO_DATA_FOUND
14:50:30 451  	      THEN RAISE BAD_OFFER_CHAIN_ID;
14:50:30 452  	END;
14:50:30 453  
14:50:30 454  	SELECT
14:50:30 455  	  COUNT(*) into var_infinity_offers_count
14:50:30 456  	FROM
14:50:30 457  	  OFFER_OFFER_CHAIN
14:50:30 458  	WHERE
14:50:30 459  	  OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
14:50:30 460  	  AND OFFER_OFFER_CHAIN.NUM_RECURRENCES = GLOBAL_ENUMS_V22.OFFER_REC_INFINITY;
14:50:30 461  
14:50:30 462  	IF var_infinity_offers_count > 0 THEN
14:50:30 463  	  -- Offer chain contains offers with infinity num of recurrences
14:50:30 464  	  RETURN NULL;
14:50:30 465  	END IF;
14:50:30 466  
14:50:30 467  	-- Select offers durations
14:50:30 468  	OPEN var_offers_set FOR
14:50:30 469  	SELECT
14:50:30 470  	  OFFER.ENTITLEMENT_DURATION,
14:50:30 471  	  OFFER_OFFER_CHAIN.NUM_RECURRENCES
14:50:30 472  	FROM
14:50:30 473  	  OFFER_OFFER_CHAIN
14:50:30 474  	  INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
14:50:30 475  	WHERE
14:50:30 476  	  OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id;
14:50:30 477  
14:50:30 478  	-- Calculate sum of offers durations
14:50:30 479  	LOOP
14:50:30 480  	  FETCH var_offers_set INTO var_offer_duration, var_offer_recurrences;
14:50:30 481  	  EXIT WHEN var_offers_set%NOTFOUND;
14:50:30 482  	  PROCS_COMMON_V22.ISO8601DURATION_TO_INTERVALS(var_offer_duration, var_offer_years, var_offer_months, var_offer_days);
14:50:30 483  	  var_offer_ym_interval := var_offer_years||'-'||var_offer_months;
14:50:30 484  	  var_offer_ds_interval := var_offer_days||' 0:0:0';
14:50:30 485  	  var_end_date := var_end_date + ( var_offer_ym_interval * ( var_offer_recurrences + 1) ) + ( var_offer_ds_interval * ( var_offer_recurrences + 1) );
14:50:30 486  	END LOOP;
14:50:30 487  
14:50:30 488  	RETURN var_end_date;
14:50:30 489  
14:50:30 490  EXCEPTION
14:50:30 491  WHEN BAD_OFFER_CHAIN_ID THEN
14:50:30 492  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:30 493  	  SPROC_NAME, 'No such offer chain');
14:50:30 494  WHEN OTHERS THEN
14:50:30 495  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:30 496  	  SPROC_NAME, 'Unknown error', SQLERRM);
14:50:30 497  END CALCULATE_OFFER_CHAIN_END_DATE;
