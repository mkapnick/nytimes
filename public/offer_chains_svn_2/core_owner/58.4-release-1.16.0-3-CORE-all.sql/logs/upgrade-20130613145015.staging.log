14:50:30  88  	PROCEDURE CHASE_PROFILE_BY_REQ_FILE_ID(
14:50:30  89  	  in_request_file_id IN CC_UPDATE.CC_REQUEST_FILE_ID%TYPE,
14:50:30  90  	  in_start	     IN NUMBER,
14:50:30  91  	  in_end	     IN NUMBER,
14:50:30  92  	  out_result_set     OUT SYS_REFCURSOR
14:50:30  93  	) AS
14:50:30  94  	SPROC_NAME CONSTANT VARCHAR2(32) := 'CHASE_PROFILE_BY_REQ_FILE_ID';
14:50:30  95  	var_range_diff	    NUMBER := 0;
14:50:30  96  	var_upper_bond_diff NUMBER := 0;
14:50:30  97  	var_l_start	    NUMBER := 0;
14:50:30  98  	var_l_end	    NUMBER := 0;
14:50:30  99  	BEGIN
14:50:30 100  	  --Normalize the end points [START]
14:50:30 101  	  IF (in_start IS NULL OR in_start < 0) Then
14:50:30 102  	    var_l_start := 0;
14:50:30 103  	  ELSE
14:50:30 104  	    var_l_start := in_start;
14:50:30 105  	  END IF;
14:50:30 106  
14:50:30 107  	  IF (in_end IS NULL) Then
14:50:30 108  	    var_l_end := 500;
14:50:30 109  	  ELSE
14:50:30 110  	    var_l_end := in_end;
14:50:30 111  	  END IF;
14:50:30 112  
14:50:30 113  	  var_l_start := var_l_start + 1;
14:50:30 114  	  var_l_end   := var_l_end   + 1;
14:50:30 115  
14:50:30 116  	  var_range_diff  := var_l_end - var_l_start;
14:50:30 117  	  var_upper_bond_diff :=  var_range_diff - 1000;
14:50:30 118  
14:50:30 119  	  IF (var_upper_bond_diff > 0) Then
14:50:30 120  	    var_l_end := var_l_end - var_upper_bond_diff;
14:50:30 121  	  END IF;
14:50:30 122  	  --Normalize the end points [END]
14:50:30 123  
14:50:30 124  	  OPEN out_result_set FOR
14:50:30 125  	    SELECT CHASE_PROFILE_ID FROM
14:50:30 126  	      (SELECT rownum rnum, q.* FROM
14:50:30 127  		 (SELECT
14:50:30 128  		    cc.CHASE_PROFILE_ID
14:50:30 129  		  FROM
14:50:30 130  		    CREDIT_CARD cc,
14:50:30 131  		    CC_UPDATE ccu
14:50:30 132  		  WHERE
14:50:30 133  		    ccu.CC_REQUEST_FILE_ID = in_request_file_id
14:50:30 134  		    AND ccu.CREDIT_CARD_ID = cc.id
14:50:30 135  		) Q
14:50:30 136  	      WHERE rownum <= var_l_end)
14:50:30 137  	    WHERE rnum >= var_l_Start;
14:50:30 138  	END CHASE_PROFILE_BY_REQ_FILE_ID;
