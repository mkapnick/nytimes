14:50:31 659  
14:50:31 660  PROCEDURE GET_SUBSCRIPTION_INFO (
14:50:31 661  /*
14:50:31 662  Throws exceptions:
14:50:31 663  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
14:50:31 664  */
14:50:31 665  	  in_subscription_id  IN  NUMBER,
14:50:31 666  	  out_result_set      OUT SYS_REFCURSOR
14:50:31 667  ) AS
14:50:31 668  SPROC_NAME CONSTANT VARCHAR2(21) := 'GET_SUBSCRIPTION_INFO';
14:50:31 669  -- VARIABLES
14:50:31 670  temp_subscription_id NUMBER;
14:50:31 671  -- EXCEPTIONS
14:50:31 672  BAD_SUBSCRIPTION_ID EXCEPTION;
14:50:31 673  BEGIN
14:50:31 674  	-- Find subscription by id
14:50:31 675  	-- Return its details
14:50:31 676  
14:50:31 677  	BEGIN
14:50:31 678  	  SELECT
14:50:31 679  	    SUBSCRIPTION.ID into temp_subscription_id
14:50:31 680  	  FROM
14:50:31 681  	    SUBSCRIPTION
14:50:31 682  	  WHERE
14:50:31 683  	    SUBSCRIPTION.ID = in_subscription_id;
14:50:31 684  	  EXCEPTION
14:50:31 685  	    WHEN NO_DATA_FOUND THEN
14:50:31 686  	      RAISE BAD_SUBSCRIPTION_ID;
14:50:31 687  	END;
14:50:31 688  
14:50:31 689  	OPEN out_result_set FOR
14:50:31 690  	SELECT
14:50:31 691  	  SUBSCRIPTION.ID AS "SUBSCRIPTION_ID",
14:50:31 692  	  SUBSCRIPTION.SUBSCRIPTION_STATUS_ID,
14:50:31 693  	  SUBSCRIPTION.PURCHASE_DATE,
14:50:31 694  	  SUBSCRIPTION.SUSPEND_DATE,
14:50:31 695  	  SUBSCRIPTION.REACTIVATION_DATE,
14:50:31 696  	  SUBSCRIPTION.CANCELLATION_DATE,
14:50:31 697  	  SUBSCRIPTION_CANCEL_REASON.VALUE as "CANCEL_TYPE",
14:50:31 698  	  OFFER_CHAIN.ID AS "OFFER_CHAIN_ID",
14:50:31 699  	  OFFER_CHAIN.NAME,
14:50:31 700  	  OFFER_CHAIN.DESCRIPTION,
14:50:31 701  	  OFFER_CHAIN.PRODUCT_URI,
14:50:31 702  	  SUBSCRIPTION.INSTRUMENT_ID,
14:50:31 703  	  SUBSCRIPTION.INSTRUMENT_TYPE_ID,
14:50:31 704  	  PROCS_SUBSCRIPTION_V22.CALC_SUBSCRIPTION_END_DATE(SUBSCRIPTION.ID) as "END_DATE",
14:50:31 705  	  PROCS_SUBSCRIPTION_V22.GET_RECENT_CHARGE(SUBSCRIPTION.ID) AS "RECENT_CHARGE",
14:50:31 706  	  PROCS_SUBSCRIPTION_V22.GET_RENEWAL_DATE(SUBSCRIPTION.ID) AS "RENEWAL_DATE",
14:50:31 707  	  PROCS_SUBSCRIPTION_V22.GET_BILLING_CYCLE(SUBSCRIPTION.ID) AS "BILLING_CYCLE",
14:50:31 708  	  (
14:50:31 709  	    SELECT
14:50:31 710  	      ACCOUNT.GROUP_ID
14:50:31 711  	      FROM ACCOUNT
14:50:31 712  	      WHERE ACCOUNT.ID = SUBSCRIPTION.ACCOUNT_ID
14:50:31 713  	  ) as "GROUP_ID",
14:50:31 714  	  (
14:50:31 715  	    SELECT
14:50:31 716  	      MAX(ENTITLEMENT_END_DATE)
14:50:31 717  	      FROM LICENSE
14:50:31 718  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
14:50:31 719  	  )
14:50:31 720  	  as "ENT_END_DATE",
14:50:31 721  	  (
14:50:31 722  	    SELECT
14:50:31 723  	      MIN(START_DATE)
14:50:31 724  	      FROM LICENSE
14:50:31 725  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
14:50:31 726  	  )
14:50:31 727  	  as "ENT_START_DATE",
14:50:31 728  	  PROCS_SUBSCRIPTION_V22.IS_SUBSCRIPTION_CANCELABLE(SUBSCRIPTION.ID) AS "IS_CANCELABLE",
14:50:31 729  	  ITUNES_RECEIPT.ID AS "ITUNES_RECEIPT_ID",
14:50:31 730  	  GROUP_ACCOUNT.ID GA_ID,
14:50:31 731  	  GROUP_ACCOUNT.SUBSCRIPTION_ID GA_SUBSCRIPTION_ID,
14:50:31 732  	  GROUP_ACCOUNT.GROUP_NAME GA_GROUP_NAME,
14:50:31 733  	  GROUP_ACCOUNT.FIRST_NAME GA_FIRST_NAME,
14:50:31 734  	  GROUP_ACCOUNT.LAST_NAME GA_LAST_NAME,
14:50:31 735  	  GROUP_ACCOUNT.EMAIL GA_EMAIL,
14:50:31 736  	  GROUP_ACCOUNT.PHONE GA_PHONE,
14:50:31 737  	  GROUP_ACCOUNT.ORGANIZATION_TYPE GA_ORGANIZATION_TYPE,
14:50:31 738  	  GROUP_ACCOUNT.SEATS GA_SEATS,
14:50:31 739  	  PROCS_GROUP_ACCOUNT_V22.F_GET_NUM_OCCUPIED_GROUP_SEATS(GROUP_ACCOUNT.ID) GA_SEATS_USED,
14:50:31 740  	  GROUP_ACCOUNT.IP GA_IP,
14:50:31 741  	  PROCS_SUBSCRIPTION_V22.GET_GIFT_CERT_CODE_BY_SUB_ID(SUBSCRIPTION.ID) gift_certificate_code,
14:50:31 742  	  PROCS_ACCOUNT_V22.GET_GRACE_START_DATE(SUBSCRIPTION.ID) GRACE_START_DATE,
14:50:31 743  	  PROCS_ACCOUNT_V22.GET_GRACE_END_DATE(SUBSCRIPTION.ID) GRACE_END_DATE
14:50:31 744  	FROM
14:50:31 745  	  SUBSCRIPTION
14:50:31 746  	  INNER JOIN OFFER_CHAIN ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
14:50:31 747  	  LEFT JOIN SUBSCRIPTION_CANCEL_REASON ON SUBSCRIPTION.SCT_ID = SUBSCRIPTION_CANCEL_REASON.ID
14:50:31 748  	  LEFT JOIN ITUNES_RECEIPT ON SUBSCRIPTION.ID = ITUNES_RECEIPT.SUBSCRIPTION_ID
14:50:31 749  	  LEFT JOIN GROUP_ACCOUNT ON SUBSCRIPTION.ID = GROUP_ACCOUNT.SUBSCRIPTION_ID
14:50:31 750  	WHERE
14:50:31 751  	  SUBSCRIPTION.ID = in_subscription_id;
14:50:31 752  
14:50:31 753  EXCEPTION
14:50:31 754  WHEN BAD_SUBSCRIPTION_ID THEN
14:50:31 755  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 756  	  SPROC_NAME, 'No such subscription');
14:50:31 757  WHEN OTHERS THEN
14:50:31 758  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:31 759  	  SPROC_NAME, 'Unknown error', SQLERRM);
14:50:31 760  END GET_SUBSCRIPTION_INFO;
14:50:31 761  
14:50:31 762  /******************************************************************************/
14:50:31 763  
14:50:31 764  PROCEDURE GET_SUBSCRIPTION_INVOICES (
14:50:31 765  /*
14:50:31 766  Throws exceptions:
14:50:31 767  APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 768  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
14:50:31 769  */
14:50:31 770  	in_subscription_id  IN	NUMBER,
14:50:31 771  	out_result_set	    OUT SYS_REFCURSOR
14:50:31 772  ) AS
14:50:31 773  SPROC_NAME	   CONSTANT VARCHAR2(25) := 'GET_SUBSCRIPTION_INVOICES';
14:50:31 774  temp_subscription_id NUMBER;
14:50:31 775  BEGIN
14:50:31 776  	-- Note: A subscription has one or more associated licenses, each of which has an associated invoice.
14:50:31 777  	-- Find associated LICENSES for the subscription by "LICENSE.subscription_id"
14:50:31 778  	--   for each license
14:50:31 779  	--     get associated invoice
14:50:31 780  	--     add to results list
14:50:31 781  	--   end loop
14:50:31 782  	-- end
14:50:31 783  
14:50:31 784  	SELECT
14:50:31 785  	  SUBSCRIPTION.ID into temp_subscription_id
14:50:31 786  	FROM
14:50:31 787  	  SUBSCRIPTION
14:50:31 788  	WHERE
14:50:31 789  	  SUBSCRIPTION.ID = in_subscription_id;
14:50:31 790  
14:50:31 791  	OPEN out_result_set FOR
14:50:31 792  	SELECT
14:50:31 793  	  INVOICE.ID,
14:50:31 794  	  INVOICE.INVOICE_STATUS_ID,
14:50:31 795  	  INVOICE.CREATE_DATE,
14:50:31 796  	  INVOICE.CREATED_BY,
14:50:31 797  	  INVOICE.UPDATE_DATE,
14:50:31 798  	  INVOICE.UPDATED_BY,
14:50:31 799  	  INVOICE.TAX_EXEMPT_ID
14:50:31 800  	FROM
14:50:31 801  	  LICENSE
14:50:31 802  	    INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
14:50:31 803  	WHERE
14:50:31 804  	  LICENSE.SUBSCRIPTION_ID = in_subscription_id;
14:50:31 805  
14:50:31 806  EXCEPTION
14:50:31 807  WHEN NO_DATA_FOUND THEN
14:50:31 808  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 809  	  SPROC_NAME, 'Could not find subscription with given ID');
14:50:31 810  WHEN OTHERS THEN
14:50:31 811  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:31 812  	  SPROC_NAME, 'Unknown error', SQLERRM);
14:50:31 813  END GET_SUBSCRIPTION_INVOICES;
14:50:31 814  
14:50:31 815  /******************************************************************************/
14:50:31 816  
14:50:31 817  PROCEDURE GET_SUBSCRIPTION_NOTES (
14:50:31 818  /*
14:50:31 819  Throws exceptions:
14:50:31 820  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
14:50:31 821  */
14:50:31 822  	in_subscription_id  IN	NUMBER,
14:50:31 823  	out_result_set	    OUT SYS_REFCURSOR
14:50:31 824  ) AS
14:50:31 825  -- VARIABLES
14:50:31 826  SPROC_NAME	   CONSTANT VARCHAR2(22) := 'GET_SUBSCRIPTION_NOTES';
14:50:31 827  temp_subscription_id NUMBER;
14:50:31 828  -- EXCEPTIONS
14:50:31 829  BAD_SUBSCRIPTION_ID EXCEPTION;
14:50:31 830  BEGIN
