14:50:31 1969  	 end if;
14:50:31 1970  
14:50:31 1971  	 SELECT
14:50:31 1972  	   t.order_id,
14:50:31 1973  	   ta.external_transaction_id
14:50:31 1974  	   into
14:50:31 1975  	   out_order_id,
14:50:31 1976  	   out_external_transaction_id
14:50:31 1977  	 from
14:50:31 1978  	   charge ch
14:50:31 1979  	   inner join transaction t on ch.transaction_id = t.id
14:50:31 1980  	   inner join transaction_attempt ta on ta.transaction_id = t.id
14:50:31 1981  	 where
14:50:31 1982  	   ch.invoice_id = in_invoice_id
14:50:31 1983  	   and ta.transaction_attempt_status_id = GLOBAL_STATUSES_V22.TRANS_ATTEMPT_SUCCESS
14:50:31 1984  	   and ch.charge_amount > 0; -- We are not creating charges for the 0-amount invoices
14:50:31 1985  
14:50:31 1986  EXCEPTION
14:50:31 1987  WHEN BAD_INVOICE_ID THEN
14:50:31 1988  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 1989  	   SPROC_NAME, 'No such invoice');
