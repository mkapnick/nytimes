14:50:31 437  PROCEDURE GET_TRANSACTION_INVOICE (
14:50:31 438  /*
14:50:31 439  Throws exceptions:
14:50:31 440  APP_EXCEPTION_CODES_V22.NOT_FOUND
14:50:31 441  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
14:50:31 442  */
14:50:31 443  	in_transaction_id  IN  NUMBER,
14:50:31 444  	out_result_set	      OUT SYS_REFCURSOR
14:50:31 445  ) AS
14:50:31 446  SPROC_NAME CONSTANT  VARCHAR2(23) := 'GET_TRANSACTION_INVOICE';
14:50:31 447  -- VARIABLES
14:50:31 448  temp_transaction_id  NUMBER;
14:50:31 449  var_invoice_id	   NUMBER;
14:50:31 450  var_subscription_id  NUMBER;
14:50:31 451  var_offer_chain_id   NUMBER;
14:50:31 452  var_offer_chain_name VARCHAR2(255);
14:50:31 453  -- EXCEPTIONS
14:50:31 454  BAD_TRANSACTION_ID     EXCEPTION;
14:50:31 455  EXCEPTION_MESSAGE      VARCHAR2(1024);
14:50:31 456  BEGIN
14:50:31 457   -- check if there is the transaction
14:50:31 458  	BEGIN
14:50:31 459  	  SELECT
14:50:31 460  	    TRANSACTION.ID into temp_transaction_id
14:50:31 461  	  FROM
14:50:31 462  	    TRANSACTION
14:50:31 463  	  WHERE
14:50:31 464  	    TRANSACTION.ID = in_transaction_id;
14:50:31 465  	  EXCEPTION
14:50:31 466  	    WHEN NO_DATA_FOUND THEN
14:50:31 467  	      RAISE BAD_TRANSACTION_ID;
14:50:31 468  	END;
14:50:31 469  
14:50:31 470  	-- Get invoice id
14:50:31 471  	SELECT DISTINCT
14:50:31 472  	  CHARGE.INVOICE_ID into var_invoice_id
14:50:31 473  	FROM
14:50:31 474  	  CHARGE
14:50:31 475  	WHERE
14:50:31 476  	  CHARGE.TRANSACTION_ID = in_transaction_id;
14:50:31 477  
14:50:31 478  	-- Get subscription id if exists
14:50:31 479  	BEGIN
14:50:31 480  	  SELECT DISTINCT
14:50:31 481  	    LICENSE.SUBSCRIPTION_ID into var_subscription_id
14:50:31 482  	  FROM
14:50:31 483  	    LICENSE
14:50:31 484  	  WHERE
14:50:31 485  	    LICENSE.INVOICE_ID = var_invoice_id;
14:50:31 486  	  EXCEPTION
14:50:31 487  	    WHEN NO_DATA_FOUND THEN
14:50:31 488  	      var_subscription_id := NULL;
14:50:31 489  	END;
14:50:31 490  
14:50:31 491  	IF var_subscription_id IS NOT NULL THEN
14:50:31 492  	  -- Fetch offer chain from subscription
14:50:31 493  	  SELECT
14:50:31 494  	    OFFER_CHAIN.ID,
14:50:31 495  	    OFFER_CHAIN.NAME
14:50:31 496  	    into
14:50:31 497  	    var_offer_chain_id,
14:50:31 498  	    var_offer_chain_name
14:50:31 499  	  FROM
14:50:31 500  	    OFFER_CHAIN
14:50:31 501  	    INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
14:50:31 502  	  WHERE
14:50:31 503  	    SUBSCRIPTION.ID = var_subscription_id;
14:50:31 504  	ELSE
14:50:31 505  	  -- Fetch offer chain from GC
14:50:31 506  	  SELECT
14:50:31 507  	    OFFER_CHAIN.ID,
14:50:31 508  	    OFFER_CHAIN.NAME
14:50:31 509  	    into
14:50:31 510  	    var_offer_chain_id,
14:50:31 511  	    var_offer_chain_name
14:50:31 512  	  FROM
14:50:31 513  	    OFFER_CHAIN
14:50:31 514  	    INNER JOIN GIFT_CERTIFICATE ON GIFT_CERTIFICATE.OFFER_CHAIN_ID = OFFER_CHAIN.ID
14:50:31 515  	  WHERE
14:50:31 516  	    GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = var_invoice_id;
14:50:31 517  	END IF;
14:50:31 518  
14:50:31 519  	OPEN out_result_set FOR
14:50:31 520  	SELECT DISTINCT
14:50:31 521  	  var_invoice_id       AS "INVOICE_ID",
14:50:31 522  	  var_subscription_id  AS "SUBSCRIPTION_ID",
14:50:31 523  	  var_offer_chain_id   AS "OFFER_CHAIN_ID",
14:50:31 524  	  var_offer_chain_name AS "OFFER_CHAIN_NAME"
14:50:31 525  	FROM
14:50:31 526  	  DUAL;
14:50:31 527  
14:50:31 528  EXCEPTION
14:50:31 529  WHEN BAD_TRANSACTION_ID THEN
14:50:31 530  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 531  	  SPROC_NAME, 'No such transaction');
14:50:31 532  WHEN OTHERS THEN
14:50:31 533  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:31 534  	  SPROC_NAME, 'Unknown error', SQLERRM);
14:50:31 535  END GET_TRANSACTION_INVOICE;
