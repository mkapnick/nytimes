14:50:31  77  
14:50:31  78  PROCEDURE START_SUBSCRIPTION_CREATION (
14:50:31  79  	in_group_id	      IN NUMBER,
14:50:31  80  	in_created_by	      IN VARCHAR2,
14:50:31  81  	in_offer_chain_id     IN NUMBER,
14:50:31  82  	in_instrument_type_id IN NUMBER,
14:50:31  83  	in_instrument_id      IN NUMBER,
14:50:31  84  	in_agent_id	      IN NUMBER,
14:50:31  85  	in_note 	      IN VARCHAR2,
14:50:31  86  	out_subscription_id   OUT NUMBER,
14:50:31  87  	out_invoice_id	      OUT NUMBER,
14:50:31  88  	out_new_license_id    OUT NUMBER,
14:50:31  89  	in_check_dupe_products	 IN NUMBER DEFAULT GLOBAL_CONSTANTS_V22.TRUE
14:50:31  90  ) AS
14:50:31  91  SPROC_NAME CONSTANT VARCHAR2(27) := 'START_SUBSCRIPTION_CREATION';
14:50:31  92  -- VARIABLES
14:50:31  93  var_account_id		 NUMBER;
14:50:31  94  var_account_status	 NUMBER;
14:50:31  95  var_offer_chain_status_id  NUMBER;
14:50:31  96  var_is_gift_certificate	 NUMBER;
14:50:31  97  var_is_for_redemption	 NUMBER;
14:50:31  98  var_same_offer_chains_num  NUMBER;
14:50:31  99  var_max_concurrent_subscrs NUMBER;
14:50:31 100  var_first_offer_id	 NUMBER;
14:50:31 101  var_new_invoice_id	 NUMBER;
14:50:31 102  var_new_subscription_id	 NUMBER;
14:50:31 103  var_date			 DATE := SYSDATE;
14:50:31 104  var_account_tax_exempt_id  VARCHAR2(255);
14:50:31 105  var_concur_subscription_id NUMBER;
14:50:31 106  -- EXCEPTIONS
14:50:31 107  BAD_GROUP_ID		    EXCEPTION;
14:50:31 108  CAN_NOT_CREATE_FOR_DISABLE    EXCEPTION;
14:50:31 109  BAD_OFFER_CHAIN		    EXCEPTION;
14:50:31 110  BAD_OFFER_CHAIN_STATUS	    EXCEPTION;
14:50:31 111  CAN_NOT_SUBSCRIBE_TO_GC	    EXCEPTION;
14:50:31 112  CAN_NOT_SUBSCRIBE_TO_RGC	    EXCEPTION;
14:50:31 113  LIMIT_REACHED		    EXCEPTION;
14:50:31 114  PRODUCT_ALREADY_PURCHASED     EXCEPTION;
14:50:31 115  CAN_NOT_GET_FIRST_OFFER_CHAIN EXCEPTION;
14:50:31 116  CAN_NOT_CREATE_INVOICE	    EXCEPTION;
14:50:31 117  CAN_NOT_CREATE_LINE_ITEMS     EXCEPTION;
14:50:31 118  CAN_NOT_CREATE_LICENSE	    EXCEPTION;
14:50:31 119  CAN_NOT_CREATE_NOTE	    EXCEPTION;
14:50:31 120  
14:50:31 121  EXCEPTION_MESSAGE VARCHAR2(1024);
14:50:31 122  BEGIN
14:50:31 123  
14:50:31 124  	-- Get account id and status
14:50:31 125  	BEGIN
14:50:31 126  	  SELECT
14:50:31 127  	    ACCOUNT.ID,
14:50:31 128  	    ACCOUNT.ACCOUNT_STATUS_ID,
14:50:31 129  	    ACCOUNT.TAX_EXEMPT_ID
14:50:31 130  	    into
14:50:31 131  	    var_account_id,
14:50:31 132  	    var_account_status,
14:50:31 133  	    var_account_tax_exempt_id
14:50:31 134  	  FROM
14:50:31 135  	    ACCOUNT
14:50:31 136  	  WHERE
14:50:31 137  	    ACCOUNT.GROUP_ID = in_group_id;
14:50:31 138  	  EXCEPTION
14:50:31 139  	  WHEN NO_DATA_FOUND THEN
14:50:31 140  	    RAISE BAD_GROUP_ID;
14:50:31 141  	END;
14:50:31 142  
14:50:31 143  	-- Could not create subscription for disabled account
14:50:31 144  	IF var_account_status = GLOBAL_STATUSES_V22.ACCOUNT_DISABLED THEN
14:50:31 145  	  RAISE CAN_NOT_CREATE_FOR_DISABLE;
14:50:31 146  	END IF;
14:50:31 147  
14:50:31 148  	-- Get offer chain status
14:50:31 149  	BEGIN
14:50:31 150  	  SELECT
14:50:31 151  	    OFFER_CHAIN.OFFER_CHAIN_STATUS_ID,
14:50:31 152  	    OFFER_CHAIN.IS_GIFT_CERTIFICATE
14:50:31 153  	    into
14:50:31 154  	    var_offer_chain_status_id,
14:50:31 155  	    var_is_gift_certificate
14:50:31 156  	  FROM
14:50:31 157  	    OFFER_CHAIN
14:50:31 158  	  WHERE
14:50:31 159  	    OFFER_CHAIN.ID = in_offer_chain_id;
14:50:31 160  
14:50:31 161  	  EXCEPTION
14:50:31 162  	    WHEN NO_DATA_FOUND THEN
14:50:31 163  	      RAISE BAD_OFFER_CHAIN;
14:50:31 164  	END;
14:50:31 165  
14:50:31 166  	-- Could not subscribe to inactive/disabled offer chain
14:50:31 167  	IF var_offer_chain_status_id != GLOBAL_STATUSES_V22.OFFER_CHAIN_ACTIVE THEN
14:50:31 168  	  RAISE BAD_OFFER_CHAIN_STATUS;
14:50:31 169  	END IF;
14:50:31 170  
14:50:31 171  	-- Can not subscribe to Offer Chain for a Gift Certfiicate
14:50:31 172  	IF var_is_gift_certificate = GLOBAL_CONSTANTS_V22.TRUE THEN
14:50:31 173  	  RAISE CAN_NOT_SUBSCRIBE_TO_GC;
14:50:31 174  	END IF;
14:50:31 175  
14:50:31 176  	-- check if the OC is for Redemption:
14:50:31 177  	SELECT
14:50:31 178  	  COUNT(*) into var_is_for_redemption
14:50:31 179  	FROM
14:50:31 180  	  OFFER_CHAIN_ELIGIBILITY
14:50:31 181  	WHERE
14:50:31 182  	  OFFER_CHAIN_ELIGIBILITY.OFFER_CHAIN_ID = in_offer_chain_id
14:50:31 183  	  AND OFFER_CHAIN_ELIGIBILITY.NAME = GLOBAL_CONSTANTS_V22.GIFT_CERTIFICATE_REQUIRED
14:50:31 184  	  AND OFFER_CHAIN_ELIGIBILITY.VALUE = GLOBAL_CONSTANTS_V22.ELIGIBILITY_FLAG_SET;
14:50:31 185  
14:50:31 186  	IF var_is_for_redemption > 0 THEN
14:50:31 187  	  RAISE CAN_NOT_SUBSCRIBE_TO_RGC;
14:50:31 188  	END IF;
14:50:31 189  
14:50:31 190  	SELECT
14:50:31 191  	  COUNT(*) into var_same_offer_chains_num
14:50:31 192  	FROM
14:50:31 193  	  SUBSCRIPTION
14:50:31 194  	WHERE
14:50:31 195  	  SUBSCRIPTION.ACCOUNT_ID = var_account_id
14:50:31 196  	  AND SUBSCRIPTION.OFFER_CHAIN_ID = in_offer_chain_id
14:50:31 197  	  AND (SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE
14:50:31 198  	       OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_IN_GRACE_PERIOD);
14:50:31 199  
14:50:31 200  	-- ELIGIBILITY LOGIC CHANGED TO:
14:50:31 201  	-- FOR EACH offer chain eligibility rule in OC:
14:50:31 202  	--   IF offer chain eligibility rule fails:
14:50:31 203  	--     deny purchase;
14:50:31 204  	--   END IF
14:50:31 205  	-- END FOR
14:50:31 206  	-- FOR EACH product eligibility rule in OC:
14:50:31 207  	--   IF product eligibilty rule fails:
14:50:31 208  	--     deny purchase;
14:50:31 209  	--   END IF
14:50:31 210  	-- END FOR
14:50:31 211  	-- allow purchase;
14:50:31 212  
14:50:31 213  	-- if user have any active existing subscriptions to the offer chain
14:50:31 214  	-- and if MAX_CONCURRENT_SUBS <= [user's subscription count for the offer chain]
14:50:31 215  	-- then deny purchase
14:50:31 216  	var_max_concurrent_subscrs := PROCS_OFFER_CHAIN_V22.GET_OFFER_CHAIN_MAX_CONC_SUBSC(in_offer_chain_id);
14:50:31 217  	IF var_max_concurrent_subscrs != GLOBAL_CONSTANTS_V22.INFINITY
14:50:31 218  	  AND var_max_concurrent_subscrs <= var_same_offer_chains_num THEN
14:50:31 219  	  -- Find first concurrent subscription id:
14:50:31 220  	  SELECT
14:50:31 221  	    ID into var_concur_subscription_id
14:50:31 222  	  FROM (
14:50:31 223  	    SELECT
14:50:31 224  	      ID
14:50:31 225  	    FROM
14:50:31 226  	      SUBSCRIPTION
14:50:31 227  	    WHERE
14:50:31 228  	      SUBSCRIPTION.ACCOUNT_ID = var_account_id
14:50:31 229  	      AND SUBSCRIPTION.OFFER_CHAIN_ID = in_offer_chain_id
14:50:31 230  	      AND (SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE
14:50:31 231  		   OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_IN_GRACE_PERIOD)
14:50:31 232  	    ORDER BY
14:50:31 233  	      ID
14:50:31 234  	  )
14:50:31 235  	  WHERE
14:50:31 236  	    ROWNUM <= 1;
14:50:31 237  	  RAISE LIMIT_REACHED;
14:50:31 238  	END IF;
14:50:31 239  
14:50:31 240  	-- if user does not have any active existing subscriptions to the offer chain
14:50:31 241  	-- and if product from the offer chain is already owned from another offer chain
14:50:31 242  	-- then deny purchase
14:50:31 243  	IF (in_check_dupe_products != GLOBAL_CONSTANTS_V22.FALSE) THEN
14:50:31 244  	  FOR f_account_offer_chains IN (
14:50:31 245  	    SELECT DISTINCT
14:50:31 246  	      OFFER_CHAIN_ID
14:50:31 247  	    FROM
14:50:31 248  	      SUBSCRIPTION
14:50:31 249  	    WHERE
14:50:31 250  	      ACCOUNT_ID = var_account_id
14:50:31 251  	      AND (
14:50:31 252  		SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE
14:50:31 253  		OR SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_SUSPENDED
14:50:31 254  		OR SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_IN_GRACE_PERIOD)
14:50:31 255  	  )
14:50:31 256  	  LOOP
14:50:31 257  	    IF PROCS_OFFER_CHAIN_V22.CHECK_FOR_SAME_PRODUCTS(
14:50:31 258  	      in_offer_chain_1	       => in_offer_chain_id,
14:50:31 259  	      in_offer_chain_2	       => f_account_offer_chains.OFFER_CHAIN_ID,
14:50:31 260  	      in_use_eligibility_rules => GLOBAL_CONSTANTS_V22.TRUE
14:50:31 261  	    ) = GLOBAL_CONSTANTS_V22.TRUE THEN
14:50:31 262  
14:50:31 263  	      -- Find first concurrent subscription id:
14:50:31 264  	      SELECT
14:50:31 265  		ID into var_concur_subscription_id
14:50:31 266  	      FROM (
14:50:31 267  		SELECT
14:50:31 268  		  ID
14:50:31 269  		FROM
14:50:31 270  		  SUBSCRIPTION
14:50:31 271  		WHERE
14:50:31 272  		  SUBSCRIPTION.ACCOUNT_ID = var_account_id
14:50:31 273  		  AND SUBSCRIPTION.OFFER_CHAIN_ID = f_account_offer_chains.OFFER_CHAIN_ID
14:50:31 274  		  AND (
14:50:31 275  		    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE
14:50:31 276  		    OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_SUSPENDED
14:50:31 277  		    OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_IN_GRACE_PERIOD)
14:50:31 278  		ORDER BY
14:50:31 279  		  ID
14:50:31 280  	      )
14:50:31 281  	      WHERE
14:50:31 282  		ROWNUM <= 1;
14:50:31 283  
14:50:31 284  	      RAISE PRODUCT_ALREADY_PURCHASED;
14:50:31 285  	    END IF;
14:50:31 286  	  END LOOP;
14:50:31 287  	END IF;
14:50:31 288  
14:50:31 289  	BEGIN
14:50:31 290  	  PROCS_OFFER_CHAIN_V22.GET_FIRST_OFFER(in_offer_chain_id, var_first_offer_id);
14:50:31 291  	  EXCEPTION
14:50:31 292  	    WHEN OTHERS THEN
14:50:31 293  	      EXCEPTION_MESSAGE := SQLERRM;
14:50:31 294  	      RAISE CAN_NOT_GET_FIRST_OFFER_CHAIN;
14:50:31 295  	END;
14:50:31 296  
14:50:31 297  	BEGIN
14:50:31 298  	  PROCS_INVOICE_V22.CREATE_INVOICE(
14:50:31 299  	    in_invoice_status => GLOBAL_STATUSES_V22.INVOICE_OPEN,
14:50:31 300  	    in_created_by     => in_created_by,
14:50:31 301  	    in_tax_exempt_id  => var_account_tax_exempt_id,
14:50:31 302  	    out_invoice_id    => var_new_invoice_id
14:50:31 303  	  );
14:50:31 304  	  EXCEPTION
14:50:31 305  	    WHEN OTHERS THEN
14:50:31 306  	      EXCEPTION_MESSAGE := SQLERRM;
14:50:31 307  	      RAISE CAN_NOT_CREATE_INVOICE;
14:50:31 308  	END;
14:50:31 309  
14:50:31 310  	BEGIN
14:50:31 311  	  PROCS_LINE_ITEMS_V22.ADD_LINE_ITEMS(
14:50:31 312  	    in_invoice_id => var_new_invoice_id,
14:50:31 313  	    in_offer_id   => var_first_offer_id,
14:50:31 314  	    in_created_by => in_created_by
14:50:31 315  	  );
14:50:31 316  	  EXCEPTION
14:50:31 317  	    WHEN OTHERS THEN
14:50:31 318  	      EXCEPTION_MESSAGE := SQLERRM;
14:50:31 319  	      RAISE CAN_NOT_CREATE_LINE_ITEMS;
14:50:31 320  	END;
14:50:31 321  
14:50:31 322  	PROCS_SUBSCRIPTION_CRU_V22.CREATE_SUBSCRIPTION(
14:50:31 323  	  out_subscription_id	    => var_new_subscription_id,
14:50:31 324  	  in_account_id 	    => var_account_id,
14:50:31 325  	  in_purchase_date	    => var_date,
14:50:31 326  	  in_offer_chain_id	    => in_offer_chain_id,
14:50:31 327  	  in_created_by 	    => in_created_by,
14:50:31 328  	  in_instrument_type_id     => in_instrument_type_id,
14:50:31 329  	  in_instrument_id	    => in_instrument_id,
14:50:31 330  	  in_subscription_status_id => GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE
14:50:31 331  	);
14:50:31 332  
14:50:31 333  	BEGIN
14:50:31 334  	  PROCS_SUBSCRIPTION_V22.ANNOTATE_SUBSCRIPTION(
14:50:31 335  	    in_subscription_id => var_new_subscription_id,
14:50:31 336  	    in_agent_id        => in_agent_id,
14:50:31 337  	    in_note	       => in_note,
14:50:31 338  	    in_created_by      => in_created_by
14:50:31 339  	  );
14:50:31 340  	  EXCEPTION
14:50:31 341  	   WHEN OTHERS THEN
14:50:31 342  	     EXCEPTION_MESSAGE := SQLERRM;
14:50:31 343  	     RAISE CAN_NOT_CREATE_NOTE;
14:50:31 344  	END;
14:50:31 345  
14:50:31 346  	BEGIN
14:50:31 347  	  PROCS_LICENSE_V22.CREATE_LICENSE(
14:50:31 348  	    in_status_id		=> GLOBAL_STATUSES_V22.LICENSE_ACTIVE,
14:50:31 349  	    in_needs_entitlements	=> GLOBAL_CONSTANTS_V22.TRUE,
14:50:31 350  	    in_start_date		=> var_date,
14:50:31 351  	    in_end_date 		=> NULL, -- Will be calculated automatically
14:50:31 352  	    in_offer_id 		=> var_first_offer_id,
14:50:31 353  	    in_subscription_id		=> var_new_subscription_id,
14:50:31 354  	    in_invoice_id		=> var_new_invoice_id,
14:50:31 355  	    in_created_by		=> in_created_by,
14:50:31 356  	    in_is_extension		=> GLOBAL_CONSTANTS_V22.FALSE,
14:50:31 357  	    in_current_offer_index	=> PROCS_OFFER_CHAIN_V22.GET_FIRST_OFFER_INDEX(in_offer_chain_id),
14:50:31 358  	    in_current_offer_recurr_num => 1,
14:50:31 359  	    out_license_id		=> out_new_license_id
14:50:31 360  	  );
14:50:31 361  	  EXCEPTION
14:50:31 362  	    WHEN OTHERS THEN
14:50:31 363  	      EXCEPTION_MESSAGE := SQLERRM;
14:50:31 364  	      RAISE CAN_NOT_CREATE_LICENSE;
14:50:31 365  	END;
14:50:31 366  
14:50:31 367  	out_subscription_id := var_new_subscription_id;
14:50:31 368  	out_invoice_id := var_new_invoice_id;
14:50:31 369  
14:50:31 370  EXCEPTION
14:50:31 371  WHEN BAD_OFFER_CHAIN_STATUS THEN
14:50:31 372  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.STATE_ERROR,
14:50:31 373  	  SPROC_NAME, 'Offer chain is not active');
14:50:31 374  WHEN LIMIT_REACHED THEN
14:50:31 375  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.CONFLICT_ERROR,
14:50:31 376  	  SPROC_NAME, 'Limit reached for given offer chain. Concurrent subscription id: ' || var_concur_subscription_id);
14:50:31 377  WHEN CAN_NOT_CREATE_FOR_DISABLE THEN
14:50:31 378  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.STATE_ERROR,
14:50:31 379  	  SPROC_NAME, 'Can not create subsscription for disabled account');
14:50:31 380  WHEN CAN_NOT_SUBSCRIBE_TO_GC THEN
14:50:31 381  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.STATE_ERROR,
14:50:31 382  	  SPROC_NAME, 'Can not subscribe to Offer Chain for Gift Certificate');
14:50:31 383  WHEN CAN_NOT_SUBSCRIBE_TO_RGC THEN
14:50:31 384  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.STATE_ERROR,
14:50:31 385  	  SPROC_NAME, 'Can not subscribe to Offer Chain that is for redemption');
14:50:31 386  WHEN BAD_OFFER_CHAIN THEN
14:50:31 387  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 388  	  SPROC_NAME, 'No such offer chain');
14:50:31 389  WHEN PRODUCT_ALREADY_PURCHASED THEN
14:50:31 390  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.CONFLICT_ERROR,
14:50:31 391  	  SPROC_NAME, 'User already subscribed to some product in given offer chain. Concurrent subscription id: ' || var_concur_subscription_id);
14:50:31 392  WHEN CAN_NOT_GET_FIRST_OFFER_CHAIN THEN
14:50:31 393  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 394  	  SPROC_NAME, 'Could not find first offer in offer chain', EXCEPTION_MESSAGE);
14:50:31 395  WHEN CAN_NOT_CREATE_INVOICE THEN
14:50:31 396  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 397  	  SPROC_NAME, 'Could not create invoice', EXCEPTION_MESSAGE);
14:50:31 398  WHEN CAN_NOT_CREATE_LINE_ITEMS THEN
14:50:31 399  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 400  	  SPROC_NAME, 'Could not create line items', EXCEPTION_MESSAGE);
14:50:31 401  WHEN CAN_NOT_CREATE_LICENSE THEN
14:50:31 402  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 403  	  SPROC_NAME, 'Could not create new license', EXCEPTION_MESSAGE);
14:50:31 404  WHEN BAD_GROUP_ID THEN
14:50:31 405  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 406  	  SPROC_NAME, 'Bad group id');
14:50:31 407  WHEN CAN_NOT_CREATE_NOTE THEN
14:50:31 408  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 409  	  SPROC_NAME, 'Could not annotate subscription', EXCEPTION_MESSAGE);
14:50:31 410  WHEN OTHERS THEN
14:50:31 411  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:31 412  	  SPROC_NAME, 'Unknown error', SQLERRM);
14:50:31 413  END START_SUBSCRIPTION_CREATION;
