14:50:30 329  PROCEDURE GET_RECURRING_OFFER (
14:50:30 330  	in_license_id  IN NUMBER,
14:50:30 331  	out_result_set OUT SYS_REFCURSOR
14:50:30 332  ) AS
14:50:30 333  -- VARIABLES
14:50:30 334  SPROC_NAME		    CONSTANT VARCHAR2(19) := 'GET_RECURRING_OFFER';
14:50:30 335  var_offer_chain_id	    NUMBER;
14:50:30 336  var_offer_id		    NUMBER;
14:50:30 337  var_offer_num_recurrences     NUMBER;
14:50:30 338  var_license_cur_offer_rec_num NUMBER;
14:50:30 339  var_offer_index		    NUMBER;
14:50:30 340  -- EXCEPTIONS
14:50:30 341  BAD_LICENSE_ID	     EXCEPTION;
14:50:30 342  CAN_NOT_GET_OFFER_INFO EXCEPTION;
14:50:30 343  BEGIN
14:50:30 344  
14:50:30 345  	BEGIN
14:50:30 346  	  SELECT
14:50:30 347  	    SUBSCRIPTION.OFFER_CHAIN_ID,
14:50:30 348  	    LICENSE.OFFER_ID,
14:50:30 349  	    LICENSE.CURRENT_OFFER_RECURR_NUM
14:50:30 350  	    into
14:50:30 351  	    var_offer_chain_id,
14:50:30 352  	    var_offer_id,
14:50:30 353  	    var_license_cur_offer_rec_num
14:50:30 354  	  FROM
14:50:30 355  	    LICENSE
14:50:30 356  	    INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
14:50:30 357  	  WHERE
14:50:30 358  	    LICENSE.ID = in_license_id;
14:50:30 359  	  EXCEPTION
14:50:30 360  	    WHEN NO_DATA_FOUND THEN
14:50:30 361  	      RAISE BAD_LICENSE_ID;
14:50:30 362  	END;
14:50:30 363  
14:50:30 364  	BEGIN
14:50:30 365  	  SELECT
14:50:30 366  	    OFFER_OFFER_CHAIN.NUM_RECURRENCES,
14:50:30 367  	    OFFER_OFFER_CHAIN.INDEX_VALUE
14:50:30 368  	    into
14:50:30 369  	    var_offer_num_recurrences,
14:50:30 370  	    var_offer_index
14:50:30 371  	  FROM
14:50:30 372  	    OFFER_OFFER_CHAIN
14:50:30 373  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
14:50:30 374  	  WHERE
14:50:30 375  	    OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
14:50:30 376  	    AND OFFER_OFFER_CHAIN.OFFER_ID = var_offer_id;
14:50:30 377  	  EXCEPTION
14:50:30 378  	    WHEN NO_DATA_FOUND THEN
14:50:30 379  	      RAISE CAN_NOT_GET_OFFER_INFO;
14:50:30 380  	END;
14:50:30 381  
14:50:30 382  	IF var_offer_num_recurrences = 0 THEN
14:50:30 383  	  --out_result_set := NULL;
14:50:30 384  	  -- TODO: Remove this hardcode
14:50:30 385  	  OPEN out_result_set FOR
14:50:30 386  	  SELECT
14:50:30 387  	    OFFER.ID,
14:50:30 388  	    OFFER.OFFER_STATUS_ID,
14:50:30 389  	    OFFER.ENTITLEMENT_DURATION,
14:50:30 390  	    OFFER.CREATED_BY,
14:50:30 391  	    OFFER.CREATE_DATE,
14:50:30 392  	    OFFER.UPDATED_BY,
14:50:30 393  	    OFFER.UPDATE_DATE,
14:50:30 394  	    var_offer_num_recurrences as "RECURRENCE_NUMBER",
14:50:30 395  	    var_offer_index as "OFFER_INDEX"
14:50:30 396  	  FROM
14:50:30 397  	    OFFER_OFFER_CHAIN
14:50:30 398  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
14:50:30 399  	  WHERE ROWNUM = 0;
14:50:30 400  	ELSIF var_license_cur_offer_rec_num = var_offer_num_recurrences THEN
14:50:30 401  	  --out_result_set := NULL;
14:50:30 402  	  -- TODO: Remove this hardcode
14:50:30 403  	  OPEN out_result_set FOR
14:50:30 404  	  SELECT
14:50:30 405  	    OFFER.ID,
14:50:30 406  	    OFFER.OFFER_STATUS_ID,
14:50:30 407  	    OFFER.ENTITLEMENT_DURATION,
14:50:30 408  	    OFFER.CREATED_BY,
14:50:30 409  	    OFFER.CREATE_DATE,
14:50:30 410  	    OFFER.UPDATED_BY,
14:50:30 411  	    OFFER.UPDATE_DATE,
14:50:30 412  	    var_offer_num_recurrences as "RECURRENCE_NUMBER",
14:50:30 413  	    var_offer_index as "OFFER_INDEX"
14:50:30 414  	  FROM
14:50:30 415  	    OFFER_OFFER_CHAIN
14:50:30 416  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
14:50:30 417  	  WHERE ROWNUM = 0;
14:50:30 418  	ELSE
14:50:30 419  	  OPEN out_result_set FOR
14:50:30 420  	  SELECT
14:50:30 421  	    OFFER.ID,
14:50:30 422  	    OFFER.OFFER_STATUS_ID,
14:50:30 423  	    OFFER.ENTITLEMENT_DURATION,
14:50:30 424  	    OFFER.CREATED_BY,
14:50:30 425  	    OFFER.CREATE_DATE,
14:50:30 426  	    OFFER.UPDATED_BY,
14:50:30 427  	    OFFER.UPDATE_DATE,
14:50:30 428  	    var_offer_num_recurrences as "RECURRENCE_NUMBER",
14:50:30 429  	    var_offer_index as "OFFER_INDEX"
14:50:30 430  	  FROM
14:50:30 431  	    OFFER
14:50:30 432  	  WHERE
14:50:30 433  	    OFFER.ID = var_offer_id;
14:50:30 434  	END IF;
14:50:30 435  
14:50:30 436  EXCEPTION
14:50:30 437  WHEN BAD_LICENSE_ID THEN
14:50:30 438  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:30 439  	  SPROC_NAME, 'No such license');
14:50:30 440  WHEN CAN_NOT_GET_OFFER_INFO THEN
14:50:30 441  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:30 442  	  SPROC_NAME, 'Could not get offer information. Offer id = '||var_offer_id||', Offer chain id = '||var_offer_chain_id);
14:50:30 443  WHEN OTHERS THEN
14:50:30 444  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:30 445  	  SPROC_NAME, 'Unknown error', SQLERRM);
14:50:30 446  END GET_RECURRING_OFFER;
