14:50:31 1658  
14:50:31 1659  PROCEDURE REFUND_INVOICE (
14:50:31 1660  	 in_invoice_id	    IN NUMBER,
14:50:31 1661  	 in_refund_amount   IN NUMBER,
14:50:31 1662  	 in_note	    IN VARCHAR2,
14:50:31 1663  	 in_created_by	    IN VARCHAR2,
14:50:31 1664  	 out_charge_id	    OUT NUMBER
14:50:31 1665  ) AS
14:50:31 1666  SPROC_NAME CONSTANT VARCHAR2(14) := 'REFUND_INVOICE';
14:50:31 1667  -- VARIABLES
14:50:31 1668  var_invoice_status_id  NUMBER;
14:50:31 1669  var_subscription_id    NUMBER;
14:50:31 1670  var_account_id	      NUMBER;
14:50:31 1671  var_group_id	      NUMBER;
14:50:31 1672  var_account_status_id  NUMBER;
14:50:31 1673  var_new_transaction_id NUMBER;
14:50:31 1674  var_instrument_type_id NUMBER;
14:50:31 1675  var_instrument_id      NUMBER;
14:50:31 1676  var_new_charge_id      NUMBER;
14:50:31 1677  var_invoice_amount     NUMBER(10,2);
14:50:31 1678  var_refunds_before     NUMBER(10,2);
14:50:31 1679  var_charges_amount     NUMBER(10,2);
14:50:31 1680  -- EXCEPTIONS
14:50:31 1681  CAN_NOT_FIND_SUBSCR_OR_GC     EXCEPTION;
14:50:31 1682  ACCOUNT_IS_FROZEN	     EXCEPTION;
14:50:31 1683  BAD_INVOICE_ID		     EXCEPTION;
14:50:31 1684  CAN_NOT_CREATE_TRANSACTION    EXCEPTION;
14:50:31 1685  CAN_NOT_CREATE_CHARGE	     EXCEPTION;
14:50:31 1686  CAN_NOT_CALC_INVOICE_AMOUNT   EXCEPTION;
14:50:31 1687  REFUND_IS_GREATER_THAN_ANOUNT EXCEPTION;
14:50:31 1688  CAN_NOT_ANNOTATE_SUBSCRIPTION EXCEPTION;
14:50:31 1689  TOT_REF_IS_GREATER_THAN_ANOUNT EXCEPTION;
14:50:31 1690  INVOICE_IS_NOT_CLOSED	     EXCEPTION;
14:50:31 1691  TOT_REF_IS_GRATER_THAN_CHARGES EXCEPTION;
14:50:31 1692  EXCEPTION_MESSAGE	      VARCHAR2(1024);
14:50:31 1693  BEGIN
14:50:31 1694  
14:50:31 1695  	 BEGIN
14:50:31 1696  	   SELECT
14:50:31 1697  	     INVOICE.INVOICE_STATUS_ID into var_invoice_status_id
14:50:31 1698  	   FROM
14:50:31 1699  	     INVOICE
14:50:31 1700  	   WHERE
14:50:31 1701  	     INVOICE.ID = in_invoice_id;
14:50:31 1702  	   EXCEPTION
14:50:31 1703  	     WHEN NO_DATA_FOUND THEN
14:50:31 1704  	       RAISE BAD_INVOICE_ID;
14:50:31 1705  	 END;
14:50:31 1706  
14:50:31 1707  	 -- Get instrument and subscription id if exists
14:50:31 1708  	 BEGIN
14:50:31 1709  	   SELECT
14:50:31 1710  	     SUBSCRIPTION.INSTRUMENT_ID,
14:50:31 1711  	     SUBSCRIPTION.INSTRUMENT_TYPE_ID,
14:50:31 1712  	     SUBSCRIPTION.ACCOUNT_ID,
14:50:31 1713  	     SUBSCRIPTION.ID
14:50:31 1714  	     into
14:50:31 1715  	     var_instrument_id,
14:50:31 1716  	     var_instrument_type_id,
14:50:31 1717  	     var_account_id,
14:50:31 1718  	     var_subscription_id
14:50:31 1719  	   FROM
14:50:31 1720  	     SUBSCRIPTION
14:50:31 1721  	     INNER JOIN LICENSE ON SUBSCRIPTION.ID = LICENSE.SUBSCRIPTION_ID
14:50:31 1722  	   WHERE
14:50:31 1723  	     LICENSE.INVOICE_ID = in_invoice_id
14:50:31 1724  	     AND ROWNUM <= 1;
14:50:31 1725  	   EXCEPTION
14:50:31 1726  	     WHEN NO_DATA_FOUND THEN
14:50:31 1727  	       BEGIN
14:50:31 1728  
14:50:31 1729  		 var_subscription_id := NULL;
14:50:31 1730  
14:50:31 1731  		 SELECT
14:50:31 1732  		   CHARGE.INSTRUMENT_ID,
14:50:31 1733  		   CHARGE.INSTRUMENT_TYPE_ID,
14:50:31 1734  		   GIFT_CERTIFICATE.PURCHASER_GROUP_ID
14:50:31 1735  		   into
14:50:31 1736  		   var_instrument_id,
14:50:31 1737  		   var_instrument_type_id,
14:50:31 1738  		   var_group_id
14:50:31 1739  		 FROM
14:50:31 1740  		   INVOICE
14:50:31 1741  		   INNER JOIN CHARGE ON INVOICE.ID = CHARGE.INVOICE_ID
14:50:31 1742  		   INNER JOIN GIFT_CERTIFICATE ON GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = INVOICE.ID
14:50:31 1743  		 WHERE
14:50:31 1744  		   INVOICE.ID = in_invoice_id
14:50:31 1745  		   AND ROWNUM <= 1;
14:50:31 1746  
14:50:31 1747  		 SELECT
14:50:31 1748  		   ACCOUNT.ID into var_account_id
14:50:31 1749  		 FROM
14:50:31 1750  		   ACCOUNT
14:50:31 1751  		 WHERE
14:50:31 1752  		   ACCOUNT.GROUP_ID = var_group_id;
14:50:31 1753  
14:50:31 1754  		 EXCEPTION
14:50:31 1755  		   WHEN NO_DATA_FOUND THEN
14:50:31 1756  		     RAISE CAN_NOT_FIND_SUBSCR_OR_GC;
14:50:31 1757  	       END;
14:50:31 1758  	 END;
14:50:31 1759  
14:50:31 1760  	 -- Check account status. It should not to be frozen
14:50:31 1761  	 SELECT
14:50:31 1762  	   ACCOUNT.ACCOUNT_STATUS_ID into var_account_status_id
14:50:31 1763  	 FROM
14:50:31 1764  	   ACCOUNT
14:50:31 1765  	 WHERE
14:50:31 1766  	   ACCOUNT.ID = var_account_id;
14:50:31 1767  
14:50:31 1768  	 IF var_account_status_id = GLOBAL_STATUSES_V22.ACCOUNT_FROZEN THEN
14:50:31 1769  	   RAISE ACCOUNT_IS_FROZEN;
14:50:31 1770  	 END IF;
14:50:31 1771  
14:50:31 1772  	 IF var_invoice_status_id != GLOBAL_STATUSES_V22.INVOICE_CLOSED THEN
14:50:31 1773  	   RAISE INVOICE_IS_NOT_CLOSED;
14:50:31 1774  	 END IF;
14:50:31 1775  
14:50:31 1776  	 BEGIN
14:50:31 1777  	   PROCS_INVOICE_V22.CALCULATE_INVOICE_AMOUNT (
14:50:31 1778  	     in_invoice_id => in_invoice_id,
14:50:31 1779  	     out_amount    => var_invoice_amount
14:50:31 1780  	   );
14:50:31 1781  	   EXCEPTION
14:50:31 1782  	     WHEN OTHERS THEN
14:50:31 1783  	       EXCEPTION_MESSAGE := SQLERRM;
14:50:31 1784  	       RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
14:50:31 1785  	 END;
14:50:31 1786  
14:50:31 1787  	 IF ( in_refund_amount > var_invoice_amount ) THEN
14:50:31 1788  	   RAISE REFUND_IS_GREATER_THAN_ANOUNT;
14:50:31 1789  	 END IF;
14:50:31 1790  
14:50:31 1791  	 SELECT /*+ STAR_TRANSFORMATION */
14:50:31 1792  	   SUM(CHARGE.CHARGE_AMOUNT) into var_refunds_before
14:50:31 1793  	 FROM
14:50:31 1794  	   CHARGE
14:50:31 1795  	 WHERE
14:50:31 1796  	   CHARGE.INVOICE_ID = in_invoice_id
14:50:31 1797  	   AND (
14:50:31 1798  	     CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V22.CHARGE_OPENED
14:50:31 1799  	     OR (
14:50:31 1800  	       CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V22.CHARGE_PROCESSED
14:50:31 1801  	       AND EXISTS (
14:50:31 1802  		 SELECT 1 FROM TRANSACTION_ATTEMPT ta where ta.transaction_id = CHARGE.TRANSACTION_ID and ta.transaction_attempt_status_id = GLOBAL_STATUSES_V22.TRANS_ATTEMPT_SUCCESS
14:50:31 1803  	       )
14:50:31 1804  	     )
14:50:31 1805  	   )
14:50:31 1806  	   AND CHARGE.CHARGE_AMOUNT < 0;
14:50:31 1807  
14:50:31 1808  	 -- Refunds are negative
14:50:31 1809  	 IF var_refunds_before IS NULL THEN var_refunds_before := 0; END IF;
14:50:31 1810  	 var_refunds_before := 0 - var_refunds_before;
14:50:31 1811  
14:50:31 1812  	 var_charges_amount := 0;
14:50:31 1813  
14:50:31 1814  	 FOR f_processed_charges IN (
14:50:31 1815  	   SELECT
14:50:31 1816  	     CHARGE.CHARGE_AMOUNT
14:50:31 1817  	   FROM
14:50:31 1818  	     CHARGE
14:50:31 1819  	   WHERE
14:50:31 1820  	     CHARGE.INVOICE_ID = in_invoice_id
14:50:31 1821  	     AND CHARGE.CHARGE_AMOUNT > 0
14:50:31 1822  	     AND CHARGE.CHARGE_STATUS_ID IN (SELECT GLOBAL_STATUSES_V22.CHARGE_PROCESSED FROM DUAL)
14:50:31 1823  	     AND EXISTS (SELECT 1 FROM TRANSACTION_ATTEMPT ta where ta.transaction_id = CHARGE.TRANSACTION_ID and ta.transaction_attempt_status_id = GLOBAL_STATUSES_V22.TRANS_ATTEMPT_SUCCESS)
14:50:31 1824  	 )
14:50:31 1825  	 LOOP
14:50:31 1826  	   var_charges_amount := var_charges_amount + f_processed_charges.CHARGE_AMOUNT;
14:50:31 1827  	 END LOOP;
14:50:31 1828  
14:50:31 1829  	 IF (in_refund_amount + var_refunds_before > var_invoice_amount) THEN
14:50:31 1830  	   RAISE TOT_REF_IS_GREATER_THAN_ANOUNT;
14:50:31 1831  	 END IF;
14:50:31 1832  
14:50:31 1833  	 IF (in_refund_amount + var_refunds_before > var_charges_amount) THEN
14:50:31 1834  	   RAISE TOT_REF_IS_GRATER_THAN_CHARGES;
14:50:31 1835  	 END IF;
14:50:31 1836  
14:50:31 1837  	 BEGIN
14:50:31 1838  	   PROCS_TRANSACTION_V22.CREATE_TRANSACTION(
14:50:31 1839  	     in_transaction_id	       => NULL,
14:50:31 1840  	     in_status_id	       => GLOBAL_STATUSES_V22.TRANSACTION_PREPARE,
14:50:31 1841  	     in_amount		       => -in_refund_amount,
14:50:31 1842  	     in_created_by	       => in_created_by,
14:50:31 1843  	     in_order_id	       => NULL,
14:50:31 1844  	     in_is_refund	       => GLOBAL_CONSTANTS_V22.TRUE,
14:50:31 1845  	     in_transaction_type_code  => 'REFUND',
14:50:31 1846  	     out_transaction_id        => var_new_transaction_id
14:50:31 1847  	   );
14:50:31 1848  	   EXCEPTION
14:50:31 1849  	     WHEN OTHERS THEN
14:50:31 1850  	       EXCEPTION_MESSAGE := SQLERRM;
14:50:31 1851  	       RAISE CAN_NOT_CREATE_TRANSACTION;
14:50:31 1852  	 END;
14:50:31 1853  
14:50:31 1854  	 BEGIN
14:50:31 1855  	   PROCS_CHARGE_V22.CREATE_CHARGE(
