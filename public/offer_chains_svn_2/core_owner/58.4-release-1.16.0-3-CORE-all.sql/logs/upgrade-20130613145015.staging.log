14:50:30 1076  FUNCTION CHECK_FOR_SAME_PRODUCTS (
14:50:30 1077  	 in_offer_chain_1	  IN OFFER_CHAIN.ID%TYPE,
14:50:30 1078  	 in_offer_chain_2	  IN OFFER_CHAIN.ID%TYPE,
14:50:30 1079  	 in_use_eligibility_rules IN NUMBER DEFAULT GLOBAL_CONSTANTS_V22.FALSE
14:50:30 1080  ) RETURN NUMBER AS
14:50:30 1081  SPROC_NAME CONSTANT VARCHAR2(23) := 'CHECK_FOR_SAME_PRODUCTS';
14:50:30 1082  -- CONSTANTS
14:50:30 1083  PRODUCT_ELIGIBILITY_NAME CONSTANT VARCHAR2(19) := 'MAX_CONCURRENT_SUBS';
14:50:30 1084  -- VARIABLES
14:50:30 1085  temp_offer_chain_id	OFFER_CHAIN.ID%TYPE;
14:50:30 1086  var_same_products	SYS_REFCURSOR;
14:50:30 1087  var_same_product_id	NUMBER;
14:50:30 1088  same_product_count	NUMBER;
14:50:30 1089  var_product_eligibility_limit NUMBER;
14:50:30 1090  s_product_eligibility_limit   VARCHAR2(100);
14:50:30 1091  -- EXCEPTIONS
14:50:30 1092  BAD_FIRST_OFFER_CHAIN	      EXCEPTION;
14:50:30 1093  BAD_SECOND_OFFER_CHAIN	      EXCEPTION;
14:50:30 1094  BEGIN
14:50:30 1095  
14:50:30 1096  	 -- Check that first offer chain exists
14:50:30 1097  	 BEGIN
14:50:30 1098  	   SELECT
14:50:30 1099  	     OFFER_CHAIN.ID into temp_offer_chain_id
14:50:30 1100  	   FROM
14:50:30 1101  	     OFFER_CHAIN
14:50:30 1102  	   WHERE
14:50:30 1103  	     OFFER_CHAIN.ID = in_offer_chain_1;
14:50:30 1104  	   EXCEPTION
14:50:30 1105  	     WHEN NO_DATA_FOUND THEN
14:50:30 1106  	       RAISE BAD_FIRST_OFFER_CHAIN;
14:50:30 1107  	 END;
14:50:30 1108  
14:50:30 1109  	 -- Check that second offer chain exists
14:50:30 1110  	 BEGIN
14:50:30 1111  	   SELECT
14:50:30 1112  	     OFFER_CHAIN.ID into temp_offer_chain_id
14:50:30 1113  	   FROM
14:50:30 1114  	     OFFER_CHAIN
14:50:30 1115  	   WHERE
14:50:30 1116  	     OFFER_CHAIN.ID = in_offer_chain_2;
14:50:30 1117  	   EXCEPTION
14:50:30 1118  	     WHEN NO_DATA_FOUND THEN
14:50:30 1119  	       RAISE BAD_SECOND_OFFER_CHAIN;
14:50:30 1120  	 END;
14:50:30 1121  
14:50:30 1122  	 PROCS_OFFER_CHAIN_V22.GET_OFF_CHAINS_SAME_PRODUCTS(
14:50:30 1123  	   in_offer_chain_1 => in_offer_chain_1,
14:50:30 1124  	   in_offer_chain_2 => in_offer_chain_2,
14:50:30 1125  	   out_result_set   => var_same_products
14:50:30 1126  	 );
14:50:30 1127  
14:50:30 1128  	 LOOP
14:50:30 1129  	   FETCH var_same_products INTO var_same_product_id, same_product_count;
14:50:30 1130  	   EXIT WHEN var_same_products%NOTFOUND;
14:50:30 1131  
14:50:30 1132  	   IF in_use_eligibility_rules = GLOBAL_CONSTANTS_V22.FALSE THEN
14:50:30 1133  	     -- Return false because this offer chains having same products
14:50:30 1134  	     RETURN GLOBAL_CONSTANTS_V22.TRUE;
14:50:30 1135  	   ELSE
14:50:30 1136  
14:50:30 1137  	     -- Get eligibility rule for given product
14:50:30 1138  	     BEGIN
14:50:30 1139  	       SELECT
14:50:30 1140  		 PRODUCT_ELIGIBILITY.VALUE into s_product_eligibility_limit
14:50:30 1141  	       FROM
14:50:30 1142  		 PRODUCT_ELIGIBILITY
14:50:30 1143  	       WHERE
14:50:30 1144  		 PRODUCT_ELIGIBILITY.PRODUCT_ID = var_same_product_id
14:50:30 1145  		 AND PRODUCT_ELIGIBILITY.NAME = PRODUCT_ELIGIBILITY_NAME;
14:50:30 1146  
14:50:30 1147  	       -- REVU: What should to be here? 1?
14:50:30 1148  	       EXCEPTION
14:50:30 1149  		 WHEN NO_DATA_FOUND THEN
14:50:30 1150  		   s_product_eligibility_limit := '1';
14:50:30 1151  	     END;
14:50:30 1152  
14:50:30 1153  	     IF UPPER(s_product_eligibility_limit) = GLOBAL_CONSTANTS_V22.MAX_CONSURRENT_PRD_UNLIM THEN
14:50:30 1154  	       RETURN GLOBAL_CONSTANTS_V22.FALSE;
14:50:30 1155  	     END IF;
14:50:30 1156  
14:50:30 1157  	     var_product_eligibility_limit := TO_NUMBER(s_product_eligibility_limit);
14:50:30 1158  
14:50:30 1159  	     -- Check for limit
14:50:30 1160  	     IF var_product_eligibility_limit < same_product_count THEN
14:50:30 1161  	       RETURN GLOBAL_CONSTANTS_V22.TRUE;
14:50:30 1162  	     END IF;
14:50:30 1163  
14:50:30 1164  	   END IF;
14:50:30 1165  	 END LOOP;
14:50:30 1166  
14:50:30 1167  	 RETURN GLOBAL_CONSTANTS_V22.FALSE;
14:50:30 1168  
14:50:30 1169  EXCEPTION
14:50:30 1170  WHEN BAD_FIRST_OFFER_CHAIN THEN
14:50:30 1171  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:30 1172  	   SPROC_NAME, 'First offer chain not found');
14:50:30 1173  WHEN BAD_SECOND_OFFER_CHAIN THEN
14:50:30 1174  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:30 1175  	   SPROC_NAME, 'Second offer chain not found');
14:50:30 1176  WHEN OTHERS THEN
14:50:30 1177  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:30 1178  	   SPROC_NAME, 'Unknown error', SQLERRM);
14:50:30 1179  END CHECK_FOR_SAME_PRODUCTS;
