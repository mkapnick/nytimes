14:50:30 916  var_offer_years	 NUMBER;
14:50:30 917  var_offer_months	 NUMBER;
14:50:30 918  var_offer_days	 NUMBER;
14:50:30 919  var_offer_end_date DATE;
14:50:30 920  -- EXCEPTIONS
14:50:30 921  BAD_OFFER_ID EXCEPTION;
14:50:30 922  BEGIN
14:50:30 923  
14:50:30 924  	BEGIN
14:50:30 925  	  SELECT
14:50:30 926  	    OFFER.ENTITLEMENT_DURATION into var_offer_duration
14:50:30 927  	  FROM
14:50:30 928  	    OFFER
14:50:30 929  	  WHERE
14:50:30 930  	    OFFER.ID = in_offer_id;
14:50:30 931  	  EXCEPTION
14:50:30 932  	    WHEN NO_DATA_FOUND THEN
14:50:30 933  	      RAISE BAD_OFFER_ID;
14:50:30 934  	END;
14:50:30 935  
14:50:30 936  	PROCS_COMMON_V22.ISO8601DURATION_TO_INTERVALS (
14:50:30 937  	  var_offer_duration,
14:50:30 938  	  var_offer_years,
14:50:30 939  	  var_offer_months,
14:50:30 940  	  var_offer_days
14:50:30 941  	);
14:50:30 942  
14:50:30 943  	var_offer_end_date := ( ( in_start_date
14:50:30 944  	  + GLOBAL_CONSTANTS_V22.ONE_DAY_INTERVAL * var_offer_days )
14:50:30 945  	  + GLOBAL_CONSTANTS_V22.ONE_MONTH_INTERVAL * var_offer_months )
14:50:30 946  	  + GLOBAL_CONSTANTS_V22.ONE_YEAR_INTERVAL * var_offer_years;
14:50:30 947  
14:50:30 948  	out_days := var_offer_end_date - in_start_date;
