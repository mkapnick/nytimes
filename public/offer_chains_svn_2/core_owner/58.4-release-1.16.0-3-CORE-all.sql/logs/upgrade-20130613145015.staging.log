14:50:31 2572  
14:50:31 2573  FUNCTION IS_SUBSCRIPTION_CANCELABLE (
14:50:31 2574  	 in_subscription_id IN NUMBER
14:50:31 2575  ) RETURN NUMBER AS
14:50:31 2576  SPROC_NAME CONSTANT VARCHAR2(26) := 'IS_SUBSCRIPTION_CANCELABLE';
14:50:31 2577  -- VARIABLES
14:50:31 2578  var_is_offer_chain_cancelable NUMBER;
14:50:31 2579  --64603
14:50:31 2580  var_end_date date;
14:50:31 2581  today_date date := current_date;
14:50:31 2582  offer_id number;
14:50:31 2583  -- EXCEPTIONS
14:50:31 2584  COULD_NOT_CHECK	   EXCEPTION;
14:50:31 2585  BAD_SUBSCRIPTION_ID EXCEPTION;
14:50:31 2586  EXCEPTION_MESSAGE   VARCHAR2(1024);
14:50:31 2587  BEGIN
14:50:31 2588  
14:50:31 2589  	 BEGIN
14:50:31 2590  	   -- find offer_chain_id for given in_subscription_id
14:50:31 2591  	   SELECT OFFER_CHAIN_ID into offer_id
14:50:31 2592  	   FROM SUBSCRIPTION
14:50:31 2593  	   WHERE ID = in_subscription_id;
14:50:31 2594  	   EXCEPTION
14:50:31 2595  	     WHEN NO_DATA_FOUND THEN
14:50:31 2596  	       RAISE BAD_SUBSCRIPTION_ID;
14:50:31 2597  	     WHEN OTHERS THEN
14:50:31 2598  	       EXCEPTION_MESSAGE := SQLERRM;
14:50:31 2599  	       RAISE COULD_NOT_CHECK;
14:50:31 2600  	 END;
14:50:31 2601  
14:50:31 2602  	 -- find if it was redeemed from a gift certificate
14:50:31 2603  	 BEGIN
14:50:31 2604  	   SELECT l.end_date INTO var_end_date
14:50:31 2605  	   FROM LICENSE l, GIFT_CERTIFICATE g
14:50:31 2606  	   WHERE l.invoice_id = g.finalized_invoice_id
14:50:31 2607  	   AND l.subscription_id = in_subscription_id;
14:50:31 2608  
14:50:31 2609  	   -- if the license end_date is bigger than today, we are in the
14:50:31 2610  	   -- first period, so we cannot cancel; otherwise can cancel
14:50:31 2611  	   IF var_end_date > today_date THEN
14:50:31 2612  	       RETURN GLOBAL_CONSTANTS_V22.FALSE;
14:50:31 2613  	   ELSE
14:50:31 2614  	       RETURN GLOBAL_CONSTANTS_V22.TRUE;
14:50:31 2615  	   END IF;
14:50:31 2616  
14:50:31 2617  	   EXCEPTION
14:50:31 2618  	       -- not coming from a gift certificate,
14:50:31 2619  	       -- use old logic
14:50:31 2620  	       WHEN NO_DATA_FOUND THEN
14:50:31 2621  		   SELECT
14:50:31 2622  		       PROCS_OFFER_CHAIN_V22.IS_OFFER_CHAIN_CANCELABLE(offer_id)
14:50:31 2623  		       INTO var_is_offer_chain_cancelable
14:50:31 2624  		   FROM DUAL;
14:50:31 2625  		   RETURN var_is_offer_chain_cancelable;
14:50:31 2626  	 END;
14:50:31 2627  
14:50:31 2628  EXCEPTION
14:50:31 2629  WHEN BAD_SUBSCRIPTION_ID THEN
14:50:31 2630  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 2631  	   SPROC_NAME, 'No such subscription');
14:50:31 2632  WHEN COULD_NOT_CHECK THEN
14:50:31 2633  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 2634  	   SPROC_NAME, 'Could not check if offer chain calcelable', EXCEPTION_MESSAGE);
14:50:31 2635  WHEN OTHERS THEN
14:50:31 2636  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:31 2637  	   SPROC_NAME, 'Unknown error', SQLERRM);
14:50:31 2638  
14:50:31 2639  END IS_SUBSCRIPTION_CANCELABLE;
