14:50:31 2823  PROCEDURE ARE_REFUNDS_PENDING_FOR_SUBSCR (
14:50:31 2824  /*
14:50:31 2825  APP_EXCEPTION_CODES_V22.NOT_FOUND
14:50:31 2826  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
14:50:31 2827  */
14:50:31 2828  	 in_subscription_id IN NUMBER,
14:50:31 2829  	 out_result	    OUT NUMBER
14:50:31 2830  ) AS
14:50:31 2831  SPROC_NAME CONSTANT VARCHAR2(30) := 'ARE_REFUNDS_PENDING_FOR_SUBSCR';
14:50:31 2832  -- VARIABLES
14:50:31 2833  temp_subscription_id NUMBER;
14:50:31 2834  var_local_result     NUMBER;
14:50:31 2835  -- EXCEPTIONS
14:50:31 2836  BAD_SUBSCRIPTION_ID EXCEPTION;
14:50:31 2837  BEGIN
14:50:31 2838  
14:50:31 2839  	 -- Check that subscription exists
14:50:31 2840  	 BEGIN
14:50:31 2841  	   SELECT
14:50:31 2842  	     SUBSCRIPTION.ID into temp_subscription_id
14:50:31 2843  	   FROM
14:50:31 2844  	     SUBSCRIPTION
14:50:31 2845  	   WHERE
14:50:31 2846  	     SUBSCRIPTION.ID = in_subscription_id;
14:50:31 2847  	   EXCEPTION
14:50:31 2848  	     WHEN NO_DATA_FOUND THEN
14:50:31 2849  	       RAISE BAD_SUBSCRIPTION_ID;
14:50:31 2850  	 END;
14:50:31 2851  
14:50:31 2852  	 var_local_result := NULL;
14:50:31 2853  
14:50:31 2854  	 -- Check charges for each invoice associated with gived subscription
14:50:31 2855  	 FOR f_invoice IN (
14:50:31 2856  	   SELECT DISTINCT
14:50:31 2857  	     LICENSE.INVOICE_ID as "ID"
14:50:31 2858  	   FROM
14:50:31 2859  	     LICENSE
14:50:31 2860  	   WHERE
14:50:31 2861  	     LICENSE.SUBSCRIPTION_ID = in_subscription_id
14:50:31 2862  	     AND LICENSE.LICENSE_STATUS_ID IN ( SELECT GLOBAL_STATUSES_V22.LICENSE_ACTIVE FROM DUAL )
14:50:31 2863  	 )
14:50:31 2864  	 LOOP
14:50:31 2865  
14:50:31 2866  	   -- Check each charge in invoice
14:50:31 2867  	   FOR f_charge IN (
14:50:31 2868  	     SELECT
14:50:31 2869  	       CHARGE.ID,
14:50:31 2870  	       CHARGE.CHARGE_STATUS_ID,
14:50:31 2871  	       CHARGE.CHARGE_AMOUNT
14:50:31 2872  	     FROM
14:50:31 2873  	       CHARGE
14:50:31 2874  	     WHERE
14:50:31 2875  	       CHARGE.INVOICE_ID = f_invoice.ID
14:50:31 2876  	   )
14:50:31 2877  	   LOOP
14:50:31 2878  
14:50:31 2879  	     -- Charge amount < 0     => it is a refund
14:50:31 2880  	     -- Charge status is OPEN => means that it is not processed yet
14:50:31 2881  	     IF f_charge.CHARGE_AMOUNT < 0
14:50:31 2882  		AND f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V22.CHARGE_OPENED THEN
14:50:31 2883  	       var_local_result := GLOBAL_CONSTANTS_V22.TRUE;
14:50:31 2884  	     END IF;
14:50:31 2885  
14:50:31 2886  	   END LOOP;
14:50:31 2887  
14:50:31 2888  	 END LOOP;
14:50:31 2889  
14:50:31 2890  	 IF var_local_result IS NULL THEN
14:50:31 2891  	   out_result := GLOBAL_CONSTANTS_V22.FALSE;
14:50:31 2892  	 ELSE
14:50:31 2893  	   out_result := GLOBAL_CONSTANTS_V22.TRUE;
14:50:31 2894  	 END IF;
14:50:31 2895  
14:50:31 2896  EXCEPTION
14:50:31 2897  WHEN BAD_SUBSCRIPTION_ID THEN
14:50:31 2898  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 2899  	   SPROC_NAME, 'No such transaction id');
14:50:31 2900  WHEN OTHERS THEN
14:50:31 2901  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:31 2902  	   SPROC_NAME, 'Unknown error', SQLERRM);
14:50:31 2903  END ARE_REFUNDS_PENDING_FOR_SUBSCR;
