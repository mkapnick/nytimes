14:50:31 906  	in_created_by IN GROUP_ACCOUNT_IP_RANGE.CREATED_BY%TYPE
14:50:31 907  ) AS
14:50:31 908  SPROC_NAME CONSTANT VARCHAR2(32) := 'ADD_IP_RANGE';
14:50:31 909  var_ga_type OFFER_CHAIN.GROUP_ACCOUNT_TYPE_ID%TYPE;
14:50:31 910  var_is_dupe NUMBER(1);
14:50:31 911  NOT_GL EXCEPTION;
