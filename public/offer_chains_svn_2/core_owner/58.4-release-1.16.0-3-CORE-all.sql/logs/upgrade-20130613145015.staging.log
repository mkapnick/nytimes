14:50:31 2943  /*
14:50:31 2944  	   Note, this returns paypal records which are in the given window of time
14:50:31 2945  	   based on their creation date, not their expiration date, because we've seen
14:50:31 2946  	   suspicious values in the data for expiration dates. The java layer should
14:50:31 2947  	   inspect the expiration dates and decide whether or not to act on the data.
14:50:31 2948  */
14:50:31 2949  
14:50:31 2950  PROCEDURE GET_EXPIRING_PAYPAL (
14:50:31 2951  	 in_expire_window_days	 IN NUMBER,
14:50:31 2952  	 in_creation_limit_days  IN NUMBER,
14:50:31 2953  	 in_retry_throttle_name  IN PROCESS_RETRY_THROTTLE.PROCESS_NAME%TYPE,
14:50:31 2954  	 out_result_set 	 OUT SYS_REFCURSOR
14:50:31 2955  ) AS
14:50:31 2956  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_EXPIRING_PAYPAL';
14:50:31 2957  var_rows_to_return CONSTANT NUMBER := 1000;
14:50:31 2958  BEGIN
14:50:31 2959  	 OPEN out_result_set FOR
14:50:31 2960  	 SELECT * FROM (
14:50:31 2961  	       SELECT
14:50:31 2962  	       ac.group_id	   AS group_id,
14:50:31 2963  	       pp.id		   AS paypal_id,
14:50:31 2964  	       sb.id		   AS subscription_id,
14:50:31 2965  	       pp.create_date	   AS paypal_creation_date,
14:50:31 2966  	       pp.expiration_date  AS paypal_expiration_date,
14:50:31 2967  	       lc.end_date	   AS license_end_date
14:50:31 2968  	   FROM license lc
14:50:31 2969  	   INNER JOIN subscription sb
14:50:31 2970  	   ON lc.subscription_id = sb.id
14:50:31 2971  	   INNER JOIN paypal pp
14:50:31 2972  	   ON pp.account_id = sb.account_id
14:50:31 2973  	   INNER JOIN account ac
14:50:31 2974  	   ON pp.account_id = ac.id
14:50:31 2975  	   WHERE SYSDATE <= lc.end_date
14:50:31 2976  	     AND SYSDATE + in_expire_window_days > lc.end_date
14:50:31 2977  	     AND pp.paypal_status_id = GLOBAL_STATUSES_V22.PAYPAL_ACTIVE
14:50:31 2978  	     AND lc.license_status_id IN (GLOBAL_STATUSES_V22.LICENSE_ACTIVE,
14:50:31 2979  					  GLOBAL_STATUSES_V22.LICENSE_IN_GRACE_PERIOD)
14:50:31 2980  	     AND pp.paypal_status_id = GLOBAL_STATUSES_V22.PAYPAL_ACTIVE
14:50:31 2981  	     AND sb.instrument_type_id = GLOBAL_ENUMS_V22.INSTRUMENT_PAYPAL
14:50:31 2982  	     AND sb.subscription_status_id IN (GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE,
14:50:31 2983  					       GLOBAL_STATUSES_V22.SUBSCRIPTION_IN_GRACE_PERIOD)
14:50:31 2984  	     AND pp.create_date + in_creation_limit_days <= lc.end_date
14:50:31 2985  	     AND NOT EXISTS(
14:50:31 2986  	       SELECT NULL
14:50:31 2987  	       FROM process_retry_throttle rt
14:50:31 2988  	       WHERE rt.process_name = in_retry_throttle_name
14:50:31 2989  		 AND rt.generic_id = pp.id
14:50:31 2990  	     )
14:50:31 2991  	     AND ROWNUM <= 10 * var_rows_to_return
14:50:31 2992  	   ORDER BY dbms_random.value
14:50:31 2993  	 )
14:50:31 2994  	 WHERE ROWNUM <= var_rows_to_return;
14:50:31 2995  END GET_EXPIRING_PAYPAL;
