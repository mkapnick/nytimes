14:50:31 1345  	   SELECT
14:50:31 1346  	     var_subscrib_group_id as "GROUP_ID"
14:50:31 1347  	   FROM
14:50:31 1348  	     DUAL
14:50:31 1349  	   UNION
14:50:31 1350  	   SELECT
14:50:31 1351  	     var_gc_purchaser_group_id as "GROUP_ID"
14:50:31 1352  	   FROM
14:50:31 1353  	     DUAL
14:50:31 1354  	   UNION
14:50:31 1355  	   SELECT
14:50:31 1356  	     var_gc_redeemer_group_id as "GROUP_ID"
14:50:31 1357  	   FROM
14:50:31 1358  	     DUAL
14:50:31 1359  	 )
14:50:31 1360  	 WHERE
14:50:31 1361  	   GROUP_ID IS NOT NULL;
14:50:31 1362  
14:50:31 1363  EXCEPTION
14:50:31 1364  WHEN BAD_INVOICE_ID THEN
14:50:31 1365  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 1366  	   SPROC_NAME, 'No such invoice');
14:50:31 1367  WHEN CAN_NOT_FIND_ACCOUNT THEN
