14:50:31 1376  WHEN OCH_IS_NOT_GIFT_CERTIFICATE THEN
14:50:31 1377  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INVALID_PARAMETER,
14:50:31 1378  	   SPROC_NAME, 'This offer chain can not be used for gift certificate');
14:50:31 1379  WHEN CAN_NOT_PURCHASE_GC_FOR_RDMPN THEN
14:50:31 1380  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INVALID_PARAMETER,
