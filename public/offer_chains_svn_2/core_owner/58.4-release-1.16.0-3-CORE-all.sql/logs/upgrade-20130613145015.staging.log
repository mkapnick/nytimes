14:50:30 209  PROCEDURE GET_ENDING_LICENSES (
14:50:30 210  	in_hours_number IN NUMBER,
14:50:30 211  	out_result_set	OUT SYS_REFCURSOR
14:50:30 212  ) AS
14:50:30 213  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_ENDING_LICENSES';
14:50:30 214  -- VARIABLES
14:50:30 215  var_days		NUMBER;
14:50:30 216  var_hours 	NUMBER;
14:50:30 217  var_time_interval INTERVAL DAY (3) TO SECOND;
14:50:30 218  BEGIN
14:50:30 219  
14:50:30 220  	var_hours := mod(in_hours_number,24);
14:50:30 221  	var_days := (in_hours_number - var_hours) / 24;
14:50:30 222  	var_time_interval := var_days||' '||var_hours||':0:0';
14:50:30 223  
14:50:30 224  	OPEN out_result_set FOR
14:50:30 225  SELECT * FROM
14:50:30 226  (
14:50:30 227  	SELECT
14:50:30 228  	  LICENSE.ID,
14:50:30 229  	  LICENSE.CREATE_DATE,
14:50:30 230  	  LICENSE.CREATED_BY,
14:50:30 231  	  LICENSE.CURRENT_OFFER_INDEX,
14:50:30 232  	  LICENSE.CURRENT_OFFER_RECURR_NUM,
14:50:30 233  	  LICENSE.END_DATE,
14:50:30 234  	  LICENSE.ENTITLEMENT_END_DATE,
14:50:30 235  	  LICENSE.INVOICE_ID,
14:50:30 236  	  LICENSE.IS_EXTENSION,
