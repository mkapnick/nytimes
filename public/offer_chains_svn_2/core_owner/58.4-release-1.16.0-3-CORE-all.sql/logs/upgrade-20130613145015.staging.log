14:50:31 721  	) AS
14:50:31 722  	  SPROC_NAME CONSTANT VARCHAR2(19) := 'START_GC_PURCHASING';
14:50:31 723  -- VARIABLES
14:50:31 724  	  var_account_id	    NUMBER;
14:50:31 725  	  temp_gc_code		    GIFT_CERTIFICATE.CODE%TYPE;
14:50:31 726  	  var_och_is_gc 	    NUMBER;
14:50:31 727  	  var_offer_chain_status_id NUMBER;
14:50:31 728  	  var_is_for_redemption     NUMBER;
14:50:31 729  	  var_new_invoice_id	    NUMBER;
14:50:31 730  	  var_gift_cert_id	    NUMBER;
14:50:31 731  	  var_account_tax_exempt_id VARCHAR2(255);
14:50:31 732  -- EXCEPTIONS
14:50:31 733  	    BAD_GROUP_ID		  EXCEPTION;
14:50:31 734  	    GC_CODE_ALREADY_EXISTS	  EXCEPTION;
14:50:31 735  	    BAD_OFFER_CHAIN_ID		  EXCEPTION;
14:50:31 736  	    OCH_IS_NOT_GIFT_CERTIFICATE   EXCEPTION;
14:50:31 737  	    BAD_OFFER_CHAIN_STATUS	  EXCEPTION;
14:50:31 738  	    CAN_NOT_PURCHASE_GC_FOR_RDMPN EXCEPTION;
14:50:31 739  	    CAN_NOT_CREATE_INVOICE	  EXCEPTION;
14:50:31 740  	    OFFER_REC_NUM_LESS_THAN_ONE   EXCEPTION;
14:50:31 741  	    CAN_NOT_CREATE_LINE_ITEMS	  EXCEPTION;
14:50:31 742  	    REF_CODE_ALREADY_EXISTS	  EXCEPTION;
14:50:31 743  
14:50:31 744  	  EXCEPTION_MESSAGE VARCHAR2(1024);
14:50:31 745  	  BEGIN
14:50:31 746  -- Get account id
14:50:31 747  	    BEGIN
14:50:31 748  	      SELECT
14:50:31 749  		ACCOUNT.ID,
14:50:31 750  		ACCOUNT.TAX_EXEMPT_ID
14:50:31 751  	      into
14:50:31 752  		var_account_id,
14:50:31 753  		var_account_tax_exempt_id
14:50:31 754  	      FROM
14:50:31 755  		ACCOUNT
14:50:31 756  	      WHERE
14:50:31 757  		ACCOUNT.GROUP_ID = in_group_id;
14:50:31 758  	      EXCEPTION
14:50:31 759  	      WHEN NO_DATA_FOUND THEN
14:50:31 760  	      RAISE BAD_GROUP_ID;
14:50:31 761  	    END;
14:50:31 762  
14:50:31 763  -- Check for the same code
14:50:31 764  	    BEGIN
14:50:31 765  	      SELECT
14:50:31 766  		GIFT_CERTIFICATE.CODE into temp_gc_code
14:50:31 767  	      FROM
14:50:31 768  		GIFT_CERTIFICATE
14:50:31 769  	      WHERE
14:50:31 770  		GIFT_CERTIFICATE.CODE = in_gift_certificate_code;
14:50:31 771  
14:50:31 772  	      RAISE GC_CODE_ALREADY_EXISTS;
14:50:31 773  
14:50:31 774  	      EXCEPTION
14:50:31 775  	      WHEN NO_DATA_FOUND THEN
14:50:31 776  	      NULL;
14:50:31 777  	    END;
14:50:31 778  
14:50:31 779  -- Get offer chain flag "is_gift_certificate"
14:50:31 780  	    BEGIN
14:50:31 781  	      SELECT
14:50:31 782  		OFFER_CHAIN.IS_GIFT_CERTIFICATE,
14:50:31 783  		OFFER_CHAIN.OFFER_CHAIN_STATUS_ID
14:50:31 784  	      into
14:50:31 785  		var_och_is_gc,
14:50:31 786  		var_offer_chain_status_id
14:50:31 787  	      FROM
14:50:31 788  		OFFER_CHAIN
14:50:31 789  	      WHERE
14:50:31 790  		OFFER_CHAIN.ID = in_offer_chain_id;
14:50:31 791  	      EXCEPTION
14:50:31 792  	      WHEN NO_DATA_FOUND THEN
14:50:31 793  	      RAISE BAD_OFFER_CHAIN_ID;
14:50:31 794  	    END;
14:50:31 795  
14:50:31 796  	    IF var_och_is_gc != GLOBAL_CONSTANTS_V22.TRUE
14:50:31 797  	       OR var_och_is_gc IS NULL THEN
14:50:31 798  	      RAISE OCH_IS_NOT_GIFT_CERTIFICATE;
14:50:31 799  	    END IF;
14:50:31 800  
14:50:31 801  	    IF var_offer_chain_status_id != GLOBAL_STATUSES_V22.OFFER_CHAIN_ACTIVE THEN
14:50:31 802  	      RAISE BAD_OFFER_CHAIN_STATUS;
14:50:31 803  	    END IF;
14:50:31 804  
14:50:31 805  -- norlov: #38151 check if the OC is for Redemption:
14:50:31 806  	    SELECT
14:50:31 807  	      COUNT(*) into var_is_for_redemption
14:50:31 808  	    FROM
14:50:31 809  	      OFFER_CHAIN_ELIGIBILITY
14:50:31 810  	    WHERE
14:50:31 811  	      OFFER_CHAIN_ELIGIBILITY.OFFER_CHAIN_ID = in_offer_chain_id
14:50:31 812  	      AND OFFER_CHAIN_ELIGIBILITY.NAME = GLOBAL_CONSTANTS_V22.GIFT_CERTIFICATE_REQUIRED
14:50:31 813  	      AND OFFER_CHAIN_ELIGIBILITY.VALUE = GLOBAL_CONSTANTS_V22.ELIGIBILITY_FLAG_SET;
14:50:31 814  
14:50:31 815  	    IF var_is_for_redemption > 0 THEN
14:50:31 816  	      RAISE CAN_NOT_PURCHASE_GC_FOR_RDMPN;
14:50:31 817  	    END IF;
14:50:31 818  
14:50:31 819  -- Create new invoice
14:50:31 820  	    BEGIN
14:50:31 821  	      PROCS_INVOICE_V22.CREATE_INVOICE(
14:50:31 822  		  in_invoice_status => GLOBAL_STATUSES_V22.INVOICE_OPEN,
14:50:31 823  		  in_created_by     => in_created_by,
14:50:31 824  		  in_tax_exempt_id  => var_account_tax_exempt_id,
14:50:31 825  		  out_invoice_id    => var_new_invoice_id
14:50:31 826  	      );
14:50:31 827  	      EXCEPTION
14:50:31 828  	      WHEN OTHERS THEN
14:50:31 829  	      EXCEPTION_MESSAGE := SQLERRM;
14:50:31 830  	      RAISE CAN_NOT_CREATE_INVOICE;
14:50:31 831  	    END;
14:50:31 832  
14:50:31 833  -- Add line items for new invoice
14:50:31 834  	    BEGIN
14:50:31 835  	      FOR f_offer_data IN (
14:50:31 836  	      SELECT
14:50:31 837  		OFFER_ID,
14:50:31 838  		NUM_RECURRENCES
14:50:31 839  	      FROM
14:50:31 840  		OFFER_OFFER_CHAIN
14:50:31 841  	      WHERE
14:50:31 842  		OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
14:50:31 843  	      )
14:50:31 844  	      LOOP
14:50:31 845  	      IF f_offer_data.NUM_RECURRENCES < 1 THEN
14:50:31 846  		RAISE OFFER_REC_NUM_LESS_THAN_ONE;
14:50:31 847  	      END IF;
14:50:31 848  	      FOR i_offer_recurrences_iterator IN 1..f_offer_data.NUM_RECURRENCES
14:50:31 849  	      LOOP
14:50:31 850  		PROCS_LINE_ITEMS_V22.ADD_LINE_ITEMS(
14:50:31 851  		    in_invoice_id => var_new_invoice_id,
14:50:31 852  		    in_offer_id   => f_offer_data.OFFER_ID,
14:50:31 853  		    in_created_by => in_created_by
14:50:31 854  		);
14:50:31 855  	      END LOOP;
14:50:31 856  	      END LOOP;
14:50:31 857  
14:50:31 858  	      EXCEPTION
14:50:31 859  	      WHEN OTHERS THEN
14:50:31 860  	      EXCEPTION_MESSAGE := SQLERRM;
14:50:31 861  	      RAISE CAN_NOT_CREATE_LINE_ITEMS;
14:50:31 862  	    END;
14:50:31 863  
14:50:31 864  	    BEGIN
14:50:31 865  	      PROCS_FIN_INSTRUMENTS_CRU_V22.CREATE_GIFT_CERTIFICATE(
14:50:31 866  		  out_gift_certificate_id	=> var_gift_cert_id,
14:50:31 867  		  in_purchaser_group_id 	=> in_group_id,
14:50:31 868  		  in_purchaser_invoice_id	=> var_new_invoice_id,
14:50:31 869  		  in_offer_chain_id		=> in_offer_chain_id,
14:50:31 870  		  in_expiration_date		=> in_expiration_date,
14:50:31 871  		  in_purchase_date		=> SYSDATE,
14:50:31 872  		  in_gift_certificate_status_id => GLOBAL_STATUSES_V22.GIFT_CERTIFICATE_ACTIVE,
14:50:31 873  		  in_code			=> in_gift_certificate_code,
14:50:31 874  		  in_created_by 		=> in_created_by,
14:50:31 875  		  in_recipient_name		=> in_recipient_name,
14:50:31 876  		  in_gift_message		=> in_gift_message,
14:50:31 877  		  in_recipient_email		=> in_recipient_email,
14:50:31 878  		  in_sender_email		=> in_sender_email,
14:50:31 879  		  in_sender_name		=> in_sender_name,
14:50:31 880  		  in_recipient_address_id	=> in_recipient_address_id,
14:50:31 881  		  in_recipient_notify_date	=> in_recipient_notify_date,
14:50:31 882  		  in_campaign			=> in_campaign,
14:50:31 883  		  in_reference_code		=> in_reference_code
14:50:31 884  	      );
14:50:31 885  	      EXCEPTION WHEN DUP_VAL_ON_INDEX THEN RAISE REF_CODE_ALREADY_EXISTS;
14:50:31 886  	    END;
14:50:31 887  
14:50:31 888  	    out_gift_certificate_id := var_gift_cert_id;
14:50:31 889  	    out_invoice_id := var_new_invoice_id;
14:50:31 890  
14:50:31 891  	    EXCEPTION
14:50:31 892  	    WHEN BAD_OFFER_CHAIN_STATUS THEN
14:50:31 893  	    PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.STATE_ERROR,
14:50:31 894  					     SPROC_NAME, 'Offer chain is not active');
14:50:31 895  	    WHEN GC_CODE_ALREADY_EXISTS THEN
14:50:31 896  	    PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.DUPLICATE_ERROR,
14:50:31 897  					     SPROC_NAME, 'Gift certificate with same code already exists');
14:50:31 898  	    WHEN OCH_IS_NOT_GIFT_CERTIFICATE THEN
14:50:31 899  	    PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INVALID_PARAMETER,
14:50:31 900  					     SPROC_NAME, 'This offer chain can not be used for gift certificate');
14:50:31 901  	    WHEN CAN_NOT_PURCHASE_GC_FOR_RDMPN THEN
14:50:31 902  	    PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INVALID_PARAMETER,
14:50:31 903  					     SPROC_NAME, 'This offer chain can not be purchased for gift certificate since it is for redemption');
14:50:31 904  	    WHEN CAN_NOT_CREATE_INVOICE THEN
14:50:31 905  	    PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 906  					     SPROC_NAME, 'Could not create new invoice', EXCEPTION_MESSAGE);
14:50:31 907  	    WHEN CAN_NOT_CREATE_LINE_ITEMS THEN
14:50:31 908  	    PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 909  					     SPROC_NAME, 'Could not create line items', EXCEPTION_MESSAGE);
14:50:31 910  	    WHEN BAD_GROUP_ID THEN
14:50:31 911  	    PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 912  					     SPROC_NAME, 'No such group id');
14:50:31 913  	    WHEN BAD_OFFER_CHAIN_ID THEN
14:50:31 914  	    PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 915  					     SPROC_NAME, 'No such offer chain');
14:50:31 916  	    WHEN OFFER_REC_NUM_LESS_THAN_ONE THEN
14:50:31 917  	    PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 918  					     SPROC_NAME, 'Some offer has recurrences number less than 1');
14:50:31 919  	    WHEN REF_CODE_ALREADY_EXISTS THEN
14:50:31 920  	    PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:31 921  					     SPROC_NAME, 'reference_code already used');
14:50:31 922  	    WHEN OTHERS THEN
14:50:31 923  	    PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:31 924  					     SPROC_NAME, 'Unknown error', SQLERRM);
14:50:31 925  	  END START_GC_PURCHASING;
14:50:31 926  /******************************************************************************/
14:50:31 927  
14:50:31 928  	PROCEDURE FINALIZE_GC_PURCHASING (
14:50:31 929  	  in_invoice_id 	IN NUMBER,
14:50:31 930  	  in_created_by 	IN VARCHAR2,
14:50:31 931  	  in_instrument_id	IN NUMBER,
14:50:31 932  	  in_instrument_type_id IN NUMBER,
14:50:31 933  	  in_order_id		IN VARCHAR2,
14:50:31 934  	  in_transaction_id	IN NUMBER,
14:50:31 935  	  out_charge_amount	OUT NUMBER
14:50:31 936  	) AS
14:50:31 937  	  SPROC_NAME CONSTANT VARCHAR2(22) := 'FINALIZE_GC_PURCHASING';
14:50:31 938  -- VARIABLES
14:50:31 939  	  temp_transaction_id_count NUMBER;
14:50:31 940  	  var_invoice_amount	    NUMBER(10,2);
14:50:31 941  	  var_transaction_id	    NUMBER;
14:50:31 942  	  var_new_charge_id	    NUMBER;
14:50:31 943  -- EXCEPTIONS
14:50:31 944  	    BAD_CREDIT_CARD_ID		EXCEPTION;
14:50:31 945  	    BAD_PAYPAL_ID		EXCEPTION;
14:50:31 946  	    BAD_INSTRUMENT_TYPE 	EXCEPTION;
14:50:31 947  	    TRANSACTION_EXISTS		EXCEPTION;
14:50:31 948  	    CAN_NOT_CALC_INVOICE_AMOUNT EXCEPTION;
14:50:31 949  	    CAN_NOT_USE_FCINSTR 	EXCEPTION;
14:50:31 950  	    CAN_NOT_CREATE_TRANSACTION	EXCEPTION;
14:50:31 951  	    CAN_NOT_CREATE_CHARGE	EXCEPTION;
14:50:31 952  	  EXCEPTION_MESSAGE   VARCHAR2(1024);
14:50:31 953  	  BEGIN
14:50:31 954  
14:50:31 955  -- Check that instrument exists
14:50:31 956  	    IF in_instrument_type_id = GLOBAL_ENUMS_V22.INSTRUMENT_CREDIT_CARD THEN
14:50:31 957  	      IF IS_CREDIT_CARD_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V22.FALSE THEN
14:50:31 958  		RAISE BAD_CREDIT_CARD_ID;
14:50:31 959  	      END IF;
14:50:31 960  	    ELSIF in_instrument_type_id = GLOBAL_ENUMS_V22.INSTRUMENT_PAYPAL THEN
14:50:31 961  	      IF IS_PAYPAL_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V22.FALSE THEN
14:50:31 962  		RAISE BAD_PAYPAL_ID;
14:50:31 963  	      END IF;
14:50:31 964  	    ELSIF in_instrument_type_id = GLOBAL_ENUMS_V22.INSTRUMENT_ZCI_INSTRUMENT THEN
14:50:31 965  	      NULL;
14:50:31 966  	    ELSE
14:50:31 967  	      RAISE BAD_INSTRUMENT_TYPE;
14:50:31 968  	    END IF;
14:50:31 969  
14:50:31 970  -- Check that transaction with given id do not exists
14:50:31 971  	    SELECT
14:50:31 972  	      COUNT(*) into temp_transaction_id_count
14:50:31 973  	    FROM
14:50:31 974  	      TRANSACTION
14:50:31 975  	    WHERE
14:50:31 976  	      TRANSACTION.ID = in_transaction_id;
14:50:31 977  
14:50:31 978  	    IF temp_transaction_id_count > 0 THEN
14:50:31 979  	      RAISE TRANSACTION_EXISTS;
14:50:31 980  	    END IF;
14:50:31 981  
14:50:31 982  -- Calculate new invoice amount
14:50:31 983  	    BEGIN
