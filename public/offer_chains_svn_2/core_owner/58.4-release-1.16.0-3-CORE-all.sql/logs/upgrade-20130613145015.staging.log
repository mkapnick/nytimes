14:50:31 596  FUNCTION IS_INVOICE_PAYING_STARTED (
14:50:31 597  /*
14:50:31 598  Throws exceptions:
14:50:31 599  APP_EXCEPTION_CODES_V22.NOT_FOUND
14:50:31 600  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
14:50:31 601  */
14:50:31 602  	in_invoice_id IN NUMBER
14:50:31 603  ) RETURN NUMBER AS
14:50:31 604  -- VARIABLES
14:50:31 605  SPROC_NAME		 CONSTANT VARCHAR2(30) := 'IS_INVOICE_PAYING_STARTED';
14:50:31 606  temp_invoice_id		 NUMBER;
14:50:31 607  var_processed_charges_num  NUMBER;
14:50:31 608  var_processed_transac_num  NUMBER;
14:50:31 609  var_success_attempts_num	 NUMBER;
14:50:31 610  var_is_gc 		 NUMBER;
14:50:31 611  -- EXCEPTIONS
14:50:31 612  BAD_INVOICE_ID EXCEPTION;
14:50:31 613  BEGIN
14:50:31 614  
14:50:31 615  	-- Check that invoice exists
14:50:31 616  	BEGIN
14:50:31 617  	  SELECT
14:50:31 618  	    INVOICE.ID into temp_invoice_id
14:50:31 619  	  FROM
14:50:31 620  	    INVOICE
14:50:31 621  	  WHERE
14:50:31 622  	    INVOICE.ID = in_invoice_id;
14:50:31 623  	  EXCEPTION
14:50:31 624  	   WHEN NO_DATA_FOUND THEN
14:50:31 625  	     RAISE BAD_INVOICE_ID;
14:50:31 626  	END;
14:50:31 627  
14:50:31 628  	-- Check that there are tansaction attempts with status success
14:50:31 629  	SELECT
14:50:31 630  	  COUNT(1) into var_success_attempts_num
14:50:31 631  	FROM
14:50:31 632  	  TRANSACTION_ATTEMPT ta,
14:50:31 633  	  TRANSACTION t,
14:50:31 634  	  CHARGE c
14:50:31 635  	WHERE
14:50:31 636  	  ta.TRANSACTION_ATTEMPT_STATUS_ID = GLOBAL_STATUSES_V22.TRANS_ATTEMPT_SUCCESS and
14:50:31 637  	  ta.transaction_id = t.id and
14:50:31 638  	  c.transaction_id = t.id and
14:50:31 639  	  t.is_refund = GLOBAL_CONSTANTS_V22.FALSE and
14:50:31 640  	  c.invoice_id = in_invoice_id
14:50:31 641  	;
14:50:31 642  
14:50:31 643  	IF var_success_attempts_num > 0 THEN
14:50:31 644  	  RETURN GLOBAL_CONSTANTS_V22.TRUE;
14:50:31 645  	END IF;
14:50:31 646  
14:50:31 647  	SELECT
14:50:31 648  	  COUNT(1) into var_success_attempts_num
14:50:31 649  	FROM
14:50:31 650  	  TRANSACTION t,
14:50:31 651  	  CHARGE c
14:50:31 652  	WHERE
14:50:31 653  	  c.transaction_id = t.id and
14:50:31 654  	  t.is_refund = GLOBAL_CONSTANTS_V22.FALSE and
14:50:31 655  	  t.is_settled = GLOBAL_CONSTANTS_V22.TRUE and
14:50:31 656  	  c.invoice_id = in_invoice_id
14:50:31 657  	;
14:50:31 658  
14:50:31 659  	IF var_success_attempts_num > 0 THEN
14:50:31 660  	  RETURN GLOBAL_CONSTANTS_V22.TRUE;
14:50:31 661  	END IF;
14:50:31 662  
14:50:31 663  	SELECT
14:50:31 664  	  COUNT(1) into var_is_gc
14:50:31 665  	FROM
14:50:31 666  	  gift_certificate gc
14:50:31 667  	WHERE
14:50:31 668  	  gc.finalized_invoice_id = in_invoice_id
14:50:31 669  	;
14:50:31 670  
14:50:31 671  	IF var_is_gc > 0 THEN
14:50:31 672  	  RETURN GLOBAL_CONSTANTS_V22.TRUE;
14:50:31 673  	END IF;
14:50:31 674  
14:50:31 675  	RETURN GLOBAL_CONSTANTS_V22.FALSE;
14:50:31 676  
14:50:31 677  EXCEPTION
14:50:31 678  WHEN BAD_INVOICE_ID THEN
14:50:31 679  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 680  	  SPROC_NAME, 'No such invoice');
14:50:31 681  WHEN OTHERS THEN
14:50:31 682  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:31 683  	  SPROC_NAME, 'Unknown error', SQLERRM);
14:50:31 684  END IS_INVOICE_PAYING_STARTED;
