14:50:31 1166  
14:50:31 1167  PROCEDURE REFUND_SUBSCRIPTION (
14:50:31 1168  /*
14:50:31 1169  Throws exceptions:
14:50:31 1170  APP_EXCEPTION_CODES_V22.NOT_FOUND
14:50:31 1171  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
14:50:31 1172  */
14:50:31 1173  	 in_subscription_id IN NUMBER,
14:50:31 1174  	 in_invoice_id	    IN NUMBER,
14:50:31 1175  	 in_refund_amount   IN NUMBER,
14:50:31 1176  	 in_note	    IN VARCHAR2,
14:50:31 1177  	 in_created_by	    IN VARCHAR2,
14:50:31 1178  	 out_charge_id	    OUT NUMBER
14:50:31 1179  ) AS
14:50:31 1180  -- VARIABLES
14:50:31 1181  SPROC_NAME	      CONSTANT VARCHAR2(19) := 'REFUND_SUBSCRIPTION';
14:50:31 1182  var_invoice_status_id  NUMBER;
14:50:31 1183  var_account_id	      NUMBER;
14:50:31 1184  var_account_status_id  NUMBER;
14:50:31 1185  var_new_transaction_id NUMBER;
14:50:31 1186  var_instrument_type_id NUMBER;
14:50:31 1187  var_instrument_id      NUMBER;
14:50:31 1188  var_new_charge_id      NUMBER;
14:50:31 1189  var_invoice_amount     NUMBER(10,2);
14:50:31 1190  var_refunds_before     NUMBER(10,2);
14:50:31 1191  var_charges_amount     NUMBER(10,2);
14:50:31 1192  -- EXCEPTIONS
14:50:31 1193  BAD_SUBSCRIPTION_ID	     EXCEPTION;
14:50:31 1194  ACCOUNT_IS_FROZEN	     EXCEPTION;
14:50:31 1195  BAD_INVOICE_ID		     EXCEPTION;
14:50:31 1196  CAN_NOT_CREATE_TRANSACTION    EXCEPTION;
14:50:31 1197  CAN_NOT_CREATE_CHARGE	     EXCEPTION;
14:50:31 1198  CAN_NOT_CALC_INVOICE_AMOUNT   EXCEPTION;
14:50:31 1199  REFUND_IS_GREATER_THAN_ANOUNT EXCEPTION;
14:50:31 1200  CAN_NOT_ANNOTATE_SUBSCRIPTION EXCEPTION;
14:50:31 1201  TOT_REF_IS_GREATER_THAN_ANOUNT EXCEPTION;
14:50:31 1202  INVOICE_IS_NOT_CLOSED	     EXCEPTION;
14:50:31 1203  TOT_REF_IS_GRATER_THAN_CHARGES EXCEPTION;
14:50:31 1204  EXCEPTION_MESSAGE	      VARCHAR2(1024);
14:50:31 1205  BEGIN
14:50:31 1206  
14:50:31 1207  	 BEGIN
14:50:31 1208  	   SELECT
14:50:31 1209  	     SUBSCRIPTION.INSTRUMENT_ID,
14:50:31 1210  	     SUBSCRIPTION.INSTRUMENT_TYPE_ID,
14:50:31 1211  	     SUBSCRIPTION.ACCOUNT_ID
14:50:31 1212  	     into
14:50:31 1213  	     var_instrument_id,
14:50:31 1214  	     var_instrument_type_id,
14:50:31 1215  	     var_account_id
14:50:31 1216  	   FROM
14:50:31 1217  	     SUBSCRIPTION
14:50:31 1218  	   WHERE
14:50:31 1219  	     SUBSCRIPTION.ID = in_subscription_id;
14:50:31 1220  	   EXCEPTION
14:50:31 1221  	     WHEN NO_DATA_FOUND THEN
14:50:31 1222  	       RAISE BAD_SUBSCRIPTION_ID;
14:50:31 1223  	 END;
14:50:31 1224  
14:50:31 1225  	 -- Check account status. It should not to be frozen
14:50:31 1226  	 SELECT
14:50:31 1227  	   ACCOUNT.ACCOUNT_STATUS_ID into var_account_status_id
14:50:31 1228  	 FROM
14:50:31 1229  	   ACCOUNT
14:50:31 1230  	 WHERE
14:50:31 1231  	   ACCOUNT.ID = var_account_id;
14:50:31 1232  
14:50:31 1233  	 IF var_account_status_id = GLOBAL_STATUSES_V22.ACCOUNT_FROZEN THEN
14:50:31 1234  	   RAISE ACCOUNT_IS_FROZEN;
14:50:31 1235  	 END IF;
14:50:31 1236  
14:50:31 1237  	 BEGIN
14:50:31 1238  	   SELECT
14:50:31 1239  	     INVOICE.INVOICE_STATUS_ID into var_invoice_status_id
14:50:31 1240  	   FROM
14:50:31 1241  	     INVOICE
14:50:31 1242  	   WHERE
14:50:31 1243  	     INVOICE.ID = in_invoice_id;
14:50:31 1244  	   EXCEPTION
14:50:31 1245  	     WHEN NO_DATA_FOUND THEN
14:50:31 1246  	       RAISE BAD_INVOICE_ID;
14:50:31 1247  	 END;
14:50:31 1248  
14:50:31 1249  	 IF var_invoice_status_id != GLOBAL_STATUSES_V22.INVOICE_CLOSED THEN
14:50:31 1250  	   RAISE INVOICE_IS_NOT_CLOSED;
14:50:31 1251  	 END IF;
14:50:31 1252  
14:50:31 1253  	 BEGIN
14:50:31 1254  	   PROCS_INVOICE_V22.CALCULATE_INVOICE_AMOUNT (
14:50:31 1255  	     in_invoice_id => in_invoice_id,
14:50:31 1256  	     out_amount    => var_invoice_amount
14:50:31 1257  	   );
14:50:31 1258  	   EXCEPTION
14:50:31 1259  	     WHEN OTHERS THEN
14:50:31 1260  	       EXCEPTION_MESSAGE := SQLERRM;
14:50:31 1261  	       RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
14:50:31 1262  	 END;
14:50:31 1263  
14:50:31 1264  	 IF ( in_refund_amount > var_invoice_amount ) THEN
14:50:31 1265  	   RAISE REFUND_IS_GREATER_THAN_ANOUNT;
14:50:31 1266  	 END IF;
14:50:31 1267  
14:50:31 1268  	 SELECT /*+ STAR_TRANSFORMATION */
14:50:31 1269  	   SUM(CHARGE.CHARGE_AMOUNT) into var_refunds_before
14:50:31 1270  	 FROM
14:50:31 1271  	   CHARGE
14:50:31 1272  	 WHERE
14:50:31 1273  	   CHARGE.INVOICE_ID = in_invoice_id
14:50:31 1274  	   AND (
14:50:31 1275  	     CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V22.CHARGE_OPENED
14:50:31 1276  	     OR CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V22.CHARGE_PROCESSED
14:50:31 1277  	   )
14:50:31 1278  	   AND CHARGE.CHARGE_AMOUNT < 0;
14:50:31 1279  
14:50:31 1280  	 -- Refunds are negative
14:50:31 1281  	 var_refunds_before := -var_refunds_before;
14:50:31 1282  
14:50:31 1283  	 var_charges_amount := 0;
14:50:31 1284  
14:50:31 1285  	 FOR f_processed_charges IN (
14:50:31 1286  	   SELECT
14:50:31 1287  	     CHARGE.CHARGE_AMOUNT
14:50:31 1288  	   FROM
14:50:31 1289  	     CHARGE
14:50:31 1290  	   WHERE
14:50:31 1291  	     CHARGE.INVOICE_ID = in_invoice_id
14:50:31 1292  	     AND CHARGE.CHARGE_STATUS_ID IN (SELECT GLOBAL_STATUSES_V22.CHARGE_PROCESSED FROM DUAL)
14:50:31 1293  	 )
14:50:31 1294  	 LOOP
14:50:31 1295  	   IF f_processed_charges.CHARGE_AMOUNT > 0 THEN
14:50:31 1296  	     var_charges_amount := var_charges_amount + f_processed_charges.CHARGE_AMOUNT;
14:50:31 1297  	   END IF;
14:50:31 1298  	 END LOOP;
14:50:31 1299  
14:50:31 1300  	 IF (in_refund_amount + var_refunds_before > var_invoice_amount) THEN
14:50:31 1301  	   RAISE TOT_REF_IS_GREATER_THAN_ANOUNT;
14:50:31 1302  	 END IF;
14:50:31 1303  
14:50:31 1304  	 IF (in_refund_amount + var_refunds_before > var_charges_amount) THEN
14:50:31 1305  	   RAISE TOT_REF_IS_GRATER_THAN_CHARGES;
14:50:31 1306  	 END IF;
14:50:31 1307  
14:50:31 1308  	 BEGIN
14:50:31 1309  	   PROCS_TRANSACTION_V22.CREATE_TRANSACTION(
14:50:31 1310  	     in_transaction_id	       => NULL,
14:50:31 1311  	     in_status_id	       => GLOBAL_STATUSES_V22.TRANSACTION_PREPARE,
14:50:31 1312  	     in_amount		       => -in_refund_amount,
14:50:31 1313  	     in_created_by	       => in_created_by,
14:50:31 1314  	     in_order_id	       => NULL,
14:50:31 1315  	     in_is_refund	       => GLOBAL_CONSTANTS_V22.TRUE,
14:50:31 1316  	     in_transaction_type_code  => 'REFUND',
14:50:31 1317  	     out_transaction_id        => var_new_transaction_id
14:50:31 1318  	   );
14:50:31 1319  	   EXCEPTION
14:50:31 1320  	     WHEN OTHERS THEN
14:50:31 1321  	       EXCEPTION_MESSAGE := SQLERRM;
14:50:31 1322  	       RAISE CAN_NOT_CREATE_TRANSACTION;
14:50:31 1323  	 END;
14:50:31 1324  
14:50:31 1325  	 BEGIN
14:50:31 1326  	   PROCS_CHARGE_V22.CREATE_CHARGE(
14:50:31 1327  	     in_invoice_id	   => in_invoice_id,
14:50:31 1328  	     in_transaction_id	   => var_new_transaction_id,
14:50:31 1329  	     in_instrument_type_id => var_instrument_type_id,
14:50:31 1330  	     in_instrument_id	   => var_instrument_id,
14:50:31 1331  	     in_charge_amount	   => -in_refund_amount,
14:50:31 1332  	     in_created_by	   => in_created_by,
14:50:31 1333  	     in_charge_status_id   => GLOBAL_STATUSES_V22.CHARGE_OPENED,
14:50:31 1334  	     out_charge_id	   => var_new_charge_id
14:50:31 1335  	   );
14:50:31 1336  	   EXCEPTION
14:50:31 1337  	     WHEN OTHERS THEN
14:50:31 1338  	       EXCEPTION_MESSAGE := SQLERRM;
14:50:31 1339  	       RAISE CAN_NOT_CREATE_CHARGE;
14:50:31 1340  	 END;
14:50:31 1341  
14:50:31 1342  	 IF in_note IS NOT NULL THEN
14:50:31 1343  	   BEGIN
14:50:31 1344  	     PROCS_SUBSCRIPTION_V22.ANNOTATE_SUBSCRIPTION(
14:50:31 1345  	       in_subscription_id => in_subscription_id,
14:50:31 1346  	       in_agent_id	  => 0, -- AGENT_ID??
14:50:31 1347  	       in_note		  => in_note,
14:50:31 1348  	       in_created_by	  => in_created_by
14:50:31 1349  	     );
14:50:31 1350  	     EXCEPTION
14:50:31 1351  	       WHEN OTHERS THEN
14:50:31 1352  		 EXCEPTION_MESSAGE := SQLERRM;
14:50:31 1353  		 RAISE CAN_NOT_ANNOTATE_SUBSCRIPTION;
14:50:31 1354  	   END;
14:50:31 1355  	 END IF;
14:50:31 1356  
14:50:31 1357  	 out_charge_id := var_new_charge_id;
14:50:31 1358  
14:50:31 1359  EXCEPTION
14:50:31 1360  WHEN INVOICE_IS_NOT_CLOSED THEN
14:50:31 1361  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.STATE_ERROR,
14:50:31 1362  	   SPROC_NAME, 'Invoice is not closed');
14:50:31 1363  WHEN BAD_SUBSCRIPTION_ID THEN
14:50:31 1364  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 1365  	   SPROC_NAME, 'No such subscription');
14:50:31 1366  WHEN ACCOUNT_IS_FROZEN THEN
14:50:31 1367  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.STATE_ERROR,
14:50:31 1368  	   SPROC_NAME, 'Could not refund subscription for frozen account');
14:50:31 1369  WHEN BAD_INVOICE_ID THEN
14:50:31 1370  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 1371  	   SPROC_NAME, 'No such invoice');
14:50:31 1372  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
14:50:31 1373  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 1374  	   SPROC_NAME, 'Could not calculate invoice amount', EXCEPTION_MESSAGE);
14:50:31 1375  WHEN REFUND_IS_GREATER_THAN_ANOUNT THEN
14:50:31 1376  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INVALID_PARAMETER,
14:50:31 1377  	   SPROC_NAME, 'Refund is greater than amount');
14:50:31 1378  WHEN TOT_REF_IS_GREATER_THAN_ANOUNT THEN
14:50:31 1379  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INVALID_PARAMETER,
14:50:31 1380  	   SPROC_NAME, 'There were refunds before and sum of all refunds and new refund more than invoice amount');
14:50:31 1381  WHEN TOT_REF_IS_GRATER_THAN_CHARGES THEN
14:50:31 1382  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INVALID_PARAMETER,
14:50:31 1383  	   SPROC_NAME, 'Total refund amount is greater than sum of processed charges');
14:50:31 1384  WHEN CAN_NOT_CREATE_TRANSACTION THEN
14:50:31 1385  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 1386  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
14:50:31 1387  WHEN CAN_NOT_CREATE_CHARGE THEN
14:50:31 1388  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 1389  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
14:50:31 1390  WHEN CAN_NOT_ANNOTATE_SUBSCRIPTION THEN
14:50:31 1391  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 1392  	   SPROC_NAME, 'Could not annotate subscription', EXCEPTION_MESSAGE);
14:50:31 1393  WHEN OTHERS THEN
14:50:31 1394  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:31 1395  	   SPROC_NAME, 'Unknown error', SQLERRM);
14:50:31 1396  END REFUND_SUBSCRIPTION;
