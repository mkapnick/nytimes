14:50:31 317  
14:50:31 318  PROCEDURE DISABLE_CREDIT_CARD (
14:50:31 319  /*
14:50:31 320  Throws exceptions:
14:50:31 321  APP_EXCEPTION_CODES_V22.NOT_FOUND
14:50:31 322  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
14:50:31 323  APP_EXCEPTION_CODES_V22.INTERNAL_ERROR
14:50:31 324  */
14:50:31 325  	in_credit_card_id IN NUMBER,
14:50:31 326  	in_updated_by	  IN VARCHAR2
14:50:31 327  ) AS
14:50:31 328  SPROC_NAME		   CONSTANT VARCHAR2(19) := 'DISABLE_CREDIT_CARD';
14:50:31 329  -- VARIBLES
14:50:31 330  var_account_id		     NUMBER;
14:50:31 331  var_group_id		     NUMBER;
14:50:31 332  var_credit_card_status	     NUMBER;
14:50:31 333  var_pending_transactions_num   NUMBER;
14:50:31 334  var_pending_invoices_num	     NUMBER;
14:50:31 335  current_def_instrument_type_id NUMBER;
14:50:31 336  current_def_instrument_id      NUMBER;
14:50:31 337  -- EXCEPTIONS
14:50:31 338  BAD_CC_STATUS		   EXCEPTION;
14:50:31 339  PENDING_TRANSACTIONS_FOUNDED EXCEPTION;
14:50:31 340  CAN_NOT_GET_DEF_FINANCIAL    EXCEPTION;
14:50:31 341  CAN_NOT_DEL_DEF_FINANCIAL    EXCEPTION;
14:50:31 342  CAN_NOT_DISABLE_CREDIT_CARD  EXCEPTION;
14:50:31 343  EXCEPTION_MESSAGE 	   VARCHAR2(1024);
14:50:31 344  BEGIN
14:50:31 345  
14:50:31 346  	-- Get credit card status
14:50:31 347  	-- Get account id
14:50:31 348  	SELECT
14:50:31 349  	  CREDIT_CARD.CREDIT_CARD_STATUS_ID,
14:50:31 350  	  CREDIT_CARD.ACCOUNT_ID
14:50:31 351  	  into
14:50:31 352  	  var_credit_card_status,
14:50:31 353  	  var_account_id
14:50:31 354  	FROM
14:50:31 355  	  CREDIT_CARD
14:50:31 356  	WHERE
14:50:31 357  	  CREDIT_CARD.ID = in_credit_card_id;
14:50:31 358  
14:50:31 359  	-- Check that we can disable this credit card (STUB)
14:50:31 360  	IF F_CAN_DISABLE_CREDIT_CARD(in_credit_card_id) = GLOBAL_CONSTANTS_V22.FALSE THEN
14:50:31 361  	  RAISE CAN_NOT_DISABLE_CREDIT_CARD;
14:50:31 362  	END IF;
14:50:31 363  
14:50:31 364  	-- Get account id
14:50:31 365  	SELECT
14:50:31 366  	  ACCOUNT.GROUP_ID into var_group_id
14:50:31 367  	FROM
14:50:31 368  	  ACCOUNT
14:50:31 369  	WHERE
14:50:31 370  	  ACCOUNT.ID = var_account_id;
14:50:31 371  
14:50:31 372  	-- Card should to be active
14:50:31 373  	IF var_credit_card_status != GLOBAL_STATUSES_V22.CREDIT_CARD_ACTIVE THEN
14:50:31 374  	  RAISE BAD_CC_STATUS;
14:50:31 375  	END IF;
14:50:31 376  
14:50:31 377  	-- Looking for pending transactions associated with given credit card
14:50:31 378  	SELECT
14:50:31 379  	  COUNT(*) into var_pending_invoices_num
14:50:31 380  	FROM
14:50:31 381  	  CHARGE
14:50:31 382  	  INNER JOIN TRANSACTION ON CHARGE.TRANSACTION_ID = TRANSACTION.ID
14:50:31 383  	WHERE
14:50:31 384  	  CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V22.INSTRUMENT_CREDIT_CARD
14:50:31 385  	  AND CHARGE.INSTRUMENT_ID = in_credit_card_id
14:50:31 386  	  AND TRANSACTION.TRANSACTION_STATUS_ID = GLOBAL_STATUSES_V22.TRANSACTION_PENDING;
14:50:31 387  
14:50:31 388  	IF var_pending_invoices_num > 0 THEN
14:50:31 389  	  RAISE PENDING_TRANSACTIONS_FOUNDED;
14:50:31 390  	END IF;
14:50:31 391  
14:50:31 392  	-- Getting current default financial instrument
14:50:31 393  	BEGIN
14:50:31 394  	  GET_DEF_FINANCIAL_INSTRUMENT(
14:50:31 395  	    in_group_id 	   => var_group_id,
14:50:31 396  	    out_instrument_type_id => current_def_instrument_type_id,
14:50:31 397  	    out_instrument_id	   => current_def_instrument_id
14:50:31 398  	  );
14:50:31 399  	  EXCEPTION
14:50:31 400  	    WHEN OTHERS THEN
14:50:31 401  	      EXCEPTION_MESSAGE := SQLERRM;
14:50:31 402  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
14:50:31 403  	END;
14:50:31 404  
14:50:31 405  	-- Checking that credit card is not default
14:50:31 406  	IF current_def_instrument_type_id = GLOBAL_ENUMS_V22.INSTRUMENT_CREDIT_CARD
14:50:31 407  	  AND current_def_instrument_id = in_credit_card_id THEN
14:50:31 408  	  BEGIN
14:50:31 409  	    DEL_DEF_FINANCIAL_INSTRUMENT(
14:50:31 410  	      in_group_id => var_group_id
14:50:31 411  	    );
14:50:31 412  	  EXCEPTION
14:50:31 413  	    WHEN OTHERS THEN
14:50:31 414  	      EXCEPTION_MESSAGE := SQLERRM;
14:50:31 415  	      RAISE CAN_NOT_DEL_DEF_FINANCIAL;
14:50:31 416  	  END;
14:50:31 417  	END IF;
14:50:31 418  
14:50:31 419  	-- Update credit card status
14:50:31 420  	PROCS_FIN_INSTRUMENTS_V22.UPDATE_CREDIT_CARD_STATUS(
14:50:31 421  	  in_credit_card_id	   => in_credit_card_id,
14:50:31 422  	  in_updated_by 	   => in_updated_by,
14:50:31 423  	  in_credit_card_status_id => GLOBAL_STATUSES_V22.CREDIT_CARD_DISABLED
14:50:31 424  	);
14:50:31 425  
14:50:31 426  EXCEPTION
14:50:31 427  WHEN NO_DATA_FOUND THEN
14:50:31 428  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 429  	  SPROC_NAME, 'No such credit card');
14:50:31 430  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
14:50:31 431  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 432  	  SPROC_NAME, 'Could not get current default financial instrument', EXCEPTION_MESSAGE);
14:50:31 433  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
14:50:31 434  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 435  	  SPROC_NAME, 'Could not delete information about default financial instrument from account', EXCEPTION_MESSAGE);
14:50:31 436  WHEN BAD_CC_STATUS THEN
14:50:31 437  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.STATE_ERROR,
14:50:31 438  	  SPROC_NAME, 'Credit card is not active');
14:50:31 439  WHEN PENDING_TRANSACTIONS_FOUNDED THEN
14:50:31 440  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.STATE_ERROR,
14:50:31 441  	  SPROC_NAME, 'Account has pending charge which is using this card');
14:50:31 442  WHEN CAN_NOT_DISABLE_CREDIT_CARD THEN
14:50:31 443  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 444  	  SPROC_NAME, 'Could not disable this credit card', EXCEPTION_MESSAGE);
14:50:31 445  WHEN OTHERS THEN
14:50:31 446  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:31 447  	  SPROC_NAME, 'Unknown error', SQLERRM);
14:50:31 448  END DISABLE_CREDIT_CARD;
