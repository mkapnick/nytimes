14:50:31 1415  
14:50:31 1416  PROCEDURE REDEEM_GIFT_CERTIFICATE (
14:50:31 1417  	 in_group_id			 IN NUMBER,
14:50:31 1418  	 in_gift_certificate_code	 IN VARCHAR2,
14:50:31 1419  	 in_created_by			 IN VARCHAR2,
14:50:31 1420  	 in_redeemer_address_id 	 IN NUMBER,
14:50:31 1421  	 in_fin_instrument_id		 IN NUMBER,
14:50:31 1422  	 in_fin_instrument_type_id	 IN NUMBER,
14:50:31 1423  	 in_redemption_offer_chain_id	 IN NUMBER,
14:50:31 1424  	 out_subscription_id		 OUT NUMBER,
14:50:31 1425  	 out_license_id 		 OUT NUMBER
14:50:31 1426  ) AS
14:50:31 1427  SPROC_NAME	       CONSTANT VARCHAR2(23) := 'REDEEM_GIFT_CERTIFICATE';
14:50:31 1428  -- VARIABLES
14:50:31 1429  var_gift_certificate_id NUMBER;
14:50:31 1430  -- norlov: #38151 var_offer_chain_id replaced by var_purchased_oc_id and var_oc_id_to_redeem
14:50:31 1431  var_purchased_oc_id     NUMBER;
14:50:31 1432  var_oc_id_to_redeem     NUMBER := in_redemption_offer_chain_id;
14:50:31 1433  var_offer_duration      VARCHAR2(30);
14:50:31 1434  var_invoice_id	       NUMBER;
14:50:31 1435  var_succ_purch_attempts_num NUMBER;
14:50:31 1436  var_subscription_id     NUMBER;
14:50:31 1437  var_license_id	       NUMBER;
14:50:31 1438  var_account_id	       NUMBER;
14:50:31 1439  var_gc_status_id        NUMBER;
14:50:31 1440  var_gc_charges_amount   NUMBER;
14:50:31 1441  var_gc_expiration_date  DATE;
14:50:31 1442  var_gc_redeemer_group_id NUMBER;
14:50:31 1443  var_gc_purchase_invoice_id NUMBER;
14:50:31 1444  var_gc_purchase_inv_status_id NUMBER;
14:50:31 1445  var_offer_index		     NUMBER;
14:50:31 1446  var_purchaser_group_id	     NUMBER;
14:50:31 1447  temp_license_id		     NUMBER;
14:50:31 1448  var_same_offer_chains_num     NUMBER;
14:50:31 1449  var_max_concurrent_subscrs    NUMBER;
14:50:31 1450  var_account_tax_exempt_id     VARCHAR2(255);
14:50:31 1451  var_fin_instrument_type_id    NUMBER := in_fin_instrument_type_id;
14:50:31 1452  var_fin_instrument_id	     NUMBER := in_fin_instrument_id;
14:50:31 1453  var_first_offer_id	  NUMBER;
14:50:31 1454  var_date 	     DATE := SYSDATE;
14:50:31 1455  
14:50:31 1456  var_offers SYS_REFCURSOR;
14:50:31 1457  
14:50:31 1458  -- EXCEPTIONS
14:50:31 1459  BAD_GIFT_CERTIFICATE_CODE      EXCEPTION;
14:50:31 1460  BAD_GROUP_ID		      EXCEPTION;
14:50:31 1461  CAN_NOT_CREATE_LICENSE	      EXCEPTION;
14:50:31 1462  GIFT_CERT_IS_FINALIZED	      EXCEPTION;
14:50:31 1463  GIFT_CERT_IS_REFUNDED	      EXCEPTION;
14:50:31 1464  CAN_NOT_UPDATE_CERTIFICATE     EXCEPTION;
14:50:31 1465  GIFT_CERTIFICATE_EXPIRED       EXCEPTION;
14:50:31 1466  GIFT_CERTIFICATE_REDEEMED      EXCEPTION;
14:50:31 1467  USER_ALREADY_SUBSCRIBED_TO_PRD EXCEPTION;
14:50:31 1468  LIMIT_REACHED		      EXCEPTION;
14:50:31 1469  GC_PURCHASE_INVOICE_NOT_CLOSED EXCEPTION;
14:50:31 1470  PURCHASE_INVOICES_NOT_PAID     EXCEPTION;
14:50:31 1471  OC_TO_REDEEM_NOT_FOUND	      EXCEPTION;
14:50:31 1472  CAN_NOT_GET_FIRST_OFFER_CHAIN  EXCEPTION;
14:50:31 1473  EXCEPTION_MESSAGE	      VARCHAR2(1024);
14:50:31 1474  BEGIN
14:50:31 1475  
14:50:31 1476  	 -- Get account id
14:50:31 1477  	 BEGIN
14:50:31 1478  	   SELECT
14:50:31 1479  	     ACCOUNT.ID,
14:50:31 1480  	     ACCOUNT.TAX_EXEMPT_ID
14:50:31 1481  	     into
14:50:31 1482  	     var_account_id,
14:50:31 1483  	     var_account_tax_exempt_id
14:50:31 1484  	   FROM
14:50:31 1485  	     ACCOUNT
14:50:31 1486  	   WHERE
14:50:31 1487  	     ACCOUNT.GROUP_ID = in_group_id
14:50:31 1488  	     AND ROWNUM <= 1;
14:50:31 1489  
14:50:31 1490  	   EXCEPTION
14:50:31 1491  	   WHEN NO_DATA_FOUND THEN
14:50:31 1492  	     RAISE BAD_GROUP_ID;
14:50:31 1493  	 END;
14:50:31 1494  
14:50:31 1495  	 -- Get gift certificate data
14:50:31 1496  	 BEGIN
14:50:31 1497  	   SELECT
14:50:31 1498  	     GIFT_CERTIFICATE.ID,
14:50:31 1499  	     GIFT_CERTIFICATE.OFFER_CHAIN_ID,
14:50:31 1500  	     GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID,
14:50:31 1501  	     GIFT_CERTIFICATE.PURCHASER_GROUP_ID,
14:50:31 1502  	     GIFT_CERTIFICATE.EXPIRATION_DATE,
14:50:31 1503  	     GIFT_CERTIFICATE.REDEEMER_GROUP_ID,
14:50:31 1504  	     GIFT_CERTIFICATE.PURCHASE_INVOICE_ID
14:50:31 1505  	     into
14:50:31 1506  	     var_gift_certificate_id,
14:50:31 1507  	     var_purchased_oc_id,
14:50:31 1508  	     var_gc_status_id,
14:50:31 1509  	     var_purchaser_group_id,
14:50:31 1510  	     var_gc_expiration_date,
14:50:31 1511  	     var_gc_redeemer_group_id,
14:50:31 1512  	     var_gc_purchase_invoice_id
14:50:31 1513  	   FROM
14:50:31 1514  	     GIFT_CERTIFICATE
14:50:31 1515  	   WHERE
14:50:31 1516  	     GIFT_CERTIFICATE.CODE = in_gift_certificate_code
14:50:31 1517  	     AND ROWNUM <= 1;
14:50:31 1518  
14:50:31 1519  	   EXCEPTION
14:50:31 1520  	   WHEN NO_DATA_FOUND THEN
14:50:31 1521  	     RAISE BAD_GIFT_CERTIFICATE_CODE;
14:50:31 1522  	 END;
14:50:31 1523  
14:50:31 1524  	 -- get redemption oc id from meta data if it wasn't passed in, parsing will fail for gcs with multiple redemption offer chains,
14:50:31 1525  	 -- but in that case a redemption offer chain id should always be passed in
14:50:31 1526  	 IF var_oc_id_to_redeem IS NULL THEN
14:50:31 1527  	   BEGIN
14:50:31 1528  	     SELECT
14:50:31 1529  	       to_number(OFFER_CHAIN_META_DATA.VALUE)
14:50:31 1530  	       into
14:50:31 1531  	       var_oc_id_to_redeem
14:50:31 1532  	     FROM
14:50:31 1533  	       OFFER_CHAIN_META_DATA
14:50:31 1534  	     WHERE
14:50:31 1535  	       OFFER_CHAIN_META_DATA.OFFER_CHAIN_ID = var_purchased_oc_id
14:50:31 1536  	       AND OFFER_CHAIN_META_DATA.NAME = GLOBAL_CONSTANTS_V22.REDEMPTION_OC_ID
14:50:31 1537  	       AND ROWNUM = 1;
14:50:31 1538  -- requested by ticket so (but above is correct for the actual migrated data):
14:50:31 1539  --  SELECT
14:50:31 1540  --      OFFER_CHAIN.ID
14:50:31 1541  --      into
14:50:31 1542  --      var_oc_id_to_redeem
14:50:31 1543  --    FROM
14:50:31 1544  --      OFFER_CHAIN
14:50:31 1545  --	 INNER JOIN ELIGIBILITY ON OFFER_CHAIN.ID = ELIGIBILITY.OFFER_CHAIN_ID
14:50:31 1546  --	 INNER JOIN OFFER_CHAIN_META_DATA ON OFFER_CHAIN.ID = OFFER_CHAIN_META_DATA.OFFER_CHAIN_ID
14:50:31 1547  --    WHERE
14:50:31 1548  --      ELIGIBILITY.OFFER_CHAIN_ID = OFFER_CHAIN.ID
14:50:31 1549  --      AND ELIGIBILITY.NAME = GLOBAL_CONSTANTS_V22.GIFT_CERTIFICATE_REQUIRED
14:50:31 1550  --      AND ELIGIBILITY.VALUE = GLOBAL_CONSTANTS_V22.ELIGIBILITY_FLAG_SET
14:50:31 1551  --      AND OFFER_CHAIN_META_DATA.OFFER_CHAIN_ID = OFFER_CHAIN.ID
14:50:31 1552  --      AND OFFER_CHAIN_META_DATA.NAME = GLOBAL_CONSTANTS_V22.REDEMPTION_OC_ID
14:50:31 1553  --      AND to_number(OFFER_CHAIN_META_DATA.VALUE) = var_purchased_oc_id
14:50:31 1554  --      AND ROWNUM = 1;
14:50:31 1555  
14:50:31 1556  	     EXCEPTION
14:50:31 1557  	     WHEN NO_DATA_FOUND THEN
14:50:31 1558  	       RAISE OC_TO_REDEEM_NOT_FOUND;
14:50:31 1559  	   END;
14:50:31 1560  	 END IF;
14:50:31 1561  
14:50:31 1562  	 -- Check that purchase invoice for this GC was closed
14:50:31 1563  	 SELECT
14:50:31 1564  	   INVOICE.INVOICE_STATUS_ID into var_gc_purchase_inv_status_id
14:50:31 1565  	 FROM
14:50:31 1566  	   INVOICE
14:50:31 1567  	 WHERE
14:50:31 1568  	   INVOICE.ID = var_gc_purchase_invoice_id;
14:50:31 1569  
14:50:31 1570  	 IF var_gc_purchase_inv_status_id != GLOBAL_STATUSES_V22.INVOICE_CLOSED THEN
14:50:31 1571  	   RAISE GC_PURCHASE_INVOICE_NOT_CLOSED;
14:50:31 1572  	 END IF;
14:50:31 1573  
14:50:31 1574  	 -- Check that this invoice was successfully processed by billing
14:50:31 1575  	 SELECT
14:50:31 1576  	   COUNT(1) into var_succ_purch_attempts_num
14:50:31 1577  	 FROM
14:50:31 1578  	   TRANSACTION_ATTEMPT TA
14:50:31 1579  	   INNER JOIN TRANSACTION T ON T.ID = TA.TRANSACTION_ID
14:50:31 1580  	   INNER JOIN CHARGE CH ON CH.TRANSACTION_ID = T.ID
14:50:31 1581  	 WHERE
14:50:31 1582  	   CH.INVOICE_ID = var_gc_purchase_invoice_id
14:50:31 1583  	   AND TA.TRANSACTION_ATTEMPT_STATUS_ID = GLOBAL_STATUSES_V22.TRANS_ATTEMPT_SUCCESS;
14:50:31 1584  
14:50:31 1585  	 IF var_succ_purch_attempts_num = 0 THEN
14:50:31 1586  	   SELECT
14:50:31 1587  	     COUNT(1) into var_succ_purch_attempts_num
14:50:31 1588  	   FROM
14:50:31 1589  	     DUAL
14:50:31 1590  	   WHERE
14:50:31 1591  	     PROCS_INVOICE_V22.F_CALCULATE_INVOICE_AMOUNT(var_gc_purchase_invoice_id) = 0;
14:50:31 1592  	 END IF;
14:50:31 1593  
14:50:31 1594  	 IF var_succ_purch_attempts_num = 0 THEN
14:50:31 1595  	   RAISE PURCHASE_INVOICES_NOT_PAID;
14:50:31 1596  	 END IF;
14:50:31 1597  
14:50:31 1598  	 -- Check limit for gc's offer chain
14:50:31 1599  	 SELECT
14:50:31 1600  	   COUNT(*) into var_same_offer_chains_num
14:50:31 1601  	 FROM
14:50:31 1602  	   SUBSCRIPTION
14:50:31 1603  	 WHERE
14:50:31 1604  	   SUBSCRIPTION.ACCOUNT_ID = var_account_id
14:50:31 1605  	   AND SUBSCRIPTION.OFFER_CHAIN_ID = var_oc_id_to_redeem
14:50:31 1606  	   AND (
14:50:31 1607  	     SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE
14:50:31 1608  	     OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_IN_GRACE_PERIOD);
14:50:31 1609  IF var_same_offer_chains_num = 0 THEN
14:50:31 1610  	   -- if user does not have any active existing subscriptions to the offer chain
14:50:31 1611  	   -- and if product from the offer chain is already owned from another offer chain
14:50:31 1612  	   -- then deny purchase
14:50:31 1613  	   FOR f_account_offer_chains IN (
14:50:31 1614  	     SELECT DISTINCT
14:50:31 1615  	       OFFER_CHAIN_ID
14:50:31 1616  	     FROM
14:50:31 1617  	       SUBSCRIPTION
14:50:31 1618  	     WHERE
14:50:31 1619  	       ACCOUNT_ID = var_account_id
14:50:31 1620  	       AND (
14:50:31 1621  		 SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE
14:50:31 1622  		 OR SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_SUSPENDED
14:50:31 1623  		 OR SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_IN_GRACE_PERIOD)
14:50:31 1624  	   )
14:50:31 1625  	   LOOP
14:50:31 1626  	     IF PROCS_OFFER_CHAIN_V22.CHECK_FOR_SAME_PRODUCTS(var_oc_id_to_redeem, f_account_offer_chains.OFFER_CHAIN_ID) = GLOBAL_CONSTANTS_V22.TRUE THEN
14:50:31 1627  	       RAISE USER_ALREADY_SUBSCRIBED_TO_PRD;
14:50:31 1628  	     END IF;
14:50:31 1629  	   END LOOP;
14:50:31 1630  	 ELSE
14:50:31 1631  
14:50:31 1632  	   -- if user have any active existing subscriptions to the offer chain
14:50:31 1633  	   -- and if MAX_CONCURRENT_SUBS <= [user's subscription count for the offer chain]
14:50:31 1634  	   -- then deny purchase
14:50:31 1635  	   var_max_concurrent_subscrs := PROCS_OFFER_CHAIN_V22.GET_OFFER_CHAIN_MAX_CONC_SUBSC(var_oc_id_to_redeem);
14:50:31 1636  	   IF var_max_concurrent_subscrs != GLOBAL_CONSTANTS_V22.INFINITY
14:50:31 1637  	     AND var_max_concurrent_subscrs <= var_same_offer_chains_num THEN
14:50:31 1638  	     RAISE LIMIT_REACHED;
14:50:31 1639  	   END IF;
14:50:31 1640  	 END IF;
14:50:31 1641  -- norlov: END OF TODO
14:50:31 1642  
14:50:31 1643  
14:50:31 1644  	 IF var_gc_redeemer_group_id IS NOT NULL THEN
14:50:31 1645  	   RAISE GIFT_CERTIFICATE_REDEEMED;
14:50:31 1646  	 END IF;
14:50:31 1647  
14:50:31 1648  	 IF var_gc_expiration_date < sysdate THEN
14:50:31 1649  	   RAISE GIFT_CERTIFICATE_EXPIRED;
14:50:31 1650  	 END IF;
14:50:31 1651  
14:50:31 1652  	  IF var_gc_status_id = GLOBAL_STATUSES_V22.GIFT_CERTIFICATE_REFUNDED THEN
14:50:31 1653  	   RAISE GIFT_CERT_IS_REFUNDED;
14:50:31 1654  	 END IF;
14:50:31 1655  
14:50:31 1656  	 IF var_gc_status_id = GLOBAL_STATUSES_V22.GIFT_CERTIFICATE_FINALIZED THEN
14:50:31 1657  	   RAISE GIFT_CERT_IS_FINALIZED;
14:50:31 1658  	 END IF;
14:50:31 1659  
14:50:31 1660  	 -- Check that user did not subscribed to same product already
14:50:31 1661  	 -- norlov: get rid of this since there is already the check?
14:50:31 1662  	 FOR f_user_offer_chain IN (
14:50:31 1663  	   SELECT DISTINCT
14:50:31 1664  	     OFFER_CHAIN_ID
14:50:31 1665  	   FROM
14:50:31 1666  	     SUBSCRIPTION
14:50:31 1667  	   WHERE
14:50:31 1668  	     ACCOUNT_ID=var_account_id
14:50:31 1669  	     AND (
14:50:31 1670  	       SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE
14:50:31 1671  	       OR SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_SUSPENDED
14:50:31 1672  	       OR SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_IN_GRACE_PERIOD)
14:50:31 1673  	 )
14:50:31 1674  	 LOOP
14:50:31 1675  	   IF PROCS_OFFER_CHAIN_V22.CHECK_FOR_SAME_PRODUCTS(var_oc_id_to_redeem, f_user_offer_chain.OFFER_CHAIN_ID)=GLOBAL_CONSTANTS_V22.TRUE THEN
14:50:31 1676  	     RAISE USER_ALREADY_SUBSCRIBED_TO_PRD;
14:50:31 1677  	   END IF;
14:50:31 1678  	 END LOOP;
14:50:31 1679  
14:50:31 1680  	 -- Check for gift certificate amount
14:50:31 1681  	 SELECT
14:50:31 1682  	   SUM(CHARGE.CHARGE_AMOUNT) into var_gc_charges_amount
14:50:31 1683  	 FROM
14:50:31 1684  	   CHARGE
14:50:31 1685  	 WHERE
14:50:31 1686  	   CHARGE.INSTRUMENT_ID = var_gift_certificate_id
14:50:31 1687  	   AND CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V22.INSTRUMENT_GIFT_CERTIFICATE;
14:50:31 1688  
14:50:31 1689  	 -- Create new invoice
14:50:31 1690  	 PROCS_INVOICE_V22.CREATE_INVOICE(
14:50:31 1691  	   out_invoice_id    => var_invoice_id,
14:50:31 1692  	   in_invoice_status => GLOBAL_STATUSES_V22.INVOICE_CLOSED,
14:50:31 1693  	   in_tax_exempt_id  => var_account_tax_exempt_id,
14:50:31 1694  	   in_created_by     => in_created_by
14:50:31 1695  	 );
14:50:31 1696  
14:50:31 1697  	 -- If a financial instrument wasn't passed in, use the gift certificate id
14:50:31 1698  	 -- Real financial instrument is required for upsell/till forbid gift subscriptions
14:50:31 1699  	 IF var_fin_instrument_id is null THEN
