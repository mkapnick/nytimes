14:50:31 1844  	   ch.name,
14:50:31 1845  	   ch.id,
14:50:31 1846  	   gc.sender_email,
14:50:31 1847  	   gc.sender_name,
14:50:31 1848  	   gc.recipient_email,
14:50:31 1849  	   gc.recipient_name,
14:50:31 1850  	   gc.purchase_date,
14:50:31 1851  	   gc.redemption_date,
14:50:31 1852  	   gc.purchaser_group_id,
14:50:31 1853  	   gc.redeemer_group_id,
14:50:31 1854  	   gc.gift_message,
