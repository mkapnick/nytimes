14:50:31 1051  
14:50:31 1052  FUNCTION GET_RECENT_CHARGE (
14:50:31 1053  /*
14:50:31 1054  Throws exceptions:
14:50:31 1055  APP_EXCEPTION_CODES_V22.NOT_FOUND
14:50:31 1056  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
14:50:31 1057  */
14:50:31 1058  	 in_subscription_id IN NUMBER
14:50:31 1059  ) RETURN NUMBER AS
14:50:31 1060  -- VARIABLES
14:50:31 1061  SPROC_NAME	    CONSTANT VARCHAR2(17) := 'GET_RECENT_CHARGE';
14:50:31 1062  temp_subscription_id NUMBER;
14:50:31 1063  var_recent_charge    NUMBER(10,2);
14:50:31 1064  
14:50:31 1065  -- EXCEPTIONS
14:50:31 1066  BAD_SUBSCRIPTION_ID EXCEPTION;
14:50:31 1067  BEGIN
14:50:31 1068  
14:50:31 1069  	 BEGIN
14:50:31 1070  	   SELECT
14:50:31 1071  	     SUBSCRIPTION.ID into temp_subscription_id
14:50:31 1072  	   FROM
14:50:31 1073  	     SUBSCRIPTION
14:50:31 1074  	   WHERE
14:50:31 1075  	     SUBSCRIPTION.ID = in_subscription_id;
14:50:31 1076  	   EXCEPTION
14:50:31 1077  	     WHEN NO_DATA_FOUND THEN
14:50:31 1078  	       RAISE BAD_SUBSCRIPTION_ID;
14:50:31 1079  	 END;
14:50:31 1080  
14:50:31 1081  	 BEGIN
14:50:31 1082  	   SELECT
14:50:31 1083  	     CHARGE.CHARGE_AMOUNT into var_recent_charge
14:50:31 1084  	   FROM
14:50:31 1085  	     LICENSE
14:50:31 1086  	     INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
14:50:31 1087  	     INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
14:50:31 1088  	     INNER JOIN CHARGE ON CHARGE.INVOICE_ID = INVOICE.ID
14:50:31 1089  	   WHERE
14:50:31 1090  	     -- TODO: Review
14:50:31 1091  	     -- LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V22.LICENSE_PROCESSED
14:50:31 1092  	     -- AND
14:50:31 1093  	     SUBSCRIPTION.ID = in_subscription_id
14:50:31 1094  	     AND CHARGE.CHARGE_AMOUNT >= 0
14:50:31 1095  	     AND ROWNUM <= 1
14:50:31 1096  	   ORDER BY
14:50:31 1097  	     LICENSE.ID ASC, CHARGE.ID DESC;
14:50:31 1098  	   EXCEPTION
14:50:31 1099  	     WHEN NO_DATA_FOUND THEN
14:50:31 1100  	       var_recent_charge := 0;
14:50:31 1101  	 END;
14:50:31 1102  
14:50:31 1103  	 RETURN var_recent_charge;
14:50:31 1104  
14:50:31 1105  EXCEPTION
14:50:31 1106  WHEN BAD_SUBSCRIPTION_ID THEN
14:50:31 1107  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 1108  	   SPROC_NAME, 'No such subscription');
14:50:31 1109  WHEN OTHERS THEN
14:50:31 1110  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:31 1111  	   SPROC_NAME, 'Unknown error', SQLERRM);
14:50:31 1112  END GET_RECENT_CHARGE;
