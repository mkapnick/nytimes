14:50:31 1300  	       RAISE BAD_INVOICE_ID;
14:50:31 1301  	 END;
14:50:31 1302  
14:50:31 1303  	 BEGIN
14:50:31 1304  	   SELECT DISTINCT
14:50:31 1305  	     ACCOUNT.GROUP_ID into var_subscrib_group_id
14:50:31 1306  	   FROM
14:50:31 1307  	     ACCOUNT
14:50:31 1308  	     INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.ACCOUNT_ID = ACCOUNT.ID
14:50:31 1309  	     INNER JOIN LICENSE ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
14:50:31 1310  	   WHERE
