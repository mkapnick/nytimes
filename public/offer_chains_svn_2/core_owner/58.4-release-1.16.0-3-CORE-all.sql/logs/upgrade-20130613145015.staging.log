14:50:30 162  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:30 163  	  SPROC_NAME, 'No such license');
14:50:30 164  WHEN BAD_OFFER_ID THEN
14:50:30 165  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:30 166  	  SPROC_NAME, 'No such offer');
