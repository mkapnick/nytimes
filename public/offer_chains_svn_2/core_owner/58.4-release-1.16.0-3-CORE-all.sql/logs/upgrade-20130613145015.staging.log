14:50:31 2114  PROCEDURE CANCEL_SUBSCRIPTION_INVOICE (
14:50:31 2115  /*
14:50:31 2116  APP_EXCEPTION_CODES_V22.NOT_FOUND
14:50:31 2117  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
14:50:31 2118  APP_EXCEPTION_CODES_V22.INTERNAL_ERROR
14:50:31 2119  */
14:50:31 2120  	 in_invoice_id	      IN NUMBER,
14:50:31 2121  	 in_updated_by	      IN VARCHAR2,
14:50:31 2122  	 in_refundable	      IN NUMBER DEFAULT GLOBAL_CONSTANTS_V22.FALSE
14:50:31 2123  ) AS
14:50:31 2124  SPROC_NAME CONSTANT VARCHAR2(27) := 'CANCEL_SUBSCRIPTION_INVOICE';
14:50:31 2125  -- VARIABLES
14:50:31 2126  temp_invoice_id	      INVOICE.ID%TYPE;
14:50:31 2127  var_chargeback_amount  NUMBER(10,2);
14:50:31 2128  ver_refund_charge_id   NUMBER;
14:50:31 2129  -- EXCEPTIONS
14:50:31 2130  BAD_INVOICE_ID		    EXCEPTION;
14:50:31 2131  CAN_NOT_CALCULATE_CHARGEBACK EXCEPTION;
14:50:31 2132  CAN_NOT_APPLY_CHARGEBACK     EXCEPTION;
14:50:31 2133  EXCEPTION_MESSAGE	    VARCHAR2(1024);
14:50:31 2134  -- STUB
14:50:31 2135  var_now DATE;
14:50:31 2136  var_revoke NUMBER;
14:50:31 2137  var_refund NUMBER;
14:50:31 2138  var_billed NUMBER;
14:50:31 2139  var_subscription_in_grace NUMBER;
14:50:31 2140  BEGIN
14:50:31 2141  
14:50:31 2142  	 var_now := sysdate;
14:50:31 2143  
14:50:31 2144  	 -- Check that invoice exists
14:50:31 2145  	 BEGIN
14:50:31 2146  	   SELECT
14:50:31 2147  	     INVOICE.ID into temp_invoice_id
14:50:31 2148  	   FROM
14:50:31 2149  	     INVOICE
14:50:31 2150  	   WHERE
14:50:31 2151  	     INVOICE.ID = in_invoice_id;
14:50:31 2152  	   EXCEPTION
14:50:31 2153  	     WHEN NO_DATA_FOUND THEN
14:50:31 2154  	       RAISE BAD_INVOICE_ID;
14:50:31 2155  	 END;
14:50:31 2156  
14:50:31 2157  	 select decode(count(1), 0, GLOBAL_CONSTANTS_V22.FALSE, GLOBAL_CONSTANTS_V22.TRUE) into var_revoke
14:50:31 2158  	 from license l, subscription s, offer_chain oc
14:50:31 2159  	 where
14:50:31 2160  	   l.subscription_id = s.id and
14:50:31 2161  	   s.offer_chain_id = oc.id and
14:50:31 2162  	   l.invoice_id = in_invoice_id and
14:50:31 2163  	   oc.revoke_entitlements = GLOBAL_CONSTANTS_V22.TRUE and
14:50:31 2164  	   rownum < 2;
14:50:31 2165  
14:50:31 2166  	 select
14:50:31 2167  	   decode(s.subscription_status_id, GLOBAL_STATUSES_V22.SUBSCRIPTION_IN_GRACE_PERIOD, 1, 0)
14:50:31 2168  	 into
14:50:31 2169  	   var_subscription_in_grace
14:50:31 2170  	 from license l, subscription s
14:50:31 2171  	 where
14:50:31 2172  	   l.subscription_id = s.id and
14:50:31 2173  	   l.invoice_id = in_invoice_id and
14:50:31 2174  	   rownum < 2;
14:50:31 2175  
14:50:31 2176  	 var_billed := PROCS_INVOICE_V22.IS_INVOICE_PAYING_STARTED(in_invoice_id);
14:50:31 2177  	 var_refund := GLOBAL_CONSTANTS_V22.FALSE;
14:50:31 2178  
14:50:31 2179  	 -- Check that transaction for given invoice not started
14:50:31 2180  	 -- if refund enabled calculate and apply chargeback
14:50:31 2181  	 IF (
14:50:31 2182  	     var_billed = GLOBAL_CONSTANTS_V22.TRUE
14:50:31 2183  	   )THEN
14:50:31 2184  	   if (in_refundable = GLOBAL_CONSTANTS_V22.TRUE) then
14:50:31 2185  	     -- If started then we need to calculate refund
14:50:31 2186  	     BEGIN
14:50:31 2187  	       PROCS_INVOICE_V22.CALCULATE_INVOICE_CHARGEBACK(
14:50:31 2188  		 in_invoice_id,
14:50:31 2189  		 var_now,
14:50:31 2190  		 var_chargeback_amount
14:50:31 2191  	       );
14:50:31 2192  	       EXCEPTION
14:50:31 2193  		 WHEN OTHERS THEN
14:50:31 2194  		   EXCEPTION_MESSAGE := SQLERRM;
14:50:31 2195  		   RAISE CAN_NOT_CALCULATE_CHARGEBACK;
14:50:31 2196  	     END;
14:50:31 2197  	     IF var_chargeback_amount > 0 THEN
14:50:31 2198  	       BEGIN
14:50:31 2199  		 PROCS_INVOICE_V22.APPLY_REFUND(
14:50:31 2200  		   in_invoice_id,
14:50:31 2201  		   var_chargeback_amount,
14:50:31 2202  		   in_updated_by,
14:50:31 2203  		   ver_refund_charge_id
14:50:31 2204  		 );
14:50:31 2205  		 EXCEPTION
14:50:31 2206  		   WHEN OTHERS THEN
14:50:31 2207  		     EXCEPTION_MESSAGE := SQLERRM;
14:50:31 2208  		     RAISE CAN_NOT_APPLY_CHARGEBACK;
14:50:31 2209  	       END;
14:50:31 2210  	       var_refund := GLOBAL_CONSTANTS_V22.TRUE;
14:50:31 2211  	     END IF;
14:50:31 2212  	   end if;
14:50:31 2213  	 ELSE
14:50:31 2214  
14:50:31 2215  	   FOR f_transaction_to_close IN (
14:50:31 2216  	     SELECT DISTINCT CHARGE.TRANSACTION_ID AS "ID" FROM CHARGE WHERE CHARGE.INVOICE_ID = in_invoice_id and CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V22.CHARGE_OPENED
14:50:31 2217  	   )
14:50:31 2218  	   LOOP
14:50:31 2219  	     PROCS_TRANSACTION_V22.UPDATE_TRANSACTION_STATUS(
14:50:31 2220  	       in_transaction_id	=> f_transaction_to_close.ID,
14:50:31 2221  	       in_updated_by		=> in_updated_by,
14:50:31 2222  	       in_transaction_status_id => GLOBAL_STATUSES_V22.TRANSACTION_CLOSED
14:50:31 2223  	     );
14:50:31 2224  	   END LOOP;
14:50:31 2225  	   -- Needs to close charges. No refund.
14:50:31 2226  	   FOR f_charge_to_close IN (
14:50:31 2227  	     SELECT CHARGE.ID FROM CHARGE WHERE CHARGE.INVOICE_ID = in_invoice_id and CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V22.CHARGE_OPENED
14:50:31 2228  	   )
14:50:31 2229  	   LOOP
14:50:31 2230  	     PROCS_CHARGE_V22.UPDATE_CHARGE_STATUS(
14:50:31 2231  	       in_charge_id	   => f_charge_to_close.ID,
14:50:31 2232  	       in_updated_by	   => in_updated_by,
14:50:31 2233  	       in_charge_status_id => GLOBAL_STATUSES_V22.CHARGE_CANCELED
14:50:31 2234  	     );
14:50:31 2235  	   END LOOP;
14:50:31 2236  
14:50:31 2237  	   PROCS_INVOICE_V22.UPDATE_INVOICE_STATUS(
14:50:31 2238  	     in_invoice_id		    => in_invoice_id,
14:50:31 2239  	     in_updated_by		    => in_updated_by,
14:50:31 2240  	     in_invoice_status_id	    => GLOBAL_STATUSES_V22.INVOICE_CLOSED
14:50:31 2241  	   );
14:50:31 2242  
14:50:31 2243  	 END IF;
14:50:31 2244  	 -- update licenses
14:50:31 2245  	 IF(var_revoke = GLOBAL_CONSTANTS_V22.TRUE OR var_chargeback_amount > 0 OR (var_subscription_in_grace = GLOBAL_CONSTANTS_V22.FALSE AND var_billed = GLOBAL_CONSTANTS_V22.FALSE)) THEN
14:50:31 2246  	   FOR f_license_to_cancel IN (
14:50:31 2247  	     SELECT LICENSE.ID FROM LICENSE WHERE LICENSE.INVOICE_ID = in_invoice_id AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V22.LICENSE_ACTIVE
14:50:31 2248  	   )
14:50:31 2249  	   LOOP
14:50:31 2250  	     PROCS_LICENSE_CRU_V22.UPDATE_LICENSE(
14:50:31 2251  	       in_license_id	     => f_license_to_cancel.ID,
14:50:31 2252  	       in_license_status_id  => GLOBAL_STATUSES_V22.LICENSE_CLOSED,
14:50:31 2253  	       in_needs_entitlements => GLOBAL_CONSTANTS_V22.TRUE,
14:50:31 2254  	       in_updated_by	     => in_updated_by,
14:50:31 2255  	       in_entitlement_end_date => var_now
14:50:31 2256  	     );
14:50:31 2257  	   END LOOP;
14:50:31 2258  	 ELSE
14:50:31 2259  	   FOR f_license_to_cancel IN (
14:50:31 2260  	     SELECT LICENSE.ID FROM LICENSE WHERE LICENSE.INVOICE_ID = in_invoice_id AND LICENSE.LICENSE_STATUS_ID in (GLOBAL_STATUSES_V22.LICENSE_ACTIVE, GLOBAL_STATUSES_V22.LICENSE_IN_GRACE_PERIOD)
14:50:31 2261  	   )
14:50:31 2262  	   LOOP
14:50:31 2263  	     PROCS_LICENSE_CRU_V22.UPDATE_LICENSE(
14:50:31 2264  	       in_license_id	     => f_license_to_cancel.ID,
14:50:31 2265  	       in_license_status_id  => GLOBAL_STATUSES_V22.LICENSE_CLOSED,
14:50:31 2266  	       in_updated_by	     => in_updated_by
14:50:31 2267  	     );
14:50:31 2268  	   END LOOP;
14:50:31 2269  	 END IF;
14:50:31 2270  
14:50:31 2271  
14:50:31 2272  EXCEPTION
14:50:31 2273  WHEN BAD_INVOICE_ID THEN
14:50:31 2274  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 2275  	   SPROC_NAME, 'No such invoice');
14:50:31 2276  WHEN CAN_NOT_CALCULATE_CHARGEBACK THEN
14:50:31 2277  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 2278  	   SPROC_NAME, 'Could not calculate invoice refund', EXCEPTION_MESSAGE);
14:50:31 2279  WHEN CAN_NOT_APPLY_CHARGEBACK THEN
14:50:31 2280  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 2281  	   SPROC_NAME, 'Could not apply chargeback', EXCEPTION_MESSAGE);
14:50:31 2282  WHEN OTHERS THEN
14:50:31 2283  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:31 2284  	   SPROC_NAME, 'Unknown error', SQLERRM);
14:50:31 2285  END CANCEL_SUBSCRIPTION_INVOICE;
14:50:31 2286  
14:50:31 2287  /******************************************************************************/
14:50:31 2288  
14:50:31 2289  PROCEDURE FINALIZE_CANCELATION (
14:50:31 2290  /*
14:50:31 2291  APP_EXCEPTION_CODES_V22.NOT_FOUND
14:50:31 2292  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
14:50:31 2293  APP_EXCEPTION_CODES_V22.INTERNAL_ERROR
14:50:31 2294  */
14:50:31 2295  --  in_subscription_id	 IN SUBSCRIPTION.ID%TYPE,
14:50:31 2296  --  in_cancelation_reason IN SUBSCRIPTION_CANCEL_REASON.VALUE%TYPE,
14:50:31 2297  --  in_cancelation_date	 IN SUBSCRIPTION.CANCELLATION_DATE%TYPE,
14:50:31 2298  --  in_note		 IN SUBSCRIPTION_NOTE.NOTE%TYPE,
14:50:31 2299  --  in_agent_id		 IN SUBSCRIPTION_NOTE.AGENT_ID%TYPE,
14:50:31 2300  --  in_updated_by	 IN SUBSCRIPTION.UPDATED_BY%TYPE
14:50:31 2301  	 in_subscription_id    IN NUMBER,
14:50:31 2302  	 in_cancelation_reason IN VARCHAR2,
14:50:31 2303  	 in_cancelation_date   IN DATE,
14:50:31 2304  	 in_note	       IN VARCHAR2,
14:50:31 2305  	 in_agent_id	       IN NUMBER,
14:50:31 2306  	 in_updated_by	       IN VARCHAR2
14:50:31 2307  ) AS
14:50:31 2308  SPROC_NAME CONSTANT VARCHAR2(20) := 'FINALIZE_CANCELATION';
14:50:31 2309  -- VARIABLES
14:50:31 2310  var_current_subscr_status SUBSCRIPTION.SUBSCRIPTION_STATUS_ID%TYPE;
14:50:31 2311  var_sct_id		 SUBSCRIPTION.SCT_ID%TYPE;
14:50:31 2312  var_active_invoices_count NUMBER;
14:50:31 2313  var_license_to_disgrace	 LICENSE.ID%TYPE;
14:50:31 2314  var_now			 DATE := SYSDATE;
14:50:31 2315  -- EXCEPTIONS
14:50:31 2316  BAD_SUBSCRIPTION_ID	   EXCEPTION;
14:50:31 2317  BAD_SUBSCRIPTION_STATUS	   EXCEPTION;
14:50:31 2318  BAD_CANCELATION_REASON	   EXCEPTION;
14:50:31 2319  CAN_NOT_UPDATE_SUBSCRIPTION EXCEPTION;
14:50:31 2320  ACTIVE_INVOICES_FOUND	   EXCEPTION;
14:50:31 2321  CAN_NOT_CREATE_NOTE	   EXCEPTION;
14:50:31 2322  EXCEPTION_MESSAGE	   VARCHAR2(1024);
14:50:31 2323  BEGIN
14:50:31 2324  
14:50:31 2325  	 -- Get current subscription status
14:50:31 2326  	 BEGIN
14:50:31 2327  	   SELECT
14:50:31 2328  	     SUBSCRIPTION.SUBSCRIPTION_STATUS_ID into var_current_subscr_status
14:50:31 2329  	   FROM
14:50:31 2330  	     SUBSCRIPTION
14:50:31 2331  	   WHERE
14:50:31 2332  	     SUBSCRIPTION.ID = in_subscription_id;
14:50:31 2333  	   EXCEPTION
14:50:31 2334  	     WHEN NO_DATA_FOUND THEN
14:50:31 2335  	       RAISE BAD_SUBSCRIPTION_ID;
14:50:31 2336  	 END;
14:50:31 2337  
14:50:31 2338  	 -- Check that subscription reason is correct
14:50:31 2339  	 BEGIN
14:50:31 2340  	   SELECT
14:50:31 2341  	     SUBSCRIPTION_CANCEL_REASON.ID into var_sct_id
14:50:31 2342  	   FROM
14:50:31 2343  	     SUBSCRIPTION_CANCEL_REASON
14:50:31 2344  	   WHERE
14:50:31 2345  	     SUBSCRIPTION_CANCEL_REASON.VALUE LIKE in_cancelation_reason
14:50:31 2346  	     AND ROWNUM <= 1;
14:50:31 2347  	   EXCEPTION
14:50:31 2348  	     WHEN NO_DATA_FOUND THEN
14:50:31 2349  	       RAISE BAD_CANCELATION_REASON;
14:50:31 2350  	 END;
14:50:31 2351  
14:50:31 2352  	 -- Check for invoices with active licenses
14:50:31 2353  	 SELECT
14:50:31 2354  	   COUNT(*) into var_active_invoices_count
14:50:31 2355  	 FROM
14:50:31 2356  	   LICENSE
14:50:31 2357  	   INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
14:50:31 2358  	 WHERE
14:50:31 2359  	   LICENSE.LICENSE_STATUS_ID in (GLOBAL_STATUSES_V22.LICENSE_ACTIVE, GLOBAL_STATUSES_V22.LICENSE_IN_GRACE_PERIOD)
14:50:31 2360  	   AND LICENSE.SUBSCRIPTION_ID = in_subscription_id;
14:50:31 2361  
14:50:31 2362  	 IF var_active_invoices_count > 0 THEN
14:50:31 2363  	   RAISE ACTIVE_INVOICES_FOUND;
14:50:31 2364  	 END IF;
14:50:31 2365  
14:50:31 2366  	 -- Check that subscription is active
14:50:31 2367  	 IF var_current_subscr_status != GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE AND
14:50:31 2368  	    var_current_subscr_status != GLOBAL_STATUSES_V22.SUBSCRIPTION_SUSPENDED AND
14:50:31 2369  	    var_current_subscr_status != GLOBAL_STATUSES_V22.SUBSCRIPTION_IN_GRACE_PERIOD THEN
14:50:31 2370  	   RAISE BAD_SUBSCRIPTION_STATUS;
14:50:31 2371  	 END IF;
14:50:31 2372  
14:50:31 2373  	 -- Update subscription data
14:50:31 2374  	 BEGIN
14:50:31 2375  	   PROCS_SUBSCRIPTION_CRU_V22.UPDATE_SUBSCRIPTION(
14:50:31 2376  	     in_subscription_id        => in_subscription_id,
14:50:31 2377  	     in_subscription_status_id => GLOBAL_STATUSES_V22.SUBSCRIPTION_CANCELED,
14:50:31 2378  	     in_cancelation_date       => in_cancelation_date,
14:50:31 2379  	     in_updated_by	       => in_updated_by,
14:50:31 2380  	     in_sct_id		       => var_sct_id
14:50:31 2381  	   );
14:50:31 2382  	   EXCEPTION
14:50:31 2383  	     WHEN OTHERS THEN
14:50:31 2384  	       EXCEPTION_MESSAGE := SQLERRM;
14:50:31 2385  	       RAISE CAN_NOT_UPDATE_SUBSCRIPTION;
14:50:31 2386  	 END;
14:50:31 2387  
14:50:31 2388  	 -- Terminate grace period for licenses in grace [SAR-31]
14:50:31 2389  	 BEGIN
14:50:31 2390  	   SELECT
14:50:31 2391  	     LICENSE.ID into var_license_to_disgrace
14:50:31 2392  	   FROM
14:50:31 2393  	     LICENSE
14:50:31 2394  	   WHERE
14:50:31 2395  	     LICENSE.SUBSCRIPTION_ID = in_subscription_id
14:50:31 2396  	     AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V22.LICENSE_IN_GRACE_PERIOD
14:50:31 2397  	     AND ROWNUM <= 1
14:50:31 2398  	   ORDER BY
14:50:31 2399  	     CREATE_DATE DESC;
14:50:31 2400  	 EXCEPTION
14:50:31 2401  	   WHEN NO_DATA_FOUND THEN
14:50:31 2402  	     var_license_to_disgrace := NULL;
14:50:31 2403  	 END;
14:50:31 2404  
14:50:31 2405  	 IF var_license_to_disgrace IS NOT NULL THEN
14:50:31 2406  	   PROCS_LICENSE_CRU_V22.UPDATE_LICENSE(
14:50:31 2407  	     in_license_id	     => var_license_to_disgrace,
14:50:31 2408  	     in_license_status_id    => GLOBAL_STATUSES_V22.LICENSE_CLOSED,
14:50:31 2409  	     in_updated_by	     => in_updated_by
14:50:31 2410  	   );
14:50:31 2411  	 END IF;
14:50:31 2412  
14:50:31 2413  	 -- Annotate subscription
14:50:31 2414  	 IF in_note IS NOT NULL THEN
14:50:31 2415  	   BEGIN
14:50:31 2416  	     PROCS_SUBSCRIPTION_V22.ANNOTATE_SUBSCRIPTION(
14:50:31 2417  	       in_subscription_id,
14:50:31 2418  	       in_agent_id,
14:50:31 2419  	       in_note,
14:50:31 2420  	       in_updated_by
14:50:31 2421  	     );
14:50:31 2422  	     EXCEPTION
14:50:31 2423  	      WHEN OTHERS THEN
