14:50:30   5  	PROCEDURE POPULATE_REQUEST_INFO(
14:50:30   6  	  in_hours_prior    IN	NUMBER,
14:50:30   7  	  in_filename	    IN	CC_REQUEST_FILE.FILE_NAME%TYPE,
14:50:30   8  	  in_creator	    IN	CC_REQUEST_FILE.UPDATED_BY%TYPE
14:50:30   9  	) AS
14:50:30  10  	SPROC_NAME CONSTANT VARCHAR2(32) := 'POPULATE_REQUEST_INFO';
14:50:30  11  	var_start_date	    DATE := SYSDATE;
14:50:30  12  	var_end_date	    DATE := var_start_date + (in_hours_prior/24);
14:50:30  13  	var_request_file_id NUMBER := 0;
14:50:30  14  	var_license_count   NUMBER := 0;
14:50:30  15  	var_cc_update_count NUMBER := 0;
14:50:30  16  	BEGIN
14:50:30  17  	    SELECT CC_REQUEST_FILE_ID_SEQ.NEXTVAL INTO var_request_file_id  FROM DUAL;
14:50:30  18  	    INSERT INTO CC_REQUEST_FILE (ID,
14:50:30  19  					 FILE_NAME,
14:50:30  20  					 CC_REQUEST_FILE_STATUS,
14:50:30  21  					 CREATE_DATE,
14:50:30  22  					 CREATED_BY,
14:50:30  23  					 UPDATE_DATE,
14:50:30  24  					 UPDATED_BY)
14:50:30  25  					 VALUES (
14:50:30  26  					 var_request_file_id,
14:50:30  27  					 in_filename,
14:50:30  28  					 'NOT_CREATED',
14:50:30  29  					 var_start_date,
14:50:30  30  					 in_creator,
14:50:30  31  					 var_start_date,
14:50:30  32  					 in_creator);
14:50:30  33  
14:50:30  34  	   FOR record IN (SELECT
14:50:30  35  			    l.ID LICENSE_ID, cc.ID CREDIT_CARD_ID
14:50:30  36  			  FROM
14:50:30  37  			    LICENSE l INNER JOIN SUBSCRIPTION s ON L.SUBSCRIPTION_ID = s.ID
14:50:30  38  				      INNER JOIN CREDIT_CARD cc ON S.INSTRUMENT_ID   = cc.ID
14:50:30  39  			  WHERE
14:50:30  40  			    s.INSTRUMENT_TYPE_ID	 = 1
14:50:30  41  			    AND cc.CREDIT_CARD_STATUS_ID = 1
14:50:30  42  			    AND s.SUBSCRIPTION_STATUS_ID = 1
14:50:30  43  			    AND l.LICENSE_STATUS_ID	 = 2
14:50:30  44  			    AND cc.CREDIT_CARD_TYPE_ID IN (2,3)
14:50:30  45  			    AND l.END_DATE BETWEEN var_start_date AND var_end_date
14:50:30  46  			    AND l.ID NOT IN (SELECT LICENSE_ID FROM CC_UPDATE))
14:50:30  47  	   LOOP
14:50:30  48  	     var_license_count := 0;
14:50:30  49  	     SELECT COUNT(1) INTO  var_license_count FROM CC_UPDATE WHERE LICENSE_ID = record.LICENSE_ID;
14:50:30  50  
14:50:30  51  	     IF var_license_count = 0 THEN
14:50:30  52  		INSERT INTO CC_UPDATE (ID,
14:50:30  53  				       CREDIT_CARD_ID,
14:50:30  54  				       LICENSE_ID,
14:50:30  55  				       CC_UPDATE_STATUS,
14:50:30  56  				       CC_REQUEST_FILE_ID,
14:50:30  57  				       CREATE_DATE,
14:50:30  58  				       UPDATE_DATE,
14:50:30  59  				       CREATED_BY,
14:50:30  60  				       UPDATED_BY
14:50:30  61  				       ) VALUES (
14:50:30  62  				       CC_UPDATE_SEQ.NEXTVAL,
14:50:30  63  				       record.CREDIT_CARD_ID,
14:50:30  64  				       record.LICENSE_ID,
14:50:30  65  				       'NOT_ADDED_TO_FILE',
14:50:30  66  				       var_request_file_id,
14:50:30  67  				       var_start_date,
14:50:30  68  				       var_start_date,
14:50:30  69  				       in_creator,
14:50:30  70  				       in_creator
14:50:30  71  				       );
14:50:30  72  	     END IF;
14:50:30  73  	   END LOOP;
14:50:30  74  
14:50:30  75  	   SELECT COUNT(1) INTO var_cc_update_count
14:50:30  76  	   FROM CC_UPDATE
14:50:30  77  	   WHERE CC_REQUEST_FILE_ID = var_request_file_id;
14:50:30  78  	   IF var_cc_update_count <= 0 THEN
14:50:30  79  	     UPDATE CC_REQUEST_FILE
14:50:30  80  	     SET CC_REQUEST_FILE_STATUS = 'EMPTY'
14:50:30  81  	     WHERE ID = var_request_file_id;
14:50:30  82  	   END IF;
14:50:30  83  
14:50:30  84  	END POPULATE_REQUEST_INFO;
