14:50:31 1982  
14:50:31 1983  PROCEDURE GET_SUBSCRIPTION_PRODUCTS (
14:50:31 1984  /*
14:50:31 1985  APP_EXCEPTION_CODES_V22.NOT_FOUND
14:50:31 1986  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
14:50:31 1987  */
14:50:31 1988  	 in_subscription_id IN NUMBER,
14:50:31 1989  	 out_result_set     OUT SYS_REFCURSOR
14:50:31 1990  ) AS
14:50:31 1991  SPROC_NAME CONSTANT VARCHAR2(25) := 'GET_SUBSCRIPTION_PRODUCTS';
14:50:31 1992  -- VARIABLES
14:50:31 1993  var_offer_chain NUMBER;
14:50:31 1994  -- EXCEPTIONS
14:50:31 1995  BAD_SUBSCRIPTION_ID EXCEPTION;
14:50:31 1996  BEGIN
14:50:31 1997  	 BEGIN
14:50:31 1998  	   SELECT
14:50:31 1999  	     SUBSCRIPTION.OFFER_CHAIN_ID into var_offer_chain
14:50:31 2000  	   FROM
14:50:31 2001  	     SUBSCRIPTION
14:50:31 2002  	   WHERE
14:50:31 2003  	     SUBSCRIPTION.ID = in_subscription_id;
14:50:31 2004  	   EXCEPTION
14:50:31 2005  	     WHEN NO_DATA_FOUND THEN
14:50:31 2006  	       RAISE BAD_SUBSCRIPTION_ID;
14:50:31 2007  	 END;
14:50:31 2008  
14:50:31 2009  	 OPEN out_result_set FOR
14:50:31 2010  	 SELECT DISTINCT
14:50:31 2011  	   PRODUCT.ID,
14:50:31 2012  	   PRODUCT.NAME
14:50:31 2013  	 FROM
14:50:31 2014  	   PRODUCT
14:50:31 2015  	 WHERE
14:50:31 2016  	   PRODUCT.ID IN (
14:50:31 2017  	     SELECT DISTINCT
14:50:31 2018  	       PRODUCT_OFFERING.PRODUCT_ID
14:50:31 2019  	     FROM
14:50:31 2020  	       PRODUCT_OFFERING
14:50:31 2021  	     WHERE
14:50:31 2022  	       PRODUCT_OFFERING.ID IN (
14:50:31 2023  		 SELECT DISTINCT
14:50:31 2024  		   OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID
14:50:31 2025  		 FROM
14:50:31 2026  		   OFFER_PRODUCT_OFFERING
14:50:31 2027  		 WHERE
14:50:31 2028  		   OFFER_PRODUCT_OFFERING.OFFER_ID IN (
14:50:31 2029  		     SELECT DISTINCT
14:50:31 2030  		       OFFER_OFFER_CHAIN.OFFER_ID
14:50:31 2031  		     FROM
14:50:31 2032  		       OFFER_OFFER_CHAIN
14:50:31 2033  		     WHERE
14:50:31 2034  		       OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain
14:50:31 2035  		   )
14:50:31 2036  	       )
14:50:31 2037  	   );
14:50:31 2038  
