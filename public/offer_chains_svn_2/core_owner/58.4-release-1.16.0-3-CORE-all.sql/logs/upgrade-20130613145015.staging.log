14:50:30  12  ) AS
14:50:30  13  -- VARIABLES
14:50:30  14  SPROC_NAME      CONSTANT VARCHAR2(14) := 'ADD_LINE_ITEMS';
14:50:30  15  temp_invoice_id NUMBER;
14:50:30  16  temp_offer_id   NUMBER;
14:50:30  17  
14:50:30  18  var_line_item_data SYS_REFCURSOR;
14:50:30  19  var_new_line_item_id NUMBER;
14:50:30  20  var_product_unit_price NUMBER (10,6);
14:50:30  21  var_product_offering_price NUMBER(10,6);
14:50:30  22  var_product_offering_oprice NUMBER(10,6);
14:50:30  23  var_product_quantity NUMBER;
14:50:30  24  var_product_offering_id NUMBER;
14:50:30  25  
14:50:30  26  var_line_item_price	  NUMBER(10,2);
14:50:30  27  var_discount_fixed_amount   NUMBER(10,6);
14:50:30  28  var_discount_percent_amount NUMBER(10,2);
14:50:30  29  
14:50:30  30  
14:50:30  31  -- EXCEPTIONS
14:50:30  32  BAD_INVOICE_ID EXCEPTION;
14:50:30  33  BAD_OFFER_ID EXCEPTION;
14:50:30  34  BAD_DISCOUNT EXCEPTION;
14:50:30  35  BEGIN
14:50:30  36  
14:50:30  37  	-- Check that given invoice exists
14:50:30  38  	BEGIN
14:50:30  39  	  SELECT
14:50:30  40  	    INVOICE.ID into temp_invoice_id
14:50:30  41  	  FROM
14:50:30  42  	    INVOICE
14:50:30  43  	  WHERE
14:50:30  44  	    INVOICE.ID = in_invoice_id;
14:50:30  45  
14:50:30  46  	  EXCEPTION
