14:50:31 399  
14:50:31 400  	-- Check that incoming data is correct
14:50:31 401  	IF in_instr_status != GLOBAL_CONSTANTS_V22.TRUE AND in_instr_status != GLOBAL_CONSTANTS_V22.FALSE THEN
14:50:31 402  	  RAISE WRONG_INSTR_EXCEPTION;
14:50:31 403  	END IF;
14:50:31 404  
14:50:31 405  	OPEN out_result_gc_set FOR
14:50:31 406  	SELECT
