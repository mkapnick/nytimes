14:50:31 1399  
14:50:31 1400  PROCEDURE ADD_SUBSCRIPTION_EXTENSION (
14:50:31 1401  /*
14:50:31 1402  APP_EXCEPTION_CODES_V22.NOT_FOUND
14:50:31 1403  APP_EXCEPTION_CODES_V22.INTERNAL_ERROR
14:50:31 1404  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
14:50:31 1405  */
14:50:31 1406  	 in_subscription_id	 IN NUMBER,
14:50:31 1407  	 in_effective_start_date IN DATE,
14:50:31 1408  	 in_effective_end_date	 IN DATE,
14:50:31 1409  	 in_note		 IN VARCHAR2,
14:50:31 1410  	 in_updated_by		 IN VARCHAR2
14:50:31 1411  ) AS
14:50:31 1412  -- VARIABLES
14:50:31 1413  SPROC_NAME		    CONSTANT VARCHAR2(26) := 'ADD_SUBSCRIPTION_EXTENSION';
14:50:31 1414  temp_subscription_id	    NUMBER;
14:50:31 1415  var_current_license_id	    NUMBER;
14:50:31 1416  var_current_license_start_date DATE;
14:50:31 1417  var_current_license_end_date DATE;
14:50:31 1418  var_current_offer_id	    NUMBER;
14:50:31 1419  var_current_invoice_id	    NUMBER;
14:50:31 1420  var_current_date 	    DATE;
14:50:31 1421  var_current_offer_index	    NUMBER;
14:50:31 1422  var_current_offer_recurr_num NUMBER;
14:50:31 1423  var_account_tax_exempt_id    VARCHAR2(255);
14:50:31 1424  
14:50:31 1425  var_free_invoice_id NUMBER;
14:50:31 1426  var_free_license_id NUMBER;
14:50:31 1427  var_new_license_id  NUMBER;
14:50:31 1428  var_ext_license_id  NUMBER;
14:50:31 1429  -- EXCEPTIONS
14:50:31 1430  BAD_SUBSCRIPTION_ID	     EXCEPTION;
14:50:31 1431  CAN_NOT_FIND_OFFER_OR_LICENSE EXCEPTION;
14:50:31 1432  CAN_NOT_CHANGE_LICENSE_STATUS EXCEPTION;
14:50:31 1433  CAN_NOT_CREATE_INVOICE	     EXCEPTION;
14:50:31 1434  CAN_NOT_CREATE_NEW_LICENSE    EXCEPTION;
14:50:31 1435  CAN_NOT_CREATE_END_LICENSE    EXCEPTION;
14:50:31 1436  CAN_NOT_ANNOTATE_SUBSCRIPTION EXCEPTION;
14:50:31 1437  EXTENS_START_DATE_IS_TOO_FAR  EXCEPTION;
14:50:31 1438  EXT_START_DATE_LATER_THEN_END EXCEPTION;
14:50:31 1439  EXTENS_START_DATE_IS_TOO_SMALL EXCEPTION;
14:50:31 1440  EXCEPTION_MESSAGE	     VARCHAR2(1024);
14:50:31 1441  BEGIN
14:50:31 1442  
14:50:31 1443  	 var_current_date := PROCS_COMMON_V22.NORMALIZE_DATE(SYSDATE);
14:50:31 1444  
14:50:31 1445  	 -- Check that subscription exists
14:50:31 1446  	 BEGIN
14:50:31 1447  	   SELECT
14:50:31 1448  	     SUBSCRIPTION.ID into temp_subscription_id
14:50:31 1449  	   FROM
14:50:31 1450  	     SUBSCRIPTION
14:50:31 1451  	   WHERE
14:50:31 1452  	     SUBSCRIPTION.ID = in_subscription_id;
14:50:31 1453  	   EXCEPTION
14:50:31 1454  	     WHEN NO_DATA_FOUND THEN
14:50:31 1455  	       RAISE BAD_SUBSCRIPTION_ID;
14:50:31 1456  	 END;
14:50:31 1457  
14:50:31 1458  	 -- Get account tax exempt id
14:50:31 1459  	 SELECT DISTINCT
14:50:31 1460  	   ACCOUNT.TAX_EXEMPT_ID into var_account_tax_exempt_id
14:50:31 1461  	 FROM
14:50:31 1462  	   ACCOUNT
14:50:31 1463  	   INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.ACCOUNT_ID = ACCOUNT.ID
14:50:31 1464  	 WHERE
14:50:31 1465  	   SUBSCRIPTION.ID = in_subscription_id;
14:50:31 1466  
14:50:31 1467  	 -- Select current data
14:50:31 1468  	 BEGIN
14:50:31 1469  	   SELECT
14:50:31 1470  	     LICENSE.ID,
14:50:31 1471  	     LICENSE.START_DATE,
14:50:31 1472  	     LICENSE.END_DATE,
14:50:31 1473  	     LICENSE.CURRENT_OFFER_INDEX,
14:50:31 1474  	     LICENSE.CURRENT_OFFER_RECURR_NUM,
14:50:31 1475  	     OFFER.ID,
14:50:31 1476  	     INVOICE.ID
14:50:31 1477  	   INTO
14:50:31 1478  	     var_current_license_id,
14:50:31 1479  	     var_current_license_start_date,
14:50:31 1480  	     var_current_license_end_date,
14:50:31 1481  	     var_current_offer_index,
14:50:31 1482  	     var_current_offer_recurr_num,
14:50:31 1483  	     var_current_offer_id,
14:50:31 1484  	     var_current_invoice_id
14:50:31 1485  	   FROM
14:50:31 1486  	     LICENSE
14:50:31 1487  	     INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
14:50:31 1488  	     INNER JOIN OFFER ON LICENSE.OFFER_ID = OFFER.ID
14:50:31 1489  	     INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
14:50:31 1490  	   WHERE
14:50:31 1491  	     SUBSCRIPTION.ID = in_subscription_id
14:50:31 1492  	     AND PROCS_COMMON_V22.NORMALIZE_DATE(LICENSE.END_DATE) > var_current_date
14:50:31 1493  	     AND PROCS_COMMON_V22.NORMALIZE_DATE(LICENSE.START_DATE) <= var_current_date
14:50:31 1494  	     AND ROWNUM <= 1
14:50:31 1495  	   ORDER BY
14:50:31 1496  	     LICENSE.ID DESC;
14:50:31 1497  	   EXCEPTION
14:50:31 1498  	     WHEN NO_DATA_FOUND THEN
14:50:31 1499  	       RAISE CAN_NOT_FIND_OFFER_OR_LICENSE;
14:50:31 1500  	 END;
14:50:31 1501  
14:50:31 1502  	 IF var_current_license_end_date < in_effective_start_date THEN
14:50:31 1503  	   RAISE EXTENS_START_DATE_IS_TOO_FAR;
14:50:31 1504  	 END IF;
14:50:31 1505  
14:50:31 1506  	 IF var_current_license_start_date > in_effective_start_date THEN
14:50:31 1507  	   RAISE EXTENS_START_DATE_IS_TOO_SMALL;
14:50:31 1508  	 END IF;
14:50:31 1509  
14:50:31 1510  	 IF in_effective_start_date > in_effective_end_date THEN
14:50:31 1511  	   RAISE EXT_START_DATE_LATER_THEN_END;
14:50:31 1512  	 END IF;
14:50:31 1513  
14:50:31 1514  	 -- Closing curent license
14:50:31 1515  	 BEGIN
14:50:31 1516  	   PROCS_LICENSE_CRU_V22.UPDATE_LICENSE(
14:50:31 1517  	     in_license_id	   => var_current_license_id,
14:50:31 1518  	     in_updated_by	   => in_updated_by,
14:50:31 1519  	     in_license_status_id  => GLOBAL_STATUSES_V22.LICENSE_CLOSED,
14:50:31 1520  	     in_end_date	   => in_effective_start_date,
14:50:31 1521  	     in_needs_entitlements => GLOBAL_CONSTANTS_V22.TRUE
14:50:31 1522  	   );
14:50:31 1523  	   EXCEPTION
14:50:31 1524  	     WHEN OTHERS THEN
14:50:31 1525  	       EXCEPTION_MESSAGE := SQLERRM;
14:50:31 1526  	       RAISE CAN_NOT_CHANGE_LICENSE_STATUS;
14:50:31 1527  	 END;
14:50:31 1528  
14:50:31 1529  	 -- Creating new "free" invoice
14:50:31 1530  	 BEGIN
14:50:31 1531  	   PROCS_INVOICE_V22.CREATE_INVOICE(
14:50:31 1532  	     in_invoice_status => GLOBAL_STATUSES_V22.INVOICE_CLOSED,
14:50:31 1533  	     in_created_by     => in_updated_by,
14:50:31 1534  	     in_tax_exempt_id  => var_account_tax_exempt_id,
14:50:31 1535  	     out_invoice_id    => var_free_invoice_id
14:50:31 1536  	   );
14:50:31 1537  	   EXCEPTION
14:50:31 1538  	     WHEN OTHERS THEN
14:50:31 1539  	       EXCEPTION_MESSAGE := SQLERRM;
14:50:31 1540  	       RAISE CAN_NOT_CREATE_INVOICE;
14:50:31 1541  	 END;
14:50:31 1542  
14:50:31 1543  	 -- Creating new "free" license
14:50:31 1544  	 BEGIN
14:50:31 1545  	   PROCS_LICENSE_V22.CREATE_LICENSE (
14:50:31 1546  	     in_status_id		 => GLOBAL_STATUSES_V22.LICENSE_ACTIVE,
14:50:31 1547  	     in_needs_entitlements	 => GLOBAL_CONSTANTS_V22.TRUE,
14:50:31 1548  	     in_start_date		 => in_effective_start_date,
14:50:31 1549  	     in_end_date		 => in_effective_end_date,
14:50:31 1550  	     in_offer_id		 => var_current_offer_id,
14:50:31 1551  	     in_subscription_id 	 => in_subscription_id,
14:50:31 1552  	     in_invoice_id		 => var_free_invoice_id,
14:50:31 1553  	     in_created_by		 => in_updated_by,
14:50:31 1554  	     in_is_extension		 => GLOBAL_CONSTANTS_V22.TRUE,
14:50:31 1555  	     in_current_offer_index	 => var_current_offer_index,
14:50:31 1556  	     in_current_offer_recurr_num => var_current_offer_recurr_num,
14:50:31 1557  	     out_license_id		 => var_free_license_id
14:50:31 1558  	   );
14:50:31 1559  	   EXCEPTION
14:50:31 1560  	     WHEN OTHERS THEN
14:50:31 1561  	       EXCEPTION_MESSAGE := SQLERRM;
14:50:31 1562  	       RAISE CAN_NOT_CREATE_NEW_LICENSE;
14:50:31 1563  	 END;
14:50:31 1564  
14:50:31 1565  	 -- Creating new license
14:50:31 1566  	 IF PROCS_COMMON_V22.NORMALIZE_DATE(var_current_license_end_date) >
14:50:31 1567  	    PROCS_COMMON_V22.NORMALIZE_DATE(in_effective_start_date) THEN
14:50:31 1568  	   BEGIN
14:50:31 1569  	     PROCS_LICENSE_V22.CREATE_LICENSE (
14:50:31 1570  	       in_status_id		   => GLOBAL_STATUSES_V22.LICENSE_ACTIVE,
14:50:31 1571  	       in_needs_entitlements	   => GLOBAL_CONSTANTS_V22.TRUE,
14:50:31 1572  	       in_start_date		   => in_effective_end_date,
14:50:31 1573  	       in_end_date		   => var_current_license_end_date + (in_effective_end_date - in_effective_start_date),
14:50:31 1574  	       in_offer_id		   => var_current_offer_id,
14:50:31 1575  	       in_subscription_id	   => in_subscription_id,
14:50:31 1576  	       in_invoice_id		   => var_current_invoice_id,
14:50:31 1577  	       in_created_by		   => in_updated_by,
14:50:31 1578  	       in_is_extension		   => GLOBAL_CONSTANTS_V22.FALSE,
14:50:31 1579  	       in_current_offer_index	   => var_current_offer_index,
14:50:31 1580  	       in_current_offer_recurr_num => var_current_offer_recurr_num,
14:50:31 1581  	       out_license_id		   => var_ext_license_id
14:50:31 1582  	     );
14:50:31 1583  	     EXCEPTION
14:50:31 1584  	       WHEN OTHERS THEN
14:50:31 1585  		 EXCEPTION_MESSAGE := SQLERRM;
14:50:31 1586  		 RAISE CAN_NOT_CREATE_END_LICENSE;
14:50:31 1587  	   END;
14:50:31 1588  	 END IF;
14:50:31 1589  
14:50:31 1590  	 -- Create new note for subscription
14:50:31 1591  	 BEGIN
14:50:31 1592  	   PROCS_SUBSCRIPTION_V22.ANNOTATE_SUBSCRIPTION (
14:50:31 1593  	     in_subscription_id => in_subscription_id,
14:50:31 1594  	     in_agent_id	=> 0, -- FIXME: What should to be here (agent id)?
14:50:31 1595  	     in_note		=> in_note,
14:50:31 1596  	     in_created_by	=> in_updated_by
14:50:31 1597  	   );
14:50:31 1598  	   EXCEPTION
14:50:31 1599  	     WHEN OTHERS THEN
14:50:31 1600  	       EXCEPTION_MESSAGE := SQLERRM;
14:50:31 1601  	       RAISE CAN_NOT_ANNOTATE_SUBSCRIPTION;
14:50:31 1602  	 END;
14:50:31 1603  
14:50:31 1604  EXCEPTION
14:50:31 1605  WHEN EXT_START_DATE_LATER_THEN_END THEN
14:50:31 1606  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INVALID_PARAMETER,
14:50:31 1607  	   SPROC_NAME, 'Extension start date is bigger then end date');
14:50:31 1608  WHEN EXTENS_START_DATE_IS_TOO_FAR THEN
14:50:31 1609  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INVALID_PARAMETER,
14:50:31 1610  	   SPROC_NAME, 'Extension start date is too far');
14:50:31 1611  WHEN EXTENS_START_DATE_IS_TOO_SMALL THEN
14:50:31 1612  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INVALID_PARAMETER,
14:50:31 1613  	   SPROC_NAME, 'Extension start date is too small');
14:50:31 1614  WHEN BAD_SUBSCRIPTION_ID THEN
14:50:31 1615  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 1616  	   SPROC_NAME, 'No such subscription');
14:50:31 1617  WHEN CAN_NOT_FIND_OFFER_OR_LICENSE THEN
14:50:31 1618  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 1619  	   SPROC_NAME, 'Could not find license and/or offer for given subscription');
14:50:31 1620  WHEN CAN_NOT_CHANGE_LICENSE_STATUS THEN
