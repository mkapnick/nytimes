14:50:31 2716  PROCEDURE SWITCH_FINANCIAL_INSTRUMENT (
14:50:31 2717  	 in_old_fin_instrument_id   IN NUMBER,
14:50:31 2718  	 in_old_fin_instrument_type IN NUMBER,
14:50:31 2719  	 in_new_fin_instrument_id   IN NUMBER,
14:50:31 2720  	 in_new_fin_instrument_type IN NUMBER,
14:50:31 2721  	 in_updated_by		    IN VARCHAR2
14:50:31 2722  ) AS
14:50:31 2723  SPROC_NAME CONSTANT VARCHAR2(27) := 'SWITCH_FINANCIAL_INSTRUMENT';
14:50:31 2724  -- variables
14:50:31 2725  temp_out_transaction_id NUMBER;
14:50:31 2726  temp_out_charge_id      NUMBER;
14:50:31 2727  var_accounts_count      NUMBER;
14:50:31 2728  var_transaction_type_old  "TRANSACTION".TRANSACTION_TYPE_CODE%TYPE;
14:50:31 2729  var_transaction_type	 "TRANSACTION".TRANSACTION_TYPE_CODE%TYPE;
14:50:31 2730  -- EXCEPTIONS
14:50:31 2731  BAD_OLD_CC   EXCEPTION;
14:50:31 2732  BAD_OLD_PP   EXCEPTION;
14:50:31 2733  BAD_OLD_TYPE EXCEPTION;
14:50:31 2734  BAD_NEW_CC   EXCEPTION;
14:50:31 2735  BAD_NEW_PP   EXCEPTION;
14:50:31 2736  BAD_NEW_TYPE EXCEPTION;
14:50:31 2737  DIFFERENT_OWNERS EXCEPTION;
14:50:31 2738  BEGIN
14:50:31 2739  
14:50:31 2740  	 IF in_old_fin_instrument_type = GLOBAL_ENUMS_V22.INSTRUMENT_CREDIT_CARD THEN
14:50:31 2741  	   IF PROCS_FIN_INSTRUMENTS_V22.IS_CREDIT_CARD_EXISTS(in_old_fin_instrument_id) = GLOBAL_CONSTANTS_V22.FALSE THEN
14:50:31 2742  	     -- throw exception: bad old credit card
14:50:31 2743  	     RAISE BAD_OLD_CC;
14:50:31 2744  	   END IF;
14:50:31 2745  	 ELSIF in_old_fin_instrument_type = GLOBAL_ENUMS_V22.INSTRUMENT_PAYPAL THEN
14:50:31 2746  	   IF PROCS_FIN_INSTRUMENTS_V22.IS_PAYPAL_EXISTS(in_old_fin_instrument_id) = GLOBAL_CONSTANTS_V22.FALSE THEN
14:50:31 2747  	     -- throw exception: bad old paypal
14:50:31 2748  	     RAISE BAD_OLD_PP;
14:50:31 2749  	   END IF;
14:50:31 2750  	 ELSE
14:50:31 2751  	   -- throw exception: bad instrument type
14:50:31 2752  	   RAISE BAD_OLD_TYPE;
14:50:31 2753  	 END IF;
14:50:31 2754  
14:50:31 2755  	 IF in_new_fin_instrument_type = GLOBAL_ENUMS_V22.INSTRUMENT_CREDIT_CARD THEN
14:50:31 2756  	   IF PROCS_FIN_INSTRUMENTS_V22.IS_CREDIT_CARD_EXISTS(in_new_fin_instrument_id) = GLOBAL_CONSTANTS_V22.FALSE THEN
14:50:31 2757  	     -- throw exception: bad new credit card
14:50:31 2758  	     RAISE BAD_NEW_CC;
14:50:31 2759  	   END IF;
14:50:31 2760  	 ELSIF in_new_fin_instrument_type = GLOBAL_ENUMS_V22.INSTRUMENT_PAYPAL THEN
14:50:31 2761  	   IF PROCS_FIN_INSTRUMENTS_V22.IS_PAYPAL_EXISTS(in_new_fin_instrument_id) = GLOBAL_CONSTANTS_V22.FALSE THEN
14:50:31 2762  	     -- throw exception: bad new paypal
14:50:31 2763  	     RAISE BAD_NEW_PP;
14:50:31 2764  	   END IF;
14:50:31 2765  	 ELSE
14:50:31 2766  	   -- throw exception: bad new instrument type
14:50:31 2767  	   RAISE BAD_NEW_TYPE;
14:50:31 2768  	 END IF;
14:50:31 2769  
14:50:31 2770  	 -- Check that owner of both instruments - same man
14:50:31 2771  
14:50:31 2772  	 SELECT count(1) into var_accounts_count FROM (
14:50:31 2773  	   SELECT
14:50:31 2774  	     CC.ACCOUNT_ID
14:50:31 2775  	   FROM
14:50:31 2776  	     CREDIT_CARD CC
14:50:31 2777  	   WHERE
14:50:31 2778  	     (
14:50:31 2779  	       CC.ID = in_old_fin_instrument_id
14:50:31 2780  	       AND in_old_fin_instrument_type = GLOBAL_ENUMS_V22.INSTRUMENT_CREDIT_CARD
14:50:31 2781  	     )
14:50:31 2782  	     OR
14:50:31 2783  	     (
14:50:31 2784  	       CC.ID = in_new_fin_instrument_id
14:50:31 2785  	       AND in_new_fin_instrument_type = GLOBAL_ENUMS_V22.INSTRUMENT_CREDIT_CARD
14:50:31 2786  	     )
14:50:31 2787  	   UNION
14:50:31 2788  	   SELECT
14:50:31 2789  	     PP.ACCOUNT_ID
14:50:31 2790  	   FROM
14:50:31 2791  	     PAYPAL PP
14:50:31 2792  	   WHERE
14:50:31 2793  	     (
14:50:31 2794  	       PP.ID = in_old_fin_instrument_id
14:50:31 2795  	       AND in_old_fin_instrument_type = GLOBAL_ENUMS_V22.INSTRUMENT_PAYPAL
14:50:31 2796  	     )
14:50:31 2797  	     OR
14:50:31 2798  	     (
14:50:31 2799  	       PP.ID = in_new_fin_instrument_id
14:50:31 2800  	       AND in_new_fin_instrument_type = GLOBAL_ENUMS_V22.INSTRUMENT_PAYPAL
14:50:31 2801  	     )
14:50:31 2802  	 )
14:50:31 2803  	 WHERE
14:50:31 2804  	   account_id IS NOT NULL;
14:50:31 2805  
14:50:31 2806  	 IF (var_accounts_count > 1) THEN
14:50:31 2807  	   -- Throw exception: different owners of instruments
14:50:31 2808  	   RAISE DIFFERENT_OWNERS;
14:50:31 2809  	 END IF;
14:50:31 2810  
14:50:31 2811  	 FOR f_sub IN (
14:50:31 2812  	   select
14:50:31 2813  	     s.id
14:50:31 2814  	   FROM
14:50:31 2815  	     subscription s
14:50:31 2816  	   WHERE
14:50:31 2817  	     (
14:50:31 2818  	       s.subscription_status_id = GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE OR
14:50:31 2819  	       s.subscription_status_id = GLOBAL_STATUSES_V22.SUBSCRIPTION_SUSPENDED OR
14:50:31 2820  	       s.subscription_status_id = GLOBAL_STATUSES_V22.SUBSCRIPTION_IN_GRACE_PERIOD
14:50:31 2821  	     )
14:50:31 2822  	     AND
14:50:31 2823  	     s.instrument_type_id = in_old_fin_instrument_type AND
14:50:31 2824  	     s.instrument_id = in_old_fin_instrument_id
14:50:31 2825  	 ) LOOP
14:50:31 2826  	   PROCS_SUBSCRIPTION_CRU_V22.UPDATE_SUBSCRIPTION(
14:50:31 2827  	     in_subscription_id => f_sub.id,
14:50:31 2828  	     in_instrument_type_id => in_new_fin_instrument_type,
14:50:31 2829  	     in_instrument_id => in_new_fin_instrument_id,
14:50:31 2830  	     in_updated_by => in_updated_by
14:50:31 2831  	   );
14:50:31 2832  	 END LOOP;
14:50:31 2833  
14:50:31 2834  	 FOR f_open_charge IN (
14:50:31 2835  	   select
14:50:31 2836  	     ch.id,
14:50:31 2837  	     ch.invoice_id,
14:50:31 2838  	     ch.transaction_id,
14:50:31 2839  	     ch.charge_amount
14:50:31 2840  	   FROM
14:50:31 2841  	     charge ch
14:50:31 2842  	   WHERE
14:50:31 2843  	     ch.instrument_type_id = in_old_fin_instrument_type
14:50:31 2844  	     AND ch.instrument_id = in_old_fin_instrument_id
14:50:31 2845  	     AND ch.charge_status_id = GLOBAL_STATUSES_V22.CHARGE_OPENED
14:50:31 2846  	 ) LOOP
14:50:31 2847  
14:50:31 2848  	   FOR f_pending_transaction IN (
14:50:31 2849  	     select
14:50:31 2850  	       id, transaction_amount, order_id, is_refund
14:50:31 2851  	     from
14:50:31 2852  	       transaction
14:50:31 2853  	     where
14:50:31 2854  	       id = f_open_charge.transaction_id
14:50:31 2855  	       and transaction_status_id = GLOBAL_STATUSES_V22.TRANSACTION_PENDING
14:50:31 2856  	   ) LOOP
14:50:31 2857  
14:50:31 2858  	     SELECT
14:50:31 2859  	       DECODE(TRANSACTION_TYPE_CODE, 'RECURRING_BILLING', 'RECURRING_BILLING_USER_UPDATE',
14:50:31 2860  					     'GRACE_PERIOD_FINAL', 'GRACE_PERIOD_USER_UPDATE',
14:50:31 2861  					     TRANSACTION_TYPE_CODE)
14:50:31 2862  	     INTO var_transaction_type
14:50:31 2863  	     FROM
14:50:31 2864  	       Transaction
14:50:31 2865  	     WHERE
14:50:31 2866  	       id = f_pending_transaction.id
14:50:31 2867  	       AND ROWNUM <= 1;
14:50:31 2868  
14:50:31 2869  	     PROCS_TRANSACTION_V22.CREATE_TRANSACTION (
14:50:31 2870  	       in_transaction_id => NULL,
14:50:31 2871  	       in_status_id  => GLOBAL_STATUSES_V22.TRANSACTION_PENDING,
14:50:31 2872  	       in_amount     => f_pending_transaction.transaction_amount,
14:50:31 2873  	       in_created_by => in_updated_by,
14:50:31 2874  	       in_order_id   => null,
14:50:31 2875  	       in_is_refund  => f_pending_transaction.is_refund,
14:50:31 2876  	       in_transaction_type_code => var_transaction_type,
14:50:31 2877  	       out_transaction_id => temp_out_transaction_id
