14:50:31 416  
14:50:31 417  PROCEDURE FINALIZE_SUBSCRIPTION_CREATION (
14:50:31 418  	in_subscription_id    IN NUMBER,
14:50:31 419  	in_invoice_id	      IN NUMBER,
14:50:31 420  	in_instrument_type_id IN NUMBER,
14:50:31 421  	in_instrument_id      IN NUMBER,
14:50:31 422  	in_created_by	      IN VARCHAR2
14:50:31 423  ) AS
14:50:31 424  SPROC_NAME CONSTANT VARCHAR2(30) := 'FINALIZE_SUBSCRIPTION_CREATION';
14:50:31 425  -- VARIABLES
14:50:31 426  var_invoice_amount     NUMBER(10, 2);
14:50:31 427  var_new_transaction_id NUMBER;
14:50:31 428  var_new_charge_id      NUMBER;
14:50:31 429  -- EXCEPTIONS
14:50:31 430  CAN_NOT_USE_FCINSTR	  EXCEPTION;
14:50:31 431  CAN_NOT_CALC_INVOICE_AMOUNT EXCEPTION;
14:50:31 432  CAN_NOT_CREATE_TRANSACTION  EXCEPTION;
14:50:31 433  CAN_NOT_CREATE_CHARGE	  EXCEPTION;
14:50:31 434  
14:50:31 435  EXCEPTION_MESSAGE VARCHAR2(1024);
14:50:31 436  BEGIN
14:50:31 437  
14:50:31 438  	-- Calculate invoice amount ( + discounts, taxes)
14:50:31 439  	BEGIN
14:50:31 440  	  PROCS_INVOICE_V22.CALCULATE_INVOICE_AMOUNT(
14:50:31 441  	    in_invoice_id => in_invoice_id,
14:50:31 442  	    out_amount	  => var_invoice_amount
14:50:31 443  	  );
14:50:31 444  	  EXCEPTION
14:50:31 445  	   WHEN OTHERS THEN
14:50:31 446  	     EXCEPTION_MESSAGE := SQLERRM;
14:50:31 447  	     RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
14:50:31 448  	END;
14:50:31 449  
14:50:31 450  	IF in_instrument_type_id = GLOBAL_ENUMS_V22.INSTRUMENT_ZCI_INSTRUMENT
14:50:31 451  	  AND var_invoice_amount > 0 THEN
14:50:31 452  	  RAISE CAN_NOT_USE_FCINSTR;
14:50:31 453  	END IF;
14:50:31 454  
14:50:31 455  	IF var_invoice_amount = 0 THEN
14:50:31 456  	  -- UPDATE INVOICE. SET STATUS TO PROCESSED
14:50:31 457  	  PROCS_INVOICE_CRU_V22.UPDATE_INVOICE(
14:50:31 458  	    in_invoice_id		   => in_invoice_id,
14:50:31 459  	    in_updated_by		   => in_created_by,
14:50:31 460  	    in_invoice_status_id	   => GLOBAL_STATUSES_V22.INVOICE_CLOSED
14:50:31 461  	  );
14:50:31 462  	ELSE
14:50:31 463  	  -- Create transaction and charge
14:50:31 464  	  BEGIN
14:50:31 465  	    PROCS_TRANSACTION_V22.CREATE_TRANSACTION(
14:50:31 466  	      in_transaction_id 	=> NULL,
14:50:31 467  	      in_status_id		=> GLOBAL_STATUSES_V22.TRANSACTION_PENDING,
14:50:31 468  	      in_amount 		=> var_invoice_amount,
14:50:31 469  	      in_created_by		=> in_created_by,
14:50:31 470  	      in_order_id		=> NULL,
14:50:31 471  	      in_transaction_type_code	=> 'START_SUBSCRIPTION',
14:50:31 472  	      out_transaction_id	=> var_new_transaction_id
14:50:31 473  	    );
14:50:31 474  	    EXCEPTION
14:50:31 475  	      WHEN OTHERS THEN
14:50:31 476  		EXCEPTION_MESSAGE := SQLERRM;
14:50:31 477  		RAISE CAN_NOT_CREATE_TRANSACTION;
14:50:31 478  	  END;
14:50:31 479  
14:50:31 480  	  BEGIN
14:50:31 481  	    PROCS_CHARGE_V22.CREATE_CHARGE(
14:50:31 482  	      in_invoice_id	    => in_invoice_id,
14:50:31 483  	      in_transaction_id     => var_new_transaction_id,
14:50:31 484  	      in_instrument_type_id => in_instrument_type_id,
14:50:31 485  	      in_instrument_id	    => in_instrument_id,
14:50:31 486  	      in_charge_amount	    => var_invoice_amount,
14:50:31 487  	      in_created_by	    => in_created_by,
14:50:31 488  	      in_charge_status_id   => GLOBAL_STATUSES_V22.CHARGE_OPENED,
14:50:31 489  	      out_charge_id	    => var_new_charge_id
14:50:31 490  	    );
14:50:31 491  	    EXCEPTION
14:50:31 492  	      WHEN OTHERS THEN
14:50:31 493  		EXCEPTION_MESSAGE := SQLERRM;
14:50:31 494  		RAISE CAN_NOT_CREATE_CHARGE;
14:50:31 495  	  END;
14:50:31 496  	END IF;
14:50:31 497  
14:50:31 498  EXCEPTION
14:50:31 499  WHEN CAN_NOT_USE_FCINSTR THEN
14:50:31 500  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.STATE_ERROR,
14:50:31 501  	  SPROC_NAME, 'Could not use "free charge instrument" for non-zero invoice');
14:50:31 502  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
14:50:31 503  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 504  	  SPROC_NAME, 'Could not calculate invoice amount', EXCEPTION_MESSAGE);
14:50:31 505  WHEN CAN_NOT_CREATE_TRANSACTION THEN
14:50:31 506  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 507  	  SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
14:50:31 508  WHEN CAN_NOT_CREATE_CHARGE THEN
14:50:31 509  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 510  	  SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
14:50:31 511  WHEN OTHERS THEN
14:50:31 512  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:31 513  	  SPROC_NAME, 'Unknown error', SQLERRM);
14:50:31 514  END FINALIZE_SUBSCRIPTION_CREATION;
