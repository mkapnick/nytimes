14:50:31 581  PROCEDURE GET_ACCOUNT_SUBSCRIPTIONS (
14:50:31 582  	  in_group_id	 IN  NUMBER,
14:50:31 583  	  in_start_date  IN DATE,
14:50:31 584  	  in_end_date	 IN DATE,
14:50:31 585  	  in_status	 IN NUMBER,
14:50:31 586  	  in_group_account_type IN VARCHAR2,
14:50:31 587  	  out_result_set OUT SYS_REFCURSOR
14:50:31 588  ) AS
14:50:31 589  -- VARIABLES
14:50:31 590  SPROC_NAME     CONSTANT VARCHAR2(25) := 'GET_ACCOUNT_SUBSCRIPTIONS';
14:50:31 591  var_account_id NUMBER;
14:50:31 592  -- EXCEPTIONS
14:50:31 593  BAD_GROUP_ID	      EXCEPTION;
14:50:31 594  BEGIN
14:50:31 595  	-- Get account id
14:50:31 596  	BEGIN
14:50:31 597  	  SELECT
14:50:31 598  	    ACCOUNT.ID INTO var_account_id
14:50:31 599  	  FROM
14:50:31 600  	    ACCOUNT
14:50:31 601  	  WHERE
14:50:31 602  	    ACCOUNT.GROUP_ID = in_group_id;
14:50:31 603  	  EXCEPTION
14:50:31 604  	    WHEN NO_DATA_FOUND THEN
14:50:31 605  	      RAISE BAD_GROUP_ID;
14:50:31 606  	END;
14:50:31 607  
14:50:31 608  	-- Get information about account subscriptions
14:50:31 609  	OPEN out_result_set FOR
14:50:31 610  	SELECT
14:50:31 611  	  in_group_id AS "GROUP_ID",
14:50:31 612  	  SUBSCRIPTION.ID AS "SUBSCRIPTION_ID",
14:50:31 613  	  SUBSCRIPTION.SUBSCRIPTION_STATUS_ID,
14:50:31 614  	  SUBSCRIPTION.PURCHASE_DATE,
14:50:31 615  	  SUBSCRIPTION.SUSPEND_DATE,
14:50:31 616  	  SUBSCRIPTION.REACTIVATION_DATE,
14:50:31 617  	  SUBSCRIPTION.CANCELLATION_DATE,
14:50:31 618  	  SUBSCRIPTION_CANCEL_REASON.VALUE as "CANCEL_TYPE",
14:50:31 619  	  SUBSCRIPTION.INSTRUMENT_ID,
14:50:31 620  	  SUBSCRIPTION.INSTRUMENT_TYPE_ID,
14:50:31 621  	  OFFER_CHAIN.ID AS "OFFER_CHAIN_ID",
14:50:31 622  	  OFFER_CHAIN.NAME,
14:50:31 623  	  OFFER_CHAIN.DESCRIPTION,
14:50:31 624  	  OFFER_CHAIN.PRODUCT_URI,
14:50:31 625  	  PROCS_SUBSCRIPTION_V22.CALC_SUBSCRIPTION_END_DATE(SUBSCRIPTION.ID) as "END_DATE",
14:50:31 626  	  PROCS_SUBSCRIPTION_V22.GET_RECENT_CHARGE(SUBSCRIPTION.ID) AS "RECENT_CHARGE",
14:50:31 627  	  PROCS_SUBSCRIPTION_V22.GET_RENEWAL_DATE(SUBSCRIPTION.ID) AS "RENEWAL_DATE",
14:50:31 628  	  PROCS_SUBSCRIPTION_V22.GET_BILLING_CYCLE(SUBSCRIPTION.ID) AS "BILLING_CYCLE",
14:50:31 629  	  PROCS_SUBSCRIPTION_V22.IS_SUBSCRIPTION_CANCELABLE(SUBSCRIPTION.ID) AS "IS_CANCELABLE",
14:50:31 630  	  ITUNES_RECEIPT.ID AS "ITUNES_RECEIPT_ID",
14:50:31 631  	  (
14:50:31 632  	    SELECT
14:50:31 633  	      MAX(ENTITLEMENT_END_DATE)
14:50:31 634  	      FROM LICENSE
14:50:31 635  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
14:50:31 636  	  )
14:50:31 637  	  as "ENT_END_DATE",
14:50:31 638  	  (
14:50:31 639  	    SELECT
14:50:31 640  	      MIN(START_DATE)
14:50:31 641  	      FROM LICENSE
14:50:31 642  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
14:50:31 643  	  )
14:50:31 644  	  as "ENT_START_DATE",
14:50:31 645  	  GROUP_ACCOUNT.SUBSCRIPTION_ID GA_SUBSCRIPTION_ID,
14:50:31 646  	  GROUP_ACCOUNT.ID GA_ID,
14:50:31 647  	  GROUP_ACCOUNT.GROUP_NAME GA_GROUP_NAME,
14:50:31 648  	  GROUP_ACCOUNT.FIRST_NAME GA_FIRST_NAME,
14:50:31 649  	  GROUP_ACCOUNT.LAST_NAME GA_LAST_NAME,
14:50:31 650  	  GROUP_ACCOUNT.EMAIL GA_EMAIL,
14:50:31 651  	  GROUP_ACCOUNT.PHONE GA_PHONE,
14:50:31 652  	  GROUP_ACCOUNT.ORGANIZATION_TYPE GA_ORGANIZATION_TYPE,
14:50:31 653  	  GROUP_ACCOUNT.SEATS GA_SEATS,
14:50:31 654  	  PROCS_GROUP_ACCOUNT_V22.F_GET_NUM_OCCUPIED_GROUP_SEATS(GROUP_ACCOUNT.ID) GA_SEATS_USED,
14:50:31 655  	  GROUP_ACCOUNT.IP GA_IP,
14:50:31 656  	  PROCS_SUBSCRIPTION_V22.GET_GIFT_CERT_CODE_BY_SUB_ID(SUBSCRIPTION.ID) GIFT_CERTIFICATE_CODE,
14:50:31 657  	  PROCS_ACCOUNT_V22.GET_GRACE_START_DATE(SUBSCRIPTION.ID) GRACE_START_DATE,
14:50:31 658  	  PROCS_ACCOUNT_V22.GET_GRACE_END_DATE(SUBSCRIPTION.ID) GRACE_END_DATE
14:50:31 659  	FROM
14:50:31 660  	  SUBSCRIPTION
14:50:31 661  	  INNER JOIN OFFER_CHAIN ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
14:50:31 662  	  LEFT JOIN SUBSCRIPTION_CANCEL_REASON ON SUBSCRIPTION.SCT_ID = SUBSCRIPTION_CANCEL_REASON.ID
14:50:31 663  	  LEFT JOIN ITUNES_RECEIPT ON SUBSCRIPTION.ID = ITUNES_RECEIPT.SUBSCRIPTION_ID
14:50:31 664  	  LEFT JOIN GROUP_ACCOUNT ON SUBSCRIPTION.ID = GROUP_ACCOUNT.SUBSCRIPTION_ID
14:50:31 665  	WHERE
14:50:31 666  	  SUBSCRIPTION.ACCOUNT_ID = var_account_id
14:50:31 667  	  AND (SUBSCRIPTION.SCT_ID IS NULL OR SUBSCRIPTION.SCT_ID != GLOBAL_STATUSES_V22.REAL_TIME_CANCEL_REASON)
14:50:31 668  	  AND SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = NVL(in_status, SUBSCRIPTION.SUBSCRIPTION_STATUS_ID)
14:50:31 669  	  AND PROCS_COMMON_V22.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE) >= NVL(in_start_date, PROCS_COMMON_V22.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE))
14:50:31 670  	  AND PROCS_COMMON_V22.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE) <= NVL(in_end_date, PROCS_COMMON_V22.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE))
14:50:31 671  	  AND (in_group_account_type IS NULL OR OFFER_CHAIN.GROUP_ACCOUNT_TYPE_ID = in_group_account_type);
14:50:31 672  
14:50:31 673  EXCEPTION
14:50:31 674  WHEN BAD_GROUP_ID THEN
14:50:31 675  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 676  	  SPROC_NAME, 'Can not find account with given group id');
14:50:31 677  WHEN OTHERS THEN
14:50:31 678  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:31 679  	  SPROC_NAME, 'Unknown error', SQLERRM);
14:50:31 680  END GET_ACCOUNT_SUBSCRIPTIONS;
