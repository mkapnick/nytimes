14:50:31 1097  
14:50:31 1098  PROCEDURE GET_INVOICE_DAYS_USED_NUMBER (
14:50:31 1099  	 in_invoice_id	     IN NUMBER,
14:50:31 1100  	 in_chargeback_date  IN DATE DEFAULT SYSDATE,
14:50:31 1101  	 out_days_num	     OUT NUMBER
14:50:31 1102  ) AS
14:50:31 1103  SPROC_NAME CONSTANT VARCHAR2(28) := 'GET_INVOICE_DAYS_USED_NUMBER';
14:50:31 1104  -- VARIABLES
14:50:31 1105  temp_invoice_id	      NUMBER;
14:50:31 1106  var_license_start_date DATE;
14:50:31 1107  var_license_end_date   DATE;
14:50:31 1108  var_chargeback_date    DATE;
14:50:31 1109  var_invoice_days_used  NUMBER;
14:50:31 1110  -- EXCEPTIONS
14:50:31 1111  BAD_INVOICE_ID EXCEPTION;
14:50:31 1112  BEGIN
14:50:31 1113  
14:50:31 1114  	 var_chargeback_date := NVL(in_chargeback_date, SYSDATE);
14:50:31 1115  
14:50:31 1116  	 BEGIN
14:50:31 1117  	   SELECT
14:50:31 1118  	     INVOICE.ID into temp_invoice_id
14:50:31 1119  	   FROM
14:50:31 1120  	     INVOICE
14:50:31 1121  	   WHERE
14:50:31 1122  	     INVOICE.ID = in_invoice_id;
14:50:31 1123  	   EXCEPTION
14:50:31 1124  	     WHEN NO_DATA_FOUND THEN
14:50:31 1125  	       RAISE BAD_INVOICE_ID;
14:50:31 1126  	 END;
14:50:31 1127  
14:50:31 1128  	 var_invoice_days_used := 0;
14:50:31 1129  
14:50:31 1130  	 FOR f_license IN (
14:50:31 1131  	   SELECT
14:50:31 1132  	     LICENSE.START_DATE,
14:50:31 1133  	     LICENSE.END_DATE
14:50:31 1134  	   FROM
14:50:31 1135  	     LICENSE
14:50:31 1136  	   WHERE
14:50:31 1137  	     LICENSE.INVOICE_ID = in_invoice_id
14:50:31 1138  	     AND LICENSE.IS_EXTENSION = GLOBAL_CONSTANTS_V22.FALSE
14:50:31 1139  	 )
14:50:31 1140  	 LOOP
14:50:31 1141  	   var_license_start_date := PROCS_COMMON_V22.NORMALIZE_DATE(f_license.START_DATE);
14:50:31 1142  	   var_license_end_date := PROCS_COMMON_V22.NORMALIZE_DATE(f_license.END_DATE);
14:50:31 1143  
14:50:31 1144  	   IF var_license_start_date <= var_chargeback_date THEN
14:50:31 1145  	     IF var_license_end_date <= var_chargeback_date THEN
14:50:31 1146  	       -- License is passed
14:50:31 1147  	       var_invoice_days_used := var_invoice_days_used + (var_license_end_date - var_license_start_date);
14:50:31 1148  	     ELSE
14:50:31 1149  	       -- This is current license
14:50:31 1150  	       var_invoice_days_used := var_invoice_days_used + (var_chargeback_date - var_license_start_date);
14:50:31 1151  	     END IF;
14:50:31 1152  	   ELSE
14:50:31 1153  	     -- if var_license_start_date > in_chargeback_date then do nothing
14:50:31 1154  	     NULL;
14:50:31 1155  	   END IF;
14:50:31 1156  	 END LOOP;
14:50:31 1157  
14:50:31 1158  	 out_days_num := var_invoice_days_used;
14:50:31 1159  
14:50:31 1160  EXCEPTION
14:50:31 1161  WHEN BAD_INVOICE_ID THEN
14:50:31 1162  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 1163  	   SPROC_NAME, 'No such invoice');
14:50:31 1164  WHEN OTHERS THEN
14:50:31 1165  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:31 1166  	   SPROC_NAME, 'Unknown error', SQLERRM);
14:50:31 1167  END GET_INVOICE_DAYS_USED_NUMBER;
