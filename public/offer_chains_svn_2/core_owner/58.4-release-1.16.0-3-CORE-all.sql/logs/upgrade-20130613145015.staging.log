14:50:31 913  BEGIN
14:50:31 914  	  SELECT OFFER_CHAIN.GROUP_ACCOUNT_TYPE_ID into var_ga_type
14:50:31 915  	  FROM
14:50:31 916  	    GROUP_ACCOUNT,
14:50:31 917  	    SUBSCRIPTION,
14:50:31 918  	    OFFER_CHAIN
14:50:31 919  	  WHERE
14:50:31 920  	    GROUP_ACCOUNT.ID = in_group_account_id AND
14:50:31 921  	    GROUP_ACCOUNT.SUBSCRIPTION_ID = SUBSCRIPTION.ID AND
14:50:31 922  	    SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
14:50:31 923  	  ;
14:50:31 924  	  IF(var_ga_type != 'GL') THEN
14:50:31 925  	    RAISE NOT_GL;
14:50:31 926  	  END IF;
14:50:31 927  
14:50:31 928  	  PROCS_GROUP_ACCOUNT_CRU_V22.ADD_IP_RANGE(
14:50:31 929  	    in_group_account_id => in_group_account_id,
14:50:31 930  	    in_minimum_ip_string => in_minimum_ip_string,
14:50:31 931  	    in_minimum_ip_low => in_minimum_ip_low,
14:50:31 932  	    in_minimum_ip_high => in_minimum_ip_high,
14:50:31 933  	    in_maximum_ip_string => in_maximum_ip_string,
14:50:31 934  	    in_maximum_ip_low => in_maximum_ip_low,
14:50:31 935  	    in_maximum_ip_high => in_maximum_ip_high,
14:50:31 936  	    in_created_by => in_created_by
14:50:31 937  	  );
14:50:31 938  
14:50:31 939  	  -- Check for overlapping ip address range after insert.  Note that if another
14:50:31 940  	  -- call to add_ip_range has not completed, overlapping ip entries can occur.
14:50:31 941  	  SELECT count(1) into var_is_dupe
14:50:31 942  	  FROM
14:50:31 943  	    GROUP_ACCOUNT_IP_RANGE
14:50:31 944  	  WHERE
14:50:31 945  	    GROUP_ACC_IP_RNG_STATUS_ID = GLOBAL_STATUSES_V22.GROUP_ACC_IP_RNG_ACTIVE AND
14:50:31 946  		((
14:50:31 947  		  (in_minimum_ip_high > minimum_ip_high and in_minimum_ip_high < maximum_ip_high) or
14:50:31 948  		  (in_minimum_ip_high = minimum_ip_high and (in_minimum_ip_low >= minimum_ip_low and in_minimum_ip_low <= maximum_ip_low)) or
14:50:31 949  		  (in_minimum_ip_high = maximum_ip_high and (in_minimum_ip_low >= minimum_ip_low and in_minimum_ip_low <= maximum_ip_low))
