14:50:31 1480  
14:50:31 1481  	 IF var_is_tax_exempt IS NULL THEN
14:50:31 1482  	   out_is_tax_exempt := GLOBAL_CONSTANTS_V22.FALSE;
14:50:31 1483  	 ELSE
