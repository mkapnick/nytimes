14:50:31 2454  
14:50:31 2455  PROCEDURE FINALIZE_FALSE_START (
14:50:31 2456  /*
14:50:31 2457  APP_EXCEPTION_CODES_V22.NOT_FOUND
14:50:31 2458  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
14:50:31 2459  APP_EXCEPTION_CODES_V22.INTERNAL_ERROR
14:50:31 2460  */
14:50:31 2461  --  in_subscription_id	 IN SUBSCRIPTION.ID%TYPE,
14:50:31 2462  --  in_cancelation_date	 IN SUBSCRIPTION.CANCELLATION_DATE%TYPE,
14:50:31 2463  --  in_note		 IN SUBSCRIPTION_NOTE.NOTE%TYPE,
14:50:31 2464  --  in_agent_id		 IN SUBSCRIPTION_NOTE.AGENT_ID%TYPE,
14:50:31 2465  --  in_updated_by	 IN SUBSCRIPTION.UPDATED_BY%TYPE
14:50:31 2466  	 in_subscription_id    IN NUMBER,
14:50:31 2467  	 in_cancelation_date   IN DATE,
14:50:31 2468  	 in_note	       IN VARCHAR2,
14:50:31 2469  	 in_agent_id	       IN NUMBER,
14:50:31 2470  	 in_updated_by	       IN VARCHAR2
14:50:31 2471  ) AS
14:50:31 2472  SPROC_NAME CONSTANT VARCHAR2(20) := 'FINALIZE_FALSE_START';
14:50:31 2473  FALSE_START_REASON CONSTANT NUMBER := 41;
14:50:31 2474  -- VARIABLES
14:50:31 2475  var_current_subscr_status SUBSCRIPTION.SUBSCRIPTION_STATUS_ID%TYPE;
14:50:31 2476  var_active_invoices_count NUMBER;
14:50:31 2477  -- EXCEPTIONS
14:50:31 2478  BAD_SUBSCRIPTION_ID	   EXCEPTION;
14:50:31 2479  BAD_SUBSCRIPTION_STATUS	   EXCEPTION;
14:50:31 2480  CAN_NOT_UPDATE_SUBSCRIPTION EXCEPTION;
14:50:31 2481  ACTIVE_INVOICES_FOUND	   EXCEPTION;
14:50:31 2482  CAN_NOT_CREATE_NOTE	   EXCEPTION;
14:50:31 2483  EXCEPTION_MESSAGE	   VARCHAR2(1024);
14:50:31 2484  BEGIN
14:50:31 2485  
14:50:31 2486  	 -- Get current subscription status
14:50:31 2487  	 BEGIN
14:50:31 2488  	   SELECT
14:50:31 2489  	     SUBSCRIPTION.SUBSCRIPTION_STATUS_ID into var_current_subscr_status
14:50:31 2490  	   FROM
14:50:31 2491  	     SUBSCRIPTION
14:50:31 2492  	   WHERE
14:50:31 2493  	     SUBSCRIPTION.ID = in_subscription_id;
14:50:31 2494  	   EXCEPTION
14:50:31 2495  	     WHEN NO_DATA_FOUND THEN
14:50:31 2496  	       RAISE BAD_SUBSCRIPTION_ID;
14:50:31 2497  	 END;
14:50:31 2498  
14:50:31 2499  	 -- Check for invoices with active licenses
14:50:31 2500  	 SELECT
14:50:31 2501  	   COUNT(*) into var_active_invoices_count
14:50:31 2502  	 FROM
14:50:31 2503  	   LICENSE
14:50:31 2504  	   INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
14:50:31 2505  	 WHERE
14:50:31 2506  	   LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V22.LICENSE_ACTIVE
14:50:31 2507  	   AND LICENSE.SUBSCRIPTION_ID = in_subscription_id;
14:50:31 2508  
14:50:31 2509  	 IF var_active_invoices_count > 0 THEN
14:50:31 2510  	   RAISE ACTIVE_INVOICES_FOUND;
14:50:31 2511  	 END IF;
14:50:31 2512  
14:50:31 2513  	 -- Check that subscription is active
14:50:31 2514  	 IF var_current_subscr_status != GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE
14:50:31 2515  	    AND var_current_subscr_status != GLOBAL_STATUSES_V22.SUBSCRIPTION_SUSPENDED THEN
14:50:31 2516  	   RAISE BAD_SUBSCRIPTION_STATUS;
14:50:31 2517  	 END IF;
14:50:31 2518  
14:50:31 2519  	 -- Update subscription data
14:50:31 2520  	 BEGIN
14:50:31 2521  	   PROCS_SUBSCRIPTION_CRU_V22.UPDATE_SUBSCRIPTION(
14:50:31 2522  	     in_subscription_id        => in_subscription_id,
14:50:31 2523  	     in_subscription_status_id => GLOBAL_STATUSES_V22.SUBSCRIPTION_FALSE_START,
14:50:31 2524  	     in_cancelation_date       => in_cancelation_date,
14:50:31 2525  	     in_updated_by	       => in_updated_by,
14:50:31 2526  	     in_sct_id		       => FALSE_START_REASON
14:50:31 2527  	   );
14:50:31 2528  	   EXCEPTION
14:50:31 2529  	     WHEN OTHERS THEN
14:50:31 2530  	       EXCEPTION_MESSAGE := SQLERRM;
14:50:31 2531  	       RAISE CAN_NOT_UPDATE_SUBSCRIPTION;
14:50:31 2532  	 END;
14:50:31 2533  
14:50:31 2534  	 -- Annotate subscription
14:50:31 2535  	 IF in_note IS NOT NULL THEN
14:50:31 2536  	   BEGIN
14:50:31 2537  	     PROCS_SUBSCRIPTION_V22.ANNOTATE_SUBSCRIPTION(
14:50:31 2538  	       in_subscription_id,
14:50:31 2539  	       in_agent_id,
14:50:31 2540  	       in_note,
14:50:31 2541  	       in_updated_by
14:50:31 2542  	     );
14:50:31 2543  	     EXCEPTION
14:50:31 2544  	      WHEN OTHERS THEN
14:50:31 2545  		EXCEPTION_MESSAGE := SQLERRM;
14:50:31 2546  		RAISE CAN_NOT_CREATE_NOTE;
14:50:31 2547  	   END;
14:50:31 2548  	 END IF;
14:50:31 2549  
14:50:31 2550  EXCEPTION
14:50:31 2551  WHEN BAD_SUBSCRIPTION_ID THEN
14:50:31 2552  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 2553  	   SPROC_NAME, 'No such subscription');
14:50:31 2554  WHEN BAD_SUBSCRIPTION_STATUS THEN
14:50:31 2555  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INVALID_PARAMETER,
14:50:31 2556  	   SPROC_NAME, 'Bad current subscription status');
14:50:31 2557  WHEN CAN_NOT_UPDATE_SUBSCRIPTION THEN
14:50:31 2558  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 2559  	   SPROC_NAME, 'Could not update subscription data', EXCEPTION_MESSAGE);
14:50:31 2560  WHEN ACTIVE_INVOICES_FOUND THEN
14:50:31 2561  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.STATE_ERROR,
14:50:31 2562  	   SPROC_NAME, 'Invoices with active licenses found');
14:50:31 2563  WHEN CAN_NOT_CREATE_NOTE THEN
14:50:31 2564  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 2565  	   SPROC_NAME, 'Could not annotate subscription', EXCEPTION_MESSAGE);
14:50:31 2566  WHEN OTHERS THEN
14:50:31 2567  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:31 2568  	   SPROC_NAME, 'Unknown error', SQLERRM);
14:50:31 2569  END FINALIZE_FALSE_START;
