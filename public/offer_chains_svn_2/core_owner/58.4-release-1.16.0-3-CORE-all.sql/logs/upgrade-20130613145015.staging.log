14:50:31 1641  
14:50:31 1642  FUNCTION CALC_SUBSCRIPTION_END_DATE (
14:50:31 1643  /*
14:50:31 1644  APP_EXCEPTION_CODES_V22.NOT_FOUND
14:50:31 1645  APP_EXCEPTION_CODES_V22.INTERNAL_ERROR
14:50:31 1646  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
14:50:31 1647  Returns:
14:50:31 1648  NULL if it is impossible to calculate end date (for example,
14:50:31 1649  	 offer chain includes offer with infinity recurrences number)
14:50:31 1650  DATE else
14:50:31 1651  */
14:50:31 1652  	 in_subscription_id IN NUMBER
14:50:31 1653  ) RETURN DATE AS
14:50:31 1654  -- VARIABLES
14:50:31 1655  SPROC_NAME		     CONSTANT VARCHAR2(26) := 'CALC_SUBSCRIPTION_END_DATE';
14:50:31 1656  last_license_id		     NUMBER;
14:50:31 1657  last_license_end_date	     DATE;
14:50:31 1658  last_license_offer_id	     NUMBER;
14:50:31 1659  last_license_offer_index      NUMBER;
14:50:31 1660  last_license_offer_recurr_num NUMBER;
14:50:31 1661  var_last_license_id	     NUMBER;
14:50:31 1662  var_offer_chain_id	     NUMBER;
14:50:31 1663  var_current_offer_rec_number  NUMBER;
14:50:31 1664  var_next_offers_set	     SYS_REFCURSOR;
14:50:31 1665  var_next_offer_duration	     VARCHAR2(30);
14:50:31 1666  var_next_offer_recur	     NUMBER;
14:50:31 1667  var_infinity_offers_number    NUMBER;
14:50:31 1668  
14:50:31 1669  var_result_date DATE;
14:50:31 1670  
14:50:31 1671  var_ym_interval	INTERVAL YEAR TO MONTH;
14:50:31 1672  var_ds_interval	INTERVAL DAY(3) TO SECOND;
14:50:31 1673  var_offer_years	NUMBER;
14:50:31 1674  var_offer_months NUMBER;
14:50:31 1675  var_offer_days	NUMBER;
14:50:31 1676  
14:50:31 1677  -- EXCEPTIONS
14:50:31 1678  BAD_SUBSCRIPTION_ID	  EXCEPTION;
14:50:31 1679  CAN_NOT_FIND_LAST_LICENSE  EXCEPTION;
14:50:31 1680  CAN_NOT_CALC_OFFER_LENGTH  EXCEPTION;
14:50:31 1681  CAN_NOT_CALC_OFFER_LENGTH2 EXCEPTION;
14:50:31 1682  EXCEPTION_MESSAGE	  VARCHAR2(1024);
14:50:31 1683  BEGIN
14:50:31 1684  
14:50:31 1685  	 BEGIN
14:50:31 1686  	   SELECT
14:50:31 1687  	     SUBSCRIPTION.OFFER_CHAIN_ID into var_offer_chain_id
14:50:31 1688  	   FROM
14:50:31 1689  	     SUBSCRIPTION
14:50:31 1690  	   WHERE
14:50:31 1691  	     SUBSCRIPTION.ID = in_subscription_id;
14:50:31 1692  	   EXCEPTION
14:50:31 1693  	     WHEN NO_DATA_FOUND THEN
14:50:31 1694  	       RAISE BAD_SUBSCRIPTION_ID;
14:50:31 1695  	 END;
14:50:31 1696  
14:50:31 1697  	 BEGIN
14:50:31 1698  	   SELECT
14:50:31 1699  	     ID into var_last_license_id
14:50:31 1700  	   FROM
14:50:31 1701  	     (
14:50:31 1702  	       SELECT
14:50:31 1703  		 LICENSE.ID
14:50:31 1704  	       FROM
14:50:31 1705  		 LICENSE
14:50:31 1706  	       WHERE
14:50:31 1707  		 LICENSE.SUBSCRIPTION_ID = in_subscription_id
14:50:31 1708  	       ORDER BY
14:50:31 1709  		 LICENSE.END_DATE DESC
14:50:31 1710  	     )
14:50:31 1711  	   WHERE
14:50:31 1712  	     ROWNUM <= 1;
14:50:31 1713  	   EXCEPTION
14:50:31 1714  	     WHEN NO_DATA_FOUND THEN
14:50:31 1715  	       RAISE CAN_NOT_FIND_LAST_LICENSE;
14:50:31 1716  	 END;
14:50:31 1717  
14:50:31 1718  	 SELECT
14:50:31 1719  	   COUNT(*) into var_infinity_offers_number
14:50:31 1720  	 FROM
14:50:31 1721  	   OFFER_OFFER_CHAIN
14:50:31 1722  	 WHERE
14:50:31 1723  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
14:50:31 1724  	   AND OFFER_OFFER_CHAIN.NUM_RECURRENCES = GLOBAL_ENUMS_V22.OFFER_REC_INFINITY;
14:50:31 1725  
14:50:31 1726  	 IF var_infinity_offers_number > 0 THEN
14:50:31 1727  	   RETURN NULL;
14:50:31 1728  	 END IF;
14:50:31 1729  
14:50:31 1730  	 BEGIN
14:50:31 1731  	   SELECT
14:50:31 1732  	     LICENSE.ID,
14:50:31 1733  	     LICENSE.END_DATE,
14:50:31 1734  	     LICENSE.CURRENT_OFFER_INDEX,
14:50:31 1735  	     LICENSE.CURRENT_OFFER_RECURR_NUM,
14:50:31 1736  	     LICENSE.OFFER_ID
14:50:31 1737  	     into
14:50:31 1738  	     last_license_id,
14:50:31 1739  	     last_license_end_date,
14:50:31 1740  	     last_license_offer_index,
14:50:31 1741  	     last_license_offer_recurr_num,
14:50:31 1742  	     last_license_offer_id
14:50:31 1743  	   FROM
14:50:31 1744  	     LICENSE
14:50:31 1745  	   WHERE
14:50:31 1746  	     LICENSE.ID = var_last_license_id;
14:50:31 1747  	   EXCEPTION
14:50:31 1748  	     WHEN NO_DATA_FOUND THEN
14:50:31 1749  	       RAISE CAN_NOT_FIND_LAST_LICENSE;
14:50:31 1750  	 END;
14:50:31 1751  
14:50:31 1752  	 var_result_date := last_license_end_date;
14:50:31 1753  
14:50:31 1754  	 -- Find current recurrence number
14:50:31 1755  	 SELECT
14:50:31 1756  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES into var_current_offer_rec_number
14:50:31 1757  	 FROM
14:50:31 1758  	   OFFER_OFFER_CHAIN
14:50:31 1759  	 WHERE
14:50:31 1760  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
14:50:31 1761  	   AND OFFER_OFFER_CHAIN.OFFER_ID = last_license_offer_id
14:50:31 1762  	   AND OFFER_OFFER_CHAIN.INDEX_VALUE = last_license_offer_index;
14:50:31 1763  
14:50:31 1764  	 IF var_current_offer_rec_number > last_license_offer_recurr_num THEN
14:50:31 1765  	   BEGIN
14:50:31 1766  	     PROCS_OFFER_CHAIN_V22.GET_OFFER_LENGTH(
14:50:31 1767  	       last_license_offer_id,
14:50:31 1768  	       var_offer_years,
14:50:31 1769  	       var_offer_months,
14:50:31 1770  	       var_offer_days
14:50:31 1771  	     );
14:50:31 1772  
14:50:31 1773  	     var_ym_interval := var_offer_years||'-'||var_offer_months;
14:50:31 1774  	     var_ds_interval := var_offer_days||' 0:0:0';
14:50:31 1775  
14:50:31 1776  	     var_result_date := var_result_date
14:50:31 1777  	       + ( var_ym_interval * ( var_current_offer_rec_number - last_license_offer_recurr_num ) )
14:50:31 1778  	       + ( var_ds_interval * ( var_current_offer_rec_number - last_license_offer_recurr_num ) );
14:50:31 1779  	     EXCEPTION
14:50:31 1780  	       WHEN OTHERS THEN
14:50:31 1781  		 EXCEPTION_MESSAGE := SQLERRM;
14:50:31 1782  		 RAISE CAN_NOT_CALC_OFFER_LENGTH;
14:50:31 1783  	   END;
14:50:31 1784  	 END IF;
14:50:31 1785  
14:50:31 1786  	 OPEN var_next_offers_set FOR
14:50:31 1787  	 SELECT
14:50:31 1788  	   OFFER.ENTITLEMENT_DURATION,
14:50:31 1789  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES
14:50:31 1790  	 FROM
14:50:31 1791  	   OFFER_OFFER_CHAIN
14:50:31 1792  	   INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
14:50:31 1793  	 WHERE
14:50:31 1794  	   OFFER_OFFER_CHAIN.INDEX_VALUE > last_license_offer_index
14:50:31 1795  	   AND OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id;
14:50:31 1796  
14:50:31 1797  	 LOOP
14:50:31 1798  	   FETCH var_next_offers_set into var_next_offer_duration, var_next_offer_recur;
14:50:31 1799  	   EXIT WHEN var_next_offers_set%NOTFOUND;
14:50:31 1800  	   BEGIN
14:50:31 1801  	     PROCS_COMMON_V22.ISO8601DURATION_TO_INTERVALS(
14:50:31 1802  	       var_next_offer_duration,
14:50:31 1803  	       var_offer_years,
14:50:31 1804  	       var_offer_months,
14:50:31 1805  	       var_offer_days
14:50:31 1806  	     );
14:50:31 1807  
14:50:31 1808  	     var_ym_interval := var_offer_years||'-'||var_offer_months;
14:50:31 1809  	     var_ds_interval := var_offer_days||' 0:0:0';
14:50:31 1810  
14:50:31 1811  	     var_result_date := var_result_date
14:50:31 1812  	       + ( var_ym_interval * var_next_offer_recur )
14:50:31 1813  	       + ( var_ds_interval * var_next_offer_recur );
14:50:31 1814  	     EXCEPTION
14:50:31 1815  	       WHEN OTHERS THEN
14:50:31 1816  		 EXCEPTION_MESSAGE := SQLERRM;
14:50:31 1817  		 RAISE CAN_NOT_CALC_OFFER_LENGTH2;
14:50:31 1818  	   END;
14:50:31 1819  	 END LOOP;
14:50:31 1820  
14:50:31 1821  	 RETURN var_result_date;
14:50:31 1822  
14:50:31 1823  EXCEPTION
14:50:31 1824  WHEN BAD_SUBSCRIPTION_ID THEN
14:50:31 1825  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 1826  	   SPROC_NAME, 'No such subscription');
14:50:31 1827  WHEN CAN_NOT_FIND_LAST_LICENSE THEN
14:50:31 1828  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 1829  	   SPROC_NAME, 'Could not find last license for given subscription');
14:50:31 1830  WHEN CAN_NOT_CALC_OFFER_LENGTH THEN
14:50:31 1831  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 1832  	   SPROC_NAME, 'Could not calculate offer length', EXCEPTION_MESSAGE);
14:50:31 1833  WHEN CAN_NOT_CALC_OFFER_LENGTH2 THEN
14:50:31 1834  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 1835  	   SPROC_NAME, 'Could not calculate last offer length', EXCEPTION_MESSAGE);
14:50:31 1836  WHEN OTHERS THEN
14:50:31 1837  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:31 1838  	   SPROC_NAME, 'Unkown error', SQLERRM);
14:50:31 1839  END CALC_SUBSCRIPTION_END_DATE;
