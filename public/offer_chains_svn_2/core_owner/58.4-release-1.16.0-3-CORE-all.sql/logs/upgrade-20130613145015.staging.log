14:50:31 703  
14:50:31 704  	PROCEDURE START_GC_PURCHASING (
14:50:31 705  	  in_group_id		    IN NUMBER,
14:50:31 706  	  in_offer_chain_id	    IN VARCHAR2,
14:50:31 707  	  in_gift_certificate_code  IN	VARCHAR2,
14:50:31 708  	  in_created_by 	    IN	VARCHAR2,
14:50:31 709  	  in_recipient_name	    IN	VARCHAR2,
14:50:31 710  	  in_recipient_email	    IN	VARCHAR2,
14:50:31 711  	  in_recipient_address_id   IN NUMBER,
14:50:31 712  	  in_recipient_notify_date  IN DATE,
14:50:31 713  	  in_sender_name	    IN VARCHAR2,
14:50:31 714  	  in_sender_email	    IN VARCHAR2,
14:50:31 715  	  in_gift_message	    IN	VARCHAR2,
14:50:31 716  	  in_expiration_date	    IN DATE,
14:50:31 717  	  in_campaign		    IN VARCHAR2,
14:50:31 718  	  in_reference_code	    IN VARCHAR2,
