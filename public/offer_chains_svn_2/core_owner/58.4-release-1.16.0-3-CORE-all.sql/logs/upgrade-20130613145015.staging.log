14:50:30 238  	  LICENSE.OFFER_ID,
14:50:30 239  	  LICENSE.START_DATE,
14:50:30 240  	  LICENSE.SUBSCRIPTION_ID,
14:50:30 241  	  LICENSE.UPDATE_DATE,
14:50:30 242  	  LICENSE.UPDATED_BY
14:50:30 243  	FROM
14:50:30 244  	  LICENSE
14:50:30 245  	WHERE
14:50:30 246  	  TO_DATE(LICENSE.END_DATE) <= (current_timestamp + var_time_interval)
14:50:30 247  	  AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V22.LICENSE_ACTIVE
14:50:30 248  	  AND NOT EXISTS
