14:50:30 680  FUNCTION GET_NEXT_OFFER_INDEX (
14:50:30 681  	in_offer_chain_id	     IN NUMBER,
14:50:30 682  	in_offer_chain_current_index IN NUMBER
14:50:30 683  ) RETURN NUMBER AS
14:50:30 684  -- VARIABLES
14:50:30 685  SPROC_NAME		     CONSTANT VARCHAR2(20) := 'GET_NEXT_OFFER_INDEX';
14:50:30 686  temp_offer_chain_id	     NUMBER;
14:50:30 687  temp_offer_chain_current_index NUMBER;
14:50:30 688  var_result		     NUMBER;
14:50:30 689  -- EXCEPTIONS
14:50:30 690  BAD_OFFER_CHAIN_ID      EXCEPTION;
14:50:30 691  BAD_CURRENT_INDEX_VALUE EXCEPTION;
14:50:30 692  BEGIN
14:50:30 693  
14:50:30 694  	-- Check that offer chain exists
14:50:30 695  	BEGIN
14:50:30 696  	  SELECT
14:50:30 697  	    OFFER_CHAIN.ID into temp_offer_chain_id
14:50:30 698  	  FROM
14:50:30 699  	    OFFER_CHAIN
14:50:30 700  	  WHERE
14:50:30 701  	    OFFER_CHAIN.ID = in_offer_chain_id;
14:50:30 702  	  EXCEPTION
14:50:30 703  	    WHEN NO_DATA_FOUND THEN
14:50:30 704  	      RAISE BAD_OFFER_CHAIN_ID;
14:50:30 705  	END;
14:50:30 706  
14:50:30 707  	-- Check that current offer index exists
14:50:30 708  	BEGIN
14:50:30 709  	  SELECT
14:50:30 710  	    OFFER_OFFER_CHAIN.INDEX_VALUE into temp_offer_chain_current_index
14:50:30 711  	  FROM
14:50:30 712  	    OFFER_OFFER_CHAIN
14:50:30 713  	  WHERE
14:50:30 714  	    OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
14:50:30 715  	    AND OFFER_OFFER_CHAIN.INDEX_VALUE = in_offer_chain_current_index
14:50:30 716  	    -- TODO: delete next line
14:50:30 717  	    AND ROWNUM <= 1;
14:50:30 718  	  EXCEPTION
14:50:30 719  	    WHEN NO_DATA_FOUND THEN
14:50:30 720  	      RAISE BAD_CURRENT_INDEX_VALUE;
14:50:30 721  	END;
14:50:30 722  
14:50:30 723  	-- Get next offer index
14:50:30 724  	BEGIN
14:50:30 725  	  SELECT
14:50:30 726  	    INDEX_VALUE into var_result
14:50:30 727  	  FROM (
14:50:30 728  	    SELECT
14:50:30 729  	      OFFER_OFFER_CHAIN.INDEX_VALUE
14:50:30 730  	    FROM
14:50:30 731  	      OFFER_OFFER_CHAIN
14:50:30 732  	    WHERE
14:50:30 733  	      OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
14:50:30 734  	      AND OFFER_OFFER_CHAIN.INDEX_VALUE > in_offer_chain_current_index
14:50:30 735  	    ORDER BY
14:50:30 736  	      OFFER_OFFER_CHAIN.INDEX_VALUE ASC
14:50:30 737  	  )
14:50:30 738  	  WHERE
14:50:30 739  	    ROWNUM <= 1;
14:50:30 740  	  EXCEPTION
14:50:30 741  	    WHEN NO_DATA_FOUND THEN
14:50:30 742  	      var_result := NULL;
14:50:30 743  	END;
14:50:30 744  
14:50:30 745  	RETURN var_result;
14:50:30 746  
14:50:30 747  EXCEPTION
14:50:30 748  WHEN BAD_OFFER_CHAIN_ID THEN
14:50:30 749  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:30 750  	  SPROC_NAME, 'No such offer chain');
14:50:30 751  WHEN BAD_CURRENT_INDEX_VALUE THEN
14:50:30 752  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:30 753  	  SPROC_NAME, 'Bad current index value');
14:50:30 754  WHEN OTHERS THEN
14:50:30 755  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:30 756  	  SPROC_NAME, 'Unknown error', SQLERRM);
14:50:30 757  END GET_NEXT_OFFER_INDEX;
