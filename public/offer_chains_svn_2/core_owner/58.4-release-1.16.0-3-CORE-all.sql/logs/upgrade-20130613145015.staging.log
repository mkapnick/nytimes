14:50:31 679  		  RAISE CAN_NOT_DEL_DEF_FINANCIAL;
14:50:31 680  	    END;
14:50:31 681  	  END IF;
14:50:31 682  	END IF;
14:50:31 683  
14:50:31 684  EXCEPTION
