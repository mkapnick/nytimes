14:50:30  73  	  INNER JOIN PRODUCT_OFFERING ON OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = PRODUCT_OFFERING.ID
14:50:30  74  	WHERE
14:50:30  75  	  OFFER_PRODUCT_OFFERING.OFFER_ID = in_offer_id;
14:50:30  76  
14:50:30  77  	-- insert line items
14:50:30  78  	-- add discounts to line items
14:50:30  79  	LOOP
14:50:30  80  	  FETCH var_line_item_data INTO
14:50:30  81  	    var_product_offering_id,
14:50:30  82  	    var_product_unit_price,
14:50:30  83  	    var_product_quantity;
14:50:30  84  	  EXIT WHEN var_line_item_data%NOTFOUND;
14:50:30  85  
14:50:30  86  	  var_product_offering_oprice := var_product_unit_price * var_product_quantity;
14:50:30  87  	  var_product_offering_price := var_product_offering_oprice;
14:50:30  88  
14:50:30  89  	  -- Apply discounts to line_item
14:50:30  90  	  BEGIN
14:50:30  91  	    SELECT
14:50:30  92  	      SUM (DISCOUNT.FIXED_AMOUNT) into var_discount_fixed_amount
14:50:30  93  	    FROM
14:50:30  94  	      DISCOUNT_PRODUCT_OFFERING
14:50:30  95  	      INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
14:50:30  96  	    WHERE
14:50:30  97  	      DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
14:50:30  98  	      AND DISCOUNT.FIXED_AMOUNT IS NOT NULL;
14:50:30  99  	    EXCEPTION
14:50:30 100  	    WHEN NO_DATA_FOUND THEN
14:50:30 101  	      var_discount_fixed_amount := NULL;
14:50:30 102  	  END;
14:50:30 103  
14:50:30 104  	  BEGIN
14:50:30 105  	    SELECT
14:50:30 106  	      SUM (DISCOUNT.PERCENT_AMOUNT) into var_discount_percent_amount
14:50:30 107  	    FROM
14:50:30 108  	      DISCOUNT_PRODUCT_OFFERING
14:50:30 109  	      INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
14:50:30 110  	    WHERE
14:50:30 111  	      DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
14:50:30 112  	      AND DISCOUNT.PERCENT_AMOUNT IS NOT NULL;
14:50:30 113  	    EXCEPTION
14:50:30 114  	    WHEN NO_DATA_FOUND THEN
14:50:30 115  	      var_discount_percent_amount := NULL;
14:50:30 116  	  END;
14:50:30 117  
14:50:30 118  	  IF (var_discount_percent_amount IS NOT NULL) THEN
14:50:30 119  	    var_product_offering_price := var_product_offering_price * ( 1 - var_discount_percent_amount / 100 );
14:50:30 120  	  END IF;
14:50:30 121  
14:50:30 122  	  IF (var_discount_fixed_amount IS NOT NULL) THEN
14:50:30 123  	    var_product_offering_price := var_product_offering_price - var_discount_fixed_amount;
14:50:30 124  	  END IF;
14:50:30 125  
14:50:30 126  	  var_line_item_price := PROCS_COMMON_V22.ROUND_10_6_TO_10_2(var_product_offering_price);
14:50:30 127  
14:50:30 128  	  IF (var_line_item_price < 0) THEN
14:50:30 129  	      RAISE BAD_DISCOUNT;
14:50:30 130  	  END IF;
14:50:30 131  
14:50:30 132  	  var_new_line_item_id := NULL;
14:50:30 133  	  PROCS_LINE_ITEMS_CRU_V22.CREATE_LINE_ITEM(
14:50:30 134  	    inout_line_item_id	=> var_new_line_item_id,
14:50:30 135  	    in_product_offer_id => var_product_offering_id,
14:50:30 136  	    in_invoice_id	=> in_invoice_id,
14:50:30 137  	    in_amount		=> var_line_item_price,
14:50:30 138  	    in_created_by	=> in_created_by,
14:50:30 139  	    in_discount_amount	=> var_product_offering_oprice - var_line_item_price,
14:50:30 140  	    in_taxes_amount	=> NULL
14:50:30 141  	  );
14:50:30 142  
14:50:30 143  	  FOR f_discount IN (
14:50:30 144  	    SELECT
14:50:30 145  	      DISCOUNT.ID
14:50:30 146  	    FROM
14:50:30 147  	      DISCOUNT_PRODUCT_OFFERING
14:50:30 148  	      INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
14:50:30 149  	    WHERE
14:50:30 150  	      DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
14:50:30 151  	  )
14:50:30 152  	  LOOP
14:50:30 153  	    PROCS_LINE_ITEMS_CRU_V22.CREATE_DISCOUNT_LINE_ITEM(
14:50:30 154  	      in_discount_id =>  f_discount.ID,
14:50:30 155  	      in_line_item_id => var_new_line_item_id
14:50:30 156  	    );
14:50:30 157  	  END LOOP;
14:50:30 158  	END LOOP;
14:50:30 159  
14:50:30 160  EXCEPTION
