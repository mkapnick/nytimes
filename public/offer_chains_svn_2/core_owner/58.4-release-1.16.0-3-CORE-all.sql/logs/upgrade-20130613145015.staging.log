14:50:31 1312  	   EXCEPTION
14:50:31 1313  	     WHEN NO_DATA_FOUND THEN
14:50:31 1314  	       var_subscrib_group_id := NULL;
14:50:31 1315  	 END;
14:50:31 1316  
14:50:31 1317  	 IF var_subscrib_group_id IS NULL THEN
14:50:31 1318  	   BEGIN
14:50:31 1319  	     SELECT
14:50:31 1320  	       GIFT_CERTIFICATE.PURCHASER_GROUP_ID,
14:50:31 1321  	       GIFT_CERTIFICATE.REDEEMER_GROUP_ID
14:50:31 1322  	       into
14:50:31 1323  	       var_gc_purchaser_group_id,
