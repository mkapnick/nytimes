14:50:31 697  /******************************************************************************/
14:50:31 698  
14:50:31 699  PROCEDURE CALCULATE_INVOICE_CHARGEBACK (
14:50:31 700  	in_invoice_id	      IN NUMBER,
14:50:31 701  	in_chargeback_date    IN DATE,
14:50:31 702  	out_chargeback_amount OUT NUMBER
14:50:31 703  ) AS
14:50:31 704  -- VARIABLES
14:50:31 705  SPROC_NAME	     CONSTANT VARCHAR2(28) := 'CALCULATE_INVOICE_CHARGEBACK';
14:50:31 706  var_chargeback_date    DATE;
14:50:31 707  temp_invoice_id	     NUMBER;
14:50:31 708  var_licenses_number    NUMBER;
14:50:31 709  var_invoice_start_date DATE;
14:50:31 710  var_invoice_end_date   DATE;
14:50:31 711  var_offer_id	     NUMBER;
14:50:31 712  var_offer_days_interval NUMBER;
14:50:31 713  var_license_days_used  NUMBER;
14:50:31 714  var_invoice_amount     NUMBER(10,2);
14:50:31 715  var_offer_chain_id     NUMBER;
14:50:31 716  var_offer_chain_meta_data_val VARCHAR2(1024);
14:50:31 717  var_offer_chain_full_refund   NUMBER;
14:50:31 718  var_offer_chain_prorated_ref  NUMBER;
14:50:31 719  var_chargeback_calculated NUMBER;
14:50:31 720  var_max_invoice_refund	NUMBER;
14:50:31 721  -- EXCEPTIONS
14:50:31 722  BAD_INVOICE_ID		  EXCEPTION;
14:50:31 723  NO_LICENSES_FOUND_EXCEPTION EXCEPTION;
14:50:31 724  OFFER_LENGTH_IS_ZERO	  EXCEPTION;
14:50:31 725  BEGIN
14:50:31 726  
14:50:31 727  	IF in_chargeback_date IS NULL THEN
14:50:31 728  	  var_chargeback_date := PROCS_COMMON_V22.NORMALIZE_DATE(current_date);
14:50:31 729  	ELSE
14:50:31 730  	  var_chargeback_date := PROCS_COMMON_V22.NORMALIZE_DATE(in_chargeback_date);
14:50:31 731  	END IF;
14:50:31 732  
14:50:31 733  	-- Check that invoice exists
14:50:31 734  	BEGIN
14:50:31 735  	  SELECT
14:50:31 736  	    INVOICE.ID into temp_invoice_id
14:50:31 737  	  FROM
14:50:31 738  	    INVOICE
14:50:31 739  	  WHERE
14:50:31 740  	    INVOICE.ID = in_invoice_id;
14:50:31 741  	  EXCEPTION
14:50:31 742  	    WHEN NO_DATA_FOUND THEN
14:50:31 743  	      RAISE BAD_INVOICE_ID;
14:50:31 744  	END;
14:50:31 745  
14:50:31 746  	-- Check that invoice has at least one license
14:50:31 747  	SELECT
14:50:31 748  	  COUNT(*) into var_licenses_number
14:50:31 749  	FROM
14:50:31 750  	  LICENSE
14:50:31 751  	WHERE
14:50:31 752  	  LICENSE.INVOICE_ID = in_invoice_id;
14:50:31 753  
14:50:31 754  	IF var_licenses_number = 0 THEN
14:50:31 755  	  RAISE NO_LICENSES_FOUND_EXCEPTION;
14:50:31 756  	END IF;
14:50:31 757  
14:50:31 758  	SELECT
14:50:31 759  	  MIN(LICENSE.START_DATE) into var_invoice_start_date
14:50:31 760  	FROM
14:50:31 761  	  LICENSE
14:50:31 762  	WHERE
14:50:31 763  	  LICENSE.INVOICE_ID = in_invoice_id;
14:50:31 764  
14:50:31 765  	var_invoice_start_date := PROCS_COMMON_V22.NORMALIZE_DATE(var_invoice_start_date);
14:50:31 766  
14:50:31 767  	SELECT DISTINCT
14:50:31 768  	  LICENSE.OFFER_ID into var_offer_id
14:50:31 769  	FROM
14:50:31 770  	  LICENSE
14:50:31 771  	WHERE
14:50:31 772  	  LICENSE.INVOICE_ID = in_invoice_id;
14:50:31 773  
14:50:31 774  	SELECT
14:50:31 775  	  MAX (LICENSE.END_DATE) into var_invoice_end_date
14:50:31 776  	FROM
14:50:31 777  	  LICENSE
14:50:31 778  	WHERE
14:50:31 779  	  LICENSE.INVOICE_ID = in_invoice_id;
14:50:31 780  
14:50:31 781  	-- All licenses for given invoice should point into the same offer
14:50:31 782  
14:50:31 783  	PROCS_OFFER_CHAIN_V22.GET_OFFER_LENGTH_IN_DAYS(
14:50:31 784  	  in_offer_id	=> var_offer_id,
14:50:31 785  	  in_start_date => var_invoice_start_date,
14:50:31 786  	  out_days	=> var_offer_days_interval
14:50:31 787  	);
14:50:31 788  
14:50:31 789  	IF var_offer_days_interval = 0 THEN
14:50:31 790  	  RAISE OFFER_LENGTH_IS_ZERO;
14:50:31 791  	END IF;
14:50:31 792  
14:50:31 793  	PROCS_INVOICE_V22.GET_INVOICE_DAYS_USED_NUMBER(
14:50:31 794  	  in_invoice_id      => in_invoice_id,
14:50:31 795  	  in_chargeback_date => var_chargeback_date,
14:50:31 796  	  out_days_num	     => var_license_days_used
14:50:31 797  	);
14:50:31 798  
14:50:31 799  	PROCS_INVOICE_V22.CALCULATE_INVOICE_AMOUNT(
14:50:31 800  	  in_invoice_id => in_invoice_id,
14:50:31 801  	  out_amount	=> var_invoice_amount
14:50:31 802  	);
14:50:31 803  
14:50:31 804  	-- 39437
14:50:31 805  	-- Get offer chain id by invoice id
14:50:31 806  	SELECT DISTINCT
14:50:31 807  	  SUBSCRIPTION.OFFER_CHAIN_ID into var_offer_chain_id
14:50:31 808  	FROM
14:50:31 809  	  SUBSCRIPTION
14:50:31 810  	  INNER JOIN LICENSE ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
14:50:31 811  	WHERE
14:50:31 812  	  LICENSE.INVOICE_ID = in_invoice_id;
14:50:31 813  
14:50:31 814  	-- Get offer chain's meta data full amount value
14:50:31 815  	PROCS_OFFER_CHAIN_V22.GET_OFFER_CHAIN_MD_VALUE(
14:50:31 816  	  in_offer_chain_id => var_offer_chain_id,
14:50:31 817  	  in_meta_data_name => GLOBAL_CONSTANTS_V22.OFFER_CHAIN_FULL_REFUND,
14:50:31 818  	  out_value	    => var_offer_chain_meta_data_val
14:50:31 819  	);
14:50:31 820  	IF var_offer_chain_meta_data_val IS NULL THEN
14:50:31 821  	  var_offer_chain_full_refund := NULL;
14:50:31 822  	ELSE
14:50:31 823  	  var_offer_chain_full_refund := TO_NUMBER(var_offer_chain_meta_data_val);
14:50:31 824  	END IF;
14:50:31 825  
14:50:31 826  	-- Get offer chain's meta data prorated amount value
14:50:31 827  	PROCS_OFFER_CHAIN_V22.GET_OFFER_CHAIN_MD_VALUE(
14:50:31 828  	  in_offer_chain_id => var_offer_chain_id,
14:50:31 829  	  in_meta_data_name => GLOBAL_CONSTANTS_V22.OFFER_CHAIN_PRORATED_REFUND,
14:50:31 830  	  out_value	    => var_offer_chain_meta_data_val
14:50:31 831  	);
14:50:31 832  	IF var_offer_chain_meta_data_val IS NULL THEN
14:50:31 833  	  var_offer_chain_prorated_ref := NULL;
14:50:31 834  	ELSE
14:50:31 835  	  var_offer_chain_prorated_ref := TO_NUMBER(var_offer_chain_meta_data_val);
14:50:31 836  	END IF;
14:50:31 837  
14:50:31 838  	var_chargeback_calculated := GLOBAL_CONSTANTS_V22.FALSE;
14:50:31 839  
14:50:31 840  	IF var_offer_chain_full_refund IS NOT NULL
14:50:31 841  	   AND var_chargeback_calculated = GLOBAL_CONSTANTS_V22.FALSE THEN
14:50:31 842  	  IF var_license_days_used < var_offer_chain_full_refund THEN
14:50:31 843  	    out_chargeback_amount := var_invoice_amount;
14:50:31 844  	    var_chargeback_calculated := GLOBAL_CONSTANTS_V22.TRUE;
14:50:31 845  	  END IF;
14:50:31 846  	END IF;
14:50:31 847  
14:50:31 848  	IF var_offer_chain_prorated_ref IS NOT NULL
14:50:31 849  	   AND var_chargeback_calculated = GLOBAL_CONSTANTS_V22.FALSE THEN
14:50:31 850  	  IF var_license_days_used < var_offer_chain_prorated_ref THEN
14:50:31 851  	    out_chargeback_amount := ( var_invoice_amount * (var_offer_days_interval - var_license_days_used) ) / var_offer_days_interval;
14:50:31 852  	    var_chargeback_calculated := GLOBAL_CONSTANTS_V22.TRUE;
14:50:31 853  	  END IF;
14:50:31 854  	END IF;
14:50:31 855  
14:50:31 856  	IF var_chargeback_calculated = GLOBAL_CONSTANTS_V22.TRUE THEN
14:50:31 857  	  PROCS_INVOICE_V22.GET_MAX_REFUND(
14:50:31 858  	    in_invoice_id => in_invoice_id,
14:50:31 859  	    out_amount	  => var_max_invoice_refund
14:50:31 860  	  );
14:50:31 861  	  IF var_max_invoice_refund < out_chargeback_amount THEN
14:50:31 862  	    out_chargeback_amount := var_max_invoice_refund;
14:50:31 863  	  END IF;
14:50:31 864  	END IF;
14:50:31 865  
14:50:31 866  	IF var_chargeback_calculated = GLOBAL_CONSTANTS_V22.FALSE THEN
14:50:31 867  	  out_chargeback_amount := 0;
14:50:31 868  	END IF;
14:50:31 869  
14:50:31 870  EXCEPTION
14:50:31 871  WHEN BAD_INVOICE_ID THEN
14:50:31 872  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 873  	  SPROC_NAME, 'No such invoice');
14:50:31 874  WHEN NO_LICENSES_FOUND_EXCEPTION THEN
14:50:31 875  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 876  	  SPROC_NAME, 'No licenses found');
14:50:31 877  WHEN OFFER_LENGTH_IS_ZERO THEN
14:50:31 878  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 879  	  SPROC_NAME, 'Offer length is zero');
14:50:31 880  WHEN OTHERS THEN
14:50:31 881  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:31 882  	  SPROC_NAME, 'Unknown error', SQLERRM);
14:50:31 883  END CALCULATE_INVOICE_CHARGEBACK;
14:50:31 884  
14:50:31 885  /********************************************************************/
14:50:31 886  
14:50:31 887  PROCEDURE APPLY_REFUND (
14:50:31 888  	in_invoice_id	     IN NUMBER,
14:50:31 889  	in_chargeback_amount IN NUMBER,
14:50:31 890  	in_created_by	     IN VARCHAR2,
14:50:31 891  	out_charge_id	     OUT NUMBER
14:50:31 892  ) AS
14:50:31 893  -- VARIABLES
14:50:31 894  SPROC_NAME		 CONSTANT VARCHAR2(16) := 'APPLY_CHARGEBACK';
14:50:31 895  temp_invoice_id		 NUMBER;
14:50:31 896  var_total_charges_amount	 NUMBER(10,2);
14:50:31 897  var_charge_amount_to_apply NUMBER(10,2);
14:50:31 898  var_transaction_id	 NUMBER;
14:50:31 899  var_instrument_type_id	 NUMBER;
14:50:31 900  var_instrument_id 	 NUMBER;
14:50:31 901  var_charge_id		 NUMBER;
14:50:31 902  -- EXCEPTIONS
14:50:31 903  BAD_INVOICE_ID		 EXCEPTION;
14:50:31 904  CAN_NOT_CREATE_TRANSACTION EXCEPTION;
14:50:31 905  CAN_NOT_FIND_INSTRUMENT	 EXCEPTION;
14:50:31 906  CAN_NOT_CREATE_CHARGE	 EXCEPTION;
14:50:31 907  EXCEPTION_MESSAGE 	 VARCHAR2(1024);
14:50:31 908  BEGIN
14:50:31 909  
14:50:31 910  	BEGIN
14:50:31 911  	  SELECT
14:50:31 912  	    INVOICE.ID into temp_invoice_id
14:50:31 913  	  FROM
14:50:31 914  	    INVOICE
14:50:31 915  	  WHERE
14:50:31 916  	    INVOICE.ID = in_invoice_id;
14:50:31 917  	  EXCEPTION
14:50:31 918  	    WHEN NO_DATA_FOUND THEN
14:50:31 919  	      RAISE BAD_INVOICE_ID;
14:50:31 920  	END;
14:50:31 921  
14:50:31 922  	SELECT
14:50:31 923  	  SUM (CHARGE.CHARGE_AMOUNT) into var_total_charges_amount
14:50:31 924  	FROM
14:50:31 925  	  CHARGE
14:50:31 926  	WHERE
14:50:31 927  	  CHARGE.INVOICE_ID = in_invoice_id
14:50:31 928  	  AND CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V22.CHARGE_PROCESSED
14:50:31 929  	  AND CHARGE.INSTRUMENT_TYPE_ID != GLOBAL_ENUMS_V22.INSTRUMENT_GIFT_CERTIFICATE;
14:50:31 930  
14:50:31 931  	-- FIXME: Maybe whe should throw exception here?
14:50:31 932  	IF var_total_charges_amount < in_chargeback_amount THEN
14:50:31 933  	  var_charge_amount_to_apply := var_total_charges_amount;
14:50:31 934  	ELSE
14:50:31 935  	  var_charge_amount_to_apply := in_chargeback_amount;
14:50:31 936  	END IF;
14:50:31 937  
14:50:31 938  	BEGIN
14:50:31 939  	  PROCS_TRANSACTION_V22.CREATE_TRANSACTION(
14:50:31 940  	    in_transaction_id	      => NULL,
14:50:31 941  	    in_status_id	      => GLOBAL_STATUSES_V22.TRANSACTION_PREPARE,
14:50:31 942  	    in_amount		      => -var_charge_amount_to_apply,
14:50:31 943  	    in_created_by	      => in_created_by,
14:50:31 944  	    in_order_id 	      => NULL,
14:50:31 945  	    in_is_refund	      => GLOBAL_CONSTANTS_V22.TRUE,
14:50:31 946  	    in_transaction_type_code  => 'REFUND',
14:50:31 947  	    out_transaction_id	      => var_transaction_id
14:50:31 948  	  );
14:50:31 949  	  EXCEPTION
14:50:31 950  	    WHEN OTHERS THEN
14:50:31 951  	      EXCEPTION_MESSAGE := SQLERRM;
14:50:31 952  	      RAISE CAN_NOT_CREATE_TRANSACTION;
14:50:31 953  	END;
14:50:31 954  
14:50:31 955  	BEGIN
14:50:31 956  	  SELECT
14:50:31 957  	    C.INSTRUMENT_TYPE_ID,
14:50:31 958  	    C.INSTRUMENT_ID
14:50:31 959  	    into
14:50:31 960  	    var_instrument_type_id,
14:50:31 961  	    var_instrument_id
14:50:31 962  	  FROM
14:50:31 963  	    CHARGE C,
14:50:31 964  	    TRANSACTION_ATTEMPT TA,
14:50:31 965  	    TRANSACTION T
14:50:31 966  	  WHERE
14:50:31 967  	    C.INVOICE_ID = in_invoice_id and
14:50:31 968  	    C.TRANSACTION_ID = T.ID and
14:50:31 969  	    TA.TRANSACTION_ID = T.ID and
14:50:31 970  	    TA.TRANSACTION_ATTEMPT_STATUS_ID = GLOBAL_STATUSES_V22.TRANS_ATTEMPT_SUCCESS and
14:50:31 971  	    T.IS_REFUND = GLOBAL_CONSTANTS_V22.FALSE and
14:50:31 972  	    T.TRANSACTION_AMOUNT >= 0 and
14:50:31 973  	    rownum < 2;
14:50:31 974  	EXCEPTION
14:50:31 975  	    WHEN NO_DATA_FOUND THEN
14:50:31 976  	      RAISE CAN_NOT_FIND_INSTRUMENT;
14:50:31 977  	END;
14:50:31 978  	BEGIN
14:50:31 979  	  PROCS_CHARGE_V22.CREATE_CHARGE(
14:50:31 980  	    in_invoice_id	  => in_invoice_id,
14:50:31 981  	    in_transaction_id	  => var_transaction_id,
14:50:31 982  	    in_instrument_type_id => var_instrument_type_id,
14:50:31 983  	    in_instrument_id	  => var_instrument_id,
14:50:31 984  	    in_charge_amount	  => -var_charge_amount_to_apply,
14:50:31 985  	    in_created_by	  => in_created_by,
14:50:31 986  	    in_charge_status_id   => GLOBAL_STATUSES_V22.CHARGE_OPENED,
14:50:31 987  	    out_charge_id	  => var_charge_id
14:50:31 988  	  );
14:50:31 989  	  EXCEPTION
14:50:31 990  	    WHEN OTHERS THEN
14:50:31 991  	      EXCEPTION_MESSAGE := SQLERRM;
14:50:31 992  	      RAISE CAN_NOT_CREATE_CHARGE;
14:50:31 993  	END;
14:50:31 994  
14:50:31 995  	out_charge_id := var_charge_id;
14:50:31 996  
14:50:31 997  EXCEPTION
14:50:31 998  WHEN BAD_INVOICE_ID THEN
14:50:31 999  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 1000  	   SPROC_NAME, 'No such invoice');
14:50:31 1001  WHEN CAN_NOT_CREATE_TRANSACTION THEN
14:50:31 1002  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 1003  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
14:50:31 1004  WHEN CAN_NOT_FIND_INSTRUMENT THEN
14:50:31 1005  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 1006  	   SPROC_NAME, 'Could not find financial instrument');
14:50:31 1007  WHEN CAN_NOT_CREATE_CHARGE THEN
14:50:31 1008  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
14:50:31 1009  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
14:50:31 1010  WHEN OTHERS THEN
14:50:31 1011  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:31 1012  	   SPROC_NAME, 'Unknown error', SQLERRM);
14:50:31 1013  END APPLY_REFUND;
14:50:31 1014  
14:50:31 1015  /******************************************************************************/
14:50:31 1016  
14:50:31 1017  PROCEDURE GET_MAX_REFUND (
14:50:31 1018  	 in_invoice_id IN NUMBER,
14:50:31 1019  	 out_amount    OUT NUMBER
14:50:31 1020  ) AS
14:50:31 1021  SPROC_NAME CONSTANT VARCHAR2(14) := 'GET_MAX_REFUND';
14:50:31 1022  -- VARIABLES
14:50:31 1023  temp_invoice_id	       NUMBER;
14:50:31 1024  var_invoice_refunds_sum NUMBER(10,2);
14:50:31 1025  var_invoice_charges_sum NUMBER(10,2);
14:50:31 1026  -- EXCEPTIONS
14:50:31 1027  BAD_INVOICE_ID EXCEPTION;
14:50:31 1028  BEGIN
14:50:31 1029  
14:50:31 1030  	 BEGIN
14:50:31 1031  	   SELECT
14:50:31 1032  	     INVOICE.ID into temp_invoice_id
14:50:31 1033  	   FROM
14:50:31 1034  	     INVOICE
14:50:31 1035  	   WHERE
14:50:31 1036  	     INVOICE.ID = in_invoice_id;
14:50:31 1037  	   EXCEPTION
14:50:31 1038  	     WHEN NO_DATA_FOUND THEN
14:50:31 1039  	       RAISE BAD_INVOICE_ID;
14:50:31 1040  	 END;
14:50:31 1041  
14:50:31 1042  	 var_invoice_refunds_sum := 0;
14:50:31 1043  	 var_invoice_charges_sum := 0;
14:50:31 1044  
14:50:31 1045  	 FOR f_charge IN (
14:50:31 1046  	   SELECT
14:50:31 1047  	     CHARGE.ID,
14:50:31 1048  	     CHARGE.CHARGE_STATUS_ID,
14:50:31 1049  	     CHARGE.CHARGE_AMOUNT,
14:50:31 1050  	     CHARGE.TRANSACTION_ID
14:50:31 1051  	   FROM
14:50:31 1052  	     CHARGE
14:50:31 1053  	   WHERE
14:50:31 1054  	     CHARGE.INVOICE_ID = in_invoice_id
14:50:31 1055  	 )
14:50:31 1056  	 LOOP
14:50:31 1057  	   -- If charge.status = canceled then continue
14:50:31 1058  	   IF f_charge.CHARGE_STATUS_ID != GLOBAL_STATUSES_V22.CHARGE_CANCELED THEN
14:50:31 1059  
14:50:31 1060  	     IF f_charge.CHARGE_AMOUNT > 0 THEN
14:50:31 1061  	       IF f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V22.CHARGE_PROCESSED
14:50:31 1062  		  AND PROCS_TRANSACTION_V22.IS_TRANSACTION_COLLECTED(f_charge.TRANSACTION_ID) = GLOBAL_CONSTANTS_V22.TRUE THEN
14:50:31 1063  		 -- Transaction collected
14:50:31 1064  		 var_invoice_charges_sum := var_invoice_charges_sum + f_charge.CHARGE_AMOUNT;
14:50:31 1065  	       ELSE
14:50:31 1066  		 -- Transaction is not collected. Do nothing
14:50:31 1067  		 NULL;
14:50:31 1068  	       END IF;
14:50:31 1069  	     ELSE
14:50:31 1070  	       IF f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V22.CHARGE_PROCESSED
14:50:31 1071  		  AND PROCS_TRANSACTION_V22.IS_TRANSACTION_COLLECTED(f_charge.TRANSACTION_ID) = GLOBAL_CONSTANTS_V22.FALSE THEN
14:50:31 1072  		 -- If charge is processed transaction is not collected then do nothing
14:50:31 1073  		 NULL;
14:50:31 1074  	       ELSE
14:50:31 1075  		 var_invoice_refunds_sum := var_invoice_refunds_sum + f_charge.CHARGE_AMOUNT;
14:50:31 1076  	       END IF;
14:50:31 1077  	     END IF;
14:50:31 1078  
14:50:31 1079  	   END IF;
14:50:31 1080  	 END LOOP;
14:50:31 1081  
14:50:31 1082  	 -- Refunds are negative
14:50:31 1083  	 var_invoice_refunds_sum := 0 - var_invoice_refunds_sum;
14:50:31 1084  
14:50:31 1085  	 out_amount := var_invoice_charges_sum - var_invoice_refunds_sum;
14:50:31 1086  
14:50:31 1087  EXCEPTION
14:50:31 1088  WHEN BAD_INVOICE_ID THEN
14:50:31 1089  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
14:50:31 1090  	   SPROC_NAME, 'No such invoice');
14:50:31 1091  WHEN OTHERS THEN
14:50:31 1092  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
14:50:31 1093  	   SPROC_NAME, 'Unknown error', SQLERRM);
14:50:31 1094  END GET_MAX_REFUND;
