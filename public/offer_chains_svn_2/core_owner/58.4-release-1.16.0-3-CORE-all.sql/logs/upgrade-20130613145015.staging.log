14:50:30  48  	BEGIN
14:50:30  49  	  OPEN out_result_set FOR
14:50:30  50  
14:50:30  51  	SELECT
14:50:30  52  	  C.CODE NAME,
14:50:30  53  	  C.DESCRIPTION,
14:50:30  54  	  0 INHERITED,
14:50:30  55  	  C.SHAREABLE,
14:50:30  56  	  MAX(LIC.ENTITLEMENT_END_DATE) EXPIRES
14:50:30  57  	FROM
14:50:30  58  	  SUBSCRIPTION SB
14:50:30  59  	  INNER JOIN ACCOUNT AC ON AC.ID = SB.ACCOUNT_ID
14:50:30  60  	  INNER JOIN LICENSE LIC ON LIC.SUBSCRIPTION_ID = SB.ID
14:50:30  61  	  INNER JOIN OFFER_PRODUCT_OFFERING OPO ON OPO.OFFER_ID = LIC.OFFER_ID
14:50:30  62  	  INNER JOIN PRODUCT_OFFERING PO ON PO.ID = OPO.PRODUCT_OFFERING_ID
14:50:30  63  	  INNER JOIN CAPABILITY C ON PO.CAPABILITY_ID = C.ID
14:50:30  64  	WHERE
14:50:30  65  	  LIC.ENTITLEMENT_END_DATE >= TRUNC(SYSDATE)
14:50:30  66  	  AND LIC.START_DATE <= SYSDATE
14:50:30  67  	  AND AC.GROUP_ID = in_group_id
14:50:30  68  	GROUP BY
14:50:30  69  	  C.CODE, 0, C.SHAREABLE, C.DESCRIPTION
14:50:30  70  UNION ALL
14:50:30  71  	SELECT
14:50:30  72  	  C.CODE NAME,
14:50:30  73  	  C.DESCRIPTION,
14:50:30  74  	  1 INHERITED,
14:50:30  75  	  C.SHAREABLE,
14:50:30  76  	  MAX(LEAST(SS.END_DATE, LIC.ENTITLEMENT_END_DATE)) EXPIRES
14:50:30  77  	FROM
14:50:30  78  	  ACCOUNT BORROWER,
14:50:30  79  	  SUBSCRIPTION S,
14:50:30  80  	  LICENSE LIC,
14:50:30  81  	  OFFER_PRODUCT_OFFERING OPO,
14:50:30  82  	  PRODUCT_OFFERING PO,
14:50:30  83  	  CAPABILITY C,
14:50:30  84  	  GROUP_ACCOUNT GA,
14:50:30  85  	  SUBSCRIPTION_SHARE SS
14:50:30  86  	WHERE
14:50:30  87  	  BORROWER.GROUP_ID = in_group_id
14:50:30  88  	  AND LIC.SUBSCRIPTION_ID = S.ID
14:50:30  89  	  AND OPO.OFFER_ID = LIC.OFFER_ID
14:50:30  90  	  AND PO.ID = OPO.PRODUCT_OFFERING_ID
14:50:30  91  	  AND PO.CAPABILITY_ID = C.ID
14:50:30  92  	  AND GA.SUBSCRIPTION_ID = S.ID
14:50:30  93  	  AND SS.BORROWER_ACCOUNT_ID = BORROWER.ID
14:50:30  94  	  AND SS.GROUP_ACCOUNT_ID = GA.ID
14:50:30  95  	  AND SYSDATE BETWEEN SS.START_DATE AND SS.END_DATE
14:50:30  96  	  AND SYSDATE BETWEEN LIC.START_DATE AND LIC.ENTITLEMENT_END_DATE
14:50:30  97  	  AND C.SHAREABLE = 1
14:50:30  98  	GROUP BY
