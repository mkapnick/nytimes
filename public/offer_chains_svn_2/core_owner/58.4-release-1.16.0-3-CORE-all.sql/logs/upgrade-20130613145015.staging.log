14:50:30 965  var_tax_amount NUMBER(10,2);
14:50:30 966  BEGIN
14:50:30 967  
14:50:30 968  	SELECT
14:50:30 969  	  SUM(LI.TAXES_AMOUNT) into var_tax_amount
14:50:30 970  	FROM
14:50:30 971  	  LINE_ITEM LI
