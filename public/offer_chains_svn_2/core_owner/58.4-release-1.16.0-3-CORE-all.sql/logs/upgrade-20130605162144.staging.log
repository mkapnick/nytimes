16:22:01 1398  /******************************************************************************/
16:22:01 1399  
16:22:01 1400  PROCEDURE ADD_SUBSCRIPTION_EXTENSION (
16:22:01 1401  /*
16:22:01 1402  APP_EXCEPTION_CODES_V22.NOT_FOUND
16:22:01 1403  APP_EXCEPTION_CODES_V22.INTERNAL_ERROR
16:22:01 1404  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
16:22:01 1405  */
16:22:01 1406  	 in_subscription_id	 IN NUMBER,
16:22:01 1407  	 in_effective_start_date IN DATE,
16:22:01 1408  	 in_effective_end_date	 IN DATE,
16:22:01 1409  	 in_note		 IN VARCHAR2,
16:22:01 1410  	 in_updated_by		 IN VARCHAR2
16:22:01 1411  ) AS
16:22:01 1412  -- VARIABLES
16:22:01 1413  SPROC_NAME		    CONSTANT VARCHAR2(26) := 'ADD_SUBSCRIPTION_EXTENSION';
16:22:01 1414  temp_subscription_id	    NUMBER;
16:22:01 1415  var_current_license_id	    NUMBER;
16:22:01 1416  var_current_license_start_date DATE;
16:22:01 1417  var_current_license_end_date DATE;
16:22:01 1418  var_current_offer_id	    NUMBER;
16:22:01 1419  var_current_invoice_id	    NUMBER;
16:22:01 1420  var_current_date 	    DATE;
16:22:01 1421  var_current_offer_index	    NUMBER;
16:22:01 1422  var_current_offer_recurr_num NUMBER;
16:22:01 1423  var_account_tax_exempt_id    VARCHAR2(255);
16:22:01 1424  
16:22:01 1425  var_free_invoice_id NUMBER;
16:22:01 1426  var_free_license_id NUMBER;
16:22:01 1427  var_new_license_id  NUMBER;
16:22:01 1428  var_ext_license_id  NUMBER;
16:22:01 1429  -- EXCEPTIONS
16:22:01 1430  BAD_SUBSCRIPTION_ID	     EXCEPTION;
16:22:01 1431  CAN_NOT_FIND_OFFER_OR_LICENSE EXCEPTION;
16:22:01 1432  CAN_NOT_CHANGE_LICENSE_STATUS EXCEPTION;
16:22:01 1433  CAN_NOT_CREATE_INVOICE	     EXCEPTION;
16:22:01 1434  CAN_NOT_CREATE_NEW_LICENSE    EXCEPTION;
16:22:01 1435  CAN_NOT_CREATE_END_LICENSE    EXCEPTION;
16:22:01 1436  CAN_NOT_ANNOTATE_SUBSCRIPTION EXCEPTION;
16:22:01 1437  EXTENS_START_DATE_IS_TOO_FAR  EXCEPTION;
16:22:01 1438  EXT_START_DATE_LATER_THEN_END EXCEPTION;
16:22:01 1439  EXTENS_START_DATE_IS_TOO_SMALL EXCEPTION;
16:22:01 1440  EXCEPTION_MESSAGE	     VARCHAR2(1024);
16:22:01 1441  BEGIN
16:22:01 1442  
16:22:01 1443  	 var_current_date := PROCS_COMMON_V22.NORMALIZE_DATE(SYSDATE);
16:22:01 1444  
16:22:01 1445  	 -- Check that subscription exists
16:22:01 1446  	 BEGIN
16:22:01 1447  	   SELECT
16:22:01 1448  	     SUBSCRIPTION.ID into temp_subscription_id
16:22:01 1449  	   FROM
16:22:01 1450  	     SUBSCRIPTION
16:22:01 1451  	   WHERE
16:22:01 1452  	     SUBSCRIPTION.ID = in_subscription_id;
16:22:01 1453  	   EXCEPTION
16:22:01 1454  	     WHEN NO_DATA_FOUND THEN
16:22:01 1455  	       RAISE BAD_SUBSCRIPTION_ID;
16:22:01 1456  	 END;
16:22:01 1457  
16:22:01 1458  	 -- Get account tax exempt id
16:22:01 1459  	 SELECT DISTINCT
16:22:01 1460  	   ACCOUNT.TAX_EXEMPT_ID into var_account_tax_exempt_id
16:22:01 1461  	 FROM
16:22:01 1462  	   ACCOUNT
16:22:01 1463  	   INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.ACCOUNT_ID = ACCOUNT.ID
16:22:01 1464  	 WHERE
16:22:01 1465  	   SUBSCRIPTION.ID = in_subscription_id;
16:22:01 1466  
16:22:01 1467  	 -- Select current data
16:22:01 1468  	 BEGIN
16:22:01 1469  	   SELECT
16:22:01 1470  	     LICENSE.ID,
16:22:01 1471  	     LICENSE.START_DATE,
16:22:01 1472  	     LICENSE.END_DATE,
16:22:01 1473  	     LICENSE.CURRENT_OFFER_INDEX,
16:22:01 1474  	     LICENSE.CURRENT_OFFER_RECURR_NUM,
16:22:01 1475  	     OFFER.ID,
16:22:01 1476  	     INVOICE.ID
16:22:01 1477  	   INTO
16:22:01 1478  	     var_current_license_id,
16:22:01 1479  	     var_current_license_start_date,
16:22:01 1480  	     var_current_license_end_date,
16:22:01 1481  	     var_current_offer_index,
16:22:01 1482  	     var_current_offer_recurr_num,
16:22:01 1483  	     var_current_offer_id,
16:22:01 1484  	     var_current_invoice_id
16:22:01 1485  	   FROM
16:22:01 1486  	     LICENSE
16:22:01 1487  	     INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
16:22:01 1488  	     INNER JOIN OFFER ON LICENSE.OFFER_ID = OFFER.ID
16:22:01 1489  	     INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
16:22:01 1490  	   WHERE
16:22:01 1491  	     SUBSCRIPTION.ID = in_subscription_id
16:22:01 1492  	     AND PROCS_COMMON_V22.NORMALIZE_DATE(LICENSE.END_DATE) > var_current_date
16:22:01 1493  	     AND PROCS_COMMON_V22.NORMALIZE_DATE(LICENSE.START_DATE) <= var_current_date
16:22:01 1494  	     AND ROWNUM <= 1
16:22:01 1495  	   ORDER BY
16:22:01 1496  	     LICENSE.ID DESC;
16:22:01 1497  	   EXCEPTION
16:22:01 1498  	     WHEN NO_DATA_FOUND THEN
16:22:01 1499  	       RAISE CAN_NOT_FIND_OFFER_OR_LICENSE;
16:22:01 1500  	 END;
16:22:01 1501  
16:22:01 1502  	 IF var_current_license_end_date < in_effective_start_date THEN
16:22:01 1503  	   RAISE EXTENS_START_DATE_IS_TOO_FAR;
16:22:01 1504  	 END IF;
16:22:01 1505  
16:22:01 1506  	 IF var_current_license_start_date > in_effective_start_date THEN
16:22:01 1507  	   RAISE EXTENS_START_DATE_IS_TOO_SMALL;
16:22:01 1508  	 END IF;
16:22:01 1509  
16:22:01 1510  	 IF in_effective_start_date > in_effective_end_date THEN
16:22:01 1511  	   RAISE EXT_START_DATE_LATER_THEN_END;
16:22:01 1512  	 END IF;
16:22:01 1513  
16:22:01 1514  	 -- Closing curent license
16:22:01 1515  	 BEGIN
16:22:01 1516  	   PROCS_LICENSE_CRU_V22.UPDATE_LICENSE(
16:22:01 1517  	     in_license_id	   => var_current_license_id,
16:22:01 1518  	     in_updated_by	   => in_updated_by,
16:22:01 1519  	     in_license_status_id  => GLOBAL_STATUSES_V22.LICENSE_CLOSED,
16:22:01 1520  	     in_end_date	   => in_effective_start_date,
16:22:01 1521  	     in_needs_entitlements => GLOBAL_CONSTANTS_V22.TRUE
16:22:01 1522  	   );
16:22:01 1523  	   EXCEPTION
16:22:01 1524  	     WHEN OTHERS THEN
16:22:01 1525  	       EXCEPTION_MESSAGE := SQLERRM;
16:22:01 1526  	       RAISE CAN_NOT_CHANGE_LICENSE_STATUS;
16:22:01 1527  	 END;
16:22:01 1528  
16:22:01 1529  	 -- Creating new "free" invoice
16:22:01 1530  	 BEGIN
16:22:01 1531  	   PROCS_INVOICE_V22.CREATE_INVOICE(
16:22:01 1532  	     in_invoice_status => GLOBAL_STATUSES_V22.INVOICE_CLOSED,
16:22:01 1533  	     in_created_by     => in_updated_by,
16:22:01 1534  	     in_tax_exempt_id  => var_account_tax_exempt_id,
16:22:01 1535  	     out_invoice_id    => var_free_invoice_id
16:22:01 1536  	   );
16:22:01 1537  	   EXCEPTION
16:22:01 1538  	     WHEN OTHERS THEN
16:22:01 1539  	       EXCEPTION_MESSAGE := SQLERRM;
16:22:01 1540  	       RAISE CAN_NOT_CREATE_INVOICE;
16:22:01 1541  	 END;
16:22:01 1542  
16:22:01 1543  	 -- Creating new "free" license
16:22:01 1544  	 BEGIN
16:22:01 1545  	   PROCS_LICENSE_V22.CREATE_LICENSE (
16:22:01 1546  	     in_status_id		 => GLOBAL_STATUSES_V22.LICENSE_ACTIVE,
16:22:01 1547  	     in_needs_entitlements	 => GLOBAL_CONSTANTS_V22.TRUE,
16:22:01 1548  	     in_start_date		 => in_effective_start_date,
16:22:01 1549  	     in_end_date		 => in_effective_end_date,
16:22:01 1550  	     in_offer_id		 => var_current_offer_id,
16:22:01 1551  	     in_subscription_id 	 => in_subscription_id,
16:22:01 1552  	     in_invoice_id		 => var_free_invoice_id,
16:22:01 1553  	     in_created_by		 => in_updated_by,
16:22:01 1554  	     in_is_extension		 => GLOBAL_CONSTANTS_V22.TRUE,
16:22:01 1555  	     in_current_offer_index	 => var_current_offer_index,
16:22:01 1556  	     in_current_offer_recurr_num => var_current_offer_recurr_num,
16:22:01 1557  	     out_license_id		 => var_free_license_id
16:22:01 1558  	   );
16:22:01 1559  	   EXCEPTION
16:22:01 1560  	     WHEN OTHERS THEN
16:22:01 1561  	       EXCEPTION_MESSAGE := SQLERRM;
16:22:01 1562  	       RAISE CAN_NOT_CREATE_NEW_LICENSE;
16:22:01 1563  	 END;
16:22:01 1564  
16:22:01 1565  	 -- Creating new license
16:22:01 1566  	 IF PROCS_COMMON_V22.NORMALIZE_DATE(var_current_license_end_date) >
16:22:01 1567  	    PROCS_COMMON_V22.NORMALIZE_DATE(in_effective_start_date) THEN
16:22:01 1568  	   BEGIN
16:22:01 1569  	     PROCS_LICENSE_V22.CREATE_LICENSE (
16:22:01 1570  	       in_status_id		   => GLOBAL_STATUSES_V22.LICENSE_ACTIVE,
16:22:01 1571  	       in_needs_entitlements	   => GLOBAL_CONSTANTS_V22.TRUE,
16:22:01 1572  	       in_start_date		   => in_effective_end_date,
16:22:01 1573  	       in_end_date		   => var_current_license_end_date + (in_effective_end_date - in_effective_start_date),
16:22:01 1574  	       in_offer_id		   => var_current_offer_id,
16:22:01 1575  	       in_subscription_id	   => in_subscription_id,
16:22:01 1576  	       in_invoice_id		   => var_current_invoice_id,
16:22:01 1577  	       in_created_by		   => in_updated_by,
16:22:01 1578  	       in_is_extension		   => GLOBAL_CONSTANTS_V22.FALSE,
16:22:01 1579  	       in_current_offer_index	   => var_current_offer_index,
16:22:01 1580  	       in_current_offer_recurr_num => var_current_offer_recurr_num,
16:22:01 1581  	       out_license_id		   => var_ext_license_id
16:22:01 1582  	     );
16:22:01 1583  	     EXCEPTION
16:22:01 1584  	       WHEN OTHERS THEN
16:22:01 1585  		 EXCEPTION_MESSAGE := SQLERRM;
16:22:01 1586  		 RAISE CAN_NOT_CREATE_END_LICENSE;
16:22:01 1587  	   END;
16:22:01 1588  	 END IF;
16:22:01 1589  
16:22:01 1590  	 -- Create new note for subscription
16:22:01 1591  	 BEGIN
16:22:01 1592  	   PROCS_SUBSCRIPTION_V22.ANNOTATE_SUBSCRIPTION (
16:22:01 1593  	     in_subscription_id => in_subscription_id,
16:22:01 1594  	     in_agent_id	=> 0, -- FIXME: What should to be here (agent id)?
16:22:01 1595  	     in_note		=> in_note,
16:22:01 1596  	     in_created_by	=> in_updated_by
16:22:01 1597  	   );
16:22:01 1598  	   EXCEPTION
16:22:01 1599  	     WHEN OTHERS THEN
16:22:01 1600  	       EXCEPTION_MESSAGE := SQLERRM;
16:22:01 1601  	       RAISE CAN_NOT_ANNOTATE_SUBSCRIPTION;
16:22:01 1602  	 END;
16:22:01 1603  
16:22:01 1604  EXCEPTION
16:22:01 1605  WHEN EXT_START_DATE_LATER_THEN_END THEN
16:22:01 1606  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INVALID_PARAMETER,
16:22:01 1607  	   SPROC_NAME, 'Extension start date is bigger then end date');
16:22:01 1608  WHEN EXTENS_START_DATE_IS_TOO_FAR THEN
16:22:01 1609  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INVALID_PARAMETER,
16:22:01 1610  	   SPROC_NAME, 'Extension start date is too far');
16:22:01 1611  WHEN EXTENS_START_DATE_IS_TOO_SMALL THEN
16:22:01 1612  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INVALID_PARAMETER,
16:22:01 1613  	   SPROC_NAME, 'Extension start date is too small');
16:22:01 1614  WHEN BAD_SUBSCRIPTION_ID THEN
16:22:01 1615  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 1616  	   SPROC_NAME, 'No such subscription');
16:22:01 1617  WHEN CAN_NOT_FIND_OFFER_OR_LICENSE THEN
16:22:01 1618  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 1619  	   SPROC_NAME, 'Could not find license and/or offer for given subscription');
16:22:01 1620  WHEN CAN_NOT_CHANGE_LICENSE_STATUS THEN
16:22:01 1621  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:01 1622  	   SPROC_NAME, 'Could not change license status', EXCEPTION_MESSAGE);
