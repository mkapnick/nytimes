16:22:02 476  EXCEPTION_MESSAGE VARCHAR2(1024);
16:22:02 477  BEGIN
16:22:02 478  
16:22:02 479  	-- Get Paypal status
16:22:02 480  	-- Get account
16:22:02 481  	BEGIN
