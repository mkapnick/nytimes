16:22:01 1166  
16:22:01 1167  PROCEDURE REFUND_SUBSCRIPTION (
16:22:01 1168  /*
16:22:01 1169  Throws exceptions:
16:22:01 1170  APP_EXCEPTION_CODES_V22.NOT_FOUND
16:22:01 1171  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
16:22:01 1172  */
16:22:01 1173  	 in_subscription_id IN NUMBER,
16:22:01 1174  	 in_invoice_id	    IN NUMBER,
16:22:01 1175  	 in_refund_amount   IN NUMBER,
16:22:01 1176  	 in_note	    IN VARCHAR2,
16:22:01 1177  	 in_created_by	    IN VARCHAR2,
16:22:01 1178  	 out_charge_id	    OUT NUMBER
16:22:01 1179  ) AS
16:22:01 1180  -- VARIABLES
16:22:01 1181  SPROC_NAME	      CONSTANT VARCHAR2(19) := 'REFUND_SUBSCRIPTION';
16:22:01 1182  var_invoice_status_id  NUMBER;
16:22:01 1183  var_account_id	      NUMBER;
16:22:01 1184  var_account_status_id  NUMBER;
16:22:01 1185  var_new_transaction_id NUMBER;
16:22:01 1186  var_instrument_type_id NUMBER;
16:22:01 1187  var_instrument_id      NUMBER;
16:22:01 1188  var_new_charge_id      NUMBER;
16:22:01 1189  var_invoice_amount     NUMBER(10,2);
16:22:01 1190  var_refunds_before     NUMBER(10,2);
16:22:01 1191  var_charges_amount     NUMBER(10,2);
16:22:01 1192  -- EXCEPTIONS
16:22:01 1193  BAD_SUBSCRIPTION_ID	     EXCEPTION;
16:22:01 1194  ACCOUNT_IS_FROZEN	     EXCEPTION;
16:22:01 1195  BAD_INVOICE_ID		     EXCEPTION;
16:22:01 1196  CAN_NOT_CREATE_TRANSACTION    EXCEPTION;
16:22:01 1197  CAN_NOT_CREATE_CHARGE	     EXCEPTION;
16:22:01 1198  CAN_NOT_CALC_INVOICE_AMOUNT   EXCEPTION;
16:22:01 1199  REFUND_IS_GREATER_THAN_ANOUNT EXCEPTION;
16:22:01 1200  CAN_NOT_ANNOTATE_SUBSCRIPTION EXCEPTION;
16:22:01 1201  TOT_REF_IS_GREATER_THAN_ANOUNT EXCEPTION;
16:22:01 1202  INVOICE_IS_NOT_CLOSED	     EXCEPTION;
16:22:01 1203  TOT_REF_IS_GRATER_THAN_CHARGES EXCEPTION;
16:22:01 1204  EXCEPTION_MESSAGE	      VARCHAR2(1024);
16:22:01 1205  BEGIN
16:22:01 1206  
16:22:01 1207  	 BEGIN
16:22:01 1208  	   SELECT
16:22:01 1209  	     SUBSCRIPTION.INSTRUMENT_ID,
16:22:01 1210  	     SUBSCRIPTION.INSTRUMENT_TYPE_ID,
16:22:01 1211  	     SUBSCRIPTION.ACCOUNT_ID
16:22:01 1212  	     into
16:22:01 1213  	     var_instrument_id,
16:22:01 1214  	     var_instrument_type_id,
16:22:01 1215  	     var_account_id
16:22:01 1216  	   FROM
16:22:01 1217  	     SUBSCRIPTION
16:22:01 1218  	   WHERE
16:22:01 1219  	     SUBSCRIPTION.ID = in_subscription_id;
16:22:01 1220  	   EXCEPTION
16:22:01 1221  	     WHEN NO_DATA_FOUND THEN
16:22:01 1222  	       RAISE BAD_SUBSCRIPTION_ID;
16:22:01 1223  	 END;
16:22:01 1224  
16:22:01 1225  	 -- Check account status. It should not to be frozen
16:22:01 1226  	 SELECT
16:22:01 1227  	   ACCOUNT.ACCOUNT_STATUS_ID into var_account_status_id
16:22:01 1228  	 FROM
16:22:01 1229  	   ACCOUNT
16:22:01 1230  	 WHERE
16:22:01 1231  	   ACCOUNT.ID = var_account_id;
16:22:01 1232  
16:22:01 1233  	 IF var_account_status_id = GLOBAL_STATUSES_V22.ACCOUNT_FROZEN THEN
16:22:01 1234  	   RAISE ACCOUNT_IS_FROZEN;
16:22:01 1235  	 END IF;
16:22:01 1236  
16:22:01 1237  	 BEGIN
16:22:01 1238  	   SELECT
16:22:01 1239  	     INVOICE.INVOICE_STATUS_ID into var_invoice_status_id
16:22:01 1240  	   FROM
16:22:01 1241  	     INVOICE
16:22:01 1242  	   WHERE
16:22:01 1243  	     INVOICE.ID = in_invoice_id;
16:22:01 1244  	   EXCEPTION
16:22:01 1245  	     WHEN NO_DATA_FOUND THEN
16:22:01 1246  	       RAISE BAD_INVOICE_ID;
16:22:01 1247  	 END;
16:22:01 1248  
16:22:01 1249  	 IF var_invoice_status_id != GLOBAL_STATUSES_V22.INVOICE_CLOSED THEN
16:22:01 1250  	   RAISE INVOICE_IS_NOT_CLOSED;
16:22:01 1251  	 END IF;
16:22:01 1252  
16:22:01 1253  	 BEGIN
16:22:01 1254  	   PROCS_INVOICE_V22.CALCULATE_INVOICE_AMOUNT (
16:22:01 1255  	     in_invoice_id => in_invoice_id,
16:22:01 1256  	     out_amount    => var_invoice_amount
16:22:01 1257  	   );
16:22:01 1258  	   EXCEPTION
16:22:01 1259  	     WHEN OTHERS THEN
16:22:01 1260  	       EXCEPTION_MESSAGE := SQLERRM;
16:22:01 1261  	       RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
16:22:01 1262  	 END;
16:22:01 1263  
16:22:01 1264  	 IF ( in_refund_amount > var_invoice_amount ) THEN
16:22:01 1265  	   RAISE REFUND_IS_GREATER_THAN_ANOUNT;
16:22:01 1266  	 END IF;
16:22:01 1267  
16:22:01 1268  	 SELECT /*+ STAR_TRANSFORMATION */
16:22:01 1269  	   SUM(CHARGE.CHARGE_AMOUNT) into var_refunds_before
16:22:01 1270  	 FROM
16:22:01 1271  	   CHARGE
16:22:01 1272  	 WHERE
16:22:01 1273  	   CHARGE.INVOICE_ID = in_invoice_id
16:22:01 1274  	   AND (
16:22:01 1275  	     CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V22.CHARGE_OPENED
16:22:01 1276  	     OR CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V22.CHARGE_PROCESSED
16:22:01 1277  	   )
16:22:01 1278  	   AND CHARGE.CHARGE_AMOUNT < 0;
16:22:01 1279  
16:22:01 1280  	 -- Refunds are negative
16:22:01 1281  	 var_refunds_before := -var_refunds_before;
16:22:01 1282  
16:22:01 1283  	 var_charges_amount := 0;
16:22:01 1284  
16:22:01 1285  	 FOR f_processed_charges IN (
16:22:01 1286  	   SELECT
16:22:01 1287  	     CHARGE.CHARGE_AMOUNT
16:22:01 1288  	   FROM
16:22:01 1289  	     CHARGE
16:22:01 1290  	   WHERE
16:22:01 1291  	     CHARGE.INVOICE_ID = in_invoice_id
16:22:01 1292  	     AND CHARGE.CHARGE_STATUS_ID IN (SELECT GLOBAL_STATUSES_V22.CHARGE_PROCESSED FROM DUAL)
16:22:01 1293  	 )
16:22:01 1294  	 LOOP
16:22:01 1295  	   IF f_processed_charges.CHARGE_AMOUNT > 0 THEN
16:22:01 1296  	     var_charges_amount := var_charges_amount + f_processed_charges.CHARGE_AMOUNT;
16:22:01 1297  	   END IF;
16:22:01 1298  	 END LOOP;
16:22:01 1299  
16:22:01 1300  	 IF (in_refund_amount + var_refunds_before > var_invoice_amount) THEN
16:22:01 1301  	   RAISE TOT_REF_IS_GREATER_THAN_ANOUNT;
16:22:01 1302  	 END IF;
16:22:01 1303  
16:22:01 1304  	 IF (in_refund_amount + var_refunds_before > var_charges_amount) THEN
16:22:01 1305  	   RAISE TOT_REF_IS_GRATER_THAN_CHARGES;
16:22:01 1306  	 END IF;
16:22:01 1307  
16:22:01 1308  	 BEGIN
16:22:01 1309  	   PROCS_TRANSACTION_V22.CREATE_TRANSACTION(
16:22:01 1310  	     in_transaction_id	       => NULL,
16:22:01 1311  	     in_status_id	       => GLOBAL_STATUSES_V22.TRANSACTION_PREPARE,
16:22:01 1312  	     in_amount		       => -in_refund_amount,
16:22:01 1313  	     in_created_by	       => in_created_by,
16:22:01 1314  	     in_order_id	       => NULL,
16:22:01 1315  	     in_is_refund	       => GLOBAL_CONSTANTS_V22.TRUE,
16:22:01 1316  	     in_transaction_type_code  => 'REFUND',
16:22:01 1317  	     out_transaction_id        => var_new_transaction_id
16:22:01 1318  	   );
16:22:01 1319  	   EXCEPTION
16:22:01 1320  	     WHEN OTHERS THEN
16:22:01 1321  	       EXCEPTION_MESSAGE := SQLERRM;
16:22:01 1322  	       RAISE CAN_NOT_CREATE_TRANSACTION;
16:22:01 1323  	 END;
16:22:01 1324  
16:22:01 1325  	 BEGIN
16:22:01 1326  	   PROCS_CHARGE_V22.CREATE_CHARGE(
16:22:01 1327  	     in_invoice_id	   => in_invoice_id,
16:22:01 1328  	     in_transaction_id	   => var_new_transaction_id,
16:22:01 1329  	     in_instrument_type_id => var_instrument_type_id,
16:22:01 1330  	     in_instrument_id	   => var_instrument_id,
16:22:01 1331  	     in_charge_amount	   => -in_refund_amount,
16:22:01 1332  	     in_created_by	   => in_created_by,
16:22:01 1333  	     in_charge_status_id   => GLOBAL_STATUSES_V22.CHARGE_OPENED,
16:22:01 1334  	     out_charge_id	   => var_new_charge_id
16:22:01 1335  	   );
16:22:01 1336  	   EXCEPTION
16:22:01 1337  	     WHEN OTHERS THEN
16:22:01 1338  	       EXCEPTION_MESSAGE := SQLERRM;
16:22:01 1339  	       RAISE CAN_NOT_CREATE_CHARGE;
16:22:01 1340  	 END;
16:22:01 1341  
16:22:01 1342  	 IF in_note IS NOT NULL THEN
16:22:01 1343  	   BEGIN
16:22:01 1344  	     PROCS_SUBSCRIPTION_V22.ANNOTATE_SUBSCRIPTION(
16:22:01 1345  	       in_subscription_id => in_subscription_id,
16:22:01 1346  	       in_agent_id	  => 0, -- AGENT_ID??
16:22:01 1347  	       in_note		  => in_note,
16:22:01 1348  	       in_created_by	  => in_created_by
16:22:01 1349  	     );
16:22:01 1350  	     EXCEPTION
16:22:01 1351  	       WHEN OTHERS THEN
16:22:01 1352  		 EXCEPTION_MESSAGE := SQLERRM;
16:22:01 1353  		 RAISE CAN_NOT_ANNOTATE_SUBSCRIPTION;
16:22:01 1354  	   END;
16:22:01 1355  	 END IF;
16:22:01 1356  
16:22:01 1357  	 out_charge_id := var_new_charge_id;
16:22:01 1358  
16:22:01 1359  EXCEPTION
16:22:01 1360  WHEN INVOICE_IS_NOT_CLOSED THEN
16:22:01 1361  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.STATE_ERROR,
16:22:01 1362  	   SPROC_NAME, 'Invoice is not closed');
16:22:01 1363  WHEN BAD_SUBSCRIPTION_ID THEN
16:22:01 1364  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 1365  	   SPROC_NAME, 'No such subscription');
16:22:01 1366  WHEN ACCOUNT_IS_FROZEN THEN
16:22:01 1367  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.STATE_ERROR,
16:22:01 1368  	   SPROC_NAME, 'Could not refund subscription for frozen account');
16:22:01 1369  WHEN BAD_INVOICE_ID THEN
16:22:01 1370  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 1371  	   SPROC_NAME, 'No such invoice');
16:22:01 1372  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
16:22:01 1373  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:01 1374  	   SPROC_NAME, 'Could not calculate invoice amount', EXCEPTION_MESSAGE);
16:22:01 1375  WHEN REFUND_IS_GREATER_THAN_ANOUNT THEN
16:22:01 1376  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INVALID_PARAMETER,
16:22:01 1377  	   SPROC_NAME, 'Refund is greater than amount');
16:22:01 1378  WHEN TOT_REF_IS_GREATER_THAN_ANOUNT THEN
16:22:01 1379  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INVALID_PARAMETER,
16:22:01 1380  	   SPROC_NAME, 'There were refunds before and sum of all refunds and new refund more than invoice amount');
16:22:01 1381  WHEN TOT_REF_IS_GRATER_THAN_CHARGES THEN
16:22:01 1382  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INVALID_PARAMETER,
16:22:01 1383  	   SPROC_NAME, 'Total refund amount is greater than sum of processed charges');
16:22:01 1384  WHEN CAN_NOT_CREATE_TRANSACTION THEN
16:22:01 1385  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:01 1386  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
16:22:01 1387  WHEN CAN_NOT_CREATE_CHARGE THEN
16:22:01 1388  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:01 1389  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
16:22:01 1390  WHEN CAN_NOT_ANNOTATE_SUBSCRIPTION THEN
16:22:01 1391  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:01 1392  	   SPROC_NAME, 'Could not annotate subscription', EXCEPTION_MESSAGE);
16:22:01 1393  WHEN OTHERS THEN
16:22:01 1394  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:01 1395  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:22:01 1396  END REFUND_SUBSCRIPTION;
