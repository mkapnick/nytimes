16:22:02 396  	  ACCOUNT
16:22:02 397  	WHERE
16:22:02 398  	  ACCOUNT.GROUP_ID = in_group_id;
16:22:02 399  
16:22:02 400  	-- Check that incoming data is correct
16:22:02 401  	IF in_instr_status != GLOBAL_CONSTANTS_V22.TRUE AND in_instr_status != GLOBAL_CONSTANTS_V22.FALSE THEN
16:22:02 402  	  RAISE WRONG_INSTR_EXCEPTION;
16:22:02 403  	END IF;
16:22:02 404  
16:22:02 405  	OPEN out_result_gc_set FOR
16:22:02 406  	SELECT
16:22:02 407  	  GIFT_CERTIFICATE.ID,
16:22:02 408  	  GIFT_CERTIFICATE.PURCHASER_GROUP_ID,
16:22:02 409  	  GIFT_CERTIFICATE.PURCHASE_INVOICE_ID,
16:22:02 410  	  GIFT_CERTIFICATE.PURCHASE_DATE,
16:22:02 411  	  GIFT_CERTIFICATE.OFFER_CHAIN_ID,
16:22:02 412  	  GIFT_CERTIFICATE.EXPIRATION_DATE,
16:22:02 413  	  GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID,
16:22:02 414  	  GIFT_CERTIFICATE.CODE,
16:22:02 415  	  GIFT_CERTIFICATE.CREATE_DATE,
16:22:02 416  	  GIFT_CERTIFICATE.CREATED_BY,
16:22:02 417  	  GIFT_CERTIFICATE.UPDATE_DATE,
16:22:02 418  	  GIFT_CERTIFICATE.UPDATED_BY,
16:22:02 419  	  GIFT_CERTIFICATE.RECIPIENT_NAME,
16:22:02 420  	  GIFT_CERTIFICATE.RECIPIENT_EMAIL,
16:22:02 421  	  GIFT_CERTIFICATE.SENDER_NAME,
16:22:02 422  	  GIFT_CERTIFICATE.SENDER_EMAIL,
