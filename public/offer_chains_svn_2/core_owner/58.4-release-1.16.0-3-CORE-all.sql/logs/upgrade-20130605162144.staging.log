16:22:01 1901  PROCEDURE CLOSE_SUBSCRIPTION (
16:22:01 1902  	 in_subscription_id IN NUMBER,
16:22:01 1903  	 in_updated_by	    IN VARCHAR2
16:22:01 1904  ) AS
16:22:01 1905  -- VARIABLES
16:22:01 1906  SPROC_NAME	    CONSTANT VARCHAR2(18) := 'CLOSE_SUBSCRIPTION';
16:22:01 1907  temp_subscription_id NUMBER;
16:22:01 1908  var_licenses_count   NUMBER;
16:22:01 1909  -- EXCEPTIONS
