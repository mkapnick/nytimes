16:22:02 154  EXCEPTION_MESSAGE 	VARCHAR2(1024);
16:22:02 155  BEGIN
16:22:02 156  
16:22:02 157  	-- Get account's status and id
16:22:02 158  	BEGIN
16:22:02 159  	  SELECT
16:22:02 160  	    ACCOUNT.ACCOUNT_STATUS_ID,
16:22:02 161  	    ACCOUNT.ID
16:22:02 162  	  INTO
