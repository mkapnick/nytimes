16:22:02 490  	  WHERE
16:22:02 491  	    PAYPAL.ID = in_paypal_id;
16:22:02 492  	  EXCEPTION
16:22:02 493  	    WHEN NO_DATA_FOUND THEN
16:22:02 494  	      RAISE BAD_PAYPAL_ID;
16:22:02 495  	END;
16:22:02 496  
16:22:02 497  	-- Get group id
16:22:02 498  	SELECT
16:22:02 499  	  ACCOUNT.GROUP_ID into var_group_id
16:22:02 500  	FROM
16:22:02 501  	  ACCOUNT
16:22:02 502  	WHERE
16:22:02 503  	  ACCOUNT.ID = var_account_id;
16:22:02 504  
16:22:02 505  	-- Card should not be disabled
16:22:02 506  	IF var_paypal_status_id = GLOBAL_STATUSES_V22.PAYPAL_INACTIVE THEN
16:22:02 507  	  RAISE PAYPAL_ALREADY_INACTIVE;
16:22:02 508  	END IF;
16:22:02 509  
16:22:02 510  	-- Looking for pending transactions associated with given credit card
16:22:02 511  	SELECT
16:22:02 512  	  COUNT(*) into var_pending_invoices_num
16:22:02 513  	FROM
16:22:02 514  	  CHARGE
16:22:02 515  	  INNER JOIN TRANSACTION ON CHARGE.TRANSACTION_ID = TRANSACTION.ID
16:22:02 516  	WHERE
16:22:02 517  	  CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V22.INSTRUMENT_PAYPAL
16:22:02 518  	  AND CHARGE.INSTRUMENT_ID = in_paypal_id
16:22:02 519  	  AND TRANSACTION.TRANSACTION_STATUS_ID = GLOBAL_STATUSES_V22.TRANSACTION_PENDING;
16:22:02 520  
16:22:02 521  	IF var_pending_invoices_num > 0 THEN
16:22:02 522  	  RAISE PENDING_TRANSACTIONS_FOUND;
16:22:02 523  	END IF;
16:22:02 524  
16:22:02 525  	-- Getting current default financial instrument
16:22:02 526  	BEGIN
16:22:02 527  	  GET_DEF_FINANCIAL_INSTRUMENT(
16:22:02 528  	    in_group_id 	   => var_group_id,
16:22:02 529  	    out_instrument_type_id => current_def_instrument_type_id,
16:22:02 530  	    out_instrument_id	   => current_def_instrument_id
16:22:02 531  	  );
16:22:02 532  	  EXCEPTION
16:22:02 533  	    WHEN OTHERS THEN
16:22:02 534  	      EXCEPTION_MESSAGE := SQLERRM;
16:22:02 535  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
16:22:02 536  	END;
16:22:02 537  
16:22:02 538  	-- Checking that credit card is not default
16:22:02 539  	IF current_def_instrument_type_id = GLOBAL_ENUMS_V22.INSTRUMENT_PAYPAL
16:22:02 540  	  AND current_def_instrument_id = in_paypal_id THEN
16:22:02 541  	  BEGIN
16:22:02 542  	    DEL_DEF_FINANCIAL_INSTRUMENT(
16:22:02 543  	      in_group_id => var_group_id
16:22:02 544  	    );
16:22:02 545  	  EXCEPTION
16:22:02 546  	    WHEN OTHERS THEN
16:22:02 547  	      EXCEPTION_MESSAGE := SQLERRM;
16:22:02 548  	      RAISE CAN_NOT_DEL_DEF_FINANCIAL;
16:22:02 549  	  END;
16:22:02 550  	END IF;
16:22:02 551  
16:22:02 552  	PROCS_FIN_INSTRUMENTS_V22.UPDATE_PAYPAL_STATUS(
16:22:02 553  	  in_paypal_id	      => in_paypal_id,
16:22:02 554  	  in_updated_by       => in_updated_by,
16:22:02 555  	  in_paypal_status_id => GLOBAL_STATUSES_V22.PAYPAL_INACTIVE
16:22:02 556  	);
16:22:02 557  
16:22:02 558  EXCEPTION
16:22:02 559  WHEN BAD_PAYPAL_ID THEN
