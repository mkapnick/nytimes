16:22:01 1917  	     SUBSCRIPTION.ID into temp_subscription_id
16:22:01 1918  	   FROM
16:22:01 1919  	     SUBSCRIPTION
16:22:01 1920  	   WHERE
16:22:01 1921  	     SUBSCRIPTION.ID = in_subscription_id;
