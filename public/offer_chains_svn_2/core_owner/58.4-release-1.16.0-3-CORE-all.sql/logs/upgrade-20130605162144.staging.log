16:22:02 581  PROCEDURE GET_ACCOUNT_SUBSCRIPTIONS (
16:22:02 582  	  in_group_id	 IN  NUMBER,
16:22:02 583  	  in_start_date  IN DATE,
16:22:02 584  	  in_end_date	 IN DATE,
16:22:02 585  	  in_status	 IN NUMBER,
16:22:02 586  	  in_group_account_type IN VARCHAR2,
16:22:02 587  	  out_result_set OUT SYS_REFCURSOR
16:22:02 588  ) AS
16:22:02 589  -- VARIABLES
16:22:02 590  SPROC_NAME     CONSTANT VARCHAR2(25) := 'GET_ACCOUNT_SUBSCRIPTIONS';
16:22:02 591  var_account_id NUMBER;
16:22:02 592  -- EXCEPTIONS
16:22:02 593  BAD_GROUP_ID	      EXCEPTION;
16:22:02 594  BEGIN
16:22:02 595  	-- Get account id
16:22:02 596  	BEGIN
16:22:02 597  	  SELECT
16:22:02 598  	    ACCOUNT.ID INTO var_account_id
16:22:02 599  	  FROM
16:22:02 600  	    ACCOUNT
16:22:02 601  	  WHERE
16:22:02 602  	    ACCOUNT.GROUP_ID = in_group_id;
16:22:02 603  	  EXCEPTION
16:22:02 604  	    WHEN NO_DATA_FOUND THEN
16:22:02 605  	      RAISE BAD_GROUP_ID;
16:22:02 606  	END;
16:22:02 607  
16:22:02 608  	-- Get information about account subscriptions
16:22:02 609  	OPEN out_result_set FOR
16:22:02 610  	SELECT
16:22:02 611  	  in_group_id AS "GROUP_ID",
16:22:02 612  	  SUBSCRIPTION.ID AS "SUBSCRIPTION_ID",
16:22:02 613  	  SUBSCRIPTION.SUBSCRIPTION_STATUS_ID,
16:22:02 614  	  SUBSCRIPTION.PURCHASE_DATE,
16:22:02 615  	  SUBSCRIPTION.SUSPEND_DATE,
16:22:02 616  	  SUBSCRIPTION.REACTIVATION_DATE,
16:22:02 617  	  SUBSCRIPTION.CANCELLATION_DATE,
16:22:02 618  	  SUBSCRIPTION_CANCEL_REASON.VALUE as "CANCEL_TYPE",
16:22:02 619  	  SUBSCRIPTION.INSTRUMENT_ID,
16:22:02 620  	  SUBSCRIPTION.INSTRUMENT_TYPE_ID,
16:22:02 621  	  OFFER_CHAIN.ID AS "OFFER_CHAIN_ID",
16:22:02 622  	  OFFER_CHAIN.NAME,
16:22:02 623  	  OFFER_CHAIN.DESCRIPTION,
16:22:02 624  	  OFFER_CHAIN.PRODUCT_URI,
16:22:02 625  	  PROCS_SUBSCRIPTION_V22.CALC_SUBSCRIPTION_END_DATE(SUBSCRIPTION.ID) as "END_DATE",
16:22:02 626  	  PROCS_SUBSCRIPTION_V22.GET_RECENT_CHARGE(SUBSCRIPTION.ID) AS "RECENT_CHARGE",
16:22:02 627  	  PROCS_SUBSCRIPTION_V22.GET_RENEWAL_DATE(SUBSCRIPTION.ID) AS "RENEWAL_DATE",
16:22:02 628  	  PROCS_SUBSCRIPTION_V22.GET_BILLING_CYCLE(SUBSCRIPTION.ID) AS "BILLING_CYCLE",
16:22:02 629  	  PROCS_SUBSCRIPTION_V22.IS_SUBSCRIPTION_CANCELABLE(SUBSCRIPTION.ID) AS "IS_CANCELABLE",
16:22:02 630  	  ITUNES_RECEIPT.ID AS "ITUNES_RECEIPT_ID",
16:22:02 631  	  (
16:22:02 632  	    SELECT
16:22:02 633  	      MAX(ENTITLEMENT_END_DATE)
16:22:02 634  	      FROM LICENSE
16:22:02 635  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
16:22:02 636  	  )
16:22:02 637  	  as "ENT_END_DATE",
16:22:02 638  	  (
16:22:02 639  	    SELECT
16:22:02 640  	      MIN(START_DATE)
16:22:02 641  	      FROM LICENSE
16:22:02 642  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
16:22:02 643  	  )
16:22:02 644  	  as "ENT_START_DATE",
16:22:02 645  	  GROUP_ACCOUNT.SUBSCRIPTION_ID GA_SUBSCRIPTION_ID,
16:22:02 646  	  GROUP_ACCOUNT.ID GA_ID,
16:22:02 647  	  GROUP_ACCOUNT.GROUP_NAME GA_GROUP_NAME,
16:22:02 648  	  GROUP_ACCOUNT.FIRST_NAME GA_FIRST_NAME,
16:22:02 649  	  GROUP_ACCOUNT.LAST_NAME GA_LAST_NAME,
16:22:02 650  	  GROUP_ACCOUNT.EMAIL GA_EMAIL,
16:22:02 651  	  GROUP_ACCOUNT.PHONE GA_PHONE,
16:22:02 652  	  GROUP_ACCOUNT.ORGANIZATION_TYPE GA_ORGANIZATION_TYPE,
16:22:02 653  	  GROUP_ACCOUNT.SEATS GA_SEATS,
16:22:02 654  	  PROCS_GROUP_ACCOUNT_V22.F_GET_NUM_OCCUPIED_GROUP_SEATS(GROUP_ACCOUNT.ID) GA_SEATS_USED,
16:22:02 655  	  GROUP_ACCOUNT.IP GA_IP,
16:22:02 656  	  PROCS_SUBSCRIPTION_V22.GET_GIFT_CERT_CODE_BY_SUB_ID(SUBSCRIPTION.ID) GIFT_CERTIFICATE_CODE,
16:22:02 657  	  PROCS_ACCOUNT_V22.GET_GRACE_START_DATE(SUBSCRIPTION.ID) GRACE_START_DATE,
16:22:02 658  	  PROCS_ACCOUNT_V22.GET_GRACE_END_DATE(SUBSCRIPTION.ID) GRACE_END_DATE
16:22:02 659  	FROM
16:22:02 660  	  SUBSCRIPTION
16:22:02 661  	  INNER JOIN OFFER_CHAIN ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
16:22:02 662  	  LEFT JOIN SUBSCRIPTION_CANCEL_REASON ON SUBSCRIPTION.SCT_ID = SUBSCRIPTION_CANCEL_REASON.ID
16:22:02 663  	  LEFT JOIN ITUNES_RECEIPT ON SUBSCRIPTION.ID = ITUNES_RECEIPT.SUBSCRIPTION_ID
16:22:02 664  	  LEFT JOIN GROUP_ACCOUNT ON SUBSCRIPTION.ID = GROUP_ACCOUNT.SUBSCRIPTION_ID
16:22:02 665  	WHERE
16:22:02 666  	  SUBSCRIPTION.ACCOUNT_ID = var_account_id
16:22:02 667  	  AND (SUBSCRIPTION.SCT_ID IS NULL OR SUBSCRIPTION.SCT_ID != GLOBAL_STATUSES_V22.REAL_TIME_CANCEL_REASON)
16:22:02 668  	  AND SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = NVL(in_status, SUBSCRIPTION.SUBSCRIPTION_STATUS_ID)
16:22:02 669  	  AND PROCS_COMMON_V22.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE) >= NVL(in_start_date, PROCS_COMMON_V22.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE))
16:22:02 670  	  AND PROCS_COMMON_V22.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE) <= NVL(in_end_date, PROCS_COMMON_V22.NORMALIZE_DATE(SUBSCRIPTION.PURCHASE_DATE))
16:22:02 671  	  AND (in_group_account_type IS NULL OR OFFER_CHAIN.GROUP_ACCOUNT_TYPE_ID = in_group_account_type);
16:22:02 672  
16:22:02 673  EXCEPTION
16:22:02 674  WHEN BAD_GROUP_ID THEN
16:22:02 675  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:02 676  	  SPROC_NAME, 'Can not find account with given group id');
16:22:02 677  WHEN OTHERS THEN
16:22:02 678  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:02 679  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:22:02 680  END GET_ACCOUNT_SUBSCRIPTIONS;
