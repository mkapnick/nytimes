16:22:00   9  	in_invoice_id IN NUMBER,
16:22:00  10  	in_offer_id   IN NUMBER,
16:22:00  11  	in_created_by IN VARCHAR2
16:22:00  12  ) AS
16:22:00  13  -- VARIABLES
16:22:00  14  SPROC_NAME      CONSTANT VARCHAR2(14) := 'ADD_LINE_ITEMS';
16:22:00  15  temp_invoice_id NUMBER;
16:22:00  16  temp_offer_id   NUMBER;
16:22:00  17  
16:22:00  18  var_line_item_data SYS_REFCURSOR;
16:22:00  19  var_new_line_item_id NUMBER;
16:22:00  20  var_product_unit_price NUMBER (10,6);
16:22:00  21  var_product_offering_price NUMBER(10,6);
16:22:00  22  var_product_offering_oprice NUMBER(10,6);
16:22:00  23  var_product_quantity NUMBER;
16:22:00  24  var_product_offering_id NUMBER;
16:22:00  25  
16:22:00  26  var_line_item_price	  NUMBER(10,2);
16:22:00  27  var_discount_fixed_amount   NUMBER(10,6);
16:22:00  28  var_discount_percent_amount NUMBER(10,2);
16:22:00  29  
16:22:00  30  
16:22:00  31  -- EXCEPTIONS
16:22:00  32  BAD_INVOICE_ID EXCEPTION;
16:22:00  33  BAD_OFFER_ID EXCEPTION;
16:22:00  34  BAD_DISCOUNT EXCEPTION;
16:22:00  35  BEGIN
16:22:00  36  
16:22:00  37  	-- Check that given invoice exists
16:22:00  38  	BEGIN
16:22:00  39  	  SELECT
16:22:00  40  	    INVOICE.ID into temp_invoice_id
16:22:00  41  	  FROM
16:22:00  42  	    INVOICE
16:22:00  43  	  WHERE
16:22:00  44  	    INVOICE.ID = in_invoice_id;
16:22:00  45  
16:22:00  46  	  EXCEPTION
16:22:00  47  	    WHEN NO_DATA_FOUND THEN
16:22:00  48  	      RAISE BAD_INVOICE_ID;
16:22:00  49  	END;
16:22:00  50  
16:22:00  51  	-- Check that given offer exists
16:22:00  52  	BEGIN
16:22:00  53  	  SELECT
16:22:00  54  	    OFFER.ID into temp_offer_id
16:22:00  55  	  FROM
16:22:00  56  	    OFFER
16:22:00  57  	  WHERE
16:22:00  58  	    OFFER.ID = in_offer_id;
16:22:00  59  
16:22:00  60  	  EXCEPTION
16:22:00  61  	    WHEN NO_DATA_FOUND THEN
16:22:00  62  	      RAISE BAD_OFFER_ID;
16:22:00  63  	END;
16:22:00  64  
16:22:00  65  	-- Get product_offering data
16:22:00  66  	OPEN var_line_item_data FOR
16:22:00  67  	SELECT
16:22:00  68  	  PRODUCT_OFFERING.ID,
16:22:00  69  	  PRODUCT_OFFERING.UNIT_PRICE,
16:22:00  70  	  PRODUCT_OFFERING.QUANTITY
16:22:00  71  	FROM
16:22:00  72  	  OFFER_PRODUCT_OFFERING
16:22:00  73  	  INNER JOIN PRODUCT_OFFERING ON OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = PRODUCT_OFFERING.ID
16:22:00  74  	WHERE
16:22:00  75  	  OFFER_PRODUCT_OFFERING.OFFER_ID = in_offer_id;
16:22:00  76  
16:22:00  77  	-- insert line items
16:22:00  78  	-- add discounts to line items
16:22:00  79  	LOOP
16:22:00  80  	  FETCH var_line_item_data INTO
16:22:00  81  	    var_product_offering_id,
16:22:00  82  	    var_product_unit_price,
16:22:00  83  	    var_product_quantity;
16:22:00  84  	  EXIT WHEN var_line_item_data%NOTFOUND;
16:22:00  85  
16:22:00  86  	  var_product_offering_oprice := var_product_unit_price * var_product_quantity;
16:22:00  87  	  var_product_offering_price := var_product_offering_oprice;
16:22:00  88  
16:22:00  89  	  -- Apply discounts to line_item
16:22:00  90  	  BEGIN
16:22:00  91  	    SELECT
16:22:00  92  	      SUM (DISCOUNT.FIXED_AMOUNT) into var_discount_fixed_amount
16:22:00  93  	    FROM
16:22:00  94  	      DISCOUNT_PRODUCT_OFFERING
16:22:00  95  	      INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
16:22:00  96  	    WHERE
16:22:00  97  	      DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
16:22:00  98  	      AND DISCOUNT.FIXED_AMOUNT IS NOT NULL;
16:22:00  99  	    EXCEPTION
16:22:00 100  	    WHEN NO_DATA_FOUND THEN
16:22:00 101  	      var_discount_fixed_amount := NULL;
16:22:00 102  	  END;
16:22:00 103  
16:22:00 104  	  BEGIN
16:22:00 105  	    SELECT
16:22:00 106  	      SUM (DISCOUNT.PERCENT_AMOUNT) into var_discount_percent_amount
16:22:00 107  	    FROM
16:22:00 108  	      DISCOUNT_PRODUCT_OFFERING
16:22:00 109  	      INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
16:22:00 110  	    WHERE
16:22:00 111  	      DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
16:22:00 112  	      AND DISCOUNT.PERCENT_AMOUNT IS NOT NULL;
16:22:00 113  	    EXCEPTION
16:22:00 114  	    WHEN NO_DATA_FOUND THEN
16:22:00 115  	      var_discount_percent_amount := NULL;
16:22:00 116  	  END;
16:22:00 117  
16:22:00 118  	  IF (var_discount_percent_amount IS NOT NULL) THEN
16:22:00 119  	    var_product_offering_price := var_product_offering_price * ( 1 - var_discount_percent_amount / 100 );
16:22:00 120  	  END IF;
16:22:00 121  
16:22:00 122  	  IF (var_discount_fixed_amount IS NOT NULL) THEN
16:22:00 123  	    var_product_offering_price := var_product_offering_price - var_discount_fixed_amount;
16:22:00 124  	  END IF;
16:22:00 125  
16:22:00 126  	  var_line_item_price := PROCS_COMMON_V22.ROUND_10_6_TO_10_2(var_product_offering_price);
16:22:00 127  
16:22:00 128  	  IF (var_line_item_price < 0) THEN
16:22:00 129  	      RAISE BAD_DISCOUNT;
16:22:00 130  	  END IF;
16:22:00 131  
16:22:00 132  	  var_new_line_item_id := NULL;
16:22:00 133  	  PROCS_LINE_ITEMS_CRU_V22.CREATE_LINE_ITEM(
16:22:00 134  	    inout_line_item_id	=> var_new_line_item_id,
16:22:00 135  	    in_product_offer_id => var_product_offering_id,
16:22:00 136  	    in_invoice_id	=> in_invoice_id,
16:22:00 137  	    in_amount		=> var_line_item_price,
16:22:00 138  	    in_created_by	=> in_created_by,
16:22:00 139  	    in_discount_amount	=> var_product_offering_oprice - var_line_item_price,
16:22:00 140  	    in_taxes_amount	=> NULL
16:22:00 141  	  );
16:22:00 142  
16:22:00 143  	  FOR f_discount IN (
16:22:00 144  	    SELECT
16:22:00 145  	      DISCOUNT.ID
16:22:00 146  	    FROM
16:22:00 147  	      DISCOUNT_PRODUCT_OFFERING
16:22:00 148  	      INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
16:22:00 149  	    WHERE
16:22:00 150  	      DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
16:22:00 151  	  )
16:22:00 152  	  LOOP
16:22:00 153  	    PROCS_LINE_ITEMS_CRU_V22.CREATE_DISCOUNT_LINE_ITEM(
16:22:00 154  	      in_discount_id =>  f_discount.ID,
16:22:00 155  	      in_line_item_id => var_new_line_item_id
16:22:00 156  	    );
16:22:00 157  	  END LOOP;
16:22:00 158  	END LOOP;
16:22:00 159  
16:22:00 160  EXCEPTION
16:22:00 161  WHEN BAD_INVOICE_ID THEN
16:22:00 162  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:00 163  	  SPROC_NAME, 'No such license');
16:22:00 164  WHEN BAD_OFFER_ID THEN
16:22:00 165  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:00 166  	  SPROC_NAME, 'No such offer');
16:22:00 167  WHEN BAD_DISCOUNT THEN
16:22:00 168  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:00 169  	  SPROC_NAME, 'Bad Discount');
16:22:00 170  WHEN OTHERS THEN
16:22:00 171  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:00 172  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:22:00 173  END ADD_LINE_ITEMS;
