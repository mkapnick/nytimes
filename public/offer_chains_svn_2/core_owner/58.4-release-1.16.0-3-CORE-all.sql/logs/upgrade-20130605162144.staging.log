16:22:02 372  range_diff NUMBER := 0;
16:22:02 373  upper_bond_diff NUMBER := 0;
16:22:02 374  l_start NUMBER := 0;
16:22:02 375  l_end   NUMBER := 0;
16:22:02 376  BEGIN
16:22:02 377  	-- Normalize the end points [START]
16:22:02 378  	IF (in_start IS NULL OR in_start < 0) Then
16:22:02 379  	  l_start := 0;
