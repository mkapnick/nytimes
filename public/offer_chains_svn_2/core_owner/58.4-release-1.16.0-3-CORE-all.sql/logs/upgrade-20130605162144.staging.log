16:22:02 437  PROCEDURE GET_TRANSACTION_INVOICE (
16:22:02 438  /*
16:22:02 439  Throws exceptions:
16:22:02 440  APP_EXCEPTION_CODES_V22.NOT_FOUND
16:22:02 441  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
16:22:02 442  */
16:22:02 443  	in_transaction_id  IN  NUMBER,
16:22:02 444  	out_result_set	      OUT SYS_REFCURSOR
16:22:02 445  ) AS
16:22:02 446  SPROC_NAME CONSTANT  VARCHAR2(23) := 'GET_TRANSACTION_INVOICE';
16:22:02 447  -- VARIABLES
16:22:02 448  temp_transaction_id  NUMBER;
16:22:02 449  var_invoice_id	   NUMBER;
16:22:02 450  var_subscription_id  NUMBER;
16:22:02 451  var_offer_chain_id   NUMBER;
16:22:02 452  var_offer_chain_name VARCHAR2(255);
16:22:02 453  -- EXCEPTIONS
16:22:02 454  BAD_TRANSACTION_ID     EXCEPTION;
16:22:02 455  EXCEPTION_MESSAGE      VARCHAR2(1024);
16:22:02 456  BEGIN
16:22:02 457   -- check if there is the transaction
16:22:02 458  	BEGIN
16:22:02 459  	  SELECT
16:22:02 460  	    TRANSACTION.ID into temp_transaction_id
16:22:02 461  	  FROM
16:22:02 462  	    TRANSACTION
16:22:02 463  	  WHERE
16:22:02 464  	    TRANSACTION.ID = in_transaction_id;
16:22:02 465  	  EXCEPTION
16:22:02 466  	    WHEN NO_DATA_FOUND THEN
16:22:02 467  	      RAISE BAD_TRANSACTION_ID;
16:22:02 468  	END;
16:22:02 469  
16:22:02 470  	-- Get invoice id
16:22:02 471  	SELECT DISTINCT
16:22:02 472  	  CHARGE.INVOICE_ID into var_invoice_id
16:22:02 473  	FROM
16:22:02 474  	  CHARGE
16:22:02 475  	WHERE
16:22:02 476  	  CHARGE.TRANSACTION_ID = in_transaction_id;
16:22:02 477  
16:22:02 478  	-- Get subscription id if exists
16:22:02 479  	BEGIN
16:22:02 480  	  SELECT DISTINCT
16:22:02 481  	    LICENSE.SUBSCRIPTION_ID into var_subscription_id
16:22:02 482  	  FROM
16:22:02 483  	    LICENSE
16:22:02 484  	  WHERE
16:22:02 485  	    LICENSE.INVOICE_ID = var_invoice_id;
16:22:02 486  	  EXCEPTION
16:22:02 487  	    WHEN NO_DATA_FOUND THEN
16:22:02 488  	      var_subscription_id := NULL;
16:22:02 489  	END;
16:22:02 490  
16:22:02 491  	IF var_subscription_id IS NOT NULL THEN
16:22:02 492  	  -- Fetch offer chain from subscription
16:22:02 493  	  SELECT
16:22:02 494  	    OFFER_CHAIN.ID,
16:22:02 495  	    OFFER_CHAIN.NAME
16:22:02 496  	    into
16:22:02 497  	    var_offer_chain_id,
16:22:02 498  	    var_offer_chain_name
16:22:02 499  	  FROM
16:22:02 500  	    OFFER_CHAIN
16:22:02 501  	    INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
16:22:02 502  	  WHERE
16:22:02 503  	    SUBSCRIPTION.ID = var_subscription_id;
16:22:02 504  	ELSE
16:22:02 505  	  -- Fetch offer chain from GC
16:22:02 506  	  SELECT
16:22:02 507  	    OFFER_CHAIN.ID,
16:22:02 508  	    OFFER_CHAIN.NAME
16:22:02 509  	    into
16:22:02 510  	    var_offer_chain_id,
16:22:02 511  	    var_offer_chain_name
16:22:02 512  	  FROM
16:22:02 513  	    OFFER_CHAIN
16:22:02 514  	    INNER JOIN GIFT_CERTIFICATE ON GIFT_CERTIFICATE.OFFER_CHAIN_ID = OFFER_CHAIN.ID
16:22:02 515  	  WHERE
16:22:02 516  	    GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = var_invoice_id;
16:22:02 517  	END IF;
16:22:02 518  
16:22:02 519  	OPEN out_result_set FOR
16:22:02 520  	SELECT DISTINCT
16:22:02 521  	  var_invoice_id       AS "INVOICE_ID",
16:22:02 522  	  var_subscription_id  AS "SUBSCRIPTION_ID",
16:22:02 523  	  var_offer_chain_id   AS "OFFER_CHAIN_ID",
16:22:02 524  	  var_offer_chain_name AS "OFFER_CHAIN_NAME"
16:22:02 525  	FROM
16:22:02 526  	  DUAL;
16:22:02 527  
16:22:02 528  EXCEPTION
16:22:02 529  WHEN BAD_TRANSACTION_ID THEN
16:22:02 530  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:02 531  	  SPROC_NAME, 'No such transaction');
16:22:02 532  WHEN OTHERS THEN
16:22:02 533  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:02 534  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:22:02 535  END GET_TRANSACTION_INVOICE;
