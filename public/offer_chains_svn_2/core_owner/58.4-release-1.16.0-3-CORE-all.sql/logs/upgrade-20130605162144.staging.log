16:22:01 659  
16:22:01 660  PROCEDURE GET_SUBSCRIPTION_INFO (
16:22:01 661  /*
16:22:01 662  Throws exceptions:
16:22:01 663  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
16:22:01 664  */
16:22:01 665  	  in_subscription_id  IN  NUMBER,
16:22:01 666  	  out_result_set      OUT SYS_REFCURSOR
16:22:01 667  ) AS
16:22:01 668  SPROC_NAME CONSTANT VARCHAR2(21) := 'GET_SUBSCRIPTION_INFO';
16:22:01 669  -- VARIABLES
16:22:01 670  temp_subscription_id NUMBER;
16:22:01 671  -- EXCEPTIONS
16:22:01 672  BAD_SUBSCRIPTION_ID EXCEPTION;
16:22:01 673  BEGIN
16:22:01 674  	-- Find subscription by id
16:22:01 675  	-- Return its details
16:22:01 676  
16:22:01 677  	BEGIN
16:22:01 678  	  SELECT
16:22:01 679  	    SUBSCRIPTION.ID into temp_subscription_id
16:22:01 680  	  FROM
16:22:01 681  	    SUBSCRIPTION
16:22:01 682  	  WHERE
16:22:01 683  	    SUBSCRIPTION.ID = in_subscription_id;
16:22:01 684  	  EXCEPTION
16:22:01 685  	    WHEN NO_DATA_FOUND THEN
16:22:01 686  	      RAISE BAD_SUBSCRIPTION_ID;
16:22:01 687  	END;
16:22:01 688  
16:22:01 689  	OPEN out_result_set FOR
16:22:01 690  	SELECT
16:22:01 691  	  SUBSCRIPTION.ID AS "SUBSCRIPTION_ID",
16:22:01 692  	  SUBSCRIPTION.SUBSCRIPTION_STATUS_ID,
16:22:01 693  	  SUBSCRIPTION.PURCHASE_DATE,
16:22:01 694  	  SUBSCRIPTION.SUSPEND_DATE,
16:22:01 695  	  SUBSCRIPTION.REACTIVATION_DATE,
16:22:01 696  	  SUBSCRIPTION.CANCELLATION_DATE,
16:22:01 697  	  SUBSCRIPTION_CANCEL_REASON.VALUE as "CANCEL_TYPE",
16:22:01 698  	  OFFER_CHAIN.ID AS "OFFER_CHAIN_ID",
16:22:01 699  	  OFFER_CHAIN.NAME,
16:22:01 700  	  OFFER_CHAIN.DESCRIPTION,
16:22:01 701  	  OFFER_CHAIN.PRODUCT_URI,
16:22:01 702  	  SUBSCRIPTION.INSTRUMENT_ID,
16:22:01 703  	  SUBSCRIPTION.INSTRUMENT_TYPE_ID,
16:22:01 704  	  PROCS_SUBSCRIPTION_V22.CALC_SUBSCRIPTION_END_DATE(SUBSCRIPTION.ID) as "END_DATE",
16:22:01 705  	  PROCS_SUBSCRIPTION_V22.GET_RECENT_CHARGE(SUBSCRIPTION.ID) AS "RECENT_CHARGE",
16:22:01 706  	  PROCS_SUBSCRIPTION_V22.GET_RENEWAL_DATE(SUBSCRIPTION.ID) AS "RENEWAL_DATE",
16:22:01 707  	  PROCS_SUBSCRIPTION_V22.GET_BILLING_CYCLE(SUBSCRIPTION.ID) AS "BILLING_CYCLE",
16:22:01 708  	  (
16:22:01 709  	    SELECT
16:22:01 710  	      ACCOUNT.GROUP_ID
16:22:01 711  	      FROM ACCOUNT
16:22:01 712  	      WHERE ACCOUNT.ID = SUBSCRIPTION.ACCOUNT_ID
16:22:01 713  	  ) as "GROUP_ID",
16:22:01 714  	  (
16:22:01 715  	    SELECT
16:22:01 716  	      MAX(ENTITLEMENT_END_DATE)
16:22:01 717  	      FROM LICENSE
16:22:01 718  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
16:22:01 719  	  )
16:22:01 720  	  as "ENT_END_DATE",
16:22:01 721  	  (
16:22:01 722  	    SELECT
16:22:01 723  	      MIN(START_DATE)
16:22:01 724  	      FROM LICENSE
16:22:01 725  	      WHERE LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
16:22:01 726  	  )
16:22:01 727  	  as "ENT_START_DATE",
16:22:01 728  	  PROCS_SUBSCRIPTION_V22.IS_SUBSCRIPTION_CANCELABLE(SUBSCRIPTION.ID) AS "IS_CANCELABLE",
16:22:01 729  	  ITUNES_RECEIPT.ID AS "ITUNES_RECEIPT_ID",
16:22:01 730  	  GROUP_ACCOUNT.ID GA_ID,
16:22:01 731  	  GROUP_ACCOUNT.SUBSCRIPTION_ID GA_SUBSCRIPTION_ID,
16:22:01 732  	  GROUP_ACCOUNT.GROUP_NAME GA_GROUP_NAME,
16:22:01 733  	  GROUP_ACCOUNT.FIRST_NAME GA_FIRST_NAME,
16:22:01 734  	  GROUP_ACCOUNT.LAST_NAME GA_LAST_NAME,
16:22:01 735  	  GROUP_ACCOUNT.EMAIL GA_EMAIL,
16:22:01 736  	  GROUP_ACCOUNT.PHONE GA_PHONE,
16:22:01 737  	  GROUP_ACCOUNT.ORGANIZATION_TYPE GA_ORGANIZATION_TYPE,
16:22:01 738  	  GROUP_ACCOUNT.SEATS GA_SEATS,
16:22:01 739  	  PROCS_GROUP_ACCOUNT_V22.F_GET_NUM_OCCUPIED_GROUP_SEATS(GROUP_ACCOUNT.ID) GA_SEATS_USED,
16:22:01 740  	  GROUP_ACCOUNT.IP GA_IP,
16:22:01 741  	  PROCS_SUBSCRIPTION_V22.GET_GIFT_CERT_CODE_BY_SUB_ID(SUBSCRIPTION.ID) gift_certificate_code,
16:22:01 742  	  PROCS_ACCOUNT_V22.GET_GRACE_START_DATE(SUBSCRIPTION.ID) GRACE_START_DATE,
16:22:01 743  	  PROCS_ACCOUNT_V22.GET_GRACE_END_DATE(SUBSCRIPTION.ID) GRACE_END_DATE
16:22:01 744  	FROM
16:22:01 745  	  SUBSCRIPTION
16:22:01 746  	  INNER JOIN OFFER_CHAIN ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
16:22:01 747  	  LEFT JOIN SUBSCRIPTION_CANCEL_REASON ON SUBSCRIPTION.SCT_ID = SUBSCRIPTION_CANCEL_REASON.ID
16:22:01 748  	  LEFT JOIN ITUNES_RECEIPT ON SUBSCRIPTION.ID = ITUNES_RECEIPT.SUBSCRIPTION_ID
16:22:01 749  	  LEFT JOIN GROUP_ACCOUNT ON SUBSCRIPTION.ID = GROUP_ACCOUNT.SUBSCRIPTION_ID
16:22:01 750  	WHERE
16:22:01 751  	  SUBSCRIPTION.ID = in_subscription_id;
16:22:01 752  
16:22:01 753  EXCEPTION
16:22:01 754  WHEN BAD_SUBSCRIPTION_ID THEN
16:22:01 755  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 756  	  SPROC_NAME, 'No such subscription');
16:22:01 757  WHEN OTHERS THEN
16:22:01 758  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:01 759  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:22:01 760  END GET_SUBSCRIPTION_INFO;
16:22:01 761  
16:22:01 762  /******************************************************************************/
16:22:01 763  
16:22:01 764  PROCEDURE GET_SUBSCRIPTION_INVOICES (
16:22:01 765  /*
16:22:01 766  Throws exceptions:
16:22:01 767  APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 768  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
16:22:01 769  */
16:22:01 770  	in_subscription_id  IN	NUMBER,
16:22:01 771  	out_result_set	    OUT SYS_REFCURSOR
16:22:01 772  ) AS
16:22:01 773  SPROC_NAME	   CONSTANT VARCHAR2(25) := 'GET_SUBSCRIPTION_INVOICES';
16:22:01 774  temp_subscription_id NUMBER;
16:22:01 775  BEGIN
16:22:01 776  	-- Note: A subscription has one or more associated licenses, each of which has an associated invoice.
16:22:01 777  	-- Find associated LICENSES for the subscription by "LICENSE.subscription_id"
16:22:01 778  	--   for each license
16:22:01 779  	--     get associated invoice
16:22:01 780  	--     add to results list
16:22:01 781  	--   end loop
16:22:01 782  	-- end
16:22:01 783  
16:22:01 784  	SELECT
16:22:01 785  	  SUBSCRIPTION.ID into temp_subscription_id
16:22:01 786  	FROM
16:22:01 787  	  SUBSCRIPTION
16:22:01 788  	WHERE
16:22:01 789  	  SUBSCRIPTION.ID = in_subscription_id;
16:22:01 790  
16:22:01 791  	OPEN out_result_set FOR
16:22:01 792  	SELECT
16:22:01 793  	  INVOICE.ID,
16:22:01 794  	  INVOICE.INVOICE_STATUS_ID,
16:22:01 795  	  INVOICE.CREATE_DATE,
16:22:01 796  	  INVOICE.CREATED_BY,
16:22:01 797  	  INVOICE.UPDATE_DATE,
16:22:01 798  	  INVOICE.UPDATED_BY,
16:22:01 799  	  INVOICE.TAX_EXEMPT_ID
16:22:01 800  	FROM
16:22:01 801  	  LICENSE
16:22:01 802  	    INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
16:22:01 803  	WHERE
16:22:01 804  	  LICENSE.SUBSCRIPTION_ID = in_subscription_id;
16:22:01 805  
16:22:01 806  EXCEPTION
16:22:01 807  WHEN NO_DATA_FOUND THEN
16:22:01 808  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 809  	  SPROC_NAME, 'Could not find subscription with given ID');
16:22:01 810  WHEN OTHERS THEN
16:22:01 811  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:01 812  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:22:01 813  END GET_SUBSCRIPTION_INVOICES;
16:22:01 814  
16:22:01 815  /******************************************************************************/
16:22:01 816  
16:22:01 817  PROCEDURE GET_SUBSCRIPTION_NOTES (
16:22:01 818  /*
16:22:01 819  Throws exceptions:
16:22:01 820  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
16:22:01 821  */
16:22:01 822  	in_subscription_id  IN	NUMBER,
16:22:01 823  	out_result_set	    OUT SYS_REFCURSOR
16:22:01 824  ) AS
16:22:01 825  -- VARIABLES
16:22:01 826  SPROC_NAME	   CONSTANT VARCHAR2(22) := 'GET_SUBSCRIPTION_NOTES';
16:22:01 827  temp_subscription_id NUMBER;
16:22:01 828  -- EXCEPTIONS
16:22:01 829  BAD_SUBSCRIPTION_ID EXCEPTION;
16:22:01 830  BEGIN
16:22:01 831  
