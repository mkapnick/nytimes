16:22:02 1450  PROCEDURE IS_INVOICE_TAX_EXEMPT (
16:22:02 1451  /*
16:22:02 1452  Throws exceptions:
16:22:02 1453  APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:02 1454  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
16:22:02 1455  Return:
16:22:02 1456  	 GLOBAL_CONSTANTS_V22.TRUE if ACCOUNT.EXEMPT_ID is not null
16:22:02 1457  	 GLOBAL_CONSTANTS_V22.FALSE else
16:22:02 1458  */
16:22:02 1459  	 in_invoice_id	   IN NUMBER,
16:22:02 1460  	 out_is_tax_exempt OUT NUMBER
16:22:02 1461  ) AS
16:22:02 1462  SPROC_NAME CONSTANT VARCHAR2(21) := 'IS_INVOICE_TAX_EXEMPT';
16:22:02 1463  -- VARIABLES
16:22:02 1464  var_is_tax_exempt INVOICE.TAX_EXEMPT_ID%TYPE;
16:22:02 1465  -- EXCEPTIONS
16:22:02 1466  BAD_INVOICE_ID EXCEPTION;
16:22:02 1467  BEGIN
16:22:02 1468  
16:22:02 1469  	 BEGIN
16:22:02 1470  	   SELECT
16:22:02 1471  	     INVOICE.TAX_EXEMPT_ID into var_is_tax_exempt
16:22:02 1472  	   FROM
16:22:02 1473  	     INVOICE
16:22:02 1474  	   WHERE
16:22:02 1475  	     INVOICE.ID = in_invoice_id;
16:22:02 1476  	   EXCEPTION
16:22:02 1477  	     WHEN NO_DATA_FOUND THEN
16:22:02 1478  	       RAISE BAD_INVOICE_ID;
16:22:02 1479  	 END;
16:22:02 1480  
16:22:02 1481  	 IF var_is_tax_exempt IS NULL THEN
16:22:02 1482  	   out_is_tax_exempt := GLOBAL_CONSTANTS_V22.FALSE;
16:22:02 1483  	 ELSE
16:22:02 1484  	   out_is_tax_exempt := GLOBAL_CONSTANTS_V22.TRUE;
16:22:02 1485  	 END IF;
16:22:02 1486  
16:22:02 1487  EXCEPTION
16:22:02 1488  WHEN BAD_INVOICE_ID THEN
16:22:02 1489  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:02 1490  	   SPROC_NAME, 'No such invoice');
16:22:02 1491  WHEN OTHERS THEN
16:22:02 1492  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:02 1493  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:22:02 1494  END IS_INVOICE_TAX_EXEMPT;
