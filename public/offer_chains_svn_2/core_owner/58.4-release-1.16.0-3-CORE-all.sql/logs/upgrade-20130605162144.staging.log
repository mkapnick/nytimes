16:22:00  88  	PROCEDURE CHASE_PROFILE_BY_REQ_FILE_ID(
16:22:00  89  	  in_request_file_id IN CC_UPDATE.CC_REQUEST_FILE_ID%TYPE,
16:22:00  90  	  in_start	     IN NUMBER,
16:22:00  91  	  in_end	     IN NUMBER,
16:22:00  92  	  out_result_set     OUT SYS_REFCURSOR
16:22:00  93  	) AS
16:22:00  94  	SPROC_NAME CONSTANT VARCHAR2(32) := 'CHASE_PROFILE_BY_REQ_FILE_ID';
16:22:00  95  	var_range_diff	    NUMBER := 0;
16:22:00  96  	var_upper_bond_diff NUMBER := 0;
16:22:00  97  	var_l_start	    NUMBER := 0;
16:22:00  98  	var_l_end	    NUMBER := 0;
16:22:00  99  	BEGIN
16:22:00 100  	  --Normalize the end points [START]
16:22:00 101  	  IF (in_start IS NULL OR in_start < 0) Then
16:22:00 102  	    var_l_start := 0;
16:22:00 103  	  ELSE
16:22:00 104  	    var_l_start := in_start;
16:22:00 105  	  END IF;
16:22:00 106  
16:22:00 107  	  IF (in_end IS NULL) Then
16:22:00 108  	    var_l_end := 500;
16:22:00 109  	  ELSE
16:22:00 110  	    var_l_end := in_end;
16:22:00 111  	  END IF;
16:22:00 112  
16:22:00 113  	  var_l_start := var_l_start + 1;
16:22:00 114  	  var_l_end   := var_l_end   + 1;
16:22:00 115  
16:22:00 116  	  var_range_diff  := var_l_end - var_l_start;
16:22:00 117  	  var_upper_bond_diff :=  var_range_diff - 1000;
16:22:00 118  
16:22:00 119  	  IF (var_upper_bond_diff > 0) Then
16:22:00 120  	    var_l_end := var_l_end - var_upper_bond_diff;
16:22:00 121  	  END IF;
16:22:00 122  	  --Normalize the end points [END]
16:22:00 123  
16:22:00 124  	  OPEN out_result_set FOR
16:22:00 125  	    SELECT CHASE_PROFILE_ID FROM
16:22:00 126  	      (SELECT rownum rnum, q.* FROM
16:22:00 127  		 (SELECT
16:22:00 128  		    cc.CHASE_PROFILE_ID
16:22:00 129  		  FROM
16:22:00 130  		    CREDIT_CARD cc,
16:22:00 131  		    CC_UPDATE ccu
16:22:00 132  		  WHERE
16:22:00 133  		    ccu.CC_REQUEST_FILE_ID = in_request_file_id
16:22:00 134  		    AND ccu.CREDIT_CARD_ID = cc.id
16:22:00 135  		) Q
16:22:00 136  	      WHERE rownum <= var_l_end)
16:22:00 137  	    WHERE rnum >= var_l_Start;
16:22:00 138  	END CHASE_PROFILE_BY_REQ_FILE_ID;
