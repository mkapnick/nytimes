16:22:02 697  /******************************************************************************/
16:22:02 698  
16:22:02 699  PROCEDURE CALCULATE_INVOICE_CHARGEBACK (
16:22:02 700  	in_invoice_id	      IN NUMBER,
16:22:02 701  	in_chargeback_date    IN DATE,
16:22:02 702  	out_chargeback_amount OUT NUMBER
16:22:02 703  ) AS
16:22:02 704  -- VARIABLES
16:22:02 705  SPROC_NAME	     CONSTANT VARCHAR2(28) := 'CALCULATE_INVOICE_CHARGEBACK';
16:22:02 706  var_chargeback_date    DATE;
16:22:02 707  temp_invoice_id	     NUMBER;
16:22:02 708  var_licenses_number    NUMBER;
16:22:02 709  var_invoice_start_date DATE;
16:22:02 710  var_invoice_end_date   DATE;
16:22:02 711  var_offer_id	     NUMBER;
16:22:02 712  var_offer_days_interval NUMBER;
16:22:02 713  var_license_days_used  NUMBER;
16:22:02 714  var_invoice_amount     NUMBER(10,2);
16:22:02 715  var_offer_chain_id     NUMBER;
16:22:02 716  var_offer_chain_meta_data_val VARCHAR2(1024);
16:22:02 717  var_offer_chain_full_refund   NUMBER;
16:22:02 718  var_offer_chain_prorated_ref  NUMBER;
16:22:02 719  var_chargeback_calculated NUMBER;
16:22:02 720  var_max_invoice_refund	NUMBER;
16:22:02 721  -- EXCEPTIONS
16:22:02 722  BAD_INVOICE_ID		  EXCEPTION;
16:22:02 723  NO_LICENSES_FOUND_EXCEPTION EXCEPTION;
16:22:02 724  OFFER_LENGTH_IS_ZERO	  EXCEPTION;
16:22:02 725  BEGIN
16:22:02 726  
16:22:02 727  	IF in_chargeback_date IS NULL THEN
16:22:02 728  	  var_chargeback_date := PROCS_COMMON_V22.NORMALIZE_DATE(current_date);
16:22:02 729  	ELSE
16:22:02 730  	  var_chargeback_date := PROCS_COMMON_V22.NORMALIZE_DATE(in_chargeback_date);
16:22:02 731  	END IF;
16:22:02 732  
16:22:02 733  	-- Check that invoice exists
16:22:02 734  	BEGIN
16:22:02 735  	  SELECT
16:22:02 736  	    INVOICE.ID into temp_invoice_id
16:22:02 737  	  FROM
16:22:02 738  	    INVOICE
16:22:02 739  	  WHERE
16:22:02 740  	    INVOICE.ID = in_invoice_id;
16:22:02 741  	  EXCEPTION
16:22:02 742  	    WHEN NO_DATA_FOUND THEN
16:22:02 743  	      RAISE BAD_INVOICE_ID;
16:22:02 744  	END;
16:22:02 745  
16:22:02 746  	-- Check that invoice has at least one license
16:22:02 747  	SELECT
16:22:02 748  	  COUNT(*) into var_licenses_number
16:22:02 749  	FROM
16:22:02 750  	  LICENSE
16:22:02 751  	WHERE
16:22:02 752  	  LICENSE.INVOICE_ID = in_invoice_id;
16:22:02 753  
16:22:02 754  	IF var_licenses_number = 0 THEN
16:22:02 755  	  RAISE NO_LICENSES_FOUND_EXCEPTION;
16:22:02 756  	END IF;
16:22:02 757  
16:22:02 758  	SELECT
16:22:02 759  	  MIN(LICENSE.START_DATE) into var_invoice_start_date
16:22:02 760  	FROM
16:22:02 761  	  LICENSE
16:22:02 762  	WHERE
16:22:02 763  	  LICENSE.INVOICE_ID = in_invoice_id;
16:22:02 764  
16:22:02 765  	var_invoice_start_date := PROCS_COMMON_V22.NORMALIZE_DATE(var_invoice_start_date);
16:22:02 766  
16:22:02 767  	SELECT DISTINCT
16:22:02 768  	  LICENSE.OFFER_ID into var_offer_id
16:22:02 769  	FROM
16:22:02 770  	  LICENSE
16:22:02 771  	WHERE
16:22:02 772  	  LICENSE.INVOICE_ID = in_invoice_id;
16:22:02 773  
16:22:02 774  	SELECT
16:22:02 775  	  MAX (LICENSE.END_DATE) into var_invoice_end_date
16:22:02 776  	FROM
16:22:02 777  	  LICENSE
16:22:02 778  	WHERE
16:22:02 779  	  LICENSE.INVOICE_ID = in_invoice_id;
16:22:02 780  
16:22:02 781  	-- All licenses for given invoice should point into the same offer
16:22:02 782  
16:22:02 783  	PROCS_OFFER_CHAIN_V22.GET_OFFER_LENGTH_IN_DAYS(
16:22:02 784  	  in_offer_id	=> var_offer_id,
16:22:02 785  	  in_start_date => var_invoice_start_date,
16:22:02 786  	  out_days	=> var_offer_days_interval
16:22:02 787  	);
16:22:02 788  
16:22:02 789  	IF var_offer_days_interval = 0 THEN
16:22:02 790  	  RAISE OFFER_LENGTH_IS_ZERO;
16:22:02 791  	END IF;
16:22:02 792  
16:22:02 793  	PROCS_INVOICE_V22.GET_INVOICE_DAYS_USED_NUMBER(
16:22:02 794  	  in_invoice_id      => in_invoice_id,
16:22:02 795  	  in_chargeback_date => var_chargeback_date,
16:22:02 796  	  out_days_num	     => var_license_days_used
16:22:02 797  	);
16:22:02 798  
16:22:02 799  	PROCS_INVOICE_V22.CALCULATE_INVOICE_AMOUNT(
16:22:02 800  	  in_invoice_id => in_invoice_id,
16:22:02 801  	  out_amount	=> var_invoice_amount
16:22:02 802  	);
16:22:02 803  
16:22:02 804  	-- 39437
16:22:02 805  	-- Get offer chain id by invoice id
16:22:02 806  	SELECT DISTINCT
16:22:02 807  	  SUBSCRIPTION.OFFER_CHAIN_ID into var_offer_chain_id
16:22:02 808  	FROM
16:22:02 809  	  SUBSCRIPTION
16:22:02 810  	  INNER JOIN LICENSE ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
16:22:02 811  	WHERE
16:22:02 812  	  LICENSE.INVOICE_ID = in_invoice_id;
16:22:02 813  
16:22:02 814  	-- Get offer chain's meta data full amount value
16:22:02 815  	PROCS_OFFER_CHAIN_V22.GET_OFFER_CHAIN_MD_VALUE(
16:22:02 816  	  in_offer_chain_id => var_offer_chain_id,
16:22:02 817  	  in_meta_data_name => GLOBAL_CONSTANTS_V22.OFFER_CHAIN_FULL_REFUND,
16:22:02 818  	  out_value	    => var_offer_chain_meta_data_val
16:22:02 819  	);
16:22:02 820  	IF var_offer_chain_meta_data_val IS NULL THEN
16:22:02 821  	  var_offer_chain_full_refund := NULL;
16:22:02 822  	ELSE
16:22:02 823  	  var_offer_chain_full_refund := TO_NUMBER(var_offer_chain_meta_data_val);
16:22:02 824  	END IF;
16:22:02 825  
16:22:02 826  	-- Get offer chain's meta data prorated amount value
16:22:02 827  	PROCS_OFFER_CHAIN_V22.GET_OFFER_CHAIN_MD_VALUE(
16:22:02 828  	  in_offer_chain_id => var_offer_chain_id,
16:22:02 829  	  in_meta_data_name => GLOBAL_CONSTANTS_V22.OFFER_CHAIN_PRORATED_REFUND,
16:22:02 830  	  out_value	    => var_offer_chain_meta_data_val
16:22:02 831  	);
16:22:02 832  	IF var_offer_chain_meta_data_val IS NULL THEN
16:22:02 833  	  var_offer_chain_prorated_ref := NULL;
16:22:02 834  	ELSE
16:22:02 835  	  var_offer_chain_prorated_ref := TO_NUMBER(var_offer_chain_meta_data_val);
16:22:02 836  	END IF;
16:22:02 837  
16:22:02 838  	var_chargeback_calculated := GLOBAL_CONSTANTS_V22.FALSE;
16:22:02 839  
16:22:02 840  	IF var_offer_chain_full_refund IS NOT NULL
16:22:02 841  	   AND var_chargeback_calculated = GLOBAL_CONSTANTS_V22.FALSE THEN
16:22:02 842  	  IF var_license_days_used < var_offer_chain_full_refund THEN
16:22:02 843  	    out_chargeback_amount := var_invoice_amount;
16:22:02 844  	    var_chargeback_calculated := GLOBAL_CONSTANTS_V22.TRUE;
16:22:02 845  	  END IF;
16:22:02 846  	END IF;
16:22:02 847  
16:22:02 848  	IF var_offer_chain_prorated_ref IS NOT NULL
16:22:02 849  	   AND var_chargeback_calculated = GLOBAL_CONSTANTS_V22.FALSE THEN
16:22:02 850  	  IF var_license_days_used < var_offer_chain_prorated_ref THEN
16:22:02 851  	    out_chargeback_amount := ( var_invoice_amount * (var_offer_days_interval - var_license_days_used) ) / var_offer_days_interval;
16:22:02 852  	    var_chargeback_calculated := GLOBAL_CONSTANTS_V22.TRUE;
16:22:02 853  	  END IF;
16:22:02 854  	END IF;
16:22:02 855  
16:22:02 856  	IF var_chargeback_calculated = GLOBAL_CONSTANTS_V22.TRUE THEN
16:22:02 857  	  PROCS_INVOICE_V22.GET_MAX_REFUND(
16:22:02 858  	    in_invoice_id => in_invoice_id,
16:22:02 859  	    out_amount	  => var_max_invoice_refund
16:22:02 860  	  );
16:22:02 861  	  IF var_max_invoice_refund < out_chargeback_amount THEN
16:22:02 862  	    out_chargeback_amount := var_max_invoice_refund;
16:22:02 863  	  END IF;
16:22:02 864  	END IF;
16:22:02 865  
16:22:02 866  	IF var_chargeback_calculated = GLOBAL_CONSTANTS_V22.FALSE THEN
16:22:02 867  	  out_chargeback_amount := 0;
16:22:02 868  	END IF;
16:22:02 869  
16:22:02 870  EXCEPTION
16:22:02 871  WHEN BAD_INVOICE_ID THEN
16:22:02 872  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:02 873  	  SPROC_NAME, 'No such invoice');
16:22:02 874  WHEN NO_LICENSES_FOUND_EXCEPTION THEN
16:22:02 875  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:02 876  	  SPROC_NAME, 'No licenses found');
16:22:02 877  WHEN OFFER_LENGTH_IS_ZERO THEN
16:22:02 878  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:02 879  	  SPROC_NAME, 'Offer length is zero');
16:22:02 880  WHEN OTHERS THEN
16:22:02 881  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:02 882  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:22:02 883  END CALCULATE_INVOICE_CHARGEBACK;
16:22:02 884  
16:22:02 885  /********************************************************************/
16:22:02 886  
16:22:02 887  PROCEDURE APPLY_REFUND (
16:22:02 888  	in_invoice_id	     IN NUMBER,
16:22:02 889  	in_chargeback_amount IN NUMBER,
16:22:02 890  	in_created_by	     IN VARCHAR2,
16:22:02 891  	out_charge_id	     OUT NUMBER
16:22:02 892  ) AS
16:22:02 893  -- VARIABLES
16:22:02 894  SPROC_NAME		 CONSTANT VARCHAR2(16) := 'APPLY_CHARGEBACK';
16:22:02 895  temp_invoice_id		 NUMBER;
16:22:02 896  var_total_charges_amount	 NUMBER(10,2);
16:22:02 897  var_charge_amount_to_apply NUMBER(10,2);
16:22:02 898  var_transaction_id	 NUMBER;
16:22:02 899  var_instrument_type_id	 NUMBER;
16:22:02 900  var_instrument_id 	 NUMBER;
16:22:02 901  var_charge_id		 NUMBER;
16:22:02 902  -- EXCEPTIONS
16:22:02 903  BAD_INVOICE_ID		 EXCEPTION;
16:22:02 904  CAN_NOT_CREATE_TRANSACTION EXCEPTION;
16:22:02 905  CAN_NOT_FIND_INSTRUMENT	 EXCEPTION;
16:22:02 906  CAN_NOT_CREATE_CHARGE	 EXCEPTION;
16:22:02 907  EXCEPTION_MESSAGE 	 VARCHAR2(1024);
16:22:02 908  BEGIN
16:22:02 909  
16:22:02 910  	BEGIN
16:22:02 911  	  SELECT
16:22:02 912  	    INVOICE.ID into temp_invoice_id
16:22:02 913  	  FROM
16:22:02 914  	    INVOICE
16:22:02 915  	  WHERE
16:22:02 916  	    INVOICE.ID = in_invoice_id;
16:22:02 917  	  EXCEPTION
16:22:02 918  	    WHEN NO_DATA_FOUND THEN
16:22:02 919  	      RAISE BAD_INVOICE_ID;
16:22:02 920  	END;
16:22:02 921  
16:22:02 922  	SELECT
16:22:02 923  	  SUM (CHARGE.CHARGE_AMOUNT) into var_total_charges_amount
16:22:02 924  	FROM
16:22:02 925  	  CHARGE
16:22:02 926  	WHERE
16:22:02 927  	  CHARGE.INVOICE_ID = in_invoice_id
16:22:02 928  	  AND CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V22.CHARGE_PROCESSED
16:22:02 929  	  AND CHARGE.INSTRUMENT_TYPE_ID != GLOBAL_ENUMS_V22.INSTRUMENT_GIFT_CERTIFICATE;
16:22:02 930  
16:22:02 931  	-- FIXME: Maybe whe should throw exception here?
16:22:02 932  	IF var_total_charges_amount < in_chargeback_amount THEN
16:22:02 933  	  var_charge_amount_to_apply := var_total_charges_amount;
16:22:02 934  	ELSE
16:22:02 935  	  var_charge_amount_to_apply := in_chargeback_amount;
16:22:02 936  	END IF;
16:22:02 937  
16:22:02 938  	BEGIN
16:22:02 939  	  PROCS_TRANSACTION_V22.CREATE_TRANSACTION(
16:22:02 940  	    in_transaction_id	      => NULL,
16:22:02 941  	    in_status_id	      => GLOBAL_STATUSES_V22.TRANSACTION_PREPARE,
16:22:02 942  	    in_amount		      => -var_charge_amount_to_apply,
16:22:02 943  	    in_created_by	      => in_created_by,
16:22:02 944  	    in_order_id 	      => NULL,
16:22:02 945  	    in_is_refund	      => GLOBAL_CONSTANTS_V22.TRUE,
16:22:02 946  	    in_transaction_type_code  => 'REFUND',
16:22:02 947  	    out_transaction_id	      => var_transaction_id
16:22:02 948  	  );
16:22:02 949  	  EXCEPTION
16:22:02 950  	    WHEN OTHERS THEN
16:22:02 951  	      EXCEPTION_MESSAGE := SQLERRM;
16:22:02 952  	      RAISE CAN_NOT_CREATE_TRANSACTION;
16:22:02 953  	END;
16:22:02 954  
16:22:02 955  	BEGIN
16:22:02 956  	  SELECT
16:22:02 957  	    C.INSTRUMENT_TYPE_ID,
16:22:02 958  	    C.INSTRUMENT_ID
16:22:02 959  	    into
16:22:02 960  	    var_instrument_type_id,
16:22:02 961  	    var_instrument_id
16:22:02 962  	  FROM
16:22:02 963  	    CHARGE C,
16:22:02 964  	    TRANSACTION_ATTEMPT TA,
16:22:02 965  	    TRANSACTION T
16:22:02 966  	  WHERE
16:22:02 967  	    C.INVOICE_ID = in_invoice_id and
16:22:02 968  	    C.TRANSACTION_ID = T.ID and
16:22:02 969  	    TA.TRANSACTION_ID = T.ID and
16:22:02 970  	    TA.TRANSACTION_ATTEMPT_STATUS_ID = GLOBAL_STATUSES_V22.TRANS_ATTEMPT_SUCCESS and
16:22:02 971  	    T.IS_REFUND = GLOBAL_CONSTANTS_V22.FALSE and
16:22:02 972  	    T.TRANSACTION_AMOUNT >= 0 and
16:22:02 973  	    rownum < 2;
16:22:02 974  	EXCEPTION
16:22:02 975  	    WHEN NO_DATA_FOUND THEN
16:22:02 976  	      RAISE CAN_NOT_FIND_INSTRUMENT;
16:22:02 977  	END;
16:22:02 978  	BEGIN
16:22:02 979  	  PROCS_CHARGE_V22.CREATE_CHARGE(
16:22:02 980  	    in_invoice_id	  => in_invoice_id,
16:22:02 981  	    in_transaction_id	  => var_transaction_id,
16:22:02 982  	    in_instrument_type_id => var_instrument_type_id,
16:22:02 983  	    in_instrument_id	  => var_instrument_id,
16:22:02 984  	    in_charge_amount	  => -var_charge_amount_to_apply,
16:22:02 985  	    in_created_by	  => in_created_by,
16:22:02 986  	    in_charge_status_id   => GLOBAL_STATUSES_V22.CHARGE_OPENED,
16:22:02 987  	    out_charge_id	  => var_charge_id
16:22:02 988  	  );
16:22:02 989  	  EXCEPTION
16:22:02 990  	    WHEN OTHERS THEN
16:22:02 991  	      EXCEPTION_MESSAGE := SQLERRM;
16:22:02 992  	      RAISE CAN_NOT_CREATE_CHARGE;
16:22:02 993  	END;
16:22:02 994  
16:22:02 995  	out_charge_id := var_charge_id;
16:22:02 996  
16:22:02 997  EXCEPTION
16:22:02 998  WHEN BAD_INVOICE_ID THEN
16:22:02 999  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:02 1000  	   SPROC_NAME, 'No such invoice');
16:22:02 1001  WHEN CAN_NOT_CREATE_TRANSACTION THEN
16:22:02 1002  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:02 1003  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
16:22:02 1004  WHEN CAN_NOT_FIND_INSTRUMENT THEN
16:22:02 1005  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:02 1006  	   SPROC_NAME, 'Could not find financial instrument');
16:22:02 1007  WHEN CAN_NOT_CREATE_CHARGE THEN
16:22:02 1008  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:02 1009  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
16:22:02 1010  WHEN OTHERS THEN
16:22:02 1011  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:02 1012  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:22:02 1013  END APPLY_REFUND;
16:22:02 1014  
16:22:02 1015  /******************************************************************************/
16:22:02 1016  
16:22:02 1017  PROCEDURE GET_MAX_REFUND (
16:22:02 1018  	 in_invoice_id IN NUMBER,
16:22:02 1019  	 out_amount    OUT NUMBER
16:22:02 1020  ) AS
16:22:02 1021  SPROC_NAME CONSTANT VARCHAR2(14) := 'GET_MAX_REFUND';
16:22:02 1022  -- VARIABLES
16:22:02 1023  temp_invoice_id	       NUMBER;
16:22:02 1024  var_invoice_refunds_sum NUMBER(10,2);
16:22:02 1025  var_invoice_charges_sum NUMBER(10,2);
16:22:02 1026  -- EXCEPTIONS
16:22:02 1027  BAD_INVOICE_ID EXCEPTION;
16:22:02 1028  BEGIN
16:22:02 1029  
16:22:02 1030  	 BEGIN
16:22:02 1031  	   SELECT
16:22:02 1032  	     INVOICE.ID into temp_invoice_id
16:22:02 1033  	   FROM
16:22:02 1034  	     INVOICE
16:22:02 1035  	   WHERE
16:22:02 1036  	     INVOICE.ID = in_invoice_id;
16:22:02 1037  	   EXCEPTION
16:22:02 1038  	     WHEN NO_DATA_FOUND THEN
16:22:02 1039  	       RAISE BAD_INVOICE_ID;
16:22:02 1040  	 END;
16:22:02 1041  
16:22:02 1042  	 var_invoice_refunds_sum := 0;
16:22:02 1043  	 var_invoice_charges_sum := 0;
16:22:02 1044  
16:22:02 1045  	 FOR f_charge IN (
16:22:02 1046  	   SELECT
16:22:02 1047  	     CHARGE.ID,
16:22:02 1048  	     CHARGE.CHARGE_STATUS_ID,
16:22:02 1049  	     CHARGE.CHARGE_AMOUNT,
16:22:02 1050  	     CHARGE.TRANSACTION_ID
16:22:02 1051  	   FROM
16:22:02 1052  	     CHARGE
16:22:02 1053  	   WHERE
16:22:02 1054  	     CHARGE.INVOICE_ID = in_invoice_id
16:22:02 1055  	 )
16:22:02 1056  	 LOOP
16:22:02 1057  	   -- If charge.status = canceled then continue
16:22:02 1058  	   IF f_charge.CHARGE_STATUS_ID != GLOBAL_STATUSES_V22.CHARGE_CANCELED THEN
16:22:02 1059  
16:22:02 1060  	     IF f_charge.CHARGE_AMOUNT > 0 THEN
16:22:02 1061  	       IF f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V22.CHARGE_PROCESSED
16:22:02 1062  		  AND PROCS_TRANSACTION_V22.IS_TRANSACTION_COLLECTED(f_charge.TRANSACTION_ID) = GLOBAL_CONSTANTS_V22.TRUE THEN
16:22:02 1063  		 -- Transaction collected
16:22:02 1064  		 var_invoice_charges_sum := var_invoice_charges_sum + f_charge.CHARGE_AMOUNT;
16:22:02 1065  	       ELSE
16:22:02 1066  		 -- Transaction is not collected. Do nothing
16:22:02 1067  		 NULL;
16:22:02 1068  	       END IF;
16:22:02 1069  	     ELSE
16:22:02 1070  	       IF f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V22.CHARGE_PROCESSED
16:22:02 1071  		  AND PROCS_TRANSACTION_V22.IS_TRANSACTION_COLLECTED(f_charge.TRANSACTION_ID) = GLOBAL_CONSTANTS_V22.FALSE THEN
16:22:02 1072  		 -- If charge is processed transaction is not collected then do nothing
16:22:02 1073  		 NULL;
16:22:02 1074  	       ELSE
16:22:02 1075  		 var_invoice_refunds_sum := var_invoice_refunds_sum + f_charge.CHARGE_AMOUNT;
16:22:02 1076  	       END IF;
16:22:02 1077  	     END IF;
16:22:02 1078  
16:22:02 1079  	   END IF;
16:22:02 1080  	 END LOOP;
16:22:02 1081  
16:22:02 1082  	 -- Refunds are negative
16:22:02 1083  	 var_invoice_refunds_sum := 0 - var_invoice_refunds_sum;
16:22:02 1084  
16:22:02 1085  	 out_amount := var_invoice_charges_sum - var_invoice_refunds_sum;
16:22:02 1086  
16:22:02 1087  EXCEPTION
16:22:02 1088  WHEN BAD_INVOICE_ID THEN
16:22:02 1089  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:02 1090  	   SPROC_NAME, 'No such invoice');
16:22:02 1091  WHEN OTHERS THEN
16:22:02 1092  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:02 1093  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:22:02 1094  END GET_MAX_REFUND;
