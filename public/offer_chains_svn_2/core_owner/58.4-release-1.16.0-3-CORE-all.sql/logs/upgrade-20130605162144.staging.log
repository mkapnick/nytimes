16:22:01 2454  
16:22:01 2455  PROCEDURE FINALIZE_FALSE_START (
16:22:01 2456  /*
16:22:01 2457  APP_EXCEPTION_CODES_V22.NOT_FOUND
16:22:01 2458  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
16:22:01 2459  APP_EXCEPTION_CODES_V22.INTERNAL_ERROR
16:22:01 2460  */
16:22:01 2461  --  in_subscription_id	 IN SUBSCRIPTION.ID%TYPE,
16:22:01 2462  --  in_cancelation_date	 IN SUBSCRIPTION.CANCELLATION_DATE%TYPE,
16:22:01 2463  --  in_note		 IN SUBSCRIPTION_NOTE.NOTE%TYPE,
16:22:01 2464  --  in_agent_id		 IN SUBSCRIPTION_NOTE.AGENT_ID%TYPE,
16:22:01 2465  --  in_updated_by	 IN SUBSCRIPTION.UPDATED_BY%TYPE
16:22:01 2466  	 in_subscription_id    IN NUMBER,
16:22:01 2467  	 in_cancelation_date   IN DATE,
16:22:01 2468  	 in_note	       IN VARCHAR2,
16:22:01 2469  	 in_agent_id	       IN NUMBER,
16:22:01 2470  	 in_updated_by	       IN VARCHAR2
16:22:01 2471  ) AS
16:22:01 2472  SPROC_NAME CONSTANT VARCHAR2(20) := 'FINALIZE_FALSE_START';
16:22:01 2473  FALSE_START_REASON CONSTANT NUMBER := 41;
16:22:01 2474  -- VARIABLES
16:22:01 2475  var_current_subscr_status SUBSCRIPTION.SUBSCRIPTION_STATUS_ID%TYPE;
16:22:01 2476  var_active_invoices_count NUMBER;
16:22:01 2477  -- EXCEPTIONS
16:22:01 2478  BAD_SUBSCRIPTION_ID	   EXCEPTION;
16:22:01 2479  BAD_SUBSCRIPTION_STATUS	   EXCEPTION;
16:22:01 2480  CAN_NOT_UPDATE_SUBSCRIPTION EXCEPTION;
16:22:01 2481  ACTIVE_INVOICES_FOUND	   EXCEPTION;
16:22:01 2482  CAN_NOT_CREATE_NOTE	   EXCEPTION;
16:22:01 2483  EXCEPTION_MESSAGE	   VARCHAR2(1024);
16:22:01 2484  BEGIN
16:22:01 2485  
16:22:01 2486  	 -- Get current subscription status
16:22:01 2487  	 BEGIN
16:22:01 2488  	   SELECT
16:22:01 2489  	     SUBSCRIPTION.SUBSCRIPTION_STATUS_ID into var_current_subscr_status
16:22:01 2490  	   FROM
16:22:01 2491  	     SUBSCRIPTION
16:22:01 2492  	   WHERE
16:22:01 2493  	     SUBSCRIPTION.ID = in_subscription_id;
16:22:01 2494  	   EXCEPTION
16:22:01 2495  	     WHEN NO_DATA_FOUND THEN
16:22:01 2496  	       RAISE BAD_SUBSCRIPTION_ID;
16:22:01 2497  	 END;
16:22:01 2498  
16:22:01 2499  	 -- Check for invoices with active licenses
16:22:01 2500  	 SELECT
16:22:01 2501  	   COUNT(*) into var_active_invoices_count
16:22:01 2502  	 FROM
16:22:01 2503  	   LICENSE
16:22:01 2504  	   INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
16:22:01 2505  	 WHERE
16:22:01 2506  	   LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V22.LICENSE_ACTIVE
16:22:01 2507  	   AND LICENSE.SUBSCRIPTION_ID = in_subscription_id;
16:22:01 2508  
16:22:01 2509  	 IF var_active_invoices_count > 0 THEN
16:22:01 2510  	   RAISE ACTIVE_INVOICES_FOUND;
16:22:01 2511  	 END IF;
16:22:01 2512  
16:22:01 2513  	 -- Check that subscription is active
16:22:01 2514  	 IF var_current_subscr_status != GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE
16:22:01 2515  	    AND var_current_subscr_status != GLOBAL_STATUSES_V22.SUBSCRIPTION_SUSPENDED THEN
16:22:01 2516  	   RAISE BAD_SUBSCRIPTION_STATUS;
16:22:01 2517  	 END IF;
16:22:01 2518  
16:22:01 2519  	 -- Update subscription data
16:22:01 2520  	 BEGIN
16:22:01 2521  	   PROCS_SUBSCRIPTION_CRU_V22.UPDATE_SUBSCRIPTION(
16:22:01 2522  	     in_subscription_id        => in_subscription_id,
16:22:01 2523  	     in_subscription_status_id => GLOBAL_STATUSES_V22.SUBSCRIPTION_FALSE_START,
16:22:01 2524  	     in_cancelation_date       => in_cancelation_date,
16:22:01 2525  	     in_updated_by	       => in_updated_by,
16:22:01 2526  	     in_sct_id		       => FALSE_START_REASON
16:22:01 2527  	   );
16:22:01 2528  	   EXCEPTION
16:22:01 2529  	     WHEN OTHERS THEN
16:22:01 2530  	       EXCEPTION_MESSAGE := SQLERRM;
16:22:01 2531  	       RAISE CAN_NOT_UPDATE_SUBSCRIPTION;
16:22:01 2532  	 END;
16:22:01 2533  
16:22:01 2534  	 -- Annotate subscription
16:22:01 2535  	 IF in_note IS NOT NULL THEN
16:22:01 2536  	   BEGIN
16:22:01 2537  	     PROCS_SUBSCRIPTION_V22.ANNOTATE_SUBSCRIPTION(
16:22:01 2538  	       in_subscription_id,
16:22:01 2539  	       in_agent_id,
16:22:01 2540  	       in_note,
16:22:01 2541  	       in_updated_by
16:22:01 2542  	     );
16:22:01 2543  	     EXCEPTION
16:22:01 2544  	      WHEN OTHERS THEN
16:22:01 2545  		EXCEPTION_MESSAGE := SQLERRM;
16:22:01 2546  		RAISE CAN_NOT_CREATE_NOTE;
16:22:01 2547  	   END;
16:22:01 2548  	 END IF;
16:22:01 2549  
16:22:01 2550  EXCEPTION
16:22:01 2551  WHEN BAD_SUBSCRIPTION_ID THEN
16:22:01 2552  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 2553  	   SPROC_NAME, 'No such subscription');
16:22:01 2554  WHEN BAD_SUBSCRIPTION_STATUS THEN
16:22:01 2555  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INVALID_PARAMETER,
16:22:01 2556  	   SPROC_NAME, 'Bad current subscription status');
16:22:01 2557  WHEN CAN_NOT_UPDATE_SUBSCRIPTION THEN
16:22:01 2558  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:01 2559  	   SPROC_NAME, 'Could not update subscription data', EXCEPTION_MESSAGE);
16:22:01 2560  WHEN ACTIVE_INVOICES_FOUND THEN
16:22:01 2561  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.STATE_ERROR,
16:22:01 2562  	   SPROC_NAME, 'Invoices with active licenses found');
16:22:01 2563  WHEN CAN_NOT_CREATE_NOTE THEN
16:22:01 2564  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:01 2565  	   SPROC_NAME, 'Could not annotate subscription', EXCEPTION_MESSAGE);
16:22:01 2566  WHEN OTHERS THEN
16:22:01 2567  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:01 2568  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:22:01 2569  END FINALIZE_FALSE_START;
