16:22:02 2943  /*
16:22:02 2944  	   Note, this returns paypal records which are in the given window of time
16:22:02 2945  	   based on their creation date, not their expiration date, because we've seen
16:22:02 2946  	   suspicious values in the data for expiration dates. The java layer should
16:22:02 2947  	   inspect the expiration dates and decide whether or not to act on the data.
16:22:02 2948  */
16:22:02 2949  
16:22:02 2950  PROCEDURE GET_EXPIRING_PAYPAL (
16:22:02 2951  	 in_expire_window_days	 IN NUMBER,
16:22:02 2952  	 in_creation_limit_days  IN NUMBER,
16:22:02 2953  	 in_retry_throttle_name  IN PROCESS_RETRY_THROTTLE.PROCESS_NAME%TYPE,
16:22:02 2954  	 out_result_set 	 OUT SYS_REFCURSOR
16:22:02 2955  ) AS
16:22:02 2956  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_EXPIRING_PAYPAL';
16:22:02 2957  -- If creation date is later than this, return the record
16:22:02 2958  var_end_creation DATE := SYSDATE - in_creation_limit_days + in_expire_window_days;
16:22:02 2959  var_rows_to_return CONSTANT NUMBER := 300;
16:22:02 2960  BEGIN
16:22:02 2961  	 OPEN out_result_set FOR
16:22:02 2962  	 SELECT * FROM (
16:22:02 2963  	   SELECT
16:22:02 2964  	       ac.group_id	   AS group_id,
16:22:02 2965  	       pp.id		   AS paypal_id,
16:22:02 2966  	       sb.id		   AS subscription_id,
16:22:02 2967  	       pp.create_date	   AS creation_date,
16:22:02 2968  	       pp.expiration_date  AS expiration_date
16:22:02 2969  	   FROM paypal pp
16:22:02 2970  	   INNER JOIN subscription sb
16:22:02 2971  	   ON pp.account_id = sb.account_id
16:22:02 2972  	   INNER JOIN account ac
16:22:02 2973  	   ON pp.account_id = ac.id
16:22:02 2974  	   WHERE pp.paypal_status_id = GLOBAL_STATUSES_V22.PAYPAL_ACTIVE
16:22:02 2975  	     AND sb.instrument_type_id = GLOBAL_ENUMS_V22.INSTRUMENT_PAYPAL
16:22:02 2976  	     AND sb.subscription_status_id IN (GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE,
16:22:02 2977  					       GLOBAL_STATUSES_V22.SUBSCRIPTION_IN_GRACE_PERIOD)
16:22:02 2978  	     AND sb.instrument_id = pp.id
16:22:02 2979  	     AND pp.create_date <= var_end_creation
16:22:02 2980  	     AND NOT EXISTS(
16:22:02 2981  	       SELECT NULL
16:22:02 2982  	       FROM process_retry_throttle rt
16:22:02 2983  	       WHERE rt.process_name = in_retry_throttle_name
16:22:02 2984  		 AND rt.generic_id = pp.id
16:22:02 2985  	     )
16:22:02 2986  	     AND ROWNUM <= 10 * var_rows_to_return
16:22:02 2987  	   ORDER BY dbms_random.value
16:22:02 2988  	 )
16:22:02 2989  	 WHERE ROWNUM <= var_rows_to_return;
16:22:02 2990  END GET_EXPIRING_PAYPAL;
