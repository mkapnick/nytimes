16:22:00  46  	  -- VARIABLES
16:22:00  47  	  var_subs	  SYS_REFCURSOR;
16:22:00  48  	BEGIN
16:22:00  49  	  OPEN out_result_set FOR
16:22:00  50  
16:22:00  51  	SELECT
16:22:00  52  	  C.CODE NAME,
16:22:00  53  	  C.DESCRIPTION,
16:22:00  54  	  0 INHERITED,
16:22:00  55  	  C.SHAREABLE,
16:22:00  56  	  MAX(LIC.ENTITLEMENT_END_DATE) EXPIRES
16:22:00  57  	FROM
16:22:00  58  	  SUBSCRIPTION SB
16:22:00  59  	  INNER JOIN ACCOUNT AC ON AC.ID = SB.ACCOUNT_ID
16:22:00  60  	  INNER JOIN LICENSE LIC ON LIC.SUBSCRIPTION_ID = SB.ID
16:22:00  61  	  INNER JOIN OFFER_PRODUCT_OFFERING OPO ON OPO.OFFER_ID = LIC.OFFER_ID
16:22:00  62  	  INNER JOIN PRODUCT_OFFERING PO ON PO.ID = OPO.PRODUCT_OFFERING_ID
16:22:00  63  	  INNER JOIN CAPABILITY C ON PO.CAPABILITY_ID = C.ID
16:22:00  64  	WHERE
16:22:00  65  	  LIC.ENTITLEMENT_END_DATE >= TRUNC(SYSDATE)
16:22:00  66  	  AND LIC.START_DATE <= SYSDATE
16:22:00  67  	  AND AC.GROUP_ID = in_group_id
16:22:00  68  	GROUP BY
16:22:00  69  	  C.CODE, 0, C.SHAREABLE, C.DESCRIPTION
16:22:00  70  UNION ALL
16:22:00  71  	SELECT
16:22:00  72  	  C.CODE NAME,
16:22:00  73  	  C.DESCRIPTION,
16:22:00  74  	  1 INHERITED,
16:22:00  75  	  C.SHAREABLE,
16:22:00  76  	  MAX(LEAST(SS.END_DATE, LIC.ENTITLEMENT_END_DATE)) EXPIRES
16:22:00  77  	FROM
16:22:00  78  	  ACCOUNT BORROWER,
16:22:00  79  	  SUBSCRIPTION S,
16:22:00  80  	  LICENSE LIC,
16:22:00  81  	  OFFER_PRODUCT_OFFERING OPO,
16:22:00  82  	  PRODUCT_OFFERING PO,
16:22:00  83  	  CAPABILITY C,
16:22:00  84  	  GROUP_ACCOUNT GA,
16:22:00  85  	  SUBSCRIPTION_SHARE SS
16:22:00  86  	WHERE
16:22:00  87  	  BORROWER.GROUP_ID = in_group_id
16:22:00  88  	  AND LIC.SUBSCRIPTION_ID = S.ID
16:22:00  89  	  AND OPO.OFFER_ID = LIC.OFFER_ID
16:22:00  90  	  AND PO.ID = OPO.PRODUCT_OFFERING_ID
16:22:00  91  	  AND PO.CAPABILITY_ID = C.ID
16:22:00  92  	  AND GA.SUBSCRIPTION_ID = S.ID
16:22:00  93  	  AND SS.BORROWER_ACCOUNT_ID = BORROWER.ID
16:22:00  94  	  AND SS.GROUP_ACCOUNT_ID = GA.ID
16:22:00  95  	  AND SYSDATE BETWEEN SS.START_DATE AND SS.END_DATE
16:22:00  96  	  AND SYSDATE BETWEEN LIC.START_DATE AND LIC.ENTITLEMENT_END_DATE
16:22:00  97  	  AND C.SHAREABLE = 1
16:22:00  98  	GROUP BY
16:22:00  99  	  C.CODE, 0, C.SHAREABLE, C.DESCRIPTION;
16:22:00 100  
16:22:00 101  	EXCEPTION
16:22:00 102  	  WHEN OTHERS THEN
16:22:00 103  	    PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:00 104  	      SPROC_NAME, 'Unknown error', SQLERRM);
16:22:00 105  	END GET_ALL_ENTITLEMENTS;
