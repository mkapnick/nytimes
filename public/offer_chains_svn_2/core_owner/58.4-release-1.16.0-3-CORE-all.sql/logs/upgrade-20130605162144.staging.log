16:22:00  40  	AS
16:22:00  41  	  -- CONSTANTS
16:22:00  42  	  SPROC_NAME	  CONSTANT VARCHAR2(20) := 'GET_ALL_ENTITLEMENTS';
16:22:00  43  	  -- EXCEPTIONS
