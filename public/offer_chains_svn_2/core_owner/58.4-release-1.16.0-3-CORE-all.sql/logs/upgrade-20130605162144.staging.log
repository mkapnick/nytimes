16:22:01 680  FUNCTION GET_NEXT_OFFER_INDEX (
16:22:01 681  	in_offer_chain_id	     IN NUMBER,
16:22:01 682  	in_offer_chain_current_index IN NUMBER
16:22:01 683  ) RETURN NUMBER AS
16:22:01 684  -- VARIABLES
16:22:01 685  SPROC_NAME		     CONSTANT VARCHAR2(20) := 'GET_NEXT_OFFER_INDEX';
16:22:01 686  temp_offer_chain_id	     NUMBER;
16:22:01 687  temp_offer_chain_current_index NUMBER;
16:22:01 688  var_result		     NUMBER;
16:22:01 689  -- EXCEPTIONS
16:22:01 690  BAD_OFFER_CHAIN_ID      EXCEPTION;
16:22:01 691  BAD_CURRENT_INDEX_VALUE EXCEPTION;
16:22:01 692  BEGIN
16:22:01 693  
16:22:01 694  	-- Check that offer chain exists
16:22:01 695  	BEGIN
16:22:01 696  	  SELECT
16:22:01 697  	    OFFER_CHAIN.ID into temp_offer_chain_id
16:22:01 698  	  FROM
16:22:01 699  	    OFFER_CHAIN
16:22:01 700  	  WHERE
16:22:01 701  	    OFFER_CHAIN.ID = in_offer_chain_id;
16:22:01 702  	  EXCEPTION
16:22:01 703  	    WHEN NO_DATA_FOUND THEN
16:22:01 704  	      RAISE BAD_OFFER_CHAIN_ID;
16:22:01 705  	END;
16:22:01 706  
16:22:01 707  	-- Check that current offer index exists
16:22:01 708  	BEGIN
16:22:01 709  	  SELECT
16:22:01 710  	    OFFER_OFFER_CHAIN.INDEX_VALUE into temp_offer_chain_current_index
16:22:01 711  	  FROM
16:22:01 712  	    OFFER_OFFER_CHAIN
16:22:01 713  	  WHERE
16:22:01 714  	    OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
16:22:01 715  	    AND OFFER_OFFER_CHAIN.INDEX_VALUE = in_offer_chain_current_index
16:22:01 716  	    -- TODO: delete next line
16:22:01 717  	    AND ROWNUM <= 1;
16:22:01 718  	  EXCEPTION
16:22:01 719  	    WHEN NO_DATA_FOUND THEN
16:22:01 720  	      RAISE BAD_CURRENT_INDEX_VALUE;
16:22:01 721  	END;
16:22:01 722  
16:22:01 723  	-- Get next offer index
16:22:01 724  	BEGIN
16:22:01 725  	  SELECT
16:22:01 726  	    INDEX_VALUE into var_result
16:22:01 727  	  FROM (
16:22:01 728  	    SELECT
16:22:01 729  	      OFFER_OFFER_CHAIN.INDEX_VALUE
16:22:01 730  	    FROM
16:22:01 731  	      OFFER_OFFER_CHAIN
16:22:01 732  	    WHERE
16:22:01 733  	      OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
16:22:01 734  	      AND OFFER_OFFER_CHAIN.INDEX_VALUE > in_offer_chain_current_index
16:22:01 735  	    ORDER BY
16:22:01 736  	      OFFER_OFFER_CHAIN.INDEX_VALUE ASC
16:22:01 737  	  )
16:22:01 738  	  WHERE
16:22:01 739  	    ROWNUM <= 1;
16:22:01 740  	  EXCEPTION
16:22:01 741  	    WHEN NO_DATA_FOUND THEN
16:22:01 742  	      var_result := NULL;
16:22:01 743  	END;
16:22:01 744  
16:22:01 745  	RETURN var_result;
16:22:01 746  
16:22:01 747  EXCEPTION
16:22:01 748  WHEN BAD_OFFER_CHAIN_ID THEN
16:22:01 749  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 750  	  SPROC_NAME, 'No such offer chain');
16:22:01 751  WHEN BAD_CURRENT_INDEX_VALUE THEN
16:22:01 752  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:01 753  	  SPROC_NAME, 'Bad current index value');
16:22:01 754  WHEN OTHERS THEN
16:22:01 755  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:01 756  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:22:01 757  END GET_NEXT_OFFER_INDEX;
16:22:01 758  
16:22:01 759  PROCEDURE P_GET_NEXT_OFFER_INDEX (
