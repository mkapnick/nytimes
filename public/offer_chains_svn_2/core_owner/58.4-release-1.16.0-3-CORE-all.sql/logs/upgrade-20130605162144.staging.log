16:22:01 1981  /******************************************************************************/
16:22:01 1982  
16:22:01 1983  PROCEDURE GET_SUBSCRIPTION_PRODUCTS (
16:22:01 1984  /*
16:22:01 1985  APP_EXCEPTION_CODES_V22.NOT_FOUND
16:22:01 1986  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
16:22:01 1987  */
16:22:01 1988  	 in_subscription_id IN NUMBER,
16:22:01 1989  	 out_result_set     OUT SYS_REFCURSOR
16:22:01 1990  ) AS
16:22:01 1991  SPROC_NAME CONSTANT VARCHAR2(25) := 'GET_SUBSCRIPTION_PRODUCTS';
16:22:01 1992  -- VARIABLES
16:22:01 1993  var_offer_chain NUMBER;
16:22:01 1994  -- EXCEPTIONS
16:22:01 1995  BAD_SUBSCRIPTION_ID EXCEPTION;
16:22:01 1996  BEGIN
16:22:01 1997  	 BEGIN
16:22:01 1998  	   SELECT
16:22:01 1999  	     SUBSCRIPTION.OFFER_CHAIN_ID into var_offer_chain
16:22:01 2000  	   FROM
16:22:01 2001  	     SUBSCRIPTION
16:22:01 2002  	   WHERE
16:22:01 2003  	     SUBSCRIPTION.ID = in_subscription_id;
16:22:01 2004  	   EXCEPTION
16:22:01 2005  	     WHEN NO_DATA_FOUND THEN
16:22:01 2006  	       RAISE BAD_SUBSCRIPTION_ID;
16:22:01 2007  	 END;
16:22:01 2008  
16:22:01 2009  	 OPEN out_result_set FOR
16:22:01 2010  	 SELECT DISTINCT
16:22:01 2011  	   PRODUCT.ID,
16:22:01 2012  	   PRODUCT.NAME
16:22:01 2013  	 FROM
16:22:01 2014  	   PRODUCT
16:22:01 2015  	 WHERE
16:22:01 2016  	   PRODUCT.ID IN (
16:22:01 2017  	     SELECT DISTINCT
16:22:01 2018  	       PRODUCT_OFFERING.PRODUCT_ID
16:22:01 2019  	     FROM
16:22:01 2020  	       PRODUCT_OFFERING
16:22:01 2021  	     WHERE
16:22:01 2022  	       PRODUCT_OFFERING.ID IN (
16:22:01 2023  		 SELECT DISTINCT
16:22:01 2024  		   OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID
16:22:01 2025  		 FROM
16:22:01 2026  		   OFFER_PRODUCT_OFFERING
16:22:01 2027  		 WHERE
16:22:01 2028  		   OFFER_PRODUCT_OFFERING.OFFER_ID IN (
16:22:01 2029  		     SELECT DISTINCT
16:22:01 2030  		       OFFER_OFFER_CHAIN.OFFER_ID
16:22:01 2031  		     FROM
16:22:01 2032  		       OFFER_OFFER_CHAIN
16:22:01 2033  		     WHERE
16:22:01 2034  		       OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain
16:22:01 2035  		   )
16:22:01 2036  	       )
16:22:01 2037  	   );
16:22:01 2038  
16:22:01 2039  EXCEPTION
16:22:01 2040  WHEN BAD_SUBSCRIPTION_ID THEN
