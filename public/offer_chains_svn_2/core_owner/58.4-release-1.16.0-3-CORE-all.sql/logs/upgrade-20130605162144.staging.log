16:22:02 453  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INVALID_PARAMETER,
16:22:02 454  	  SPROC_NAME, 'Wrong gift certificate type');
16:22:02 455  WHEN OTHERS THEN
16:22:02 456  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:02 457  	  SPROC_NAME, 'Unknown error', SQLERRM);
