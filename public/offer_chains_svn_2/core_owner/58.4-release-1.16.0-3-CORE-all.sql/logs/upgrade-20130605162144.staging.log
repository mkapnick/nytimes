16:22:01 475  	    LICENSE.CURRENT_OFFER_INDEX
16:22:01 476  	    into
16:22:01 477  	    var_offer_chain_id,
16:22:01 478  	    var_offer_id,
16:22:01 479  	    var_license_current_index
16:22:01 480  	  FROM
16:22:01 481  	    LICENSE
16:22:01 482  	    INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
16:22:01 483  	  WHERE
16:22:01 484  	    LICENSE.ID = in_license_id;
16:22:01 485  	  EXCEPTION
16:22:01 486  	    WHEN NO_DATA_FOUND THEN
16:22:01 487  	      RAISE BAD_LICENSE_ID;
16:22:01 488  	END;
16:22:01 489  
16:22:01 490  	BEGIN
