16:22:02 1097  
16:22:02 1098  PROCEDURE GET_INVOICE_DAYS_USED_NUMBER (
16:22:02 1099  	 in_invoice_id	     IN NUMBER,
16:22:02 1100  	 in_chargeback_date  IN DATE DEFAULT SYSDATE,
16:22:02 1101  	 out_days_num	     OUT NUMBER
16:22:02 1102  ) AS
16:22:02 1103  SPROC_NAME CONSTANT VARCHAR2(28) := 'GET_INVOICE_DAYS_USED_NUMBER';
16:22:02 1104  -- VARIABLES
16:22:02 1105  temp_invoice_id	      NUMBER;
16:22:02 1106  var_license_start_date DATE;
16:22:02 1107  var_license_end_date   DATE;
16:22:02 1108  var_chargeback_date    DATE;
16:22:02 1109  var_invoice_days_used  NUMBER;
16:22:02 1110  -- EXCEPTIONS
16:22:02 1111  BAD_INVOICE_ID EXCEPTION;
16:22:02 1112  BEGIN
16:22:02 1113  
16:22:02 1114  	 var_chargeback_date := NVL(in_chargeback_date, SYSDATE);
16:22:02 1115  
16:22:02 1116  	 BEGIN
16:22:02 1117  	   SELECT
16:22:02 1118  	     INVOICE.ID into temp_invoice_id
16:22:02 1119  	   FROM
16:22:02 1120  	     INVOICE
16:22:02 1121  	   WHERE
16:22:02 1122  	     INVOICE.ID = in_invoice_id;
16:22:02 1123  	   EXCEPTION
16:22:02 1124  	     WHEN NO_DATA_FOUND THEN
16:22:02 1125  	       RAISE BAD_INVOICE_ID;
16:22:02 1126  	 END;
16:22:02 1127  
16:22:02 1128  	 var_invoice_days_used := 0;
16:22:02 1129  
16:22:02 1130  	 FOR f_license IN (
16:22:02 1131  	   SELECT
16:22:02 1132  	     LICENSE.START_DATE,
16:22:02 1133  	     LICENSE.END_DATE
16:22:02 1134  	   FROM
16:22:02 1135  	     LICENSE
16:22:02 1136  	   WHERE
16:22:02 1137  	     LICENSE.INVOICE_ID = in_invoice_id
16:22:02 1138  	     AND LICENSE.IS_EXTENSION = GLOBAL_CONSTANTS_V22.FALSE
16:22:02 1139  	 )
16:22:02 1140  	 LOOP
16:22:02 1141  	   var_license_start_date := PROCS_COMMON_V22.NORMALIZE_DATE(f_license.START_DATE);
16:22:02 1142  	   var_license_end_date := PROCS_COMMON_V22.NORMALIZE_DATE(f_license.END_DATE);
16:22:02 1143  
16:22:02 1144  	   IF var_license_start_date <= var_chargeback_date THEN
16:22:02 1145  	     IF var_license_end_date <= var_chargeback_date THEN
16:22:02 1146  	       -- License is passed
16:22:02 1147  	       var_invoice_days_used := var_invoice_days_used + (var_license_end_date - var_license_start_date);
16:22:02 1148  	     ELSE
16:22:02 1149  	       -- This is current license
16:22:02 1150  	       var_invoice_days_used := var_invoice_days_used + (var_chargeback_date - var_license_start_date);
16:22:02 1151  	     END IF;
16:22:02 1152  	   ELSE
16:22:02 1153  	     -- if var_license_start_date > in_chargeback_date then do nothing
16:22:02 1154  	     NULL;
16:22:02 1155  	   END IF;
16:22:02 1156  	 END LOOP;
16:22:02 1157  
16:22:02 1158  	 out_days_num := var_invoice_days_used;
16:22:02 1159  
16:22:02 1160  EXCEPTION
16:22:02 1161  WHEN BAD_INVOICE_ID THEN
16:22:02 1162  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:02 1163  	   SPROC_NAME, 'No such invoice');
16:22:02 1164  WHEN OTHERS THEN
16:22:02 1165  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:02 1166  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:22:02 1167  END GET_INVOICE_DAYS_USED_NUMBER;
