16:22:01 329  PROCEDURE GET_RECURRING_OFFER (
16:22:01 330  	in_license_id  IN NUMBER,
16:22:01 331  	out_result_set OUT SYS_REFCURSOR
16:22:01 332  ) AS
16:22:01 333  -- VARIABLES
16:22:01 334  SPROC_NAME		    CONSTANT VARCHAR2(19) := 'GET_RECURRING_OFFER';
16:22:01 335  var_offer_chain_id	    NUMBER;
16:22:01 336  var_offer_id		    NUMBER;
16:22:01 337  var_offer_num_recurrences     NUMBER;
16:22:01 338  var_license_cur_offer_rec_num NUMBER;
16:22:01 339  var_offer_index		    NUMBER;
16:22:01 340  -- EXCEPTIONS
16:22:01 341  BAD_LICENSE_ID	     EXCEPTION;
16:22:01 342  CAN_NOT_GET_OFFER_INFO EXCEPTION;
16:22:01 343  BEGIN
16:22:01 344  
16:22:01 345  	BEGIN
16:22:01 346  	  SELECT
16:22:01 347  	    SUBSCRIPTION.OFFER_CHAIN_ID,
16:22:01 348  	    LICENSE.OFFER_ID,
16:22:01 349  	    LICENSE.CURRENT_OFFER_RECURR_NUM
16:22:01 350  	    into
16:22:01 351  	    var_offer_chain_id,
16:22:01 352  	    var_offer_id,
16:22:01 353  	    var_license_cur_offer_rec_num
16:22:01 354  	  FROM
16:22:01 355  	    LICENSE
16:22:01 356  	    INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
16:22:01 357  	  WHERE
16:22:01 358  	    LICENSE.ID = in_license_id;
16:22:01 359  	  EXCEPTION
16:22:01 360  	    WHEN NO_DATA_FOUND THEN
16:22:01 361  	      RAISE BAD_LICENSE_ID;
16:22:01 362  	END;
16:22:01 363  
16:22:01 364  	BEGIN
16:22:01 365  	  SELECT
16:22:01 366  	    OFFER_OFFER_CHAIN.NUM_RECURRENCES,
16:22:01 367  	    OFFER_OFFER_CHAIN.INDEX_VALUE
16:22:01 368  	    into
16:22:01 369  	    var_offer_num_recurrences,
16:22:01 370  	    var_offer_index
16:22:01 371  	  FROM
16:22:01 372  	    OFFER_OFFER_CHAIN
16:22:01 373  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
16:22:01 374  	  WHERE
16:22:01 375  	    OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
16:22:01 376  	    AND OFFER_OFFER_CHAIN.OFFER_ID = var_offer_id;
16:22:01 377  	  EXCEPTION
16:22:01 378  	    WHEN NO_DATA_FOUND THEN
16:22:01 379  	      RAISE CAN_NOT_GET_OFFER_INFO;
16:22:01 380  	END;
16:22:01 381  
16:22:01 382  	IF var_offer_num_recurrences = 0 THEN
16:22:01 383  	  --out_result_set := NULL;
16:22:01 384  	  -- TODO: Remove this hardcode
16:22:01 385  	  OPEN out_result_set FOR
16:22:01 386  	  SELECT
16:22:01 387  	    OFFER.ID,
16:22:01 388  	    OFFER.OFFER_STATUS_ID,
16:22:01 389  	    OFFER.ENTITLEMENT_DURATION,
16:22:01 390  	    OFFER.CREATED_BY,
16:22:01 391  	    OFFER.CREATE_DATE,
16:22:01 392  	    OFFER.UPDATED_BY,
16:22:01 393  	    OFFER.UPDATE_DATE,
16:22:01 394  	    var_offer_num_recurrences as "RECURRENCE_NUMBER",
16:22:01 395  	    var_offer_index as "OFFER_INDEX"
16:22:01 396  	  FROM
16:22:01 397  	    OFFER_OFFER_CHAIN
16:22:01 398  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
16:22:01 399  	  WHERE ROWNUM = 0;
16:22:01 400  	ELSIF var_license_cur_offer_rec_num = var_offer_num_recurrences THEN
16:22:01 401  	  --out_result_set := NULL;
16:22:01 402  	  -- TODO: Remove this hardcode
16:22:01 403  	  OPEN out_result_set FOR
16:22:01 404  	  SELECT
16:22:01 405  	    OFFER.ID,
16:22:01 406  	    OFFER.OFFER_STATUS_ID,
16:22:01 407  	    OFFER.ENTITLEMENT_DURATION,
16:22:01 408  	    OFFER.CREATED_BY,
16:22:01 409  	    OFFER.CREATE_DATE,
16:22:01 410  	    OFFER.UPDATED_BY,
16:22:01 411  	    OFFER.UPDATE_DATE,
16:22:01 412  	    var_offer_num_recurrences as "RECURRENCE_NUMBER",
16:22:01 413  	    var_offer_index as "OFFER_INDEX"
16:22:01 414  	  FROM
16:22:01 415  	    OFFER_OFFER_CHAIN
16:22:01 416  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
16:22:01 417  	  WHERE ROWNUM = 0;
16:22:01 418  	ELSE
16:22:01 419  	  OPEN out_result_set FOR
16:22:01 420  	  SELECT
16:22:01 421  	    OFFER.ID,
16:22:01 422  	    OFFER.OFFER_STATUS_ID,
16:22:01 423  	    OFFER.ENTITLEMENT_DURATION,
16:22:01 424  	    OFFER.CREATED_BY,
16:22:01 425  	    OFFER.CREATE_DATE,
16:22:01 426  	    OFFER.UPDATED_BY,
16:22:01 427  	    OFFER.UPDATE_DATE,
16:22:01 428  	    var_offer_num_recurrences as "RECURRENCE_NUMBER",
16:22:01 429  	    var_offer_index as "OFFER_INDEX"
16:22:01 430  	  FROM
16:22:01 431  	    OFFER
16:22:01 432  	  WHERE
16:22:01 433  	    OFFER.ID = var_offer_id;
16:22:01 434  	END IF;
16:22:01 435  
16:22:01 436  EXCEPTION
16:22:01 437  WHEN BAD_LICENSE_ID THEN
16:22:01 438  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 439  	  SPROC_NAME, 'No such license');
16:22:01 440  WHEN CAN_NOT_GET_OFFER_INFO THEN
16:22:01 441  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 442  	  SPROC_NAME, 'Could not get offer information. Offer id = '||var_offer_id||', Offer chain id = '||var_offer_chain_id);
16:22:01 443  WHEN OTHERS THEN
16:22:01 444  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:01 445  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:22:01 446  END GET_RECURRING_OFFER;
