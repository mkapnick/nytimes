16:22:01 1951  	   SPROC_NAME, 'Active licenses found');
16:22:01 1952  WHEN OTHERS THEN
16:22:01 1953  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:01 1954  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:22:01 1955  END CLOSE_SUBSCRIPTION;
