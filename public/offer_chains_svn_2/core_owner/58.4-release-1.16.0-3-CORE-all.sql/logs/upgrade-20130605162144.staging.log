16:22:01 1076  FUNCTION CHECK_FOR_SAME_PRODUCTS (
16:22:01 1077  	 in_offer_chain_1	  IN OFFER_CHAIN.ID%TYPE,
16:22:01 1078  	 in_offer_chain_2	  IN OFFER_CHAIN.ID%TYPE,
16:22:01 1079  	 in_use_eligibility_rules IN NUMBER DEFAULT GLOBAL_CONSTANTS_V22.FALSE
16:22:01 1080  ) RETURN NUMBER AS
16:22:01 1081  SPROC_NAME CONSTANT VARCHAR2(23) := 'CHECK_FOR_SAME_PRODUCTS';
16:22:01 1082  -- CONSTANTS
16:22:01 1083  PRODUCT_ELIGIBILITY_NAME CONSTANT VARCHAR2(19) := 'MAX_CONCURRENT_SUBS';
16:22:01 1084  -- VARIABLES
16:22:01 1085  temp_offer_chain_id	OFFER_CHAIN.ID%TYPE;
16:22:01 1086  var_same_products	SYS_REFCURSOR;
16:22:01 1087  var_same_product_id	NUMBER;
16:22:01 1088  same_product_count	NUMBER;
16:22:01 1089  var_product_eligibility_limit NUMBER;
16:22:01 1090  s_product_eligibility_limit   VARCHAR2(100);
16:22:01 1091  -- EXCEPTIONS
16:22:01 1092  BAD_FIRST_OFFER_CHAIN	      EXCEPTION;
16:22:01 1093  BAD_SECOND_OFFER_CHAIN	      EXCEPTION;
16:22:01 1094  BEGIN
16:22:01 1095  
16:22:01 1096  	 -- Check that first offer chain exists
16:22:01 1097  	 BEGIN
16:22:01 1098  	   SELECT
16:22:01 1099  	     OFFER_CHAIN.ID into temp_offer_chain_id
16:22:01 1100  	   FROM
16:22:01 1101  	     OFFER_CHAIN
16:22:01 1102  	   WHERE
16:22:01 1103  	     OFFER_CHAIN.ID = in_offer_chain_1;
16:22:01 1104  	   EXCEPTION
16:22:01 1105  	     WHEN NO_DATA_FOUND THEN
16:22:01 1106  	       RAISE BAD_FIRST_OFFER_CHAIN;
16:22:01 1107  	 END;
16:22:01 1108  
16:22:01 1109  	 -- Check that second offer chain exists
16:22:01 1110  	 BEGIN
16:22:01 1111  	   SELECT
16:22:01 1112  	     OFFER_CHAIN.ID into temp_offer_chain_id
16:22:01 1113  	   FROM
16:22:01 1114  	     OFFER_CHAIN
16:22:01 1115  	   WHERE
16:22:01 1116  	     OFFER_CHAIN.ID = in_offer_chain_2;
16:22:01 1117  	   EXCEPTION
16:22:01 1118  	     WHEN NO_DATA_FOUND THEN
16:22:01 1119  	       RAISE BAD_SECOND_OFFER_CHAIN;
16:22:01 1120  	 END;
16:22:01 1121  
16:22:01 1122  	 PROCS_OFFER_CHAIN_V22.GET_OFF_CHAINS_SAME_PRODUCTS(
16:22:01 1123  	   in_offer_chain_1 => in_offer_chain_1,
16:22:01 1124  	   in_offer_chain_2 => in_offer_chain_2,
16:22:01 1125  	   out_result_set   => var_same_products
16:22:01 1126  	 );
16:22:01 1127  
16:22:01 1128  	 LOOP
16:22:01 1129  	   FETCH var_same_products INTO var_same_product_id, same_product_count;
16:22:01 1130  	   EXIT WHEN var_same_products%NOTFOUND;
16:22:01 1131  
16:22:01 1132  	   IF in_use_eligibility_rules = GLOBAL_CONSTANTS_V22.FALSE THEN
16:22:01 1133  	     -- Return false because this offer chains having same products
16:22:01 1134  	     RETURN GLOBAL_CONSTANTS_V22.TRUE;
16:22:01 1135  	   ELSE
16:22:01 1136  
16:22:01 1137  	     -- Get eligibility rule for given product
16:22:01 1138  	     BEGIN
16:22:01 1139  	       SELECT
16:22:01 1140  		 PRODUCT_ELIGIBILITY.VALUE into s_product_eligibility_limit
16:22:01 1141  	       FROM
16:22:01 1142  		 PRODUCT_ELIGIBILITY
16:22:01 1143  	       WHERE
16:22:01 1144  		 PRODUCT_ELIGIBILITY.PRODUCT_ID = var_same_product_id
16:22:01 1145  		 AND PRODUCT_ELIGIBILITY.NAME = PRODUCT_ELIGIBILITY_NAME;
16:22:01 1146  
16:22:01 1147  	       -- REVU: What should to be here? 1?
16:22:01 1148  	       EXCEPTION
16:22:01 1149  		 WHEN NO_DATA_FOUND THEN
16:22:01 1150  		   s_product_eligibility_limit := '1';
16:22:01 1151  	     END;
16:22:01 1152  
16:22:01 1153  	     IF UPPER(s_product_eligibility_limit) = GLOBAL_CONSTANTS_V22.MAX_CONSURRENT_PRD_UNLIM THEN
16:22:01 1154  	       RETURN GLOBAL_CONSTANTS_V22.FALSE;
16:22:01 1155  	     END IF;
16:22:01 1156  
16:22:01 1157  	     var_product_eligibility_limit := TO_NUMBER(s_product_eligibility_limit);
16:22:01 1158  
16:22:01 1159  	     -- Check for limit
16:22:01 1160  	     IF var_product_eligibility_limit < same_product_count THEN
16:22:01 1161  	       RETURN GLOBAL_CONSTANTS_V22.TRUE;
16:22:01 1162  	     END IF;
16:22:01 1163  
16:22:01 1164  	   END IF;
16:22:01 1165  	 END LOOP;
16:22:01 1166  
16:22:01 1167  	 RETURN GLOBAL_CONSTANTS_V22.FALSE;
16:22:01 1168  
16:22:01 1169  EXCEPTION
16:22:01 1170  WHEN BAD_FIRST_OFFER_CHAIN THEN
16:22:01 1171  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 1172  	   SPROC_NAME, 'First offer chain not found');
16:22:01 1173  WHEN BAD_SECOND_OFFER_CHAIN THEN
16:22:01 1174  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 1175  	   SPROC_NAME, 'Second offer chain not found');
16:22:01 1176  WHEN OTHERS THEN
16:22:01 1177  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:01 1178  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:22:01 1179  END CHECK_FOR_SAME_PRODUCTS;
