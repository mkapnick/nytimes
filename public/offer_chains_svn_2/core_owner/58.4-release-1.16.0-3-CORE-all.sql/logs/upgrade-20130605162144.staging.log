16:22:02 1969  	 end if;
16:22:02 1970  
16:22:02 1971  	 SELECT
16:22:02 1972  	   t.order_id,
16:22:02 1973  	   ta.external_transaction_id
16:22:02 1974  	   into
16:22:02 1975  	   out_order_id,
16:22:02 1976  	   out_external_transaction_id
16:22:02 1977  	 from
16:22:02 1978  	   charge ch
16:22:02 1979  	   inner join transaction t on ch.transaction_id = t.id
16:22:02 1980  	   inner join transaction_attempt ta on ta.transaction_id = t.id
16:22:02 1981  	 where
16:22:02 1982  	   ch.invoice_id = in_invoice_id
16:22:02 1983  	   and ta.transaction_attempt_status_id = GLOBAL_STATUSES_V22.TRANS_ATTEMPT_SUCCESS
