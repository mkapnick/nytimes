16:22:02 703  
16:22:02 704  	PROCEDURE START_GC_PURCHASING (
16:22:02 705  	  in_group_id		    IN NUMBER,
16:22:02 706  	  in_offer_chain_id	    IN VARCHAR2,
16:22:02 707  	  in_gift_certificate_code  IN	VARCHAR2,
16:22:02 708  	  in_created_by 	    IN	VARCHAR2,
16:22:02 709  	  in_recipient_name	    IN	VARCHAR2,
16:22:02 710  	  in_recipient_email	    IN	VARCHAR2,
16:22:02 711  	  in_recipient_address_id   IN NUMBER,
16:22:02 712  	  in_recipient_notify_date  IN DATE,
16:22:02 713  	  in_sender_name	    IN VARCHAR2,
16:22:02 714  	  in_sender_email	    IN VARCHAR2,
16:22:02 715  	  in_gift_message	    IN	VARCHAR2,
16:22:02 716  	  in_expiration_date	    IN DATE,
16:22:02 717  	  in_campaign		    IN VARCHAR2,
16:22:02 718  	  in_reference_code	    IN VARCHAR2,
16:22:02 719  	  out_gift_certificate_id   OUT NUMBER,
16:22:02 720  	  out_invoice_id	    OUT NUMBER
16:22:02 721  	) AS
16:22:02 722  	  SPROC_NAME CONSTANT VARCHAR2(19) := 'START_GC_PURCHASING';
16:22:02 723  -- VARIABLES
16:22:02 724  	  var_account_id	    NUMBER;
16:22:02 725  	  temp_gc_code		    GIFT_CERTIFICATE.CODE%TYPE;
16:22:02 726  	  var_och_is_gc 	    NUMBER;
16:22:02 727  	  var_offer_chain_status_id NUMBER;
16:22:02 728  	  var_is_for_redemption     NUMBER;
16:22:02 729  	  var_new_invoice_id	    NUMBER;
16:22:02 730  	  var_gift_cert_id	    NUMBER;
16:22:02 731  	  var_account_tax_exempt_id VARCHAR2(255);
16:22:02 732  -- EXCEPTIONS
16:22:02 733  	    BAD_GROUP_ID		  EXCEPTION;
16:22:02 734  	    GC_CODE_ALREADY_EXISTS	  EXCEPTION;
16:22:02 735  	    BAD_OFFER_CHAIN_ID		  EXCEPTION;
16:22:02 736  	    OCH_IS_NOT_GIFT_CERTIFICATE   EXCEPTION;
16:22:02 737  	    BAD_OFFER_CHAIN_STATUS	  EXCEPTION;
16:22:02 738  	    CAN_NOT_PURCHASE_GC_FOR_RDMPN EXCEPTION;
16:22:02 739  	    CAN_NOT_CREATE_INVOICE	  EXCEPTION;
16:22:02 740  	    OFFER_REC_NUM_LESS_THAN_ONE   EXCEPTION;
16:22:02 741  	    CAN_NOT_CREATE_LINE_ITEMS	  EXCEPTION;
16:22:02 742  	    REF_CODE_ALREADY_EXISTS	  EXCEPTION;
16:22:02 743  
16:22:02 744  	  EXCEPTION_MESSAGE VARCHAR2(1024);
16:22:02 745  	  BEGIN
16:22:02 746  -- Get account id
16:22:02 747  	    BEGIN
16:22:02 748  	      SELECT
16:22:02 749  		ACCOUNT.ID,
16:22:02 750  		ACCOUNT.TAX_EXEMPT_ID
16:22:02 751  	      into
16:22:02 752  		var_account_id,
16:22:02 753  		var_account_tax_exempt_id
16:22:02 754  	      FROM
16:22:02 755  		ACCOUNT
16:22:02 756  	      WHERE
16:22:02 757  		ACCOUNT.GROUP_ID = in_group_id;
16:22:02 758  	      EXCEPTION
16:22:02 759  	      WHEN NO_DATA_FOUND THEN
16:22:02 760  	      RAISE BAD_GROUP_ID;
16:22:02 761  	    END;
16:22:02 762  
16:22:02 763  -- Check for the same code
16:22:02 764  	    BEGIN
16:22:02 765  	      SELECT
16:22:02 766  		GIFT_CERTIFICATE.CODE into temp_gc_code
16:22:02 767  	      FROM
16:22:02 768  		GIFT_CERTIFICATE
16:22:02 769  	      WHERE
16:22:02 770  		GIFT_CERTIFICATE.CODE = in_gift_certificate_code;
16:22:02 771  
16:22:02 772  	      RAISE GC_CODE_ALREADY_EXISTS;
16:22:02 773  
16:22:02 774  	      EXCEPTION
16:22:02 775  	      WHEN NO_DATA_FOUND THEN
16:22:02 776  	      NULL;
16:22:02 777  	    END;
16:22:02 778  
16:22:02 779  -- Get offer chain flag "is_gift_certificate"
16:22:02 780  	    BEGIN
16:22:02 781  	      SELECT
16:22:02 782  		OFFER_CHAIN.IS_GIFT_CERTIFICATE,
16:22:02 783  		OFFER_CHAIN.OFFER_CHAIN_STATUS_ID
16:22:02 784  	      into
16:22:02 785  		var_och_is_gc,
16:22:02 786  		var_offer_chain_status_id
16:22:02 787  	      FROM
16:22:02 788  		OFFER_CHAIN
16:22:02 789  	      WHERE
16:22:02 790  		OFFER_CHAIN.ID = in_offer_chain_id;
16:22:02 791  	      EXCEPTION
16:22:02 792  	      WHEN NO_DATA_FOUND THEN
16:22:02 793  	      RAISE BAD_OFFER_CHAIN_ID;
16:22:02 794  	    END;
16:22:02 795  
16:22:02 796  	    IF var_och_is_gc != GLOBAL_CONSTANTS_V22.TRUE
16:22:02 797  	       OR var_och_is_gc IS NULL THEN
16:22:02 798  	      RAISE OCH_IS_NOT_GIFT_CERTIFICATE;
16:22:02 799  	    END IF;
16:22:02 800  
16:22:02 801  	    IF var_offer_chain_status_id != GLOBAL_STATUSES_V22.OFFER_CHAIN_ACTIVE THEN
16:22:02 802  	      RAISE BAD_OFFER_CHAIN_STATUS;
16:22:02 803  	    END IF;
16:22:02 804  
16:22:02 805  -- norlov: #38151 check if the OC is for Redemption:
16:22:02 806  	    SELECT
16:22:02 807  	      COUNT(*) into var_is_for_redemption
16:22:02 808  	    FROM
16:22:02 809  	      OFFER_CHAIN_ELIGIBILITY
16:22:02 810  	    WHERE
16:22:02 811  	      OFFER_CHAIN_ELIGIBILITY.OFFER_CHAIN_ID = in_offer_chain_id
16:22:02 812  	      AND OFFER_CHAIN_ELIGIBILITY.NAME = GLOBAL_CONSTANTS_V22.GIFT_CERTIFICATE_REQUIRED
16:22:02 813  	      AND OFFER_CHAIN_ELIGIBILITY.VALUE = GLOBAL_CONSTANTS_V22.ELIGIBILITY_FLAG_SET;
16:22:02 814  
16:22:02 815  	    IF var_is_for_redemption > 0 THEN
16:22:02 816  	      RAISE CAN_NOT_PURCHASE_GC_FOR_RDMPN;
16:22:02 817  	    END IF;
16:22:02 818  
16:22:02 819  -- Create new invoice
16:22:02 820  	    BEGIN
16:22:02 821  	      PROCS_INVOICE_V22.CREATE_INVOICE(
16:22:02 822  		  in_invoice_status => GLOBAL_STATUSES_V22.INVOICE_OPEN,
16:22:02 823  		  in_created_by     => in_created_by,
16:22:02 824  		  in_tax_exempt_id  => var_account_tax_exempt_id,
16:22:02 825  		  out_invoice_id    => var_new_invoice_id
16:22:02 826  	      );
16:22:02 827  	      EXCEPTION
16:22:02 828  	      WHEN OTHERS THEN
16:22:02 829  	      EXCEPTION_MESSAGE := SQLERRM;
16:22:02 830  	      RAISE CAN_NOT_CREATE_INVOICE;
16:22:02 831  	    END;
16:22:02 832  
16:22:02 833  -- Add line items for new invoice
16:22:02 834  	    BEGIN
16:22:02 835  	      FOR f_offer_data IN (
16:22:02 836  	      SELECT
16:22:02 837  		OFFER_ID,
16:22:02 838  		NUM_RECURRENCES
16:22:02 839  	      FROM
16:22:02 840  		OFFER_OFFER_CHAIN
16:22:02 841  	      WHERE
16:22:02 842  		OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
16:22:02 843  	      )
16:22:02 844  	      LOOP
16:22:02 845  	      IF f_offer_data.NUM_RECURRENCES < 1 THEN
16:22:02 846  		RAISE OFFER_REC_NUM_LESS_THAN_ONE;
16:22:02 847  	      END IF;
16:22:02 848  	      FOR i_offer_recurrences_iterator IN 1..f_offer_data.NUM_RECURRENCES
16:22:02 849  	      LOOP
16:22:02 850  		PROCS_LINE_ITEMS_V22.ADD_LINE_ITEMS(
16:22:02 851  		    in_invoice_id => var_new_invoice_id,
16:22:02 852  		    in_offer_id   => f_offer_data.OFFER_ID,
16:22:02 853  		    in_created_by => in_created_by
16:22:02 854  		);
16:22:02 855  	      END LOOP;
16:22:02 856  	      END LOOP;
16:22:02 857  
16:22:02 858  	      EXCEPTION
16:22:02 859  	      WHEN OTHERS THEN
16:22:02 860  	      EXCEPTION_MESSAGE := SQLERRM;
16:22:02 861  	      RAISE CAN_NOT_CREATE_LINE_ITEMS;
16:22:02 862  	    END;
16:22:02 863  
16:22:02 864  	    BEGIN
16:22:02 865  	      PROCS_FIN_INSTRUMENTS_CRU_V22.CREATE_GIFT_CERTIFICATE(
16:22:02 866  		  out_gift_certificate_id	=> var_gift_cert_id,
16:22:02 867  		  in_purchaser_group_id 	=> in_group_id,
16:22:02 868  		  in_purchaser_invoice_id	=> var_new_invoice_id,
16:22:02 869  		  in_offer_chain_id		=> in_offer_chain_id,
16:22:02 870  		  in_expiration_date		=> in_expiration_date,
16:22:02 871  		  in_purchase_date		=> SYSDATE,
16:22:02 872  		  in_gift_certificate_status_id => GLOBAL_STATUSES_V22.GIFT_CERTIFICATE_ACTIVE,
16:22:02 873  		  in_code			=> in_gift_certificate_code,
16:22:02 874  		  in_created_by 		=> in_created_by,
16:22:02 875  		  in_recipient_name		=> in_recipient_name,
16:22:02 876  		  in_gift_message		=> in_gift_message,
16:22:02 877  		  in_recipient_email		=> in_recipient_email,
16:22:02 878  		  in_sender_email		=> in_sender_email,
16:22:02 879  		  in_sender_name		=> in_sender_name,
16:22:02 880  		  in_recipient_address_id	=> in_recipient_address_id,
16:22:02 881  		  in_recipient_notify_date	=> in_recipient_notify_date,
16:22:02 882  		  in_campaign			=> in_campaign,
16:22:02 883  		  in_reference_code		=> in_reference_code
16:22:02 884  	      );
16:22:02 885  	      EXCEPTION WHEN DUP_VAL_ON_INDEX THEN RAISE REF_CODE_ALREADY_EXISTS;
16:22:02 886  	    END;
16:22:02 887  
16:22:02 888  	    out_gift_certificate_id := var_gift_cert_id;
16:22:02 889  	    out_invoice_id := var_new_invoice_id;
16:22:02 890  
16:22:02 891  	    EXCEPTION
16:22:02 892  	    WHEN BAD_OFFER_CHAIN_STATUS THEN
16:22:02 893  	    PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.STATE_ERROR,
16:22:02 894  					     SPROC_NAME, 'Offer chain is not active');
16:22:02 895  	    WHEN GC_CODE_ALREADY_EXISTS THEN
16:22:02 896  	    PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.DUPLICATE_ERROR,
16:22:02 897  					     SPROC_NAME, 'Gift certificate with same code already exists');
16:22:02 898  	    WHEN OCH_IS_NOT_GIFT_CERTIFICATE THEN
16:22:02 899  	    PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INVALID_PARAMETER,
16:22:02 900  					     SPROC_NAME, 'This offer chain can not be used for gift certificate');
16:22:02 901  	    WHEN CAN_NOT_PURCHASE_GC_FOR_RDMPN THEN
16:22:02 902  	    PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INVALID_PARAMETER,
16:22:02 903  					     SPROC_NAME, 'This offer chain can not be purchased for gift certificate since it is for redemption');
16:22:02 904  	    WHEN CAN_NOT_CREATE_INVOICE THEN
16:22:02 905  	    PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:02 906  					     SPROC_NAME, 'Could not create new invoice', EXCEPTION_MESSAGE);
16:22:02 907  	    WHEN CAN_NOT_CREATE_LINE_ITEMS THEN
16:22:02 908  	    PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:02 909  					     SPROC_NAME, 'Could not create line items', EXCEPTION_MESSAGE);
16:22:02 910  	    WHEN BAD_GROUP_ID THEN
16:22:02 911  	    PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:02 912  					     SPROC_NAME, 'No such group id');
16:22:02 913  	    WHEN BAD_OFFER_CHAIN_ID THEN
16:22:02 914  	    PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:02 915  					     SPROC_NAME, 'No such offer chain');
16:22:02 916  	    WHEN OFFER_REC_NUM_LESS_THAN_ONE THEN
16:22:02 917  	    PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:02 918  					     SPROC_NAME, 'Some offer has recurrences number less than 1');
16:22:02 919  	    WHEN REF_CODE_ALREADY_EXISTS THEN
16:22:02 920  	    PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:02 921  					     SPROC_NAME, 'reference_code already used');
16:22:02 922  	    WHEN OTHERS THEN
16:22:02 923  	    PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:02 924  					     SPROC_NAME, 'Unknown error', SQLERRM);
16:22:02 925  	  END START_GC_PURCHASING;
16:22:02 926  /******************************************************************************/
16:22:02 927  
16:22:02 928  	PROCEDURE FINALIZE_GC_PURCHASING (
16:22:02 929  	  in_invoice_id 	IN NUMBER,
16:22:02 930  	  in_created_by 	IN VARCHAR2,
16:22:02 931  	  in_instrument_id	IN NUMBER,
16:22:02 932  	  in_instrument_type_id IN NUMBER,
16:22:02 933  	  in_order_id		IN VARCHAR2,
16:22:02 934  	  in_transaction_id	IN NUMBER,
16:22:02 935  	  out_charge_amount	OUT NUMBER
16:22:02 936  	) AS
16:22:02 937  	  SPROC_NAME CONSTANT VARCHAR2(22) := 'FINALIZE_GC_PURCHASING';
16:22:02 938  -- VARIABLES
16:22:02 939  	  temp_transaction_id_count NUMBER;
16:22:02 940  	  var_invoice_amount	    NUMBER(10,2);
16:22:02 941  	  var_transaction_id	    NUMBER;
16:22:02 942  	  var_new_charge_id	    NUMBER;
16:22:02 943  -- EXCEPTIONS
16:22:02 944  	    BAD_CREDIT_CARD_ID		EXCEPTION;
16:22:02 945  	    BAD_PAYPAL_ID		EXCEPTION;
16:22:02 946  	    BAD_INSTRUMENT_TYPE 	EXCEPTION;
16:22:02 947  	    TRANSACTION_EXISTS		EXCEPTION;
16:22:02 948  	    CAN_NOT_CALC_INVOICE_AMOUNT EXCEPTION;
16:22:02 949  	    CAN_NOT_USE_FCINSTR 	EXCEPTION;
16:22:02 950  	    CAN_NOT_CREATE_TRANSACTION	EXCEPTION;
16:22:02 951  	    CAN_NOT_CREATE_CHARGE	EXCEPTION;
16:22:02 952  	  EXCEPTION_MESSAGE   VARCHAR2(1024);
16:22:02 953  	  BEGIN
16:22:02 954  
16:22:02 955  -- Check that instrument exists
16:22:02 956  	    IF in_instrument_type_id = GLOBAL_ENUMS_V22.INSTRUMENT_CREDIT_CARD THEN
16:22:02 957  	      IF IS_CREDIT_CARD_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V22.FALSE THEN
16:22:02 958  		RAISE BAD_CREDIT_CARD_ID;
16:22:02 959  	      END IF;
16:22:02 960  	    ELSIF in_instrument_type_id = GLOBAL_ENUMS_V22.INSTRUMENT_PAYPAL THEN
16:22:02 961  	      IF IS_PAYPAL_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V22.FALSE THEN
16:22:02 962  		RAISE BAD_PAYPAL_ID;
16:22:02 963  	      END IF;
16:22:02 964  	    ELSIF in_instrument_type_id = GLOBAL_ENUMS_V22.INSTRUMENT_ZCI_INSTRUMENT THEN
16:22:02 965  	      NULL;
16:22:02 966  	    ELSE
16:22:02 967  	      RAISE BAD_INSTRUMENT_TYPE;
16:22:02 968  	    END IF;
16:22:02 969  
16:22:02 970  -- Check that transaction with given id do not exists
16:22:02 971  	    SELECT
16:22:02 972  	      COUNT(*) into temp_transaction_id_count
16:22:02 973  	    FROM
16:22:02 974  	      TRANSACTION
16:22:02 975  	    WHERE
16:22:02 976  	      TRANSACTION.ID = in_transaction_id;
16:22:02 977  
16:22:02 978  	    IF temp_transaction_id_count > 0 THEN
16:22:02 979  	      RAISE TRANSACTION_EXISTS;
16:22:02 980  	    END IF;
16:22:02 981  
16:22:02 982  -- Calculate new invoice amount
16:22:02 983  	    BEGIN
16:22:02 984  	      PROCS_INVOICE_V22.CALCULATE_INVOICE_AMOUNT(in_invoice_id, var_invoice_amount);
