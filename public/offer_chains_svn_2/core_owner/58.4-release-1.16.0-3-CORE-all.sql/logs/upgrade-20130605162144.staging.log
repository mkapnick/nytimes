16:22:02 384  ) AS
16:22:02 385  SPROC_NAME     CONSTANT VARCHAR2(29) := 'GET_ACCOUNT_GIFT_CERTIFICATES';
16:22:02 386  var_account_id NUMBER;
16:22:02 387  
16:22:02 388  -- Exceptions
16:22:02 389  WRONG_INSTR_EXCEPTION	 EXCEPTION;
16:22:02 390  BEGIN
16:22:02 391  
16:22:02 392  	-- Get account id
16:22:02 393  	SELECT
16:22:02 394  	  ACCOUNT.ID INTO var_account_id
