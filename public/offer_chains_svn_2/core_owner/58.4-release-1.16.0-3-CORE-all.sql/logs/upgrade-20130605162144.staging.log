16:22:02 207  	END IF;
16:22:02 208  
16:22:02 209  	PROCS_ACCOUNT_V22.UPDATE_ACCOUNT_STATUS(
16:22:02 210  	  in_account_id        => var_account_id,
16:22:02 211  	  in_account_status_id => GLOBAL_STATUSES_V22.ACCOUNT_DISABLED,
16:22:02 212  	  in_updated_by        => in_updated_by
