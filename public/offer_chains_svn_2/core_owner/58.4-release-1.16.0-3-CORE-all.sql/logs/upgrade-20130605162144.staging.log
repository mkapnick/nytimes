16:22:02 238  PROCEDURE GET_ACCOUNT_BY_INVOICE_ID (
16:22:02 239  /*
16:22:02 240  Throws exceptions:
16:22:02 241  APP_EXCEPTION_CODES_V22.NOT_FOUND
16:22:02 242  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
16:22:02 243  */
16:22:02 244  	in_invoice_id  IN  NUMBER,
16:22:02 245  	out_account_id OUT NUMBER
16:22:02 246  ) AS
16:22:02 247  -- VARIABLES
16:22:02 248  SPROC_NAME	 CONSTANT VARCHAR2(25) := 'GET_ACCOUNT_BY_INVOICE_ID';
16:22:02 249  temp_gc_account_id NUMBER;
16:22:02 250  temp_ss_account_id NUMBER;
16:22:02 251  temp_invoice_id	 NUMBER;
16:22:02 252  -- EXCEPTIONS
16:22:02 253  BAD_INVOICE_ID	   EXCEPTION;
16:22:02 254  CAN_NOT_FIND_ACCOUNT EXCEPTION;
16:22:02 255  BEGIN
16:22:02 256  
16:22:02 257  	-- Check that given invoice exists
16:22:02 258  	BEGIN
16:22:02 259  	  SELECT
16:22:02 260  	    INVOICE.ID into temp_invoice_id
16:22:02 261  	  FROM
16:22:02 262  	    INVOICE
16:22:02 263  	  WHERE
16:22:02 264  	    INVOICE.ID = in_invoice_id;
16:22:02 265  	  EXCEPTION
16:22:02 266  	    WHEN NO_DATA_FOUND THEN
16:22:02 267  	      RAISE BAD_INVOICE_ID;
16:22:02 268  	END;
16:22:02 269  
16:22:02 270  	-- Try to find gift certificate with given invoice
16:22:02 271  	BEGIN
16:22:02 272  	  SELECT
16:22:02 273  	    ACCOUNT.GROUP_ID into temp_gc_account_id
16:22:02 274  	  FROM
16:22:02 275  	    GIFT_CERTIFICATE
16:22:02 276  	    INNER JOIN ACCOUNT ON GIFT_CERTIFICATE.PURCHASER_GROUP_ID = ACCOUNT.GROUP_ID
16:22:02 277  	  WHERE
16:22:02 278  	    GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = in_invoice_id
16:22:02 279  	    AND ROWNUM <= 1;
16:22:02 280  	  EXCEPTION
16:22:02 281  	    WHEN NO_DATA_FOUND THEN
16:22:02 282  	      temp_gc_account_id := NULL;
16:22:02 283  	END;
16:22:02 284  
16:22:02 285  	-- check subscriptions for given invoice
16:22:02 286  	IF temp_gc_account_id IS NOT NULL THEN
16:22:02 287  	  out_account_id := temp_gc_account_id;
16:22:02 288  	ELSE
16:22:02 289  	  BEGIN
16:22:02 290  	    SELECT
16:22:02 291  	      ACCOUNT.GROUP_ID into temp_ss_account_id
16:22:02 292  	    FROM
16:22:02 293  	      LICENSE
16:22:02 294  	      INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
16:22:02 295  	      INNER JOIN ACCOUNT ON SUBSCRIPTION.ACCOUNT_ID = ACCOUNT.ID
16:22:02 296  	    WHERE
16:22:02 297  	      LICENSE.INVOICE_ID = in_invoice_id
16:22:02 298  	      AND ROWNUM <= 1;
16:22:02 299  	    EXCEPTION
16:22:02 300  	      WHEN NO_DATA_FOUND THEN
16:22:02 301  		temp_ss_account_id := NULL;
16:22:02 302  	  END;
16:22:02 303  	  IF temp_ss_account_id IS NULL THEN
16:22:02 304  	    RAISE CAN_NOT_FIND_ACCOUNT;
16:22:02 305  	  ELSE
16:22:02 306  	    out_account_id := temp_ss_account_id;
16:22:02 307  	  END IF;
16:22:02 308  	END IF;
16:22:02 309  
16:22:02 310  EXCEPTION
16:22:02 311  WHEN BAD_INVOICE_ID THEN
16:22:02 312  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:02 313  	  SPROC_NAME, 'No such invoice');
16:22:02 314  WHEN CAN_NOT_FIND_ACCOUNT THEN
16:22:02 315  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:02 316  	  SPROC_NAME, 'Could not find account by given invoice id');
16:22:02 317  WHEN OTHERS THEN
16:22:02 318  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:02 319  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:22:02 320  END GET_ACCOUNT_BY_INVOICE_ID;
