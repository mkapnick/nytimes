16:22:02 1358  WHEN BAD_OFFER_CHAIN_STATUS THEN
16:22:02 1359  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.STATE_ERROR,
16:22:02 1360  	   SPROC_NAME, 'Offer chain is not active');
16:22:02 1361  WHEN CAN_NOT_CALCULATE_OCH_AMOUNT THEN
16:22:02 1362  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:02 1363  	   SPROC_NAME, 'Could not calculate offer chain amount', EXCEPTION_MESSAGE);
16:22:02 1364  WHEN BAD_CREDIT_CARD_ID THEN
16:22:02 1365  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:02 1366  	   SPROC_NAME, 'Bad credit card id');
16:22:02 1367  WHEN BAD_PAYPAL_ID THEN
16:22:02 1368  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:02 1369  	   SPROC_NAME, 'Bad paypal id');
16:22:02 1370  WHEN BAD_INSTRUMENT_TYPE THEN
16:22:02 1371  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INVALID_PARAMETER,
16:22:02 1372  	   SPROC_NAME, 'Bad instrument type');
16:22:02 1373  WHEN GC_CODE_ALREADY_EXISTS THEN
16:22:02 1374  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.DUPLICATE_ERROR,
16:22:02 1375  	   SPROC_NAME, 'Gift certificate with same code already exists');
16:22:02 1376  WHEN OCH_IS_NOT_GIFT_CERTIFICATE THEN
16:22:02 1377  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INVALID_PARAMETER,
16:22:02 1378  	   SPROC_NAME, 'This offer chain can not be used for gift certificate');
16:22:02 1379  WHEN CAN_NOT_PURCHASE_GC_FOR_RDMPN THEN
16:22:02 1380  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INVALID_PARAMETER,
16:22:02 1381  	   SPROC_NAME, 'This offer chain can not be purchased for gift certificate since it is for redemption');
16:22:02 1382  WHEN TRANSACTION_EXISTS THEN
16:22:02 1383  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.DUPLICATE_ERROR,
16:22:02 1384  	   SPROC_NAME, 'Transaction with given id already exists');
16:22:02 1385  WHEN CAN_NOT_CREATE_INVOICE THEN
16:22:02 1386  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:02 1387  	   SPROC_NAME, 'Could not create new invoice', EXCEPTION_MESSAGE);
16:22:02 1388  WHEN CAN_NOT_CREATE_TRANSACTION THEN
16:22:02 1389  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:02 1390  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
16:22:02 1391  WHEN CAN_NOT_CREATE_CHARGE THEN
16:22:02 1392  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:02 1393  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
16:22:02 1394  WHEN CAN_NOT_CREATE_LINE_ITEMS THEN
16:22:02 1395  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:02 1396  	   SPROC_NAME, 'Could not create line items', EXCEPTION_MESSAGE);
16:22:02 1397  WHEN BAD_GROUP_ID THEN
16:22:02 1398  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:02 1399  	   SPROC_NAME, 'No such group id');
16:22:02 1400  WHEN BAD_OFFER_CHAIN_ID THEN
16:22:02 1401  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:02 1402  	   SPROC_NAME, 'No such offer chain');
16:22:02 1403  WHEN OFFER_REC_NUM_LESS_THAN_ONE THEN
16:22:02 1404  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:02 1405  	   SPROC_NAME, 'Some offer has recurrences number less than 1');
16:22:02 1406  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
16:22:02 1407  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:02 1408  	   SPROC_NAME, 'COuold not calculate amount for new invoice', EXCEPTION_MESSAGE);
16:22:02 1409  WHEN OTHERS THEN
16:22:02 1410  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:02 1411  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:22:02 1412  END PURCHASE_GIFT_CERTIFICATE;
16:22:02 1413  
16:22:02 1414  /******************************************************************************/
16:22:02 1415  
16:22:02 1416  PROCEDURE REDEEM_GIFT_CERTIFICATE (
16:22:02 1417  	 in_group_id			 IN NUMBER,
16:22:02 1418  	 in_gift_certificate_code	 IN VARCHAR2,
16:22:02 1419  	 in_created_by			 IN VARCHAR2,
16:22:02 1420  	 in_redeemer_address_id 	 IN NUMBER,
16:22:02 1421  	 in_fin_instrument_id		 IN NUMBER,
16:22:02 1422  	 in_fin_instrument_type_id	 IN NUMBER,
16:22:02 1423  	 in_redemption_offer_chain_id	 IN NUMBER,
16:22:02 1424  	 out_subscription_id		 OUT NUMBER,
16:22:02 1425  	 out_license_id 		 OUT NUMBER
16:22:02 1426  ) AS
16:22:02 1427  SPROC_NAME	       CONSTANT VARCHAR2(23) := 'REDEEM_GIFT_CERTIFICATE';
16:22:02 1428  -- VARIABLES
16:22:02 1429  var_gift_certificate_id NUMBER;
16:22:02 1430  -- norlov: #38151 var_offer_chain_id replaced by var_purchased_oc_id and var_oc_id_to_redeem
16:22:02 1431  var_purchased_oc_id     NUMBER;
16:22:02 1432  var_oc_id_to_redeem     NUMBER := in_redemption_offer_chain_id;
16:22:02 1433  var_offer_duration      VARCHAR2(30);
16:22:02 1434  var_invoice_id	       NUMBER;
16:22:02 1435  var_succ_purch_attempts_num NUMBER;
16:22:02 1436  var_subscription_id     NUMBER;
16:22:02 1437  var_license_id	       NUMBER;
16:22:02 1438  var_account_id	       NUMBER;
16:22:02 1439  var_gc_status_id        NUMBER;
16:22:02 1440  var_gc_charges_amount   NUMBER;
16:22:02 1441  var_gc_expiration_date  DATE;
16:22:02 1442  var_gc_redeemer_group_id NUMBER;
16:22:02 1443  var_gc_purchase_invoice_id NUMBER;
16:22:02 1444  var_gc_purchase_inv_status_id NUMBER;
16:22:02 1445  var_offer_index		     NUMBER;
16:22:02 1446  var_purchaser_group_id	     NUMBER;
16:22:02 1447  temp_license_id		     NUMBER;
16:22:02 1448  var_same_offer_chains_num     NUMBER;
16:22:02 1449  var_max_concurrent_subscrs    NUMBER;
16:22:02 1450  var_account_tax_exempt_id     VARCHAR2(255);
16:22:02 1451  var_fin_instrument_type_id    NUMBER := in_fin_instrument_type_id;
16:22:02 1452  var_fin_instrument_id	     NUMBER := in_fin_instrument_id;
16:22:02 1453  var_first_offer_id	  NUMBER;
16:22:02 1454  var_date 	     DATE := SYSDATE;
16:22:02 1455  
16:22:02 1456  var_offers SYS_REFCURSOR;
16:22:02 1457  
16:22:02 1458  -- EXCEPTIONS
16:22:02 1459  BAD_GIFT_CERTIFICATE_CODE      EXCEPTION;
16:22:02 1460  BAD_GROUP_ID		      EXCEPTION;
16:22:02 1461  CAN_NOT_CREATE_LICENSE	      EXCEPTION;
16:22:02 1462  GIFT_CERT_IS_FINALIZED	      EXCEPTION;
16:22:02 1463  GIFT_CERT_IS_REFUNDED	      EXCEPTION;
16:22:02 1464  CAN_NOT_UPDATE_CERTIFICATE     EXCEPTION;
16:22:02 1465  GIFT_CERTIFICATE_EXPIRED       EXCEPTION;
16:22:02 1466  GIFT_CERTIFICATE_REDEEMED      EXCEPTION;
16:22:02 1467  USER_ALREADY_SUBSCRIBED_TO_PRD EXCEPTION;
16:22:02 1468  LIMIT_REACHED		      EXCEPTION;
16:22:02 1469  GC_PURCHASE_INVOICE_NOT_CLOSED EXCEPTION;
16:22:02 1470  PURCHASE_INVOICES_NOT_PAID     EXCEPTION;
16:22:02 1471  OC_TO_REDEEM_NOT_FOUND	      EXCEPTION;
16:22:02 1472  CAN_NOT_GET_FIRST_OFFER_CHAIN  EXCEPTION;
16:22:02 1473  EXCEPTION_MESSAGE	      VARCHAR2(1024);
16:22:02 1474  BEGIN
16:22:02 1475  
16:22:02 1476  	 -- Get account id
16:22:02 1477  	 BEGIN
16:22:02 1478  	   SELECT
16:22:02 1479  	     ACCOUNT.ID,
16:22:02 1480  	     ACCOUNT.TAX_EXEMPT_ID
16:22:02 1481  	     into
16:22:02 1482  	     var_account_id,
16:22:02 1483  	     var_account_tax_exempt_id
16:22:02 1484  	   FROM
16:22:02 1485  	     ACCOUNT
16:22:02 1486  	   WHERE
16:22:02 1487  	     ACCOUNT.GROUP_ID = in_group_id
16:22:02 1488  	     AND ROWNUM <= 1;
16:22:02 1489  
16:22:02 1490  	   EXCEPTION
16:22:02 1491  	   WHEN NO_DATA_FOUND THEN
16:22:02 1492  	     RAISE BAD_GROUP_ID;
16:22:02 1493  	 END;
16:22:02 1494  
16:22:02 1495  	 -- Get gift certificate data
16:22:02 1496  	 BEGIN
16:22:02 1497  	   SELECT
16:22:02 1498  	     GIFT_CERTIFICATE.ID,
16:22:02 1499  	     GIFT_CERTIFICATE.OFFER_CHAIN_ID,
16:22:02 1500  	     GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID,
16:22:02 1501  	     GIFT_CERTIFICATE.PURCHASER_GROUP_ID,
16:22:02 1502  	     GIFT_CERTIFICATE.EXPIRATION_DATE,
16:22:02 1503  	     GIFT_CERTIFICATE.REDEEMER_GROUP_ID,
16:22:02 1504  	     GIFT_CERTIFICATE.PURCHASE_INVOICE_ID
16:22:02 1505  	     into
16:22:02 1506  	     var_gift_certificate_id,
16:22:02 1507  	     var_purchased_oc_id,
16:22:02 1508  	     var_gc_status_id,
16:22:02 1509  	     var_purchaser_group_id,
16:22:02 1510  	     var_gc_expiration_date,
16:22:02 1511  	     var_gc_redeemer_group_id,
16:22:02 1512  	     var_gc_purchase_invoice_id
16:22:02 1513  	   FROM
16:22:02 1514  	     GIFT_CERTIFICATE
16:22:02 1515  	   WHERE
16:22:02 1516  	     GIFT_CERTIFICATE.CODE = in_gift_certificate_code
16:22:02 1517  	     AND ROWNUM <= 1;
16:22:02 1518  
16:22:02 1519  	   EXCEPTION
16:22:02 1520  	   WHEN NO_DATA_FOUND THEN
16:22:02 1521  	     RAISE BAD_GIFT_CERTIFICATE_CODE;
16:22:02 1522  	 END;
16:22:02 1523  
16:22:02 1524  	 -- get redemption oc id from meta data if it wasn't passed in, parsing will fail for gcs with multiple redemption offer chains,
16:22:02 1525  	 -- but in that case a redemption offer chain id should always be passed in
16:22:02 1526  	 IF var_oc_id_to_redeem IS NULL THEN
16:22:02 1527  	   BEGIN
16:22:02 1528  	     SELECT
16:22:02 1529  	       to_number(OFFER_CHAIN_META_DATA.VALUE)
16:22:02 1530  	       into
16:22:02 1531  	       var_oc_id_to_redeem
16:22:02 1532  	     FROM
16:22:02 1533  	       OFFER_CHAIN_META_DATA
16:22:02 1534  	     WHERE
16:22:02 1535  	       OFFER_CHAIN_META_DATA.OFFER_CHAIN_ID = var_purchased_oc_id
16:22:02 1536  	       AND OFFER_CHAIN_META_DATA.NAME = GLOBAL_CONSTANTS_V22.REDEMPTION_OC_ID
16:22:02 1537  	       AND ROWNUM = 1;
16:22:02 1538  -- requested by ticket so (but above is correct for the actual migrated data):
16:22:02 1539  --  SELECT
16:22:02 1540  --      OFFER_CHAIN.ID
16:22:02 1541  --      into
16:22:02 1542  --      var_oc_id_to_redeem
16:22:02 1543  --    FROM
16:22:02 1544  --      OFFER_CHAIN
16:22:02 1545  --	 INNER JOIN ELIGIBILITY ON OFFER_CHAIN.ID = ELIGIBILITY.OFFER_CHAIN_ID
16:22:02 1546  --	 INNER JOIN OFFER_CHAIN_META_DATA ON OFFER_CHAIN.ID = OFFER_CHAIN_META_DATA.OFFER_CHAIN_ID
16:22:02 1547  --    WHERE
16:22:02 1548  --      ELIGIBILITY.OFFER_CHAIN_ID = OFFER_CHAIN.ID
16:22:02 1549  --      AND ELIGIBILITY.NAME = GLOBAL_CONSTANTS_V22.GIFT_CERTIFICATE_REQUIRED
16:22:02 1550  --      AND ELIGIBILITY.VALUE = GLOBAL_CONSTANTS_V22.ELIGIBILITY_FLAG_SET
16:22:02 1551  --      AND OFFER_CHAIN_META_DATA.OFFER_CHAIN_ID = OFFER_CHAIN.ID
16:22:02 1552  --      AND OFFER_CHAIN_META_DATA.NAME = GLOBAL_CONSTANTS_V22.REDEMPTION_OC_ID
16:22:02 1553  --      AND to_number(OFFER_CHAIN_META_DATA.VALUE) = var_purchased_oc_id
16:22:02 1554  --      AND ROWNUM = 1;
16:22:02 1555  
16:22:02 1556  	     EXCEPTION
16:22:02 1557  	     WHEN NO_DATA_FOUND THEN
16:22:02 1558  	       RAISE OC_TO_REDEEM_NOT_FOUND;
16:22:02 1559  	   END;
16:22:02 1560  	 END IF;
16:22:02 1561  
16:22:02 1562  	 -- Check that purchase invoice for this GC was closed
16:22:02 1563  	 SELECT
16:22:02 1564  	   INVOICE.INVOICE_STATUS_ID into var_gc_purchase_inv_status_id
16:22:02 1565  	 FROM
16:22:02 1566  	   INVOICE
16:22:02 1567  	 WHERE
16:22:02 1568  	   INVOICE.ID = var_gc_purchase_invoice_id;
16:22:02 1569  
16:22:02 1570  	 IF var_gc_purchase_inv_status_id != GLOBAL_STATUSES_V22.INVOICE_CLOSED THEN
16:22:02 1571  	   RAISE GC_PURCHASE_INVOICE_NOT_CLOSED;
16:22:02 1572  	 END IF;
16:22:02 1573  
16:22:02 1574  	 -- Check that this invoice was successfully processed by billing
16:22:02 1575  	 SELECT
16:22:02 1576  	   COUNT(1) into var_succ_purch_attempts_num
16:22:02 1577  	 FROM
16:22:02 1578  	   TRANSACTION_ATTEMPT TA
16:22:02 1579  	   INNER JOIN TRANSACTION T ON T.ID = TA.TRANSACTION_ID
16:22:02 1580  	   INNER JOIN CHARGE CH ON CH.TRANSACTION_ID = T.ID
16:22:02 1581  	 WHERE
16:22:02 1582  	   CH.INVOICE_ID = var_gc_purchase_invoice_id
16:22:02 1583  	   AND TA.TRANSACTION_ATTEMPT_STATUS_ID = GLOBAL_STATUSES_V22.TRANS_ATTEMPT_SUCCESS;
16:22:02 1584  
16:22:02 1585  	 IF var_succ_purch_attempts_num = 0 THEN
16:22:02 1586  	   SELECT
16:22:02 1587  	     COUNT(1) into var_succ_purch_attempts_num
16:22:02 1588  	   FROM
16:22:02 1589  	     DUAL
16:22:02 1590  	   WHERE
16:22:02 1591  	     PROCS_INVOICE_V22.F_CALCULATE_INVOICE_AMOUNT(var_gc_purchase_invoice_id) = 0;
16:22:02 1592  	 END IF;
16:22:02 1593  
16:22:02 1594  	 IF var_succ_purch_attempts_num = 0 THEN
16:22:02 1595  	   RAISE PURCHASE_INVOICES_NOT_PAID;
16:22:02 1596  	 END IF;
16:22:02 1597  
16:22:02 1598  	 -- Check limit for gc's offer chain
16:22:02 1599  	 SELECT
16:22:02 1600  	   COUNT(*) into var_same_offer_chains_num
16:22:02 1601  	 FROM
16:22:02 1602  	   SUBSCRIPTION
16:22:02 1603  	 WHERE
16:22:02 1604  	   SUBSCRIPTION.ACCOUNT_ID = var_account_id
16:22:02 1605  	   AND SUBSCRIPTION.OFFER_CHAIN_ID = var_oc_id_to_redeem
16:22:02 1606  	   AND (
16:22:02 1607  	     SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE
16:22:02 1608  	     OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_IN_GRACE_PERIOD);
16:22:02 1609  IF var_same_offer_chains_num = 0 THEN
16:22:02 1610  	   -- if user does not have any active existing subscriptions to the offer chain
16:22:02 1611  	   -- and if product from the offer chain is already owned from another offer chain
16:22:02 1612  	   -- then deny purchase
16:22:02 1613  	   FOR f_account_offer_chains IN (
16:22:02 1614  	     SELECT DISTINCT
16:22:02 1615  	       OFFER_CHAIN_ID
16:22:02 1616  	     FROM
16:22:02 1617  	       SUBSCRIPTION
16:22:02 1618  	     WHERE
16:22:02 1619  	       ACCOUNT_ID = var_account_id
16:22:02 1620  	       AND (
16:22:02 1621  		 SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE
16:22:02 1622  		 OR SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_SUSPENDED
16:22:02 1623  		 OR SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_IN_GRACE_PERIOD)
16:22:02 1624  	   )
16:22:02 1625  	   LOOP
16:22:02 1626  	     IF PROCS_OFFER_CHAIN_V22.CHECK_FOR_SAME_PRODUCTS(var_oc_id_to_redeem, f_account_offer_chains.OFFER_CHAIN_ID) = GLOBAL_CONSTANTS_V22.TRUE THEN
16:22:02 1627  	       RAISE USER_ALREADY_SUBSCRIBED_TO_PRD;
16:22:02 1628  	     END IF;
16:22:02 1629  	   END LOOP;
16:22:02 1630  	 ELSE
16:22:02 1631  
16:22:02 1632  	   -- if user have any active existing subscriptions to the offer chain
16:22:02 1633  	   -- and if MAX_CONCURRENT_SUBS <= [user's subscription count for the offer chain]
16:22:02 1634  	   -- then deny purchase
16:22:02 1635  	   var_max_concurrent_subscrs := PROCS_OFFER_CHAIN_V22.GET_OFFER_CHAIN_MAX_CONC_SUBSC(var_oc_id_to_redeem);
16:22:02 1636  	   IF var_max_concurrent_subscrs != GLOBAL_CONSTANTS_V22.INFINITY
16:22:02 1637  	     AND var_max_concurrent_subscrs <= var_same_offer_chains_num THEN
16:22:02 1638  	     RAISE LIMIT_REACHED;
16:22:02 1639  	   END IF;
16:22:02 1640  	 END IF;
16:22:02 1641  -- norlov: END OF TODO
16:22:02 1642  
16:22:02 1643  
16:22:02 1644  	 IF var_gc_redeemer_group_id IS NOT NULL THEN
16:22:02 1645  	   RAISE GIFT_CERTIFICATE_REDEEMED;
16:22:02 1646  	 END IF;
16:22:02 1647  
16:22:02 1648  	 IF var_gc_expiration_date < sysdate THEN
16:22:02 1649  	   RAISE GIFT_CERTIFICATE_EXPIRED;
16:22:02 1650  	 END IF;
16:22:02 1651  
16:22:02 1652  	  IF var_gc_status_id = GLOBAL_STATUSES_V22.GIFT_CERTIFICATE_REFUNDED THEN
16:22:02 1653  	   RAISE GIFT_CERT_IS_REFUNDED;
16:22:02 1654  	 END IF;
16:22:02 1655  
16:22:02 1656  	 IF var_gc_status_id = GLOBAL_STATUSES_V22.GIFT_CERTIFICATE_FINALIZED THEN
16:22:02 1657  	   RAISE GIFT_CERT_IS_FINALIZED;
16:22:02 1658  	 END IF;
16:22:02 1659  
16:22:02 1660  	 -- Check that user did not subscribed to same product already
16:22:02 1661  	 -- norlov: get rid of this since there is already the check?
16:22:02 1662  	 FOR f_user_offer_chain IN (
16:22:02 1663  	   SELECT DISTINCT
16:22:02 1664  	     OFFER_CHAIN_ID
16:22:02 1665  	   FROM
16:22:02 1666  	     SUBSCRIPTION
16:22:02 1667  	   WHERE
16:22:02 1668  	     ACCOUNT_ID=var_account_id
16:22:02 1669  	     AND (
16:22:02 1670  	       SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE
16:22:02 1671  	       OR SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_SUSPENDED
16:22:02 1672  	       OR SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_IN_GRACE_PERIOD)
16:22:02 1673  	 )
16:22:02 1674  	 LOOP
16:22:02 1675  	   IF PROCS_OFFER_CHAIN_V22.CHECK_FOR_SAME_PRODUCTS(var_oc_id_to_redeem, f_user_offer_chain.OFFER_CHAIN_ID)=GLOBAL_CONSTANTS_V22.TRUE THEN
16:22:02 1676  	     RAISE USER_ALREADY_SUBSCRIBED_TO_PRD;
16:22:02 1677  	   END IF;
16:22:02 1678  	 END LOOP;
16:22:02 1679  
16:22:02 1680  	 -- Check for gift certificate amount
16:22:02 1681  	 SELECT
16:22:02 1682  	   SUM(CHARGE.CHARGE_AMOUNT) into var_gc_charges_amount
16:22:02 1683  	 FROM
16:22:02 1684  	   CHARGE
16:22:02 1685  	 WHERE
16:22:02 1686  	   CHARGE.INSTRUMENT_ID = var_gift_certificate_id
16:22:02 1687  	   AND CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V22.INSTRUMENT_GIFT_CERTIFICATE;
16:22:02 1688  
16:22:02 1689  	 -- Create new invoice
16:22:02 1690  	 PROCS_INVOICE_V22.CREATE_INVOICE(
16:22:02 1691  	   out_invoice_id    => var_invoice_id,
16:22:02 1692  	   in_invoice_status => GLOBAL_STATUSES_V22.INVOICE_CLOSED,
16:22:02 1693  	   in_tax_exempt_id  => var_account_tax_exempt_id,
16:22:02 1694  	   in_created_by     => in_created_by
16:22:02 1695  	 );
16:22:02 1696  
16:22:02 1697  	 -- If a financial instrument wasn't passed in, use the gift certificate id
16:22:02 1698  	 -- Real financial instrument is required for upsell/till forbid gift subscriptions
16:22:02 1699  	 IF var_fin_instrument_id is null THEN
