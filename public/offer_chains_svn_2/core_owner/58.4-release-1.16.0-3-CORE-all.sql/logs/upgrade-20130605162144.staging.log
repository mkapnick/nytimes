16:22:02 226  		EXCEPTION_MESSAGE := SQLERRM;
16:22:02 227  		RAISE CAN_NOT_ANNOTATE_ACCOUNT;
16:22:02 228  	  END;
16:22:02 229  	END IF;
16:22:02 230  
16:22:02 231  EXCEPTION
