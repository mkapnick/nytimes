16:22:02 440  		  in_instr_status = GLOBAL_CONSTANTS_V22.FALSE AND
16:22:02 441  		  (
16:22:02 442  		    GIFT_CERTIFICATE.EXPIRATION_DATE < current_date
16:22:02 443  		    OR GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID != GLOBAL_STATUSES_V22.GIFT_CERTIFICATE_ACTIVE
16:22:02 444  		  )
