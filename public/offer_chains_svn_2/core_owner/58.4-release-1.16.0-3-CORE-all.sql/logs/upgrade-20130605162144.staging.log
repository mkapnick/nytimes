16:22:02 1288  CAN_NOT_FIND_ACCOUNT EXCEPTION;
16:22:02 1289  BEGIN
16:22:02 1290  
16:22:02 1291  	 BEGIN
16:22:02 1292  	   SELECT
16:22:02 1293  	     INVOICE.ID into temp_invoice_id
16:22:02 1294  	   FROM
16:22:02 1295  	     INVOICE
16:22:02 1296  	   WHERE
16:22:02 1297  	     INVOICE.ID = in_invoice_id;
16:22:02 1298  	   EXCEPTION
16:22:02 1299  	     WHEN NO_DATA_FOUND THEN
16:22:02 1300  	       RAISE BAD_INVOICE_ID;
16:22:02 1301  	 END;
16:22:02 1302  
16:22:02 1303  	 BEGIN
16:22:02 1304  	   SELECT DISTINCT
16:22:02 1305  	     ACCOUNT.GROUP_ID into var_subscrib_group_id
16:22:02 1306  	   FROM
16:22:02 1307  	     ACCOUNT
16:22:02 1308  	     INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.ACCOUNT_ID = ACCOUNT.ID
16:22:02 1309  	     INNER JOIN LICENSE ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
16:22:02 1310  	   WHERE
16:22:02 1311  	     LICENSE.INVOICE_ID = in_invoice_id;
16:22:02 1312  	   EXCEPTION
16:22:02 1313  	     WHEN NO_DATA_FOUND THEN
16:22:02 1314  	       var_subscrib_group_id := NULL;
16:22:02 1315  	 END;
16:22:02 1316  
16:22:02 1317  	 IF var_subscrib_group_id IS NULL THEN
16:22:02 1318  	   BEGIN
16:22:02 1319  	     SELECT
16:22:02 1320  	       GIFT_CERTIFICATE.PURCHASER_GROUP_ID,
16:22:02 1321  	       GIFT_CERTIFICATE.REDEEMER_GROUP_ID
16:22:02 1322  	       into
16:22:02 1323  	       var_gc_purchaser_group_id,
16:22:02 1324  	       var_gc_redeemer_group_id
16:22:02 1325  	     FROM
16:22:02 1326  	       GIFT_CERTIFICATE
16:22:02 1327  	     WHERE
16:22:02 1328  	       GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = in_invoice_id
16:22:02 1329  	       OR GIFT_CERTIFICATE.FINALIZED_INVOICE_ID = in_invoice_id;
16:22:02 1330  	     EXCEPTION
16:22:02 1331  	       WHEN NO_DATA_FOUND THEN
16:22:02 1332  		 var_gc_purchaser_group_id := NULL;
16:22:02 1333  		 var_gc_redeemer_group_id  := NULL;
16:22:02 1334  	   END;
16:22:02 1335  	 END IF;
16:22:02 1336  
16:22:02 1337  	 IF var_subscrib_group_id IS NULL
16:22:02 1338  	   AND var_gc_purchaser_group_id IS NULL
16:22:02 1339  	   AND var_gc_redeemer_group_id IS NULL THEN
16:22:02 1340  	     RAISE CAN_NOT_FIND_ACCOUNT;
16:22:02 1341  	 END IF;
16:22:02 1342  
16:22:02 1343  	 OPEN out_group_ids FOR
16:22:02 1344  	 SELECT GROUP_ID FROM (
16:22:02 1345  	   SELECT
16:22:02 1346  	     var_subscrib_group_id as "GROUP_ID"
16:22:02 1347  	   FROM
16:22:02 1348  	     DUAL
16:22:02 1349  	   UNION
16:22:02 1350  	   SELECT
16:22:02 1351  	     var_gc_purchaser_group_id as "GROUP_ID"
16:22:02 1352  	   FROM
16:22:02 1353  	     DUAL
16:22:02 1354  	   UNION
16:22:02 1355  	   SELECT
16:22:02 1356  	     var_gc_redeemer_group_id as "GROUP_ID"
16:22:02 1357  	   FROM
16:22:02 1358  	     DUAL
16:22:02 1359  	 )
16:22:02 1360  	 WHERE
16:22:02 1361  	   GROUP_ID IS NOT NULL;
16:22:02 1362  
16:22:02 1363  EXCEPTION
16:22:02 1364  WHEN BAD_INVOICE_ID THEN
16:22:02 1365  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:02 1366  	   SPROC_NAME, 'No such invoice');
16:22:02 1367  WHEN CAN_NOT_FIND_ACCOUNT THEN
16:22:02 1368  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:02 1369  	   SPROC_NAME, 'Could not find account for given invoice');
16:22:02 1370  WHEN OTHERS THEN
16:22:02 1371  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:02 1372  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:22:02 1373  END GET_GROUPS_ID_BY_INVOICE_ID;
