16:22:01 1257  	 RETURN var_max_concurrent_subs;
16:22:01 1258  
16:22:01 1259  EXCEPTION
16:22:01 1260  WHEN OTHERS THEN
16:22:01 1261  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:01 1262  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:22:01 1263  END GET_OFFER_CHAIN_MAX_CONC_SUBSC;
