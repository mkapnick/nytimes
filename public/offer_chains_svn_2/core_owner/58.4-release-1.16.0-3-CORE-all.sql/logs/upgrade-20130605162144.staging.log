16:22:01 414  FUNCTION CALCULATE_OFFER_CHAIN_END_DATE (
16:22:01 415  	in_offer_chain_id	  IN NUMBER,
16:22:01 416  	in_offer_chain_start_date IN DATE
16:22:01 417  ) RETURN DATE AS
16:22:01 418  -- VARIABLES
16:22:01 419  SPROC_NAME	     CONSTANT VARCHAR2(30) := 'CALCULATE_OFFER_CHAIN_END_DATE';
16:22:01 420  temp_offer_chain_id    NUMBER;
16:22:01 421  var_offer_chain_length NUMBER;
16:22:01 422  var_offer_duration     VARCHAR2(30);
16:22:01 423  var_offer_recurrences  NUMBER;
16:22:01 424  var_end_date	     DATE;
16:22:01 425  
16:22:01 426  var_offer_ym_interval INTERVAL YEAR TO MONTH;
16:22:01 427  var_offer_ds_interval INTERVAL DAY(3) TO SECOND;
16:22:01 428  var_offer_years	    NUMBER;
16:22:01 429  var_offer_months	    NUMBER;
16:22:01 430  var_offer_days	    NUMBER;
16:22:01 431  var_infinity_offers_count NUMBER;
16:22:01 432  
16:22:01 433  var_offers_set SYS_REFCURSOR;
16:22:01 434  
16:22:01 435  -- EXCEPTIONS
16:22:01 436  BAD_OFFER_CHAIN_ID EXCEPTION;
16:22:01 437  BEGIN
16:22:01 438  
16:22:01 439  	var_end_date := in_offer_chain_start_date;
16:22:01 440  
16:22:01 441  	-- Check that offer chain exists
16:22:01 442  	BEGIN
16:22:01 443  	  SELECT
16:22:01 444  	    OFFER_CHAIN.ID into temp_offer_chain_id
16:22:01 445  	  FROM
16:22:01 446  	    OFFER_CHAIN
16:22:01 447  	  WHERE
16:22:01 448  	    OFFER_CHAIN.ID = in_offer_chain_id;
16:22:01 449  	  EXCEPTION
16:22:01 450  	    WHEN NO_DATA_FOUND
16:22:01 451  	      THEN RAISE BAD_OFFER_CHAIN_ID;
16:22:01 452  	END;
16:22:01 453  
16:22:01 454  	SELECT
16:22:01 455  	  COUNT(*) into var_infinity_offers_count
16:22:01 456  	FROM
16:22:01 457  	  OFFER_OFFER_CHAIN
16:22:01 458  	WHERE
16:22:01 459  	  OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
16:22:01 460  	  AND OFFER_OFFER_CHAIN.NUM_RECURRENCES = GLOBAL_ENUMS_V22.OFFER_REC_INFINITY;
16:22:01 461  
16:22:01 462  	IF var_infinity_offers_count > 0 THEN
16:22:01 463  	  -- Offer chain contains offers with infinity num of recurrences
16:22:01 464  	  RETURN NULL;
16:22:01 465  	END IF;
16:22:01 466  
16:22:01 467  	-- Select offers durations
16:22:01 468  	OPEN var_offers_set FOR
16:22:01 469  	SELECT
16:22:01 470  	  OFFER.ENTITLEMENT_DURATION,
16:22:01 471  	  OFFER_OFFER_CHAIN.NUM_RECURRENCES
16:22:01 472  	FROM
16:22:01 473  	  OFFER_OFFER_CHAIN
16:22:01 474  	  INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
16:22:01 475  	WHERE
16:22:01 476  	  OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id;
16:22:01 477  
16:22:01 478  	-- Calculate sum of offers durations
16:22:01 479  	LOOP
16:22:01 480  	  FETCH var_offers_set INTO var_offer_duration, var_offer_recurrences;
16:22:01 481  	  EXIT WHEN var_offers_set%NOTFOUND;
16:22:01 482  	  PROCS_COMMON_V22.ISO8601DURATION_TO_INTERVALS(var_offer_duration, var_offer_years, var_offer_months, var_offer_days);
16:22:01 483  	  var_offer_ym_interval := var_offer_years||'-'||var_offer_months;
16:22:01 484  	  var_offer_ds_interval := var_offer_days||' 0:0:0';
16:22:01 485  	  var_end_date := var_end_date + ( var_offer_ym_interval * ( var_offer_recurrences + 1) ) + ( var_offer_ds_interval * ( var_offer_recurrences + 1) );
16:22:01 486  	END LOOP;
16:22:01 487  
16:22:01 488  	RETURN var_end_date;
16:22:01 489  
16:22:01 490  EXCEPTION
16:22:01 491  WHEN BAD_OFFER_CHAIN_ID THEN
16:22:01 492  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 493  	  SPROC_NAME, 'No such offer chain');
16:22:01 494  WHEN OTHERS THEN
16:22:01 495  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:01 496  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:22:01 497  END CALCULATE_OFFER_CHAIN_END_DATE;
