16:22:02 595  
16:22:02 596  FUNCTION IS_INVOICE_PAYING_STARTED (
16:22:02 597  /*
16:22:02 598  Throws exceptions:
16:22:02 599  APP_EXCEPTION_CODES_V22.NOT_FOUND
16:22:02 600  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
16:22:02 601  */
16:22:02 602  	in_invoice_id IN NUMBER
16:22:02 603  ) RETURN NUMBER AS
16:22:02 604  -- VARIABLES
16:22:02 605  SPROC_NAME		 CONSTANT VARCHAR2(30) := 'IS_INVOICE_PAYING_STARTED';
16:22:02 606  temp_invoice_id		 NUMBER;
16:22:02 607  var_processed_charges_num  NUMBER;
16:22:02 608  var_processed_transac_num  NUMBER;
16:22:02 609  var_success_attempts_num	 NUMBER;
16:22:02 610  var_is_gc 		 NUMBER;
16:22:02 611  -- EXCEPTIONS
16:22:02 612  BAD_INVOICE_ID EXCEPTION;
16:22:02 613  BEGIN
16:22:02 614  
16:22:02 615  	-- Check that invoice exists
16:22:02 616  	BEGIN
16:22:02 617  	  SELECT
16:22:02 618  	    INVOICE.ID into temp_invoice_id
16:22:02 619  	  FROM
16:22:02 620  	    INVOICE
16:22:02 621  	  WHERE
16:22:02 622  	    INVOICE.ID = in_invoice_id;
16:22:02 623  	  EXCEPTION
16:22:02 624  	   WHEN NO_DATA_FOUND THEN
16:22:02 625  	     RAISE BAD_INVOICE_ID;
16:22:02 626  	END;
16:22:02 627  
16:22:02 628  	-- Check that there are tansaction attempts with status success
16:22:02 629  	SELECT
16:22:02 630  	  COUNT(1) into var_success_attempts_num
16:22:02 631  	FROM
16:22:02 632  	  TRANSACTION_ATTEMPT ta,
16:22:02 633  	  TRANSACTION t,
16:22:02 634  	  CHARGE c
16:22:02 635  	WHERE
16:22:02 636  	  ta.TRANSACTION_ATTEMPT_STATUS_ID = GLOBAL_STATUSES_V22.TRANS_ATTEMPT_SUCCESS and
16:22:02 637  	  ta.transaction_id = t.id and
16:22:02 638  	  c.transaction_id = t.id and
16:22:02 639  	  t.is_refund = GLOBAL_CONSTANTS_V22.FALSE and
16:22:02 640  	  c.invoice_id = in_invoice_id
16:22:02 641  	;
16:22:02 642  
16:22:02 643  	IF var_success_attempts_num > 0 THEN
16:22:02 644  	  RETURN GLOBAL_CONSTANTS_V22.TRUE;
16:22:02 645  	END IF;
16:22:02 646  
16:22:02 647  	SELECT
16:22:02 648  	  COUNT(1) into var_success_attempts_num
16:22:02 649  	FROM
16:22:02 650  	  TRANSACTION t,
16:22:02 651  	  CHARGE c
16:22:02 652  	WHERE
16:22:02 653  	  c.transaction_id = t.id and
16:22:02 654  	  t.is_refund = GLOBAL_CONSTANTS_V22.FALSE and
16:22:02 655  	  t.is_settled = GLOBAL_CONSTANTS_V22.TRUE and
16:22:02 656  	  c.invoice_id = in_invoice_id
16:22:02 657  	;
16:22:02 658  
16:22:02 659  	IF var_success_attempts_num > 0 THEN
16:22:02 660  	  RETURN GLOBAL_CONSTANTS_V22.TRUE;
16:22:02 661  	END IF;
16:22:02 662  
16:22:02 663  	SELECT
16:22:02 664  	  COUNT(1) into var_is_gc
16:22:02 665  	FROM
16:22:02 666  	  gift_certificate gc
16:22:02 667  	WHERE
16:22:02 668  	  gc.finalized_invoice_id = in_invoice_id
16:22:02 669  	;
16:22:02 670  
16:22:02 671  	IF var_is_gc > 0 THEN
16:22:02 672  	  RETURN GLOBAL_CONSTANTS_V22.TRUE;
16:22:02 673  	END IF;
16:22:02 674  
16:22:02 675  	RETURN GLOBAL_CONSTANTS_V22.FALSE;
16:22:02 676  
16:22:02 677  EXCEPTION
16:22:02 678  WHEN BAD_INVOICE_ID THEN
16:22:02 679  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:02 680  	  SPROC_NAME, 'No such invoice');
16:22:02 681  WHEN OTHERS THEN
16:22:02 682  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:02 683  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:22:02 684  END IS_INVOICE_PAYING_STARTED;
