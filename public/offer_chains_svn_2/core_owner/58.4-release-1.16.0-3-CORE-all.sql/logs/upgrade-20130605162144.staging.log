16:22:01 492  	    var_offer_chain_id,
16:22:01 493  	    var_license_current_index
16:22:01 494  	  );
16:22:01 495  	  EXCEPTION
16:22:01 496  	    WHEN OTHERS THEN
16:22:01 497  	      EXCEPTION_MESSAGE := SQLERRM;
16:22:01 498  	      RAISE CAN_NOT_FIND_NEXT_OFFER;
16:22:01 499  	END;
16:22:01 500  
16:22:01 501  	IF var_next_offer_index IS NULL THEN
16:22:01 502  	  -- RETURN NULL;
16:22:01 503  	  -- TODO: Remove this hardcode
16:22:01 504  	  OPEN out_result_set FOR
16:22:01 505  	  SELECT
16:22:01 506  	    OFFER.ID,
16:22:01 507  	    OFFER.OFFER_STATUS_ID,
16:22:01 508  	    OFFER.ENTITLEMENT_DURATION,
16:22:01 509  	    OFFER.CREATED_BY,
16:22:01 510  	    OFFER.CREATE_DATE,
16:22:01 511  	    OFFER.UPDATED_BY,
16:22:01 512  	    OFFER.UPDATE_DATE,
16:22:01 513  	    OFFER_OFFER_CHAIN.NUM_RECURRENCES as "RECURRENCE_NUMBER"
16:22:01 514  	  FROM
16:22:01 515  	    OFFER_OFFER_CHAIN
16:22:01 516  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
16:22:01 517  	  WHERE
16:22:01 518  	    1=2;
16:22:01 519  	ELSE
16:22:01 520  	  OPEN out_result_set FOR
16:22:01 521  	  SELECT
