16:22:01 992  var_intrl_tax_amount NUMBER(10, 2);
16:22:01 993  BEGIN
16:22:01 994  
16:22:01 995  	SELECT SUM(T.CALCULATED_AMOUNT) into var_intrl_tax_amount
16:22:01 996  	FROM
16:22:01 997  	  LINE_ITEM LI
16:22:01 998  	  INNER JOIN INVOICE I ON LI.INVOICE_ID = I.ID
16:22:01 999  	  INNER JOIN CHARGE CH ON CH.INVOICE_ID=  I.ID
16:22:01 1000  	   INNER JOIN TAX T ON T.LINE_ITEM_ID = LI.ID
16:22:01 1001  	 WHERE
16:22:01 1002  	   CH.TRANSACTION_ID = in_transaction_id
16:22:01 1003  	   AND T.TAX_TYPE_ID IN (
16:22:01 1004  	     SELECT GLOBAL_ENUMS_V22.TAX_TYPE_VAT FROM DUAL
16:22:01 1005  	   );
16:22:01 1006  
16:22:01 1007  	 IF var_intrl_tax_amount IS NULL THEN
16:22:01 1008  	   var_intrl_tax_amount := 0;
16:22:01 1009  	 END IF;
16:22:01 1010  
16:22:01 1011  	 RETURN var_intrl_tax_amount;
16:22:01 1012  
16:22:01 1013  END GET_TRANSACTION_INTRL_TAXES;
