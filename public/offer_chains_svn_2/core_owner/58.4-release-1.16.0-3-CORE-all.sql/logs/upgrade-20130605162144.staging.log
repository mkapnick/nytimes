16:22:01 209  PROCEDURE GET_ENDING_LICENSES (
16:22:01 210  	in_hours_number IN NUMBER,
16:22:01 211  	out_result_set	OUT SYS_REFCURSOR
16:22:01 212  ) AS
16:22:01 213  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_ENDING_LICENSES';
16:22:01 214  -- VARIABLES
16:22:01 215  var_days		NUMBER;
16:22:01 216  var_hours 	NUMBER;
16:22:01 217  var_time_interval INTERVAL DAY (3) TO SECOND;
16:22:01 218  BEGIN
16:22:01 219  
16:22:01 220  	var_hours := mod(in_hours_number,24);
16:22:01 221  	var_days := (in_hours_number - var_hours) / 24;
16:22:01 222  	var_time_interval := var_days||' '||var_hours||':0:0';
16:22:01 223  
16:22:01 224  	OPEN out_result_set FOR
16:22:01 225  SELECT * FROM
16:22:01 226  (
16:22:01 227  	SELECT
16:22:01 228  	  LICENSE.ID,
16:22:01 229  	  LICENSE.CREATE_DATE,
16:22:01 230  	  LICENSE.CREATED_BY,
16:22:01 231  	  LICENSE.CURRENT_OFFER_INDEX,
16:22:01 232  	  LICENSE.CURRENT_OFFER_RECURR_NUM,
16:22:01 233  	  LICENSE.END_DATE,
16:22:01 234  	  LICENSE.ENTITLEMENT_END_DATE,
16:22:01 235  	  LICENSE.INVOICE_ID,
16:22:01 236  	  LICENSE.IS_EXTENSION,
16:22:01 237  	  LICENSE.LICENSE_STATUS_ID,
16:22:01 238  	  LICENSE.OFFER_ID,
16:22:01 239  	  LICENSE.START_DATE,
16:22:01 240  	  LICENSE.SUBSCRIPTION_ID,
16:22:01 241  	  LICENSE.UPDATE_DATE,
16:22:01 242  	  LICENSE.UPDATED_BY
16:22:01 243  	FROM
16:22:01 244  	  LICENSE
16:22:01 245  	WHERE
16:22:01 246  	  TO_DATE(LICENSE.END_DATE) <= (current_timestamp + var_time_interval)
16:22:01 247  	  AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V22.LICENSE_ACTIVE
16:22:01 248  	  AND NOT EXISTS
16:22:01 249  	  (
16:22:01 250  	    SELECT NULL
16:22:01 251  	    FROM PROCESS_RETRY_THROTTLE
16:22:01 252  	    WHERE PROCESS_NAME = SPROC_NAME
16:22:01 253  	      AND GENERIC_ID = LICENSE.ID
16:22:01 254  	  )
16:22:01 255  	  AND ROWNUM <= 10000
16:22:01 256  	  ORDER BY dbms_random.value
16:22:01 257  ) WHERE
16:22:01 258  	  ROWNUM <= 1000
16:22:01 259  	  ;
16:22:01 260  EXCEPTION
