16:22:01 965  var_tax_amount NUMBER(10,2);
16:22:01 966  BEGIN
16:22:01 967  
16:22:01 968  	SELECT
16:22:01 969  	  SUM(LI.TAXES_AMOUNT) into var_tax_amount
16:22:01 970  	FROM
16:22:01 971  	  LINE_ITEM LI
16:22:01 972  	  INNER JOIN INVOICE I ON LI.INVOICE_ID = I.ID
16:22:01 973  	  INNER JOIN CHARGE CH ON CH.INVOICE_ID = I.ID
16:22:01 974  	WHERE
16:22:01 975  	  CH.TRANSACTION_ID = in_transaction_id;
16:22:01 976  
16:22:01 977  	IF var_tax_amount IS NULL THEN
16:22:01 978  	  var_tax_amount := 0;
16:22:01 979  	END IF;
16:22:01 980  
16:22:01 981  	RETURN var_tax_amount;
16:22:01 982  
16:22:01 983  END GET_TRANSACTION_TAX_AMOUNT;
