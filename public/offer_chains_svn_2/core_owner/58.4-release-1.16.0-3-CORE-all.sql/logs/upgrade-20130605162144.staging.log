16:22:01 2823  PROCEDURE ARE_REFUNDS_PENDING_FOR_SUBSCR (
16:22:01 2824  /*
16:22:01 2825  APP_EXCEPTION_CODES_V22.NOT_FOUND
16:22:01 2826  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
16:22:01 2827  */
16:22:01 2828  	 in_subscription_id IN NUMBER,
16:22:01 2829  	 out_result	    OUT NUMBER
16:22:01 2830  ) AS
16:22:01 2831  SPROC_NAME CONSTANT VARCHAR2(30) := 'ARE_REFUNDS_PENDING_FOR_SUBSCR';
16:22:01 2832  -- VARIABLES
16:22:01 2833  temp_subscription_id NUMBER;
16:22:01 2834  var_local_result     NUMBER;
16:22:01 2835  -- EXCEPTIONS
16:22:01 2836  BAD_SUBSCRIPTION_ID EXCEPTION;
16:22:01 2837  BEGIN
16:22:01 2838  
16:22:01 2839  	 -- Check that subscription exists
16:22:01 2840  	 BEGIN
16:22:01 2841  	   SELECT
16:22:01 2842  	     SUBSCRIPTION.ID into temp_subscription_id
16:22:01 2843  	   FROM
16:22:01 2844  	     SUBSCRIPTION
16:22:01 2845  	   WHERE
16:22:01 2846  	     SUBSCRIPTION.ID = in_subscription_id;
16:22:01 2847  	   EXCEPTION
16:22:01 2848  	     WHEN NO_DATA_FOUND THEN
16:22:01 2849  	       RAISE BAD_SUBSCRIPTION_ID;
16:22:01 2850  	 END;
16:22:01 2851  
16:22:01 2852  	 var_local_result := NULL;
16:22:01 2853  
16:22:01 2854  	 -- Check charges for each invoice associated with gived subscription
16:22:01 2855  	 FOR f_invoice IN (
16:22:01 2856  	   SELECT DISTINCT
16:22:01 2857  	     LICENSE.INVOICE_ID as "ID"
16:22:01 2858  	   FROM
16:22:01 2859  	     LICENSE
16:22:01 2860  	   WHERE
16:22:01 2861  	     LICENSE.SUBSCRIPTION_ID = in_subscription_id
16:22:01 2862  	     AND LICENSE.LICENSE_STATUS_ID IN ( SELECT GLOBAL_STATUSES_V22.LICENSE_ACTIVE FROM DUAL )
16:22:01 2863  	 )
16:22:01 2864  	 LOOP
16:22:01 2865  
16:22:01 2866  	   -- Check each charge in invoice
16:22:01 2867  	   FOR f_charge IN (
16:22:01 2868  	     SELECT
16:22:01 2869  	       CHARGE.ID,
16:22:01 2870  	       CHARGE.CHARGE_STATUS_ID,
16:22:01 2871  	       CHARGE.CHARGE_AMOUNT
16:22:01 2872  	     FROM
16:22:01 2873  	       CHARGE
16:22:01 2874  	     WHERE
16:22:01 2875  	       CHARGE.INVOICE_ID = f_invoice.ID
16:22:01 2876  	   )
16:22:01 2877  	   LOOP
16:22:01 2878  
16:22:01 2879  	     -- Charge amount < 0     => it is a refund
16:22:01 2880  	     -- Charge status is OPEN => means that it is not processed yet
16:22:01 2881  	     IF f_charge.CHARGE_AMOUNT < 0
16:22:01 2882  		AND f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V22.CHARGE_OPENED THEN
16:22:01 2883  	       var_local_result := GLOBAL_CONSTANTS_V22.TRUE;
16:22:01 2884  	     END IF;
16:22:01 2885  
16:22:01 2886  	   END LOOP;
16:22:01 2887  
16:22:01 2888  	 END LOOP;
16:22:01 2889  
16:22:01 2890  	 IF var_local_result IS NULL THEN
16:22:01 2891  	   out_result := GLOBAL_CONSTANTS_V22.FALSE;
16:22:01 2892  	 ELSE
16:22:01 2893  	   out_result := GLOBAL_CONSTANTS_V22.TRUE;
16:22:01 2894  	 END IF;
16:22:01 2895  
16:22:01 2896  EXCEPTION
16:22:01 2897  WHEN BAD_SUBSCRIPTION_ID THEN
16:22:01 2898  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 2899  	   SPROC_NAME, 'No such transaction id');
16:22:01 2900  WHEN OTHERS THEN
16:22:01 2901  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:01 2902  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:22:01 2903  END ARE_REFUNDS_PENDING_FOR_SUBSCR;
