16:22:01 986  
16:22:01 987  FUNCTION GET_TRANSACTION_INTRL_TAXES (
16:22:01 988  	in_transaction_id IN NUMBER
16:22:01 989  ) RETURN NUMBER AS
16:22:01 990  SPROC_NAME CONSTANT VARCHAR2(27) := 'GET_TRANSACTION_INTRL_TAXES';
