16:22:01 2113  
16:22:01 2114  PROCEDURE CANCEL_SUBSCRIPTION_INVOICE (
16:22:01 2115  /*
16:22:01 2116  APP_EXCEPTION_CODES_V22.NOT_FOUND
16:22:01 2117  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
16:22:01 2118  APP_EXCEPTION_CODES_V22.INTERNAL_ERROR
16:22:01 2119  */
16:22:01 2120  	 in_invoice_id	      IN NUMBER,
16:22:01 2121  	 in_updated_by	      IN VARCHAR2,
16:22:01 2122  	 in_refundable	      IN NUMBER DEFAULT GLOBAL_CONSTANTS_V22.FALSE
16:22:01 2123  ) AS
16:22:01 2124  SPROC_NAME CONSTANT VARCHAR2(27) := 'CANCEL_SUBSCRIPTION_INVOICE';
16:22:01 2125  -- VARIABLES
16:22:01 2126  temp_invoice_id	      INVOICE.ID%TYPE;
16:22:01 2127  var_chargeback_amount  NUMBER(10,2);
16:22:01 2128  ver_refund_charge_id   NUMBER;
16:22:01 2129  -- EXCEPTIONS
16:22:01 2130  BAD_INVOICE_ID		    EXCEPTION;
16:22:01 2131  CAN_NOT_CALCULATE_CHARGEBACK EXCEPTION;
16:22:01 2132  CAN_NOT_APPLY_CHARGEBACK     EXCEPTION;
16:22:01 2133  EXCEPTION_MESSAGE	    VARCHAR2(1024);
16:22:01 2134  -- STUB
16:22:01 2135  var_now DATE;
16:22:01 2136  var_revoke NUMBER;
16:22:01 2137  var_refund NUMBER;
16:22:01 2138  var_billed NUMBER;
16:22:01 2139  var_subscription_in_grace NUMBER;
16:22:01 2140  BEGIN
16:22:01 2141  
16:22:01 2142  	 var_now := sysdate;
16:22:01 2143  
16:22:01 2144  	 -- Check that invoice exists
16:22:01 2145  	 BEGIN
16:22:01 2146  	   SELECT
16:22:01 2147  	     INVOICE.ID into temp_invoice_id
16:22:01 2148  	   FROM
16:22:01 2149  	     INVOICE
16:22:01 2150  	   WHERE
16:22:01 2151  	     INVOICE.ID = in_invoice_id;
16:22:01 2152  	   EXCEPTION
16:22:01 2153  	     WHEN NO_DATA_FOUND THEN
16:22:01 2154  	       RAISE BAD_INVOICE_ID;
16:22:01 2155  	 END;
16:22:01 2156  
16:22:01 2157  	 select decode(count(1), 0, GLOBAL_CONSTANTS_V22.FALSE, GLOBAL_CONSTANTS_V22.TRUE) into var_revoke
16:22:01 2158  	 from license l, subscription s, offer_chain oc
16:22:01 2159  	 where
16:22:01 2160  	   l.subscription_id = s.id and
16:22:01 2161  	   s.offer_chain_id = oc.id and
16:22:01 2162  	   l.invoice_id = in_invoice_id and
16:22:01 2163  	   oc.revoke_entitlements = GLOBAL_CONSTANTS_V22.TRUE and
16:22:01 2164  	   rownum < 2;
16:22:01 2165  
16:22:01 2166  	 select
16:22:01 2167  	   decode(s.subscription_status_id, GLOBAL_STATUSES_V22.SUBSCRIPTION_IN_GRACE_PERIOD, 1, 0)
16:22:01 2168  	 into
16:22:01 2169  	   var_subscription_in_grace
16:22:01 2170  	 from license l, subscription s
16:22:01 2171  	 where
16:22:01 2172  	   l.subscription_id = s.id and
16:22:01 2173  	   l.invoice_id = in_invoice_id and
16:22:01 2174  	   rownum < 2;
16:22:01 2175  
16:22:01 2176  	 var_billed := PROCS_INVOICE_V22.IS_INVOICE_PAYING_STARTED(in_invoice_id);
16:22:01 2177  	 var_refund := GLOBAL_CONSTANTS_V22.FALSE;
16:22:01 2178  
16:22:01 2179  	 -- Check that transaction for given invoice not started
16:22:01 2180  	 -- if refund enabled calculate and apply chargeback
16:22:01 2181  	 IF (
16:22:01 2182  	     var_billed = GLOBAL_CONSTANTS_V22.TRUE
16:22:01 2183  	   )THEN
16:22:01 2184  	   if (in_refundable = GLOBAL_CONSTANTS_V22.TRUE) then
16:22:01 2185  	     -- If started then we need to calculate refund
16:22:01 2186  	     BEGIN
16:22:01 2187  	       PROCS_INVOICE_V22.CALCULATE_INVOICE_CHARGEBACK(
16:22:01 2188  		 in_invoice_id,
16:22:01 2189  		 var_now,
16:22:01 2190  		 var_chargeback_amount
16:22:01 2191  	       );
16:22:01 2192  	       EXCEPTION
16:22:01 2193  		 WHEN OTHERS THEN
16:22:01 2194  		   EXCEPTION_MESSAGE := SQLERRM;
16:22:01 2195  		   RAISE CAN_NOT_CALCULATE_CHARGEBACK;
16:22:01 2196  	     END;
16:22:01 2197  	     IF var_chargeback_amount > 0 THEN
16:22:01 2198  	       BEGIN
16:22:01 2199  		 PROCS_INVOICE_V22.APPLY_REFUND(
16:22:01 2200  		   in_invoice_id,
16:22:01 2201  		   var_chargeback_amount,
16:22:01 2202  		   in_updated_by,
16:22:01 2203  		   ver_refund_charge_id
16:22:01 2204  		 );
16:22:01 2205  		 EXCEPTION
16:22:01 2206  		   WHEN OTHERS THEN
16:22:01 2207  		     EXCEPTION_MESSAGE := SQLERRM;
16:22:01 2208  		     RAISE CAN_NOT_APPLY_CHARGEBACK;
16:22:01 2209  	       END;
16:22:01 2210  	       var_refund := GLOBAL_CONSTANTS_V22.TRUE;
16:22:01 2211  	     END IF;
16:22:01 2212  	   end if;
16:22:01 2213  	 ELSE
16:22:01 2214  
16:22:01 2215  	   FOR f_transaction_to_close IN (
16:22:01 2216  	     SELECT DISTINCT CHARGE.TRANSACTION_ID AS "ID" FROM CHARGE WHERE CHARGE.INVOICE_ID = in_invoice_id and CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V22.CHARGE_OPENED
16:22:01 2217  	   )
16:22:01 2218  	   LOOP
16:22:01 2219  	     PROCS_TRANSACTION_V22.UPDATE_TRANSACTION_STATUS(
16:22:01 2220  	       in_transaction_id	=> f_transaction_to_close.ID,
16:22:01 2221  	       in_updated_by		=> in_updated_by,
16:22:01 2222  	       in_transaction_status_id => GLOBAL_STATUSES_V22.TRANSACTION_CLOSED
16:22:01 2223  	     );
16:22:01 2224  	   END LOOP;
16:22:01 2225  	   -- Needs to close charges. No refund.
16:22:01 2226  	   FOR f_charge_to_close IN (
16:22:01 2227  	     SELECT CHARGE.ID FROM CHARGE WHERE CHARGE.INVOICE_ID = in_invoice_id and CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V22.CHARGE_OPENED
16:22:01 2228  	   )
16:22:01 2229  	   LOOP
16:22:01 2230  	     PROCS_CHARGE_V22.UPDATE_CHARGE_STATUS(
16:22:01 2231  	       in_charge_id	   => f_charge_to_close.ID,
16:22:01 2232  	       in_updated_by	   => in_updated_by,
16:22:01 2233  	       in_charge_status_id => GLOBAL_STATUSES_V22.CHARGE_CANCELED
16:22:01 2234  	     );
16:22:01 2235  	   END LOOP;
16:22:01 2236  
16:22:01 2237  	   PROCS_INVOICE_V22.UPDATE_INVOICE_STATUS(
16:22:01 2238  	     in_invoice_id		    => in_invoice_id,
16:22:01 2239  	     in_updated_by		    => in_updated_by,
16:22:01 2240  	     in_invoice_status_id	    => GLOBAL_STATUSES_V22.INVOICE_CLOSED
16:22:01 2241  	   );
16:22:01 2242  
16:22:01 2243  	 END IF;
16:22:01 2244  	 -- update licenses
16:22:01 2245  	 IF(var_revoke = GLOBAL_CONSTANTS_V22.TRUE OR var_chargeback_amount > 0 OR (var_subscription_in_grace = GLOBAL_CONSTANTS_V22.FALSE AND var_billed = GLOBAL_CONSTANTS_V22.FALSE)) THEN
16:22:01 2246  	   FOR f_license_to_cancel IN (
16:22:01 2247  	     SELECT LICENSE.ID FROM LICENSE WHERE LICENSE.INVOICE_ID = in_invoice_id AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V22.LICENSE_ACTIVE
16:22:01 2248  	   )
16:22:01 2249  	   LOOP
16:22:01 2250  	     PROCS_LICENSE_CRU_V22.UPDATE_LICENSE(
16:22:01 2251  	       in_license_id	     => f_license_to_cancel.ID,
16:22:01 2252  	       in_license_status_id  => GLOBAL_STATUSES_V22.LICENSE_CLOSED,
16:22:01 2253  	       in_needs_entitlements => GLOBAL_CONSTANTS_V22.TRUE,
16:22:01 2254  	       in_updated_by	     => in_updated_by,
16:22:01 2255  	       in_entitlement_end_date => var_now
16:22:01 2256  	     );
16:22:01 2257  	   END LOOP;
16:22:01 2258  	 ELSE
16:22:01 2259  	   FOR f_license_to_cancel IN (
16:22:01 2260  	     SELECT LICENSE.ID FROM LICENSE WHERE LICENSE.INVOICE_ID = in_invoice_id AND LICENSE.LICENSE_STATUS_ID in (GLOBAL_STATUSES_V22.LICENSE_ACTIVE, GLOBAL_STATUSES_V22.LICENSE_IN_GRACE_PERIOD)
16:22:01 2261  	   )
16:22:01 2262  	   LOOP
16:22:01 2263  	     PROCS_LICENSE_CRU_V22.UPDATE_LICENSE(
16:22:01 2264  	       in_license_id	     => f_license_to_cancel.ID,
16:22:01 2265  	       in_license_status_id  => GLOBAL_STATUSES_V22.LICENSE_CLOSED,
16:22:01 2266  	       in_updated_by	     => in_updated_by
16:22:01 2267  	     );
16:22:01 2268  	   END LOOP;
16:22:01 2269  	 END IF;
16:22:01 2270  
16:22:01 2271  
16:22:01 2272  EXCEPTION
16:22:01 2273  WHEN BAD_INVOICE_ID THEN
16:22:01 2274  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 2275  	   SPROC_NAME, 'No such invoice');
16:22:01 2276  WHEN CAN_NOT_CALCULATE_CHARGEBACK THEN
16:22:01 2277  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:01 2278  	   SPROC_NAME, 'Could not calculate invoice refund', EXCEPTION_MESSAGE);
16:22:01 2279  WHEN CAN_NOT_APPLY_CHARGEBACK THEN
16:22:01 2280  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:01 2281  	   SPROC_NAME, 'Could not apply chargeback', EXCEPTION_MESSAGE);
16:22:01 2282  WHEN OTHERS THEN
16:22:01 2283  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:01 2284  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:22:01 2285  END CANCEL_SUBSCRIPTION_INVOICE;
16:22:01 2286  
16:22:01 2287  /******************************************************************************/
16:22:01 2288  
16:22:01 2289  PROCEDURE FINALIZE_CANCELATION (
16:22:01 2290  /*
16:22:01 2291  APP_EXCEPTION_CODES_V22.NOT_FOUND
16:22:01 2292  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
16:22:01 2293  APP_EXCEPTION_CODES_V22.INTERNAL_ERROR
16:22:01 2294  */
16:22:01 2295  --  in_subscription_id	 IN SUBSCRIPTION.ID%TYPE,
16:22:01 2296  --  in_cancelation_reason IN SUBSCRIPTION_CANCEL_REASON.VALUE%TYPE,
16:22:01 2297  --  in_cancelation_date	 IN SUBSCRIPTION.CANCELLATION_DATE%TYPE,
16:22:01 2298  --  in_note		 IN SUBSCRIPTION_NOTE.NOTE%TYPE,
16:22:01 2299  --  in_agent_id		 IN SUBSCRIPTION_NOTE.AGENT_ID%TYPE,
16:22:01 2300  --  in_updated_by	 IN SUBSCRIPTION.UPDATED_BY%TYPE
16:22:01 2301  	 in_subscription_id    IN NUMBER,
16:22:01 2302  	 in_cancelation_reason IN VARCHAR2,
16:22:01 2303  	 in_cancelation_date   IN DATE,
16:22:01 2304  	 in_note	       IN VARCHAR2,
16:22:01 2305  	 in_agent_id	       IN NUMBER,
16:22:01 2306  	 in_updated_by	       IN VARCHAR2
16:22:01 2307  ) AS
16:22:01 2308  SPROC_NAME CONSTANT VARCHAR2(20) := 'FINALIZE_CANCELATION';
16:22:01 2309  -- VARIABLES
16:22:01 2310  var_current_subscr_status SUBSCRIPTION.SUBSCRIPTION_STATUS_ID%TYPE;
16:22:01 2311  var_sct_id		 SUBSCRIPTION.SCT_ID%TYPE;
16:22:01 2312  var_active_invoices_count NUMBER;
16:22:01 2313  var_license_to_disgrace	 LICENSE.ID%TYPE;
16:22:01 2314  var_now			 DATE := SYSDATE;
16:22:01 2315  -- EXCEPTIONS
16:22:01 2316  BAD_SUBSCRIPTION_ID	   EXCEPTION;
16:22:01 2317  BAD_SUBSCRIPTION_STATUS	   EXCEPTION;
16:22:01 2318  BAD_CANCELATION_REASON	   EXCEPTION;
16:22:01 2319  CAN_NOT_UPDATE_SUBSCRIPTION EXCEPTION;
16:22:01 2320  ACTIVE_INVOICES_FOUND	   EXCEPTION;
16:22:01 2321  CAN_NOT_CREATE_NOTE	   EXCEPTION;
16:22:01 2322  EXCEPTION_MESSAGE	   VARCHAR2(1024);
16:22:01 2323  BEGIN
16:22:01 2324  
16:22:01 2325  	 -- Get current subscription status
16:22:01 2326  	 BEGIN
16:22:01 2327  	   SELECT
16:22:01 2328  	     SUBSCRIPTION.SUBSCRIPTION_STATUS_ID into var_current_subscr_status
16:22:01 2329  	   FROM
16:22:01 2330  	     SUBSCRIPTION
16:22:01 2331  	   WHERE
16:22:01 2332  	     SUBSCRIPTION.ID = in_subscription_id;
16:22:01 2333  	   EXCEPTION
16:22:01 2334  	     WHEN NO_DATA_FOUND THEN
16:22:01 2335  	       RAISE BAD_SUBSCRIPTION_ID;
16:22:01 2336  	 END;
16:22:01 2337  
16:22:01 2338  	 -- Check that subscription reason is correct
16:22:01 2339  	 BEGIN
16:22:01 2340  	   SELECT
16:22:01 2341  	     SUBSCRIPTION_CANCEL_REASON.ID into var_sct_id
16:22:01 2342  	   FROM
16:22:01 2343  	     SUBSCRIPTION_CANCEL_REASON
16:22:01 2344  	   WHERE
16:22:01 2345  	     SUBSCRIPTION_CANCEL_REASON.VALUE LIKE in_cancelation_reason
16:22:01 2346  	     AND ROWNUM <= 1;
16:22:01 2347  	   EXCEPTION
16:22:01 2348  	     WHEN NO_DATA_FOUND THEN
16:22:01 2349  	       RAISE BAD_CANCELATION_REASON;
16:22:01 2350  	 END;
16:22:01 2351  
16:22:01 2352  	 -- Check for invoices with active licenses
16:22:01 2353  	 SELECT
16:22:01 2354  	   COUNT(*) into var_active_invoices_count
16:22:01 2355  	 FROM
16:22:01 2356  	   LICENSE
16:22:01 2357  	   INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
16:22:01 2358  	 WHERE
16:22:01 2359  	   LICENSE.LICENSE_STATUS_ID in (GLOBAL_STATUSES_V22.LICENSE_ACTIVE, GLOBAL_STATUSES_V22.LICENSE_IN_GRACE_PERIOD)
16:22:01 2360  	   AND LICENSE.SUBSCRIPTION_ID = in_subscription_id;
16:22:01 2361  
16:22:01 2362  	 IF var_active_invoices_count > 0 THEN
16:22:01 2363  	   RAISE ACTIVE_INVOICES_FOUND;
16:22:01 2364  	 END IF;
16:22:01 2365  
16:22:01 2366  	 -- Check that subscription is active
16:22:01 2367  	 IF var_current_subscr_status != GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE AND
16:22:01 2368  	    var_current_subscr_status != GLOBAL_STATUSES_V22.SUBSCRIPTION_SUSPENDED AND
16:22:01 2369  	    var_current_subscr_status != GLOBAL_STATUSES_V22.SUBSCRIPTION_IN_GRACE_PERIOD THEN
16:22:01 2370  	   RAISE BAD_SUBSCRIPTION_STATUS;
16:22:01 2371  	 END IF;
16:22:01 2372  
16:22:01 2373  	 -- Update subscription data
16:22:01 2374  	 BEGIN
16:22:01 2375  	   PROCS_SUBSCRIPTION_CRU_V22.UPDATE_SUBSCRIPTION(
16:22:01 2376  	     in_subscription_id        => in_subscription_id,
16:22:01 2377  	     in_subscription_status_id => GLOBAL_STATUSES_V22.SUBSCRIPTION_CANCELED,
16:22:01 2378  	     in_cancelation_date       => in_cancelation_date,
16:22:01 2379  	     in_updated_by	       => in_updated_by,
16:22:01 2380  	     in_sct_id		       => var_sct_id
16:22:01 2381  	   );
16:22:01 2382  	   EXCEPTION
16:22:01 2383  	     WHEN OTHERS THEN
16:22:01 2384  	       EXCEPTION_MESSAGE := SQLERRM;
16:22:01 2385  	       RAISE CAN_NOT_UPDATE_SUBSCRIPTION;
16:22:01 2386  	 END;
16:22:01 2387  
16:22:01 2388  	 -- Terminate grace period for licenses in grace [SAR-31]
16:22:01 2389  	 BEGIN
16:22:01 2390  	   SELECT
16:22:01 2391  	     LICENSE.ID into var_license_to_disgrace
16:22:01 2392  	   FROM
16:22:01 2393  	     LICENSE
16:22:01 2394  	   WHERE
16:22:01 2395  	     LICENSE.SUBSCRIPTION_ID = in_subscription_id
16:22:01 2396  	     AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V22.LICENSE_IN_GRACE_PERIOD
16:22:01 2397  	     AND ROWNUM <= 1
16:22:01 2398  	   ORDER BY
16:22:01 2399  	     CREATE_DATE DESC;
16:22:01 2400  	 EXCEPTION
16:22:01 2401  	   WHEN NO_DATA_FOUND THEN
16:22:01 2402  	     var_license_to_disgrace := NULL;
16:22:01 2403  	 END;
16:22:01 2404  
16:22:01 2405  	 IF var_license_to_disgrace IS NOT NULL THEN
16:22:01 2406  	   PROCS_LICENSE_CRU_V22.UPDATE_LICENSE(
16:22:01 2407  	     in_license_id	     => var_license_to_disgrace,
16:22:01 2408  	     in_license_status_id    => GLOBAL_STATUSES_V22.LICENSE_CLOSED,
16:22:01 2409  	     in_updated_by	     => in_updated_by
16:22:01 2410  	   );
16:22:01 2411  	 END IF;
16:22:01 2412  
16:22:01 2413  	 -- Annotate subscription
16:22:01 2414  	 IF in_note IS NOT NULL THEN
16:22:01 2415  	   BEGIN
16:22:01 2416  	     PROCS_SUBSCRIPTION_V22.ANNOTATE_SUBSCRIPTION(
16:22:01 2417  	       in_subscription_id,
16:22:01 2418  	       in_agent_id,
16:22:01 2419  	       in_note,
16:22:01 2420  	       in_updated_by
16:22:01 2421  	     );
16:22:01 2422  	     EXCEPTION
16:22:01 2423  	      WHEN OTHERS THEN
