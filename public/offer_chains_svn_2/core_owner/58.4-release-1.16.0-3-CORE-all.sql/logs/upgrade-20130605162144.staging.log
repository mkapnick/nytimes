16:22:02 1278  	 out_group_ids OUT SYS_REFCURSOR
16:22:02 1279  ) AS
16:22:02 1280  SPROC_NAME CONSTANT VARCHAR2(27) := 'GET_GROUPS_ID_BY_INVOICE_ID';
16:22:02 1281  -- VARIABLES
16:22:02 1282  temp_invoice_id		 NUMBER;
16:22:02 1283  var_subscrib_group_id	 NUMBER;
16:22:02 1284  var_gc_purchaser_group_id NUMBER;
16:22:02 1285  var_gc_redeemer_group_id  NUMBER;
16:22:02 1286  -- EXCEPTIONS
