16:22:01 524  	    OFFER.ENTITLEMENT_DURATION,
16:22:01 525  	    OFFER.CREATED_BY,
16:22:01 526  	    OFFER.CREATE_DATE,
16:22:01 527  	    OFFER.UPDATED_BY,
16:22:01 528  	    OFFER.UPDATE_DATE,
16:22:01 529  	    OFFER_OFFER_CHAIN.NUM_RECURRENCES as "RECURRENCE_NUMBER"
16:22:01 530  	  FROM
16:22:01 531  	    OFFER_OFFER_CHAIN
16:22:01 532  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
16:22:01 533  	  WHERE
16:22:01 534  	    OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
16:22:01 535  	    AND OFFER_OFFER_CHAIN.INDEX_VALUE = var_next_offer_index;
16:22:01 536  	END IF;
16:22:01 537  
16:22:01 538  EXCEPTION
16:22:01 539  WHEN BAD_LICENSE_ID THEN
16:22:01 540  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
