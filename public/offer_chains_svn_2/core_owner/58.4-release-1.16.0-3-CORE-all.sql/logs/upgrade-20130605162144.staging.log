16:22:01 546  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:01 547  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:22:01 548  END GET_NEXT_OFFER;
