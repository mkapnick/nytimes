16:22:02 2716  PROCEDURE SWITCH_FINANCIAL_INSTRUMENT (
16:22:02 2717  	 in_old_fin_instrument_id   IN NUMBER,
16:22:02 2718  	 in_old_fin_instrument_type IN NUMBER,
16:22:02 2719  	 in_new_fin_instrument_id   IN NUMBER,
16:22:02 2720  	 in_new_fin_instrument_type IN NUMBER,
16:22:02 2721  	 in_updated_by		    IN VARCHAR2
16:22:02 2722  ) AS
16:22:02 2723  SPROC_NAME CONSTANT VARCHAR2(27) := 'SWITCH_FINANCIAL_INSTRUMENT';
16:22:02 2724  -- variables
16:22:02 2725  temp_out_transaction_id NUMBER;
16:22:02 2726  temp_out_charge_id      NUMBER;
16:22:02 2727  var_accounts_count      NUMBER;
16:22:02 2728  var_transaction_type_old  "TRANSACTION".TRANSACTION_TYPE_CODE%TYPE;
16:22:02 2729  var_transaction_type	 "TRANSACTION".TRANSACTION_TYPE_CODE%TYPE;
16:22:02 2730  -- EXCEPTIONS
16:22:02 2731  BAD_OLD_CC   EXCEPTION;
16:22:02 2732  BAD_OLD_PP   EXCEPTION;
16:22:02 2733  BAD_OLD_TYPE EXCEPTION;
16:22:02 2734  BAD_NEW_CC   EXCEPTION;
16:22:02 2735  BAD_NEW_PP   EXCEPTION;
16:22:02 2736  BAD_NEW_TYPE EXCEPTION;
16:22:02 2737  DIFFERENT_OWNERS EXCEPTION;
16:22:02 2738  BEGIN
16:22:02 2739  
16:22:02 2740  	 IF in_old_fin_instrument_type = GLOBAL_ENUMS_V22.INSTRUMENT_CREDIT_CARD THEN
16:22:02 2741  	   IF PROCS_FIN_INSTRUMENTS_V22.IS_CREDIT_CARD_EXISTS(in_old_fin_instrument_id) = GLOBAL_CONSTANTS_V22.FALSE THEN
16:22:02 2742  	     -- throw exception: bad old credit card
16:22:02 2743  	     RAISE BAD_OLD_CC;
16:22:02 2744  	   END IF;
16:22:02 2745  	 ELSIF in_old_fin_instrument_type = GLOBAL_ENUMS_V22.INSTRUMENT_PAYPAL THEN
16:22:02 2746  	   IF PROCS_FIN_INSTRUMENTS_V22.IS_PAYPAL_EXISTS(in_old_fin_instrument_id) = GLOBAL_CONSTANTS_V22.FALSE THEN
16:22:02 2747  	     -- throw exception: bad old paypal
16:22:02 2748  	     RAISE BAD_OLD_PP;
16:22:02 2749  	   END IF;
16:22:02 2750  	 ELSE
16:22:02 2751  	   -- throw exception: bad instrument type
16:22:02 2752  	   RAISE BAD_OLD_TYPE;
16:22:02 2753  	 END IF;
16:22:02 2754  
16:22:02 2755  	 IF in_new_fin_instrument_type = GLOBAL_ENUMS_V22.INSTRUMENT_CREDIT_CARD THEN
16:22:02 2756  	   IF PROCS_FIN_INSTRUMENTS_V22.IS_CREDIT_CARD_EXISTS(in_new_fin_instrument_id) = GLOBAL_CONSTANTS_V22.FALSE THEN
16:22:02 2757  	     -- throw exception: bad new credit card
16:22:02 2758  	     RAISE BAD_NEW_CC;
16:22:02 2759  	   END IF;
16:22:02 2760  	 ELSIF in_new_fin_instrument_type = GLOBAL_ENUMS_V22.INSTRUMENT_PAYPAL THEN
16:22:02 2761  	   IF PROCS_FIN_INSTRUMENTS_V22.IS_PAYPAL_EXISTS(in_new_fin_instrument_id) = GLOBAL_CONSTANTS_V22.FALSE THEN
16:22:02 2762  	     -- throw exception: bad new paypal
16:22:02 2763  	     RAISE BAD_NEW_PP;
16:22:02 2764  	   END IF;
16:22:02 2765  	 ELSE
16:22:02 2766  	   -- throw exception: bad new instrument type
16:22:02 2767  	   RAISE BAD_NEW_TYPE;
16:22:02 2768  	 END IF;
16:22:02 2769  
16:22:02 2770  	 -- Check that owner of both instruments - same man
16:22:02 2771  
16:22:02 2772  	 SELECT count(1) into var_accounts_count FROM (
16:22:02 2773  	   SELECT
16:22:02 2774  	     CC.ACCOUNT_ID
16:22:02 2775  	   FROM
16:22:02 2776  	     CREDIT_CARD CC
16:22:02 2777  	   WHERE
16:22:02 2778  	     (
16:22:02 2779  	       CC.ID = in_old_fin_instrument_id
16:22:02 2780  	       AND in_old_fin_instrument_type = GLOBAL_ENUMS_V22.INSTRUMENT_CREDIT_CARD
16:22:02 2781  	     )
16:22:02 2782  	     OR
16:22:02 2783  	     (
16:22:02 2784  	       CC.ID = in_new_fin_instrument_id
16:22:02 2785  	       AND in_new_fin_instrument_type = GLOBAL_ENUMS_V22.INSTRUMENT_CREDIT_CARD
16:22:02 2786  	     )
16:22:02 2787  	   UNION
16:22:02 2788  	   SELECT
16:22:02 2789  	     PP.ACCOUNT_ID
16:22:02 2790  	   FROM
16:22:02 2791  	     PAYPAL PP
16:22:02 2792  	   WHERE
16:22:02 2793  	     (
16:22:02 2794  	       PP.ID = in_old_fin_instrument_id
16:22:02 2795  	       AND in_old_fin_instrument_type = GLOBAL_ENUMS_V22.INSTRUMENT_PAYPAL
16:22:02 2796  	     )
16:22:02 2797  	     OR
16:22:02 2798  	     (
16:22:02 2799  	       PP.ID = in_new_fin_instrument_id
16:22:02 2800  	       AND in_new_fin_instrument_type = GLOBAL_ENUMS_V22.INSTRUMENT_PAYPAL
16:22:02 2801  	     )
16:22:02 2802  	 )
16:22:02 2803  	 WHERE
16:22:02 2804  	   account_id IS NOT NULL;
16:22:02 2805  
16:22:02 2806  	 IF (var_accounts_count > 1) THEN
16:22:02 2807  	   -- Throw exception: different owners of instruments
16:22:02 2808  	   RAISE DIFFERENT_OWNERS;
16:22:02 2809  	 END IF;
16:22:02 2810  
16:22:02 2811  	 FOR f_sub IN (
16:22:02 2812  	   select
16:22:02 2813  	     s.id
16:22:02 2814  	   FROM
16:22:02 2815  	     subscription s
16:22:02 2816  	   WHERE
16:22:02 2817  	     (
16:22:02 2818  	       s.subscription_status_id = GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE OR
16:22:02 2819  	       s.subscription_status_id = GLOBAL_STATUSES_V22.SUBSCRIPTION_SUSPENDED OR
16:22:02 2820  	       s.subscription_status_id = GLOBAL_STATUSES_V22.SUBSCRIPTION_IN_GRACE_PERIOD
16:22:02 2821  	     )
16:22:02 2822  	     AND
16:22:02 2823  	     s.instrument_type_id = in_old_fin_instrument_type AND
16:22:02 2824  	     s.instrument_id = in_old_fin_instrument_id
16:22:02 2825  	 ) LOOP
16:22:02 2826  	   PROCS_SUBSCRIPTION_CRU_V22.UPDATE_SUBSCRIPTION(
16:22:02 2827  	     in_subscription_id => f_sub.id,
16:22:02 2828  	     in_instrument_type_id => in_new_fin_instrument_type,
16:22:02 2829  	     in_instrument_id => in_new_fin_instrument_id,
16:22:02 2830  	     in_updated_by => in_updated_by
16:22:02 2831  	   );
16:22:02 2832  	 END LOOP;
16:22:02 2833  
16:22:02 2834  	 FOR f_open_charge IN (
16:22:02 2835  	   select
16:22:02 2836  	     ch.id,
16:22:02 2837  	     ch.invoice_id,
16:22:02 2838  	     ch.transaction_id,
16:22:02 2839  	     ch.charge_amount
16:22:02 2840  	   FROM
16:22:02 2841  	     charge ch
16:22:02 2842  	   WHERE
16:22:02 2843  	     ch.instrument_type_id = in_old_fin_instrument_type
16:22:02 2844  	     AND ch.instrument_id = in_old_fin_instrument_id
16:22:02 2845  	     AND ch.charge_status_id = GLOBAL_STATUSES_V22.CHARGE_OPENED
16:22:02 2846  	 ) LOOP
16:22:02 2847  
16:22:02 2848  	   FOR f_pending_transaction IN (
16:22:02 2849  	     select
16:22:02 2850  	       id, transaction_amount, order_id, is_refund
16:22:02 2851  	     from
16:22:02 2852  	       transaction
16:22:02 2853  	     where
16:22:02 2854  	       id = f_open_charge.transaction_id
16:22:02 2855  	       and transaction_status_id = GLOBAL_STATUSES_V22.TRANSACTION_PENDING
16:22:02 2856  	   ) LOOP
16:22:02 2857  
16:22:02 2858  	     SELECT
16:22:02 2859  	       DECODE(TRANSACTION_TYPE_CODE, 'RECURRING_BILLING', 'RECURRING_BILLING_USER_UPDATE',
16:22:02 2860  					     'GRACE_PERIOD_FINAL', 'GRACE_PERIOD_USER_UPDATE',
16:22:02 2861  					     TRANSACTION_TYPE_CODE)
16:22:02 2862  	     INTO var_transaction_type
16:22:02 2863  	     FROM
16:22:02 2864  	       Transaction
16:22:02 2865  	     WHERE
16:22:02 2866  	       id = f_pending_transaction.id
16:22:02 2867  	       AND ROWNUM <= 1;
16:22:02 2868  
16:22:02 2869  	     PROCS_TRANSACTION_V22.CREATE_TRANSACTION (
16:22:02 2870  	       in_transaction_id => NULL,
16:22:02 2871  	       in_status_id  => GLOBAL_STATUSES_V22.TRANSACTION_PENDING,
16:22:02 2872  	       in_amount     => f_pending_transaction.transaction_amount,
16:22:02 2873  	       in_created_by => in_updated_by,
16:22:02 2874  	       in_order_id   => null,
16:22:02 2875  	       in_is_refund  => f_pending_transaction.is_refund,
16:22:02 2876  	       in_transaction_type_code => var_transaction_type,
16:22:02 2877  	       out_transaction_id => temp_out_transaction_id
