16:22:01 859  	out_months  OUT NUMBER,
16:22:01 860  	out_days    OUT NUMBER
16:22:01 861  ) AS
16:22:01 862  -- VARIABLES
16:22:01 863  var_offer_duration VARCHAR2(30);
16:22:01 864  SPROC_NAME	 CONSTANT VARCHAR2(16) := 'GET_OFFER_LENGTH';
16:22:01 865  -- EXCEPTIONS
16:22:01 866  BAD_OFFER_ID	     EXCEPTION;
16:22:01 867  CAN_NOT_PARSE_DURATION EXCEPTION;
16:22:01 868  EXCEPTION_MESSAGE       VARCHAR2(1024);
16:22:01 869  BEGIN
16:22:01 870  
16:22:01 871  	BEGIN
16:22:01 872  	  SELECT
16:22:01 873  	    OFFER.ENTITLEMENT_DURATION into var_offer_duration
16:22:01 874  	  FROM
16:22:01 875  	    OFFER
16:22:01 876  	  WHERE
16:22:01 877  	    OFFER.ID = in_offer_id;
16:22:01 878  	  EXCEPTION
16:22:01 879  	    WHEN NO_DATA_FOUND THEN
16:22:01 880  	      RAISE BAD_OFFER_ID;
