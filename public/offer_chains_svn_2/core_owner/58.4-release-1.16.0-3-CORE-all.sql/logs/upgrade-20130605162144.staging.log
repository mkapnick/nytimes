16:22:01 467  CAN_NOT_FIND_NEXT_OFFER EXCEPTION;
16:22:01 468  EXCEPTION_MESSAGE       VARCHAR2(1024);
16:22:01 469  BEGIN
16:22:01 470  
16:22:01 471  	BEGIN
