16:22:02 317  
16:22:02 318  PROCEDURE DISABLE_CREDIT_CARD (
16:22:02 319  /*
16:22:02 320  Throws exceptions:
16:22:02 321  APP_EXCEPTION_CODES_V22.NOT_FOUND
16:22:02 322  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
16:22:02 323  APP_EXCEPTION_CODES_V22.INTERNAL_ERROR
16:22:02 324  */
16:22:02 325  	in_credit_card_id IN NUMBER,
16:22:02 326  	in_updated_by	  IN VARCHAR2
16:22:02 327  ) AS
16:22:02 328  SPROC_NAME		   CONSTANT VARCHAR2(19) := 'DISABLE_CREDIT_CARD';
16:22:02 329  -- VARIBLES
16:22:02 330  var_account_id		     NUMBER;
16:22:02 331  var_group_id		     NUMBER;
16:22:02 332  var_credit_card_status	     NUMBER;
16:22:02 333  var_pending_transactions_num   NUMBER;
16:22:02 334  var_pending_invoices_num	     NUMBER;
16:22:02 335  current_def_instrument_type_id NUMBER;
16:22:02 336  current_def_instrument_id      NUMBER;
16:22:02 337  -- EXCEPTIONS
16:22:02 338  BAD_CC_STATUS		   EXCEPTION;
16:22:02 339  PENDING_TRANSACTIONS_FOUNDED EXCEPTION;
16:22:02 340  CAN_NOT_GET_DEF_FINANCIAL    EXCEPTION;
16:22:02 341  CAN_NOT_DEL_DEF_FINANCIAL    EXCEPTION;
16:22:02 342  CAN_NOT_DISABLE_CREDIT_CARD  EXCEPTION;
16:22:02 343  EXCEPTION_MESSAGE 	   VARCHAR2(1024);
16:22:02 344  BEGIN
16:22:02 345  
16:22:02 346  	-- Get credit card status
16:22:02 347  	-- Get account id
16:22:02 348  	SELECT
16:22:02 349  	  CREDIT_CARD.CREDIT_CARD_STATUS_ID,
16:22:02 350  	  CREDIT_CARD.ACCOUNT_ID
16:22:02 351  	  into
16:22:02 352  	  var_credit_card_status,
16:22:02 353  	  var_account_id
16:22:02 354  	FROM
16:22:02 355  	  CREDIT_CARD
16:22:02 356  	WHERE
16:22:02 357  	  CREDIT_CARD.ID = in_credit_card_id;
16:22:02 358  
16:22:02 359  	-- Check that we can disable this credit card (STUB)
16:22:02 360  	IF F_CAN_DISABLE_CREDIT_CARD(in_credit_card_id) = GLOBAL_CONSTANTS_V22.FALSE THEN
16:22:02 361  	  RAISE CAN_NOT_DISABLE_CREDIT_CARD;
16:22:02 362  	END IF;
16:22:02 363  
16:22:02 364  	-- Get account id
16:22:02 365  	SELECT
16:22:02 366  	  ACCOUNT.GROUP_ID into var_group_id
16:22:02 367  	FROM
16:22:02 368  	  ACCOUNT
16:22:02 369  	WHERE
16:22:02 370  	  ACCOUNT.ID = var_account_id;
16:22:02 371  
16:22:02 372  	-- Card should to be active
16:22:02 373  	IF var_credit_card_status != GLOBAL_STATUSES_V22.CREDIT_CARD_ACTIVE THEN
16:22:02 374  	  RAISE BAD_CC_STATUS;
16:22:02 375  	END IF;
16:22:02 376  
16:22:02 377  	-- Looking for pending transactions associated with given credit card
16:22:02 378  	SELECT
16:22:02 379  	  COUNT(*) into var_pending_invoices_num
16:22:02 380  	FROM
16:22:02 381  	  CHARGE
16:22:02 382  	  INNER JOIN TRANSACTION ON CHARGE.TRANSACTION_ID = TRANSACTION.ID
16:22:02 383  	WHERE
16:22:02 384  	  CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V22.INSTRUMENT_CREDIT_CARD
16:22:02 385  	  AND CHARGE.INSTRUMENT_ID = in_credit_card_id
16:22:02 386  	  AND TRANSACTION.TRANSACTION_STATUS_ID = GLOBAL_STATUSES_V22.TRANSACTION_PENDING;
16:22:02 387  
16:22:02 388  	IF var_pending_invoices_num > 0 THEN
16:22:02 389  	  RAISE PENDING_TRANSACTIONS_FOUNDED;
16:22:02 390  	END IF;
16:22:02 391  
16:22:02 392  	-- Getting current default financial instrument
16:22:02 393  	BEGIN
16:22:02 394  	  GET_DEF_FINANCIAL_INSTRUMENT(
16:22:02 395  	    in_group_id 	   => var_group_id,
16:22:02 396  	    out_instrument_type_id => current_def_instrument_type_id,
16:22:02 397  	    out_instrument_id	   => current_def_instrument_id
16:22:02 398  	  );
16:22:02 399  	  EXCEPTION
16:22:02 400  	    WHEN OTHERS THEN
16:22:02 401  	      EXCEPTION_MESSAGE := SQLERRM;
16:22:02 402  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
16:22:02 403  	END;
16:22:02 404  
16:22:02 405  	-- Checking that credit card is not default
16:22:02 406  	IF current_def_instrument_type_id = GLOBAL_ENUMS_V22.INSTRUMENT_CREDIT_CARD
16:22:02 407  	  AND current_def_instrument_id = in_credit_card_id THEN
16:22:02 408  	  BEGIN
16:22:02 409  	    DEL_DEF_FINANCIAL_INSTRUMENT(
16:22:02 410  	      in_group_id => var_group_id
16:22:02 411  	    );
16:22:02 412  	  EXCEPTION
16:22:02 413  	    WHEN OTHERS THEN
16:22:02 414  	      EXCEPTION_MESSAGE := SQLERRM;
16:22:02 415  	      RAISE CAN_NOT_DEL_DEF_FINANCIAL;
16:22:02 416  	  END;
16:22:02 417  	END IF;
16:22:02 418  
16:22:02 419  	-- Update credit card status
16:22:02 420  	PROCS_FIN_INSTRUMENTS_V22.UPDATE_CREDIT_CARD_STATUS(
16:22:02 421  	  in_credit_card_id	   => in_credit_card_id,
16:22:02 422  	  in_updated_by 	   => in_updated_by,
16:22:02 423  	  in_credit_card_status_id => GLOBAL_STATUSES_V22.CREDIT_CARD_DISABLED
16:22:02 424  	);
16:22:02 425  
16:22:02 426  EXCEPTION
16:22:02 427  WHEN NO_DATA_FOUND THEN
16:22:02 428  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:02 429  	  SPROC_NAME, 'No such credit card');
16:22:02 430  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
16:22:02 431  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:02 432  	  SPROC_NAME, 'Could not get current default financial instrument', EXCEPTION_MESSAGE);
16:22:02 433  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
16:22:02 434  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:02 435  	  SPROC_NAME, 'Could not delete information about default financial instrument from account', EXCEPTION_MESSAGE);
16:22:02 436  WHEN BAD_CC_STATUS THEN
16:22:02 437  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.STATE_ERROR,
16:22:02 438  	  SPROC_NAME, 'Credit card is not active');
16:22:02 439  WHEN PENDING_TRANSACTIONS_FOUNDED THEN
16:22:02 440  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.STATE_ERROR,
16:22:02 441  	  SPROC_NAME, 'Account has pending charge which is using this card');
16:22:02 442  WHEN CAN_NOT_DISABLE_CREDIT_CARD THEN
16:22:02 443  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:02 444  	  SPROC_NAME, 'Could not disable this credit card', EXCEPTION_MESSAGE);
16:22:02 445  WHEN OTHERS THEN
16:22:02 446  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:02 447  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:22:02 448  END DISABLE_CREDIT_CARD;
