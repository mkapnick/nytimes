16:22:02 180  	SELECT
16:22:02 181  	  COUNT(*) into var_pending_invoices_num
16:22:02 182  	FROM
16:22:02 183  	  LICENSE
16:22:02 184  	    INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
16:22:02 185  	    INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
16:22:02 186  	WHERE
