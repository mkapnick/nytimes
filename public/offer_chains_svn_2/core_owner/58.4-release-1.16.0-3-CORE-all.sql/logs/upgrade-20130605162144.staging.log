16:22:02 1658  
16:22:02 1659  PROCEDURE REFUND_INVOICE (
16:22:02 1660  	 in_invoice_id	    IN NUMBER,
16:22:02 1661  	 in_refund_amount   IN NUMBER,
16:22:02 1662  	 in_note	    IN VARCHAR2,
16:22:02 1663  	 in_created_by	    IN VARCHAR2,
16:22:02 1664  	 out_charge_id	    OUT NUMBER
16:22:02 1665  ) AS
16:22:02 1666  SPROC_NAME CONSTANT VARCHAR2(14) := 'REFUND_INVOICE';
16:22:02 1667  -- VARIABLES
16:22:02 1668  var_invoice_status_id  NUMBER;
16:22:02 1669  var_subscription_id    NUMBER;
16:22:02 1670  var_account_id	      NUMBER;
16:22:02 1671  var_group_id	      NUMBER;
16:22:02 1672  var_account_status_id  NUMBER;
16:22:02 1673  var_new_transaction_id NUMBER;
16:22:02 1674  var_instrument_type_id NUMBER;
16:22:02 1675  var_instrument_id      NUMBER;
16:22:02 1676  var_new_charge_id      NUMBER;
16:22:02 1677  var_invoice_amount     NUMBER(10,2);
16:22:02 1678  var_refunds_before     NUMBER(10,2);
16:22:02 1679  var_charges_amount     NUMBER(10,2);
16:22:02 1680  -- EXCEPTIONS
16:22:02 1681  CAN_NOT_FIND_SUBSCR_OR_GC     EXCEPTION;
16:22:02 1682  ACCOUNT_IS_FROZEN	     EXCEPTION;
16:22:02 1683  BAD_INVOICE_ID		     EXCEPTION;
16:22:02 1684  CAN_NOT_CREATE_TRANSACTION    EXCEPTION;
16:22:02 1685  CAN_NOT_CREATE_CHARGE	     EXCEPTION;
16:22:02 1686  CAN_NOT_CALC_INVOICE_AMOUNT   EXCEPTION;
16:22:02 1687  REFUND_IS_GREATER_THAN_ANOUNT EXCEPTION;
16:22:02 1688  CAN_NOT_ANNOTATE_SUBSCRIPTION EXCEPTION;
16:22:02 1689  TOT_REF_IS_GREATER_THAN_ANOUNT EXCEPTION;
16:22:02 1690  INVOICE_IS_NOT_CLOSED	     EXCEPTION;
16:22:02 1691  TOT_REF_IS_GRATER_THAN_CHARGES EXCEPTION;
16:22:02 1692  EXCEPTION_MESSAGE	      VARCHAR2(1024);
16:22:02 1693  BEGIN
16:22:02 1694  
16:22:02 1695  	 BEGIN
16:22:02 1696  	   SELECT
16:22:02 1697  	     INVOICE.INVOICE_STATUS_ID into var_invoice_status_id
16:22:02 1698  	   FROM
16:22:02 1699  	     INVOICE
16:22:02 1700  	   WHERE
16:22:02 1701  	     INVOICE.ID = in_invoice_id;
16:22:02 1702  	   EXCEPTION
16:22:02 1703  	     WHEN NO_DATA_FOUND THEN
16:22:02 1704  	       RAISE BAD_INVOICE_ID;
16:22:02 1705  	 END;
16:22:02 1706  
16:22:02 1707  	 -- Get instrument and subscription id if exists
16:22:02 1708  	 BEGIN
16:22:02 1709  	   SELECT
16:22:02 1710  	     SUBSCRIPTION.INSTRUMENT_ID,
16:22:02 1711  	     SUBSCRIPTION.INSTRUMENT_TYPE_ID,
16:22:02 1712  	     SUBSCRIPTION.ACCOUNT_ID,
16:22:02 1713  	     SUBSCRIPTION.ID
16:22:02 1714  	     into
16:22:02 1715  	     var_instrument_id,
16:22:02 1716  	     var_instrument_type_id,
16:22:02 1717  	     var_account_id,
16:22:02 1718  	     var_subscription_id
16:22:02 1719  	   FROM
16:22:02 1720  	     SUBSCRIPTION
16:22:02 1721  	     INNER JOIN LICENSE ON SUBSCRIPTION.ID = LICENSE.SUBSCRIPTION_ID
16:22:02 1722  	   WHERE
16:22:02 1723  	     LICENSE.INVOICE_ID = in_invoice_id
16:22:02 1724  	     AND ROWNUM <= 1;
16:22:02 1725  	   EXCEPTION
16:22:02 1726  	     WHEN NO_DATA_FOUND THEN
16:22:02 1727  	       BEGIN
16:22:02 1728  
16:22:02 1729  		 var_subscription_id := NULL;
16:22:02 1730  
16:22:02 1731  		 SELECT
16:22:02 1732  		   CHARGE.INSTRUMENT_ID,
16:22:02 1733  		   CHARGE.INSTRUMENT_TYPE_ID,
16:22:02 1734  		   GIFT_CERTIFICATE.PURCHASER_GROUP_ID
16:22:02 1735  		   into
16:22:02 1736  		   var_instrument_id,
16:22:02 1737  		   var_instrument_type_id,
16:22:02 1738  		   var_group_id
16:22:02 1739  		 FROM
16:22:02 1740  		   INVOICE
16:22:02 1741  		   INNER JOIN CHARGE ON INVOICE.ID = CHARGE.INVOICE_ID
16:22:02 1742  		   INNER JOIN GIFT_CERTIFICATE ON GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = INVOICE.ID
16:22:02 1743  		 WHERE
16:22:02 1744  		   INVOICE.ID = in_invoice_id
16:22:02 1745  		   AND ROWNUM <= 1;
16:22:02 1746  
16:22:02 1747  		 SELECT
16:22:02 1748  		   ACCOUNT.ID into var_account_id
16:22:02 1749  		 FROM
16:22:02 1750  		   ACCOUNT
16:22:02 1751  		 WHERE
16:22:02 1752  		   ACCOUNT.GROUP_ID = var_group_id;
16:22:02 1753  
16:22:02 1754  		 EXCEPTION
16:22:02 1755  		   WHEN NO_DATA_FOUND THEN
16:22:02 1756  		     RAISE CAN_NOT_FIND_SUBSCR_OR_GC;
16:22:02 1757  	       END;
16:22:02 1758  	 END;
16:22:02 1759  
16:22:02 1760  	 -- Check account status. It should not to be frozen
16:22:02 1761  	 SELECT
16:22:02 1762  	   ACCOUNT.ACCOUNT_STATUS_ID into var_account_status_id
16:22:02 1763  	 FROM
16:22:02 1764  	   ACCOUNT
16:22:02 1765  	 WHERE
16:22:02 1766  	   ACCOUNT.ID = var_account_id;
16:22:02 1767  
16:22:02 1768  	 IF var_account_status_id = GLOBAL_STATUSES_V22.ACCOUNT_FROZEN THEN
16:22:02 1769  	   RAISE ACCOUNT_IS_FROZEN;
16:22:02 1770  	 END IF;
16:22:02 1771  
16:22:02 1772  	 IF var_invoice_status_id != GLOBAL_STATUSES_V22.INVOICE_CLOSED THEN
16:22:02 1773  	   RAISE INVOICE_IS_NOT_CLOSED;
16:22:02 1774  	 END IF;
16:22:02 1775  
16:22:02 1776  	 BEGIN
16:22:02 1777  	   PROCS_INVOICE_V22.CALCULATE_INVOICE_AMOUNT (
16:22:02 1778  	     in_invoice_id => in_invoice_id,
16:22:02 1779  	     out_amount    => var_invoice_amount
16:22:02 1780  	   );
16:22:02 1781  	   EXCEPTION
16:22:02 1782  	     WHEN OTHERS THEN
16:22:02 1783  	       EXCEPTION_MESSAGE := SQLERRM;
16:22:02 1784  	       RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
16:22:02 1785  	 END;
16:22:02 1786  
16:22:02 1787  	 IF ( in_refund_amount > var_invoice_amount ) THEN
16:22:02 1788  	   RAISE REFUND_IS_GREATER_THAN_ANOUNT;
16:22:02 1789  	 END IF;
16:22:02 1790  
16:22:02 1791  	 SELECT /*+ STAR_TRANSFORMATION */
16:22:02 1792  	   SUM(CHARGE.CHARGE_AMOUNT) into var_refunds_before
16:22:02 1793  	 FROM
16:22:02 1794  	   CHARGE
16:22:02 1795  	 WHERE
16:22:02 1796  	   CHARGE.INVOICE_ID = in_invoice_id
16:22:02 1797  	   AND (
16:22:02 1798  	     CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V22.CHARGE_OPENED
16:22:02 1799  	     OR (
16:22:02 1800  	       CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V22.CHARGE_PROCESSED
16:22:02 1801  	       AND EXISTS (
16:22:02 1802  		 SELECT 1 FROM TRANSACTION_ATTEMPT ta where ta.transaction_id = CHARGE.TRANSACTION_ID and ta.transaction_attempt_status_id = GLOBAL_STATUSES_V22.TRANS_ATTEMPT_SUCCESS
16:22:02 1803  	       )
16:22:02 1804  	     )
16:22:02 1805  	   )
16:22:02 1806  	   AND CHARGE.CHARGE_AMOUNT < 0;
16:22:02 1807  
16:22:02 1808  	 -- Refunds are negative
16:22:02 1809  	 IF var_refunds_before IS NULL THEN var_refunds_before := 0; END IF;
16:22:02 1810  	 var_refunds_before := 0 - var_refunds_before;
16:22:02 1811  
16:22:02 1812  	 var_charges_amount := 0;
16:22:02 1813  
16:22:02 1814  	 FOR f_processed_charges IN (
16:22:02 1815  	   SELECT
16:22:02 1816  	     CHARGE.CHARGE_AMOUNT
16:22:02 1817  	   FROM
16:22:02 1818  	     CHARGE
16:22:02 1819  	   WHERE
16:22:02 1820  	     CHARGE.INVOICE_ID = in_invoice_id
16:22:02 1821  	     AND CHARGE.CHARGE_AMOUNT > 0
16:22:02 1822  	     AND CHARGE.CHARGE_STATUS_ID IN (SELECT GLOBAL_STATUSES_V22.CHARGE_PROCESSED FROM DUAL)
16:22:02 1823  	     AND EXISTS (SELECT 1 FROM TRANSACTION_ATTEMPT ta where ta.transaction_id = CHARGE.TRANSACTION_ID and ta.transaction_attempt_status_id = GLOBAL_STATUSES_V22.TRANS_ATTEMPT_SUCCESS)
16:22:02 1824  	 )
16:22:02 1825  	 LOOP
16:22:02 1826  	   var_charges_amount := var_charges_amount + f_processed_charges.CHARGE_AMOUNT;
16:22:02 1827  	 END LOOP;
16:22:02 1828  
16:22:02 1829  	 IF (in_refund_amount + var_refunds_before > var_invoice_amount) THEN
16:22:02 1830  	   RAISE TOT_REF_IS_GREATER_THAN_ANOUNT;
16:22:02 1831  	 END IF;
16:22:02 1832  
16:22:02 1833  	 IF (in_refund_amount + var_refunds_before > var_charges_amount) THEN
16:22:02 1834  	   RAISE TOT_REF_IS_GRATER_THAN_CHARGES;
16:22:02 1835  	 END IF;
16:22:02 1836  
16:22:02 1837  	 BEGIN
16:22:02 1838  	   PROCS_TRANSACTION_V22.CREATE_TRANSACTION(
16:22:02 1839  	     in_transaction_id	       => NULL,
16:22:02 1840  	     in_status_id	       => GLOBAL_STATUSES_V22.TRANSACTION_PREPARE,
16:22:02 1841  	     in_amount		       => -in_refund_amount,
16:22:02 1842  	     in_created_by	       => in_created_by,
16:22:02 1843  	     in_order_id	       => NULL,
16:22:02 1844  	     in_is_refund	       => GLOBAL_CONSTANTS_V22.TRUE,
16:22:02 1845  	     in_transaction_type_code  => 'REFUND',
16:22:02 1846  	     out_transaction_id        => var_new_transaction_id
16:22:02 1847  	   );
16:22:02 1848  	   EXCEPTION
16:22:02 1849  	     WHEN OTHERS THEN
16:22:02 1850  	       EXCEPTION_MESSAGE := SQLERRM;
16:22:02 1851  	       RAISE CAN_NOT_CREATE_TRANSACTION;
16:22:02 1852  	 END;
16:22:02 1853  
16:22:02 1854  	 BEGIN
16:22:02 1855  	   PROCS_CHARGE_V22.CREATE_CHARGE(
16:22:02 1856  	     in_invoice_id	   => in_invoice_id,
