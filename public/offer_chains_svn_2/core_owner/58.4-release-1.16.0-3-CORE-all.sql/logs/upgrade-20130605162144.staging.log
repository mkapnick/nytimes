16:22:01 936  
16:22:01 937  FUNCTION GET_RENEWAL_DATE (
16:22:01 938  /*
16:22:01 939  Throws exceptions:
16:22:01 940  APP_EXCEPTION_CODES_V22.NOT_FOUND
16:22:01 941  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
16:22:01 942  */
16:22:01 943  	in_subscription_id in NUMBER
16:22:01 944  ) RETURN DATE AS
16:22:01 945  -- VARIABLES
16:22:01 946  SPROC_NAME	      CONSTANT VARCHAR2(16) := 'GET_RENEWAL_DATE';
16:22:01 947  var_subscription_status NUMBER;
16:22:01 948  var_licenses_count      NUMBER;
16:22:01 949  var_license_end_date    DATE;
16:22:01 950  var_last_offer_id       NUMBER;
16:22:01 951  var_offer_chain_id      NUMBER;
16:22:01 952  var_last_license_id     NUMBER;
16:22:01 953  var_current_offer_index NUMBER;
16:22:01 954  var_current_offer_recurr_num NUMBER;
16:22:01 955  var_offer_recurr_num    NUMBER;
16:22:01 956  -- EXCEPTIONS
16:22:01 957  BAD_SUBSCRIPTION_ID EXCEPTION;
16:22:01 958  NO_LICENSES_FOUND EXCEPTION;
16:22:01 959  BEGIN
16:22:01 960  
16:22:01 961  	-- Get subscription id and offer chain id
16:22:01 962  	BEGIN
16:22:01 963  	  SELECT
16:22:01 964  	    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID,
16:22:01 965  	    SUBSCRIPTION.OFFER_CHAIN_ID
16:22:01 966  	    into
16:22:01 967  	    var_subscription_status,
16:22:01 968  	    var_offer_chain_id
16:22:01 969  	  FROM
16:22:01 970  	    SUBSCRIPTION
16:22:01 971  	  WHERE
16:22:01 972  	    SUBSCRIPTION.ID = in_subscription_id;
16:22:01 973  	  EXCEPTION
16:22:01 974  	    WHEN NO_DATA_FOUND THEN
16:22:01 975  	      RAISE BAD_SUBSCRIPTION_ID;
16:22:01 976  	END;
16:22:01 977  
16:22:01 978  	IF var_subscription_status != GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE THEN
16:22:01 979  	  -- TODO: Is suspended subscription has renewal date? (For the phase II)
16:22:01 980  	  -- AND var_subscription_status != GLOBAL_STATUSES_V22.SUBSCRIPTION_SUSPENDED THEN
16:22:01 981  	  RETURN NULL;
16:22:01 982  	END IF;
16:22:01 983  
16:22:01 984  	BEGIN
16:22:01 985  	  SELECT
16:22:01 986  	    LICENSE_ID,
16:22:01 987  	    END_DATE,
16:22:01 988  	    OFFER_ID,
16:22:01 989  	    CURRENT_OFFER_INDEX,
16:22:01 990  	    CURRENT_OFFER_RECURR_NUM
16:22:01 991  	    into
16:22:01 992  	    var_last_license_id,
16:22:01 993  	    var_license_end_date,
16:22:01 994  	    var_last_offer_id,
16:22:01 995  	    var_current_offer_index,
16:22:01 996  	    var_current_offer_recurr_num
16:22:01 997  	  FROM
16:22:01 998  	    (
16:22:01 999  	      SELECT
16:22:01 1000  		 LICENSE.ID as "LICENSE_ID",
16:22:01 1001  		 LICENSE.END_DATE,
16:22:01 1002  		 LICENSE.OFFER_ID,
16:22:01 1003  		 LICENSE.CURRENT_OFFER_INDEX,
16:22:01 1004  		 LICENSE.CURRENT_OFFER_RECURR_NUM
16:22:01 1005  	       FROM
16:22:01 1006  		 LICENSE
16:22:01 1007  	       WHERE
16:22:01 1008  		 LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V22.LICENSE_ACTIVE
16:22:01 1009  		 AND LICENSE.SUBSCRIPTION_ID = in_subscription_id
16:22:01 1010  	       ORDER BY END_DATE DESC
16:22:01 1011  	     )
16:22:01 1012  	     INNER JOIN OFFER ON OFFER_ID = OFFER.ID
16:22:01 1013  	   WHERE
16:22:01 1014  	     ROWNUM <= 1;
16:22:01 1015  
16:22:01 1016  	   EXCEPTION
16:22:01 1017  	     WHEN NO_DATA_FOUND THEN
16:22:01 1018  	       -- RAISE NO_LICENSES_FOUND;
16:22:01 1019  	       RETURN NULL;
16:22:01 1020  	 END;
16:22:01 1021  
16:22:01 1022  	 SELECT
16:22:01 1023  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES into var_offer_recurr_num
16:22:01 1024  	 FROM
16:22:01 1025  	   OFFER_OFFER_CHAIN
16:22:01 1026  	 WHERE
16:22:01 1027  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
16:22:01 1028  	   AND OFFER_OFFER_CHAIN.OFFER_ID = var_last_offer_id;
16:22:01 1029  
16:22:01 1030  	 IF PROCS_OFFER_CHAIN_V22.GET_NEXT_OFFER_INDEX(var_offer_chain_id, var_current_offer_index) IS NULL
16:22:01 1031  	   AND var_offer_recurr_num = var_current_offer_recurr_num THEN
16:22:01 1032  	   -- There is no next offer for this subscription
16:22:01 1033  	   RETURN NULL;
16:22:01 1034  	 END IF;
16:22:01 1035  
16:22:01 1036  	 RETURN var_license_end_date;
16:22:01 1037  
16:22:01 1038  EXCEPTION
16:22:01 1039  WHEN BAD_SUBSCRIPTION_ID THEN
16:22:01 1040  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 1041  	   SPROC_NAME, 'No such subscription');
16:22:01 1042  WHEN NO_LICENSES_FOUND THEN
16:22:01 1043  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 1044  	   SPROC_NAME, 'No licenses found');
16:22:01 1045  WHEN OTHERS THEN
16:22:01 1046  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:01 1047  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:22:01 1048  END GET_RENEWAL_DATE;
16:22:01 1049  
16:22:01 1050  /******************************************************************************/
16:22:01 1051  
16:22:01 1052  FUNCTION GET_RECENT_CHARGE (
16:22:01 1053  /*
16:22:01 1054  Throws exceptions:
16:22:01 1055  APP_EXCEPTION_CODES_V22.NOT_FOUND
16:22:01 1056  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
16:22:01 1057  */
16:22:01 1058  	 in_subscription_id IN NUMBER
16:22:01 1059  ) RETURN NUMBER AS
16:22:01 1060  -- VARIABLES
16:22:01 1061  SPROC_NAME	    CONSTANT VARCHAR2(17) := 'GET_RECENT_CHARGE';
16:22:01 1062  temp_subscription_id NUMBER;
16:22:01 1063  var_recent_charge    NUMBER(10,2);
16:22:01 1064  
16:22:01 1065  -- EXCEPTIONS
16:22:01 1066  BAD_SUBSCRIPTION_ID EXCEPTION;
16:22:01 1067  BEGIN
16:22:01 1068  
16:22:01 1069  	 BEGIN
16:22:01 1070  	   SELECT
16:22:01 1071  	     SUBSCRIPTION.ID into temp_subscription_id
16:22:01 1072  	   FROM
16:22:01 1073  	     SUBSCRIPTION
16:22:01 1074  	   WHERE
16:22:01 1075  	     SUBSCRIPTION.ID = in_subscription_id;
16:22:01 1076  	   EXCEPTION
16:22:01 1077  	     WHEN NO_DATA_FOUND THEN
16:22:01 1078  	       RAISE BAD_SUBSCRIPTION_ID;
16:22:01 1079  	 END;
16:22:01 1080  
16:22:01 1081  	 BEGIN
16:22:01 1082  	   SELECT
16:22:01 1083  	     CHARGE.CHARGE_AMOUNT into var_recent_charge
16:22:01 1084  	   FROM
16:22:01 1085  	     LICENSE
16:22:01 1086  	     INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
16:22:01 1087  	     INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
16:22:01 1088  	     INNER JOIN CHARGE ON CHARGE.INVOICE_ID = INVOICE.ID
16:22:01 1089  	   WHERE
16:22:01 1090  	     -- TODO: Review
16:22:01 1091  	     -- LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V22.LICENSE_PROCESSED
16:22:01 1092  	     -- AND
16:22:01 1093  	     SUBSCRIPTION.ID = in_subscription_id
16:22:01 1094  	     AND CHARGE.CHARGE_AMOUNT >= 0
16:22:01 1095  	     AND ROWNUM <= 1
16:22:01 1096  	   ORDER BY
16:22:01 1097  	     LICENSE.ID ASC, CHARGE.ID DESC;
16:22:01 1098  	   EXCEPTION
16:22:01 1099  	     WHEN NO_DATA_FOUND THEN
16:22:01 1100  	       var_recent_charge := 0;
16:22:01 1101  	 END;
16:22:01 1102  
16:22:01 1103  	 RETURN var_recent_charge;
16:22:01 1104  
16:22:01 1105  EXCEPTION
16:22:01 1106  WHEN BAD_SUBSCRIPTION_ID THEN
16:22:01 1107  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 1108  	   SPROC_NAME, 'No such subscription');
16:22:01 1109  WHEN OTHERS THEN
16:22:01 1110  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:01 1111  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:22:01 1112  END GET_RECENT_CHARGE;
