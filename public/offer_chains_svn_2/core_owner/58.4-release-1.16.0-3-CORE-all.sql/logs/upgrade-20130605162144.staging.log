16:22:02 239  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:02 240  	  SPROC_NAME, 'Opened/Pending invoices founded');
16:22:02 241  WHEN BAD_ACOUNT_ID THEN
16:22:02 242  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
