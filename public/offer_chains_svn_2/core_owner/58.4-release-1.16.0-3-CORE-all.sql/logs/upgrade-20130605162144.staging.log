16:22:02 564  	  SPROC_NAME, 'Paypal already inactive');
16:22:02 565  WHEN PENDING_TRANSACTIONS_FOUND THEN
16:22:02 566  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.STATE_ERROR,
16:22:02 567  	  SPROC_NAME, 'Account has pending charge which are using this paypal');
16:22:02 568  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
16:22:02 569  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:02 570  	  SPROC_NAME, 'Could not get current default financial instrument', EXCEPTION_MESSAGE);
16:22:02 571  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
16:22:02 572  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:02 573  	  SPROC_NAME, 'Could not delete information about default financial instrument from account', EXCEPTION_MESSAGE);
16:22:02 574  WHEN OTHERS THEN
16:22:02 575  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
