16:22:01 2572  
16:22:01 2573  FUNCTION IS_SUBSCRIPTION_CANCELABLE (
16:22:01 2574  	 in_subscription_id IN NUMBER
16:22:01 2575  ) RETURN NUMBER AS
16:22:01 2576  SPROC_NAME CONSTANT VARCHAR2(26) := 'IS_SUBSCRIPTION_CANCELABLE';
16:22:01 2577  -- VARIABLES
16:22:01 2578  var_is_offer_chain_cancelable NUMBER;
16:22:01 2579  --64603
16:22:01 2580  var_end_date date;
16:22:01 2581  today_date date := current_date;
16:22:01 2582  offer_id number;
16:22:01 2583  -- EXCEPTIONS
16:22:01 2584  COULD_NOT_CHECK	   EXCEPTION;
16:22:01 2585  BAD_SUBSCRIPTION_ID EXCEPTION;
16:22:01 2586  EXCEPTION_MESSAGE   VARCHAR2(1024);
16:22:01 2587  BEGIN
16:22:01 2588  
16:22:01 2589  	 BEGIN
16:22:01 2590  	   -- find offer_chain_id for given in_subscription_id
16:22:01 2591  	   SELECT OFFER_CHAIN_ID into offer_id
16:22:01 2592  	   FROM SUBSCRIPTION
16:22:01 2593  	   WHERE ID = in_subscription_id;
16:22:01 2594  	   EXCEPTION
16:22:01 2595  	     WHEN NO_DATA_FOUND THEN
16:22:01 2596  	       RAISE BAD_SUBSCRIPTION_ID;
16:22:01 2597  	     WHEN OTHERS THEN
16:22:01 2598  	       EXCEPTION_MESSAGE := SQLERRM;
16:22:01 2599  	       RAISE COULD_NOT_CHECK;
16:22:01 2600  	 END;
16:22:01 2601  
16:22:01 2602  	 -- find if it was redeemed from a gift certificate
16:22:01 2603  	 BEGIN
16:22:01 2604  	   SELECT l.end_date INTO var_end_date
16:22:01 2605  	   FROM LICENSE l, GIFT_CERTIFICATE g
16:22:01 2606  	   WHERE l.invoice_id = g.finalized_invoice_id
16:22:01 2607  	   AND l.subscription_id = in_subscription_id;
16:22:01 2608  
16:22:01 2609  	   -- if the license end_date is bigger than today, we are in the
16:22:01 2610  	   -- first period, so we cannot cancel; otherwise can cancel
16:22:01 2611  	   IF var_end_date > today_date THEN
16:22:01 2612  	       RETURN GLOBAL_CONSTANTS_V22.FALSE;
16:22:01 2613  	   ELSE
16:22:01 2614  	       RETURN GLOBAL_CONSTANTS_V22.TRUE;
16:22:01 2615  	   END IF;
16:22:01 2616  
16:22:01 2617  	   EXCEPTION
16:22:01 2618  	       -- not coming from a gift certificate,
16:22:01 2619  	       -- use old logic
16:22:01 2620  	       WHEN NO_DATA_FOUND THEN
16:22:01 2621  		   SELECT
16:22:01 2622  		       PROCS_OFFER_CHAIN_V22.IS_OFFER_CHAIN_CANCELABLE(offer_id)
16:22:01 2623  		       INTO var_is_offer_chain_cancelable
16:22:01 2624  		   FROM DUAL;
16:22:01 2625  		   RETURN var_is_offer_chain_cancelable;
16:22:01 2626  	 END;
16:22:01 2627  
16:22:01 2628  EXCEPTION
16:22:01 2629  WHEN BAD_SUBSCRIPTION_ID THEN
16:22:01 2630  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 2631  	   SPROC_NAME, 'No such subscription');
16:22:01 2632  WHEN COULD_NOT_CHECK THEN
16:22:01 2633  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:01 2634  	   SPROC_NAME, 'Could not check if offer chain calcelable', EXCEPTION_MESSAGE);
16:22:01 2635  WHEN OTHERS THEN
16:22:01 2636  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:01 2637  	   SPROC_NAME, 'Unknown error', SQLERRM);
16:22:01 2638  
16:22:01 2639  END IS_SUBSCRIPTION_CANCELABLE;
