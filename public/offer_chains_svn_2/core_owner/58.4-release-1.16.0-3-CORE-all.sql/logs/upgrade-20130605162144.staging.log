16:22:01 415  /******************************************************************************/
16:22:01 416  
16:22:01 417  PROCEDURE FINALIZE_SUBSCRIPTION_CREATION (
16:22:01 418  	in_subscription_id    IN NUMBER,
16:22:01 419  	in_invoice_id	      IN NUMBER,
16:22:01 420  	in_instrument_type_id IN NUMBER,
16:22:01 421  	in_instrument_id      IN NUMBER,
16:22:01 422  	in_created_by	      IN VARCHAR2
16:22:01 423  ) AS
16:22:01 424  SPROC_NAME CONSTANT VARCHAR2(30) := 'FINALIZE_SUBSCRIPTION_CREATION';
16:22:01 425  -- VARIABLES
16:22:01 426  var_invoice_amount     NUMBER(10, 2);
16:22:01 427  var_new_transaction_id NUMBER;
16:22:01 428  var_new_charge_id      NUMBER;
16:22:01 429  -- EXCEPTIONS
16:22:01 430  CAN_NOT_USE_FCINSTR	  EXCEPTION;
16:22:01 431  CAN_NOT_CALC_INVOICE_AMOUNT EXCEPTION;
16:22:01 432  CAN_NOT_CREATE_TRANSACTION  EXCEPTION;
16:22:01 433  CAN_NOT_CREATE_CHARGE	  EXCEPTION;
16:22:01 434  
16:22:01 435  EXCEPTION_MESSAGE VARCHAR2(1024);
16:22:01 436  BEGIN
16:22:01 437  
16:22:01 438  	-- Calculate invoice amount ( + discounts, taxes)
16:22:01 439  	BEGIN
16:22:01 440  	  PROCS_INVOICE_V22.CALCULATE_INVOICE_AMOUNT(
16:22:01 441  	    in_invoice_id => in_invoice_id,
16:22:01 442  	    out_amount	  => var_invoice_amount
16:22:01 443  	  );
16:22:01 444  	  EXCEPTION
16:22:01 445  	   WHEN OTHERS THEN
16:22:01 446  	     EXCEPTION_MESSAGE := SQLERRM;
16:22:01 447  	     RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
16:22:01 448  	END;
16:22:01 449  
16:22:01 450  	IF in_instrument_type_id = GLOBAL_ENUMS_V22.INSTRUMENT_ZCI_INSTRUMENT
16:22:01 451  	  AND var_invoice_amount > 0 THEN
16:22:01 452  	  RAISE CAN_NOT_USE_FCINSTR;
16:22:01 453  	END IF;
16:22:01 454  
16:22:01 455  	IF var_invoice_amount = 0 THEN
16:22:01 456  	  -- UPDATE INVOICE. SET STATUS TO PROCESSED
16:22:01 457  	  PROCS_INVOICE_CRU_V22.UPDATE_INVOICE(
16:22:01 458  	    in_invoice_id		   => in_invoice_id,
16:22:01 459  	    in_updated_by		   => in_created_by,
16:22:01 460  	    in_invoice_status_id	   => GLOBAL_STATUSES_V22.INVOICE_CLOSED
16:22:01 461  	  );
16:22:01 462  	ELSE
16:22:01 463  	  -- Create transaction and charge
16:22:01 464  	  BEGIN
16:22:01 465  	    PROCS_TRANSACTION_V22.CREATE_TRANSACTION(
16:22:01 466  	      in_transaction_id 	=> NULL,
16:22:01 467  	      in_status_id		=> GLOBAL_STATUSES_V22.TRANSACTION_PENDING,
16:22:01 468  	      in_amount 		=> var_invoice_amount,
16:22:01 469  	      in_created_by		=> in_created_by,
16:22:01 470  	      in_order_id		=> NULL,
16:22:01 471  	      in_transaction_type_code	=> 'START_SUBSCRIPTION',
16:22:01 472  	      out_transaction_id	=> var_new_transaction_id
16:22:01 473  	    );
16:22:01 474  	    EXCEPTION
16:22:01 475  	      WHEN OTHERS THEN
16:22:01 476  		EXCEPTION_MESSAGE := SQLERRM;
16:22:01 477  		RAISE CAN_NOT_CREATE_TRANSACTION;
16:22:01 478  	  END;
16:22:01 479  
16:22:01 480  	  BEGIN
16:22:01 481  	    PROCS_CHARGE_V22.CREATE_CHARGE(
16:22:01 482  	      in_invoice_id	    => in_invoice_id,
16:22:01 483  	      in_transaction_id     => var_new_transaction_id,
16:22:01 484  	      in_instrument_type_id => in_instrument_type_id,
16:22:01 485  	      in_instrument_id	    => in_instrument_id,
16:22:01 486  	      in_charge_amount	    => var_invoice_amount,
16:22:01 487  	      in_created_by	    => in_created_by,
16:22:01 488  	      in_charge_status_id   => GLOBAL_STATUSES_V22.CHARGE_OPENED,
16:22:01 489  	      out_charge_id	    => var_new_charge_id
16:22:01 490  	    );
16:22:01 491  	    EXCEPTION
16:22:01 492  	      WHEN OTHERS THEN
16:22:01 493  		EXCEPTION_MESSAGE := SQLERRM;
16:22:01 494  		RAISE CAN_NOT_CREATE_CHARGE;
16:22:01 495  	  END;
16:22:01 496  	END IF;
16:22:01 497  
16:22:01 498  EXCEPTION
16:22:01 499  WHEN CAN_NOT_USE_FCINSTR THEN
16:22:01 500  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.STATE_ERROR,
16:22:01 501  	  SPROC_NAME, 'Could not use "free charge instrument" for non-zero invoice');
16:22:01 502  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
16:22:01 503  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:01 504  	  SPROC_NAME, 'Could not calculate invoice amount', EXCEPTION_MESSAGE);
16:22:01 505  WHEN CAN_NOT_CREATE_TRANSACTION THEN
16:22:01 506  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:01 507  	  SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
16:22:01 508  WHEN CAN_NOT_CREATE_CHARGE THEN
16:22:01 509  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:01 510  	  SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
16:22:01 511  WHEN OTHERS THEN
16:22:01 512  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:01 513  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:22:01 514  END FINALIZE_SUBSCRIPTION_CREATION;
