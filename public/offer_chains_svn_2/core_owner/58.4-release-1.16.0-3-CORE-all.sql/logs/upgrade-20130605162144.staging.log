16:22:01 160  PROCEDURE GET_TRANSFER_SET
16:22:01 161  	  (
16:22:01 162  	      in_set_maximum NUMBER,
16:22:01 163  	      in_max_retries NUMBER,
16:22:01 164  	      out_refcursor OUT sys_refcursor)
16:22:01 165  IS
16:22:01 166  	  v_set_id core_owner.polling_sync.set_id%type;
16:22:01 167  BEGIN
16:22:01 168  	  /* Look for previously sent but unconfirmed sets and
16:22:01 169  	  send again until max_retries calls */
16:22:01 170  	  FOR x IN
16:22:01 171  	  (
16:22:01 172  	      SELECT
16:22:01 173  		  ps.set_id,
16:22:01 174  		  COUNT( *) set_size
16:22:01 175  	      FROM
16:22:01 176  		  core_owner.polling_sync ps
16:22:01 177  	      WHERE
16:22:01 178  		  ps.event_date > sysdate - 14
16:22:01 179  	      AND ps.confirm_date IS NULL
16:22:01 180  	      AND ps.last_send_date IS NOT NULL
16:22:01 181  	      AND ps.num_calls < in_max_retries
16:22:01 182  	      GROUP BY
16:22:01 183  		  ps.set_id
16:22:01 184  	      ORDER BY
16:22:01 185  		  ps.set_id
16:22:01 186  	  )
16:22:01 187  	  LOOP
16:22:01 188  	      out_refcursor := Retreive_Transfer_Set(x.set_id) ;
16:22:01 189  	      RETURN;
16:22:01 190  	  END LOOP;
16:22:01 191  	  v_set_id := Create_New_Transfer_Set(in_set_maximum) ;
16:22:01 192  	  out_refcursor := Retreive_Transfer_Set(v_set_id) ;
16:22:01 193  	  RETURN;
