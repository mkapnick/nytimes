16:22:00   5  	PROCEDURE POPULATE_REQUEST_INFO(
16:22:00   6  	  in_hours_prior    IN	NUMBER,
16:22:00   7  	  in_filename	    IN	CC_REQUEST_FILE.FILE_NAME%TYPE,
16:22:00   8  	  in_creator	    IN	CC_REQUEST_FILE.UPDATED_BY%TYPE
16:22:00   9  	) AS
16:22:00  10  	SPROC_NAME CONSTANT VARCHAR2(32) := 'POPULATE_REQUEST_INFO';
16:22:00  11  	var_start_date	    DATE := SYSDATE;
16:22:00  12  	var_end_date	    DATE := var_start_date + (in_hours_prior/24);
16:22:00  13  	var_request_file_id NUMBER := 0;
16:22:00  14  	var_license_count   NUMBER := 0;
16:22:00  15  	var_cc_update_count NUMBER := 0;
16:22:00  16  	BEGIN
16:22:00  17  	    SELECT CC_REQUEST_FILE_ID_SEQ.NEXTVAL INTO var_request_file_id  FROM DUAL;
16:22:00  18  	    INSERT INTO CC_REQUEST_FILE (ID,
16:22:00  19  					 FILE_NAME,
16:22:00  20  					 CC_REQUEST_FILE_STATUS,
16:22:00  21  					 CREATE_DATE,
16:22:00  22  					 CREATED_BY,
16:22:00  23  					 UPDATE_DATE,
16:22:00  24  					 UPDATED_BY)
16:22:00  25  					 VALUES (
16:22:00  26  					 var_request_file_id,
16:22:00  27  					 in_filename,
16:22:00  28  					 'NOT_CREATED',
16:22:00  29  					 var_start_date,
16:22:00  30  					 in_creator,
16:22:00  31  					 var_start_date,
16:22:00  32  					 in_creator);
16:22:00  33  
16:22:00  34  	   FOR record IN (SELECT
16:22:00  35  			    l.ID LICENSE_ID, cc.ID CREDIT_CARD_ID
16:22:00  36  			  FROM
16:22:00  37  			    LICENSE l INNER JOIN SUBSCRIPTION s ON L.SUBSCRIPTION_ID = s.ID
16:22:00  38  				      INNER JOIN CREDIT_CARD cc ON S.INSTRUMENT_ID   = cc.ID
16:22:00  39  			  WHERE
16:22:00  40  			    s.INSTRUMENT_TYPE_ID	 = 1
16:22:00  41  			    AND cc.CREDIT_CARD_STATUS_ID = 1
16:22:00  42  			    AND s.SUBSCRIPTION_STATUS_ID = 1
16:22:00  43  			    AND l.LICENSE_STATUS_ID	 = 2
16:22:00  44  			    AND cc.CREDIT_CARD_TYPE_ID IN (2,3)
16:22:00  45  			    AND l.END_DATE BETWEEN var_start_date AND var_end_date
16:22:00  46  			    AND l.ID NOT IN (SELECT LICENSE_ID FROM CC_UPDATE))
16:22:00  47  	   LOOP
16:22:00  48  	     var_license_count := 0;
16:22:00  49  	     SELECT COUNT(1) INTO  var_license_count FROM CC_UPDATE WHERE LICENSE_ID = record.LICENSE_ID;
16:22:00  50  
16:22:00  51  	     IF var_license_count = 0 THEN
16:22:00  52  		INSERT INTO CC_UPDATE (ID,
16:22:00  53  				       CREDIT_CARD_ID,
16:22:00  54  				       LICENSE_ID,
16:22:00  55  				       CC_UPDATE_STATUS,
16:22:00  56  				       CC_REQUEST_FILE_ID,
16:22:00  57  				       CREATE_DATE,
16:22:00  58  				       UPDATE_DATE,
16:22:00  59  				       CREATED_BY,
16:22:00  60  				       UPDATED_BY
16:22:00  61  				       ) VALUES (
16:22:00  62  				       CC_UPDATE_SEQ.NEXTVAL,
16:22:00  63  				       record.CREDIT_CARD_ID,
16:22:00  64  				       record.LICENSE_ID,
16:22:00  65  				       'NOT_ADDED_TO_FILE',
16:22:00  66  				       var_request_file_id,
16:22:00  67  				       var_start_date,
16:22:00  68  				       var_start_date,
16:22:00  69  				       in_creator,
16:22:00  70  				       in_creator
16:22:00  71  				       );
16:22:00  72  	     END IF;
16:22:00  73  	   END LOOP;
16:22:00  74  
16:22:00  75  	   SELECT COUNT(1) INTO var_cc_update_count
16:22:00  76  	   FROM CC_UPDATE
16:22:00  77  	   WHERE CC_REQUEST_FILE_ID = var_request_file_id;
16:22:00  78  	   IF var_cc_update_count <= 0 THEN
16:22:00  79  	     UPDATE CC_REQUEST_FILE
16:22:00  80  	     SET CC_REQUEST_FILE_STATUS = 'EMPTY'
16:22:00  81  	     WHERE ID = var_request_file_id;
16:22:00  82  	   END IF;
16:22:00  83  
16:22:00  84  	END POPULATE_REQUEST_INFO;
