16:22:02 621  	  ACCOUNT
16:22:02 622  	WHERE
16:22:02 623  	  ACCOUNT.ID = var_account_id;
16:22:02 624  
16:22:02 625  	-- Check that passed data is correct
16:22:02 626  	IF in_is_default != GLOBAL_CONSTANTS_V22.TRUE
16:22:02 627  	  AND in_is_default != GLOBAL_CONSTANTS_V22.FALSE
16:22:02 628  	  AND in_is_default IS NOT NULL THEN
16:22:02 629  	  RAISE BAD_IS_DEFAULT_VALUE;
16:22:02 630  	END IF;
16:22:02 631  
16:22:02 632  	-- Update credit card
16:22:02 633  	IF in_instrument_name IS NOT NULL THEN
16:22:02 634  	  PROCS_FIN_INSTRUMENTS_CRU_V22.UPDATE_CREDIT_CARD(
16:22:02 635  	    in_credit_card_id  => in_credit_card_id,
16:22:02 636  	    in_updated_by      => in_updated_by,
16:22:02 637  	    in_instrument_name => in_instrument_name
16:22:02 638  	  );
16:22:02 639  	END IF;
16:22:02 640  
16:22:02 641  	-- Set default financial instrument
16:22:02 642  	IF in_is_default = GLOBAL_CONSTANTS_V22.TRUE THEN
16:22:02 643  	  BEGIN
16:22:02 644  	    PROCS_FIN_INSTRUMENTS_V22.SET_DEF_FINANCIAL_INSTRUMENT(
16:22:02 645  	      in_group_id	    => var_group_id,
16:22:02 646  	      in_instrument_type_id => GLOBAL_ENUMS_V22.INSTRUMENT_CREDIT_CARD,
16:22:02 647  	      in_instrument_id	    => in_credit_card_id,
16:22:02 648  	      in_updated_by	    => in_updated_by
16:22:02 649  	    );
16:22:02 650  	    EXCEPTION
16:22:02 651  	      WHEN OTHERS THEN
16:22:02 652  		EXCEPTION_MESSAGE := SQLERRM;
16:22:02 653  		RAISE CAN_NOT_SET_DEF_FINANCIAL;
16:22:02 654  	  END;
16:22:02 655  	END IF;
16:22:02 656  
16:22:02 657  	-- Set default financial instrument
16:22:02 658  	IF in_is_default = GLOBAL_CONSTANTS_V22.FALSE THEN
16:22:02 659  	  BEGIN
16:22:02 660  	    GET_DEF_FINANCIAL_INSTRUMENT(
16:22:02 661  	      in_group_id	     => var_group_id,
16:22:02 662  	      out_instrument_type_id => current_def_instrument_type_id,
16:22:02 663  	      out_instrument_id      => current_def_instrument_id
16:22:02 664  	    );
16:22:02 665  	  EXCEPTION
16:22:02 666  	    WHEN OTHERS THEN
16:22:02 667  	      EXCEPTION_MESSAGE := SQLERRM;
16:22:02 668  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
16:22:02 669  	  END;
16:22:02 670  	  IF current_def_instrument_type_id = GLOBAL_ENUMS_V22.INSTRUMENT_CREDIT_CARD
16:22:02 671  	    AND current_def_instrument_id = in_credit_card_id THEN
16:22:02 672  	    BEGIN
16:22:02 673  	      DEL_DEF_FINANCIAL_INSTRUMENT(
16:22:02 674  		in_group_id => var_group_id
16:22:02 675  	      );
16:22:02 676  	      EXCEPTION
16:22:02 677  		WHEN OTHERS THEN
16:22:02 678  		  EXCEPTION_MESSAGE := SQLERRM;
16:22:02 679  		  RAISE CAN_NOT_DEL_DEF_FINANCIAL;
16:22:02 680  	    END;
16:22:02 681  	  END IF;
16:22:02 682  	END IF;
16:22:02 683  
16:22:02 684  EXCEPTION
16:22:02 685  WHEN NO_DATA_FOUND THEN
16:22:02 686  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:02 687  	  SPROC_NAME, 'No such credit card');
16:22:02 688  WHEN CAN_NOT_SET_DEF_FINANCIAL THEN
16:22:02 689  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:02 690  	  SPROC_NAME, 'Could not set default financial instrument for account', EXCEPTION_MESSAGE);
16:22:02 691  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
16:22:02 692  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:02 693  	  SPROC_NAME, 'Could not get default financial instrument for account', EXCEPTION_MESSAGE);
16:22:02 694  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
16:22:02 695  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:02 696  	  SPROC_NAME, 'Could not delete information about default financial instrument', EXCEPTION_MESSAGE);
16:22:02 697  WHEN OTHERS THEN
16:22:02 698  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:02 699  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:22:02 700  END UPDATE_CREDIT_CARD;
