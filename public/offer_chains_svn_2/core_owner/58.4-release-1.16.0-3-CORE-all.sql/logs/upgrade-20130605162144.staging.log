16:22:01 1640  /******************************************************************************/
16:22:01 1641  
16:22:01 1642  FUNCTION CALC_SUBSCRIPTION_END_DATE (
16:22:01 1643  /*
16:22:01 1644  APP_EXCEPTION_CODES_V22.NOT_FOUND
16:22:01 1645  APP_EXCEPTION_CODES_V22.INTERNAL_ERROR
16:22:01 1646  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
16:22:01 1647  Returns:
16:22:01 1648  NULL if it is impossible to calculate end date (for example,
16:22:01 1649  	 offer chain includes offer with infinity recurrences number)
16:22:01 1650  DATE else
16:22:01 1651  */
16:22:01 1652  	 in_subscription_id IN NUMBER
16:22:01 1653  ) RETURN DATE AS
16:22:01 1654  -- VARIABLES
16:22:01 1655  SPROC_NAME		     CONSTANT VARCHAR2(26) := 'CALC_SUBSCRIPTION_END_DATE';
16:22:01 1656  last_license_id		     NUMBER;
16:22:01 1657  last_license_end_date	     DATE;
16:22:01 1658  last_license_offer_id	     NUMBER;
16:22:01 1659  last_license_offer_index      NUMBER;
16:22:01 1660  last_license_offer_recurr_num NUMBER;
16:22:01 1661  var_last_license_id	     NUMBER;
16:22:01 1662  var_offer_chain_id	     NUMBER;
16:22:01 1663  var_current_offer_rec_number  NUMBER;
16:22:01 1664  var_next_offers_set	     SYS_REFCURSOR;
16:22:01 1665  var_next_offer_duration	     VARCHAR2(30);
16:22:01 1666  var_next_offer_recur	     NUMBER;
16:22:01 1667  var_infinity_offers_number    NUMBER;
16:22:01 1668  
16:22:01 1669  var_result_date DATE;
16:22:01 1670  
16:22:01 1671  var_ym_interval	INTERVAL YEAR TO MONTH;
16:22:01 1672  var_ds_interval	INTERVAL DAY(3) TO SECOND;
16:22:01 1673  var_offer_years	NUMBER;
16:22:01 1674  var_offer_months NUMBER;
16:22:01 1675  var_offer_days	NUMBER;
16:22:01 1676  
16:22:01 1677  -- EXCEPTIONS
16:22:01 1678  BAD_SUBSCRIPTION_ID	  EXCEPTION;
16:22:01 1679  CAN_NOT_FIND_LAST_LICENSE  EXCEPTION;
16:22:01 1680  CAN_NOT_CALC_OFFER_LENGTH  EXCEPTION;
16:22:01 1681  CAN_NOT_CALC_OFFER_LENGTH2 EXCEPTION;
16:22:01 1682  EXCEPTION_MESSAGE	  VARCHAR2(1024);
16:22:01 1683  BEGIN
16:22:01 1684  
16:22:01 1685  	 BEGIN
16:22:01 1686  	   SELECT
16:22:01 1687  	     SUBSCRIPTION.OFFER_CHAIN_ID into var_offer_chain_id
16:22:01 1688  	   FROM
16:22:01 1689  	     SUBSCRIPTION
16:22:01 1690  	   WHERE
16:22:01 1691  	     SUBSCRIPTION.ID = in_subscription_id;
16:22:01 1692  	   EXCEPTION
16:22:01 1693  	     WHEN NO_DATA_FOUND THEN
16:22:01 1694  	       RAISE BAD_SUBSCRIPTION_ID;
16:22:01 1695  	 END;
16:22:01 1696  
16:22:01 1697  	 BEGIN
16:22:01 1698  	   SELECT
16:22:01 1699  	     ID into var_last_license_id
16:22:01 1700  	   FROM
16:22:01 1701  	     (
16:22:01 1702  	       SELECT
16:22:01 1703  		 LICENSE.ID
16:22:01 1704  	       FROM
16:22:01 1705  		 LICENSE
16:22:01 1706  	       WHERE
16:22:01 1707  		 LICENSE.SUBSCRIPTION_ID = in_subscription_id
16:22:01 1708  	       ORDER BY
16:22:01 1709  		 LICENSE.END_DATE DESC
16:22:01 1710  	     )
16:22:01 1711  	   WHERE
16:22:01 1712  	     ROWNUM <= 1;
16:22:01 1713  	   EXCEPTION
16:22:01 1714  	     WHEN NO_DATA_FOUND THEN
16:22:01 1715  	       RAISE CAN_NOT_FIND_LAST_LICENSE;
16:22:01 1716  	 END;
16:22:01 1717  
16:22:01 1718  	 SELECT
16:22:01 1719  	   COUNT(*) into var_infinity_offers_number
16:22:01 1720  	 FROM
16:22:01 1721  	   OFFER_OFFER_CHAIN
16:22:01 1722  	 WHERE
16:22:01 1723  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
16:22:01 1724  	   AND OFFER_OFFER_CHAIN.NUM_RECURRENCES = GLOBAL_ENUMS_V22.OFFER_REC_INFINITY;
16:22:01 1725  
16:22:01 1726  	 IF var_infinity_offers_number > 0 THEN
16:22:01 1727  	   RETURN NULL;
16:22:01 1728  	 END IF;
16:22:01 1729  
16:22:01 1730  	 BEGIN
16:22:01 1731  	   SELECT
16:22:01 1732  	     LICENSE.ID,
16:22:01 1733  	     LICENSE.END_DATE,
16:22:01 1734  	     LICENSE.CURRENT_OFFER_INDEX,
16:22:01 1735  	     LICENSE.CURRENT_OFFER_RECURR_NUM,
16:22:01 1736  	     LICENSE.OFFER_ID
16:22:01 1737  	     into
16:22:01 1738  	     last_license_id,
16:22:01 1739  	     last_license_end_date,
16:22:01 1740  	     last_license_offer_index,
16:22:01 1741  	     last_license_offer_recurr_num,
16:22:01 1742  	     last_license_offer_id
16:22:01 1743  	   FROM
16:22:01 1744  	     LICENSE
16:22:01 1745  	   WHERE
16:22:01 1746  	     LICENSE.ID = var_last_license_id;
16:22:01 1747  	   EXCEPTION
16:22:01 1748  	     WHEN NO_DATA_FOUND THEN
16:22:01 1749  	       RAISE CAN_NOT_FIND_LAST_LICENSE;
16:22:01 1750  	 END;
16:22:01 1751  
16:22:01 1752  	 var_result_date := last_license_end_date;
16:22:01 1753  
16:22:01 1754  	 -- Find current recurrence number
16:22:01 1755  	 SELECT
16:22:01 1756  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES into var_current_offer_rec_number
16:22:01 1757  	 FROM
16:22:01 1758  	   OFFER_OFFER_CHAIN
16:22:01 1759  	 WHERE
16:22:01 1760  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
16:22:01 1761  	   AND OFFER_OFFER_CHAIN.OFFER_ID = last_license_offer_id
16:22:01 1762  	   AND OFFER_OFFER_CHAIN.INDEX_VALUE = last_license_offer_index;
16:22:01 1763  
16:22:01 1764  	 IF var_current_offer_rec_number > last_license_offer_recurr_num THEN
16:22:01 1765  	   BEGIN
16:22:01 1766  	     PROCS_OFFER_CHAIN_V22.GET_OFFER_LENGTH(
16:22:01 1767  	       last_license_offer_id,
16:22:01 1768  	       var_offer_years,
16:22:01 1769  	       var_offer_months,
16:22:01 1770  	       var_offer_days
16:22:01 1771  	     );
16:22:01 1772  
16:22:01 1773  	     var_ym_interval := var_offer_years||'-'||var_offer_months;
16:22:01 1774  	     var_ds_interval := var_offer_days||' 0:0:0';
16:22:01 1775  
16:22:01 1776  	     var_result_date := var_result_date
16:22:01 1777  	       + ( var_ym_interval * ( var_current_offer_rec_number - last_license_offer_recurr_num ) )
16:22:01 1778  	       + ( var_ds_interval * ( var_current_offer_rec_number - last_license_offer_recurr_num ) );
16:22:01 1779  	     EXCEPTION
16:22:01 1780  	       WHEN OTHERS THEN
16:22:01 1781  		 EXCEPTION_MESSAGE := SQLERRM;
16:22:01 1782  		 RAISE CAN_NOT_CALC_OFFER_LENGTH;
16:22:01 1783  	   END;
16:22:01 1784  	 END IF;
16:22:01 1785  
16:22:01 1786  	 OPEN var_next_offers_set FOR
16:22:01 1787  	 SELECT
16:22:01 1788  	   OFFER.ENTITLEMENT_DURATION,
16:22:01 1789  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES
16:22:01 1790  	 FROM
16:22:01 1791  	   OFFER_OFFER_CHAIN
16:22:01 1792  	   INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
16:22:01 1793  	 WHERE
16:22:01 1794  	   OFFER_OFFER_CHAIN.INDEX_VALUE > last_license_offer_index
16:22:01 1795  	   AND OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id;
16:22:01 1796  
16:22:01 1797  	 LOOP
16:22:01 1798  	   FETCH var_next_offers_set into var_next_offer_duration, var_next_offer_recur;
16:22:01 1799  	   EXIT WHEN var_next_offers_set%NOTFOUND;
16:22:01 1800  	   BEGIN
16:22:01 1801  	     PROCS_COMMON_V22.ISO8601DURATION_TO_INTERVALS(
16:22:01 1802  	       var_next_offer_duration,
16:22:01 1803  	       var_offer_years,
16:22:01 1804  	       var_offer_months,
16:22:01 1805  	       var_offer_days
16:22:01 1806  	     );
16:22:01 1807  
16:22:01 1808  	     var_ym_interval := var_offer_years||'-'||var_offer_months;
16:22:01 1809  	     var_ds_interval := var_offer_days||' 0:0:0';
16:22:01 1810  
16:22:01 1811  	     var_result_date := var_result_date
16:22:01 1812  	       + ( var_ym_interval * var_next_offer_recur )
16:22:01 1813  	       + ( var_ds_interval * var_next_offer_recur );
16:22:01 1814  	     EXCEPTION
16:22:01 1815  	       WHEN OTHERS THEN
16:22:01 1816  		 EXCEPTION_MESSAGE := SQLERRM;
16:22:01 1817  		 RAISE CAN_NOT_CALC_OFFER_LENGTH2;
16:22:01 1818  	   END;
16:22:01 1819  	 END LOOP;
16:22:01 1820  
16:22:01 1821  	 RETURN var_result_date;
16:22:01 1822  
16:22:01 1823  EXCEPTION
16:22:01 1824  WHEN BAD_SUBSCRIPTION_ID THEN
16:22:01 1825  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 1826  	   SPROC_NAME, 'No such subscription');
16:22:01 1827  WHEN CAN_NOT_FIND_LAST_LICENSE THEN
16:22:01 1828  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 1829  	   SPROC_NAME, 'Could not find last license for given subscription');
16:22:01 1830  WHEN CAN_NOT_CALC_OFFER_LENGTH THEN
16:22:01 1831  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:01 1832  	   SPROC_NAME, 'Could not calculate offer length', EXCEPTION_MESSAGE);
16:22:01 1833  WHEN CAN_NOT_CALC_OFFER_LENGTH2 THEN
16:22:01 1834  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:01 1835  	   SPROC_NAME, 'Could not calculate last offer length', EXCEPTION_MESSAGE);
16:22:01 1836  WHEN OTHERS THEN
16:22:01 1837  	 PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:01 1838  	   SPROC_NAME, 'Unkown error', SQLERRM);
16:22:01 1839  END CALC_SUBSCRIPTION_END_DATE;
