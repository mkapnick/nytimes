16:22:01 499  FUNCTION CALCULATE_OFFER_AMOUNT (
16:22:01 500  	in_offer_id IN NUMBER
16:22:01 501  ) RETURN NUMBER AS
16:22:01 502  -- VARIABLES
16:22:01 503  SPROC_NAME    CONSTANT VARCHAR2(22) := 'CALCULATE_OFFER_AMOUNT';
16:22:01 504  temp_offer_id NUMBER;
16:22:01 505  
16:22:01 506  var_product_offering_set	    SYS_REFCURSOR;
16:22:01 507  var_product_offering_id	    NUMBER;
16:22:01 508  var_product_offering_price    NUMBER(10,6);
16:22:01 509  var_product_offering_t_amount NUMBER(10,6);
16:22:01 510  var_product_offering_quantity NUMBER;
16:22:01 511  
16:22:01 512  var_total_amount NUMBER(10,6);
16:22:01 513  var_final_amount NUMBER(10,2);
16:22:01 514  
16:22:01 515  var_percent_discount NUMBER(10,2);
16:22:01 516  var_fixed_discount NUMBER(10,6);
16:22:01 517  
16:22:01 518  -- EXCEPTIONS
16:22:01 519  BAD_OFFER_ID EXCEPTION;
16:22:01 520  BEGIN
16:22:01 521  
16:22:01 522  	BEGIN
16:22:01 523  	  SELECT
16:22:01 524  	    OFFER.ID into temp_offer_id
16:22:01 525  	  FROM
16:22:01 526  	    OFFER
16:22:01 527  	  WHERE
16:22:01 528  	    OFFER.ID = in_offer_id;
16:22:01 529  	  EXCEPTION
16:22:01 530  	    WHEN NO_DATA_FOUND THEN
16:22:01 531  	      RAISE BAD_OFFER_ID;
16:22:01 532  	END;
16:22:01 533  
16:22:01 534  	OPEN var_product_offering_set FOR
16:22:01 535  	SELECT
16:22:01 536  	  OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID,
16:22:01 537  	  PRODUCT_OFFERING.UNIT_PRICE,
16:22:01 538  	  PRODUCT_OFFERING.QUANTITY
16:22:01 539  	FROM
16:22:01 540  	  OFFER_PRODUCT_OFFERING
16:22:01 541  	  INNER JOIN PRODUCT_OFFERING ON OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = PRODUCT_OFFERING.ID
16:22:01 542  	WHERE
16:22:01 543  	  OFFER_PRODUCT_OFFERING.OFFER_ID = in_offer_id;
16:22:01 544  
16:22:01 545  	var_total_amount := 0;
16:22:01 546  
16:22:01 547  	LOOP
16:22:01 548  	  FETCH var_product_offering_set into
16:22:01 549  	    var_product_offering_id,
16:22:01 550  	    var_product_offering_price,
16:22:01 551  	    var_product_offering_quantity;
16:22:01 552  	  EXIT WHEN var_product_offering_set%NOTFOUND;
16:22:01 553  
16:22:01 554  	  SELECT
16:22:01 555  	    SUM(DISCOUNT.FIXED_AMOUNT) into var_fixed_discount
16:22:01 556  	  FROM
16:22:01 557  	    DISCOUNT_PRODUCT_OFFERING
16:22:01 558  	    INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
16:22:01 559  	  WHERE
16:22:01 560  	    DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
16:22:01 561  	    AND DISCOUNT.FIXED_AMOUNT IS NOT NULL;
16:22:01 562  
16:22:01 563  	  SELECT
16:22:01 564  	    SUM(DISCOUNT.PERCENT_AMOUNT) into var_percent_discount
16:22:01 565  	  FROM
16:22:01 566  	    DISCOUNT_PRODUCT_OFFERING
16:22:01 567  	    INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
16:22:01 568  	  WHERE
16:22:01 569  	    DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
16:22:01 570  	    AND DISCOUNT.PERCENT_AMOUNT IS NOT NULL;
16:22:01 571  
16:22:01 572  	  var_product_offering_t_amount := var_product_offering_price * var_product_offering_quantity;
16:22:01 573  
16:22:01 574  	  IF var_percent_discount IS NOT NULL THEN
16:22:01 575  	    var_product_offering_t_amount := var_product_offering_t_amount - ( var_product_offering_t_amount * var_percent_discount / 100 );
16:22:01 576  	  END IF;
16:22:01 577  
16:22:01 578  	  IF var_fixed_discount IS NOT NULL THEN
16:22:01 579  	    var_product_offering_t_amount := var_product_offering_t_amount - var_fixed_discount;
16:22:01 580  	  END IF;
16:22:01 581  
16:22:01 582  	  var_total_amount := var_total_amount + var_product_offering_t_amount;
16:22:01 583  	END LOOP;
16:22:01 584  	var_final_amount := var_total_amount;
16:22:01 585  	RETURN var_final_amount;
16:22:01 586  
16:22:01 587  EXCEPTION
16:22:01 588  WHEN BAD_OFFER_ID THEN
16:22:01 589  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 590  	  SPROC_NAME, 'No such offer');
16:22:01 591  WHEN OTHERS THEN
16:22:01 592  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:01 593  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:22:01 594  END CALCULATE_OFFER_AMOUNT;
