16:22:01  53  	  SUBSCRIPTION_ID,
16:22:01  54  	  END_DATE
16:22:01  55  	INTO var_lic_id, var_subs_id, var_lic_end_date
16:22:01  56  	FROM
16:22:01  57  	  LICENSE
16:22:01  58  	WHERE
16:22:01  59  	  INVOICE_ID = in_invoice_id
16:22:01  60  	  AND ROWNUM <= 1;
16:22:01  61  
16:22:01  62  	PROCS_LICENSE_CRU_V22.UPDATE_LICENSE(
16:22:01  63  	    in_license_id	    => var_lic_id,
16:22:01  64  	    in_updated_by	    => in_updater,
16:22:01  65  	    in_grace_end_date	    => SYSDATE,
16:22:01  66  	    in_entitlement_end_date => var_lic_end_date,
16:22:01  67  	    in_license_status_id    => GLOBAL_STATUSES_V22.LICENSE_ACTIVE
16:22:01  68  	);
16:22:01  69  
16:22:01  70  	PROCS_SUBSCRIPTION_CRU_V22.UPDATE_SUBSCRIPTION(
16:22:01  71  	    in_subscription_id	      => var_subs_id,
16:22:01  72  	    in_updated_by	      => in_updater,
16:22:01  73  	    in_subscription_status_id => GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE
16:22:01  74  	);
16:22:01  75  END STOP_GRACE_BY_INVOICE_ID;
16:22:01  76  
16:22:01  77  
16:22:01  78  PROCEDURE START_SUBSCRIPTION_CREATION (
16:22:01  79  	in_group_id	      IN NUMBER,
16:22:01  80  	in_created_by	      IN VARCHAR2,
16:22:01  81  	in_offer_chain_id     IN NUMBER,
16:22:01  82  	in_instrument_type_id IN NUMBER,
16:22:01  83  	in_instrument_id      IN NUMBER,
16:22:01  84  	in_agent_id	      IN NUMBER,
16:22:01  85  	in_note 	      IN VARCHAR2,
16:22:01  86  	out_subscription_id   OUT NUMBER,
16:22:01  87  	out_invoice_id	      OUT NUMBER,
16:22:01  88  	out_new_license_id    OUT NUMBER,
16:22:01  89  	in_check_dupe_products	 IN NUMBER DEFAULT GLOBAL_CONSTANTS_V22.TRUE
16:22:01  90  ) AS
16:22:01  91  SPROC_NAME CONSTANT VARCHAR2(27) := 'START_SUBSCRIPTION_CREATION';
16:22:01  92  -- VARIABLES
16:22:01  93  var_account_id		 NUMBER;
16:22:01  94  var_account_status	 NUMBER;
16:22:01  95  var_offer_chain_status_id  NUMBER;
16:22:01  96  var_is_gift_certificate	 NUMBER;
16:22:01  97  var_is_for_redemption	 NUMBER;
16:22:01  98  var_same_offer_chains_num  NUMBER;
16:22:01  99  var_max_concurrent_subscrs NUMBER;
16:22:01 100  var_first_offer_id	 NUMBER;
16:22:01 101  var_new_invoice_id	 NUMBER;
16:22:01 102  var_new_subscription_id	 NUMBER;
16:22:01 103  var_date			 DATE := SYSDATE;
16:22:01 104  var_account_tax_exempt_id  VARCHAR2(255);
16:22:01 105  var_concur_subscription_id NUMBER;
16:22:01 106  -- EXCEPTIONS
16:22:01 107  BAD_GROUP_ID		    EXCEPTION;
16:22:01 108  CAN_NOT_CREATE_FOR_DISABLE    EXCEPTION;
16:22:01 109  BAD_OFFER_CHAIN		    EXCEPTION;
16:22:01 110  BAD_OFFER_CHAIN_STATUS	    EXCEPTION;
16:22:01 111  CAN_NOT_SUBSCRIBE_TO_GC	    EXCEPTION;
16:22:01 112  CAN_NOT_SUBSCRIBE_TO_RGC	    EXCEPTION;
16:22:01 113  LIMIT_REACHED		    EXCEPTION;
16:22:01 114  PRODUCT_ALREADY_PURCHASED     EXCEPTION;
16:22:01 115  CAN_NOT_GET_FIRST_OFFER_CHAIN EXCEPTION;
16:22:01 116  CAN_NOT_CREATE_INVOICE	    EXCEPTION;
16:22:01 117  CAN_NOT_CREATE_LINE_ITEMS     EXCEPTION;
16:22:01 118  CAN_NOT_CREATE_LICENSE	    EXCEPTION;
16:22:01 119  CAN_NOT_CREATE_NOTE	    EXCEPTION;
16:22:01 120  
16:22:01 121  EXCEPTION_MESSAGE VARCHAR2(1024);
16:22:01 122  BEGIN
16:22:01 123  
16:22:01 124  	-- Get account id and status
16:22:01 125  	BEGIN
16:22:01 126  	  SELECT
16:22:01 127  	    ACCOUNT.ID,
16:22:01 128  	    ACCOUNT.ACCOUNT_STATUS_ID,
16:22:01 129  	    ACCOUNT.TAX_EXEMPT_ID
16:22:01 130  	    into
16:22:01 131  	    var_account_id,
16:22:01 132  	    var_account_status,
16:22:01 133  	    var_account_tax_exempt_id
16:22:01 134  	  FROM
16:22:01 135  	    ACCOUNT
16:22:01 136  	  WHERE
16:22:01 137  	    ACCOUNT.GROUP_ID = in_group_id;
16:22:01 138  	  EXCEPTION
16:22:01 139  	  WHEN NO_DATA_FOUND THEN
16:22:01 140  	    RAISE BAD_GROUP_ID;
16:22:01 141  	END;
16:22:01 142  
16:22:01 143  	-- Could not create subscription for disabled account
16:22:01 144  	IF var_account_status = GLOBAL_STATUSES_V22.ACCOUNT_DISABLED THEN
16:22:01 145  	  RAISE CAN_NOT_CREATE_FOR_DISABLE;
16:22:01 146  	END IF;
16:22:01 147  
16:22:01 148  	-- Get offer chain status
16:22:01 149  	BEGIN
16:22:01 150  	  SELECT
16:22:01 151  	    OFFER_CHAIN.OFFER_CHAIN_STATUS_ID,
16:22:01 152  	    OFFER_CHAIN.IS_GIFT_CERTIFICATE
16:22:01 153  	    into
16:22:01 154  	    var_offer_chain_status_id,
16:22:01 155  	    var_is_gift_certificate
16:22:01 156  	  FROM
16:22:01 157  	    OFFER_CHAIN
16:22:01 158  	  WHERE
16:22:01 159  	    OFFER_CHAIN.ID = in_offer_chain_id;
16:22:01 160  
16:22:01 161  	  EXCEPTION
16:22:01 162  	    WHEN NO_DATA_FOUND THEN
16:22:01 163  	      RAISE BAD_OFFER_CHAIN;
16:22:01 164  	END;
16:22:01 165  
16:22:01 166  	-- Could not subscribe to inactive/disabled offer chain
16:22:01 167  	IF var_offer_chain_status_id != GLOBAL_STATUSES_V22.OFFER_CHAIN_ACTIVE THEN
16:22:01 168  	  RAISE BAD_OFFER_CHAIN_STATUS;
16:22:01 169  	END IF;
16:22:01 170  
16:22:01 171  	-- Can not subscribe to Offer Chain for a Gift Certfiicate
16:22:01 172  	IF var_is_gift_certificate = GLOBAL_CONSTANTS_V22.TRUE THEN
16:22:01 173  	  RAISE CAN_NOT_SUBSCRIBE_TO_GC;
16:22:01 174  	END IF;
16:22:01 175  
16:22:01 176  	-- check if the OC is for Redemption:
16:22:01 177  	SELECT
16:22:01 178  	  COUNT(*) into var_is_for_redemption
16:22:01 179  	FROM
16:22:01 180  	  OFFER_CHAIN_ELIGIBILITY
16:22:01 181  	WHERE
16:22:01 182  	  OFFER_CHAIN_ELIGIBILITY.OFFER_CHAIN_ID = in_offer_chain_id
16:22:01 183  	  AND OFFER_CHAIN_ELIGIBILITY.NAME = GLOBAL_CONSTANTS_V22.GIFT_CERTIFICATE_REQUIRED
16:22:01 184  	  AND OFFER_CHAIN_ELIGIBILITY.VALUE = GLOBAL_CONSTANTS_V22.ELIGIBILITY_FLAG_SET;
16:22:01 185  
16:22:01 186  	IF var_is_for_redemption > 0 THEN
16:22:01 187  	  RAISE CAN_NOT_SUBSCRIBE_TO_RGC;
16:22:01 188  	END IF;
16:22:01 189  
16:22:01 190  	SELECT
16:22:01 191  	  COUNT(*) into var_same_offer_chains_num
16:22:01 192  	FROM
16:22:01 193  	  SUBSCRIPTION
16:22:01 194  	WHERE
16:22:01 195  	  SUBSCRIPTION.ACCOUNT_ID = var_account_id
16:22:01 196  	  AND SUBSCRIPTION.OFFER_CHAIN_ID = in_offer_chain_id
16:22:01 197  	  AND (SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE
16:22:01 198  	       OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_IN_GRACE_PERIOD);
16:22:01 199  
16:22:01 200  	-- ELIGIBILITY LOGIC CHANGED TO:
16:22:01 201  	-- FOR EACH offer chain eligibility rule in OC:
16:22:01 202  	--   IF offer chain eligibility rule fails:
16:22:01 203  	--     deny purchase;
16:22:01 204  	--   END IF
16:22:01 205  	-- END FOR
16:22:01 206  	-- FOR EACH product eligibility rule in OC:
16:22:01 207  	--   IF product eligibilty rule fails:
16:22:01 208  	--     deny purchase;
16:22:01 209  	--   END IF
16:22:01 210  	-- END FOR
16:22:01 211  	-- allow purchase;
16:22:01 212  
16:22:01 213  	-- if user have any active existing subscriptions to the offer chain
16:22:01 214  	-- and if MAX_CONCURRENT_SUBS <= [user's subscription count for the offer chain]
16:22:01 215  	-- then deny purchase
16:22:01 216  	var_max_concurrent_subscrs := PROCS_OFFER_CHAIN_V22.GET_OFFER_CHAIN_MAX_CONC_SUBSC(in_offer_chain_id);
16:22:01 217  	IF var_max_concurrent_subscrs != GLOBAL_CONSTANTS_V22.INFINITY
16:22:01 218  	  AND var_max_concurrent_subscrs <= var_same_offer_chains_num THEN
16:22:01 219  	  -- Find first concurrent subscription id:
16:22:01 220  	  SELECT
16:22:01 221  	    ID into var_concur_subscription_id
16:22:01 222  	  FROM (
16:22:01 223  	    SELECT
16:22:01 224  	      ID
16:22:01 225  	    FROM
16:22:01 226  	      SUBSCRIPTION
16:22:01 227  	    WHERE
16:22:01 228  	      SUBSCRIPTION.ACCOUNT_ID = var_account_id
16:22:01 229  	      AND SUBSCRIPTION.OFFER_CHAIN_ID = in_offer_chain_id
16:22:01 230  	      AND (SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE
16:22:01 231  		   OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_IN_GRACE_PERIOD)
16:22:01 232  	    ORDER BY
16:22:01 233  	      ID
16:22:01 234  	  )
16:22:01 235  	  WHERE
16:22:01 236  	    ROWNUM <= 1;
16:22:01 237  	  RAISE LIMIT_REACHED;
16:22:01 238  	END IF;
16:22:01 239  
16:22:01 240  	-- if user does not have any active existing subscriptions to the offer chain
16:22:01 241  	-- and if product from the offer chain is already owned from another offer chain
16:22:01 242  	-- then deny purchase
16:22:01 243  	IF (in_check_dupe_products != GLOBAL_CONSTANTS_V22.FALSE) THEN
16:22:01 244  	  FOR f_account_offer_chains IN (
16:22:01 245  	    SELECT DISTINCT
16:22:01 246  	      OFFER_CHAIN_ID
16:22:01 247  	    FROM
16:22:01 248  	      SUBSCRIPTION
16:22:01 249  	    WHERE
16:22:01 250  	      ACCOUNT_ID = var_account_id
16:22:01 251  	      AND (
16:22:01 252  		SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE
16:22:01 253  		OR SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_SUSPENDED
16:22:01 254  		OR SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_IN_GRACE_PERIOD)
16:22:01 255  	  )
16:22:01 256  	  LOOP
16:22:01 257  	    IF PROCS_OFFER_CHAIN_V22.CHECK_FOR_SAME_PRODUCTS(
16:22:01 258  	      in_offer_chain_1	       => in_offer_chain_id,
16:22:01 259  	      in_offer_chain_2	       => f_account_offer_chains.OFFER_CHAIN_ID,
16:22:01 260  	      in_use_eligibility_rules => GLOBAL_CONSTANTS_V22.TRUE
16:22:01 261  	    ) = GLOBAL_CONSTANTS_V22.TRUE THEN
16:22:01 262  
16:22:01 263  	      -- Find first concurrent subscription id:
16:22:01 264  	      SELECT
16:22:01 265  		ID into var_concur_subscription_id
16:22:01 266  	      FROM (
16:22:01 267  		SELECT
16:22:01 268  		  ID
16:22:01 269  		FROM
16:22:01 270  		  SUBSCRIPTION
16:22:01 271  		WHERE
16:22:01 272  		  SUBSCRIPTION.ACCOUNT_ID = var_account_id
16:22:01 273  		  AND SUBSCRIPTION.OFFER_CHAIN_ID = f_account_offer_chains.OFFER_CHAIN_ID
16:22:01 274  		  AND (
16:22:01 275  		    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE
16:22:01 276  		    OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_SUSPENDED
16:22:01 277  		    OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V22.SUBSCRIPTION_IN_GRACE_PERIOD)
16:22:01 278  		ORDER BY
16:22:01 279  		  ID
16:22:01 280  	      )
16:22:01 281  	      WHERE
16:22:01 282  		ROWNUM <= 1;
16:22:01 283  
16:22:01 284  	      RAISE PRODUCT_ALREADY_PURCHASED;
16:22:01 285  	    END IF;
16:22:01 286  	  END LOOP;
16:22:01 287  	END IF;
16:22:01 288  
16:22:01 289  	BEGIN
16:22:01 290  	  PROCS_OFFER_CHAIN_V22.GET_FIRST_OFFER(in_offer_chain_id, var_first_offer_id);
16:22:01 291  	  EXCEPTION
16:22:01 292  	    WHEN OTHERS THEN
16:22:01 293  	      EXCEPTION_MESSAGE := SQLERRM;
16:22:01 294  	      RAISE CAN_NOT_GET_FIRST_OFFER_CHAIN;
16:22:01 295  	END;
16:22:01 296  
16:22:01 297  	BEGIN
16:22:01 298  	  PROCS_INVOICE_V22.CREATE_INVOICE(
16:22:01 299  	    in_invoice_status => GLOBAL_STATUSES_V22.INVOICE_OPEN,
16:22:01 300  	    in_created_by     => in_created_by,
16:22:01 301  	    in_tax_exempt_id  => var_account_tax_exempt_id,
16:22:01 302  	    out_invoice_id    => var_new_invoice_id
16:22:01 303  	  );
16:22:01 304  	  EXCEPTION
16:22:01 305  	    WHEN OTHERS THEN
16:22:01 306  	      EXCEPTION_MESSAGE := SQLERRM;
16:22:01 307  	      RAISE CAN_NOT_CREATE_INVOICE;
16:22:01 308  	END;
16:22:01 309  
16:22:01 310  	BEGIN
16:22:01 311  	  PROCS_LINE_ITEMS_V22.ADD_LINE_ITEMS(
16:22:01 312  	    in_invoice_id => var_new_invoice_id,
16:22:01 313  	    in_offer_id   => var_first_offer_id,
16:22:01 314  	    in_created_by => in_created_by
16:22:01 315  	  );
16:22:01 316  	  EXCEPTION
16:22:01 317  	    WHEN OTHERS THEN
16:22:01 318  	      EXCEPTION_MESSAGE := SQLERRM;
16:22:01 319  	      RAISE CAN_NOT_CREATE_LINE_ITEMS;
16:22:01 320  	END;
16:22:01 321  
16:22:01 322  	PROCS_SUBSCRIPTION_CRU_V22.CREATE_SUBSCRIPTION(
16:22:01 323  	  out_subscription_id	    => var_new_subscription_id,
16:22:01 324  	  in_account_id 	    => var_account_id,
16:22:01 325  	  in_purchase_date	    => var_date,
16:22:01 326  	  in_offer_chain_id	    => in_offer_chain_id,
16:22:01 327  	  in_created_by 	    => in_created_by,
16:22:01 328  	  in_instrument_type_id     => in_instrument_type_id,
16:22:01 329  	  in_instrument_id	    => in_instrument_id,
16:22:01 330  	  in_subscription_status_id => GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE
16:22:01 331  	);
16:22:01 332  
16:22:01 333  	BEGIN
16:22:01 334  	  PROCS_SUBSCRIPTION_V22.ANNOTATE_SUBSCRIPTION(
16:22:01 335  	    in_subscription_id => var_new_subscription_id,
16:22:01 336  	    in_agent_id        => in_agent_id,
16:22:01 337  	    in_note	       => in_note,
16:22:01 338  	    in_created_by      => in_created_by
16:22:01 339  	  );
16:22:01 340  	  EXCEPTION
16:22:01 341  	   WHEN OTHERS THEN
16:22:01 342  	     EXCEPTION_MESSAGE := SQLERRM;
16:22:01 343  	     RAISE CAN_NOT_CREATE_NOTE;
16:22:01 344  	END;
16:22:01 345  
16:22:01 346  	BEGIN
16:22:01 347  	  PROCS_LICENSE_V22.CREATE_LICENSE(
16:22:01 348  	    in_status_id		=> GLOBAL_STATUSES_V22.LICENSE_ACTIVE,
16:22:01 349  	    in_needs_entitlements	=> GLOBAL_CONSTANTS_V22.TRUE,
16:22:01 350  	    in_start_date		=> var_date,
16:22:01 351  	    in_end_date 		=> NULL, -- Will be calculated automatically
16:22:01 352  	    in_offer_id 		=> var_first_offer_id,
16:22:01 353  	    in_subscription_id		=> var_new_subscription_id,
16:22:01 354  	    in_invoice_id		=> var_new_invoice_id,
16:22:01 355  	    in_created_by		=> in_created_by,
16:22:01 356  	    in_is_extension		=> GLOBAL_CONSTANTS_V22.FALSE,
16:22:01 357  	    in_current_offer_index	=> PROCS_OFFER_CHAIN_V22.GET_FIRST_OFFER_INDEX(in_offer_chain_id),
16:22:01 358  	    in_current_offer_recurr_num => 1,
16:22:01 359  	    out_license_id		=> out_new_license_id
16:22:01 360  	  );
16:22:01 361  	  EXCEPTION
16:22:01 362  	    WHEN OTHERS THEN
16:22:01 363  	      EXCEPTION_MESSAGE := SQLERRM;
16:22:01 364  	      RAISE CAN_NOT_CREATE_LICENSE;
16:22:01 365  	END;
16:22:01 366  
16:22:01 367  	out_subscription_id := var_new_subscription_id;
16:22:01 368  	out_invoice_id := var_new_invoice_id;
16:22:01 369  
16:22:01 370  EXCEPTION
16:22:01 371  WHEN BAD_OFFER_CHAIN_STATUS THEN
16:22:01 372  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.STATE_ERROR,
16:22:01 373  	  SPROC_NAME, 'Offer chain is not active');
16:22:01 374  WHEN LIMIT_REACHED THEN
16:22:01 375  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.CONFLICT_ERROR,
16:22:01 376  	  SPROC_NAME, 'Limit reached for given offer chain. Concurrent subscription id: ' || var_concur_subscription_id);
16:22:01 377  WHEN CAN_NOT_CREATE_FOR_DISABLE THEN
16:22:01 378  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.STATE_ERROR,
16:22:01 379  	  SPROC_NAME, 'Can not create subsscription for disabled account');
16:22:01 380  WHEN CAN_NOT_SUBSCRIBE_TO_GC THEN
16:22:01 381  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.STATE_ERROR,
16:22:01 382  	  SPROC_NAME, 'Can not subscribe to Offer Chain for Gift Certificate');
16:22:01 383  WHEN CAN_NOT_SUBSCRIBE_TO_RGC THEN
16:22:01 384  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.STATE_ERROR,
16:22:01 385  	  SPROC_NAME, 'Can not subscribe to Offer Chain that is for redemption');
16:22:01 386  WHEN BAD_OFFER_CHAIN THEN
16:22:01 387  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 388  	  SPROC_NAME, 'No such offer chain');
16:22:01 389  WHEN PRODUCT_ALREADY_PURCHASED THEN
16:22:01 390  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.CONFLICT_ERROR,
16:22:01 391  	  SPROC_NAME, 'User already subscribed to some product in given offer chain. Concurrent subscription id: ' || var_concur_subscription_id);
16:22:01 392  WHEN CAN_NOT_GET_FIRST_OFFER_CHAIN THEN
16:22:01 393  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:01 394  	  SPROC_NAME, 'Could not find first offer in offer chain', EXCEPTION_MESSAGE);
16:22:01 395  WHEN CAN_NOT_CREATE_INVOICE THEN
16:22:01 396  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:01 397  	  SPROC_NAME, 'Could not create invoice', EXCEPTION_MESSAGE);
16:22:01 398  WHEN CAN_NOT_CREATE_LINE_ITEMS THEN
16:22:01 399  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:01 400  	  SPROC_NAME, 'Could not create line items', EXCEPTION_MESSAGE);
16:22:01 401  WHEN CAN_NOT_CREATE_LICENSE THEN
16:22:01 402  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:01 403  	  SPROC_NAME, 'Could not create new license', EXCEPTION_MESSAGE);
16:22:01 404  WHEN BAD_GROUP_ID THEN
16:22:01 405  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 406  	  SPROC_NAME, 'Bad group id');
16:22:01 407  WHEN CAN_NOT_CREATE_NOTE THEN
16:22:01 408  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.INTERNAL_ERROR,
16:22:01 409  	  SPROC_NAME, 'Could not annotate subscription', EXCEPTION_MESSAGE);
16:22:01 410  WHEN OTHERS THEN
16:22:01 411  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:01 412  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:22:01 413  END START_SUBSCRIPTION_CREATION;
