16:22:02 1935  -- VARIABLES
16:22:02 1936  temp_invoice_id number;
16:22:02 1937  cnt_matched_instr number := 0;
16:22:02 1938  -- EXCEPTIONS
16:22:02 1939  BAD_INVOICE_ID EXCEPTION;
16:22:02 1940  BEGIN
16:22:02 1941  
16:22:02 1942  	 BEGIN
16:22:02 1943  	   SELECT
16:22:02 1944  	     i.id into temp_invoice_id
16:22:02 1945  	   from
16:22:02 1946  	     invoice i
16:22:02 1947  	   where
16:22:02 1948  	     i.id = in_invoice_id;
16:22:02 1949  	   EXCEPTION
16:22:02 1950  	     WHEN NO_DATA_FOUND THEN
16:22:02 1951  	       RAISE BAD_INVOICE_ID;
16:22:02 1952  	 END;
16:22:02 1953  
16:22:02 1954  	 select
16:22:02 1955  	   count(1) into cnt_matched_instr
16:22:02 1956  	 from
16:22:02 1957  	   charge ch
16:22:02 1958  	 inner join
16:22:02 1959  	   subscription s
16:22:02 1960  	 on
16:22:02 1961  	   s.instrument_id = ch.instrument_id
16:22:02 1962  	 where
16:22:02 1963  	   ch.invoice_id = in_invoice_id;
16:22:02 1964  
16:22:02 1965  	 if cnt_matched_instr = 0 then
16:22:02 1966  	   out_external_transaction_id := null;
