16:22:01 517  
16:22:01 518  PROCEDURE SUSPEND_SUBSCRIPTION(
16:22:01 519  /*
16:22:01 520  Throws exceptions:
16:22:01 521  APP_EXCEPTION_CODES_V22.NOT_FOUND
16:22:01 522  APP_EXCEPTION_CODES_V22.INTERNAL_ERROR
16:22:01 523  APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR
16:22:01 524  */
16:22:01 525  	  in_subs_id	IN NUMBER,
16:22:01 526  	  in_updated_by IN VARCHAR2
16:22:01 527  ) AS
16:22:01 528  SPROC_NAME		  CONSTANT VARCHAR2(20) := 'SUSPEND_SUBSCRIPTION';
16:22:01 529  var_subscription_status_id  NUMBER;
16:22:01 530  var_license_id		  NUMBER;
16:22:01 531  var_offer_id		  NUMBER;
16:22:01 532  var_license_start_date	  DATE;
16:22:01 533  var_license_end_date	  DATE;
16:22:01 534  
16:22:01 535  var_entitlement_dupration   VARCHAR2(30);
16:22:01 536  var_d_entitlement_dupration NUMBER;
16:22:01 537  
16:22:01 538  var_ym_interval INTERVAL YEAR TO MONTH;
16:22:01 539  var_ds_interval INTERVAL DAY(3) TO SECOND;
16:22:01 540  
16:22:01 541  -- EXCEPTIONS
16:22:01 542  BAD_SUBSCRIPTION_ID     EXCEPTION;
16:22:01 543  BAD_SUBSCRIPTION_STATUS EXCEPTION;
16:22:01 544  NO_LICENSE_FOUND	      EXCEPTION;
16:22:01 545  NO_OFFER_FOUND	      EXCEPTION;
16:22:01 546  EXCEPTION_MESSAGE       VARCHAR2(1024);
16:22:01 547  BEGIN
16:22:01 548  	-- TODO: Finish this prcedure (in Phase II)
16:22:01 549  
16:22:01 550  	-- Get subscription by id. FAULT if no such subscription.
16:22:01 551  	-- begin TX
16:22:01 552  	--   Get for update associated license (subscription.license_id). FAULT if not found.
16:22:01 553  	--   Set status to PROCESSED.
16:22:01 554  	--   updated record.
16:22:01 555  	--   compute days remaining in the subscription: original end_date - today = days_remaining_adjustment
16:22:01 556  	--   new subscription status is SUSPENDED.
16:22:01 557  	--   suspend_date is now.
16:22:01 558  	--   update subscription record.
16:22:01 559  	-- end TX
16:22:01 560  
16:22:01 561  	BEGIN
16:22:01 562  	  SELECT
16:22:01 563  	    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID into var_subscription_status_id
16:22:01 564  	  FROM
16:22:01 565  	    SUBSCRIPTION
16:22:01 566  	  WHERE
16:22:01 567  	    SUBSCRIPTION.ID = in_subs_id;
16:22:01 568  	  EXCEPTION
16:22:01 569  	  WHEN NO_DATA_FOUND THEN
16:22:01 570  	    RAISE BAD_SUBSCRIPTION_ID;
16:22:01 571  	END;
16:22:01 572  
16:22:01 573  	IF var_subscription_status_id != GLOBAL_STATUSES_V22.SUBSCRIPTION_ACTIVE THEN
16:22:01 574  	  RAISE BAD_SUBSCRIPTION_STATUS;
16:22:01 575  	END IF;
16:22:01 576  
16:22:01 577  	BEGIN
16:22:01 578  	  SELECT
16:22:01 579  	    LICENSE.ID,
16:22:01 580  	    LICENSE.OFFER_ID,
16:22:01 581  	    LICENSE.START_DATE
16:22:01 582  	    into
16:22:01 583  	    var_license_id,
16:22:01 584  	    var_offer_id,
16:22:01 585  	    var_license_start_date
16:22:01 586  	  FROM
16:22:01 587  	    LICENSE
16:22:01 588  	  WHERE
16:22:01 589  	    LICENSE.SUBSCRIPTION_ID = in_subs_id
16:22:01 590  	      AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V22.LICENSE_ACTIVE;
16:22:01 591  	  EXCEPTION
16:22:01 592  	  WHEN NO_DATA_FOUND THEN
16:22:01 593  	    RAISE NO_LICENSE_FOUND;
16:22:01 594  	END;
16:22:01 595  
16:22:01 596  	PROCS_LICENSE_CRU_V22.UPDATE_LICENSE(
16:22:01 597  	  in_license_id        => var_license_id,
16:22:01 598  	  in_updated_by        => in_updated_by,
16:22:01 599  	  in_license_status_id => GLOBAL_STATUSES_V22.LICENSE_CLOSED
16:22:01 600  	);
16:22:01 601  
16:22:01 602  	BEGIN
16:22:01 603  	  SELECT
16:22:01 604  	    OFFER.ENTITLEMENT_DURATION into var_entitlement_dupration
16:22:01 605  	  FROM
16:22:01 606  	    OFFER
16:22:01 607  	  WHERE
16:22:01 608  	    OFFER.ID = var_offer_id;
16:22:01 609  	  EXCEPTION
16:22:01 610  	  WHEN NO_DATA_FOUND THEN
16:22:01 611  	    RAISE NO_OFFER_FOUND;
16:22:01 612  	END;
16:22:01 613  
16:22:01 614  	var_ym_interval := substr(var_entitlement_dupration, 0, 4);
16:22:01 615  	var_ds_interval := substr(var_entitlement_dupration, 4);
16:22:01 616  
16:22:01 617  	var_license_end_date := var_license_start_date + var_ym_interval + var_ds_interval;
16:22:01 618  
16:22:01 619  	var_d_entitlement_dupration := var_license_end_date - current_date;
16:22:01 620  
16:22:01 621  	PROCS_SUBSCRIPTION_CRU_V22.UPDATE_SUBSCRIPTION(
16:22:01 622  	  in_subscription_id	       => in_subs_id,
16:22:01 623  	  in_updated_by 	       => in_updated_by,
16:22:01 624  	  in_suspend_date	       => SYSDATE,
16:22:01 625  	  in_subscription_status_id    => GLOBAL_STATUSES_V22.SUBSCRIPTION_SUSPENDED,
16:22:01 626  	  in_days_remainning_ajustment => var_d_entitlement_dupration
16:22:01 627  	);
16:22:01 628  
16:22:01 629  EXCEPTION
16:22:01 630  WHEN BAD_SUBSCRIPTION_ID THEN
16:22:01 631  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 632  	  SPROC_NAME, 'No such subscription');
16:22:01 633  WHEN BAD_SUBSCRIPTION_STATUS THEN
16:22:01 634  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.STATE_ERROR,
16:22:01 635  	  SPROC_NAME, 'Subscription is not active');
16:22:01 636  WHEN NO_LICENSE_FOUND THEN
16:22:01 637  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 638  	  SPROC_NAME, 'Can not find license associated with given subscription ID');
16:22:01 639  WHEN NO_OFFER_FOUND THEN
16:22:01 640  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 641  	  SPROC_NAME, 'Can not find offer associated with given subscription ID');
16:22:01 642  WHEN OTHERS THEN
16:22:01 643  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:01 644  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:22:01 645  END SUSPEND_SUBSCRIPTION;
