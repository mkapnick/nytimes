16:22:01 908  PROCEDURE GET_OFFER_LENGTH_IN_DAYS (
16:22:01 909  	in_offer_id   IN NUMBER,
16:22:01 910  	in_start_date IN DATE DEFAULT SYSDATE,
16:22:01 911  	out_days      OUT NUMBER
16:22:01 912  ) AS
16:22:01 913  SPROC_NAME CONSTANT VARCHAR2(24) := 'GET_OFFER_LENGTH_IN_DAYS';
16:22:01 914  -- VARIABLES
16:22:01 915  var_offer_duration VARCHAR2(30);
16:22:01 916  var_offer_years	 NUMBER;
16:22:01 917  var_offer_months	 NUMBER;
16:22:01 918  var_offer_days	 NUMBER;
16:22:01 919  var_offer_end_date DATE;
16:22:01 920  -- EXCEPTIONS
16:22:01 921  BAD_OFFER_ID EXCEPTION;
16:22:01 922  BEGIN
16:22:01 923  
16:22:01 924  	BEGIN
16:22:01 925  	  SELECT
16:22:01 926  	    OFFER.ENTITLEMENT_DURATION into var_offer_duration
16:22:01 927  	  FROM
16:22:01 928  	    OFFER
16:22:01 929  	  WHERE
16:22:01 930  	    OFFER.ID = in_offer_id;
16:22:01 931  	  EXCEPTION
16:22:01 932  	    WHEN NO_DATA_FOUND THEN
16:22:01 933  	      RAISE BAD_OFFER_ID;
16:22:01 934  	END;
16:22:01 935  
16:22:01 936  	PROCS_COMMON_V22.ISO8601DURATION_TO_INTERVALS (
16:22:01 937  	  var_offer_duration,
16:22:01 938  	  var_offer_years,
16:22:01 939  	  var_offer_months,
16:22:01 940  	  var_offer_days
16:22:01 941  	);
16:22:01 942  
16:22:01 943  	var_offer_end_date := ( ( in_start_date
16:22:01 944  	  + GLOBAL_CONSTANTS_V22.ONE_DAY_INTERVAL * var_offer_days )
16:22:01 945  	  + GLOBAL_CONSTANTS_V22.ONE_MONTH_INTERVAL * var_offer_months )
16:22:01 946  	  + GLOBAL_CONSTANTS_V22.ONE_YEAR_INTERVAL * var_offer_years;
16:22:01 947  
16:22:01 948  	out_days := var_offer_end_date - in_start_date;
16:22:01 949  
16:22:01 950  EXCEPTION
16:22:01 951  WHEN BAD_OFFER_ID THEN
16:22:01 952  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.NOT_FOUND,
16:22:01 953  	  SPROC_NAME, 'No such offer');
16:22:01 954  WHEN OTHERS THEN
16:22:01 955  	PROCS_COMMON_V22.THROW_EXCEPTION(APP_EXCEPTION_CODES_V22.UNKNOWN_ERROR,
16:22:01 956  	  SPROC_NAME, 'Unknown error', SQLERRM);
16:22:01 957  END GET_OFFER_LENGTH_IN_DAYS;
