16:22:01 1269  ) AS
16:22:01 1270  SPROC_NAME CONSTANT VARCHAR2(27) := 'GET_OFFER_CHAIN_ELIGIBILITY';
16:22:01 1271  -- VARIABLES
16:22:01 1272  temp_offer_chain_id NUMBER;
16:22:01 1273  var_eligibility_name OFFER_CHAIN_ELIGIBILITY.NAME%TYPE;
16:22:01 1274  -- EXCEPTIONS
16:22:01 1275  BAD_OFFER_CHAIN_ID EXCEPTION;
16:22:01 1276  BEGIN
