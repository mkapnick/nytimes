13:41:20 811  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 812  	  SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 813  END GET_SUBSCRIPTION_INVOICES;
