13:41:19 414  FUNCTION CALCULATE_OFFER_CHAIN_END_DATE (
13:41:19 415  	in_offer_chain_id	  IN NUMBER,
13:41:19 416  	in_offer_chain_start_date IN DATE
13:41:19 417  ) RETURN DATE AS
13:41:19 418  -- VARIABLES
13:41:19 419  SPROC_NAME	     CONSTANT VARCHAR2(30) := 'CALCULATE_OFFER_CHAIN_END_DATE';
13:41:19 420  temp_offer_chain_id    NUMBER;
13:41:19 421  var_offer_chain_length NUMBER;
13:41:19 422  var_offer_duration     VARCHAR2(30);
13:41:19 423  var_offer_recurrences  NUMBER;
13:41:19 424  var_end_date	     DATE;
13:41:19 425  
13:41:19 426  var_offer_ym_interval INTERVAL YEAR TO MONTH;
13:41:19 427  var_offer_ds_interval INTERVAL DAY(3) TO SECOND;
13:41:19 428  var_offer_years	    NUMBER;
13:41:19 429  var_offer_months	    NUMBER;
13:41:19 430  var_offer_days	    NUMBER;
13:41:19 431  var_infinity_offers_count NUMBER;
13:41:19 432  
13:41:19 433  var_offers_set SYS_REFCURSOR;
13:41:19 434  
13:41:19 435  -- EXCEPTIONS
13:41:19 436  BAD_OFFER_CHAIN_ID EXCEPTION;
13:41:19 437  BEGIN
13:41:19 438  
13:41:19 439  	var_end_date := in_offer_chain_start_date;
13:41:19 440  
13:41:19 441  	-- Check that offer chain exists
13:41:19 442  	BEGIN
13:41:19 443  	  SELECT
13:41:19 444  	    OFFER_CHAIN.ID into temp_offer_chain_id
13:41:19 445  	  FROM
13:41:19 446  	    OFFER_CHAIN
13:41:19 447  	  WHERE
13:41:19 448  	    OFFER_CHAIN.ID = in_offer_chain_id;
13:41:19 449  	  EXCEPTION
13:41:19 450  	    WHEN NO_DATA_FOUND
13:41:19 451  	      THEN RAISE BAD_OFFER_CHAIN_ID;
13:41:19 452  	END;
13:41:19 453  
13:41:19 454  	SELECT
13:41:19 455  	  COUNT(*) into var_infinity_offers_count
13:41:19 456  	FROM
13:41:19 457  	  OFFER_OFFER_CHAIN
13:41:19 458  	WHERE
13:41:19 459  	  OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
13:41:19 460  	  AND OFFER_OFFER_CHAIN.NUM_RECURRENCES = GLOBAL_ENUMS_V21.OFFER_REC_INFINITY;
13:41:19 461  
13:41:19 462  	IF var_infinity_offers_count > 0 THEN
13:41:19 463  	  -- Offer chain contains offers with infinity num of recurrences
13:41:19 464  	  RETURN NULL;
13:41:19 465  	END IF;
13:41:19 466  
13:41:19 467  	-- Select offers durations
13:41:19 468  	OPEN var_offers_set FOR
13:41:19 469  	SELECT
13:41:19 470  	  OFFER.ENTITLEMENT_DURATION,
13:41:19 471  	  OFFER_OFFER_CHAIN.NUM_RECURRENCES
13:41:19 472  	FROM
13:41:19 473  	  OFFER_OFFER_CHAIN
13:41:19 474  	  INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
13:41:19 475  	WHERE
13:41:19 476  	  OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id;
13:41:19 477  
13:41:19 478  	-- Calculate sum of offers durations
13:41:19 479  	LOOP
13:41:19 480  	  FETCH var_offers_set INTO var_offer_duration, var_offer_recurrences;
13:41:19 481  	  EXIT WHEN var_offers_set%NOTFOUND;
13:41:19 482  	  PROCS_COMMON_V21.ISO8601DURATION_TO_INTERVALS(var_offer_duration, var_offer_years, var_offer_months, var_offer_days);
13:41:19 483  	  var_offer_ym_interval := var_offer_years||'-'||var_offer_months;
13:41:19 484  	  var_offer_ds_interval := var_offer_days||' 0:0:0';
13:41:19 485  	  var_end_date := var_end_date + ( var_offer_ym_interval * ( var_offer_recurrences + 1) ) + ( var_offer_ds_interval * ( var_offer_recurrences + 1) );
13:41:19 486  	END LOOP;
13:41:19 487  
13:41:19 488  	RETURN var_end_date;
13:41:19 489  
13:41:19 490  EXCEPTION
13:41:19 491  WHEN BAD_OFFER_CHAIN_ID THEN
13:41:19 492  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:19 493  	  SPROC_NAME, 'No such offer chain');
13:41:19 494  WHEN OTHERS THEN
13:41:19 495  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:19 496  	  SPROC_NAME, 'Unknown error', SQLERRM);
13:41:19 497  END CALCULATE_OFFER_CHAIN_END_DATE;
