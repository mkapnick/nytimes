13:41:18  88  	PROCEDURE CHASE_PROFILE_BY_REQ_FILE_ID(
13:41:18  89  	  in_request_file_id IN CC_UPDATE.CC_REQUEST_FILE_ID%TYPE,
13:41:18  90  	  in_start	     IN NUMBER,
13:41:18  91  	  in_end	     IN NUMBER,
13:41:18  92  	  out_result_set     OUT SYS_REFCURSOR
13:41:18  93  	) AS
13:41:18  94  	SPROC_NAME CONSTANT VARCHAR2(32) := 'CHASE_PROFILE_BY_REQ_FILE_ID';
13:41:18  95  	var_range_diff	    NUMBER := 0;
13:41:18  96  	var_upper_bond_diff NUMBER := 0;
13:41:18  97  	var_l_start	    NUMBER := 0;
13:41:18  98  	var_l_end	    NUMBER := 0;
13:41:18  99  	BEGIN
13:41:18 100  	  --Normalize the end points [START]
13:41:18 101  	  IF (in_start IS NULL OR in_start < 0) Then
13:41:18 102  	    var_l_start := 0;
13:41:18 103  	  ELSE
13:41:18 104  	    var_l_start := in_start;
13:41:18 105  	  END IF;
13:41:18 106  
13:41:18 107  	  IF (in_end IS NULL) Then
13:41:18 108  	    var_l_end := 500;
13:41:18 109  	  ELSE
13:41:18 110  	    var_l_end := in_end;
13:41:18 111  	  END IF;
13:41:18 112  
13:41:18 113  	  var_l_start := var_l_start + 1;
13:41:18 114  	  var_l_end   := var_l_end   + 1;
13:41:18 115  
13:41:18 116  	  var_range_diff  := var_l_end - var_l_start;
13:41:18 117  	  var_upper_bond_diff :=  var_range_diff - 1000;
13:41:18 118  
13:41:18 119  	  IF (var_upper_bond_diff > 0) Then
13:41:18 120  	    var_l_end := var_l_end - var_upper_bond_diff;
13:41:18 121  	  END IF;
13:41:18 122  	  --Normalize the end points [END]
13:41:18 123  
13:41:18 124  	  OPEN out_result_set FOR
13:41:18 125  	    SELECT CHASE_PROFILE_ID FROM
13:41:18 126  	      (SELECT rownum rnum, q.* FROM
13:41:18 127  		 (SELECT
13:41:18 128  		    cc.CHASE_PROFILE_ID
13:41:18 129  		  FROM
13:41:18 130  		    CREDIT_CARD cc,
13:41:18 131  		    CC_UPDATE ccu
13:41:18 132  		  WHERE
13:41:18 133  		    ccu.CC_REQUEST_FILE_ID = in_request_file_id
13:41:18 134  		    AND ccu.CREDIT_CARD_ID = cc.id
13:41:18 135  		) Q
13:41:18 136  	      WHERE rownum <= var_l_end)
13:41:18 137  	    WHERE rnum >= var_l_Start;
13:41:18 138  	END CHASE_PROFILE_BY_REQ_FILE_ID;
