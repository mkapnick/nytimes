13:41:20 697  /******************************************************************************/
13:41:20 698  
13:41:20 699  PROCEDURE CALCULATE_INVOICE_CHARGEBACK (
13:41:20 700  	in_invoice_id	      IN NUMBER,
13:41:20 701  	in_chargeback_date    IN DATE,
13:41:20 702  	out_chargeback_amount OUT NUMBER
13:41:20 703  ) AS
13:41:20 704  -- VARIABLES
13:41:20 705  SPROC_NAME	     CONSTANT VARCHAR2(28) := 'CALCULATE_INVOICE_CHARGEBACK';
13:41:20 706  var_chargeback_date    DATE;
13:41:20 707  temp_invoice_id	     NUMBER;
13:41:20 708  var_licenses_number    NUMBER;
13:41:20 709  var_invoice_start_date DATE;
13:41:20 710  var_invoice_end_date   DATE;
13:41:20 711  var_offer_id	     NUMBER;
13:41:20 712  var_offer_days_interval NUMBER;
13:41:20 713  var_license_days_used  NUMBER;
13:41:20 714  var_invoice_amount     NUMBER(10,2);
13:41:20 715  var_offer_chain_id     NUMBER;
13:41:20 716  var_offer_chain_meta_data_val VARCHAR2(1024);
13:41:20 717  var_offer_chain_full_refund   NUMBER;
13:41:20 718  var_offer_chain_prorated_ref  NUMBER;
13:41:20 719  var_chargeback_calculated NUMBER;
13:41:20 720  var_max_invoice_refund	NUMBER;
13:41:20 721  -- EXCEPTIONS
13:41:20 722  BAD_INVOICE_ID		  EXCEPTION;
13:41:20 723  NO_LICENSES_FOUND_EXCEPTION EXCEPTION;
13:41:20 724  OFFER_LENGTH_IS_ZERO	  EXCEPTION;
13:41:20 725  BEGIN
13:41:20 726  
13:41:20 727  	IF in_chargeback_date IS NULL THEN
13:41:20 728  	  var_chargeback_date := PROCS_COMMON_V21.NORMALIZE_DATE(current_date);
13:41:20 729  	ELSE
13:41:20 730  	  var_chargeback_date := PROCS_COMMON_V21.NORMALIZE_DATE(in_chargeback_date);
13:41:20 731  	END IF;
13:41:20 732  
13:41:20 733  	-- Check that invoice exists
13:41:20 734  	BEGIN
13:41:20 735  	  SELECT
13:41:20 736  	    INVOICE.ID into temp_invoice_id
13:41:20 737  	  FROM
13:41:20 738  	    INVOICE
13:41:20 739  	  WHERE
13:41:20 740  	    INVOICE.ID = in_invoice_id;
13:41:20 741  	  EXCEPTION
13:41:20 742  	    WHEN NO_DATA_FOUND THEN
13:41:20 743  	      RAISE BAD_INVOICE_ID;
13:41:20 744  	END;
13:41:20 745  
13:41:20 746  	-- Check that invoice has at least one license
13:41:20 747  	SELECT
13:41:20 748  	  COUNT(*) into var_licenses_number
13:41:20 749  	FROM
13:41:20 750  	  LICENSE
13:41:20 751  	WHERE
13:41:20 752  	  LICENSE.INVOICE_ID = in_invoice_id;
13:41:20 753  
13:41:20 754  	IF var_licenses_number = 0 THEN
13:41:20 755  	  RAISE NO_LICENSES_FOUND_EXCEPTION;
13:41:20 756  	END IF;
13:41:20 757  
13:41:20 758  	SELECT
13:41:20 759  	  MIN(LICENSE.START_DATE) into var_invoice_start_date
13:41:20 760  	FROM
13:41:20 761  	  LICENSE
13:41:20 762  	WHERE
13:41:20 763  	  LICENSE.INVOICE_ID = in_invoice_id;
13:41:20 764  
13:41:20 765  	var_invoice_start_date := PROCS_COMMON_V21.NORMALIZE_DATE(var_invoice_start_date);
13:41:20 766  
13:41:20 767  	SELECT DISTINCT
13:41:20 768  	  LICENSE.OFFER_ID into var_offer_id
13:41:20 769  	FROM
13:41:20 770  	  LICENSE
13:41:20 771  	WHERE
13:41:20 772  	  LICENSE.INVOICE_ID = in_invoice_id;
13:41:20 773  
13:41:20 774  	SELECT
13:41:20 775  	  MAX (LICENSE.END_DATE) into var_invoice_end_date
13:41:20 776  	FROM
13:41:20 777  	  LICENSE
13:41:20 778  	WHERE
13:41:20 779  	  LICENSE.INVOICE_ID = in_invoice_id;
13:41:20 780  
13:41:20 781  	-- All licenses for given invoice should point into the same offer
13:41:20 782  
13:41:20 783  	PROCS_OFFER_CHAIN_V21.GET_OFFER_LENGTH_IN_DAYS(
13:41:20 784  	  in_offer_id	=> var_offer_id,
13:41:20 785  	  in_start_date => var_invoice_start_date,
13:41:20 786  	  out_days	=> var_offer_days_interval
13:41:20 787  	);
13:41:20 788  
13:41:20 789  	IF var_offer_days_interval = 0 THEN
13:41:20 790  	  RAISE OFFER_LENGTH_IS_ZERO;
13:41:20 791  	END IF;
13:41:20 792  
13:41:20 793  	PROCS_INVOICE_V21.GET_INVOICE_DAYS_USED_NUMBER(
13:41:20 794  	  in_invoice_id      => in_invoice_id,
13:41:20 795  	  in_chargeback_date => var_chargeback_date,
13:41:20 796  	  out_days_num	     => var_license_days_used
13:41:20 797  	);
13:41:20 798  
13:41:20 799  	PROCS_INVOICE_V21.CALCULATE_INVOICE_AMOUNT(
13:41:20 800  	  in_invoice_id => in_invoice_id,
13:41:20 801  	  out_amount	=> var_invoice_amount
13:41:20 802  	);
13:41:20 803  
13:41:20 804  	-- 39437
13:41:20 805  	-- Get offer chain id by invoice id
13:41:20 806  	SELECT DISTINCT
13:41:20 807  	  SUBSCRIPTION.OFFER_CHAIN_ID into var_offer_chain_id
13:41:20 808  	FROM
13:41:20 809  	  SUBSCRIPTION
13:41:20 810  	  INNER JOIN LICENSE ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
13:41:20 811  	WHERE
13:41:20 812  	  LICENSE.INVOICE_ID = in_invoice_id;
13:41:20 813  
13:41:20 814  	-- Get offer chain's meta data full amount value
13:41:20 815  	PROCS_OFFER_CHAIN_V21.GET_OFFER_CHAIN_MD_VALUE(
13:41:20 816  	  in_offer_chain_id => var_offer_chain_id,
13:41:20 817  	  in_meta_data_name => GLOBAL_CONSTANTS_V21.OFFER_CHAIN_FULL_REFUND,
13:41:20 818  	  out_value	    => var_offer_chain_meta_data_val
13:41:20 819  	);
13:41:20 820  	IF var_offer_chain_meta_data_val IS NULL THEN
13:41:20 821  	  var_offer_chain_full_refund := NULL;
13:41:20 822  	ELSE
13:41:20 823  	  var_offer_chain_full_refund := TO_NUMBER(var_offer_chain_meta_data_val);
13:41:20 824  	END IF;
13:41:20 825  
13:41:20 826  	-- Get offer chain's meta data prorated amount value
13:41:20 827  	PROCS_OFFER_CHAIN_V21.GET_OFFER_CHAIN_MD_VALUE(
13:41:20 828  	  in_offer_chain_id => var_offer_chain_id,
13:41:20 829  	  in_meta_data_name => GLOBAL_CONSTANTS_V21.OFFER_CHAIN_PRORATED_REFUND,
13:41:20 830  	  out_value	    => var_offer_chain_meta_data_val
13:41:20 831  	);
13:41:20 832  	IF var_offer_chain_meta_data_val IS NULL THEN
13:41:20 833  	  var_offer_chain_prorated_ref := NULL;
13:41:20 834  	ELSE
13:41:20 835  	  var_offer_chain_prorated_ref := TO_NUMBER(var_offer_chain_meta_data_val);
13:41:20 836  	END IF;
13:41:20 837  
13:41:20 838  	var_chargeback_calculated := GLOBAL_CONSTANTS_V21.FALSE;
13:41:20 839  
13:41:20 840  	IF var_offer_chain_full_refund IS NOT NULL
13:41:20 841  	   AND var_chargeback_calculated = GLOBAL_CONSTANTS_V21.FALSE THEN
13:41:20 842  	  IF var_license_days_used < var_offer_chain_full_refund THEN
13:41:20 843  	    out_chargeback_amount := var_invoice_amount;
13:41:20 844  	    var_chargeback_calculated := GLOBAL_CONSTANTS_V21.TRUE;
13:41:20 845  	  END IF;
13:41:20 846  	END IF;
13:41:20 847  
13:41:20 848  	IF var_offer_chain_prorated_ref IS NOT NULL
13:41:20 849  	   AND var_chargeback_calculated = GLOBAL_CONSTANTS_V21.FALSE THEN
13:41:20 850  	  IF var_license_days_used < var_offer_chain_prorated_ref THEN
13:41:20 851  	    out_chargeback_amount := ( var_invoice_amount * (var_offer_days_interval - var_license_days_used) ) / var_offer_days_interval;
13:41:20 852  	    var_chargeback_calculated := GLOBAL_CONSTANTS_V21.TRUE;
13:41:20 853  	  END IF;
13:41:20 854  	END IF;
13:41:20 855  
13:41:20 856  	IF var_chargeback_calculated = GLOBAL_CONSTANTS_V21.TRUE THEN
13:41:20 857  	  PROCS_INVOICE_V21.GET_MAX_REFUND(
13:41:20 858  	    in_invoice_id => in_invoice_id,
13:41:20 859  	    out_amount	  => var_max_invoice_refund
13:41:20 860  	  );
13:41:20 861  	  IF var_max_invoice_refund < out_chargeback_amount THEN
13:41:20 862  	    out_chargeback_amount := var_max_invoice_refund;
13:41:20 863  	  END IF;
13:41:20 864  	END IF;
13:41:20 865  
13:41:20 866  	IF var_chargeback_calculated = GLOBAL_CONSTANTS_V21.FALSE THEN
13:41:20 867  	  out_chargeback_amount := 0;
13:41:20 868  	END IF;
13:41:20 869  
13:41:20 870  EXCEPTION
13:41:20 871  WHEN BAD_INVOICE_ID THEN
13:41:20 872  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 873  	  SPROC_NAME, 'No such invoice');
13:41:20 874  WHEN NO_LICENSES_FOUND_EXCEPTION THEN
13:41:20 875  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 876  	  SPROC_NAME, 'No licenses found');
13:41:20 877  WHEN OFFER_LENGTH_IS_ZERO THEN
13:41:20 878  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 879  	  SPROC_NAME, 'Offer length is zero');
13:41:20 880  WHEN OTHERS THEN
13:41:20 881  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 882  	  SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 883  END CALCULATE_INVOICE_CHARGEBACK;
13:41:20 884  
13:41:20 885  /********************************************************************/
13:41:20 886  
13:41:20 887  PROCEDURE APPLY_REFUND (
13:41:20 888  	in_invoice_id	     IN NUMBER,
13:41:20 889  	in_chargeback_amount IN NUMBER,
13:41:20 890  	in_created_by	     IN VARCHAR2,
13:41:20 891  	out_charge_id	     OUT NUMBER
13:41:20 892  ) AS
13:41:20 893  -- VARIABLES
13:41:20 894  SPROC_NAME		 CONSTANT VARCHAR2(16) := 'APPLY_CHARGEBACK';
13:41:20 895  temp_invoice_id		 NUMBER;
13:41:20 896  var_total_charges_amount	 NUMBER(10,2);
13:41:20 897  var_charge_amount_to_apply NUMBER(10,2);
13:41:20 898  var_transaction_id	 NUMBER;
13:41:20 899  var_instrument_type_id	 NUMBER;
13:41:20 900  var_instrument_id 	 NUMBER;
13:41:20 901  var_charge_id		 NUMBER;
13:41:20 902  -- EXCEPTIONS
13:41:20 903  BAD_INVOICE_ID		 EXCEPTION;
13:41:20 904  CAN_NOT_CREATE_TRANSACTION EXCEPTION;
13:41:20 905  CAN_NOT_FIND_INSTRUMENT	 EXCEPTION;
13:41:20 906  CAN_NOT_CREATE_CHARGE	 EXCEPTION;
13:41:20 907  EXCEPTION_MESSAGE 	 VARCHAR2(1024);
13:41:20 908  BEGIN
13:41:20 909  
13:41:20 910  	BEGIN
13:41:20 911  	  SELECT
13:41:20 912  	    INVOICE.ID into temp_invoice_id
13:41:20 913  	  FROM
13:41:20 914  	    INVOICE
13:41:20 915  	  WHERE
13:41:20 916  	    INVOICE.ID = in_invoice_id;
13:41:20 917  	  EXCEPTION
13:41:20 918  	    WHEN NO_DATA_FOUND THEN
13:41:20 919  	      RAISE BAD_INVOICE_ID;
13:41:20 920  	END;
13:41:20 921  
13:41:20 922  	SELECT
13:41:20 923  	  SUM (CHARGE.CHARGE_AMOUNT) into var_total_charges_amount
13:41:20 924  	FROM
13:41:20 925  	  CHARGE
13:41:20 926  	WHERE
13:41:20 927  	  CHARGE.INVOICE_ID = in_invoice_id
13:41:20 928  	  AND CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V21.CHARGE_PROCESSED
13:41:20 929  	  AND CHARGE.INSTRUMENT_TYPE_ID != GLOBAL_ENUMS_V21.INSTRUMENT_GIFT_CERTIFICATE;
13:41:20 930  
13:41:20 931  	-- FIXME: Maybe whe should throw exception here?
13:41:20 932  	IF var_total_charges_amount < in_chargeback_amount THEN
13:41:20 933  	  var_charge_amount_to_apply := var_total_charges_amount;
13:41:20 934  	ELSE
13:41:20 935  	  var_charge_amount_to_apply := in_chargeback_amount;
13:41:20 936  	END IF;
13:41:20 937  
13:41:20 938  	BEGIN
13:41:20 939  	  PROCS_TRANSACTION_V21.CREATE_TRANSACTION(
13:41:20 940  	    in_transaction_id	      => NULL,
13:41:20 941  	    in_status_id	      => GLOBAL_STATUSES_V21.TRANSACTION_PREPARE,
13:41:20 942  	    in_amount		      => -var_charge_amount_to_apply,
13:41:20 943  	    in_created_by	      => in_created_by,
13:41:20 944  	    in_order_id 	      => NULL,
13:41:20 945  	    in_is_refund	      => GLOBAL_CONSTANTS_V21.TRUE,
13:41:20 946  	    in_transaction_type_code  => 'REFUND',
13:41:20 947  	    out_transaction_id	      => var_transaction_id
13:41:20 948  	  );
13:41:20 949  	  EXCEPTION
13:41:20 950  	    WHEN OTHERS THEN
13:41:20 951  	      EXCEPTION_MESSAGE := SQLERRM;
13:41:20 952  	      RAISE CAN_NOT_CREATE_TRANSACTION;
13:41:20 953  	END;
13:41:20 954  
13:41:20 955  	BEGIN
13:41:20 956  	  SELECT
13:41:20 957  	    C.INSTRUMENT_TYPE_ID,
13:41:20 958  	    C.INSTRUMENT_ID
13:41:20 959  	    into
13:41:20 960  	    var_instrument_type_id,
13:41:20 961  	    var_instrument_id
13:41:20 962  	  FROM
13:41:20 963  	    CHARGE C,
13:41:20 964  	    TRANSACTION_ATTEMPT TA,
13:41:20 965  	    TRANSACTION T
13:41:20 966  	  WHERE
13:41:20 967  	    C.INVOICE_ID = in_invoice_id and
13:41:20 968  	    C.TRANSACTION_ID = T.ID and
13:41:20 969  	    TA.TRANSACTION_ID = T.ID and
13:41:20 970  	    TA.TRANSACTION_ATTEMPT_STATUS_ID = GLOBAL_STATUSES_V21.TRANS_ATTEMPT_SUCCESS and
13:41:20 971  	    T.IS_REFUND = GLOBAL_CONSTANTS_V21.FALSE and
13:41:20 972  	    T.TRANSACTION_AMOUNT >= 0 and
13:41:20 973  	    rownum < 2;
13:41:20 974  	EXCEPTION
13:41:20 975  	    WHEN NO_DATA_FOUND THEN
13:41:20 976  	      RAISE CAN_NOT_FIND_INSTRUMENT;
13:41:20 977  	END;
13:41:20 978  	BEGIN
13:41:20 979  	  PROCS_CHARGE_V21.CREATE_CHARGE(
13:41:20 980  	    in_invoice_id	  => in_invoice_id,
13:41:20 981  	    in_transaction_id	  => var_transaction_id,
13:41:20 982  	    in_instrument_type_id => var_instrument_type_id,
13:41:20 983  	    in_instrument_id	  => var_instrument_id,
13:41:20 984  	    in_charge_amount	  => -var_charge_amount_to_apply,
13:41:20 985  	    in_created_by	  => in_created_by,
13:41:20 986  	    in_charge_status_id   => GLOBAL_STATUSES_V21.CHARGE_OPENED,
13:41:20 987  	    out_charge_id	  => var_charge_id
13:41:20 988  	  );
13:41:20 989  	  EXCEPTION
13:41:20 990  	    WHEN OTHERS THEN
13:41:20 991  	      EXCEPTION_MESSAGE := SQLERRM;
13:41:20 992  	      RAISE CAN_NOT_CREATE_CHARGE;
13:41:20 993  	END;
13:41:20 994  
13:41:20 995  	out_charge_id := var_charge_id;
13:41:20 996  
13:41:20 997  EXCEPTION
13:41:20 998  WHEN BAD_INVOICE_ID THEN
13:41:20 999  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 1000  	   SPROC_NAME, 'No such invoice');
13:41:20 1001  WHEN CAN_NOT_CREATE_TRANSACTION THEN
13:41:20 1002  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 1003  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
13:41:20 1004  WHEN CAN_NOT_FIND_INSTRUMENT THEN
13:41:20 1005  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 1006  	   SPROC_NAME, 'Could not find financial instrument');
13:41:20 1007  WHEN CAN_NOT_CREATE_CHARGE THEN
13:41:20 1008  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 1009  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
13:41:20 1010  WHEN OTHERS THEN
13:41:20 1011  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 1012  	   SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 1013  END APPLY_REFUND;
