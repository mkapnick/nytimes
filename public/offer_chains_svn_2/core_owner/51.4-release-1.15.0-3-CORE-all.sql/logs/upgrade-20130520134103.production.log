13:41:20 238  PROCEDURE GET_ACCOUNT_BY_INVOICE_ID (
13:41:20 239  /*
13:41:20 240  Throws exceptions:
13:41:20 241  APP_EXCEPTION_CODES_V21.NOT_FOUND
13:41:20 242  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
13:41:20 243  */
13:41:20 244  	in_invoice_id  IN  NUMBER,
13:41:20 245  	out_account_id OUT NUMBER
13:41:20 246  ) AS
13:41:20 247  -- VARIABLES
13:41:20 248  SPROC_NAME	 CONSTANT VARCHAR2(25) := 'GET_ACCOUNT_BY_INVOICE_ID';
13:41:20 249  temp_gc_account_id NUMBER;
13:41:20 250  temp_ss_account_id NUMBER;
13:41:20 251  temp_invoice_id	 NUMBER;
13:41:20 252  -- EXCEPTIONS
13:41:20 253  BAD_INVOICE_ID	   EXCEPTION;
13:41:20 254  CAN_NOT_FIND_ACCOUNT EXCEPTION;
13:41:20 255  BEGIN
13:41:20 256  
13:41:20 257  	-- Check that given invoice exists
13:41:20 258  	BEGIN
13:41:20 259  	  SELECT
13:41:20 260  	    INVOICE.ID into temp_invoice_id
13:41:20 261  	  FROM
13:41:20 262  	    INVOICE
13:41:20 263  	  WHERE
13:41:20 264  	    INVOICE.ID = in_invoice_id;
13:41:20 265  	  EXCEPTION
13:41:20 266  	    WHEN NO_DATA_FOUND THEN
13:41:20 267  	      RAISE BAD_INVOICE_ID;
13:41:20 268  	END;
13:41:20 269  
13:41:20 270  	-- Try to find gift certificate with given invoice
13:41:20 271  	BEGIN
13:41:20 272  	  SELECT
13:41:20 273  	    ACCOUNT.GROUP_ID into temp_gc_account_id
13:41:20 274  	  FROM
13:41:20 275  	    GIFT_CERTIFICATE
13:41:20 276  	    INNER JOIN ACCOUNT ON GIFT_CERTIFICATE.PURCHASER_GROUP_ID = ACCOUNT.GROUP_ID
13:41:20 277  	  WHERE
13:41:20 278  	    GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = in_invoice_id
13:41:20 279  	    AND ROWNUM <= 1;
13:41:20 280  	  EXCEPTION
13:41:20 281  	    WHEN NO_DATA_FOUND THEN
13:41:20 282  	      temp_gc_account_id := NULL;
13:41:20 283  	END;
13:41:20 284  
13:41:20 285  	-- check subscriptions for given invoice
13:41:20 286  	IF temp_gc_account_id IS NOT NULL THEN
13:41:20 287  	  out_account_id := temp_gc_account_id;
13:41:20 288  	ELSE
13:41:20 289  	  BEGIN
13:41:20 290  	    SELECT
13:41:20 291  	      ACCOUNT.GROUP_ID into temp_ss_account_id
13:41:20 292  	    FROM
13:41:20 293  	      LICENSE
13:41:20 294  	      INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
13:41:20 295  	      INNER JOIN ACCOUNT ON SUBSCRIPTION.ACCOUNT_ID = ACCOUNT.ID
13:41:20 296  	    WHERE
13:41:20 297  	      LICENSE.INVOICE_ID = in_invoice_id
13:41:20 298  	      AND ROWNUM <= 1;
13:41:20 299  	    EXCEPTION
13:41:20 300  	      WHEN NO_DATA_FOUND THEN
13:41:20 301  		temp_ss_account_id := NULL;
13:41:20 302  	  END;
13:41:20 303  	  IF temp_ss_account_id IS NULL THEN
13:41:20 304  	    RAISE CAN_NOT_FIND_ACCOUNT;
13:41:20 305  	  ELSE
13:41:20 306  	    out_account_id := temp_ss_account_id;
13:41:20 307  	  END IF;
13:41:20 308  	END IF;
13:41:20 309  
13:41:20 310  EXCEPTION
13:41:20 311  WHEN BAD_INVOICE_ID THEN
13:41:20 312  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 313  	  SPROC_NAME, 'No such invoice');
13:41:20 314  WHEN CAN_NOT_FIND_ACCOUNT THEN
13:41:20 315  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 316  	  SPROC_NAME, 'Could not find account by given invoice id');
13:41:20 317  WHEN OTHERS THEN
13:41:20 318  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 319  	  SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 320  END GET_ACCOUNT_BY_INVOICE_ID;
