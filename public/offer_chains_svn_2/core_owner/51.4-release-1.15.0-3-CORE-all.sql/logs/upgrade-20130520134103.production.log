13:41:19 106  FUNCTION CREATE_NEW_TRANSFER_SET
13:41:19 107  	  (
13:41:19 108  	      in_set_maximum NUMBER)
13:41:19 109  	  RETURN core_owner.polling_sync.set_id%type
13:41:19 110  IS
13:41:19 111  	  pragma autonomous_transaction;
13:41:19 112  	  v_set_id core_owner.polling_sync.set_id%type;
13:41:19 113  BEGIN
13:41:19 114  	  SELECT
13:41:19 115  	      pollsync_setid_seq.nextval
13:41:19 116  	  INTO
13:41:19 117  	      v_set_id
13:41:19 118  	  FROM
13:41:19 119  	      dual;
13:41:19 120  	  update
13:41:19 121  	    core_owner.polling_sync ps
13:41:19 122  	  set
13:41:19 123  	    ps.set_id = v_set_id
13:41:19 124  	  where
13:41:19 125  	    ps.set_id IS NULL and
13:41:19 126  	    rownum <= in_set_maximum
13:41:19 127  	  ;
13:41:19 128  	  COMMIT;
13:41:19 129  	  RETURN v_set_id;
13:41:19 130  END;
13:41:19 131  --------------------------------------------------------------------------------
13:41:19 132  FUNCTION RETREIVE_TRANSFER_SET
13:41:19 133  	  (
13:41:19 134  	      in_set_id core_owner.polling_sync.set_id%type)
13:41:19 135  	  RETURN sys_refcursor
13:41:19 136  IS
13:41:19 137  	  v_refcursor sys_refcursor;
13:41:19 138  BEGIN
13:41:19 139  	  UPDATE
13:41:19 140  	      core_owner.polling_sync ps
13:41:19 141  	  SET
13:41:19 142  	      ps.last_send_date = sysdate,
13:41:19 143  	      ps.num_calls = ps.num_calls + 1
13:41:19 144  	  WHERE
13:41:19 145  	      ps.set_id = in_set_id ;
13:41:19 146  	  OPEN v_refcursor FOR
13:41:19 147  	  SELECT
13:41:19 148  	      ps.set_id,
13:41:19 149  	      ps.group_id,
13:41:19 150  	      ps.event_type,
13:41:19 151  	      ps.event_date
13:41:19 152  	  FROM
13:41:19 153  	      core_owner.polling_sync ps
13:41:19 154  	  WHERE
13:41:19 155  	      ps.set_id = in_set_id
13:41:19 156  	  AND ps.group_id IS NOT NULL ;
13:41:19 157  	  RETURN v_refcursor;
13:41:19 158  END;
13:41:19 159  --------------------------------------------------------------------------------
13:41:19 160  PROCEDURE GET_TRANSFER_SET
13:41:19 161  	  (
13:41:19 162  	      in_set_maximum NUMBER,
13:41:19 163  	      in_max_retries NUMBER,
13:41:19 164  	      out_refcursor OUT sys_refcursor)
13:41:19 165  IS
13:41:19 166  	  v_set_id core_owner.polling_sync.set_id%type;
13:41:19 167  BEGIN
13:41:19 168  	  /* Look for previously sent but unconfirmed sets and
13:41:19 169  	  send again until max_retries calls */
13:41:19 170  	  FOR x IN
13:41:19 171  	  (
13:41:19 172  	      SELECT
13:41:19 173  		  ps.set_id,
13:41:19 174  		  COUNT( *) set_size
13:41:19 175  	      FROM
13:41:19 176  		  core_owner.polling_sync ps
13:41:19 177  	      WHERE
13:41:19 178  		  ps.event_date > sysdate - 14
13:41:19 179  	      AND ps.confirm_date IS NULL
13:41:19 180  	      AND ps.last_send_date IS NOT NULL
13:41:19 181  	      AND ps.num_calls < in_max_retries
13:41:19 182  	      GROUP BY
13:41:19 183  		  ps.set_id
13:41:19 184  	      ORDER BY
13:41:19 185  		  ps.set_id
13:41:19 186  	  )
13:41:19 187  	  LOOP
13:41:19 188  	      out_refcursor := Retreive_Transfer_Set(x.set_id) ;
13:41:19 189  	      RETURN;
13:41:19 190  	  END LOOP;
13:41:19 191  	  v_set_id := Create_New_Transfer_Set(in_set_maximum) ;
13:41:19 192  	  out_refcursor := Retreive_Transfer_Set(v_set_id) ;
13:41:19 193  	  RETURN;
13:41:19 194  END;
13:41:19 195  --------------------------------------------------------------------------------
13:41:19 196  PROCEDURE CONFIRM_TRANSFER_SET
13:41:19 197  	  (
13:41:19 198  	      in_set_id core_owner.polling_sync.set_id%type)
13:41:19 199  IS
13:41:19 200  	  v_unconfirmable EXCEPTION;
13:41:19 201  BEGIN
13:41:19 202  	  UPDATE
13:41:19 203  	      core_owner.polling_sync ps
13:41:19 204  	  SET
13:41:19 205  	      ps.confirm_date = systimestamp
13:41:19 206  	  WHERE
13:41:19 207  	      ps.set_id = in_set_id
13:41:19 208  	  AND ps.confirm_date IS NULL ;
13:41:19 209  	  IF(sql%rowcount < 1) THEN
13:41:19 210  	      raise v_unconfirmable;
13:41:19 211  	  END IF;
13:41:19 212  END;
13:41:19 213  PROCEDURE SET_LAST_RUN(ts in timestamp)
13:41:19 214  IS
13:41:19 215  BEGIN
13:41:19 216  	  UPDATE POLLING_SYNC_LASTRUN
13:41:19 217  	  SET last_run = current_timestamp;
13:41:19 218  	  IF ( sql%rowcount = 0 )
13:41:19 219  	  THEN
13:41:19 220  	    INSERT INTO POLLING_SYNC_LASTRUN VALUES (ts);
13:41:19 221  	  END if;
13:41:19 222  	  COMMIT;
13:41:19 223  END;
13:41:19 224  PROCEDURE GET_LAST_RUN(ts out timestamp)
13:41:19 225  IS
13:41:19 226  BEGIN
13:41:19 227  	  SELECT LAST_RUN INTO ts
13:41:19 228  	  FROM POLLING_SYNC_LASTRUN
13:41:19 229  	  WHERE ROWNUM < 2;
13:41:19 230  EXCEPTION
13:41:19 231  	WHEN NO_DATA_FOUND
13:41:19 232  	THEN
13:41:19 233  	  ts := current_timestamp;
13:41:19 234  END;
13:41:19 235  END PROCS_POLLING_SYNC;
