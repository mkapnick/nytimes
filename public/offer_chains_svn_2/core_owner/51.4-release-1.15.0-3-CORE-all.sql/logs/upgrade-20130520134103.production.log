13:41:19  61  	  WHERE
13:41:19  62  	    OFFER.ID = in_offer_id;
13:41:19  63  	  EXCEPTION
13:41:19  64  	  WHEN NO_DATA_FOUND THEN
13:41:19  65  	    RAISE BAD_OFFER_ID;
13:41:19  66  	END;
13:41:19  67  
13:41:19  68  	-- Check that subscription exists
13:41:19  69  	BEGIN
13:41:19  70  	  SELECT
13:41:19  71  	    SUBSCRIPTION.ID into temp_subscription_id
13:41:19  72  	  FROM
13:41:19  73  	    SUBSCRIPTION
13:41:19  74  	  WHERE
13:41:19  75  	    SUBSCRIPTION.ID = in_subscription_id;
13:41:19  76  	  EXCEPTION
13:41:19  77  	  WHEN NO_DATA_FOUND THEN
13:41:19  78  	    RAISE BAD_SUBSCRIPTION_ID;
13:41:19  79  	END;
13:41:19  80  
13:41:19  81  	-- Check that invoice exists
13:41:19  82  	BEGIN
13:41:19  83  	  SELECT
13:41:19  84  	    INVOICE.ID into temp_invoice_id
13:41:19  85  	  FROM
13:41:19  86  	    INVOICE
13:41:19  87  	  WHERE
13:41:19  88  	    INVOICE.ID = in_invoice_id;
13:41:19  89  	  EXCEPTION
13:41:19  90  	  WHEN NO_DATA_FOUND THEN
13:41:19  91  	    RAISE BAD_INVOICE_ID;
13:41:19  92  	END;
13:41:19  93  
13:41:19  94  	-- convert offer duration into intervals
13:41:19  95  	BEGIN
13:41:19  96  	  PROCS_COMMON_V21.ISO8601DURATION_TO_INTERVALS(
13:41:19  97  	    var_offer_duration,
13:41:19  98  	    var_offer_years,
13:41:19  99  	    var_offer_months,
13:41:19 100  	    var_offer_days);
13:41:19 101  	  var_offer_ym_interval := var_offer_years||'-'||var_offer_months;
13:41:19 102  	  var_offer_ds_interval := var_offer_days||' 0:0:0';
13:41:19 103  	  EXCEPTION
13:41:19 104  	    WHEN OTHERS THEN
13:41:19 105  	      EXCEPTION_MESSAGE := SQLERRM;
13:41:19 106  	      RAISE BAD_OFFER_DURATION;
13:41:19 107  	END;
13:41:19 108  
13:41:19 109  	-- insert new row into license table
13:41:19 110  	PROCS_LICENSE_CRU_V21.CREATE_LICENSE(
13:41:19 111  	  out_license_id	      => var_new_license_id,
13:41:19 112  	  in_license_status_id	      => in_status_id,
13:41:19 113  	  in_needs_entitlements       => in_needs_entitlements,
13:41:19 114  	  in_start_date 	      => in_start_date,
13:41:19 115  	  in_offer_id		      => in_offer_id,
13:41:19 116  	  in_subscription_id	      => in_subscription_id,
13:41:19 117  	  in_invoice_id 	      => in_invoice_id,
13:41:19 118  	  in_end_date		      => NVL(in_end_date, in_start_date + var_offer_ym_interval + var_offer_ds_interval),
13:41:19 119  	  in_created_by 	      => in_created_by,
13:41:19 120  	  in_is_extension	      => in_is_extension,
13:41:19 121  	  in_current_offer_index      => in_current_offer_index,
13:41:19 122  	  in_current_offer_recurr_num => in_current_offer_recurr_num
13:41:19 123  	);
13:41:19 124  
13:41:19 125  	out_license_id := var_new_license_id;
13:41:19 126  
13:41:19 127  EXCEPTION
13:41:19 128  WHEN BAD_OFFER_DURATION THEN
13:41:19 129  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:19 130  	  SPROC_NAME, 'Bad offer duration format', EXCEPTION_MESSAGE);
13:41:19 131  WHEN BAD_OFFER_ID THEN
13:41:19 132  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:19 133  	  SPROC_NAME, 'No such offer');
13:41:19 134  WHEN BAD_SUBSCRIPTION_ID THEN
13:41:19 135  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:19 136  	  SPROC_NAME, 'No such subscription');
13:41:19 137  WHEN BAD_INVOICE_ID THEN
13:41:19 138  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:19 139  	  SPROC_NAME, 'No such invoice');
13:41:19 140  WHEN OTHERS THEN
13:41:19 141  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:19 142  	  SPROC_NAME, 'Unknown error', SQLERRM);
13:41:19 143  END;
