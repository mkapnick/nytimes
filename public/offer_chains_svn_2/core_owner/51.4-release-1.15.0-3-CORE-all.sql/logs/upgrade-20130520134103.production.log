13:41:20 786  	FROM
13:41:20 787  	  SUBSCRIPTION
13:41:20 788  	WHERE
13:41:20 789  	  SUBSCRIPTION.ID = in_subscription_id;
13:41:20 790  
13:41:20 791  	OPEN out_result_set FOR
13:41:20 792  	SELECT
13:41:20 793  	  INVOICE.ID,
