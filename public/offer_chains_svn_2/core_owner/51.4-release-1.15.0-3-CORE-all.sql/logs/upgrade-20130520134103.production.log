13:41:20 291  PROCEDURE TEST_DELETE_USER_ACCOUNTS  (
13:41:20 292  	in_start_group_id IN NUMBER,
13:41:20 293  	in_end_group_id   IN NUMBER
13:41:20 294  ) IS
13:41:20 295   gid ACCOUNT.GROUP_ID%TYPE;
13:41:20 296   CURSOR c (v_from NUMBER, v_to NUMBER) IS SELECT ACCOUNT.GROUP_ID FROM ACCOUNT WHERE GROUP_ID BETWEEN v_from AND v_to;
13:41:20 297  BEGIN
13:41:20 298  -- arosolovskiy refactoring: call delete_user_account only "COUNT(group_id) WHERE ...." times instead of "in_end_group_id - in_start_group_id" times;
13:41:20 299  	/*
13:41:20 300  	FOR var_group_id IN in_start_group_id..in_end_group_id
13:41:20 301  	LOOP
13:41:20 302  	  TEST_DELETE_USER_ACCOUNT(var_group_id);
13:41:20 303  	END LOOP;*/
13:41:20 304  	OPEN c(in_start_group_id, in_end_group_id);
13:41:20 305  	WHILE c%ISOPEN LOOP
13:41:20 306  	  FETCH c INTO gid;
13:41:20 307  	  IF c%NOTFOUND THEN
13:41:20 308  	   CLOSE c;
13:41:20 309  	  END IF;
13:41:20 310  	  TEST_DELETE_USER_ACCOUNT(gid);
13:41:20 311  	END LOOP;
