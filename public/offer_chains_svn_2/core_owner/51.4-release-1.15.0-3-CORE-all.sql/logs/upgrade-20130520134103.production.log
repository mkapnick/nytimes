13:41:19  72  	  C.CODE NAME,
13:41:19  73  	  C.DESCRIPTION,
13:41:19  74  	  1 INHERITED,
13:41:19  75  	  C.SHAREABLE,
13:41:19  76  	  MAX(LEAST(SS.END_DATE, LIC.ENTITLEMENT_END_DATE)) EXPIRES
13:41:19  77  	FROM
13:41:19  78  	  ACCOUNT BORROWER,
13:41:19  79  	  SUBSCRIPTION S,
13:41:19  80  	  LICENSE LIC,
13:41:19  81  	  OFFER_PRODUCT_OFFERING OPO,
