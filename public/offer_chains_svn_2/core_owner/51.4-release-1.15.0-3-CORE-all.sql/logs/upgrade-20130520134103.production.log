13:41:20 517  
13:41:20 518  PROCEDURE SUSPEND_SUBSCRIPTION(
13:41:20 519  /*
13:41:20 520  Throws exceptions:
13:41:20 521  APP_EXCEPTION_CODES_V21.NOT_FOUND
13:41:20 522  APP_EXCEPTION_CODES_V21.INTERNAL_ERROR
13:41:20 523  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
13:41:20 524  */
13:41:20 525  	  in_subs_id	IN NUMBER,
13:41:20 526  	  in_updated_by IN VARCHAR2
13:41:20 527  ) AS
13:41:20 528  SPROC_NAME		  CONSTANT VARCHAR2(20) := 'SUSPEND_SUBSCRIPTION';
13:41:20 529  var_subscription_status_id  NUMBER;
13:41:20 530  var_license_id		  NUMBER;
13:41:20 531  var_offer_id		  NUMBER;
13:41:20 532  var_license_start_date	  DATE;
13:41:20 533  var_license_end_date	  DATE;
13:41:20 534  
13:41:20 535  var_entitlement_dupration   VARCHAR2(30);
13:41:20 536  var_d_entitlement_dupration NUMBER;
13:41:20 537  
13:41:20 538  var_ym_interval INTERVAL YEAR TO MONTH;
13:41:20 539  var_ds_interval INTERVAL DAY(3) TO SECOND;
13:41:20 540  
13:41:20 541  -- EXCEPTIONS
13:41:20 542  BAD_SUBSCRIPTION_ID     EXCEPTION;
13:41:20 543  BAD_SUBSCRIPTION_STATUS EXCEPTION;
13:41:20 544  NO_LICENSE_FOUND	      EXCEPTION;
13:41:20 545  NO_OFFER_FOUND	      EXCEPTION;
13:41:20 546  EXCEPTION_MESSAGE       VARCHAR2(1024);
13:41:20 547  BEGIN
13:41:20 548  	-- TODO: Finish this prcedure (in Phase II)
13:41:20 549  
13:41:20 550  	-- Get subscription by id. FAULT if no such subscription.
13:41:20 551  	-- begin TX
13:41:20 552  	--   Get for update associated license (subscription.license_id). FAULT if not found.
13:41:20 553  	--   Set status to PROCESSED.
13:41:20 554  	--   updated record.
13:41:20 555  	--   compute days remaining in the subscription: original end_date - today = days_remaining_adjustment
13:41:20 556  	--   new subscription status is SUSPENDED.
13:41:20 557  	--   suspend_date is now.
13:41:20 558  	--   update subscription record.
13:41:20 559  	-- end TX
13:41:20 560  
13:41:20 561  	BEGIN
13:41:20 562  	  SELECT
13:41:20 563  	    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID into var_subscription_status_id
13:41:20 564  	  FROM
13:41:20 565  	    SUBSCRIPTION
13:41:20 566  	  WHERE
13:41:20 567  	    SUBSCRIPTION.ID = in_subs_id;
13:41:20 568  	  EXCEPTION
13:41:20 569  	  WHEN NO_DATA_FOUND THEN
13:41:20 570  	    RAISE BAD_SUBSCRIPTION_ID;
13:41:20 571  	END;
13:41:20 572  
13:41:20 573  	IF var_subscription_status_id != GLOBAL_STATUSES_V21.SUBSCRIPTION_ACTIVE THEN
13:41:20 574  	  RAISE BAD_SUBSCRIPTION_STATUS;
13:41:20 575  	END IF;
13:41:20 576  
13:41:20 577  	BEGIN
13:41:20 578  	  SELECT
13:41:20 579  	    LICENSE.ID,
13:41:20 580  	    LICENSE.OFFER_ID,
13:41:20 581  	    LICENSE.START_DATE
13:41:20 582  	    into
13:41:20 583  	    var_license_id,
13:41:20 584  	    var_offer_id,
13:41:20 585  	    var_license_start_date
13:41:20 586  	  FROM
13:41:20 587  	    LICENSE
13:41:20 588  	  WHERE
13:41:20 589  	    LICENSE.SUBSCRIPTION_ID = in_subs_id
13:41:20 590  	      AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V21.LICENSE_ACTIVE;
13:41:20 591  	  EXCEPTION
13:41:20 592  	  WHEN NO_DATA_FOUND THEN
13:41:20 593  	    RAISE NO_LICENSE_FOUND;
13:41:20 594  	END;
13:41:20 595  
13:41:20 596  	PROCS_LICENSE_CRU_V21.UPDATE_LICENSE(
13:41:20 597  	  in_license_id        => var_license_id,
13:41:20 598  	  in_updated_by        => in_updated_by,
13:41:20 599  	  in_license_status_id => GLOBAL_STATUSES_V21.LICENSE_CLOSED
13:41:20 600  	);
13:41:20 601  
13:41:20 602  	BEGIN
13:41:20 603  	  SELECT
13:41:20 604  	    OFFER.ENTITLEMENT_DURATION into var_entitlement_dupration
13:41:20 605  	  FROM
13:41:20 606  	    OFFER
13:41:20 607  	  WHERE
13:41:20 608  	    OFFER.ID = var_offer_id;
13:41:20 609  	  EXCEPTION
13:41:20 610  	  WHEN NO_DATA_FOUND THEN
13:41:20 611  	    RAISE NO_OFFER_FOUND;
13:41:20 612  	END;
13:41:20 613  
13:41:20 614  	var_ym_interval := substr(var_entitlement_dupration, 0, 4);
13:41:20 615  	var_ds_interval := substr(var_entitlement_dupration, 4);
13:41:20 616  
13:41:20 617  	var_license_end_date := var_license_start_date + var_ym_interval + var_ds_interval;
13:41:20 618  
13:41:20 619  	var_d_entitlement_dupration := var_license_end_date - current_date;
13:41:20 620  
13:41:20 621  	PROCS_SUBSCRIPTION_CRU_V21.UPDATE_SUBSCRIPTION(
13:41:20 622  	  in_subscription_id	       => in_subs_id,
13:41:20 623  	  in_updated_by 	       => in_updated_by,
13:41:20 624  	  in_suspend_date	       => SYSDATE,
13:41:20 625  	  in_subscription_status_id    => GLOBAL_STATUSES_V21.SUBSCRIPTION_SUSPENDED,
13:41:20 626  	  in_days_remainning_ajustment => var_d_entitlement_dupration
13:41:20 627  	);
13:41:20 628  
13:41:20 629  EXCEPTION
13:41:20 630  WHEN BAD_SUBSCRIPTION_ID THEN
13:41:20 631  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 632  	  SPROC_NAME, 'No such subscription');
13:41:20 633  WHEN BAD_SUBSCRIPTION_STATUS THEN
13:41:20 634  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
13:41:20 635  	  SPROC_NAME, 'Subscription is not active');
13:41:20 636  WHEN NO_LICENSE_FOUND THEN
13:41:20 637  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 638  	  SPROC_NAME, 'Can not find license associated with given subscription ID');
13:41:20 639  WHEN NO_OFFER_FOUND THEN
13:41:20 640  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 641  	  SPROC_NAME, 'Can not find offer associated with given subscription ID');
13:41:20 642  WHEN OTHERS THEN
13:41:20 643  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 644  	  SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 645  END SUSPEND_SUBSCRIPTION;
