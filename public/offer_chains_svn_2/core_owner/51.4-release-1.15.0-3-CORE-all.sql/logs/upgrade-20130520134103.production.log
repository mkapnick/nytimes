13:41:20 155  PROCEDURE CALCULATE_INVOICE_AMOUNT (
13:41:20 156  /*
13:41:20 157  Throws exceptions:
13:41:20 158  APP_EXCEPTION_CODES_V21.NOT_FOUND
13:41:20 159  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
13:41:20 160  */
13:41:20 161  	in_invoice_id IN  NUMBER,
13:41:20 162  	out_amount    OUT NUMBER
13:41:20 163  ) AS
13:41:20 164  -- VARIABLES
13:41:20 165  SPROC_NAME	     CONSTANT VARCHAR2(24) := 'CALCULATE_INVOICE_AMOUNT';
13:41:20 166  temp_invoice_id	     NUMBER;
13:41:20 167  var_total_amount	     NUMBER(10,6);
13:41:20 168  var_final_amount	     NUMBER(10,2);
13:41:20 169  var_line_item_amount   NUMBER(10,6);
13:41:20 170  
13:41:20 171  var_line_items_set     SYS_REFCURSOR;
13:41:20 172  var_line_item_id	     NUMBER;
13:41:20 173  var_line_item_quantity NUMBER;
13:41:20 174  var_line_item_price    NUMBER (10,2);
13:41:20 175  
13:41:20 176  var_discount_fixed_amount NUMBER (10,2);
13:41:20 177  var_discount_percent_amount NUMBER (10,2);
13:41:20 178  
13:41:20 179  -- EXCEPTIONS
13:41:20 180  BAD_INVOICE_ID		    EXCEPTION;
13:41:20 181  CAN_NOT_CALC_LINE_ITEM_AMOUNT EXCEPTION;
13:41:20 182  EXCEPTION_MESSAGE VARCHAR2(1024);
13:41:20 183  BEGIN
13:41:20 184  
13:41:20 185  	var_total_amount := 0;
13:41:20 186  
13:41:20 187  	-- Check that given invoice exists
13:41:20 188  	BEGIN
13:41:20 189  	  SELECT
13:41:20 190  	    INVOICE.ID into temp_invoice_id
13:41:20 191  	  FROM
13:41:20 192  	    INVOICE
13:41:20 193  	  WHERE
13:41:20 194  	    INVOICE.ID = in_invoice_id
13:41:20 195  	    AND ROWNUM <= 1;
13:41:20 196  	  EXCEPTION
13:41:20 197  	    WHEN NO_DATA_FOUND THEN
13:41:20 198  	      RAISE BAD_INVOICE_ID;
13:41:20 199  	END;
13:41:20 200  
13:41:20 201  	-- Calculate amount for each line item in invoice
13:41:20 202  	FOR f_line_item IN (
13:41:20 203  	  SELECT
13:41:20 204  	    LINE_ITEM.ID
13:41:20 205  	  FROM
13:41:20 206  	    LINE_ITEM
13:41:20 207  	  WHERE
13:41:20 208  	    LINE_ITEM.INVOICE_ID = in_invoice_id
13:41:20 209  	)
13:41:20 210  	LOOP
13:41:20 211  	  BEGIN
13:41:20 212  	    PROCS_LINE_ITEMS_V21.CALCULATE_LINE_ITEM_AMOUNT(
13:41:20 213  	      in_line_item_id => f_line_item.ID,
13:41:20 214  	      out_amount      => var_line_item_amount
13:41:20 215  	    );
13:41:20 216  	    var_total_amount := var_total_amount + var_line_item_amount;
13:41:20 217  	    EXCEPTION
13:41:20 218  	      WHEN OTHERS THEN
13:41:20 219  		EXCEPTION_MESSAGE := SQLERRM;
13:41:20 220  		RAISE CAN_NOT_CALC_LINE_ITEM_AMOUNT;
13:41:20 221  	  END;
13:41:20 222  	END LOOP;
13:41:20 223  	var_final_amount := var_total_amount;
13:41:20 224  	out_amount := var_final_amount;
13:41:20 225  
13:41:20 226  EXCEPTION
13:41:20 227  WHEN BAD_INVOICE_ID THEN
13:41:20 228  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 229  	  SPROC_NAME, 'No such invoice');
13:41:20 230  WHEN CAN_NOT_CALC_LINE_ITEM_AMOUNT THEN
13:41:20 231  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 232  	  SPROC_NAME, 'Could not calculate line item amount', EXCEPTION_MESSAGE);
13:41:20 233  WHEN OTHERS THEN
13:41:20 234  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 235  	  SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 236  END;
