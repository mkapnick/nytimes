13:41:20 1278  	 out_group_ids OUT SYS_REFCURSOR
13:41:20 1279  ) AS
13:41:20 1280  SPROC_NAME CONSTANT VARCHAR2(27) := 'GET_GROUPS_ID_BY_INVOICE_ID';
13:41:20 1281  -- VARIABLES
13:41:20 1282  temp_invoice_id		 NUMBER;
13:41:20 1283  var_subscrib_group_id	 NUMBER;
13:41:20 1284  var_gc_purchaser_group_id NUMBER;
13:41:20 1285  var_gc_redeemer_group_id  NUMBER;
13:41:20 1286  -- EXCEPTIONS
13:41:20 1287  BAD_INVOICE_ID	    EXCEPTION;
13:41:20 1288  CAN_NOT_FIND_ACCOUNT EXCEPTION;
13:41:20 1289  BEGIN
13:41:20 1290  
13:41:20 1291  	 BEGIN
13:41:20 1292  	   SELECT
13:41:20 1293  	     INVOICE.ID into temp_invoice_id
13:41:20 1294  	   FROM
13:41:20 1295  	     INVOICE
13:41:20 1296  	   WHERE
13:41:20 1297  	     INVOICE.ID = in_invoice_id;
13:41:20 1298  	   EXCEPTION
13:41:20 1299  	     WHEN NO_DATA_FOUND THEN
13:41:20 1300  	       RAISE BAD_INVOICE_ID;
13:41:20 1301  	 END;
13:41:20 1302  
13:41:20 1303  	 BEGIN
13:41:20 1304  	   SELECT DISTINCT
13:41:20 1305  	     ACCOUNT.GROUP_ID into var_subscrib_group_id
13:41:20 1306  	   FROM
13:41:20 1307  	     ACCOUNT
13:41:20 1308  	     INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.ACCOUNT_ID = ACCOUNT.ID
13:41:20 1309  	     INNER JOIN LICENSE ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
13:41:20 1310  	   WHERE
13:41:20 1311  	     LICENSE.INVOICE_ID = in_invoice_id;
13:41:20 1312  	   EXCEPTION
13:41:20 1313  	     WHEN NO_DATA_FOUND THEN
13:41:20 1314  	       var_subscrib_group_id := NULL;
13:41:20 1315  	 END;
13:41:20 1316  
13:41:20 1317  	 IF var_subscrib_group_id IS NULL THEN
13:41:20 1318  	   BEGIN
13:41:20 1319  	     SELECT
13:41:20 1320  	       GIFT_CERTIFICATE.PURCHASER_GROUP_ID,
13:41:20 1321  	       GIFT_CERTIFICATE.REDEEMER_GROUP_ID
13:41:20 1322  	       into
13:41:20 1323  	       var_gc_purchaser_group_id,
13:41:20 1324  	       var_gc_redeemer_group_id
13:41:20 1325  	     FROM
13:41:20 1326  	       GIFT_CERTIFICATE
13:41:20 1327  	     WHERE
13:41:20 1328  	       GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = in_invoice_id
13:41:20 1329  	       OR GIFT_CERTIFICATE.FINALIZED_INVOICE_ID = in_invoice_id;
