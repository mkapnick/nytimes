13:41:20 1813  temp_gift_certificate_id NUMBER;
13:41:20 1814  -- EXCEPTIONS
13:41:20 1815  BAD_GIFT_CERTIFICATE_CODE EXCEPTION;
13:41:20 1816  BEGIN
13:41:20 1817  
13:41:20 1818  	 BEGIN
13:41:20 1819  	   SELECT
13:41:20 1820  	     GIFT_CERTIFICATE.ID into temp_gift_certificate_id
13:41:20 1821  	   FROM
13:41:20 1822  	     GIFT_CERTIFICATE
13:41:20 1823  	   WHERE
13:41:20 1824  	     GIFT_CERTIFICATE.CODE = in_code;
13:41:20 1825  	   EXCEPTION
13:41:20 1826  	     WHEN NO_DATA_FOUND THEN
13:41:20 1827  	       RAISE BAD_GIFT_CERTIFICATE_CODE;
13:41:20 1828  	 END;
13:41:20 1829  
13:41:20 1830  	 -- Select all gift certificates with given code
13:41:20 1831  	 OPEN out_result_set FOR
13:41:20 1832  	 SELECT
13:41:20 1833  	   gc.EXPIRATION_DATE,
13:41:20 1834  	   ch.name,
13:41:20 1835  	   ch.id,
13:41:20 1836  	   gc.sender_email,
13:41:20 1837  	   gc.sender_name,
13:41:20 1838  	   gc.recipient_email,
13:41:20 1839  	   gc.recipient_name,
13:41:20 1840  	   gc.purchase_date,
13:41:20 1841  	   gc.redemption_date,
13:41:20 1842  	   gc.purchaser_group_id,
13:41:20 1843  	   gc.redeemer_group_id,
13:41:20 1844  	   gc.gift_message,
13:41:20 1845  	   ocmd.value redemption_offer_chain_ids,
13:41:20 1846  	   s.offer_chain_id redeemed_offer_chain_id,
13:41:20 1847  	   gc.recipient_notify_date,
13:41:20 1848  	   gc.gift_certificate_status_id,
13:41:20 1849  	   gc.purchase_invoice_id,
13:41:20 1850  	   gc.finalized_invoice_id
13:41:20 1851  	 FROM
13:41:20 1852  	   GIFT_CERTIFICATE gc
13:41:20 1853  	 INNER JOIN OFFER_CHAIN ch ON ch.id = gc.offer_chain_id
13:41:20 1854  	 INNER JOIN OFFER_CHAIN_META_DATA ocmd ON gc.offer_chain_id = ocmd.offer_chain_id AND ocmd.name = 'redemption offer chain id'
13:41:20 1855  	 LEFT JOIN LICENSE l ON l.invoice_id = gc.finalized_invoice_id
13:41:20 1856  	 LEFT JOIN SUBSCRIPTION s ON l.subscription_id = s.id
13:41:20 1857  	 WHERE
13:41:20 1858  	   gc.code = in_code;
13:41:20 1859  
13:41:20 1860  EXCEPTION
13:41:20 1861  WHEN BAD_GIFT_CERTIFICATE_CODE THEN
13:41:20 1862  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 1863  	   SPROC_NAME, 'No such gift certificate');
13:41:20 1864  WHEN OTHERS THEN
13:41:20 1865  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 1866  	   SPROC_NAME, 'Unknown Error', SQLERRM);
13:41:20 1867  END;
