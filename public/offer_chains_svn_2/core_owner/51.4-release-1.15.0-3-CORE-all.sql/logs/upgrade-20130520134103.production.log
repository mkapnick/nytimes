13:41:19  51  	SELECT
13:41:19  52  	  C.CODE NAME,
13:41:19  53  	  C.DESCRIPTION,
13:41:19  54  	  0 INHERITED,
13:41:19  55  	  C.SHAREABLE,
13:41:19  56  	  MAX(LIC.ENTITLEMENT_END_DATE) EXPIRES
13:41:19  57  	FROM
13:41:19  58  	  SUBSCRIPTION SB
13:41:19  59  	  INNER JOIN ACCOUNT AC ON AC.ID = SB.ACCOUNT_ID
13:41:19  60  	  INNER JOIN LICENSE LIC ON LIC.SUBSCRIPTION_ID = SB.ID
13:41:19  61  	  INNER JOIN OFFER_PRODUCT_OFFERING OPO ON OPO.OFFER_ID = LIC.OFFER_ID
13:41:19  62  	  INNER JOIN PRODUCT_OFFERING PO ON PO.ID = OPO.PRODUCT_OFFERING_ID
13:41:19  63  	  INNER JOIN CAPABILITY C ON PO.CAPABILITY_ID = C.ID
13:41:19  64  	WHERE
13:41:19  65  	  LIC.ENTITLEMENT_END_DATE >= TRUNC(SYSDATE)
13:41:19  66  	  AND LIC.START_DATE <= SYSDATE
