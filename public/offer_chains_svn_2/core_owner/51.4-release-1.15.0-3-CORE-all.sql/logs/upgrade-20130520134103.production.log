13:41:20 382  	  SPROC_NAME, 'Can not subscribe to Offer Chain for Gift Certificate');
13:41:20 383  WHEN CAN_NOT_SUBSCRIBE_TO_RGC THEN
13:41:20 384  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
13:41:20 385  	  SPROC_NAME, 'Can not subscribe to Offer Chain that is for redemption');
13:41:20 386  WHEN BAD_OFFER_CHAIN THEN
13:41:20 387  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 388  	  SPROC_NAME, 'No such offer chain');
13:41:20 389  WHEN PRODUCT_ALREADY_PURCHASED THEN
13:41:20 390  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.CONFLICT_ERROR,
13:41:20 391  	  SPROC_NAME, 'User already subscribed to some product in given offer chain. Concurrent subscription id: ' || var_concur_subscription_id);
13:41:20 392  WHEN CAN_NOT_GET_FIRST_OFFER_CHAIN THEN
13:41:20 393  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 394  	  SPROC_NAME, 'Could not find first offer in offer chain', EXCEPTION_MESSAGE);
13:41:20 395  WHEN CAN_NOT_CREATE_INVOICE THEN
13:41:20 396  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 397  	  SPROC_NAME, 'Could not create invoice', EXCEPTION_MESSAGE);
13:41:20 398  WHEN CAN_NOT_CREATE_LINE_ITEMS THEN
13:41:20 399  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 400  	  SPROC_NAME, 'Could not create line items', EXCEPTION_MESSAGE);
13:41:20 401  WHEN CAN_NOT_CREATE_LICENSE THEN
13:41:20 402  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 403  	  SPROC_NAME, 'Could not create new license', EXCEPTION_MESSAGE);
13:41:20 404  WHEN BAD_GROUP_ID THEN
13:41:20 405  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 406  	  SPROC_NAME, 'Bad group id');
13:41:20 407  WHEN CAN_NOT_CREATE_NOTE THEN
13:41:20 408  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
