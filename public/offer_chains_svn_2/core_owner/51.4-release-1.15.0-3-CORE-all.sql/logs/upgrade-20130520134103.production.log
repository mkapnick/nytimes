13:41:20 936  
13:41:20 937  FUNCTION GET_RENEWAL_DATE (
13:41:20 938  /*
13:41:20 939  Throws exceptions:
13:41:20 940  APP_EXCEPTION_CODES_V21.NOT_FOUND
13:41:20 941  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
13:41:20 942  */
13:41:20 943  	in_subscription_id in NUMBER
13:41:20 944  ) RETURN DATE AS
13:41:20 945  -- VARIABLES
13:41:20 946  SPROC_NAME	      CONSTANT VARCHAR2(16) := 'GET_RENEWAL_DATE';
13:41:20 947  var_subscription_status NUMBER;
13:41:20 948  var_licenses_count      NUMBER;
13:41:20 949  var_license_end_date    DATE;
13:41:20 950  var_last_offer_id       NUMBER;
13:41:20 951  var_offer_chain_id      NUMBER;
13:41:20 952  var_last_license_id     NUMBER;
13:41:20 953  var_current_offer_index NUMBER;
13:41:20 954  var_current_offer_recurr_num NUMBER;
13:41:20 955  var_offer_recurr_num    NUMBER;
13:41:20 956  -- EXCEPTIONS
13:41:20 957  BAD_SUBSCRIPTION_ID EXCEPTION;
13:41:20 958  NO_LICENSES_FOUND EXCEPTION;
13:41:20 959  BEGIN
13:41:20 960  
13:41:20 961  	-- Get subscription id and offer chain id
13:41:20 962  	BEGIN
13:41:20 963  	  SELECT
13:41:20 964  	    SUBSCRIPTION.SUBSCRIPTION_STATUS_ID,
13:41:20 965  	    SUBSCRIPTION.OFFER_CHAIN_ID
13:41:20 966  	    into
13:41:20 967  	    var_subscription_status,
13:41:20 968  	    var_offer_chain_id
13:41:20 969  	  FROM
13:41:20 970  	    SUBSCRIPTION
13:41:20 971  	  WHERE
13:41:20 972  	    SUBSCRIPTION.ID = in_subscription_id;
13:41:20 973  	  EXCEPTION
13:41:20 974  	    WHEN NO_DATA_FOUND THEN
13:41:20 975  	      RAISE BAD_SUBSCRIPTION_ID;
13:41:20 976  	END;
13:41:20 977  
13:41:20 978  	IF var_subscription_status != GLOBAL_STATUSES_V21.SUBSCRIPTION_ACTIVE THEN
13:41:20 979  	  -- TODO: Is suspended subscription has renewal date? (For the phase II)
13:41:20 980  	  -- AND var_subscription_status != GLOBAL_STATUSES_V21.SUBSCRIPTION_SUSPENDED THEN
13:41:20 981  	  RETURN NULL;
13:41:20 982  	END IF;
13:41:20 983  
13:41:20 984  	BEGIN
13:41:20 985  	  SELECT
13:41:20 986  	    LICENSE_ID,
13:41:20 987  	    END_DATE,
13:41:20 988  	    OFFER_ID,
13:41:20 989  	    CURRENT_OFFER_INDEX,
13:41:20 990  	    CURRENT_OFFER_RECURR_NUM
13:41:20 991  	    into
13:41:20 992  	    var_last_license_id,
13:41:20 993  	    var_license_end_date,
13:41:20 994  	    var_last_offer_id,
13:41:20 995  	    var_current_offer_index,
13:41:20 996  	    var_current_offer_recurr_num
13:41:20 997  	  FROM
13:41:20 998  	    (
13:41:20 999  	      SELECT
13:41:20 1000  		 LICENSE.ID as "LICENSE_ID",
13:41:20 1001  		 LICENSE.END_DATE,
13:41:20 1002  		 LICENSE.OFFER_ID,
13:41:20 1003  		 LICENSE.CURRENT_OFFER_INDEX,
13:41:20 1004  		 LICENSE.CURRENT_OFFER_RECURR_NUM
13:41:20 1005  	       FROM
13:41:20 1006  		 LICENSE
13:41:20 1007  	       WHERE
13:41:20 1008  		 LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V21.LICENSE_ACTIVE
13:41:20 1009  		 AND LICENSE.SUBSCRIPTION_ID = in_subscription_id
13:41:20 1010  	       ORDER BY END_DATE DESC
13:41:20 1011  	     )
13:41:20 1012  	     INNER JOIN OFFER ON OFFER_ID = OFFER.ID
13:41:20 1013  	   WHERE
13:41:20 1014  	     ROWNUM <= 1;
13:41:20 1015  
13:41:20 1016  	   EXCEPTION
13:41:20 1017  	     WHEN NO_DATA_FOUND THEN
13:41:20 1018  	       -- RAISE NO_LICENSES_FOUND;
13:41:20 1019  	       RETURN NULL;
13:41:20 1020  	 END;
13:41:20 1021  
13:41:20 1022  	 SELECT
13:41:20 1023  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES into var_offer_recurr_num
13:41:20 1024  	 FROM
13:41:20 1025  	   OFFER_OFFER_CHAIN
13:41:20 1026  	 WHERE
13:41:20 1027  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
13:41:20 1028  	   AND OFFER_OFFER_CHAIN.OFFER_ID = var_last_offer_id;
13:41:20 1029  
13:41:20 1030  	 IF PROCS_OFFER_CHAIN_V21.GET_NEXT_OFFER_INDEX(var_offer_chain_id, var_current_offer_index) IS NULL
13:41:20 1031  	   AND var_offer_recurr_num = var_current_offer_recurr_num THEN
13:41:20 1032  	   -- There is no next offer for this subscription
13:41:20 1033  	   RETURN NULL;
13:41:20 1034  	 END IF;
13:41:20 1035  
13:41:20 1036  	 RETURN var_license_end_date;
13:41:20 1037  
13:41:20 1038  EXCEPTION
13:41:20 1039  WHEN BAD_SUBSCRIPTION_ID THEN
13:41:20 1040  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 1041  	   SPROC_NAME, 'No such subscription');
13:41:20 1042  WHEN NO_LICENSES_FOUND THEN
13:41:20 1043  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 1044  	   SPROC_NAME, 'No licenses found');
13:41:20 1045  WHEN OTHERS THEN
13:41:20 1046  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 1047  	   SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 1048  END GET_RENEWAL_DATE;
13:41:20 1049  
13:41:20 1050  /******************************************************************************/
13:41:20 1051  
13:41:20 1052  FUNCTION GET_RECENT_CHARGE (
13:41:20 1053  /*
13:41:20 1054  Throws exceptions:
13:41:20 1055  APP_EXCEPTION_CODES_V21.NOT_FOUND
13:41:20 1056  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
13:41:20 1057  */
13:41:20 1058  	 in_subscription_id IN NUMBER
13:41:20 1059  ) RETURN NUMBER AS
13:41:20 1060  -- VARIABLES
13:41:20 1061  SPROC_NAME	    CONSTANT VARCHAR2(17) := 'GET_RECENT_CHARGE';
13:41:20 1062  temp_subscription_id NUMBER;
13:41:20 1063  var_recent_charge    NUMBER(10,2);
13:41:20 1064  
13:41:20 1065  -- EXCEPTIONS
13:41:20 1066  BAD_SUBSCRIPTION_ID EXCEPTION;
13:41:20 1067  BEGIN
13:41:20 1068  
13:41:20 1069  	 BEGIN
13:41:20 1070  	   SELECT
13:41:20 1071  	     SUBSCRIPTION.ID into temp_subscription_id
13:41:20 1072  	   FROM
13:41:20 1073  	     SUBSCRIPTION
13:41:20 1074  	   WHERE
13:41:20 1075  	     SUBSCRIPTION.ID = in_subscription_id;
13:41:20 1076  	   EXCEPTION
13:41:20 1077  	     WHEN NO_DATA_FOUND THEN
13:41:20 1078  	       RAISE BAD_SUBSCRIPTION_ID;
13:41:20 1079  	 END;
13:41:20 1080  
13:41:20 1081  	 BEGIN
13:41:20 1082  	   SELECT
13:41:20 1083  	     CHARGE.CHARGE_AMOUNT into var_recent_charge
13:41:20 1084  	   FROM
13:41:20 1085  	     LICENSE
13:41:20 1086  	     INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
13:41:20 1087  	     INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
13:41:20 1088  	     INNER JOIN CHARGE ON CHARGE.INVOICE_ID = INVOICE.ID
13:41:20 1089  	   WHERE
13:41:20 1090  	     -- TODO: Review
13:41:20 1091  	     -- LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V21.LICENSE_PROCESSED
13:41:20 1092  	     -- AND
13:41:20 1093  	     SUBSCRIPTION.ID = in_subscription_id
13:41:20 1094  	     AND CHARGE.CHARGE_AMOUNT >= 0
13:41:20 1095  	     AND ROWNUM <= 1
13:41:20 1096  	   ORDER BY
13:41:20 1097  	     LICENSE.ID ASC, CHARGE.ID DESC;
13:41:20 1098  	   EXCEPTION
13:41:20 1099  	     WHEN NO_DATA_FOUND THEN
13:41:20 1100  	       var_recent_charge := 0;
13:41:20 1101  	 END;
13:41:20 1102  
13:41:20 1103  	 RETURN var_recent_charge;
13:41:20 1104  
13:41:20 1105  EXCEPTION
13:41:20 1106  WHEN BAD_SUBSCRIPTION_ID THEN
13:41:20 1107  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 1108  	   SPROC_NAME, 'No such subscription');
13:41:20 1109  WHEN OTHERS THEN
13:41:20 1110  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 1111  	   SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 1112  END GET_RECENT_CHARGE;
13:41:20 1113  
13:41:20 1114  /******************************************************************************/
13:41:20 1115  
13:41:20 1116  FUNCTION GET_BILLING_CYCLE (
13:41:20 1117  	 in_subscription_id IN NUMBER
13:41:20 1118  ) RETURN VARCHAR2 AS
13:41:20 1119  -- VARIABLES
13:41:20 1120  SPROC_NAME	    CONSTANT VARCHAR2(17) := 'GET_BILLING_CYCLE';
13:41:20 1121  temp_subscription_id NUMBER;
13:41:20 1122  var_offer_duration   VARCHAR2(30);
13:41:20 1123  
13:41:20 1124  -- EXCEPTIONS
13:41:20 1125  BAD_SUBSCRIPTION_ID EXCEPTION;
13:41:20 1126  BEGIN
13:41:20 1127  
13:41:20 1128  	 BEGIN
13:41:20 1129  	   SELECT
13:41:20 1130  	     SUBSCRIPTION.ID into temp_subscription_id
13:41:20 1131  	   FROM
13:41:20 1132  	     SUBSCRIPTION
13:41:20 1133  	   WHERE
13:41:20 1134  	     SUBSCRIPTION.ID = in_subscription_id;
13:41:20 1135  	   EXCEPTION
13:41:20 1136  	     WHEN NO_DATA_FOUND THEN
13:41:20 1137  	       RAISE BAD_SUBSCRIPTION_ID;
13:41:20 1138  	 END;
13:41:20 1139  
13:41:20 1140  	 SELECT
13:41:20 1141  	   OFFER.ENTITLEMENT_DURATION into var_offer_duration
13:41:20 1142  	 FROM
13:41:20 1143  	   LICENSE
13:41:20 1144  	   INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
13:41:20 1145  	   INNER JOIN OFFER ON LICENSE.OFFER_ID = OFFER.ID
13:41:20 1146  	 WHERE
13:41:20 1147  	   --LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V21.LICENSE_PROCESSED
13:41:20 1148  	   --AND
13:41:20 1149  	   SUBSCRIPTION.ID = in_subscription_id
13:41:20 1150  	   AND ROWNUM <= 1
13:41:20 1151  	 ORDER BY
13:41:20 1152  	   LICENSE.ID ASC;
13:41:20 1153  
13:41:20 1154  	 RETURN var_offer_duration;
13:41:20 1155  
13:41:20 1156  EXCEPTION
13:41:20 1157  WHEN BAD_SUBSCRIPTION_ID THEN
13:41:20 1158  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 1159  	   SPROC_NAME, 'No such subscription');
13:41:20 1160  WHEN OTHERS THEN
13:41:20 1161  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 1162  	   SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 1163  END GET_BILLING_CYCLE;
13:41:20 1164  
13:41:20 1165  /******************************************************************************/
13:41:20 1166  
13:41:20 1167  PROCEDURE REFUND_SUBSCRIPTION (
13:41:20 1168  /*
13:41:20 1169  Throws exceptions:
13:41:20 1170  APP_EXCEPTION_CODES_V21.NOT_FOUND
13:41:20 1171  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
13:41:20 1172  */
13:41:20 1173  	 in_subscription_id IN NUMBER,
13:41:20 1174  	 in_invoice_id	    IN NUMBER,
13:41:20 1175  	 in_refund_amount   IN NUMBER,
13:41:20 1176  	 in_note	    IN VARCHAR2,
13:41:20 1177  	 in_created_by	    IN VARCHAR2,
13:41:20 1178  	 out_charge_id	    OUT NUMBER
13:41:20 1179  ) AS
13:41:20 1180  -- VARIABLES
13:41:20 1181  SPROC_NAME	      CONSTANT VARCHAR2(19) := 'REFUND_SUBSCRIPTION';
13:41:20 1182  var_invoice_status_id  NUMBER;
13:41:20 1183  var_account_id	      NUMBER;
13:41:20 1184  var_account_status_id  NUMBER;
13:41:20 1185  var_new_transaction_id NUMBER;
13:41:20 1186  var_instrument_type_id NUMBER;
13:41:20 1187  var_instrument_id      NUMBER;
13:41:20 1188  var_new_charge_id      NUMBER;
13:41:20 1189  var_invoice_amount     NUMBER(10,2);
13:41:20 1190  var_refunds_before     NUMBER(10,2);
13:41:20 1191  var_charges_amount     NUMBER(10,2);
13:41:20 1192  -- EXCEPTIONS
13:41:20 1193  BAD_SUBSCRIPTION_ID	     EXCEPTION;
13:41:20 1194  ACCOUNT_IS_FROZEN	     EXCEPTION;
13:41:20 1195  BAD_INVOICE_ID		     EXCEPTION;
13:41:20 1196  CAN_NOT_CREATE_TRANSACTION    EXCEPTION;
13:41:20 1197  CAN_NOT_CREATE_CHARGE	     EXCEPTION;
13:41:20 1198  CAN_NOT_CALC_INVOICE_AMOUNT   EXCEPTION;
13:41:20 1199  REFUND_IS_GREATER_THAN_ANOUNT EXCEPTION;
13:41:20 1200  CAN_NOT_ANNOTATE_SUBSCRIPTION EXCEPTION;
13:41:20 1201  TOT_REF_IS_GREATER_THAN_ANOUNT EXCEPTION;
13:41:20 1202  INVOICE_IS_NOT_CLOSED	     EXCEPTION;
13:41:20 1203  TOT_REF_IS_GRATER_THAN_CHARGES EXCEPTION;
13:41:20 1204  EXCEPTION_MESSAGE	      VARCHAR2(1024);
13:41:20 1205  BEGIN
13:41:20 1206  
13:41:20 1207  	 BEGIN
13:41:20 1208  	   SELECT
13:41:20 1209  	     SUBSCRIPTION.INSTRUMENT_ID,
13:41:20 1210  	     SUBSCRIPTION.INSTRUMENT_TYPE_ID,
13:41:20 1211  	     SUBSCRIPTION.ACCOUNT_ID
13:41:20 1212  	     into
13:41:20 1213  	     var_instrument_id,
13:41:20 1214  	     var_instrument_type_id,
13:41:20 1215  	     var_account_id
13:41:20 1216  	   FROM
13:41:20 1217  	     SUBSCRIPTION
13:41:20 1218  	   WHERE
13:41:20 1219  	     SUBSCRIPTION.ID = in_subscription_id;
13:41:20 1220  	   EXCEPTION
13:41:20 1221  	     WHEN NO_DATA_FOUND THEN
13:41:20 1222  	       RAISE BAD_SUBSCRIPTION_ID;
13:41:20 1223  	 END;
13:41:20 1224  
13:41:20 1225  	 -- Check account status. It should not to be frozen
13:41:20 1226  	 SELECT
13:41:20 1227  	   ACCOUNT.ACCOUNT_STATUS_ID into var_account_status_id
13:41:20 1228  	 FROM
13:41:20 1229  	   ACCOUNT
13:41:20 1230  	 WHERE
13:41:20 1231  	   ACCOUNT.ID = var_account_id;
13:41:20 1232  
13:41:20 1233  	 IF var_account_status_id = GLOBAL_STATUSES_V21.ACCOUNT_FROZEN THEN
13:41:20 1234  	   RAISE ACCOUNT_IS_FROZEN;
13:41:20 1235  	 END IF;
13:41:20 1236  
13:41:20 1237  	 BEGIN
13:41:20 1238  	   SELECT
13:41:20 1239  	     INVOICE.INVOICE_STATUS_ID into var_invoice_status_id
13:41:20 1240  	   FROM
13:41:20 1241  	     INVOICE
13:41:20 1242  	   WHERE
13:41:20 1243  	     INVOICE.ID = in_invoice_id;
13:41:20 1244  	   EXCEPTION
13:41:20 1245  	     WHEN NO_DATA_FOUND THEN
13:41:20 1246  	       RAISE BAD_INVOICE_ID;
13:41:20 1247  	 END;
13:41:20 1248  
13:41:20 1249  	 IF var_invoice_status_id != GLOBAL_STATUSES_V21.INVOICE_CLOSED THEN
13:41:20 1250  	   RAISE INVOICE_IS_NOT_CLOSED;
13:41:20 1251  	 END IF;
13:41:20 1252  
13:41:20 1253  	 BEGIN
13:41:20 1254  	   PROCS_INVOICE_V21.CALCULATE_INVOICE_AMOUNT (
13:41:20 1255  	     in_invoice_id => in_invoice_id,
13:41:20 1256  	     out_amount    => var_invoice_amount
13:41:20 1257  	   );
13:41:20 1258  	   EXCEPTION
13:41:20 1259  	     WHEN OTHERS THEN
13:41:20 1260  	       EXCEPTION_MESSAGE := SQLERRM;
13:41:20 1261  	       RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
13:41:20 1262  	 END;
13:41:20 1263  
13:41:20 1264  	 IF ( in_refund_amount > var_invoice_amount ) THEN
13:41:20 1265  	   RAISE REFUND_IS_GREATER_THAN_ANOUNT;
13:41:20 1266  	 END IF;
13:41:20 1267  
13:41:20 1268  	 SELECT /*+ STAR_TRANSFORMATION */
13:41:20 1269  	   SUM(CHARGE.CHARGE_AMOUNT) into var_refunds_before
13:41:20 1270  	 FROM
13:41:20 1271  	   CHARGE
13:41:20 1272  	 WHERE
13:41:20 1273  	   CHARGE.INVOICE_ID = in_invoice_id
13:41:20 1274  	   AND (
13:41:20 1275  	     CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V21.CHARGE_OPENED
13:41:20 1276  	     OR CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V21.CHARGE_PROCESSED
13:41:20 1277  	   )
13:41:20 1278  	   AND CHARGE.CHARGE_AMOUNT < 0;
13:41:20 1279  
13:41:20 1280  	 -- Refunds are negative
13:41:20 1281  	 var_refunds_before := -var_refunds_before;
13:41:20 1282  
13:41:20 1283  	 var_charges_amount := 0;
13:41:20 1284  
13:41:20 1285  	 FOR f_processed_charges IN (
13:41:20 1286  	   SELECT
13:41:20 1287  	     CHARGE.CHARGE_AMOUNT
13:41:20 1288  	   FROM
13:41:20 1289  	     CHARGE
13:41:20 1290  	   WHERE
13:41:20 1291  	     CHARGE.INVOICE_ID = in_invoice_id
13:41:20 1292  	     AND CHARGE.CHARGE_STATUS_ID IN (SELECT GLOBAL_STATUSES_V21.CHARGE_PROCESSED FROM DUAL)
13:41:20 1293  	 )
13:41:20 1294  	 LOOP
13:41:20 1295  	   IF f_processed_charges.CHARGE_AMOUNT > 0 THEN
13:41:20 1296  	     var_charges_amount := var_charges_amount + f_processed_charges.CHARGE_AMOUNT;
13:41:20 1297  	   END IF;
13:41:20 1298  	 END LOOP;
13:41:20 1299  
13:41:20 1300  	 IF (in_refund_amount + var_refunds_before > var_invoice_amount) THEN
13:41:20 1301  	   RAISE TOT_REF_IS_GREATER_THAN_ANOUNT;
13:41:20 1302  	 END IF;
13:41:20 1303  
13:41:20 1304  	 IF (in_refund_amount + var_refunds_before > var_charges_amount) THEN
13:41:20 1305  	   RAISE TOT_REF_IS_GRATER_THAN_CHARGES;
13:41:20 1306  	 END IF;
13:41:20 1307  
13:41:20 1308  	 BEGIN
13:41:20 1309  	   PROCS_TRANSACTION_V21.CREATE_TRANSACTION(
13:41:20 1310  	     in_transaction_id	       => NULL,
13:41:20 1311  	     in_status_id	       => GLOBAL_STATUSES_V21.TRANSACTION_PREPARE,
13:41:20 1312  	     in_amount		       => -in_refund_amount,
13:41:20 1313  	     in_created_by	       => in_created_by,
13:41:20 1314  	     in_order_id	       => NULL,
13:41:20 1315  	     in_is_refund	       => GLOBAL_CONSTANTS_V21.TRUE,
13:41:20 1316  	     in_transaction_type_code  => 'REFUND',
13:41:20 1317  	     out_transaction_id        => var_new_transaction_id
13:41:20 1318  	   );
13:41:20 1319  	   EXCEPTION
13:41:20 1320  	     WHEN OTHERS THEN
13:41:20 1321  	       EXCEPTION_MESSAGE := SQLERRM;
13:41:20 1322  	       RAISE CAN_NOT_CREATE_TRANSACTION;
13:41:20 1323  	 END;
13:41:20 1324  
13:41:20 1325  	 BEGIN
13:41:20 1326  	   PROCS_CHARGE_V21.CREATE_CHARGE(
13:41:20 1327  	     in_invoice_id	   => in_invoice_id,
13:41:20 1328  	     in_transaction_id	   => var_new_transaction_id,
13:41:20 1329  	     in_instrument_type_id => var_instrument_type_id,
13:41:20 1330  	     in_instrument_id	   => var_instrument_id,
13:41:20 1331  	     in_charge_amount	   => -in_refund_amount,
13:41:20 1332  	     in_created_by	   => in_created_by,
13:41:20 1333  	     in_charge_status_id   => GLOBAL_STATUSES_V21.CHARGE_OPENED,
13:41:20 1334  	     out_charge_id	   => var_new_charge_id
13:41:20 1335  	   );
13:41:20 1336  	   EXCEPTION
13:41:20 1337  	     WHEN OTHERS THEN
13:41:20 1338  	       EXCEPTION_MESSAGE := SQLERRM;
13:41:20 1339  	       RAISE CAN_NOT_CREATE_CHARGE;
13:41:20 1340  	 END;
13:41:20 1341  
13:41:20 1342  	 IF in_note IS NOT NULL THEN
13:41:20 1343  	   BEGIN
13:41:20 1344  	     PROCS_SUBSCRIPTION_V21.ANNOTATE_SUBSCRIPTION(
13:41:20 1345  	       in_subscription_id => in_subscription_id,
13:41:20 1346  	       in_agent_id	  => 0, -- AGENT_ID??
13:41:20 1347  	       in_note		  => in_note,
13:41:20 1348  	       in_created_by	  => in_created_by
13:41:20 1349  	     );
13:41:20 1350  	     EXCEPTION
13:41:20 1351  	       WHEN OTHERS THEN
13:41:20 1352  		 EXCEPTION_MESSAGE := SQLERRM;
13:41:20 1353  		 RAISE CAN_NOT_ANNOTATE_SUBSCRIPTION;
13:41:20 1354  	   END;
13:41:20 1355  	 END IF;
13:41:20 1356  
13:41:20 1357  	 out_charge_id := var_new_charge_id;
13:41:20 1358  
13:41:20 1359  EXCEPTION
13:41:20 1360  WHEN INVOICE_IS_NOT_CLOSED THEN
13:41:20 1361  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
13:41:20 1362  	   SPROC_NAME, 'Invoice is not closed');
13:41:20 1363  WHEN BAD_SUBSCRIPTION_ID THEN
13:41:20 1364  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 1365  	   SPROC_NAME, 'No such subscription');
13:41:20 1366  WHEN ACCOUNT_IS_FROZEN THEN
13:41:20 1367  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
13:41:20 1368  	   SPROC_NAME, 'Could not refund subscription for frozen account');
13:41:20 1369  WHEN BAD_INVOICE_ID THEN
13:41:20 1370  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 1371  	   SPROC_NAME, 'No such invoice');
13:41:20 1372  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
13:41:20 1373  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 1374  	   SPROC_NAME, 'Could not calculate invoice amount', EXCEPTION_MESSAGE);
13:41:20 1375  WHEN REFUND_IS_GREATER_THAN_ANOUNT THEN
13:41:20 1376  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INVALID_PARAMETER,
13:41:20 1377  	   SPROC_NAME, 'Refund is greater than amount');
13:41:20 1378  WHEN TOT_REF_IS_GREATER_THAN_ANOUNT THEN
13:41:20 1379  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INVALID_PARAMETER,
13:41:20 1380  	   SPROC_NAME, 'There were refunds before and sum of all refunds and new refund more than invoice amount');
13:41:20 1381  WHEN TOT_REF_IS_GRATER_THAN_CHARGES THEN
13:41:20 1382  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INVALID_PARAMETER,
13:41:20 1383  	   SPROC_NAME, 'Total refund amount is greater than sum of processed charges');
13:41:20 1384  WHEN CAN_NOT_CREATE_TRANSACTION THEN
13:41:20 1385  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 1386  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
13:41:20 1387  WHEN CAN_NOT_CREATE_CHARGE THEN
13:41:20 1388  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 1389  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
13:41:20 1390  WHEN CAN_NOT_ANNOTATE_SUBSCRIPTION THEN
13:41:20 1391  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 1392  	   SPROC_NAME, 'Could not annotate subscription', EXCEPTION_MESSAGE);
13:41:20 1393  WHEN OTHERS THEN
13:41:20 1394  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 1395  	   SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 1396  END REFUND_SUBSCRIPTION;
13:41:20 1397  
13:41:20 1398  /******************************************************************************/
13:41:20 1399  
13:41:20 1400  PROCEDURE ADD_SUBSCRIPTION_EXTENSION (
13:41:20 1401  /*
13:41:20 1402  APP_EXCEPTION_CODES_V21.NOT_FOUND
13:41:20 1403  APP_EXCEPTION_CODES_V21.INTERNAL_ERROR
13:41:20 1404  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
13:41:20 1405  */
13:41:20 1406  	 in_subscription_id	 IN NUMBER,
13:41:20 1407  	 in_effective_start_date IN DATE,
13:41:20 1408  	 in_effective_end_date	 IN DATE,
13:41:20 1409  	 in_note		 IN VARCHAR2,
13:41:20 1410  	 in_updated_by		 IN VARCHAR2
13:41:20 1411  ) AS
13:41:20 1412  -- VARIABLES
13:41:20 1413  SPROC_NAME		    CONSTANT VARCHAR2(26) := 'ADD_SUBSCRIPTION_EXTENSION';
13:41:20 1414  temp_subscription_id	    NUMBER;
13:41:20 1415  var_current_license_id	    NUMBER;
13:41:20 1416  var_current_license_start_date DATE;
13:41:20 1417  var_current_license_end_date DATE;
13:41:20 1418  var_current_offer_id	    NUMBER;
13:41:20 1419  var_current_invoice_id	    NUMBER;
13:41:20 1420  var_current_date 	    DATE;
13:41:20 1421  var_current_offer_index	    NUMBER;
13:41:20 1422  var_current_offer_recurr_num NUMBER;
13:41:20 1423  var_account_tax_exempt_id    VARCHAR2(255);
13:41:20 1424  
13:41:20 1425  var_free_invoice_id NUMBER;
13:41:20 1426  var_free_license_id NUMBER;
13:41:20 1427  var_new_license_id  NUMBER;
13:41:20 1428  var_ext_license_id  NUMBER;
13:41:20 1429  -- EXCEPTIONS
13:41:20 1430  BAD_SUBSCRIPTION_ID	     EXCEPTION;
13:41:20 1431  CAN_NOT_FIND_OFFER_OR_LICENSE EXCEPTION;
13:41:20 1432  CAN_NOT_CHANGE_LICENSE_STATUS EXCEPTION;
13:41:20 1433  CAN_NOT_CREATE_INVOICE	     EXCEPTION;
13:41:20 1434  CAN_NOT_CREATE_NEW_LICENSE    EXCEPTION;
13:41:20 1435  CAN_NOT_CREATE_END_LICENSE    EXCEPTION;
13:41:20 1436  CAN_NOT_ANNOTATE_SUBSCRIPTION EXCEPTION;
13:41:20 1437  EXTENS_START_DATE_IS_TOO_FAR  EXCEPTION;
13:41:20 1438  EXT_START_DATE_LATER_THEN_END EXCEPTION;
13:41:20 1439  EXTENS_START_DATE_IS_TOO_SMALL EXCEPTION;
13:41:20 1440  EXCEPTION_MESSAGE	     VARCHAR2(1024);
13:41:20 1441  BEGIN
13:41:20 1442  
13:41:20 1443  	 var_current_date := PROCS_COMMON_V21.NORMALIZE_DATE(SYSDATE);
13:41:20 1444  
13:41:20 1445  	 -- Check that subscription exists
13:41:20 1446  	 BEGIN
13:41:20 1447  	   SELECT
13:41:20 1448  	     SUBSCRIPTION.ID into temp_subscription_id
13:41:20 1449  	   FROM
13:41:20 1450  	     SUBSCRIPTION
13:41:20 1451  	   WHERE
13:41:20 1452  	     SUBSCRIPTION.ID = in_subscription_id;
13:41:20 1453  	   EXCEPTION
13:41:20 1454  	     WHEN NO_DATA_FOUND THEN
13:41:20 1455  	       RAISE BAD_SUBSCRIPTION_ID;
13:41:20 1456  	 END;
13:41:20 1457  
13:41:20 1458  	 -- Get account tax exempt id
13:41:20 1459  	 SELECT DISTINCT
13:41:20 1460  	   ACCOUNT.TAX_EXEMPT_ID into var_account_tax_exempt_id
13:41:20 1461  	 FROM
13:41:20 1462  	   ACCOUNT
13:41:20 1463  	   INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.ACCOUNT_ID = ACCOUNT.ID
13:41:20 1464  	 WHERE
13:41:20 1465  	   SUBSCRIPTION.ID = in_subscription_id;
13:41:20 1466  
13:41:20 1467  	 -- Select current data
13:41:20 1468  	 BEGIN
13:41:20 1469  	   SELECT
13:41:20 1470  	     LICENSE.ID,
13:41:20 1471  	     LICENSE.START_DATE,
13:41:20 1472  	     LICENSE.END_DATE,
13:41:20 1473  	     LICENSE.CURRENT_OFFER_INDEX,
13:41:20 1474  	     LICENSE.CURRENT_OFFER_RECURR_NUM,
13:41:20 1475  	     OFFER.ID,
13:41:20 1476  	     INVOICE.ID
13:41:20 1477  	   INTO
13:41:20 1478  	     var_current_license_id,
13:41:20 1479  	     var_current_license_start_date,
13:41:20 1480  	     var_current_license_end_date,
13:41:20 1481  	     var_current_offer_index,
13:41:20 1482  	     var_current_offer_recurr_num,
13:41:20 1483  	     var_current_offer_id,
13:41:20 1484  	     var_current_invoice_id
13:41:20 1485  	   FROM
13:41:20 1486  	     LICENSE
13:41:20 1487  	     INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
13:41:20 1488  	     INNER JOIN OFFER ON LICENSE.OFFER_ID = OFFER.ID
13:41:20 1489  	     INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
13:41:20 1490  	   WHERE
13:41:20 1491  	     SUBSCRIPTION.ID = in_subscription_id
13:41:20 1492  	     AND PROCS_COMMON_V21.NORMALIZE_DATE(LICENSE.END_DATE) > var_current_date
13:41:20 1493  	     AND PROCS_COMMON_V21.NORMALIZE_DATE(LICENSE.START_DATE) <= var_current_date
13:41:20 1494  	     AND ROWNUM <= 1
13:41:20 1495  	   ORDER BY
13:41:20 1496  	     LICENSE.ID DESC;
13:41:20 1497  	   EXCEPTION
13:41:20 1498  	     WHEN NO_DATA_FOUND THEN
13:41:20 1499  	       RAISE CAN_NOT_FIND_OFFER_OR_LICENSE;
13:41:20 1500  	 END;
13:41:20 1501  
13:41:20 1502  	 IF var_current_license_end_date < in_effective_start_date THEN
13:41:20 1503  	   RAISE EXTENS_START_DATE_IS_TOO_FAR;
13:41:20 1504  	 END IF;
13:41:20 1505  
13:41:20 1506  	 IF var_current_license_start_date > in_effective_start_date THEN
13:41:20 1507  	   RAISE EXTENS_START_DATE_IS_TOO_SMALL;
13:41:20 1508  	 END IF;
13:41:20 1509  
13:41:20 1510  	 IF in_effective_start_date > in_effective_end_date THEN
13:41:20 1511  	   RAISE EXT_START_DATE_LATER_THEN_END;
13:41:20 1512  	 END IF;
13:41:20 1513  
13:41:20 1514  	 -- Closing curent license
13:41:20 1515  	 BEGIN
13:41:20 1516  	   PROCS_LICENSE_CRU_V21.UPDATE_LICENSE(
13:41:20 1517  	     in_license_id	   => var_current_license_id,
