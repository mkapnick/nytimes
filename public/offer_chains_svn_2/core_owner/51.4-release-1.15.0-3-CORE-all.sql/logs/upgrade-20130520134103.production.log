13:41:18  60  				       UPDATED_BY
13:41:18  61  				       ) VALUES (
13:41:18  62  				       CC_UPDATE_SEQ.NEXTVAL,
13:41:18  63  				       record.CREDIT_CARD_ID,
13:41:18  64  				       record.LICENSE_ID,
13:41:18  65  				       'NOT_ADDED_TO_FILE',
13:41:18  66  				       var_request_file_id,
13:41:18  67  				       var_start_date,
13:41:18  68  				       var_start_date,
13:41:18  69  				       in_creator,
13:41:18  70  				       in_creator
13:41:18  71  				       );
13:41:18  72  	     END IF;
13:41:18  73  	   END LOOP;
13:41:18  74  
13:41:18  75  	   SELECT COUNT(1) INTO var_cc_update_count
13:41:18  76  	   FROM CC_UPDATE
13:41:18  77  	   WHERE CC_REQUEST_FILE_ID = var_request_file_id;
13:41:18  78  	   IF var_cc_update_count <= 0 THEN
13:41:18  79  	     UPDATE CC_REQUEST_FILE
13:41:18  80  	     SET CC_REQUEST_FILE_STATUS = 'EMPTY'
13:41:18  81  	     WHERE ID = var_request_file_id;
13:41:18  82  	   END IF;
13:41:18  83  
13:41:18  84  	END POPULATE_REQUEST_INFO;
