13:41:20 122  PROCEDURE GET_PENDING_REFUND_CHARGES (
13:41:20 123  /*
13:41:20 124  Throws exceptions:
13:41:20 125  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
13:41:20 126  */
13:41:20 127  	out_result_set	    OUT SYS_REFCURSOR,
13:41:20 128  	in_row_number	    IN NUMBER DEFAULT NULL
13:41:20 129  ) AS
13:41:20 130  SPROC_NAME CONSTANT VARCHAR2(26) := 'GET_PENDING_REFUND_CHARGES';
13:41:20 131  -- COMSTANTS
13:41:20 132  DEFAULT_ROW_NUMBER CONSTANT NUMBER := 1;
13:41:20 133  -- VARIABLES
13:41:20 134  var_row_number NUMBER;
13:41:20 135  BEGIN
13:41:20 136  	IF in_row_number IS NULL THEN
13:41:20 137  	  var_row_number := DEFAULT_ROW_NUMBER;
13:41:20 138  	ELSE
13:41:20 139  	  var_row_number := in_row_number;
13:41:20 140  	END IF;
13:41:20 141  
13:41:20 142  	-- Select charges
13:41:20 143  	OPEN out_result_set FOR
13:41:20 144  SELECT * FROM
13:41:20 145  (
13:41:20 146  	SELECT
13:41:20 147  	  CHARGE.ID,
13:41:20 148  	  CHARGE.TRANSACTION_ID,
13:41:20 149  	  CHARGE.INSTRUMENT_ID,
13:41:20 150  	  CHARGE.INSTRUMENT_TYPE_ID,
13:41:20 151  	  CHARGE.CHARGE_AMOUNT,
13:41:20 152  	  CHARGE.CREATE_DATE,
13:41:20 153  	  CHARGE.CREATED_BY,
13:41:20 154  	  CHARGE.INVOICE_ID
13:41:20 155  	FROM
13:41:20 156  	  CHARGE
13:41:20 157  	  INNER JOIN TRANSACTION ON CHARGE.TRANSACTION_ID = TRANSACTION.ID
13:41:20 158  	WHERE
13:41:20 159  	  TRANSACTION.TRANSACTION_STATUS_ID = GLOBAL_STATUSES_V21.TRANSACTION_PENDING
13:41:20 160  	  AND TRANSACTION.IS_REFUND = GLOBAL_CONSTANTS_V21.TRUE
13:41:20 161  	  AND TRANSACTION.TRANSACTION_AMOUNT < 0
13:41:20 162  	  AND NOT EXISTS
13:41:20 163  	  (
13:41:20 164  	    SELECT NULL
13:41:20 165  	    FROM PROCESS_RETRY_THROTTLE
13:41:20 166  	    WHERE PROCESS_NAME = SPROC_NAME
13:41:20 167  	      AND GENERIC_ID = CHARGE.ID
13:41:20 168  	  )
13:41:20 169  	  AND ROWNUM <= var_row_number*10
13:41:20 170  	  ORDER BY dbms_random.value
13:41:20 171  ) WHERE
13:41:20 172  	  ROWNUM <= var_row_number;
13:41:20 173  
13:41:20 174  END GET_PENDING_REFUND_CHARGES;
