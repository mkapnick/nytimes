13:41:20 1546  	     in_status_id		 => GLOBAL_STATUSES_V21.LICENSE_ACTIVE,
13:41:20 1547  	     in_needs_entitlements	 => GLOBAL_CONSTANTS_V21.TRUE,
13:41:20 1548  	     in_start_date		 => in_effective_start_date,
13:41:20 1549  	     in_end_date		 => in_effective_end_date,
13:41:20 1550  	     in_offer_id		 => var_current_offer_id,
13:41:20 1551  	     in_subscription_id 	 => in_subscription_id,
13:41:20 1552  	     in_invoice_id		 => var_free_invoice_id,
13:41:20 1553  	     in_created_by		 => in_updated_by,
13:41:20 1554  	     in_is_extension		 => GLOBAL_CONSTANTS_V21.TRUE,
13:41:20 1555  	     in_current_offer_index	 => var_current_offer_index,
