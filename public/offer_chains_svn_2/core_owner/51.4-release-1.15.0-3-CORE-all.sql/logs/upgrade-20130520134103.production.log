13:41:20 1452  GIFT_CERT_IS_FINALIZED	      EXCEPTION;
13:41:20 1453  GIFT_CERT_IS_REFUNDED	      EXCEPTION;
13:41:20 1454  CAN_NOT_UPDATE_CERTIFICATE     EXCEPTION;
13:41:20 1455  GIFT_CERTIFICATE_EXPIRED       EXCEPTION;
13:41:20 1456  GIFT_CERTIFICATE_REDEEMED      EXCEPTION;
13:41:20 1457  USER_ALREADY_SUBSCRIBED_TO_PRD EXCEPTION;
13:41:20 1458  LIMIT_REACHED		      EXCEPTION;
13:41:20 1459  GC_PURCHASE_INVOICE_NOT_CLOSED EXCEPTION;
13:41:20 1460  PURCHASE_INVOICES_NOT_PAID     EXCEPTION;
13:41:20 1461  OC_TO_REDEEM_NOT_FOUND	      EXCEPTION;
13:41:20 1462  CAN_NOT_GET_FIRST_OFFER_CHAIN  EXCEPTION;
13:41:20 1463  EXCEPTION_MESSAGE	      VARCHAR2(1024);
13:41:20 1464  BEGIN
13:41:20 1465  
13:41:20 1466  	 -- Get account id
13:41:20 1467  	 BEGIN
13:41:20 1468  	   SELECT
13:41:20 1469  	     ACCOUNT.ID,
