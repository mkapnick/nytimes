13:41:20 1641  
13:41:20 1642  FUNCTION CALC_SUBSCRIPTION_END_DATE (
13:41:20 1643  /*
13:41:20 1644  APP_EXCEPTION_CODES_V21.NOT_FOUND
13:41:20 1645  APP_EXCEPTION_CODES_V21.INTERNAL_ERROR
13:41:20 1646  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
13:41:20 1647  Returns:
13:41:20 1648  NULL if it is impossible to calculate end date (for example,
13:41:20 1649  	 offer chain includes offer with infinity recurrences number)
13:41:20 1650  DATE else
13:41:20 1651  */
13:41:20 1652  	 in_subscription_id IN NUMBER
13:41:20 1653  ) RETURN DATE AS
13:41:20 1654  -- VARIABLES
13:41:20 1655  SPROC_NAME		     CONSTANT VARCHAR2(26) := 'CALC_SUBSCRIPTION_END_DATE';
13:41:20 1656  last_license_id		     NUMBER;
13:41:20 1657  last_license_end_date	     DATE;
13:41:20 1658  last_license_offer_id	     NUMBER;
13:41:20 1659  last_license_offer_index      NUMBER;
13:41:20 1660  last_license_offer_recurr_num NUMBER;
13:41:20 1661  var_last_license_id	     NUMBER;
13:41:20 1662  var_offer_chain_id	     NUMBER;
13:41:20 1663  var_current_offer_rec_number  NUMBER;
13:41:20 1664  var_next_offers_set	     SYS_REFCURSOR;
13:41:20 1665  var_next_offer_duration	     VARCHAR2(30);
13:41:20 1666  var_next_offer_recur	     NUMBER;
13:41:20 1667  var_infinity_offers_number    NUMBER;
13:41:20 1668  
13:41:20 1669  var_result_date DATE;
13:41:20 1670  
13:41:20 1671  var_ym_interval	INTERVAL YEAR TO MONTH;
13:41:20 1672  var_ds_interval	INTERVAL DAY(3) TO SECOND;
13:41:20 1673  var_offer_years	NUMBER;
13:41:20 1674  var_offer_months NUMBER;
13:41:20 1675  var_offer_days	NUMBER;
13:41:20 1676  
13:41:20 1677  -- EXCEPTIONS
13:41:20 1678  BAD_SUBSCRIPTION_ID	  EXCEPTION;
13:41:20 1679  CAN_NOT_FIND_LAST_LICENSE  EXCEPTION;
13:41:20 1680  CAN_NOT_CALC_OFFER_LENGTH  EXCEPTION;
13:41:20 1681  CAN_NOT_CALC_OFFER_LENGTH2 EXCEPTION;
13:41:20 1682  EXCEPTION_MESSAGE	  VARCHAR2(1024);
13:41:20 1683  BEGIN
13:41:20 1684  
13:41:20 1685  	 BEGIN
13:41:20 1686  	   SELECT
13:41:20 1687  	     SUBSCRIPTION.OFFER_CHAIN_ID into var_offer_chain_id
13:41:20 1688  	   FROM
13:41:20 1689  	     SUBSCRIPTION
13:41:20 1690  	   WHERE
13:41:20 1691  	     SUBSCRIPTION.ID = in_subscription_id;
13:41:20 1692  	   EXCEPTION
13:41:20 1693  	     WHEN NO_DATA_FOUND THEN
13:41:20 1694  	       RAISE BAD_SUBSCRIPTION_ID;
13:41:20 1695  	 END;
13:41:20 1696  
13:41:20 1697  	 BEGIN
13:41:20 1698  	   SELECT
13:41:20 1699  	     ID into var_last_license_id
13:41:20 1700  	   FROM
13:41:20 1701  	     (
13:41:20 1702  	       SELECT
13:41:20 1703  		 LICENSE.ID
13:41:20 1704  	       FROM
13:41:20 1705  		 LICENSE
13:41:20 1706  	       WHERE
13:41:20 1707  		 LICENSE.SUBSCRIPTION_ID = in_subscription_id
13:41:20 1708  	       ORDER BY
13:41:20 1709  		 LICENSE.END_DATE DESC
13:41:20 1710  	     )
13:41:20 1711  	   WHERE
13:41:20 1712  	     ROWNUM <= 1;
13:41:20 1713  	   EXCEPTION
13:41:20 1714  	     WHEN NO_DATA_FOUND THEN
13:41:20 1715  	       RAISE CAN_NOT_FIND_LAST_LICENSE;
13:41:20 1716  	 END;
13:41:20 1717  
13:41:20 1718  	 SELECT
13:41:20 1719  	   COUNT(*) into var_infinity_offers_number
13:41:20 1720  	 FROM
13:41:20 1721  	   OFFER_OFFER_CHAIN
13:41:20 1722  	 WHERE
13:41:20 1723  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
13:41:20 1724  	   AND OFFER_OFFER_CHAIN.NUM_RECURRENCES = GLOBAL_ENUMS_V21.OFFER_REC_INFINITY;
13:41:20 1725  
13:41:20 1726  	 IF var_infinity_offers_number > 0 THEN
13:41:20 1727  	   RETURN NULL;
13:41:20 1728  	 END IF;
13:41:20 1729  
13:41:20 1730  	 BEGIN
13:41:20 1731  	   SELECT
13:41:20 1732  	     LICENSE.ID,
13:41:20 1733  	     LICENSE.END_DATE,
13:41:20 1734  	     LICENSE.CURRENT_OFFER_INDEX,
13:41:20 1735  	     LICENSE.CURRENT_OFFER_RECURR_NUM,
13:41:20 1736  	     LICENSE.OFFER_ID
13:41:20 1737  	     into
13:41:20 1738  	     last_license_id,
13:41:20 1739  	     last_license_end_date,
13:41:20 1740  	     last_license_offer_index,
13:41:20 1741  	     last_license_offer_recurr_num,
13:41:20 1742  	     last_license_offer_id
13:41:20 1743  	   FROM
13:41:20 1744  	     LICENSE
13:41:20 1745  	   WHERE
13:41:20 1746  	     LICENSE.ID = var_last_license_id;
13:41:20 1747  	   EXCEPTION
13:41:20 1748  	     WHEN NO_DATA_FOUND THEN
13:41:20 1749  	       RAISE CAN_NOT_FIND_LAST_LICENSE;
13:41:20 1750  	 END;
13:41:20 1751  
13:41:20 1752  	 var_result_date := last_license_end_date;
13:41:20 1753  
13:41:20 1754  	 -- Find current recurrence number
13:41:20 1755  	 SELECT
13:41:20 1756  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES into var_current_offer_rec_number
13:41:20 1757  	 FROM
13:41:20 1758  	   OFFER_OFFER_CHAIN
13:41:20 1759  	 WHERE
13:41:20 1760  	   OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
13:41:20 1761  	   AND OFFER_OFFER_CHAIN.OFFER_ID = last_license_offer_id
13:41:20 1762  	   AND OFFER_OFFER_CHAIN.INDEX_VALUE = last_license_offer_index;
13:41:20 1763  
13:41:20 1764  	 IF var_current_offer_rec_number > last_license_offer_recurr_num THEN
13:41:20 1765  	   BEGIN
13:41:20 1766  	     PROCS_OFFER_CHAIN_V21.GET_OFFER_LENGTH(
13:41:20 1767  	       last_license_offer_id,
13:41:20 1768  	       var_offer_years,
13:41:20 1769  	       var_offer_months,
13:41:20 1770  	       var_offer_days
13:41:20 1771  	     );
13:41:20 1772  
13:41:20 1773  	     var_ym_interval := var_offer_years||'-'||var_offer_months;
13:41:20 1774  	     var_ds_interval := var_offer_days||' 0:0:0';
13:41:20 1775  
13:41:20 1776  	     var_result_date := var_result_date
13:41:20 1777  	       + ( var_ym_interval * ( var_current_offer_rec_number - last_license_offer_recurr_num ) )
13:41:20 1778  	       + ( var_ds_interval * ( var_current_offer_rec_number - last_license_offer_recurr_num ) );
13:41:20 1779  	     EXCEPTION
13:41:20 1780  	       WHEN OTHERS THEN
13:41:20 1781  		 EXCEPTION_MESSAGE := SQLERRM;
13:41:20 1782  		 RAISE CAN_NOT_CALC_OFFER_LENGTH;
13:41:20 1783  	   END;
13:41:20 1784  	 END IF;
13:41:20 1785  
13:41:20 1786  	 OPEN var_next_offers_set FOR
13:41:20 1787  	 SELECT
13:41:20 1788  	   OFFER.ENTITLEMENT_DURATION,
13:41:20 1789  	   OFFER_OFFER_CHAIN.NUM_RECURRENCES
13:41:20 1790  	 FROM
13:41:20 1791  	   OFFER_OFFER_CHAIN
13:41:20 1792  	   INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
13:41:20 1793  	 WHERE
13:41:20 1794  	   OFFER_OFFER_CHAIN.INDEX_VALUE > last_license_offer_index
13:41:20 1795  	   AND OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id;
13:41:20 1796  
13:41:20 1797  	 LOOP
13:41:20 1798  	   FETCH var_next_offers_set into var_next_offer_duration, var_next_offer_recur;
13:41:20 1799  	   EXIT WHEN var_next_offers_set%NOTFOUND;
13:41:20 1800  	   BEGIN
13:41:20 1801  	     PROCS_COMMON_V21.ISO8601DURATION_TO_INTERVALS(
13:41:20 1802  	       var_next_offer_duration,
13:41:20 1803  	       var_offer_years,
13:41:20 1804  	       var_offer_months,
13:41:20 1805  	       var_offer_days
13:41:20 1806  	     );
13:41:20 1807  
13:41:20 1808  	     var_ym_interval := var_offer_years||'-'||var_offer_months;
13:41:20 1809  	     var_ds_interval := var_offer_days||' 0:0:0';
13:41:20 1810  
13:41:20 1811  	     var_result_date := var_result_date
13:41:20 1812  	       + ( var_ym_interval * var_next_offer_recur )
13:41:20 1813  	       + ( var_ds_interval * var_next_offer_recur );
13:41:20 1814  	     EXCEPTION
13:41:20 1815  	       WHEN OTHERS THEN
13:41:20 1816  		 EXCEPTION_MESSAGE := SQLERRM;
13:41:20 1817  		 RAISE CAN_NOT_CALC_OFFER_LENGTH2;
13:41:20 1818  	   END;
13:41:20 1819  	 END LOOP;
13:41:20 1820  
13:41:20 1821  	 RETURN var_result_date;
13:41:20 1822  
13:41:20 1823  EXCEPTION
13:41:20 1824  WHEN BAD_SUBSCRIPTION_ID THEN
13:41:20 1825  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 1826  	   SPROC_NAME, 'No such subscription');
13:41:20 1827  WHEN CAN_NOT_FIND_LAST_LICENSE THEN
13:41:20 1828  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 1829  	   SPROC_NAME, 'Could not find last license for given subscription');
13:41:20 1830  WHEN CAN_NOT_CALC_OFFER_LENGTH THEN
13:41:20 1831  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 1832  	   SPROC_NAME, 'Could not calculate offer length', EXCEPTION_MESSAGE);
13:41:20 1833  WHEN CAN_NOT_CALC_OFFER_LENGTH2 THEN
13:41:20 1834  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 1835  	   SPROC_NAME, 'Could not calculate last offer length', EXCEPTION_MESSAGE);
13:41:20 1836  WHEN OTHERS THEN
13:41:20 1837  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 1838  	   SPROC_NAME, 'Unkown error', SQLERRM);
13:41:20 1839  END CALC_SUBSCRIPTION_END_DATE;
