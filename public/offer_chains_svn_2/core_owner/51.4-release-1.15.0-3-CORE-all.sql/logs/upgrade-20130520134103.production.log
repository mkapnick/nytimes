13:41:20 825  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 826  	  SPROC_NAME, 'No such subscription');
13:41:20 827  WHEN OTHERS THEN
13:41:20 828  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 829  	  SPROC_NAME, 'Unknown error', SQLERRM);
