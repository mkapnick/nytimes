13:41:19 499  FUNCTION CALCULATE_OFFER_AMOUNT (
13:41:19 500  	in_offer_id IN NUMBER
13:41:19 501  ) RETURN NUMBER AS
13:41:19 502  -- VARIABLES
13:41:19 503  SPROC_NAME    CONSTANT VARCHAR2(22) := 'CALCULATE_OFFER_AMOUNT';
13:41:19 504  temp_offer_id NUMBER;
13:41:19 505  
13:41:19 506  var_product_offering_set	    SYS_REFCURSOR;
13:41:19 507  var_product_offering_id	    NUMBER;
13:41:19 508  var_product_offering_price    NUMBER(10,6);
13:41:19 509  var_product_offering_t_amount NUMBER(10,6);
13:41:19 510  var_product_offering_quantity NUMBER;
13:41:19 511  
13:41:19 512  var_total_amount NUMBER(10,6);
13:41:19 513  var_final_amount NUMBER(10,2);
13:41:19 514  
13:41:19 515  var_percent_discount NUMBER(10,2);
13:41:19 516  var_fixed_discount NUMBER(10,6);
13:41:19 517  
13:41:19 518  -- EXCEPTIONS
13:41:19 519  BAD_OFFER_ID EXCEPTION;
13:41:19 520  BEGIN
13:41:19 521  
13:41:19 522  	BEGIN
13:41:19 523  	  SELECT
13:41:19 524  	    OFFER.ID into temp_offer_id
13:41:19 525  	  FROM
13:41:19 526  	    OFFER
13:41:19 527  	  WHERE
13:41:19 528  	    OFFER.ID = in_offer_id;
13:41:19 529  	  EXCEPTION
13:41:19 530  	    WHEN NO_DATA_FOUND THEN
13:41:19 531  	      RAISE BAD_OFFER_ID;
13:41:19 532  	END;
13:41:19 533  
13:41:19 534  	OPEN var_product_offering_set FOR
13:41:19 535  	SELECT
13:41:19 536  	  OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID,
13:41:19 537  	  PRODUCT_OFFERING.UNIT_PRICE,
13:41:19 538  	  PRODUCT_OFFERING.QUANTITY
13:41:19 539  	FROM
13:41:19 540  	  OFFER_PRODUCT_OFFERING
13:41:19 541  	  INNER JOIN PRODUCT_OFFERING ON OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = PRODUCT_OFFERING.ID
13:41:19 542  	WHERE
13:41:19 543  	  OFFER_PRODUCT_OFFERING.OFFER_ID = in_offer_id;
13:41:19 544  
13:41:19 545  	var_total_amount := 0;
13:41:19 546  
13:41:19 547  	LOOP
13:41:19 548  	  FETCH var_product_offering_set into
13:41:19 549  	    var_product_offering_id,
13:41:19 550  	    var_product_offering_price,
13:41:19 551  	    var_product_offering_quantity;
13:41:19 552  	  EXIT WHEN var_product_offering_set%NOTFOUND;
13:41:19 553  
13:41:19 554  	  SELECT
13:41:19 555  	    SUM(DISCOUNT.FIXED_AMOUNT) into var_fixed_discount
13:41:19 556  	  FROM
13:41:19 557  	    DISCOUNT_PRODUCT_OFFERING
13:41:19 558  	    INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
13:41:19 559  	  WHERE
13:41:19 560  	    DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
13:41:19 561  	    AND DISCOUNT.FIXED_AMOUNT IS NOT NULL;
13:41:19 562  
13:41:19 563  	  SELECT
13:41:19 564  	    SUM(DISCOUNT.PERCENT_AMOUNT) into var_percent_discount
13:41:19 565  	  FROM
13:41:19 566  	    DISCOUNT_PRODUCT_OFFERING
13:41:19 567  	    INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
13:41:19 568  	  WHERE
13:41:19 569  	    DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
13:41:19 570  	    AND DISCOUNT.PERCENT_AMOUNT IS NOT NULL;
13:41:19 571  
13:41:19 572  	  var_product_offering_t_amount := var_product_offering_price * var_product_offering_quantity;
13:41:19 573  
13:41:19 574  	  IF var_percent_discount IS NOT NULL THEN
13:41:19 575  	    var_product_offering_t_amount := var_product_offering_t_amount - ( var_product_offering_t_amount * var_percent_discount / 100 );
13:41:19 576  	  END IF;
13:41:19 577  
13:41:19 578  	  IF var_fixed_discount IS NOT NULL THEN
13:41:19 579  	    var_product_offering_t_amount := var_product_offering_t_amount - var_fixed_discount;
13:41:19 580  	  END IF;
13:41:19 581  
13:41:19 582  	  var_total_amount := var_total_amount + var_product_offering_t_amount;
13:41:19 583  	END LOOP;
13:41:19 584  	var_final_amount := var_total_amount;
13:41:19 585  	RETURN var_final_amount;
13:41:19 586  
13:41:19 587  EXCEPTION
13:41:19 588  WHEN BAD_OFFER_ID THEN
13:41:19 589  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:19 590  	  SPROC_NAME, 'No such offer');
13:41:19 591  WHEN OTHERS THEN
13:41:19 592  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:19 593  	  SPROC_NAME, 'Unknown error', SQLERRM);
13:41:19 594  END CALCULATE_OFFER_AMOUNT;
