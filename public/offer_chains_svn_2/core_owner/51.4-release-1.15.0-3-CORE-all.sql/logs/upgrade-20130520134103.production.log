13:41:21 824  PROCEDURE SHOULD_MOVE_TO_GRACE(
13:41:21 825  	in_invoice_id  IN INVOICE.ID%TYPE,
13:41:21 826  	out_result     OUT NUMBER
13:41:21 827  ) AS
13:41:21 828  BEGIN
13:41:21 829  	-- if the invoice preceding the given invoice has no transaction attempts, then
13:41:21 830  	-- it is not billed out of Sartre. if so, then the associated subscription
13:41:21 831  	-- should be canceled after a final failed billing attempt--not moved to grace.
13:41:21 832  	SELECT DECODE(COUNT(1), 0, 0, 1) INTO out_result
13:41:21 833  	FROM CHARGE c
13:41:21 834  	INNER JOIN TRANSACTION t ON c.TRANSACTION_ID = t.ID
13:41:21 835  	INNER JOIN TRANSACTION_ATTEMPT ta ON ta.TRANSACTION_ID = t.ID
13:41:21 836  	WHERE c.INVOICE_ID = (
13:41:21 837  	  -- select previous invoice_id, or -1 if there is none
13:41:21 838  	  SELECT PREV_INVOICE_ID FROM (
13:41:21 839  	    SELECT i.ID, LAG(i.ID, 1, -1) OVER (ORDER BY i.CREATE_DATE) AS PREV_INVOICE_ID
13:41:21 840  	    FROM INVOICE i
13:41:21 841  	    INNER JOIN LICENSE l ON i.ID = l.INVOICE_ID
13:41:21 842  	    WHERE l.SUBSCRIPTION_ID = (
13:41:21 843  	      SELECT SUBSCRIPTION_ID FROM LICENSE WHERE INVOICE_ID = in_invoice_id
13:41:21 844  	    )
13:41:21 845  	  ) WHERE ID = in_invoice_id
13:41:21 846  	);
13:41:21 847  END SHOULD_MOVE_TO_GRACE;
13:41:21 848  
