13:41:20 180  	SELECT
13:41:20 181  	  COUNT(*) into var_pending_invoices_num
13:41:20 182  	FROM
13:41:20 183  	  LICENSE
13:41:20 184  	    INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
13:41:20 185  	    INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
13:41:20 186  	WHERE
