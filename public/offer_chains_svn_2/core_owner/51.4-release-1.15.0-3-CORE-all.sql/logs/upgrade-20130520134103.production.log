13:41:20 275  PROCEDURE REACTIVATE_ACCOUNT (
13:41:20 276  	in_group_id   IN NUMBER,
13:41:20 277  	in_updated_by IN VARCHAR2,
13:41:20 278  	in_note       IN VARCHAR2,
13:41:20 279  	in_agent_id   IN NUMBER
13:41:20 280  ) AS
13:41:20 281  -- VARIABLES
13:41:20 282  SPROC_NAME	      CONSTANT VARCHAR2(18) := 'REACTIVATE_ACCOUNT';
13:41:20 283  var_account_id	      NUMBER;
13:41:20 284  current_account_status  NUMBER;
13:41:20 285  
13:41:20 286  -- EXCEPTIONS
13:41:20 287  BAD_CURRENT_ACC_STATUS EXCEPTION;
13:41:20 288  CAN_NOT_CREATE_NOTE    EXCEPTION;
13:41:20 289  EXCEPTION_MESSAGE      VARCHAR2(1024);
13:41:20 290  BEGIN
13:41:20 291  	-- Get account id, status
13:41:20 292  	SELECT
13:41:20 293  	  ACCOUNT.ACCOUNT_STATUS_ID,
13:41:20 294  	  ACCOUNT.ID
13:41:20 295  	INTO
13:41:20 296  	  current_account_status,
13:41:20 297  	  var_account_id
13:41:20 298  	FROM ACCOUNT
13:41:20 299  	WHERE
13:41:20 300  	  ACCOUNT.GROUP_ID = in_group_id;
13:41:20 301  
13:41:20 302  	IF current_account_status != GLOBAL_STATUSES_V21.ACCOUNT_FROZEN THEN
13:41:20 303  	  RAISE BAD_CURRENT_ACC_STATUS;
13:41:20 304  	END IF;
13:41:20 305  
13:41:20 306  	-- Change account status
13:41:20 307  	PROCS_ACCOUNT_V21.UPDATE_ACCOUNT_STATUS(
13:41:20 308  	  in_account_id        => var_account_id,
13:41:20 309  	  in_updated_by        => in_updated_by,
13:41:20 310  	  in_account_status_id => GLOBAL_STATUSES_V21.ACCOUNT_ACTIVE
13:41:20 311  	);
13:41:20 312  
13:41:20 313  	-- Add note
13:41:20 314  	BEGIN
13:41:20 315  	  PROCS_ACCOUNT_V21.ANNOTATE_ACCOUNT(
13:41:20 316  	    in_group_id   => in_group_id,
13:41:20 317  	    in_agent_id   => in_agent_id,
13:41:20 318  	    in_note	  => in_note,
13:41:20 319  	    in_created_by => in_updated_by
13:41:20 320  	  );
13:41:20 321  	  EXCEPTION
13:41:20 322  	    WHEN OTHERS THEN
13:41:20 323  	      EXCEPTION_MESSAGE := SQLERRM;
13:41:20 324  	      RAISE CAN_NOT_CREATE_NOTE;
13:41:20 325  	END;
13:41:20 326  
13:41:20 327  EXCEPTION
13:41:20 328  WHEN BAD_CURRENT_ACC_STATUS THEN
13:41:20 329  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
13:41:20 330  	  SPROC_NAME, 'Current account status is not "frozen"');
13:41:20 331  WHEN NO_DATA_FOUND THEN
13:41:20 332  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 333  	  SPROC_NAME, 'Could not find account with given group ID');
13:41:20 334  WHEN CAN_NOT_CREATE_NOTE THEN
13:41:20 335  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 336  	  SPROC_NAME, 'Could not annotate account');
13:41:20 337  WHEN OTHERS THEN
13:41:20 338  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 339  	  SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 340  END REACTIVATE_ACCOUNT;
