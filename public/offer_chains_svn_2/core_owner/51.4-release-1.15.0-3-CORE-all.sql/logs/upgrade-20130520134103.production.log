13:41:20  98  	      c.transaction_id = t.id and
13:41:20  99  	      t.is_refund = GLOBAL_CONSTANTS_V21.TRUE
13:41:20 100  	  )
13:41:20 101  	  AND ROWNUM <= in_num_rows*10
13:41:20 102  	  ORDER BY dbms_random.value
13:41:20 103  ) WHERE
13:41:20 104  	  ROWNUM <= in_num_rows;
13:41:20 105  EXCEPTION
13:41:20 106  WHEN OTHERS THEN
