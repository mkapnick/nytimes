13:41:19 697  PROCEDURE GET_ENDED_GC_LICENSES (
13:41:19 698  	out_result_set		OUT SYS_REFCURSOR,
13:41:19 699  	in_hours_number 	IN NUMBER DEFAULT 14*24,
13:41:19 700  	in_num_rows		IN NUMBER DEFAULT 10000,
13:41:19 701  	in_process_name IN PROCESS_RETRY_THROTTLE.PROCESS_NAME%TYPE
13:41:19 702  ) AS
13:41:19 703  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_ENDED_GC_INVOICES';
13:41:19 704  var_days		NUMBER;
13:41:19 705  var_hours 	NUMBER;
13:41:19 706  var_time_interval INTERVAL DAY (3) TO SECOND;
13:41:19 707  BEGIN
13:41:19 708  	var_hours := mod(in_hours_number,24);
13:41:19 709  	var_days := (in_hours_number - var_hours) / 24;
13:41:19 710  	var_time_interval := var_days||' '||var_hours||':0:0';
13:41:19 711  	OPEN out_result_set FOR
13:41:19 712  SELECT * FROM
13:41:19 713  (
13:41:19 714  	SELECT
13:41:19 715  	  l.ID,
13:41:19 716  	  l.CREATE_DATE,
13:41:19 717  	  l.CREATED_BY,
13:41:19 718  	  l.CURRENT_OFFER_INDEX,
13:41:19 719  	  l.CURRENT_OFFER_RECURR_NUM,
13:41:19 720  	  l.END_DATE,
13:41:19 721  	  l.ENTITLEMENT_END_DATE,
13:41:19 722  	  l.INVOICE_ID,
13:41:19 723  	  l.IS_EXTENSION,
13:41:19 724  	  l.LICENSE_STATUS_ID,
13:41:19 725  	  l.OFFER_ID,
13:41:19 726  	  l.START_DATE,
13:41:19 727  	  l.SUBSCRIPTION_ID,
13:41:19 728  	  l.UPDATE_DATE,
13:41:19 729  	  l.UPDATED_BY
13:41:19 730  	FROM
13:41:19 731  	  GIFT_CERTIFICATE gc
13:41:19 732  	  INNER JOIN INVOICE i ON i.id = gc.PURCHASE_INVOICE_ID
13:41:19 733  	  INNER JOIN LICENSE l ON l.invoice_id = i.id
13:41:19 734  	  LEFT JOIN SUBSCRIPTION s ON s.id = l.subscription_id
13:41:19 735  	WHERE
13:41:19 736  	  l.LICENSE_STATUS_ID != GLOBAL_STATUSES_V21.LICENSE_ACTIVE
13:41:19 737  	  AND l.ENTITLEMENT_END_DATE <= (current_timestamp)
13:41:19 738  	  AND l.ENTITLEMENT_END_DATE > (current_timestamp - var_time_interval)
13:41:19 739  	  AND s.subscription_status_id = GLOBAL_STATUSES_V21.SUBSCRIPTION_CLOSED
13:41:19 740  	  AND NOT EXISTS (
13:41:19 741  	    SELECT NULL
13:41:19 742  	    FROM PROCESS_RETRY_THROTTLE
13:41:19 743  	    WHERE PROCESS_NAME = in_process_name
13:41:19 744  	      AND GENERIC_ID = l.ID
13:41:19 745  	  )
13:41:19 746  	  AND ROWNUM <= in_num_rows*10
13:41:19 747  	  ORDER BY dbms_random.value
13:41:19 748  ) WHERE
13:41:19 749  	  ROWNUM <= in_num_rows
13:41:19 750  	  GROUP BY SUBSCRIPTION_ID;
13:41:19 751  EXCEPTION
13:41:19 752  WHEN OTHERS THEN
13:41:19 753  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:19 754  	  SPROC_NAME, 'Unknown error', SQLERRM);
13:41:19 755  END GET_ENDED_GC_LICENSES;
