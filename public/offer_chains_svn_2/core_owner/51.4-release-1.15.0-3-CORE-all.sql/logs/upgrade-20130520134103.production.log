13:41:20 2572  
13:41:20 2573  FUNCTION IS_SUBSCRIPTION_CANCELABLE (
13:41:20 2574  	 in_subscription_id IN NUMBER
13:41:20 2575  ) RETURN NUMBER AS
13:41:20 2576  SPROC_NAME CONSTANT VARCHAR2(26) := 'IS_SUBSCRIPTION_CANCELABLE';
13:41:20 2577  -- VARIABLES
13:41:20 2578  var_is_offer_chain_cancelable NUMBER;
13:41:20 2579  --64603
13:41:20 2580  var_end_date date;
13:41:20 2581  today_date date := current_date;
13:41:20 2582  offer_id number;
13:41:20 2583  -- EXCEPTIONS
13:41:20 2584  COULD_NOT_CHECK	   EXCEPTION;
13:41:20 2585  BAD_SUBSCRIPTION_ID EXCEPTION;
13:41:20 2586  EXCEPTION_MESSAGE   VARCHAR2(1024);
13:41:20 2587  BEGIN
13:41:20 2588  
13:41:20 2589  	 BEGIN
13:41:20 2590  	   -- find offer_chain_id for given in_subscription_id
13:41:20 2591  	   SELECT OFFER_CHAIN_ID into offer_id
13:41:20 2592  	   FROM SUBSCRIPTION
13:41:20 2593  	   WHERE ID = in_subscription_id;
13:41:20 2594  	   EXCEPTION
13:41:20 2595  	     WHEN NO_DATA_FOUND THEN
13:41:20 2596  	       RAISE BAD_SUBSCRIPTION_ID;
13:41:20 2597  	     WHEN OTHERS THEN
13:41:20 2598  	       EXCEPTION_MESSAGE := SQLERRM;
13:41:20 2599  	       RAISE COULD_NOT_CHECK;
13:41:20 2600  	 END;
13:41:20 2601  
13:41:20 2602  	 -- find if it was redeemed from a gift certificate
13:41:20 2603  	 BEGIN
13:41:20 2604  	   SELECT l.end_date INTO var_end_date
13:41:20 2605  	   FROM LICENSE l, GIFT_CERTIFICATE g
13:41:20 2606  	   WHERE l.invoice_id = g.finalized_invoice_id
13:41:20 2607  	   AND l.subscription_id = in_subscription_id;
13:41:20 2608  
13:41:20 2609  	   -- if the license end_date is bigger than today, we are in the
13:41:20 2610  	   -- first period, so we cannot cancel; otherwise can cancel
13:41:20 2611  	   IF var_end_date > today_date THEN
13:41:20 2612  	       RETURN GLOBAL_CONSTANTS_V21.FALSE;
13:41:20 2613  	   ELSE
13:41:20 2614  	       RETURN GLOBAL_CONSTANTS_V21.TRUE;
13:41:20 2615  	   END IF;
13:41:20 2616  
13:41:20 2617  	   EXCEPTION
13:41:20 2618  	       -- not coming from a gift certificate,
13:41:20 2619  	       -- use old logic
13:41:20 2620  	       WHEN NO_DATA_FOUND THEN
13:41:20 2621  		   SELECT
13:41:20 2622  		       PROCS_OFFER_CHAIN_V21.IS_OFFER_CHAIN_CANCELABLE(offer_id)
13:41:20 2623  		       INTO var_is_offer_chain_cancelable
13:41:20 2624  		   FROM DUAL;
13:41:20 2625  		   RETURN var_is_offer_chain_cancelable;
13:41:20 2626  	 END;
13:41:20 2627  
13:41:20 2628  EXCEPTION
13:41:20 2629  WHEN BAD_SUBSCRIPTION_ID THEN
13:41:20 2630  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 2631  	   SPROC_NAME, 'No such subscription');
13:41:20 2632  WHEN COULD_NOT_CHECK THEN
13:41:20 2633  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 2634  	   SPROC_NAME, 'Could not check if offer chain calcelable', EXCEPTION_MESSAGE);
13:41:20 2635  WHEN OTHERS THEN
13:41:20 2636  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 2637  	   SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 2638  
13:41:20 2639  END IS_SUBSCRIPTION_CANCELABLE;
