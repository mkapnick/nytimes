13:41:20 797  	  INVOICE.UPDATE_DATE,
13:41:20 798  	  INVOICE.UPDATED_BY,
13:41:20 799  	  INVOICE.TAX_EXEMPT_ID
13:41:20 800  	FROM
13:41:20 801  	  LICENSE
13:41:20 802  	    INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
13:41:20 803  	WHERE
13:41:20 804  	  LICENSE.SUBSCRIPTION_ID = in_subscription_id;
13:41:20 805  
13:41:20 806  EXCEPTION
