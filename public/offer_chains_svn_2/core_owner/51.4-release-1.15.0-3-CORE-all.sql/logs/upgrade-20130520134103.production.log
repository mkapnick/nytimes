13:41:19   3  PROCEDURE ADD_LINE_ITEMS(
13:41:19   4  /*
13:41:19   5  Throws exceptions:
13:41:19   6  APP_EXCEPTION_CODES_V21.NOT_FOUND
13:41:19   7  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
13:41:19   8  */
13:41:19   9  	in_invoice_id IN NUMBER,
13:41:19  10  	in_offer_id   IN NUMBER,
13:41:19  11  	in_created_by IN VARCHAR2
13:41:19  12  ) AS
13:41:19  13  -- VARIABLES
13:41:19  14  SPROC_NAME      CONSTANT VARCHAR2(14) := 'ADD_LINE_ITEMS';
13:41:19  15  temp_invoice_id NUMBER;
13:41:19  16  temp_offer_id   NUMBER;
13:41:19  17  
13:41:19  18  var_line_item_data SYS_REFCURSOR;
13:41:19  19  var_new_line_item_id NUMBER;
13:41:19  20  var_product_unit_price NUMBER (10,6);
13:41:19  21  var_product_offering_price NUMBER(10,6);
13:41:19  22  var_product_offering_oprice NUMBER(10,6);
13:41:19  23  var_product_quantity NUMBER;
13:41:19  24  var_product_offering_id NUMBER;
13:41:19  25  
13:41:19  26  var_line_item_price	  NUMBER(10,2);
13:41:19  27  var_discount_fixed_amount   NUMBER(10,6);
13:41:19  28  var_discount_percent_amount NUMBER(10,2);
13:41:19  29  
13:41:19  30  
13:41:19  31  -- EXCEPTIONS
13:41:19  32  BAD_INVOICE_ID EXCEPTION;
13:41:19  33  BAD_OFFER_ID EXCEPTION;
13:41:19  34  BAD_DISCOUNT EXCEPTION;
13:41:19  35  BEGIN
13:41:19  36  
13:41:19  37  	-- Check that given invoice exists
13:41:19  38  	BEGIN
13:41:19  39  	  SELECT
13:41:19  40  	    INVOICE.ID into temp_invoice_id
13:41:19  41  	  FROM
13:41:19  42  	    INVOICE
13:41:19  43  	  WHERE
13:41:19  44  	    INVOICE.ID = in_invoice_id;
13:41:19  45  
13:41:19  46  	  EXCEPTION
13:41:19  47  	    WHEN NO_DATA_FOUND THEN
13:41:19  48  	      RAISE BAD_INVOICE_ID;
13:41:19  49  	END;
13:41:19  50  
13:41:19  51  	-- Check that given offer exists
13:41:19  52  	BEGIN
13:41:19  53  	  SELECT
13:41:19  54  	    OFFER.ID into temp_offer_id
13:41:19  55  	  FROM
13:41:19  56  	    OFFER
13:41:19  57  	  WHERE
13:41:19  58  	    OFFER.ID = in_offer_id;
13:41:19  59  
13:41:19  60  	  EXCEPTION
13:41:19  61  	    WHEN NO_DATA_FOUND THEN
13:41:19  62  	      RAISE BAD_OFFER_ID;
13:41:19  63  	END;
13:41:19  64  
13:41:19  65  	-- Get product_offering data
13:41:19  66  	OPEN var_line_item_data FOR
13:41:19  67  	SELECT
13:41:19  68  	  PRODUCT_OFFERING.ID,
13:41:19  69  	  PRODUCT_OFFERING.UNIT_PRICE,
13:41:19  70  	  PRODUCT_OFFERING.QUANTITY
13:41:19  71  	FROM
13:41:19  72  	  OFFER_PRODUCT_OFFERING
13:41:19  73  	  INNER JOIN PRODUCT_OFFERING ON OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = PRODUCT_OFFERING.ID
13:41:19  74  	WHERE
13:41:19  75  	  OFFER_PRODUCT_OFFERING.OFFER_ID = in_offer_id;
13:41:19  76  
13:41:19  77  	-- insert line items
13:41:19  78  	-- add discounts to line items
13:41:19  79  	LOOP
13:41:19  80  	  FETCH var_line_item_data INTO
13:41:19  81  	    var_product_offering_id,
13:41:19  82  	    var_product_unit_price,
13:41:19  83  	    var_product_quantity;
13:41:19  84  	  EXIT WHEN var_line_item_data%NOTFOUND;
13:41:19  85  
13:41:19  86  	  var_product_offering_oprice := var_product_unit_price * var_product_quantity;
13:41:19  87  	  var_product_offering_price := var_product_offering_oprice;
13:41:19  88  
13:41:19  89  	  -- Apply discounts to line_item
13:41:19  90  	  BEGIN
13:41:19  91  	    SELECT
13:41:19  92  	      SUM (DISCOUNT.FIXED_AMOUNT) into var_discount_fixed_amount
13:41:19  93  	    FROM
13:41:19  94  	      DISCOUNT_PRODUCT_OFFERING
13:41:19  95  	      INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
13:41:19  96  	    WHERE
13:41:19  97  	      DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
13:41:19  98  	      AND DISCOUNT.FIXED_AMOUNT IS NOT NULL;
13:41:19  99  	    EXCEPTION
13:41:19 100  	    WHEN NO_DATA_FOUND THEN
13:41:19 101  	      var_discount_fixed_amount := NULL;
13:41:19 102  	  END;
13:41:19 103  
13:41:19 104  	  BEGIN
13:41:19 105  	    SELECT
13:41:19 106  	      SUM (DISCOUNT.PERCENT_AMOUNT) into var_discount_percent_amount
13:41:19 107  	    FROM
13:41:19 108  	      DISCOUNT_PRODUCT_OFFERING
13:41:19 109  	      INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
13:41:19 110  	    WHERE
13:41:19 111  	      DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
13:41:19 112  	      AND DISCOUNT.PERCENT_AMOUNT IS NOT NULL;
13:41:19 113  	    EXCEPTION
13:41:19 114  	    WHEN NO_DATA_FOUND THEN
13:41:19 115  	      var_discount_percent_amount := NULL;
13:41:19 116  	  END;
13:41:19 117  
13:41:19 118  	  IF (var_discount_percent_amount IS NOT NULL) THEN
13:41:19 119  	    var_product_offering_price := var_product_offering_price * ( 1 - var_discount_percent_amount / 100 );
13:41:19 120  	  END IF;
13:41:19 121  
13:41:19 122  	  IF (var_discount_fixed_amount IS NOT NULL) THEN
13:41:19 123  	    var_product_offering_price := var_product_offering_price - var_discount_fixed_amount;
13:41:19 124  	  END IF;
13:41:19 125  
13:41:19 126  	  var_line_item_price := PROCS_COMMON_V21.ROUND_10_6_TO_10_2(var_product_offering_price);
13:41:19 127  
13:41:19 128  	  IF (var_line_item_price < 0) THEN
13:41:19 129  	      RAISE BAD_DISCOUNT;
13:41:19 130  	  END IF;
13:41:19 131  
13:41:19 132  	  var_new_line_item_id := NULL;
13:41:19 133  	  PROCS_LINE_ITEMS_CRU_V21.CREATE_LINE_ITEM(
13:41:19 134  	    inout_line_item_id	=> var_new_line_item_id,
13:41:19 135  	    in_product_offer_id => var_product_offering_id,
13:41:19 136  	    in_invoice_id	=> in_invoice_id,
13:41:19 137  	    in_amount		=> var_line_item_price,
13:41:19 138  	    in_created_by	=> in_created_by,
13:41:19 139  	    in_discount_amount	=> var_product_offering_oprice - var_line_item_price,
13:41:19 140  	    in_taxes_amount	=> NULL
13:41:19 141  	  );
13:41:19 142  
13:41:19 143  	  FOR f_discount IN (
13:41:19 144  	    SELECT
13:41:19 145  	      DISCOUNT.ID
13:41:19 146  	    FROM
13:41:19 147  	      DISCOUNT_PRODUCT_OFFERING
13:41:19 148  	      INNER JOIN DISCOUNT ON DISCOUNT_PRODUCT_OFFERING.DISCOUNT_ID = DISCOUNT.ID
13:41:19 149  	    WHERE
13:41:19 150  	      DISCOUNT_PRODUCT_OFFERING.PRODUCT_OFFERING_ID = var_product_offering_id
13:41:19 151  	  )
13:41:19 152  	  LOOP
13:41:19 153  	    PROCS_LINE_ITEMS_CRU_V21.CREATE_DISCOUNT_LINE_ITEM(
13:41:19 154  	      in_discount_id =>  f_discount.ID,
13:41:19 155  	      in_line_item_id => var_new_line_item_id
13:41:19 156  	    );
13:41:19 157  	  END LOOP;
13:41:19 158  	END LOOP;
13:41:19 159  
13:41:19 160  EXCEPTION
13:41:19 161  WHEN BAD_INVOICE_ID THEN
13:41:19 162  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:19 163  	  SPROC_NAME, 'No such license');
13:41:19 164  WHEN BAD_OFFER_ID THEN
13:41:19 165  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:19 166  	  SPROC_NAME, 'No such offer');
13:41:19 167  WHEN BAD_DISCOUNT THEN
13:41:19 168  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:19 169  	  SPROC_NAME, 'Bad Discount');
13:41:19 170  WHEN OTHERS THEN
13:41:19 171  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:19 172  	  SPROC_NAME, 'Unknown error', SQLERRM);
13:41:19 173  END ADD_LINE_ITEMS;
