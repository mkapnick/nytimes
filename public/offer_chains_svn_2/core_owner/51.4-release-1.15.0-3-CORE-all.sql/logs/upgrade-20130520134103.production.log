13:41:20 437  PROCEDURE GET_TRANSACTION_INVOICE (
13:41:20 438  /*
13:41:20 439  Throws exceptions:
13:41:20 440  APP_EXCEPTION_CODES_V21.NOT_FOUND
13:41:20 441  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
13:41:20 442  */
13:41:20 443  	in_transaction_id  IN  NUMBER,
13:41:20 444  	out_result_set	      OUT SYS_REFCURSOR
13:41:20 445  ) AS
13:41:20 446  SPROC_NAME CONSTANT  VARCHAR2(23) := 'GET_TRANSACTION_INVOICE';
13:41:20 447  -- VARIABLES
13:41:20 448  temp_transaction_id  NUMBER;
13:41:20 449  var_invoice_id	   NUMBER;
13:41:20 450  var_subscription_id  NUMBER;
13:41:20 451  var_offer_chain_id   NUMBER;
13:41:20 452  var_offer_chain_name VARCHAR2(255);
13:41:20 453  -- EXCEPTIONS
13:41:20 454  BAD_TRANSACTION_ID     EXCEPTION;
13:41:20 455  EXCEPTION_MESSAGE      VARCHAR2(1024);
13:41:20 456  BEGIN
13:41:20 457   -- check if there is the transaction
13:41:20 458  	BEGIN
13:41:20 459  	  SELECT
13:41:20 460  	    TRANSACTION.ID into temp_transaction_id
13:41:20 461  	  FROM
13:41:20 462  	    TRANSACTION
13:41:20 463  	  WHERE
13:41:20 464  	    TRANSACTION.ID = in_transaction_id;
13:41:20 465  	  EXCEPTION
13:41:20 466  	    WHEN NO_DATA_FOUND THEN
13:41:20 467  	      RAISE BAD_TRANSACTION_ID;
13:41:20 468  	END;
13:41:20 469  
13:41:20 470  	-- Get invoice id
13:41:20 471  	SELECT DISTINCT
13:41:20 472  	  CHARGE.INVOICE_ID into var_invoice_id
13:41:20 473  	FROM
13:41:20 474  	  CHARGE
13:41:20 475  	WHERE
13:41:20 476  	  CHARGE.TRANSACTION_ID = in_transaction_id;
13:41:20 477  
13:41:20 478  	-- Get subscription id if exists
13:41:20 479  	BEGIN
13:41:20 480  	  SELECT DISTINCT
13:41:20 481  	    LICENSE.SUBSCRIPTION_ID into var_subscription_id
13:41:20 482  	  FROM
13:41:20 483  	    LICENSE
13:41:20 484  	  WHERE
13:41:20 485  	    LICENSE.INVOICE_ID = var_invoice_id;
13:41:20 486  	  EXCEPTION
13:41:20 487  	    WHEN NO_DATA_FOUND THEN
13:41:20 488  	      var_subscription_id := NULL;
13:41:20 489  	END;
13:41:20 490  
13:41:20 491  	IF var_subscription_id IS NOT NULL THEN
13:41:20 492  	  -- Fetch offer chain from subscription
13:41:20 493  	  SELECT
13:41:20 494  	    OFFER_CHAIN.ID,
13:41:20 495  	    OFFER_CHAIN.NAME
13:41:20 496  	    into
13:41:20 497  	    var_offer_chain_id,
13:41:20 498  	    var_offer_chain_name
13:41:20 499  	  FROM
13:41:20 500  	    OFFER_CHAIN
13:41:20 501  	    INNER JOIN SUBSCRIPTION ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
13:41:20 502  	  WHERE
13:41:20 503  	    SUBSCRIPTION.ID = var_subscription_id;
13:41:20 504  	ELSE
13:41:20 505  	  -- Fetch offer chain from GC
13:41:20 506  	  SELECT
13:41:20 507  	    OFFER_CHAIN.ID,
13:41:20 508  	    OFFER_CHAIN.NAME
13:41:20 509  	    into
13:41:20 510  	    var_offer_chain_id,
13:41:20 511  	    var_offer_chain_name
13:41:20 512  	  FROM
13:41:20 513  	    OFFER_CHAIN
13:41:20 514  	    INNER JOIN GIFT_CERTIFICATE ON GIFT_CERTIFICATE.OFFER_CHAIN_ID = OFFER_CHAIN.ID
13:41:20 515  	  WHERE
13:41:20 516  	    GIFT_CERTIFICATE.PURCHASE_INVOICE_ID = var_invoice_id;
13:41:20 517  	END IF;
13:41:20 518  
13:41:20 519  	OPEN out_result_set FOR
13:41:20 520  	SELECT DISTINCT
13:41:20 521  	  var_invoice_id       AS "INVOICE_ID",
13:41:20 522  	  var_subscription_id  AS "SUBSCRIPTION_ID",
13:41:20 523  	  var_offer_chain_id   AS "OFFER_CHAIN_ID",
