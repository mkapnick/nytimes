13:41:20  84  PROCEDURE GET_PENDING_INVOICES (
13:41:20  85  /*
13:41:20  86  Throws exceptions:
13:41:20  87  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
13:41:20  88  */
13:41:20  89  	out_result_set	     OUT SYS_REFCURSOR,
13:41:20  90  	in_row_number	     IN NUMBER DEFAULT NULL
13:41:20  91  ) AS
13:41:20  92  SPROC_NAME CONSTANT VARCHAR2(20) := 'GET_PENDING_INVOICES';
13:41:20  93  -- COMSTANTS
13:41:20  94  DEFAULT_ROW_NUMBER CONSTANT NUMBER := 1;
13:41:20  95  -- VARIABLES
13:41:20  96  var_row_number NUMBER;
13:41:20  97  BEGIN
13:41:20  98  
13:41:20  99  	IF in_row_number IS NULL THEN
13:41:20 100  	  var_row_number := DEFAULT_ROW_NUMBER;
13:41:20 101  	ELSE
13:41:20 102  	  var_row_number := in_row_number;
13:41:20 103  	END IF;
13:41:20 104  
13:41:20 105  	-- Invoices with one or more payments(charges) with transaction status PENDING
13:41:20 106  	OPEN out_result_set FOR
13:41:20 107  SELECT * FROM
13:41:20 108  (
13:41:20 109  	SELECT
13:41:20 110  	  INVOICE.ID
13:41:20 111  	FROM
13:41:20 112  	  CHARGE
13:41:20 113  	  INNER JOIN INVOICE ON CHARGE.INVOICE_ID = INVOICE.ID
13:41:20 114  	WHERE
13:41:20 115  	  EXISTS(
13:41:20 116  	    SELECT NULL
13:41:20 117  	    FROM TRANSACTION
13:41:20 118  	    WHERE
13:41:20 119  	      TRANSACTION.TRANSACTION_STATUS_ID = GLOBAL_STATUSES_V21.TRANSACTION_PENDING
13:41:20 120  	      AND TRANSACTION.ID = CHARGE.TRANSACTION_ID
13:41:20 121  	      AND TRANSACTION.IS_REFUND != GLOBAL_CONSTANTS_V21.TRUE
13:41:20 122  	      AND TRANSACTION.TRANSACTION_AMOUNT >= 0
13:41:20 123  	  )
13:41:20 124  	  AND
13:41:20 125  	  NOT EXISTS(
13:41:20 126  	    SELECT NULL
13:41:20 127  	    FROM PROCESS_RETRY_THROTTLE
13:41:20 128  	    WHERE PROCESS_NAME = SPROC_NAME
13:41:20 129  	      AND GENERIC_ID = INVOICE.ID
13:41:20 130  	  )
13:41:20 131  	  AND ROWNUM <= var_row_number*10
13:41:20 132  	  ORDER BY dbms_random.value
13:41:20 133  ) WHERE
13:41:20 134  	  ROWNUM <= var_row_number;
13:41:20 135  
13:41:20 136  EXCEPTION
13:41:20 137  WHEN OTHERS THEN
13:41:20 138  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 139  	  SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 140  END GET_PENDING_INVOICES;
