13:41:20 2930  /*
13:41:20 2931  	   Note, this returns paypal records which are in the given window of time
13:41:20 2932  	   based on their creation date, not their expiration date, because we've seen
13:41:20 2933  	   suspicious values in the data for expiration dates. The java layer should
13:41:20 2934  	   inspect the expiration dates and decide whether or not to act on the data.
13:41:20 2935  */
13:41:20 2936  
13:41:20 2937  PROCEDURE GET_EXPIRING_PAYPAL (
13:41:20 2938  	 in_expire_window_days	 IN NUMBER,
13:41:20 2939  	 in_creation_limit_days  IN NUMBER,
13:41:20 2940  	 in_retry_throttle_name  IN PROCESS_RETRY_THROTTLE.PROCESS_NAME%TYPE,
13:41:20 2941  	 out_result_set 	 OUT SYS_REFCURSOR
13:41:20 2942  ) AS
13:41:20 2943  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_EXPIRING_PAYPAL';
13:41:20 2944  -- If creation date is later than this, return the record
13:41:20 2945  var_end_creation DATE := SYSDATE - in_creation_limit_days + in_expire_window_days;
13:41:20 2946  var_rows_to_return CONSTANT NUMBER := 300;
13:41:20 2947  BEGIN
13:41:20 2948  	 OPEN out_result_set FOR
13:41:20 2949  	 SELECT * FROM (
13:41:20 2950  	   SELECT
13:41:20 2951  	       ac.group_id	   AS group_id,
13:41:20 2952  	       pp.id		   AS paypal_id,
13:41:20 2953  	       sb.id		   AS subscription_id,
13:41:20 2954  	       pp.create_date	   AS creation_date,
13:41:20 2955  	       pp.expiration_date  AS expiration_date
13:41:20 2956  	   FROM paypal pp
13:41:20 2957  	   INNER JOIN subscription sb
13:41:20 2958  	   ON pp.account_id = sb.account_id
13:41:20 2959  	   INNER JOIN account ac
13:41:20 2960  	   ON pp.account_id = ac.id
13:41:20 2961  	   WHERE pp.paypal_status_id = GLOBAL_STATUSES_V21.PAYPAL_ACTIVE
13:41:20 2962  	     AND sb.instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_PAYPAL
13:41:20 2963  	     AND sb.subscription_status_id IN (GLOBAL_STATUSES_V21.SUBSCRIPTION_ACTIVE,
13:41:20 2964  					       GLOBAL_STATUSES_V21.SUBSCRIPTION_IN_GRACE_PERIOD)
13:41:20 2965  	     AND sb.instrument_id = pp.id
13:41:20 2966  	     AND pp.create_date <= var_end_creation
13:41:20 2967  	     AND NOT EXISTS(
13:41:20 2968  	       SELECT NULL
13:41:20 2969  	       FROM process_retry_throttle rt
13:41:20 2970  	       WHERE rt.process_name = in_retry_throttle_name
13:41:20 2971  		 AND rt.generic_id = pp.id
13:41:20 2972  	     )
13:41:20 2973  	     AND ROWNUM <= 10 * var_rows_to_return
13:41:20 2974  	   ORDER BY dbms_random.value
13:41:20 2975  	 )
13:41:20 2976  	 WHERE ROWNUM <= var_rows_to_return;
13:41:20 2977  END GET_EXPIRING_PAYPAL;
