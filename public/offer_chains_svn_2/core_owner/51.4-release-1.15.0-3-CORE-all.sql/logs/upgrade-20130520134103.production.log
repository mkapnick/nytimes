13:41:20 595  
13:41:20 596  FUNCTION IS_INVOICE_PAYING_STARTED (
13:41:20 597  /*
13:41:20 598  Throws exceptions:
13:41:20 599  APP_EXCEPTION_CODES_V21.NOT_FOUND
13:41:20 600  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
13:41:20 601  */
13:41:20 602  	in_invoice_id IN NUMBER
13:41:20 603  ) RETURN NUMBER AS
13:41:20 604  -- VARIABLES
13:41:20 605  SPROC_NAME		 CONSTANT VARCHAR2(30) := 'IS_INVOICE_PAYING_STARTED';
13:41:20 606  temp_invoice_id		 NUMBER;
13:41:20 607  var_processed_charges_num  NUMBER;
13:41:20 608  var_processed_transac_num  NUMBER;
13:41:20 609  var_success_attempts_num	 NUMBER;
13:41:20 610  var_is_gc 		 NUMBER;
13:41:20 611  -- EXCEPTIONS
13:41:20 612  BAD_INVOICE_ID EXCEPTION;
13:41:20 613  BEGIN
13:41:20 614  
13:41:20 615  	-- Check that invoice exists
13:41:20 616  	BEGIN
13:41:20 617  	  SELECT
13:41:20 618  	    INVOICE.ID into temp_invoice_id
13:41:20 619  	  FROM
13:41:20 620  	    INVOICE
13:41:20 621  	  WHERE
13:41:20 622  	    INVOICE.ID = in_invoice_id;
13:41:20 623  	  EXCEPTION
13:41:20 624  	   WHEN NO_DATA_FOUND THEN
13:41:20 625  	     RAISE BAD_INVOICE_ID;
13:41:20 626  	END;
13:41:20 627  
13:41:20 628  	-- Check that there are tansaction attempts with status success
13:41:20 629  	SELECT
13:41:20 630  	  COUNT(1) into var_success_attempts_num
13:41:20 631  	FROM
13:41:20 632  	  TRANSACTION_ATTEMPT ta,
13:41:20 633  	  TRANSACTION t,
13:41:20 634  	  CHARGE c
13:41:20 635  	WHERE
13:41:20 636  	  ta.TRANSACTION_ATTEMPT_STATUS_ID = GLOBAL_STATUSES_V21.TRANS_ATTEMPT_SUCCESS and
13:41:20 637  	  ta.transaction_id = t.id and
13:41:20 638  	  c.transaction_id = t.id and
13:41:20 639  	  t.is_refund = GLOBAL_CONSTANTS_V21.FALSE and
13:41:20 640  	  c.invoice_id = in_invoice_id
13:41:20 641  	;
13:41:20 642  
13:41:20 643  	IF var_success_attempts_num > 0 THEN
13:41:20 644  	  RETURN GLOBAL_CONSTANTS_V21.TRUE;
13:41:20 645  	END IF;
13:41:20 646  
13:41:20 647  	SELECT
13:41:20 648  	  COUNT(1) into var_success_attempts_num
13:41:20 649  	FROM
13:41:20 650  	  TRANSACTION t,
13:41:20 651  	  CHARGE c
13:41:20 652  	WHERE
13:41:20 653  	  c.transaction_id = t.id and
13:41:20 654  	  t.is_refund = GLOBAL_CONSTANTS_V21.FALSE and
13:41:20 655  	  t.is_settled = GLOBAL_CONSTANTS_V21.TRUE and
13:41:20 656  	  c.invoice_id = in_invoice_id
13:41:20 657  	;
13:41:20 658  
13:41:20 659  	IF var_success_attempts_num > 0 THEN
13:41:20 660  	  RETURN GLOBAL_CONSTANTS_V21.TRUE;
13:41:20 661  	END IF;
13:41:20 662  
13:41:20 663  	SELECT
13:41:20 664  	  COUNT(1) into var_is_gc
13:41:20 665  	FROM
13:41:20 666  	  gift_certificate gc
13:41:20 667  	WHERE
13:41:20 668  	  gc.finalized_invoice_id = in_invoice_id
13:41:20 669  	;
13:41:20 670  
13:41:20 671  	IF var_is_gc > 0 THEN
13:41:20 672  	  RETURN GLOBAL_CONSTANTS_V21.TRUE;
13:41:20 673  	END IF;
13:41:20 674  
13:41:20 675  	RETURN GLOBAL_CONSTANTS_V21.FALSE;
13:41:20 676  
13:41:20 677  EXCEPTION
13:41:20 678  WHEN BAD_INVOICE_ID THEN
13:41:20 679  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 680  	  SPROC_NAME, 'No such invoice');
13:41:20 681  WHEN OTHERS THEN
13:41:20 682  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 683  	  SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 684  END IS_INVOICE_PAYING_STARTED;
13:41:20 685  
