13:41:20 370  ) AS
13:41:20 371  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_SUBSCRIPTION_SHARES';
13:41:20 372  range_diff NUMBER := 0;
13:41:20 373  upper_bond_diff NUMBER := 0;
13:41:20 374  l_start NUMBER := 0;
13:41:20 375  l_end   NUMBER := 0;
13:41:20 376  BEGIN
13:41:20 377  	-- Normalize the end points [START]
13:41:20 378  	IF (in_start IS NULL OR in_start < 0) Then
13:41:20 379  	  l_start := 0;
13:41:20 380  	ELSE
13:41:20 381  	  l_start := in_start;
13:41:20 382  	END IF;
13:41:20 383  
13:41:20 384  	IF (in_end IS NULL) Then
13:41:20 385  	  l_end := 11;
13:41:20 386  	ELSE
13:41:20 387  	  l_end := in_end;
13:41:20 388  	END IF;
13:41:20 389  
13:41:20 390  	l_start := l_start + 1;
13:41:20 391  	l_end	:= l_end   + 1;
13:41:20 392  
13:41:20 393  	range_diff := l_end - l_start;
13:41:20 394  	upper_bond_diff :=  range_diff - GLOBAL_CONSTANTS_V21.MAX_RETURN_COUNT;
13:41:20 395  
13:41:20 396  	IF (upper_bond_diff > 0) Then
13:41:20 397  	  l_end := l_end - upper_bond_diff;
13:41:20 398  	END IF;
13:41:20 399  	-- Normalize the end points [END]
13:41:20 400  
13:41:20 401  	BEGIN
13:41:20 402  	   OPEN out_result_set FOR
13:41:20 403  	   SELECT *
13:41:20 404  	   FROM
13:41:20 405  	     (SELECT rownum rnum, Q.*
13:41:20 406  	      FROM
13:41:20 407  	       (SELECT
13:41:20 408  		  ss.ID,
13:41:20 409  		  ss.GROUP_ACCOUNT_ID,
13:41:20 410  		  ss.BORROWER_ACCOUNT_ID,
13:41:20 411  		  ss.IP_ADDRESS,
13:41:20 412  		  ss.START_DATE,
