13:41:20 1901  PROCEDURE CLOSE_SUBSCRIPTION (
13:41:20 1902  	 in_subscription_id IN NUMBER,
13:41:20 1903  	 in_updated_by	    IN VARCHAR2
13:41:20 1904  ) AS
13:41:20 1905  -- VARIABLES
13:41:20 1906  SPROC_NAME	    CONSTANT VARCHAR2(18) := 'CLOSE_SUBSCRIPTION';
13:41:20 1907  temp_subscription_id NUMBER;
13:41:20 1908  var_licenses_count   NUMBER;
13:41:20 1909  -- EXCEPTIONS
13:41:20 1910  BAD_SUBSCRIPTION_ID    EXCEPTION;
13:41:20 1911  ACTIVE_LICENSES_FOUND  EXCEPTION;
13:41:20 1912  EXCEPTION_MESSAGE      VARCHAR2(1024);
13:41:20 1913  BEGIN
13:41:20 1914  
13:41:20 1915  	 BEGIN
13:41:20 1916  	   SELECT
13:41:20 1917  	     SUBSCRIPTION.ID into temp_subscription_id
13:41:20 1918  	   FROM
13:41:20 1919  	     SUBSCRIPTION
13:41:20 1920  	   WHERE
13:41:20 1921  	     SUBSCRIPTION.ID = in_subscription_id;
13:41:20 1922  	   EXCEPTION
13:41:20 1923  	     WHEN NO_DATA_FOUND THEN
13:41:20 1924  	       RAISE BAD_SUBSCRIPTION_ID;
13:41:20 1925  	 END;
13:41:20 1926  
13:41:20 1927  	 SELECT
13:41:20 1928  	   COUNT(*) into var_licenses_count
13:41:20 1929  	 FROM
13:41:20 1930  	   LICENSE
13:41:20 1931  	 WHERE
13:41:20 1932  	   LICENSE.SUBSCRIPTION_ID = in_subscription_id
13:41:20 1933  	   AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V21.LICENSE_ACTIVE;
13:41:20 1934  
13:41:20 1935  	 IF var_licenses_count > 0 THEN
13:41:20 1936  	   RAISE ACTIVE_LICENSES_FOUND;
13:41:20 1937  	 END IF;
13:41:20 1938  
13:41:20 1939  	 PROCS_SUBSCRIPTION_V21.UPDATE_SUBSCRIPTION_STATUS(
13:41:20 1940  	   in_subscription_id	     => in_subscription_id,
13:41:20 1941  	   in_updated_by	     => in_updated_by,
13:41:20 1942  	   in_subscription_status_id => GLOBAL_STATUSES_V21.SUBSCRIPTION_CLOSED
13:41:20 1943  	 );
13:41:20 1944  
13:41:20 1945  EXCEPTION
13:41:20 1946  WHEN BAD_SUBSCRIPTION_ID THEN
13:41:20 1947  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 1948  	   SPROC_NAME, 'No such subscription');
13:41:20 1949  WHEN ACTIVE_LICENSES_FOUND THEN
13:41:20 1950  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
13:41:20 1951  	   SPROC_NAME, 'Active licenses found');
13:41:20 1952  WHEN OTHERS THEN
13:41:20 1953  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 1954  	   SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 1955  END CLOSE_SUBSCRIPTION;
