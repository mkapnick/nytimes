13:41:20 947  	  PROCS_GROUP_ACCOUNT_CRU_V21.ADD_IP_RANGE(
13:41:20 948  	    in_group_account_id => in_group_account_id,
13:41:20 949  	    in_minimum_ip_string => in_minimum_ip_string,
13:41:20 950  	    in_minimum_ip_low => in_minimum_ip_low,
13:41:20 951  	    in_minimum_ip_high => in_minimum_ip_high,
13:41:20 952  	    in_maximum_ip_string => in_maximum_ip_string,
13:41:20 953  	    in_maximum_ip_low => in_maximum_ip_low,
13:41:20 954  	    in_maximum_ip_high => in_maximum_ip_high,
13:41:20 955  	    in_created_by => in_created_by
13:41:20 956  	  );
13:41:20 957  
13:41:20 958  	  -- Check for overlapping ip address range after insert.  Note that if another
13:41:20 959  	  -- call to add_ip_range has not completed, overlapping ip entries can occur.
13:41:20 960  	  SELECT count(1) into var_is_dupe
13:41:20 961  	  FROM
13:41:20 962  	    GROUP_ACCOUNT_IP_RANGE
13:41:20 963  	  WHERE
13:41:20 964  	    GROUP_ACC_IP_RNG_STATUS_ID = GLOBAL_STATUSES_V21.GROUP_ACC_IP_RNG_ACTIVE AND
13:41:20 965  		((
13:41:20 966  		  (in_minimum_ip_high > minimum_ip_high and in_minimum_ip_high < maximum_ip_high) or
