13:41:20 413  	  CHARGE.ID as "CHARGE_ID",
13:41:20 414  	  CHARGE.CHARGE_AMOUNT,
13:41:20 415  	  CHARGE.INSTRUMENT_ID,
13:41:20 416  	  CHARGE.CHARGE_STATUS_ID
13:41:20 417  	FROM
13:41:20 418  	  CHARGE
13:41:20 419  	WHERE
13:41:20 420  	  CHARGE.INVOICE_ID = in_invoice_id
13:41:20 421  	  AND CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V21.INSTRUMENT_GIFT_CERTIFICATE;
13:41:20 422  
13:41:20 423  EXCEPTION
13:41:20 424  WHEN BAD_INVOICE_ID THEN
13:41:20 425  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 426  	  SPROC_NAME, 'No such invoice');
13:41:20 427  WHEN CAN_NOT_FIND_ACCOUNT THEN
13:41:20 428  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 429  	  SPROC_NAME, 'Could not find account for given invoice id', EXCEPTION_MESSAGE);
13:41:20 430  WHEN OTHERS THEN
13:41:20 431  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 432  	  SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 433  END GET_INVOICE_DETAILS;
