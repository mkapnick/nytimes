13:41:19 208  
13:41:19 209  PROCEDURE GET_ENDING_LICENSES (
13:41:19 210  	in_hours_number IN NUMBER,
13:41:19 211  	out_result_set	OUT SYS_REFCURSOR
13:41:19 212  ) AS
13:41:19 213  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_ENDING_LICENSES';
13:41:19 214  -- VARIABLES
13:41:19 215  var_days		NUMBER;
13:41:19 216  var_hours 	NUMBER;
13:41:19 217  var_time_interval INTERVAL DAY (3) TO SECOND;
13:41:19 218  BEGIN
13:41:19 219  
13:41:19 220  	var_hours := mod(in_hours_number,24);
13:41:19 221  	var_days := (in_hours_number - var_hours) / 24;
13:41:19 222  	var_time_interval := var_days||' '||var_hours||':0:0';
13:41:19 223  
13:41:19 224  	OPEN out_result_set FOR
13:41:19 225  SELECT * FROM
13:41:19 226  (
13:41:19 227  	SELECT
13:41:19 228  	  LICENSE.ID,
13:41:19 229  	  LICENSE.CREATE_DATE,
13:41:19 230  	  LICENSE.CREATED_BY,
13:41:19 231  	  LICENSE.CURRENT_OFFER_INDEX,
13:41:19 232  	  LICENSE.CURRENT_OFFER_RECURR_NUM,
13:41:19 233  	  LICENSE.END_DATE,
13:41:19 234  	  LICENSE.ENTITLEMENT_END_DATE,
13:41:19 235  	  LICENSE.INVOICE_ID,
13:41:19 236  	  LICENSE.IS_EXTENSION,
13:41:19 237  	  LICENSE.LICENSE_STATUS_ID,
13:41:19 238  	  LICENSE.OFFER_ID,
13:41:19 239  	  LICENSE.START_DATE,
13:41:19 240  	  LICENSE.SUBSCRIPTION_ID,
13:41:19 241  	  LICENSE.UPDATE_DATE,
13:41:19 242  	  LICENSE.UPDATED_BY
13:41:19 243  	FROM
13:41:19 244  	  LICENSE
13:41:19 245  	WHERE
13:41:19 246  	  TO_DATE(LICENSE.END_DATE) <= (current_timestamp + var_time_interval)
13:41:19 247  	  AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V21.LICENSE_ACTIVE
13:41:19 248  	  AND NOT EXISTS
13:41:19 249  	  (
13:41:19 250  	    SELECT NULL
13:41:19 251  	    FROM PROCESS_RETRY_THROTTLE
13:41:19 252  	    WHERE PROCESS_NAME = SPROC_NAME
13:41:19 253  	      AND GENERIC_ID = LICENSE.ID
13:41:19 254  	  )
13:41:19 255  	  AND ROWNUM <= 10000
13:41:19 256  	  ORDER BY dbms_random.value
13:41:19 257  ) WHERE
13:41:19 258  	  ROWNUM <= 1000
13:41:19 259  	  ;
13:41:19 260  EXCEPTION
13:41:19 261  WHEN OTHERS THEN
13:41:19 262  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:19 263  	  SPROC_NAME, 'Unknown error', SQLERRM);
13:41:19 264  END GET_ENDING_LICENSES;
