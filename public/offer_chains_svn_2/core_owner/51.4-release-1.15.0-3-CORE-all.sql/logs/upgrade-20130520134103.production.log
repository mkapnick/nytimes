13:41:20 384  ) AS
13:41:20 385  SPROC_NAME     CONSTANT VARCHAR2(29) := 'GET_ACCOUNT_GIFT_CERTIFICATES';
13:41:20 386  var_account_id NUMBER;
13:41:20 387  
13:41:20 388  -- Exceptions
13:41:20 389  WRONG_INSTR_EXCEPTION	 EXCEPTION;
13:41:20 390  BEGIN
13:41:20 391  
13:41:20 392  	-- Get account id
13:41:20 393  	SELECT
13:41:20 394  	  ACCOUNT.ID INTO var_account_id
13:41:20 395  	FROM
13:41:20 396  	  ACCOUNT
13:41:20 397  	WHERE
13:41:20 398  	  ACCOUNT.GROUP_ID = in_group_id;
13:41:20 399  
13:41:20 400  	-- Check that incoming data is correct
13:41:20 401  	IF in_instr_status != GLOBAL_CONSTANTS_V21.TRUE AND in_instr_status != GLOBAL_CONSTANTS_V21.FALSE THEN
13:41:20 402  	  RAISE WRONG_INSTR_EXCEPTION;
13:41:20 403  	END IF;
13:41:20 404  
13:41:20 405  	OPEN out_result_gc_set FOR
13:41:20 406  	SELECT
13:41:20 407  	  GIFT_CERTIFICATE.ID,
13:41:20 408  	  GIFT_CERTIFICATE.PURCHASER_GROUP_ID,
13:41:20 409  	  GIFT_CERTIFICATE.PURCHASE_INVOICE_ID,
13:41:20 410  	  GIFT_CERTIFICATE.PURCHASE_DATE,
13:41:20 411  	  GIFT_CERTIFICATE.OFFER_CHAIN_ID,
13:41:20 412  	  GIFT_CERTIFICATE.EXPIRATION_DATE,
13:41:20 413  	  GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID,
13:41:20 414  	  GIFT_CERTIFICATE.CODE,
13:41:20 415  	  GIFT_CERTIFICATE.CREATE_DATE,
13:41:20 416  	  GIFT_CERTIFICATE.CREATED_BY,
13:41:20 417  	  GIFT_CERTIFICATE.UPDATE_DATE,
13:41:20 418  	  GIFT_CERTIFICATE.UPDATED_BY,
