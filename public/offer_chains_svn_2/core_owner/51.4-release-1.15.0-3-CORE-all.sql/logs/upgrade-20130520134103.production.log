13:41:19 147  PROCEDURE UPDATE_LICENSE_STATUS(
13:41:19 148  	  in_license_id     IN NUMBER,
13:41:19 149  	  in_license_status IN NUMBER,
13:41:19 150  	  in_updated_by     IN VARCHAR2,
13:41:19 151  	  in_ent_end	    IN NUMBER DEFAULT GLOBAL_CONSTANTS_V21.FALSE
13:41:19 152  ) AS
13:41:19 153  SPROC_NAME CONSTANT VARCHAR2(21) := 'UPDATE_LICENSE_STATUS';
