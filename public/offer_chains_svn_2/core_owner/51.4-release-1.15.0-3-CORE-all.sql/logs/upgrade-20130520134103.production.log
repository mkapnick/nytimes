13:41:20 2823  PROCEDURE ARE_REFUNDS_PENDING_FOR_SUBSCR (
13:41:20 2824  /*
13:41:20 2825  APP_EXCEPTION_CODES_V21.NOT_FOUND
13:41:20 2826  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
13:41:20 2827  */
13:41:20 2828  	 in_subscription_id IN NUMBER,
13:41:20 2829  	 out_result	    OUT NUMBER
13:41:20 2830  ) AS
13:41:20 2831  SPROC_NAME CONSTANT VARCHAR2(30) := 'ARE_REFUNDS_PENDING_FOR_SUBSCR';
13:41:20 2832  -- VARIABLES
13:41:20 2833  temp_subscription_id NUMBER;
13:41:20 2834  var_local_result     NUMBER;
13:41:20 2835  -- EXCEPTIONS
13:41:20 2836  BAD_SUBSCRIPTION_ID EXCEPTION;
13:41:20 2837  BEGIN
13:41:20 2838  
13:41:20 2839  	 -- Check that subscription exists
13:41:20 2840  	 BEGIN
13:41:20 2841  	   SELECT
13:41:20 2842  	     SUBSCRIPTION.ID into temp_subscription_id
13:41:20 2843  	   FROM
13:41:20 2844  	     SUBSCRIPTION
13:41:20 2845  	   WHERE
13:41:20 2846  	     SUBSCRIPTION.ID = in_subscription_id;
13:41:20 2847  	   EXCEPTION
13:41:20 2848  	     WHEN NO_DATA_FOUND THEN
13:41:20 2849  	       RAISE BAD_SUBSCRIPTION_ID;
13:41:20 2850  	 END;
13:41:20 2851  
13:41:20 2852  	 var_local_result := NULL;
13:41:20 2853  
13:41:20 2854  	 -- Check charges for each invoice associated with gived subscription
13:41:20 2855  	 FOR f_invoice IN (
13:41:20 2856  	   SELECT DISTINCT
13:41:20 2857  	     LICENSE.INVOICE_ID as "ID"
13:41:20 2858  	   FROM
13:41:20 2859  	     LICENSE
13:41:20 2860  	   WHERE
13:41:20 2861  	     LICENSE.SUBSCRIPTION_ID = in_subscription_id
13:41:20 2862  	     AND LICENSE.LICENSE_STATUS_ID IN ( SELECT GLOBAL_STATUSES_V21.LICENSE_ACTIVE FROM DUAL )
13:41:20 2863  	 )
13:41:20 2864  	 LOOP
13:41:20 2865  
13:41:20 2866  	   -- Check each charge in invoice
13:41:20 2867  	   FOR f_charge IN (
13:41:20 2868  	     SELECT
13:41:20 2869  	       CHARGE.ID,
13:41:20 2870  	       CHARGE.CHARGE_STATUS_ID,
13:41:20 2871  	       CHARGE.CHARGE_AMOUNT
13:41:20 2872  	     FROM
13:41:20 2873  	       CHARGE
13:41:20 2874  	     WHERE
13:41:20 2875  	       CHARGE.INVOICE_ID = f_invoice.ID
13:41:20 2876  	   )
13:41:20 2877  	   LOOP
13:41:20 2878  
13:41:20 2879  	     -- Charge amount < 0     => it is a refund
13:41:20 2880  	     -- Charge status is OPEN => means that it is not processed yet
13:41:20 2881  	     IF f_charge.CHARGE_AMOUNT < 0
13:41:20 2882  		AND f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V21.CHARGE_OPENED THEN
13:41:20 2883  	       var_local_result := GLOBAL_CONSTANTS_V21.TRUE;
13:41:20 2884  	     END IF;
13:41:20 2885  
13:41:20 2886  	   END LOOP;
13:41:20 2887  
13:41:20 2888  	 END LOOP;
13:41:20 2889  
13:41:20 2890  	 IF var_local_result IS NULL THEN
13:41:20 2891  	   out_result := GLOBAL_CONSTANTS_V21.FALSE;
13:41:20 2892  	 ELSE
13:41:20 2893  	   out_result := GLOBAL_CONSTANTS_V21.TRUE;
13:41:20 2894  	 END IF;
13:41:20 2895  
13:41:20 2896  EXCEPTION
13:41:20 2897  WHEN BAD_SUBSCRIPTION_ID THEN
13:41:20 2898  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 2899  	   SPROC_NAME, 'No such transaction id');
13:41:20 2900  WHEN OTHERS THEN
13:41:20 2901  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 2902  	   SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 2903  END ARE_REFUNDS_PENDING_FOR_SUBSCR;
