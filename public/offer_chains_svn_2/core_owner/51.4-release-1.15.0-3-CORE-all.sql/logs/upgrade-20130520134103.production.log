13:41:20 495  	END;
13:41:20 496  
13:41:20 497  	-- Get group id
13:41:20 498  	SELECT
13:41:20 499  	  ACCOUNT.GROUP_ID into var_group_id
13:41:20 500  	FROM
13:41:20 501  	  ACCOUNT
13:41:20 502  	WHERE
13:41:20 503  	  ACCOUNT.ID = var_account_id;
13:41:20 504  
13:41:20 505  	-- Card should not be disabled
13:41:20 506  	IF var_paypal_status_id = GLOBAL_STATUSES_V21.PAYPAL_INACTIVE THEN
13:41:20 507  	  RAISE PAYPAL_ALREADY_INACTIVE;
13:41:20 508  	END IF;
13:41:20 509  
13:41:20 510  	-- Looking for pending transactions associated with given credit card
13:41:20 511  	SELECT
13:41:20 512  	  COUNT(*) into var_pending_invoices_num
13:41:20 513  	FROM
13:41:20 514  	  CHARGE
13:41:20 515  	  INNER JOIN TRANSACTION ON CHARGE.TRANSACTION_ID = TRANSACTION.ID
13:41:20 516  	WHERE
13:41:20 517  	  CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V21.INSTRUMENT_PAYPAL
13:41:20 518  	  AND CHARGE.INSTRUMENT_ID = in_paypal_id
13:41:20 519  	  AND TRANSACTION.TRANSACTION_STATUS_ID = GLOBAL_STATUSES_V21.TRANSACTION_PENDING;
13:41:20 520  
13:41:20 521  	IF var_pending_invoices_num > 0 THEN
13:41:20 522  	  RAISE PENDING_TRANSACTIONS_FOUND;
13:41:20 523  	END IF;
13:41:20 524  
13:41:20 525  	-- Getting current default financial instrument
13:41:20 526  	BEGIN
13:41:20 527  	  GET_DEF_FINANCIAL_INSTRUMENT(
13:41:20 528  	    in_group_id 	   => var_group_id,
13:41:20 529  	    out_instrument_type_id => current_def_instrument_type_id,
13:41:20 530  	    out_instrument_id	   => current_def_instrument_id
13:41:20 531  	  );
13:41:20 532  	  EXCEPTION
13:41:20 533  	    WHEN OTHERS THEN
13:41:20 534  	      EXCEPTION_MESSAGE := SQLERRM;
13:41:20 535  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
13:41:20 536  	END;
13:41:20 537  
13:41:20 538  	-- Checking that credit card is not default
13:41:20 539  	IF current_def_instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_PAYPAL
13:41:20 540  	  AND current_def_instrument_id = in_paypal_id THEN
13:41:20 541  	  BEGIN
13:41:20 542  	    DEL_DEF_FINANCIAL_INSTRUMENT(
13:41:20 543  	      in_group_id => var_group_id
13:41:20 544  	    );
13:41:20 545  	  EXCEPTION
13:41:20 546  	    WHEN OTHERS THEN
13:41:20 547  	      EXCEPTION_MESSAGE := SQLERRM;
13:41:20 548  	      RAISE CAN_NOT_DEL_DEF_FINANCIAL;
13:41:20 549  	  END;
13:41:20 550  	END IF;
13:41:20 551  
13:41:20 552  	PROCS_FIN_INSTRUMENTS_V21.UPDATE_PAYPAL_STATUS(
13:41:20 553  	  in_paypal_id	      => in_paypal_id,
13:41:20 554  	  in_updated_by       => in_updated_by,
13:41:20 555  	  in_paypal_status_id => GLOBAL_STATUSES_V21.PAYPAL_INACTIVE
13:41:20 556  	);
13:41:20 557  
13:41:20 558  EXCEPTION
13:41:20 559  WHEN BAD_PAYPAL_ID THEN
13:41:20 560  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 561  	  SPROC_NAME, 'No such paypal');
13:41:20 562  WHEN PAYPAL_ALREADY_INACTIVE THEN
13:41:20 563  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
13:41:20 564  	  SPROC_NAME, 'Paypal already inactive');
13:41:20 565  WHEN PENDING_TRANSACTIONS_FOUND THEN
13:41:20 566  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
13:41:20 567  	  SPROC_NAME, 'Account has pending charge which are using this paypal');
13:41:20 568  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
13:41:20 569  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 570  	  SPROC_NAME, 'Could not get current default financial instrument', EXCEPTION_MESSAGE);
13:41:20 571  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
13:41:20 572  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 573  	  SPROC_NAME, 'Could not delete information about default financial instrument from account', EXCEPTION_MESSAGE);
13:41:20 574  WHEN OTHERS THEN
13:41:20 575  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 576  	  SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 577  END DISABLE_PAYPAL;
13:41:20 578  
13:41:20 579  /******************************************************************************/
13:41:20 580  
13:41:20 581  PROCEDURE UPDATE_CREDIT_CARD (
13:41:20 582  /*
13:41:20 583  Throws exceptions:
13:41:20 584  APP_EXCEPTION_CODES_V21.NOT_FOUND
13:41:20 585  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
13:41:20 586  */
13:41:20 587  	in_credit_card_id	  IN NUMBER,
13:41:20 588  	in_updated_by		  IN VARCHAR2,
13:41:20 589  	in_instrument_name	  IN VARCHAR2,
13:41:20 590  	in_is_default		  IN NUMBER
13:41:20 591  ) AS
13:41:20 592  SPROC_NAME CONSTANT VARCHAR2(18) := 'UPDATE_CREDIT_CARD';
13:41:20 593  -- VARIABLES
13:41:20 594  var_account_id NUMBER;
13:41:20 595  var_group_id   NUMBER;
13:41:20 596  temp_cc_rownum NUMBER;
13:41:20 597  current_def_instrument_type_id NUMBER;
13:41:20 598  current_def_instrument_id      NUMBER;
13:41:20 599  -- EXCEPTION
13:41:20 600  CAN_NOT_SET_DEF_FINANCIAL  EXCEPTION;
13:41:20 601  BAD_IS_DEFAULT_VALUE	 EXCEPTION;
13:41:20 602  CAN_NOT_GET_DEF_FINANCIAL  EXCEPTION;
13:41:20 603  CAN_NOT_DEL_DEF_FINANCIAL  EXCEPTION;
13:41:20 604  EXCEPTION_MESSAGE 	 VARCHAR2(1024);
13:41:20 605  BEGIN
13:41:20 606  
13:41:20 607  	-- Get account id
13:41:20 608  	SELECT
13:41:20 609  	  CREDIT_CARD.ACCOUNT_ID
13:41:20 610  	  into
13:41:20 611  	  var_account_id
13:41:20 612  	FROM
13:41:20 613  	  CREDIT_CARD
13:41:20 614  	WHERE
13:41:20 615  	  CREDIT_CARD.ID = in_credit_card_id;
13:41:20 616  
13:41:20 617  	-- Get group id
13:41:20 618  	SELECT
13:41:20 619  	  ACCOUNT.GROUP_ID into var_group_id
13:41:20 620  	FROM
13:41:20 621  	  ACCOUNT
13:41:20 622  	WHERE
13:41:20 623  	  ACCOUNT.ID = var_account_id;
13:41:20 624  
13:41:20 625  	-- Check that passed data is correct
13:41:20 626  	IF in_is_default != GLOBAL_CONSTANTS_V21.TRUE
13:41:20 627  	  AND in_is_default != GLOBAL_CONSTANTS_V21.FALSE
13:41:20 628  	  AND in_is_default IS NOT NULL THEN
13:41:20 629  	  RAISE BAD_IS_DEFAULT_VALUE;
13:41:20 630  	END IF;
13:41:20 631  
13:41:20 632  	-- Update credit card
13:41:20 633  	IF in_instrument_name IS NOT NULL THEN
13:41:20 634  	  PROCS_FIN_INSTRUMENTS_CRU_V21.UPDATE_CREDIT_CARD(
13:41:20 635  	    in_credit_card_id  => in_credit_card_id,
13:41:20 636  	    in_updated_by      => in_updated_by,
13:41:20 637  	    in_instrument_name => in_instrument_name
13:41:20 638  	  );
13:41:20 639  	END IF;
13:41:20 640  
13:41:20 641  	-- Set default financial instrument
13:41:20 642  	IF in_is_default = GLOBAL_CONSTANTS_V21.TRUE THEN
13:41:20 643  	  BEGIN
13:41:20 644  	    PROCS_FIN_INSTRUMENTS_V21.SET_DEF_FINANCIAL_INSTRUMENT(
13:41:20 645  	      in_group_id	    => var_group_id,
13:41:20 646  	      in_instrument_type_id => GLOBAL_ENUMS_V21.INSTRUMENT_CREDIT_CARD,
13:41:20 647  	      in_instrument_id	    => in_credit_card_id,
13:41:20 648  	      in_updated_by	    => in_updated_by
13:41:20 649  	    );
13:41:20 650  	    EXCEPTION
13:41:20 651  	      WHEN OTHERS THEN
13:41:20 652  		EXCEPTION_MESSAGE := SQLERRM;
13:41:20 653  		RAISE CAN_NOT_SET_DEF_FINANCIAL;
13:41:20 654  	  END;
13:41:20 655  	END IF;
13:41:20 656  
13:41:20 657  	-- Set default financial instrument
13:41:20 658  	IF in_is_default = GLOBAL_CONSTANTS_V21.FALSE THEN
13:41:20 659  	  BEGIN
13:41:20 660  	    GET_DEF_FINANCIAL_INSTRUMENT(
13:41:20 661  	      in_group_id	     => var_group_id,
13:41:20 662  	      out_instrument_type_id => current_def_instrument_type_id,
13:41:20 663  	      out_instrument_id      => current_def_instrument_id
13:41:20 664  	    );
13:41:20 665  	  EXCEPTION
13:41:20 666  	    WHEN OTHERS THEN
13:41:20 667  	      EXCEPTION_MESSAGE := SQLERRM;
13:41:20 668  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
13:41:20 669  	  END;
13:41:20 670  	  IF current_def_instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_CREDIT_CARD
13:41:20 671  	    AND current_def_instrument_id = in_credit_card_id THEN
13:41:20 672  	    BEGIN
13:41:20 673  	      DEL_DEF_FINANCIAL_INSTRUMENT(
13:41:20 674  		in_group_id => var_group_id
13:41:20 675  	      );
13:41:20 676  	      EXCEPTION
13:41:20 677  		WHEN OTHERS THEN
13:41:20 678  		  EXCEPTION_MESSAGE := SQLERRM;
13:41:20 679  		  RAISE CAN_NOT_DEL_DEF_FINANCIAL;
13:41:20 680  	    END;
13:41:20 681  	  END IF;
13:41:20 682  	END IF;
13:41:20 683  
13:41:20 684  EXCEPTION
13:41:20 685  WHEN NO_DATA_FOUND THEN
13:41:20 686  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 687  	  SPROC_NAME, 'No such credit card');
13:41:20 688  WHEN CAN_NOT_SET_DEF_FINANCIAL THEN
13:41:20 689  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 690  	  SPROC_NAME, 'Could not set default financial instrument for account', EXCEPTION_MESSAGE);
13:41:20 691  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
13:41:20 692  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 693  	  SPROC_NAME, 'Could not get default financial instrument for account', EXCEPTION_MESSAGE);
13:41:20 694  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
13:41:20 695  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 696  	  SPROC_NAME, 'Could not delete information about default financial instrument', EXCEPTION_MESSAGE);
13:41:20 697  WHEN OTHERS THEN
13:41:20 698  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 699  	  SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 700  END UPDATE_CREDIT_CARD;
13:41:20 701  
13:41:20 702  /******************************************************************************/
13:41:20 703  
13:41:20 704  PROCEDURE START_GC_PURCHASING (
13:41:20 705  	in_group_id		  IN NUMBER,
13:41:20 706  	in_offer_chain_id	  IN VARCHAR2,
13:41:20 707  	in_gift_certificate_code  IN  VARCHAR2,
13:41:20 708  	in_created_by		  IN  VARCHAR2,
13:41:20 709  	in_recipient_name	  IN  VARCHAR2,
13:41:20 710  	in_recipient_email	  IN  VARCHAR2,
13:41:20 711  	in_recipient_address_id   IN NUMBER,
13:41:20 712  	in_recipient_notify_date  IN DATE,
13:41:20 713  	in_sender_name		  IN VARCHAR2,
13:41:20 714  	in_sender_email 	  IN VARCHAR2,
13:41:20 715  	in_gift_message 	  IN  VARCHAR2,
13:41:20 716  	in_expiration_date	  IN DATE,
13:41:20 717  	out_gift_certificate_id   OUT NUMBER,
13:41:20 718  	out_invoice_id		  OUT NUMBER
13:41:20 719  ) AS
13:41:20 720  SPROC_NAME CONSTANT VARCHAR2(19) := 'START_GC_PURCHASING';
13:41:20 721  -- VARIABLES
13:41:20 722  var_account_id		NUMBER;
13:41:20 723  temp_gc_code		GIFT_CERTIFICATE.CODE%TYPE;
13:41:20 724  var_och_is_gc		NUMBER;
13:41:20 725  var_offer_chain_status_id NUMBER;
13:41:20 726  var_is_for_redemption	NUMBER;
13:41:20 727  var_new_invoice_id	NUMBER;
13:41:20 728  var_gift_cert_id		NUMBER;
13:41:20 729  var_account_tax_exempt_id VARCHAR2(255);
13:41:20 730  -- EXCEPTIONS
13:41:20 731  BAD_GROUP_ID		    EXCEPTION;
13:41:20 732  GC_CODE_ALREADY_EXISTS	    EXCEPTION;
13:41:20 733  BAD_OFFER_CHAIN_ID	    EXCEPTION;
13:41:20 734  OCH_IS_NOT_GIFT_CERTIFICATE   EXCEPTION;
13:41:20 735  BAD_OFFER_CHAIN_STATUS	    EXCEPTION;
13:41:20 736  CAN_NOT_PURCHASE_GC_FOR_RDMPN EXCEPTION;
13:41:20 737  CAN_NOT_CREATE_INVOICE	    EXCEPTION;
13:41:20 738  OFFER_REC_NUM_LESS_THAN_ONE   EXCEPTION;
13:41:20 739  CAN_NOT_CREATE_LINE_ITEMS     EXCEPTION;
13:41:20 740  
13:41:20 741  EXCEPTION_MESSAGE VARCHAR2(1024);
13:41:20 742  BEGIN
13:41:20 743  	-- Get account id
13:41:20 744  	BEGIN
13:41:20 745  	  SELECT
13:41:20 746  	    ACCOUNT.ID,
13:41:20 747  	    ACCOUNT.TAX_EXEMPT_ID
13:41:20 748  	    into
13:41:20 749  	    var_account_id,
13:41:20 750  	    var_account_tax_exempt_id
13:41:20 751  	  FROM
13:41:20 752  	    ACCOUNT
13:41:20 753  	  WHERE
13:41:20 754  	    ACCOUNT.GROUP_ID = in_group_id;
13:41:20 755  	  EXCEPTION
13:41:20 756  	  WHEN NO_DATA_FOUND THEN
13:41:20 757  	    RAISE BAD_GROUP_ID;
13:41:20 758  	END;
13:41:20 759  
13:41:20 760  	-- Check for the same code
13:41:20 761  	BEGIN
13:41:20 762  	  SELECT
13:41:20 763  	    GIFT_CERTIFICATE.CODE into temp_gc_code
13:41:20 764  	  FROM
13:41:20 765  	    GIFT_CERTIFICATE
13:41:20 766  	  WHERE
13:41:20 767  	    GIFT_CERTIFICATE.CODE = in_gift_certificate_code;
13:41:20 768  
13:41:20 769  	  RAISE GC_CODE_ALREADY_EXISTS;
13:41:20 770  
13:41:20 771  	  EXCEPTION
13:41:20 772  	    WHEN NO_DATA_FOUND THEN
13:41:20 773  	      NULL;
13:41:20 774  	END;
13:41:20 775  
13:41:20 776  	-- Get offer chain flag "is_gift_certificate"
13:41:20 777  	BEGIN
13:41:20 778  	  SELECT
13:41:20 779  	    OFFER_CHAIN.IS_GIFT_CERTIFICATE,
13:41:20 780  	    OFFER_CHAIN.OFFER_CHAIN_STATUS_ID
13:41:20 781  	    into
13:41:20 782  	    var_och_is_gc,
13:41:20 783  	    var_offer_chain_status_id
13:41:20 784  	  FROM
13:41:20 785  	    OFFER_CHAIN
13:41:20 786  	  WHERE
13:41:20 787  	    OFFER_CHAIN.ID = in_offer_chain_id;
13:41:20 788  	  EXCEPTION
13:41:20 789  	  WHEN NO_DATA_FOUND THEN
13:41:20 790  	    RAISE BAD_OFFER_CHAIN_ID;
13:41:20 791  	END;
13:41:20 792  
13:41:20 793  	IF var_och_is_gc != GLOBAL_CONSTANTS_V21.TRUE
13:41:20 794  	  OR var_och_is_gc IS NULL THEN
13:41:20 795  	  RAISE OCH_IS_NOT_GIFT_CERTIFICATE;
13:41:20 796  	END IF;
13:41:20 797  
13:41:20 798  	IF var_offer_chain_status_id != GLOBAL_STATUSES_V21.OFFER_CHAIN_ACTIVE THEN
13:41:20 799  	  RAISE BAD_OFFER_CHAIN_STATUS;
13:41:20 800  	END IF;
13:41:20 801  
13:41:20 802  	-- norlov: #38151 check if the OC is for Redemption:
13:41:20 803  	SELECT
13:41:20 804  	  COUNT(*) into var_is_for_redemption
13:41:20 805  	FROM
13:41:20 806  	  OFFER_CHAIN_ELIGIBILITY
13:41:20 807  	WHERE
13:41:20 808  	  OFFER_CHAIN_ELIGIBILITY.OFFER_CHAIN_ID = in_offer_chain_id
13:41:20 809  	  AND OFFER_CHAIN_ELIGIBILITY.NAME = GLOBAL_CONSTANTS_V21.GIFT_CERTIFICATE_REQUIRED
13:41:20 810  	  AND OFFER_CHAIN_ELIGIBILITY.VALUE = GLOBAL_CONSTANTS_V21.ELIGIBILITY_FLAG_SET;
13:41:20 811  
13:41:20 812  	IF var_is_for_redemption > 0 THEN
13:41:20 813  	  RAISE CAN_NOT_PURCHASE_GC_FOR_RDMPN;
13:41:20 814  	END IF;
13:41:20 815  
13:41:20 816  	-- Create new invoice
13:41:20 817  	BEGIN
13:41:20 818  	  PROCS_INVOICE_V21.CREATE_INVOICE(
13:41:20 819  	    in_invoice_status => GLOBAL_STATUSES_V21.INVOICE_OPEN,
13:41:20 820  	    in_created_by     => in_created_by,
13:41:20 821  	    in_tax_exempt_id  => var_account_tax_exempt_id,
13:41:20 822  	    out_invoice_id    => var_new_invoice_id
13:41:20 823  	  );
13:41:20 824  	  EXCEPTION
13:41:20 825  	    WHEN OTHERS THEN
13:41:20 826  	      EXCEPTION_MESSAGE := SQLERRM;
13:41:20 827  	      RAISE CAN_NOT_CREATE_INVOICE;
13:41:20 828  	END;
13:41:20 829  
13:41:20 830  	-- Add line items for new invoice
13:41:20 831  	BEGIN
13:41:20 832  	  FOR f_offer_data IN (
13:41:20 833  	    SELECT
13:41:20 834  	      OFFER_ID,
13:41:20 835  	      NUM_RECURRENCES
13:41:20 836  	    FROM
13:41:20 837  	      OFFER_OFFER_CHAIN
13:41:20 838  	    WHERE
13:41:20 839  	      OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
13:41:20 840  	  )
13:41:20 841  	  LOOP
13:41:20 842  	    IF f_offer_data.NUM_RECURRENCES < 1 THEN
13:41:20 843  	      RAISE OFFER_REC_NUM_LESS_THAN_ONE;
13:41:20 844  	    END IF;
13:41:20 845  	    FOR i_offer_recurrences_iterator IN 1..f_offer_data.NUM_RECURRENCES
13:41:20 846  	    LOOP
13:41:20 847  	      PROCS_LINE_ITEMS_V21.ADD_LINE_ITEMS(
13:41:20 848  		in_invoice_id => var_new_invoice_id,
13:41:20 849  		in_offer_id   => f_offer_data.OFFER_ID,
13:41:20 850  		in_created_by => in_created_by
13:41:20 851  	      );
13:41:20 852  	    END LOOP;
13:41:20 853  	  END LOOP;
13:41:20 854  
13:41:20 855  	  EXCEPTION
13:41:20 856  	    WHEN OTHERS THEN
13:41:20 857  	      EXCEPTION_MESSAGE := SQLERRM;
13:41:20 858  	      RAISE CAN_NOT_CREATE_LINE_ITEMS;
13:41:20 859  	END;
13:41:20 860  
13:41:20 861  	PROCS_FIN_INSTRUMENTS_CRU_V21.CREATE_GIFT_CERTIFICATE(
13:41:20 862  	  out_gift_certificate_id	=> var_gift_cert_id,
13:41:20 863  	  in_purchaser_group_id 	=> in_group_id,
13:41:20 864  	  in_purchaser_invoice_id	=> var_new_invoice_id,
13:41:20 865  	  in_offer_chain_id		=> in_offer_chain_id,
13:41:20 866  	  in_expiration_date		=> in_expiration_date,
13:41:20 867  	  in_purchase_date		=> SYSDATE,
13:41:20 868  	  in_gift_certificate_status_id => GLOBAL_STATUSES_V21.GIFT_CERTIFICATE_ACTIVE,
13:41:20 869  	  in_code			=> in_gift_certificate_code,
13:41:20 870  	  in_created_by 		=> in_created_by,
13:41:20 871  	  in_recipient_name		=> in_recipient_name,
13:41:20 872  	  in_gift_message		=> in_gift_message,
13:41:20 873  	  in_recipient_email		=> in_recipient_email,
13:41:20 874  	  in_sender_email		=> in_sender_email,
13:41:20 875  	  in_sender_name		=> in_sender_name,
13:41:20 876  	  in_recipient_address_id	=> in_recipient_address_id,
13:41:20 877  	  in_recipient_notify_date	=> in_recipient_notify_date
13:41:20 878  	);
13:41:20 879  
13:41:20 880  	out_gift_certificate_id := var_gift_cert_id;
13:41:20 881  	out_invoice_id := var_new_invoice_id;
13:41:20 882  
13:41:20 883  EXCEPTION
13:41:20 884  WHEN BAD_OFFER_CHAIN_STATUS THEN
13:41:20 885  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
13:41:20 886  	  SPROC_NAME, 'Offer chain is not active');
13:41:20 887  WHEN GC_CODE_ALREADY_EXISTS THEN
13:41:20 888  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.DUPLICATE_ERROR,
13:41:20 889  	  SPROC_NAME, 'Gift certificate with same code already exists');
13:41:20 890  WHEN OCH_IS_NOT_GIFT_CERTIFICATE THEN
13:41:20 891  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INVALID_PARAMETER,
13:41:20 892  	  SPROC_NAME, 'This offer chain can not be used for gift certificate');
13:41:20 893  WHEN CAN_NOT_PURCHASE_GC_FOR_RDMPN THEN
13:41:20 894  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INVALID_PARAMETER,
13:41:20 895  	  SPROC_NAME, 'This offer chain can not be purchased for gift certificate since it is for redemption');
13:41:20 896  WHEN CAN_NOT_CREATE_INVOICE THEN
13:41:20 897  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 898  	  SPROC_NAME, 'Could not create new invoice', EXCEPTION_MESSAGE);
13:41:20 899  WHEN CAN_NOT_CREATE_LINE_ITEMS THEN
13:41:20 900  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 901  	  SPROC_NAME, 'Could not create line items', EXCEPTION_MESSAGE);
13:41:20 902  WHEN BAD_GROUP_ID THEN
13:41:20 903  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 904  	  SPROC_NAME, 'No such group id');
13:41:20 905  WHEN BAD_OFFER_CHAIN_ID THEN
13:41:20 906  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 907  	  SPROC_NAME, 'No such offer chain');
13:41:20 908  WHEN OFFER_REC_NUM_LESS_THAN_ONE THEN
13:41:20 909  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 910  	  SPROC_NAME, 'Some offer has recurrences number less than 1');
13:41:20 911  WHEN OTHERS THEN
13:41:20 912  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 913  	  SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 914  END START_GC_PURCHASING;
