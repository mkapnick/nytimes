13:41:20  24  BAD_INVOICE_ID	     EXCEPTION;
13:41:20  25  BAD_TRANSACTION_ID     EXCEPTION;
13:41:20  26  BAD_PAYPAL_ID	     EXCEPTION;
13:41:20  27  BAD_CREDIT_CARD_ID     EXCEPTION;
13:41:20  28  BAD_INSTRUMENT_TYPE    EXCEPTION;
13:41:20  29  BAD_CHARGE_STATUS_ID   EXCEPTION;
13:41:20  30  EXCEPTION_MESSAGE      VARCHAR2(1024);
13:41:20  31  BEGIN
13:41:20  32  	-- Check that incoming data is correct
13:41:20  33  	IF in_instrument_type_id != GLOBAL_ENUMS_V21.INSTRUMENT_CREDIT_CARD
13:41:20  34  	  AND in_instrument_type_id != GLOBAL_ENUMS_V21.INSTRUMENT_PAYPAL THEN
13:41:20  35  	  RAISE BAD_INSTRUMENT_TYPE;
13:41:20  36  	END IF;
13:41:20  37  
13:41:20  38  	-- Check that status is correct
13:41:20  39  	IF in_charge_status_id != GLOBAL_STATUSES_V21.CHARGE_OPENED
13:41:20  40  	  AND in_charge_status_id != GLOBAL_STATUSES_V21.CHARGE_PROCESSED
13:41:20  41  	  AND in_charge_status_id != GLOBAL_STATUSES_V21.CHARGE_CANCELED THEN
13:41:20  42  	  RAISE BAD_CHARGE_STATUS_ID;
13:41:20  43  	END IF;
13:41:20  44  
13:41:20  45  	-- Check that invoice exists
13:41:20  46  	BEGIN
13:41:20  47  	  SELECT
13:41:20  48  	    INVOICE.ID into temp_invoice_id
13:41:20  49  	  FROM
13:41:20  50  	    INVOICE
13:41:20  51  	  WHERE
13:41:20  52  	    INVOICE.ID = in_invoice_id;
13:41:20  53  	  EXCEPTION
13:41:20  54  	    WHEN NO_DATA_FOUND THEN
13:41:20  55  	      RAISE BAD_INVOICE_ID;
13:41:20  56  	END;
13:41:20  57  
13:41:20  58  	-- Check that transaction exists
13:41:20  59  	BEGIN
13:41:20  60  	  SELECT
13:41:20  61  	    TRANSACTION.ID into temp_transaction_id
13:41:20  62  	  FROM
13:41:20  63  	    TRANSACTION
13:41:20  64  	  WHERE
13:41:20  65  	    TRANSACTION.ID = in_transaction_id;
13:41:20  66  	  EXCEPTION
13:41:20  67  	    WHEN NO_DATA_FOUND THEN
13:41:20  68  	      RAISE BAD_TRANSACTION_ID;
13:41:20  69  	END;
13:41:20  70  
13:41:20  71  	-- Check that instrument exists
13:41:20  72  	IF in_instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_CREDIT_CARD THEN
13:41:20  73  	  IF PROCS_FIN_INSTRUMENTS_V21.IS_CREDIT_CARD_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V21.FALSE THEN
13:41:20  74  	    RAISE BAD_CREDIT_CARD_ID;
13:41:20  75  	  END IF;
13:41:20  76  	ELSIF in_instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_PAYPAL THEN
13:41:20  77  	  IF PROCS_FIN_INSTRUMENTS_V21.IS_PAYPAL_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V21.FALSE THEN
13:41:20  78  	    RAISE BAD_PAYPAL_ID;
13:41:20  79  	  END IF;
13:41:20  80  	END IF;
13:41:20  81  
13:41:20  82  	-- Create new charge
13:41:20  83  	PROCS_CHARGE_CRU_V21.CREATE_CHARGE(
13:41:20  84  	  out_charge_id 	=> var_new_charge_id,
13:41:20  85  	  in_invoice_id 	=> in_invoice_id,
13:41:20  86  	  in_transaction_id	=> in_transaction_id,
13:41:20  87  	  in_instrument_type_id => in_instrument_type_id,
13:41:20  88  	  in_instrument_id	=> in_instrument_id,
13:41:20  89  	  in_charge_amount	=> in_charge_amount,
13:41:20  90  	  in_charge_status_id	=> in_charge_status_id,
13:41:20  91  	  in_created_by 	=> in_created_by
13:41:20  92  	);
13:41:20  93  
13:41:20  94  	out_charge_id := var_new_charge_id;
13:41:20  95  
13:41:20  96  EXCEPTION
13:41:20  97  WHEN BAD_CHARGE_STATUS_ID THEN
13:41:20  98  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INVALID_PARAMETER,
13:41:20  99  	  PROCS_NAME, 'Bad charge status: '||in_charge_status_id);
13:41:20 100  WHEN BAD_INSTRUMENT_TYPE THEN
13:41:20 101  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INVALID_PARAMETER,
13:41:20 102  	  PROCS_NAME, 'Bad instrument type id');
13:41:20 103  WHEN BAD_INVOICE_ID THEN
13:41:20 104  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 105  	  PROCS_NAME, 'No such invoice');
13:41:20 106  WHEN BAD_TRANSACTION_ID THEN
13:41:20 107  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 108  	  PROCS_NAME, 'No such transaction');
13:41:20 109  WHEN BAD_PAYPAL_ID THEN
13:41:20 110  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 111  	  PROCS_NAME, 'No such paypal');
13:41:20 112  WHEN BAD_CREDIT_CARD_ID THEN
13:41:20 113  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 114  	  PROCS_NAME, 'No such credit card');
13:41:20 115  WHEN OTHERS THEN
13:41:20 116  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 117  	  PROCS_NAME, 'Unknown error', SQLERRM);
13:41:20 118  END CREATE_CHARGE;
