13:41:18  37  			    LICENSE l INNER JOIN SUBSCRIPTION s ON L.SUBSCRIPTION_ID = s.ID
13:41:18  38  				      INNER JOIN CREDIT_CARD cc ON S.INSTRUMENT_ID   = cc.ID
13:41:18  39  			  WHERE
13:41:18  40  			    s.INSTRUMENT_TYPE_ID	 = 1
13:41:18  41  			    AND cc.CREDIT_CARD_STATUS_ID = 1
13:41:18  42  			    AND s.SUBSCRIPTION_STATUS_ID = 1
13:41:18  43  			    AND l.LICENSE_STATUS_ID	 = 2
13:41:18  44  			    AND cc.CREDIT_CARD_TYPE_ID IN (2,3)
13:41:18  45  			    AND l.END_DATE BETWEEN var_start_date AND var_end_date
13:41:18  46  			    AND l.ID NOT IN (SELECT LICENSE_ID FROM CC_UPDATE))
13:41:18  47  	   LOOP
13:41:18  48  	     var_license_count := 0;
13:41:18  49  	     SELECT COUNT(1) INTO  var_license_count FROM CC_UPDATE WHERE LICENSE_ID = record.LICENSE_ID;
13:41:18  50  
13:41:18  51  	     IF var_license_count = 0 THEN
13:41:18  52  		INSERT INTO CC_UPDATE (ID,
13:41:18  53  				       CREDIT_CARD_ID,
13:41:18  54  				       LICENSE_ID,
13:41:18  55  				       CC_UPDATE_STATUS,
13:41:18  56  				       CC_REQUEST_FILE_ID,
13:41:18  57  				       CREATE_DATE,
13:41:18  58  				       UPDATE_DATE,
