13:41:19 1832  		   EXCEPTION_MESSAGE := SQLERRM;
13:41:19 1833  		   RAISE CAN_NOT_CHECK_SAME_PRODUCTS;
13:41:19 1834  	     END;
13:41:19 1835  	   END LOOP;
13:41:19 1836  	 END IF;
13:41:19 1837  	 out_is_eligible := var_is_eligible;
