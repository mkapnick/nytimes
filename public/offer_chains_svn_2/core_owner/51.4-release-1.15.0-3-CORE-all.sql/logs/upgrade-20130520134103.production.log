13:41:20 1097  
13:41:20 1098  PROCEDURE GET_INVOICE_DAYS_USED_NUMBER (
13:41:20 1099  	 in_invoice_id	     IN NUMBER,
13:41:20 1100  	 in_chargeback_date  IN DATE DEFAULT SYSDATE,
13:41:20 1101  	 out_days_num	     OUT NUMBER
13:41:20 1102  ) AS
13:41:20 1103  SPROC_NAME CONSTANT VARCHAR2(28) := 'GET_INVOICE_DAYS_USED_NUMBER';
13:41:20 1104  -- VARIABLES
13:41:20 1105  temp_invoice_id	      NUMBER;
13:41:20 1106  var_license_start_date DATE;
13:41:20 1107  var_license_end_date   DATE;
13:41:20 1108  var_chargeback_date    DATE;
13:41:20 1109  var_invoice_days_used  NUMBER;
13:41:20 1110  -- EXCEPTIONS
13:41:20 1111  BAD_INVOICE_ID EXCEPTION;
13:41:20 1112  BEGIN
13:41:20 1113  
13:41:20 1114  	 var_chargeback_date := NVL(in_chargeback_date, SYSDATE);
13:41:20 1115  
13:41:20 1116  	 BEGIN
13:41:20 1117  	   SELECT
13:41:20 1118  	     INVOICE.ID into temp_invoice_id
13:41:20 1119  	   FROM
13:41:20 1120  	     INVOICE
13:41:20 1121  	   WHERE
13:41:20 1122  	     INVOICE.ID = in_invoice_id;
13:41:20 1123  	   EXCEPTION
13:41:20 1124  	     WHEN NO_DATA_FOUND THEN
13:41:20 1125  	       RAISE BAD_INVOICE_ID;
13:41:20 1126  	 END;
13:41:20 1127  
13:41:20 1128  	 var_invoice_days_used := 0;
13:41:20 1129  
13:41:20 1130  	 FOR f_license IN (
13:41:20 1131  	   SELECT
13:41:20 1132  	     LICENSE.START_DATE,
13:41:20 1133  	     LICENSE.END_DATE
13:41:20 1134  	   FROM
13:41:20 1135  	     LICENSE
13:41:20 1136  	   WHERE
13:41:20 1137  	     LICENSE.INVOICE_ID = in_invoice_id
13:41:20 1138  	     AND LICENSE.IS_EXTENSION = GLOBAL_CONSTANTS_V21.FALSE
13:41:20 1139  	 )
13:41:20 1140  	 LOOP
13:41:20 1141  	   var_license_start_date := PROCS_COMMON_V21.NORMALIZE_DATE(f_license.START_DATE);
13:41:20 1142  	   var_license_end_date := PROCS_COMMON_V21.NORMALIZE_DATE(f_license.END_DATE);
13:41:20 1143  
13:41:20 1144  	   IF var_license_start_date <= var_chargeback_date THEN
13:41:20 1145  	     IF var_license_end_date <= var_chargeback_date THEN
13:41:20 1146  	       -- License is passed
13:41:20 1147  	       var_invoice_days_used := var_invoice_days_used + (var_license_end_date - var_license_start_date);
13:41:20 1148  	     ELSE
13:41:20 1149  	       -- This is current license
13:41:20 1150  	       var_invoice_days_used := var_invoice_days_used + (var_chargeback_date - var_license_start_date);
13:41:20 1151  	     END IF;
13:41:20 1152  	   ELSE
13:41:20 1153  	     -- if var_license_start_date > in_chargeback_date then do nothing
13:41:20 1154  	     NULL;
13:41:20 1155  	   END IF;
13:41:20 1156  	 END LOOP;
13:41:20 1157  
13:41:20 1158  	 out_days_num := var_invoice_days_used;
13:41:20 1159  
13:41:20 1160  EXCEPTION
13:41:20 1161  WHEN BAD_INVOICE_ID THEN
13:41:20 1162  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 1163  	   SPROC_NAME, 'No such invoice');
13:41:20 1164  WHEN OTHERS THEN
13:41:20 1165  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 1166  	   SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 1167  END GET_INVOICE_DAYS_USED_NUMBER;
