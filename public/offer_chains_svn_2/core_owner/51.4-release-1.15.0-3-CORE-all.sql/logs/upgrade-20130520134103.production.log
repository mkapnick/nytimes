13:41:19 328  
13:41:19 329  PROCEDURE GET_RECURRING_OFFER (
13:41:19 330  	in_license_id  IN NUMBER,
13:41:19 331  	out_result_set OUT SYS_REFCURSOR
13:41:19 332  ) AS
13:41:19 333  -- VARIABLES
13:41:19 334  SPROC_NAME		    CONSTANT VARCHAR2(19) := 'GET_RECURRING_OFFER';
13:41:19 335  var_offer_chain_id	    NUMBER;
13:41:19 336  var_offer_id		    NUMBER;
13:41:19 337  var_offer_num_recurrences     NUMBER;
13:41:19 338  var_license_cur_offer_rec_num NUMBER;
13:41:19 339  var_offer_index		    NUMBER;
13:41:19 340  -- EXCEPTIONS
13:41:19 341  BAD_LICENSE_ID	     EXCEPTION;
13:41:19 342  CAN_NOT_GET_OFFER_INFO EXCEPTION;
13:41:19 343  BEGIN
13:41:19 344  
13:41:19 345  	BEGIN
13:41:19 346  	  SELECT
13:41:19 347  	    SUBSCRIPTION.OFFER_CHAIN_ID,
13:41:19 348  	    LICENSE.OFFER_ID,
13:41:19 349  	    LICENSE.CURRENT_OFFER_RECURR_NUM
13:41:19 350  	    into
13:41:19 351  	    var_offer_chain_id,
13:41:19 352  	    var_offer_id,
13:41:19 353  	    var_license_cur_offer_rec_num
13:41:19 354  	  FROM
13:41:19 355  	    LICENSE
13:41:19 356  	    INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
13:41:19 357  	  WHERE
13:41:19 358  	    LICENSE.ID = in_license_id;
13:41:19 359  	  EXCEPTION
13:41:19 360  	    WHEN NO_DATA_FOUND THEN
13:41:19 361  	      RAISE BAD_LICENSE_ID;
13:41:19 362  	END;
13:41:19 363  
13:41:19 364  	BEGIN
13:41:19 365  	  SELECT
13:41:19 366  	    OFFER_OFFER_CHAIN.NUM_RECURRENCES,
13:41:19 367  	    OFFER_OFFER_CHAIN.INDEX_VALUE
13:41:19 368  	    into
13:41:19 369  	    var_offer_num_recurrences,
13:41:19 370  	    var_offer_index
13:41:19 371  	  FROM
13:41:19 372  	    OFFER_OFFER_CHAIN
13:41:19 373  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
13:41:19 374  	  WHERE
13:41:19 375  	    OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = var_offer_chain_id
13:41:19 376  	    AND OFFER_OFFER_CHAIN.OFFER_ID = var_offer_id;
13:41:19 377  	  EXCEPTION
13:41:19 378  	    WHEN NO_DATA_FOUND THEN
13:41:19 379  	      RAISE CAN_NOT_GET_OFFER_INFO;
13:41:19 380  	END;
13:41:19 381  
13:41:19 382  	IF var_offer_num_recurrences = 0 THEN
13:41:19 383  	  --out_result_set := NULL;
13:41:19 384  	  -- TODO: Remove this hardcode
13:41:19 385  	  OPEN out_result_set FOR
13:41:19 386  	  SELECT
13:41:19 387  	    OFFER.ID,
13:41:19 388  	    OFFER.OFFER_STATUS_ID,
13:41:19 389  	    OFFER.ENTITLEMENT_DURATION,
13:41:19 390  	    OFFER.CREATED_BY,
13:41:19 391  	    OFFER.CREATE_DATE,
13:41:19 392  	    OFFER.UPDATED_BY,
13:41:19 393  	    OFFER.UPDATE_DATE,
13:41:19 394  	    var_offer_num_recurrences as "RECURRENCE_NUMBER",
13:41:19 395  	    var_offer_index as "OFFER_INDEX"
13:41:19 396  	  FROM
13:41:19 397  	    OFFER_OFFER_CHAIN
13:41:19 398  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
13:41:19 399  	  WHERE ROWNUM = 0;
13:41:19 400  	ELSIF var_license_cur_offer_rec_num = var_offer_num_recurrences THEN
13:41:19 401  	  --out_result_set := NULL;
13:41:19 402  	  -- TODO: Remove this hardcode
13:41:19 403  	  OPEN out_result_set FOR
13:41:19 404  	  SELECT
13:41:19 405  	    OFFER.ID,
13:41:19 406  	    OFFER.OFFER_STATUS_ID,
13:41:19 407  	    OFFER.ENTITLEMENT_DURATION,
13:41:19 408  	    OFFER.CREATED_BY,
13:41:19 409  	    OFFER.CREATE_DATE,
13:41:19 410  	    OFFER.UPDATED_BY,
13:41:19 411  	    OFFER.UPDATE_DATE,
13:41:19 412  	    var_offer_num_recurrences as "RECURRENCE_NUMBER",
13:41:19 413  	    var_offer_index as "OFFER_INDEX"
13:41:19 414  	  FROM
13:41:19 415  	    OFFER_OFFER_CHAIN
13:41:19 416  	    INNER JOIN OFFER ON OFFER_OFFER_CHAIN.OFFER_ID = OFFER.ID
13:41:19 417  	  WHERE ROWNUM = 0;
13:41:19 418  	ELSE
13:41:19 419  	  OPEN out_result_set FOR
13:41:19 420  	  SELECT
13:41:19 421  	    OFFER.ID,
13:41:19 422  	    OFFER.OFFER_STATUS_ID,
13:41:19 423  	    OFFER.ENTITLEMENT_DURATION,
13:41:19 424  	    OFFER.CREATED_BY,
13:41:19 425  	    OFFER.CREATE_DATE,
13:41:19 426  	    OFFER.UPDATED_BY,
13:41:19 427  	    OFFER.UPDATE_DATE,
13:41:19 428  	    var_offer_num_recurrences as "RECURRENCE_NUMBER",
13:41:19 429  	    var_offer_index as "OFFER_INDEX"
13:41:19 430  	  FROM
13:41:19 431  	    OFFER
13:41:19 432  	  WHERE
13:41:19 433  	    OFFER.ID = var_offer_id;
13:41:19 434  	END IF;
13:41:19 435  
13:41:19 436  EXCEPTION
13:41:19 437  WHEN BAD_LICENSE_ID THEN
13:41:19 438  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:19 439  	  SPROC_NAME, 'No such license');
13:41:19 440  WHEN CAN_NOT_GET_OFFER_INFO THEN
13:41:19 441  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:19 442  	  SPROC_NAME, 'Could not get offer information. Offer id = '||var_offer_id||', Offer chain id = '||var_offer_chain_id);
13:41:19 443  WHEN OTHERS THEN
13:41:19 444  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:19 445  	  SPROC_NAME, 'Unknown error', SQLERRM);
13:41:19 446  END GET_RECURRING_OFFER;
