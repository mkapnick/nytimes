13:41:20 453  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INVALID_PARAMETER,
13:41:20 454  	  SPROC_NAME, 'Wrong gift certificate type');
13:41:20 455  WHEN OTHERS THEN
13:41:20 456  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 457  	  SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 458  END GET_ACCOUNT_GIFT_CERTIFICATES;
