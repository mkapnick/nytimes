13:41:20 416  
13:41:20 417  PROCEDURE FINALIZE_SUBSCRIPTION_CREATION (
13:41:20 418  	in_subscription_id    IN NUMBER,
13:41:20 419  	in_invoice_id	      IN NUMBER,
13:41:20 420  	in_instrument_type_id IN NUMBER,
13:41:20 421  	in_instrument_id      IN NUMBER,
13:41:20 422  	in_created_by	      IN VARCHAR2
13:41:20 423  ) AS
13:41:20 424  SPROC_NAME CONSTANT VARCHAR2(30) := 'FINALIZE_SUBSCRIPTION_CREATION';
13:41:20 425  -- VARIABLES
13:41:20 426  var_invoice_amount     NUMBER(10, 2);
13:41:20 427  var_new_transaction_id NUMBER;
13:41:20 428  var_new_charge_id      NUMBER;
13:41:20 429  -- EXCEPTIONS
13:41:20 430  CAN_NOT_USE_FCINSTR	  EXCEPTION;
13:41:20 431  CAN_NOT_CALC_INVOICE_AMOUNT EXCEPTION;
13:41:20 432  CAN_NOT_CREATE_TRANSACTION  EXCEPTION;
13:41:20 433  CAN_NOT_CREATE_CHARGE	  EXCEPTION;
13:41:20 434  
13:41:20 435  EXCEPTION_MESSAGE VARCHAR2(1024);
13:41:20 436  BEGIN
13:41:20 437  
13:41:20 438  	-- Calculate invoice amount ( + discounts, taxes)
13:41:20 439  	BEGIN
13:41:20 440  	  PROCS_INVOICE_V21.CALCULATE_INVOICE_AMOUNT(
13:41:20 441  	    in_invoice_id => in_invoice_id,
13:41:20 442  	    out_amount	  => var_invoice_amount
13:41:20 443  	  );
13:41:20 444  	  EXCEPTION
13:41:20 445  	   WHEN OTHERS THEN
13:41:20 446  	     EXCEPTION_MESSAGE := SQLERRM;
13:41:20 447  	     RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
13:41:20 448  	END;
13:41:20 449  
13:41:20 450  	IF in_instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_ZCI_INSTRUMENT
13:41:20 451  	  AND var_invoice_amount > 0 THEN
13:41:20 452  	  RAISE CAN_NOT_USE_FCINSTR;
13:41:20 453  	END IF;
13:41:20 454  
13:41:20 455  	IF var_invoice_amount = 0 THEN
13:41:20 456  	  -- UPDATE INVOICE. SET STATUS TO PROCESSED
13:41:20 457  	  PROCS_INVOICE_CRU_V21.UPDATE_INVOICE(
13:41:20 458  	    in_invoice_id		   => in_invoice_id,
13:41:20 459  	    in_updated_by		   => in_created_by,
13:41:20 460  	    in_invoice_status_id	   => GLOBAL_STATUSES_V21.INVOICE_CLOSED
13:41:20 461  	  );
13:41:20 462  	ELSE
13:41:20 463  	  -- Create transaction and charge
13:41:20 464  	  BEGIN
13:41:20 465  	    PROCS_TRANSACTION_V21.CREATE_TRANSACTION(
13:41:20 466  	      in_transaction_id 	=> NULL,
13:41:20 467  	      in_status_id		=> GLOBAL_STATUSES_V21.TRANSACTION_PENDING,
13:41:20 468  	      in_amount 		=> var_invoice_amount,
13:41:20 469  	      in_created_by		=> in_created_by,
13:41:20 470  	      in_order_id		=> NULL,
13:41:20 471  	      in_transaction_type_code	=> 'START_SUBSCRIPTION',
13:41:20 472  	      out_transaction_id	=> var_new_transaction_id
13:41:20 473  	    );
13:41:20 474  	    EXCEPTION
13:41:20 475  	      WHEN OTHERS THEN
13:41:20 476  		EXCEPTION_MESSAGE := SQLERRM;
13:41:20 477  		RAISE CAN_NOT_CREATE_TRANSACTION;
13:41:20 478  	  END;
13:41:20 479  
13:41:20 480  	  BEGIN
13:41:20 481  	    PROCS_CHARGE_V21.CREATE_CHARGE(
13:41:20 482  	      in_invoice_id	    => in_invoice_id,
13:41:20 483  	      in_transaction_id     => var_new_transaction_id,
13:41:20 484  	      in_instrument_type_id => in_instrument_type_id,
13:41:20 485  	      in_instrument_id	    => in_instrument_id,
13:41:20 486  	      in_charge_amount	    => var_invoice_amount,
13:41:20 487  	      in_created_by	    => in_created_by,
13:41:20 488  	      in_charge_status_id   => GLOBAL_STATUSES_V21.CHARGE_OPENED,
13:41:20 489  	      out_charge_id	    => var_new_charge_id
13:41:20 490  	    );
13:41:20 491  	    EXCEPTION
13:41:20 492  	      WHEN OTHERS THEN
13:41:20 493  		EXCEPTION_MESSAGE := SQLERRM;
13:41:20 494  		RAISE CAN_NOT_CREATE_CHARGE;
13:41:20 495  	  END;
13:41:20 496  	END IF;
13:41:20 497  
13:41:20 498  EXCEPTION
13:41:20 499  WHEN CAN_NOT_USE_FCINSTR THEN
13:41:20 500  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
13:41:20 501  	  SPROC_NAME, 'Could not use "free charge instrument" for non-zero invoice');
13:41:20 502  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
13:41:20 503  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 504  	  SPROC_NAME, 'Could not calculate invoice amount', EXCEPTION_MESSAGE);
13:41:20 505  WHEN CAN_NOT_CREATE_TRANSACTION THEN
13:41:20 506  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 507  	  SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
13:41:20 508  WHEN CAN_NOT_CREATE_CHARGE THEN
13:41:20 509  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 510  	  SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
13:41:20 511  WHEN OTHERS THEN
13:41:20 512  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 513  	  SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 514  END FINALIZE_SUBSCRIPTION_CREATION;
