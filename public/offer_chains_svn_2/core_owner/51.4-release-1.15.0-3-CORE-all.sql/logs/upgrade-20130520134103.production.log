13:41:20 2114  PROCEDURE CANCEL_SUBSCRIPTION_INVOICE (
13:41:20 2115  /*
13:41:20 2116  APP_EXCEPTION_CODES_V21.NOT_FOUND
13:41:20 2117  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
13:41:20 2118  APP_EXCEPTION_CODES_V21.INTERNAL_ERROR
13:41:20 2119  */
13:41:20 2120  	 in_invoice_id	      IN NUMBER,
13:41:20 2121  	 in_updated_by	      IN VARCHAR2,
13:41:20 2122  	 in_refundable	      IN NUMBER DEFAULT GLOBAL_CONSTANTS_V21.FALSE
13:41:20 2123  ) AS
13:41:20 2124  SPROC_NAME CONSTANT VARCHAR2(27) := 'CANCEL_SUBSCRIPTION_INVOICE';
13:41:20 2125  -- VARIABLES
13:41:20 2126  temp_invoice_id	      INVOICE.ID%TYPE;
13:41:20 2127  var_chargeback_amount  NUMBER(10,2);
13:41:20 2128  ver_refund_charge_id   NUMBER;
13:41:20 2129  -- EXCEPTIONS
13:41:20 2130  BAD_INVOICE_ID		    EXCEPTION;
13:41:20 2131  CAN_NOT_CALCULATE_CHARGEBACK EXCEPTION;
13:41:20 2132  CAN_NOT_APPLY_CHARGEBACK     EXCEPTION;
13:41:20 2133  EXCEPTION_MESSAGE	    VARCHAR2(1024);
13:41:20 2134  -- STUB
13:41:20 2135  var_now DATE;
13:41:20 2136  var_revoke NUMBER;
13:41:20 2137  var_refund NUMBER;
13:41:20 2138  var_billed NUMBER;
13:41:20 2139  var_subscription_in_grace NUMBER;
13:41:20 2140  BEGIN
13:41:20 2141  
13:41:20 2142  	 var_now := sysdate;
13:41:20 2143  
13:41:20 2144  	 -- Check that invoice exists
13:41:20 2145  	 BEGIN
13:41:20 2146  	   SELECT
13:41:20 2147  	     INVOICE.ID into temp_invoice_id
13:41:20 2148  	   FROM
13:41:20 2149  	     INVOICE
13:41:20 2150  	   WHERE
13:41:20 2151  	     INVOICE.ID = in_invoice_id;
13:41:20 2152  	   EXCEPTION
13:41:20 2153  	     WHEN NO_DATA_FOUND THEN
13:41:20 2154  	       RAISE BAD_INVOICE_ID;
13:41:20 2155  	 END;
13:41:20 2156  
13:41:20 2157  	 select decode(count(1), 0, GLOBAL_CONSTANTS_V21.FALSE, GLOBAL_CONSTANTS_V21.TRUE) into var_revoke
13:41:20 2158  	 from license l, subscription s, offer_chain oc
13:41:20 2159  	 where
13:41:20 2160  	   l.subscription_id = s.id and
13:41:20 2161  	   s.offer_chain_id = oc.id and
13:41:20 2162  	   l.invoice_id = in_invoice_id and
13:41:20 2163  	   oc.revoke_entitlements = GLOBAL_CONSTANTS_V21.TRUE and
13:41:20 2164  	   rownum < 2;
13:41:20 2165  
13:41:20 2166  	 select
13:41:20 2167  	   decode(s.subscription_status_id, GLOBAL_STATUSES_V21.SUBSCRIPTION_IN_GRACE_PERIOD, 1, 0)
13:41:20 2168  	 into
13:41:20 2169  	   var_subscription_in_grace
13:41:20 2170  	 from license l, subscription s
13:41:20 2171  	 where
13:41:20 2172  	   l.subscription_id = s.id and
13:41:20 2173  	   l.invoice_id = in_invoice_id and
13:41:20 2174  	   rownum < 2;
13:41:20 2175  
13:41:20 2176  	 var_billed := PROCS_INVOICE_V21.IS_INVOICE_PAYING_STARTED(in_invoice_id);
13:41:20 2177  	 var_refund := GLOBAL_CONSTANTS_V21.FALSE;
13:41:20 2178  
13:41:20 2179  	 -- Check that transaction for given invoice not started
13:41:20 2180  	 -- if refund enabled calculate and apply chargeback
13:41:20 2181  	 IF (
13:41:20 2182  	     var_billed = GLOBAL_CONSTANTS_V21.TRUE
13:41:20 2183  	   )THEN
13:41:20 2184  	   if (in_refundable = GLOBAL_CONSTANTS_V21.TRUE) then
13:41:20 2185  	     -- If started then we need to calculate refund
13:41:20 2186  	     BEGIN
13:41:20 2187  	       PROCS_INVOICE_V21.CALCULATE_INVOICE_CHARGEBACK(
13:41:20 2188  		 in_invoice_id,
13:41:20 2189  		 var_now,
13:41:20 2190  		 var_chargeback_amount
13:41:20 2191  	       );
13:41:20 2192  	       EXCEPTION
13:41:20 2193  		 WHEN OTHERS THEN
13:41:20 2194  		   EXCEPTION_MESSAGE := SQLERRM;
13:41:20 2195  		   RAISE CAN_NOT_CALCULATE_CHARGEBACK;
13:41:20 2196  	     END;
13:41:20 2197  	     IF var_chargeback_amount > 0 THEN
13:41:20 2198  	       BEGIN
13:41:20 2199  		 PROCS_INVOICE_V21.APPLY_REFUND(
13:41:20 2200  		   in_invoice_id,
13:41:20 2201  		   var_chargeback_amount,
13:41:20 2202  		   in_updated_by,
13:41:20 2203  		   ver_refund_charge_id
13:41:20 2204  		 );
13:41:20 2205  		 EXCEPTION
13:41:20 2206  		   WHEN OTHERS THEN
13:41:20 2207  		     EXCEPTION_MESSAGE := SQLERRM;
13:41:20 2208  		     RAISE CAN_NOT_APPLY_CHARGEBACK;
13:41:20 2209  	       END;
13:41:20 2210  	       var_refund := GLOBAL_CONSTANTS_V21.TRUE;
13:41:20 2211  	     END IF;
13:41:20 2212  	   end if;
13:41:20 2213  	 ELSE
13:41:20 2214  
13:41:20 2215  	   FOR f_transaction_to_close IN (
13:41:20 2216  	     SELECT DISTINCT CHARGE.TRANSACTION_ID AS "ID" FROM CHARGE WHERE CHARGE.INVOICE_ID = in_invoice_id and CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V21.CHARGE_OPENED
13:41:20 2217  	   )
13:41:20 2218  	   LOOP
13:41:20 2219  	     PROCS_TRANSACTION_V21.UPDATE_TRANSACTION_STATUS(
13:41:20 2220  	       in_transaction_id	=> f_transaction_to_close.ID,
13:41:20 2221  	       in_updated_by		=> in_updated_by,
13:41:20 2222  	       in_transaction_status_id => GLOBAL_STATUSES_V21.TRANSACTION_CLOSED
13:41:20 2223  	     );
13:41:20 2224  	   END LOOP;
13:41:20 2225  	   -- Needs to close charges. No refund.
13:41:20 2226  	   FOR f_charge_to_close IN (
13:41:20 2227  	     SELECT CHARGE.ID FROM CHARGE WHERE CHARGE.INVOICE_ID = in_invoice_id and CHARGE.CHARGE_STATUS_ID = GLOBAL_STATUSES_V21.CHARGE_OPENED
13:41:20 2228  	   )
13:41:20 2229  	   LOOP
13:41:20 2230  	     PROCS_CHARGE_V21.UPDATE_CHARGE_STATUS(
13:41:20 2231  	       in_charge_id	   => f_charge_to_close.ID,
13:41:20 2232  	       in_updated_by	   => in_updated_by,
13:41:20 2233  	       in_charge_status_id => GLOBAL_STATUSES_V21.CHARGE_CANCELED
13:41:20 2234  	     );
13:41:20 2235  	   END LOOP;
13:41:20 2236  
13:41:20 2237  	   PROCS_INVOICE_V21.UPDATE_INVOICE_STATUS(
13:41:20 2238  	     in_invoice_id		    => in_invoice_id,
13:41:20 2239  	     in_updated_by		    => in_updated_by,
13:41:20 2240  	     in_invoice_status_id	    => GLOBAL_STATUSES_V21.INVOICE_CLOSED
13:41:20 2241  	   );
13:41:20 2242  
13:41:20 2243  	 END IF;
13:41:20 2244  	 -- update licenses
13:41:20 2245  	 IF(var_revoke = GLOBAL_CONSTANTS_V21.TRUE OR var_chargeback_amount > 0 OR (var_subscription_in_grace = GLOBAL_CONSTANTS_V21.FALSE AND var_billed = GLOBAL_CONSTANTS_V21.FALSE)) THEN
13:41:20 2246  	   FOR f_license_to_cancel IN (
13:41:20 2247  	     SELECT LICENSE.ID FROM LICENSE WHERE LICENSE.INVOICE_ID = in_invoice_id AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V21.LICENSE_ACTIVE
13:41:20 2248  	   )
13:41:20 2249  	   LOOP
13:41:20 2250  	     PROCS_LICENSE_CRU_V21.UPDATE_LICENSE(
13:41:20 2251  	       in_license_id	     => f_license_to_cancel.ID,
13:41:20 2252  	       in_license_status_id  => GLOBAL_STATUSES_V21.LICENSE_CLOSED,
13:41:20 2253  	       in_needs_entitlements => GLOBAL_CONSTANTS_V21.TRUE,
13:41:20 2254  	       in_updated_by	     => in_updated_by,
13:41:20 2255  	       in_entitlement_end_date => var_now
13:41:20 2256  	     );
13:41:20 2257  	   END LOOP;
13:41:20 2258  	 ELSE
13:41:20 2259  	   FOR f_license_to_cancel IN (
13:41:20 2260  	     SELECT LICENSE.ID FROM LICENSE WHERE LICENSE.INVOICE_ID = in_invoice_id AND LICENSE.LICENSE_STATUS_ID in (GLOBAL_STATUSES_V21.LICENSE_ACTIVE, GLOBAL_STATUSES_V21.LICENSE_IN_GRACE_PERIOD)
13:41:20 2261  	   )
13:41:20 2262  	   LOOP
13:41:20 2263  	     PROCS_LICENSE_CRU_V21.UPDATE_LICENSE(
13:41:20 2264  	       in_license_id	     => f_license_to_cancel.ID,
13:41:20 2265  	       in_license_status_id  => GLOBAL_STATUSES_V21.LICENSE_CLOSED,
13:41:20 2266  	       in_updated_by	     => in_updated_by
13:41:20 2267  	     );
13:41:20 2268  	   END LOOP;
13:41:20 2269  	 END IF;
13:41:20 2270  
13:41:20 2271  
13:41:20 2272  EXCEPTION
13:41:20 2273  WHEN BAD_INVOICE_ID THEN
13:41:20 2274  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 2275  	   SPROC_NAME, 'No such invoice');
13:41:20 2276  WHEN CAN_NOT_CALCULATE_CHARGEBACK THEN
13:41:20 2277  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 2278  	   SPROC_NAME, 'Could not calculate invoice refund', EXCEPTION_MESSAGE);
13:41:20 2279  WHEN CAN_NOT_APPLY_CHARGEBACK THEN
13:41:20 2280  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 2281  	   SPROC_NAME, 'Could not apply chargeback', EXCEPTION_MESSAGE);
13:41:20 2282  WHEN OTHERS THEN
13:41:20 2283  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 2284  	   SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 2285  END CANCEL_SUBSCRIPTION_INVOICE;
13:41:20 2286  
13:41:20 2287  /******************************************************************************/
13:41:20 2288  
13:41:20 2289  PROCEDURE FINALIZE_CANCELATION (
13:41:20 2290  /*
13:41:20 2291  APP_EXCEPTION_CODES_V21.NOT_FOUND
13:41:20 2292  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
13:41:20 2293  APP_EXCEPTION_CODES_V21.INTERNAL_ERROR
13:41:20 2294  */
13:41:20 2295  --  in_subscription_id	 IN SUBSCRIPTION.ID%TYPE,
13:41:20 2296  --  in_cancelation_reason IN SUBSCRIPTION_CANCEL_REASON.VALUE%TYPE,
13:41:20 2297  --  in_cancelation_date	 IN SUBSCRIPTION.CANCELLATION_DATE%TYPE,
13:41:20 2298  --  in_note		 IN SUBSCRIPTION_NOTE.NOTE%TYPE,
13:41:20 2299  --  in_agent_id		 IN SUBSCRIPTION_NOTE.AGENT_ID%TYPE,
13:41:20 2300  --  in_updated_by	 IN SUBSCRIPTION.UPDATED_BY%TYPE
13:41:20 2301  	 in_subscription_id    IN NUMBER,
13:41:20 2302  	 in_cancelation_reason IN VARCHAR2,
13:41:20 2303  	 in_cancelation_date   IN DATE,
13:41:20 2304  	 in_note	       IN VARCHAR2,
13:41:20 2305  	 in_agent_id	       IN NUMBER,
13:41:20 2306  	 in_updated_by	       IN VARCHAR2
13:41:20 2307  ) AS
13:41:20 2308  SPROC_NAME CONSTANT VARCHAR2(20) := 'FINALIZE_CANCELATION';
13:41:20 2309  -- VARIABLES
13:41:20 2310  var_current_subscr_status SUBSCRIPTION.SUBSCRIPTION_STATUS_ID%TYPE;
13:41:20 2311  var_sct_id		 SUBSCRIPTION.SCT_ID%TYPE;
13:41:20 2312  var_active_invoices_count NUMBER;
13:41:20 2313  var_license_to_disgrace	 LICENSE.ID%TYPE;
13:41:20 2314  var_now			 DATE := SYSDATE;
13:41:20 2315  -- EXCEPTIONS
13:41:20 2316  BAD_SUBSCRIPTION_ID	   EXCEPTION;
13:41:20 2317  BAD_SUBSCRIPTION_STATUS	   EXCEPTION;
13:41:20 2318  BAD_CANCELATION_REASON	   EXCEPTION;
13:41:20 2319  CAN_NOT_UPDATE_SUBSCRIPTION EXCEPTION;
13:41:20 2320  ACTIVE_INVOICES_FOUND	   EXCEPTION;
13:41:20 2321  CAN_NOT_CREATE_NOTE	   EXCEPTION;
13:41:20 2322  EXCEPTION_MESSAGE	   VARCHAR2(1024);
13:41:20 2323  BEGIN
13:41:20 2324  
13:41:20 2325  	 -- Get current subscription status
13:41:20 2326  	 BEGIN
13:41:20 2327  	   SELECT
13:41:20 2328  	     SUBSCRIPTION.SUBSCRIPTION_STATUS_ID into var_current_subscr_status
13:41:20 2329  	   FROM
13:41:20 2330  	     SUBSCRIPTION
13:41:20 2331  	   WHERE
13:41:20 2332  	     SUBSCRIPTION.ID = in_subscription_id;
13:41:20 2333  	   EXCEPTION
13:41:20 2334  	     WHEN NO_DATA_FOUND THEN
13:41:20 2335  	       RAISE BAD_SUBSCRIPTION_ID;
13:41:20 2336  	 END;
13:41:20 2337  
13:41:20 2338  	 -- Check that subscription reason is correct
13:41:20 2339  	 BEGIN
13:41:20 2340  	   SELECT
13:41:20 2341  	     SUBSCRIPTION_CANCEL_REASON.ID into var_sct_id
13:41:20 2342  	   FROM
13:41:20 2343  	     SUBSCRIPTION_CANCEL_REASON
13:41:20 2344  	   WHERE
13:41:20 2345  	     SUBSCRIPTION_CANCEL_REASON.VALUE LIKE in_cancelation_reason
13:41:20 2346  	     AND ROWNUM <= 1;
13:41:20 2347  	   EXCEPTION
13:41:20 2348  	     WHEN NO_DATA_FOUND THEN
13:41:20 2349  	       RAISE BAD_CANCELATION_REASON;
13:41:20 2350  	 END;
13:41:20 2351  
13:41:20 2352  	 -- Check for invoices with active licenses
13:41:20 2353  	 SELECT
13:41:20 2354  	   COUNT(*) into var_active_invoices_count
13:41:20 2355  	 FROM
13:41:20 2356  	   LICENSE
13:41:20 2357  	   INNER JOIN INVOICE ON LICENSE.INVOICE_ID = INVOICE.ID
13:41:20 2358  	 WHERE
13:41:20 2359  	   LICENSE.LICENSE_STATUS_ID in (GLOBAL_STATUSES_V21.LICENSE_ACTIVE, GLOBAL_STATUSES_V21.LICENSE_IN_GRACE_PERIOD)
13:41:20 2360  	   AND LICENSE.SUBSCRIPTION_ID = in_subscription_id;
13:41:20 2361  
13:41:20 2362  	 IF var_active_invoices_count > 0 THEN
13:41:20 2363  	   RAISE ACTIVE_INVOICES_FOUND;
13:41:20 2364  	 END IF;
13:41:20 2365  
13:41:20 2366  	 -- Check that subscription is active
13:41:20 2367  	 IF var_current_subscr_status != GLOBAL_STATUSES_V21.SUBSCRIPTION_ACTIVE AND
13:41:20 2368  	    var_current_subscr_status != GLOBAL_STATUSES_V21.SUBSCRIPTION_SUSPENDED AND
13:41:20 2369  	    var_current_subscr_status != GLOBAL_STATUSES_V21.SUBSCRIPTION_IN_GRACE_PERIOD THEN
13:41:20 2370  	   RAISE BAD_SUBSCRIPTION_STATUS;
13:41:20 2371  	 END IF;
13:41:20 2372  
13:41:20 2373  	 -- Update subscription data
13:41:20 2374  	 BEGIN
13:41:20 2375  	   PROCS_SUBSCRIPTION_CRU_V21.UPDATE_SUBSCRIPTION(
13:41:20 2376  	     in_subscription_id        => in_subscription_id,
13:41:20 2377  	     in_subscription_status_id => GLOBAL_STATUSES_V21.SUBSCRIPTION_CANCELED,
13:41:20 2378  	     in_cancelation_date       => in_cancelation_date,
13:41:20 2379  	     in_updated_by	       => in_updated_by,
13:41:20 2380  	     in_sct_id		       => var_sct_id
13:41:20 2381  	   );
13:41:20 2382  	   EXCEPTION
13:41:20 2383  	     WHEN OTHERS THEN
13:41:20 2384  	       EXCEPTION_MESSAGE := SQLERRM;
13:41:20 2385  	       RAISE CAN_NOT_UPDATE_SUBSCRIPTION;
13:41:20 2386  	 END;
13:41:20 2387  
13:41:20 2388  	 -- Terminate grace period for licenses in grace [SAR-31]
13:41:20 2389  	 BEGIN
13:41:20 2390  	   SELECT
13:41:20 2391  	     LICENSE.ID into var_license_to_disgrace
13:41:20 2392  	   FROM
13:41:20 2393  	     LICENSE
13:41:20 2394  	   WHERE
13:41:20 2395  	     LICENSE.SUBSCRIPTION_ID = in_subscription_id
13:41:20 2396  	     AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V21.LICENSE_IN_GRACE_PERIOD
13:41:20 2397  	     AND ROWNUM <= 1
13:41:20 2398  	   ORDER BY
13:41:20 2399  	     CREATE_DATE DESC;
13:41:20 2400  	 EXCEPTION
13:41:20 2401  	   WHEN NO_DATA_FOUND THEN
13:41:20 2402  	     var_license_to_disgrace := NULL;
13:41:20 2403  	 END;
13:41:20 2404  
13:41:20 2405  	 IF var_license_to_disgrace IS NOT NULL THEN
13:41:20 2406  	   PROCS_LICENSE_CRU_V21.UPDATE_LICENSE(
13:41:20 2407  	     in_license_id	     => var_license_to_disgrace,
13:41:20 2408  	     in_license_status_id    => GLOBAL_STATUSES_V21.LICENSE_CLOSED,
13:41:20 2409  	     in_updated_by	     => in_updated_by
13:41:20 2410  	   );
13:41:20 2411  	 END IF;
13:41:20 2412  
13:41:20 2413  	 -- Annotate subscription
13:41:20 2414  	 IF in_note IS NOT NULL THEN
13:41:20 2415  	   BEGIN
13:41:20 2416  	     PROCS_SUBSCRIPTION_V21.ANNOTATE_SUBSCRIPTION(
13:41:20 2417  	       in_subscription_id,
13:41:20 2418  	       in_agent_id,
13:41:20 2419  	       in_note,
13:41:20 2420  	       in_updated_by
13:41:20 2421  	     );
13:41:20 2422  	     EXCEPTION
13:41:20 2423  	      WHEN OTHERS THEN
13:41:20 2424  		EXCEPTION_MESSAGE := SQLERRM;
13:41:20 2425  		RAISE CAN_NOT_CREATE_NOTE;
13:41:20 2426  	   END;
13:41:20 2427  	 END IF;
13:41:20 2428  
13:41:20 2429  EXCEPTION
13:41:20 2430  WHEN BAD_SUBSCRIPTION_ID THEN
13:41:20 2431  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 2432  	   SPROC_NAME, 'No such subscription');
13:41:20 2433  WHEN BAD_SUBSCRIPTION_STATUS THEN
13:41:20 2434  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INVALID_PARAMETER,
13:41:20 2435  	   SPROC_NAME, 'Bad current subscription status');
13:41:20 2436  WHEN BAD_CANCELATION_REASON THEN
13:41:20 2437  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INVALID_PARAMETER,
13:41:20 2438  	   SPROC_NAME, 'Bad cancellation reason');
13:41:20 2439  WHEN CAN_NOT_UPDATE_SUBSCRIPTION THEN
13:41:20 2440  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 2441  	   SPROC_NAME, 'Could not update subscription data', EXCEPTION_MESSAGE);
13:41:20 2442  WHEN ACTIVE_INVOICES_FOUND THEN
13:41:20 2443  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
13:41:20 2444  	   SPROC_NAME, 'Invoices with active licenses found');
13:41:20 2445  WHEN CAN_NOT_CREATE_NOTE THEN
13:41:20 2446  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 2447  	   SPROC_NAME, 'Could not annotate subscription', EXCEPTION_MESSAGE);
13:41:20 2448  --WHEN OTHERS THEN
13:41:20 2449  --  PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 2450  --    SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 2451  END FINALIZE_CANCELATION;
