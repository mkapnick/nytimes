13:41:20 916  /******************************************************************************/
13:41:20 917  
13:41:20 918  PROCEDURE FINALIZE_GC_PURCHASING (
13:41:20 919  	in_invoice_id	      IN NUMBER,
13:41:20 920  	in_created_by	      IN VARCHAR2,
13:41:20 921  	in_instrument_id      IN NUMBER,
13:41:20 922  	in_instrument_type_id IN NUMBER,
13:41:20 923  	in_order_id	      IN VARCHAR2,
13:41:20 924  	in_transaction_id     IN NUMBER,
13:41:20 925  	out_charge_amount     OUT NUMBER
13:41:20 926  ) AS
13:41:20 927  SPROC_NAME CONSTANT VARCHAR2(22) := 'FINALIZE_GC_PURCHASING';
13:41:20 928  -- VARIABLES
13:41:20 929  temp_transaction_id_count NUMBER;
13:41:20 930  var_invoice_amount	NUMBER(10,2);
13:41:20 931  var_transaction_id	NUMBER;
13:41:20 932  var_new_charge_id 	NUMBER;
13:41:20 933  -- EXCEPTIONS
13:41:20 934  BAD_CREDIT_CARD_ID	  EXCEPTION;
13:41:20 935  BAD_PAYPAL_ID		  EXCEPTION;
13:41:20 936  BAD_INSTRUMENT_TYPE	  EXCEPTION;
13:41:20 937  TRANSACTION_EXISTS	  EXCEPTION;
13:41:20 938  CAN_NOT_CALC_INVOICE_AMOUNT EXCEPTION;
13:41:20 939  CAN_NOT_USE_FCINSTR	  EXCEPTION;
13:41:20 940  CAN_NOT_CREATE_TRANSACTION  EXCEPTION;
13:41:20 941  CAN_NOT_CREATE_CHARGE	  EXCEPTION;
13:41:20 942  EXCEPTION_MESSAGE   VARCHAR2(1024);
13:41:20 943  BEGIN
13:41:20 944  
13:41:20 945  	-- Check that instrument exists
13:41:20 946  	IF in_instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_CREDIT_CARD THEN
13:41:20 947  	  IF IS_CREDIT_CARD_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V21.FALSE THEN
13:41:20 948  	    RAISE BAD_CREDIT_CARD_ID;
13:41:20 949  	  END IF;
13:41:20 950  	ELSIF in_instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_PAYPAL THEN
13:41:20 951  	  IF IS_PAYPAL_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V21.FALSE THEN
13:41:20 952  	    RAISE BAD_PAYPAL_ID;
13:41:20 953  	  END IF;
13:41:20 954  	ELSIF in_instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_ZCI_INSTRUMENT THEN
13:41:20 955  	  NULL;
13:41:20 956  	ELSE
13:41:20 957  	  RAISE BAD_INSTRUMENT_TYPE;
13:41:20 958  	END IF;
13:41:20 959  
13:41:20 960  	-- Check that transaction with given id do not exists
13:41:20 961  	SELECT
13:41:20 962  	  COUNT(*) into temp_transaction_id_count
13:41:20 963  	FROM
13:41:20 964  	  TRANSACTION
13:41:20 965  	WHERE
13:41:20 966  	  TRANSACTION.ID = in_transaction_id;
13:41:20 967  
13:41:20 968  	IF temp_transaction_id_count > 0 THEN
13:41:20 969  	  RAISE TRANSACTION_EXISTS;
13:41:20 970  	END IF;
13:41:20 971  
13:41:20 972  	-- Calculate new invoice amount
13:41:20 973  	BEGIN
13:41:20 974  	  PROCS_INVOICE_V21.CALCULATE_INVOICE_AMOUNT(in_invoice_id, var_invoice_amount);
13:41:20 975  	  EXCEPTION
13:41:20 976  	    WHEN OTHERS THEN
13:41:20 977  	      EXCEPTION_MESSAGE := SQLERRM;
13:41:20 978  	      RAISE CAN_NOT_CALC_INVOICE_AMOUNT;
13:41:20 979  	END;
13:41:20 980  
13:41:20 981  	IF var_invoice_amount > 0
13:41:20 982  	  AND in_instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_ZCI_INSTRUMENT THEN
13:41:20 983  	  RAISE CAN_NOT_USE_FCINSTR;
13:41:20 984  	END IF;
13:41:20 985  
13:41:20 986  	-- If invoice amount iz 0 then we need to set status for this invoice to PROCCESSED
13:41:20 987  	IF var_invoice_amount = 0 THEN
13:41:20 988  	  PROCS_INVOICE_CRU_V21.UPDATE_INVOICE(
13:41:20 989  	    in_invoice_id		   => in_invoice_id,
13:41:20 990  	    in_updated_by		   => in_created_by,
13:41:20 991  	    in_invoice_status_id	   => GLOBAL_STATUSES_V21.INVOICE_CLOSED
13:41:20 992  	  );
13:41:20 993  	END IF;
13:41:20 994  
13:41:20 995  	IF var_invoice_amount > 0 THEN
13:41:20 996  	  -- Create transaction
13:41:20 997  	  BEGIN
13:41:20 998  	    PROCS_TRANSACTION_V21.CREATE_TRANSACTION(
13:41:20 999  	      in_transaction_id        => in_transaction_id,
13:41:20 1000  	       in_status_id		=> GLOBAL_STATUSES_V21.TRANSACTION_PENDING,
13:41:20 1001  	       in_amount		=> var_invoice_amount,
13:41:20 1002  	       in_created_by		=> in_created_by,
13:41:20 1003  	       in_order_id		=> in_order_id,
13:41:20 1004  	       in_transaction_type_code => 'GIFT_CERTIFICATE_PURCHASE',
13:41:20 1005  	       out_transaction_id	=> var_transaction_id
13:41:20 1006  	     );
13:41:20 1007  	     EXCEPTION
13:41:20 1008  	       WHEN OTHERS THEN
13:41:20 1009  		 EXCEPTION_MESSAGE := SQLERRM;
13:41:20 1010  		 RAISE CAN_NOT_CREATE_TRANSACTION;
13:41:20 1011  	   END;
13:41:20 1012  
13:41:20 1013  	   -- Create charge
13:41:20 1014  	   BEGIN
13:41:20 1015  	     PROCS_CHARGE_V21.CREATE_CHARGE(
13:41:20 1016  	       in_invoice_id	     => in_invoice_id,
13:41:20 1017  	       in_transaction_id     => var_transaction_id,
13:41:20 1018  	       in_instrument_type_id => in_instrument_type_id,
13:41:20 1019  	       in_instrument_id      => in_instrument_id,
13:41:20 1020  	       in_charge_amount      => var_invoice_amount,
13:41:20 1021  	       in_created_by	     => in_created_by,
13:41:20 1022  	       in_charge_status_id   => GLOBAL_STATUSES_V21.CHARGE_OPENED,
13:41:20 1023  	       out_charge_id	     => var_new_charge_id
13:41:20 1024  	     );
13:41:20 1025  	     out_charge_amount := var_invoice_amount;
13:41:20 1026  	     EXCEPTION
13:41:20 1027  	       WHEN OTHERS THEN
13:41:20 1028  		 EXCEPTION_MESSAGE := SQLERRM;
13:41:20 1029  		 RAISE CAN_NOT_CREATE_CHARGE;
13:41:20 1030  	   END;
13:41:20 1031  	 ELSE
13:41:20 1032  	   out_charge_amount := 0;
13:41:20 1033  	 END IF;
13:41:20 1034  
13:41:20 1035  EXCEPTION
13:41:20 1036  WHEN CAN_NOT_USE_FCINSTR THEN
13:41:20 1037  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
13:41:20 1038  	   SPROC_NAME, 'Could not use "free charge instrument" for non-zero invoice');
13:41:20 1039  WHEN BAD_CREDIT_CARD_ID THEN
13:41:20 1040  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 1041  	   SPROC_NAME, 'Bad credit card id');
13:41:20 1042  WHEN BAD_PAYPAL_ID THEN
13:41:20 1043  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 1044  	   SPROC_NAME, 'Bad paypal id');
13:41:20 1045  WHEN BAD_INSTRUMENT_TYPE THEN
13:41:20 1046  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INVALID_PARAMETER,
13:41:20 1047  	   SPROC_NAME, 'Bad instrument type');
13:41:20 1048  WHEN TRANSACTION_EXISTS THEN
13:41:20 1049  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.DUPLICATE_ERROR,
13:41:20 1050  	   SPROC_NAME, 'Transaction with given id already exists');
13:41:20 1051  WHEN CAN_NOT_CREATE_TRANSACTION THEN
13:41:20 1052  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 1053  	   SPROC_NAME, 'Could not create transaction', EXCEPTION_MESSAGE);
13:41:20 1054  WHEN CAN_NOT_CREATE_CHARGE THEN
13:41:20 1055  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 1056  	   SPROC_NAME, 'Could not create charge', EXCEPTION_MESSAGE);
13:41:20 1057  WHEN CAN_NOT_CALC_INVOICE_AMOUNT THEN
13:41:20 1058  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 1059  	   SPROC_NAME, 'Could not calculate amount for new invoice', EXCEPTION_MESSAGE);
13:41:20 1060  WHEN OTHERS THEN
13:41:20 1061  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 1062  	   SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 1063  END FINALIZE_GC_PURCHASING;
13:41:20 1064  
13:41:20 1065  /******************************************************************************/
13:41:20 1066  
13:41:20 1067  PROCEDURE PURCHASE_GIFT_CERTIFICATE (
13:41:20 1068  	 in_group_id		   IN NUMBER,
13:41:20 1069  	 in_offer_chain_id	   IN VARCHAR2,
13:41:20 1070  	 in_gift_certificate_code  IN VARCHAR2,
13:41:20 1071  	 in_created_by		   IN VARCHAR2,
13:41:20 1072  	 in_recipient_name	   IN VARCHAR2,
13:41:20 1073  	 in_recipient_email	   IN VARCHAR2,
13:41:20 1074  	 in_sender_name 	   IN VARCHAR2,
13:41:20 1075  	 in_sender_email	   IN VARCHAR2,
13:41:20 1076  	 in_gift_message	   IN VARCHAR2,
13:41:20 1077  	 in_instrument_id	   IN NUMBER,
13:41:20 1078  	 in_instrument_type_id	   IN NUMBER,
13:41:20 1079  	 in_expiration_date	   IN DATE,
13:41:20 1080  	 in_order_id		   IN VARCHAR2,
13:41:20 1081  	 in_transaction_id	   IN NUMBER
13:41:20 1082  ) AS
13:41:20 1083  SPROC_NAME	  CONSTANT VARCHAR2(25) := 'PURCHASE_GIFT_CERTIFICATE';
13:41:20 1084  -- VARIABLES
13:41:20 1085  var_gift_cert_id   NUMBER;
13:41:20 1086  var_account_id	  NUMBER;
13:41:20 1087  var_invoice_amount NUMBER (10,2);
13:41:20 1088  var_new_invoice_id NUMBER;
13:41:20 1089  var_new_charge_id  NUMBER;
13:41:20 1090  var_och_is_gc	  NUMBER;
13:41:20 1091  var_offer_chain_status_id NUMBER;
13:41:20 1092  var_is_for_redemption	 NUMBER;
13:41:20 1093  var_account_tax_exempt_id VARCHAR2(255);
13:41:20 1094  
13:41:20 1095  temp_transaction_id_count NUMBER;
13:41:20 1096  var_transaction_id	 NUMBER;
13:41:20 1097  temp_gc_code VARCHAR2(255);
13:41:20 1098  
13:41:20 1099  var_invoice_status_id NUMBER;
13:41:20 1100  -- EXCEPTIONS
13:41:20 1101  CAN_NOT_CREATE_INVOICE		   EXCEPTION;
13:41:20 1102  CAN_NOT_CREATE_TRANSACTION	   EXCEPTION;
13:41:20 1103  CAN_NOT_CREATE_CHARGE		   EXCEPTION;
13:41:20 1104  CAN_NOT_CREATE_LINE_ITEMS	   EXCEPTION;
13:41:20 1105  BAD_GROUP_ID			   EXCEPTION;
13:41:20 1106  BAD_OFFER_CHAIN_ID		   EXCEPTION;
13:41:20 1107  OCH_IS_NOT_GIFT_CERTIFICATE	   EXCEPTION;
13:41:20 1108  TRANSACTION_EXISTS		   EXCEPTION;
13:41:20 1109  GC_CODE_ALREADY_EXISTS		   EXCEPTION;
13:41:20 1110  BAD_INSTRUMENT_TYPE		   EXCEPTION;
13:41:20 1111  BAD_CREDIT_CARD_ID		   EXCEPTION;
13:41:20 1112  BAD_PAYPAL_ID			   EXCEPTION;
13:41:20 1113  CAN_NOT_CALCULATE_OCH_AMOUNT	   EXCEPTION;
13:41:20 1114  BAD_OFFER_CHAIN_STATUS		   EXCEPTION;
13:41:20 1115  OFFER_REC_NUM_LESS_THAN_ONE	   EXCEPTION;
13:41:20 1116  CAN_NOT_CALC_INVOICE_AMOUNT	   EXCEPTION;
13:41:20 1117  CAN_NOT_USE_FCINSTR		   EXCEPTION;
13:41:20 1118  CAN_NOT_PURCHASE_GC_FOR_RDMPN	   EXCEPTION;
13:41:20 1119  EXCEPTION_MESSAGE		   VARCHAR2(1024);
13:41:20 1120  BEGIN
13:41:20 1121  
13:41:20 1122  	 -- Get account id
13:41:20 1123  	 BEGIN
13:41:20 1124  	   SELECT
13:41:20 1125  	     ACCOUNT.ID,
13:41:20 1126  	     ACCOUNT.TAX_EXEMPT_ID
13:41:20 1127  	     into
13:41:20 1128  	     var_account_id,
13:41:20 1129  	     var_account_tax_exempt_id
13:41:20 1130  	   FROM
13:41:20 1131  	     ACCOUNT
13:41:20 1132  	   WHERE
13:41:20 1133  	     ACCOUNT.GROUP_ID = in_group_id;
13:41:20 1134  	   EXCEPTION
13:41:20 1135  	   WHEN NO_DATA_FOUND THEN
13:41:20 1136  	     RAISE BAD_GROUP_ID;
13:41:20 1137  	 END;
13:41:20 1138  
13:41:20 1139  	 -- Check that instrument exists
13:41:20 1140  	 IF in_instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_CREDIT_CARD THEN
13:41:20 1141  	   IF IS_CREDIT_CARD_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V21.FALSE THEN
13:41:20 1142  	     RAISE BAD_CREDIT_CARD_ID;
13:41:20 1143  	   END IF;
13:41:20 1144  	 ELSIF in_instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_PAYPAL THEN
13:41:20 1145  	   IF IS_PAYPAL_EXISTS(in_instrument_id) = GLOBAL_CONSTANTS_V21.FALSE THEN
13:41:20 1146  	     RAISE BAD_PAYPAL_ID;
13:41:20 1147  	   END IF;
13:41:20 1148  	 ELSIF in_instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_ZCI_INSTRUMENT THEN
13:41:20 1149  	   NULL;
13:41:20 1150  	 ELSE
13:41:20 1151  	   RAISE BAD_INSTRUMENT_TYPE;
13:41:20 1152  	 END IF;
13:41:20 1153  
13:41:20 1154  	 -- Check for the same code
13:41:20 1155  	 BEGIN
13:41:20 1156  	   SELECT
13:41:20 1157  	     GIFT_CERTIFICATE.CODE into temp_gc_code
13:41:20 1158  	   FROM
13:41:20 1159  	     GIFT_CERTIFICATE
13:41:20 1160  	   WHERE
13:41:20 1161  	     GIFT_CERTIFICATE.CODE = in_gift_certificate_code;
13:41:20 1162  
13:41:20 1163  	   RAISE GC_CODE_ALREADY_EXISTS;
13:41:20 1164  
13:41:20 1165  	   EXCEPTION
13:41:20 1166  	     WHEN NO_DATA_FOUND THEN
13:41:20 1167  	       NULL;
13:41:20 1168  	 END;
13:41:20 1169  
13:41:20 1170  	 -- Get offer chain flag "is_gift_certificate"
13:41:20 1171  	 BEGIN
13:41:20 1172  	   SELECT
13:41:20 1173  	     OFFER_CHAIN.IS_GIFT_CERTIFICATE,
13:41:20 1174  	     OFFER_CHAIN.OFFER_CHAIN_STATUS_ID
13:41:20 1175  	     into
13:41:20 1176  	     var_och_is_gc,
13:41:20 1177  	     var_offer_chain_status_id
13:41:20 1178  	   FROM
13:41:20 1179  	     OFFER_CHAIN
13:41:20 1180  	   WHERE
13:41:20 1181  	     OFFER_CHAIN.ID = in_offer_chain_id;
13:41:20 1182  	   EXCEPTION
13:41:20 1183  	   WHEN NO_DATA_FOUND THEN
13:41:20 1184  	     RAISE BAD_OFFER_CHAIN_ID;
13:41:20 1185  	 END;
13:41:20 1186  
13:41:20 1187  	 IF var_och_is_gc != GLOBAL_CONSTANTS_V21.TRUE
13:41:20 1188  	   OR var_och_is_gc IS NULL THEN
13:41:20 1189  	   RAISE OCH_IS_NOT_GIFT_CERTIFICATE;
13:41:20 1190  	 END IF;
13:41:20 1191  
13:41:20 1192  	 IF var_offer_chain_status_id != GLOBAL_STATUSES_V21.OFFER_CHAIN_ACTIVE THEN
13:41:20 1193  	   RAISE BAD_OFFER_CHAIN_STATUS;
13:41:20 1194  	 END IF;
13:41:20 1195  
13:41:20 1196  	 -- norlov: #38151 check if the OC is for Redemption:
13:41:20 1197  	 SELECT
13:41:20 1198  	   COUNT(*) into var_is_for_redemption
13:41:20 1199  	 FROM
13:41:20 1200  	   OFFER_CHAIN_ELIGIBILITY
13:41:20 1201  	 WHERE
13:41:20 1202  	   OFFER_CHAIN_ELIGIBILITY.OFFER_CHAIN_ID = in_offer_chain_id
13:41:20 1203  	   AND OFFER_CHAIN_ELIGIBILITY.NAME = GLOBAL_CONSTANTS_V21.GIFT_CERTIFICATE_REQUIRED
13:41:20 1204  	   AND OFFER_CHAIN_ELIGIBILITY.VALUE = GLOBAL_CONSTANTS_V21.ELIGIBILITY_FLAG_SET;
13:41:20 1205  
13:41:20 1206  	 IF var_is_for_redemption > 0 THEN
13:41:20 1207  	   RAISE CAN_NOT_PURCHASE_GC_FOR_RDMPN;
13:41:20 1208  	 END IF;
13:41:20 1209  
13:41:20 1210  	 -- Check that transaction with given id do not exists
13:41:20 1211  	 SELECT
13:41:20 1212  	   COUNT(*) into temp_transaction_id_count
13:41:20 1213  	 FROM
13:41:20 1214  	   TRANSACTION
13:41:20 1215  	 WHERE
13:41:20 1216  	   TRANSACTION.ID = in_transaction_id;
13:41:20 1217  
13:41:20 1218  	 IF temp_transaction_id_count > 0 THEN
13:41:20 1219  	   RAISE TRANSACTION_EXISTS;
13:41:20 1220  	 END IF;
13:41:20 1221  
13:41:20 1222  	 -- Create new invoice
13:41:20 1223  	 BEGIN
13:41:20 1224  	   PROCS_INVOICE_V21.CREATE_INVOICE(
13:41:20 1225  	     in_invoice_status => GLOBAL_STATUSES_V21.INVOICE_OPEN,
13:41:20 1226  	     in_created_by     => in_created_by,
13:41:20 1227  	     in_tax_exempt_id  => var_account_tax_exempt_id,
13:41:20 1228  	     out_invoice_id    => var_new_invoice_id
13:41:20 1229  	   );
13:41:20 1230  	   EXCEPTION
13:41:20 1231  	     WHEN OTHERS THEN
13:41:20 1232  	       EXCEPTION_MESSAGE := SQLERRM;
13:41:20 1233  	       RAISE CAN_NOT_CREATE_INVOICE;
13:41:20 1234  	 END;
13:41:20 1235  
13:41:20 1236  	 -- Add line items for new invoice
13:41:20 1237  	 BEGIN
13:41:20 1238  	   FOR f_offer_data IN (
13:41:20 1239  	     SELECT
13:41:20 1240  	       OFFER_ID,
13:41:20 1241  	       NUM_RECURRENCES
13:41:20 1242  	     FROM
13:41:20 1243  	       OFFER_OFFER_CHAIN
13:41:20 1244  	     WHERE
13:41:20 1245  	       OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
13:41:20 1246  	   )
13:41:20 1247  	   LOOP
13:41:20 1248  	     IF f_offer_data.NUM_RECURRENCES < 1 THEN
13:41:20 1249  	       RAISE OFFER_REC_NUM_LESS_THAN_ONE;
13:41:20 1250  	     END IF;
13:41:20 1251  	     FOR i_offer_recurrences_iterator IN 1..f_offer_data.NUM_RECURRENCES
13:41:20 1252  	     LOOP
13:41:20 1253  	       PROCS_LINE_ITEMS_V21.ADD_LINE_ITEMS(
13:41:20 1254  		 in_invoice_id => var_new_invoice_id,
13:41:20 1255  		 in_offer_id   => f_offer_data.OFFER_ID,
