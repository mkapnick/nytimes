13:41:20 153  CAN_NOT_ANNOTATE_ACCOUNT	EXCEPTION;
13:41:20 154  EXCEPTION_MESSAGE 	VARCHAR2(1024);
13:41:20 155  BEGIN
13:41:20 156  
13:41:20 157  	-- Get account's status and id
13:41:20 158  	BEGIN
13:41:20 159  	  SELECT
13:41:20 160  	    ACCOUNT.ACCOUNT_STATUS_ID,
