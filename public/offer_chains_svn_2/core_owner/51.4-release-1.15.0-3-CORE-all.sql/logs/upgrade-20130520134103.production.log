13:41:20 1331  	       WHEN NO_DATA_FOUND THEN
13:41:20 1332  		 var_gc_purchaser_group_id := NULL;
13:41:20 1333  		 var_gc_redeemer_group_id  := NULL;
13:41:20 1334  	   END;
13:41:20 1335  	 END IF;
13:41:20 1336  
13:41:20 1337  	 IF var_subscrib_group_id IS NULL
13:41:20 1338  	   AND var_gc_purchaser_group_id IS NULL
13:41:20 1339  	   AND var_gc_redeemer_group_id IS NULL THEN
13:41:20 1340  	     RAISE CAN_NOT_FIND_ACCOUNT;
13:41:20 1341  	 END IF;
13:41:20 1342  
13:41:20 1343  	 OPEN out_group_ids FOR
13:41:20 1344  	 SELECT GROUP_ID FROM (
13:41:20 1345  	   SELECT
13:41:20 1346  	     var_subscrib_group_id as "GROUP_ID"
13:41:20 1347  	   FROM
13:41:20 1348  	     DUAL
13:41:20 1349  	   UNION
13:41:20 1350  	   SELECT
13:41:20 1351  	     var_gc_purchaser_group_id as "GROUP_ID"
13:41:20 1352  	   FROM
13:41:20 1353  	     DUAL
13:41:20 1354  	   UNION
13:41:20 1355  	   SELECT
13:41:20 1356  	     var_gc_redeemer_group_id as "GROUP_ID"
13:41:20 1357  	   FROM
13:41:20 1358  	     DUAL
13:41:20 1359  	 )
13:41:20 1360  	 WHERE
13:41:20 1361  	   GROUP_ID IS NOT NULL;
13:41:20 1362  
13:41:20 1363  EXCEPTION
13:41:20 1364  WHEN BAD_INVOICE_ID THEN
13:41:20 1365  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 1366  	   SPROC_NAME, 'No such invoice');
13:41:20 1367  WHEN CAN_NOT_FIND_ACCOUNT THEN
13:41:20 1368  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 1369  	   SPROC_NAME, 'Could not find account for given invoice');
13:41:20 1370  WHEN OTHERS THEN
13:41:20 1371  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 1372  	   SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 1373  END GET_GROUPS_ID_BY_INVOICE_ID;
