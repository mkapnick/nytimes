13:41:20 343  	     RAISE CAN_NOT_CREATE_NOTE;
13:41:20 344  	END;
13:41:20 345  
13:41:20 346  	BEGIN
13:41:20 347  	  PROCS_LICENSE_V21.CREATE_LICENSE(
13:41:20 348  	    in_status_id		=> GLOBAL_STATUSES_V21.LICENSE_ACTIVE,
13:41:20 349  	    in_needs_entitlements	=> GLOBAL_CONSTANTS_V21.TRUE,
13:41:20 350  	    in_start_date		=> var_date,
13:41:20 351  	    in_end_date 		=> NULL, -- Will be calculated automatically
13:41:20 352  	    in_offer_id 		=> var_first_offer_id,
13:41:20 353  	    in_subscription_id		=> var_new_subscription_id,
13:41:20 354  	    in_invoice_id		=> var_new_invoice_id,
13:41:20 355  	    in_created_by		=> in_created_by,
13:41:20 356  	    in_is_extension		=> GLOBAL_CONSTANTS_V21.FALSE,
13:41:20 357  	    in_current_offer_index	=> PROCS_OFFER_CHAIN_V21.GET_FIRST_OFFER_INDEX(in_offer_chain_id),
13:41:20 358  	    in_current_offer_recurr_num => 1,
13:41:20 359  	    out_license_id		=> out_new_license_id
13:41:20 360  	  );
13:41:20 361  	  EXCEPTION
13:41:20 362  	    WHEN OTHERS THEN
13:41:20 363  	      EXCEPTION_MESSAGE := SQLERRM;
13:41:20 364  	      RAISE CAN_NOT_CREATE_LICENSE;
13:41:20 365  	END;
13:41:20 366  
13:41:20 367  	out_subscription_id := var_new_subscription_id;
13:41:20 368  	out_invoice_id := var_new_invoice_id;
13:41:20 369  
13:41:20 370  EXCEPTION
13:41:20 371  WHEN BAD_OFFER_CHAIN_STATUS THEN
13:41:20 372  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
13:41:20 373  	  SPROC_NAME, 'Offer chain is not active');
13:41:20 374  WHEN LIMIT_REACHED THEN
13:41:20 375  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.CONFLICT_ERROR,
13:41:20 376  	  SPROC_NAME, 'Limit reached for given offer chain. Concurrent subscription id: ' || var_concur_subscription_id);
13:41:20 377  WHEN CAN_NOT_CREATE_FOR_DISABLE THEN
