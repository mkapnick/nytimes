13:41:20 924  	in_maximum_ip_high IN GROUP_ACCOUNT_IP_RANGE.MAXIMUM_IP_HIGH%TYPE,
13:41:20 925  	in_created_by IN GROUP_ACCOUNT_IP_RANGE.CREATED_BY%TYPE
13:41:20 926  ) AS
13:41:20 927  SPROC_NAME CONSTANT VARCHAR2(32) := 'ADD_IP_RANGE';
13:41:20 928  var_ga_type OFFER_CHAIN.GROUP_ACCOUNT_TYPE_ID%TYPE;
13:41:20 929  var_is_dupe NUMBER(1);
13:41:20 930  NOT_GL EXCEPTION;
13:41:20 931  DUPE EXCEPTION;
13:41:20 932  BEGIN
13:41:20 933  	  SELECT OFFER_CHAIN.GROUP_ACCOUNT_TYPE_ID into var_ga_type
13:41:20 934  	  FROM
13:41:20 935  	    GROUP_ACCOUNT,
13:41:20 936  	    SUBSCRIPTION,
13:41:20 937  	    OFFER_CHAIN
13:41:20 938  	  WHERE
13:41:20 939  	    GROUP_ACCOUNT.ID = in_group_account_id AND
13:41:20 940  	    GROUP_ACCOUNT.SUBSCRIPTION_ID = SUBSCRIPTION.ID AND
13:41:20 941  	    SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
