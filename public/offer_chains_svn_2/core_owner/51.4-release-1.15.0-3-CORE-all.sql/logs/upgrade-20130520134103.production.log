13:41:20 2703  PROCEDURE SWITCH_FINANCIAL_INSTRUMENT (
13:41:20 2704  	 in_old_fin_instrument_id   IN NUMBER,
13:41:20 2705  	 in_old_fin_instrument_type IN NUMBER,
13:41:20 2706  	 in_new_fin_instrument_id   IN NUMBER,
13:41:20 2707  	 in_new_fin_instrument_type IN NUMBER,
13:41:20 2708  	 in_updated_by		    IN VARCHAR2
13:41:20 2709  ) AS
13:41:20 2710  SPROC_NAME CONSTANT VARCHAR2(27) := 'SWITCH_FINANCIAL_INSTRUMENT';
13:41:20 2711  -- variables
13:41:20 2712  temp_out_transaction_id NUMBER;
13:41:20 2713  temp_out_charge_id      NUMBER;
13:41:20 2714  var_accounts_count      NUMBER;
13:41:20 2715  var_transaction_type_old  "TRANSACTION".TRANSACTION_TYPE_CODE%TYPE;
13:41:20 2716  var_transaction_type	 "TRANSACTION".TRANSACTION_TYPE_CODE%TYPE;
13:41:20 2717  -- EXCEPTIONS
13:41:20 2718  BAD_OLD_CC   EXCEPTION;
13:41:20 2719  BAD_OLD_PP   EXCEPTION;
13:41:20 2720  BAD_OLD_TYPE EXCEPTION;
13:41:20 2721  BAD_NEW_CC   EXCEPTION;
13:41:20 2722  BAD_NEW_PP   EXCEPTION;
13:41:20 2723  BAD_NEW_TYPE EXCEPTION;
13:41:20 2724  DIFFERENT_OWNERS EXCEPTION;
13:41:20 2725  BEGIN
13:41:20 2726  
13:41:20 2727  	 IF in_old_fin_instrument_type = GLOBAL_ENUMS_V21.INSTRUMENT_CREDIT_CARD THEN
13:41:20 2728  	   IF PROCS_FIN_INSTRUMENTS_V21.IS_CREDIT_CARD_EXISTS(in_old_fin_instrument_id) = GLOBAL_CONSTANTS_V21.FALSE THEN
13:41:20 2729  	     -- throw exception: bad old credit card
13:41:20 2730  	     RAISE BAD_OLD_CC;
13:41:20 2731  	   END IF;
13:41:20 2732  	 ELSIF in_old_fin_instrument_type = GLOBAL_ENUMS_V21.INSTRUMENT_PAYPAL THEN
13:41:20 2733  	   IF PROCS_FIN_INSTRUMENTS_V21.IS_PAYPAL_EXISTS(in_old_fin_instrument_id) = GLOBAL_CONSTANTS_V21.FALSE THEN
13:41:20 2734  	     -- throw exception: bad old paypal
13:41:20 2735  	     RAISE BAD_OLD_PP;
13:41:20 2736  	   END IF;
13:41:20 2737  	 ELSE
13:41:20 2738  	   -- throw exception: bad instrument type
13:41:20 2739  	   RAISE BAD_OLD_TYPE;
13:41:20 2740  	 END IF;
13:41:20 2741  
13:41:20 2742  	 IF in_new_fin_instrument_type = GLOBAL_ENUMS_V21.INSTRUMENT_CREDIT_CARD THEN
13:41:20 2743  	   IF PROCS_FIN_INSTRUMENTS_V21.IS_CREDIT_CARD_EXISTS(in_new_fin_instrument_id) = GLOBAL_CONSTANTS_V21.FALSE THEN
13:41:20 2744  	     -- throw exception: bad new credit card
13:41:20 2745  	     RAISE BAD_NEW_CC;
13:41:20 2746  	   END IF;
13:41:20 2747  	 ELSIF in_new_fin_instrument_type = GLOBAL_ENUMS_V21.INSTRUMENT_PAYPAL THEN
13:41:20 2748  	   IF PROCS_FIN_INSTRUMENTS_V21.IS_PAYPAL_EXISTS(in_new_fin_instrument_id) = GLOBAL_CONSTANTS_V21.FALSE THEN
13:41:20 2749  	     -- throw exception: bad new paypal
13:41:20 2750  	     RAISE BAD_NEW_PP;
13:41:20 2751  	   END IF;
13:41:20 2752  	 ELSE
13:41:20 2753  	   -- throw exception: bad new instrument type
13:41:20 2754  	   RAISE BAD_NEW_TYPE;
13:41:20 2755  	 END IF;
13:41:20 2756  
13:41:20 2757  	 -- Check that owner of both instruments - same man
13:41:20 2758  
13:41:20 2759  	 SELECT count(1) into var_accounts_count FROM (
13:41:20 2760  	   SELECT
13:41:20 2761  	     CC.ACCOUNT_ID
13:41:20 2762  	   FROM
13:41:20 2763  	     CREDIT_CARD CC
13:41:20 2764  	   WHERE
13:41:20 2765  	     (
13:41:20 2766  	       CC.ID = in_old_fin_instrument_id
13:41:20 2767  	       AND in_old_fin_instrument_type = GLOBAL_ENUMS_V21.INSTRUMENT_CREDIT_CARD
13:41:20 2768  	     )
13:41:20 2769  	     OR
13:41:20 2770  	     (
13:41:20 2771  	       CC.ID = in_new_fin_instrument_id
13:41:20 2772  	       AND in_new_fin_instrument_type = GLOBAL_ENUMS_V21.INSTRUMENT_CREDIT_CARD
13:41:20 2773  	     )
13:41:20 2774  	   UNION
13:41:20 2775  	   SELECT
