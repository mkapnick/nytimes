13:41:20 1326  	 in_invoice_id IN NUMBER,
13:41:20 1327  	 in_updated_by IN VARCHAR2
13:41:20 1328  ) AS
13:41:20 1329  SPROC_NAME CONSTANT VARCHAR2(30) := 'CLOSE_INVOICE_AS_NOT_COLLECTED';
13:41:20 1330  -- VARIABLES
13:41:20 1331  temp_invoice_id NUMBER;
13:41:20 1332  -- EXCEPTIONS
13:41:20 1333  BAD_INVOICE_ID EXCEPTION;
13:41:20 1334  BEGIN
13:41:20 1335  
13:41:20 1336  	 BEGIN
13:41:20 1337  	   SELECT
13:41:20 1338  	     INVOICE.ID into temp_invoice_id
13:41:20 1339  	   FROM
13:41:20 1340  	     INVOICE
13:41:20 1341  	   WHERE
13:41:20 1342  	     INVOICE.ID = in_invoice_id;
13:41:20 1343  	   EXCEPTION
13:41:20 1344  	     WHEN NO_DATA_FOUND THEN
13:41:20 1345  	       RAISE BAD_INVOICE_ID;
13:41:20 1346  	 END;
13:41:20 1347  
13:41:20 1348  	 -- Needs to close charges. No refund.
13:41:20 1349  	 FOR f_charge_to_close IN (
13:41:20 1350  	   SELECT CHARGE.ID FROM CHARGE WHERE CHARGE.INVOICE_ID = in_invoice_id
13:41:20 1351  	 )
13:41:20 1352  	 LOOP
13:41:20 1353  	   PROCS_CHARGE_V21.UPDATE_CHARGE_STATUS(
13:41:20 1354  	     in_charge_id	 => f_charge_to_close.ID,
13:41:20 1355  	     in_updated_by	 => in_updated_by,
13:41:20 1356  	     in_charge_status_id => GLOBAL_STATUSES_V21.CHARGE_CANCELED
13:41:20 1357  	   );
13:41:20 1358  	 END LOOP;
13:41:20 1359  
13:41:20 1360  	 --FOR f_license_to_cancel IN (
13:41:20 1361  	 --  SELECT LICENSE.ID FROM LICENSE WHERE LICENSE.INVOICE_ID = in_invoice_id AND LICENSE.LICENSE_STATUS_ID = GLOBAL_STATUSES_V21.LICENSE_ACTIVE
13:41:20 1362  	 --)
13:41:20 1363  	 --LOOP
13:41:20 1364  	 --  PROCS_LICENSE_CRU_V21.UPDATE_LICENSE(
13:41:20 1365  	 --    in_license_id	     => f_license_to_cancel.ID,
13:41:20 1366  	 --    in_license_status_id  => GLOBAL_STATUSES_V21.LICENSE_CLOSED,
13:41:20 1367  	 --    in_needs_entitlements => GLOBAL_CONSTANTS_V21.FALSE,
13:41:20 1368  	 --    in_updated_by	     => in_updated_by
13:41:20 1369  	 --  );
13:41:20 1370  	 --END LOOP;
13:41:20 1371  
13:41:20 1372  	 PROCS_INVOICE_V21.UPDATE_INVOICE_STATUS(
13:41:20 1373  	   in_invoice_id		  => in_invoice_id,
13:41:20 1374  	   in_updated_by		  => in_updated_by,
13:41:20 1375  	   in_invoice_status_id 	  => GLOBAL_STATUSES_V21.INVOICE_CLOSED
13:41:20 1376  	 );
13:41:20 1377  
13:41:20 1378  	 FOR f_transaction_to_close IN (
13:41:20 1379  	   SELECT DISTINCT CHARGE.TRANSACTION_ID AS "ID" FROM CHARGE WHERE CHARGE.INVOICE_ID = in_invoice_id
13:41:20 1380  	 )
13:41:20 1381  	 LOOP
13:41:20 1382  	   PROCS_TRANSACTION_V21.UPDATE_TRANSACTION_STATUS(
13:41:20 1383  	     in_transaction_id	      => f_transaction_to_close.ID,
13:41:20 1384  	     in_updated_by	      => in_updated_by,
13:41:20 1385  	     in_transaction_status_id => GLOBAL_STATUSES_V21.TRANSACTION_CLOSED
13:41:20 1386  	   );
13:41:20 1387  	 END LOOP;
13:41:20 1388  
13:41:20 1389  EXCEPTION
13:41:20 1390  WHEN BAD_INVOICE_ID THEN
13:41:20 1391  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 1392  	   SPROC_NAME, 'No such invoice');
13:41:20 1393  WHEN OTHERS THEN
13:41:20 1394  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 1395  	   SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 1396  END CLOSE_INVOICE_AS_NOT_COLLECTED;
