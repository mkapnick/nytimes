13:41:20 1843  PROCEDURE HAS_FUTURE_LICENSE (
13:41:20 1844  /*
13:41:20 1845  APP_EXCEPTION_CODES_V21.NOT_FOUND
13:41:20 1846  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
13:41:20 1847  --
13:41:20 1848  RETURNS:
13:41:20 1849  GLOBAL_CONSTANTS_V21.TRUE - if has,
13:41:20 1850  GLOBAL_CONSTANTS_V21.FALSE - else
13:41:20 1851  */
13:41:20 1852  	 in_license_id IN  NUMBER,
13:41:20 1853  	 out_result    OUT NUMBER
13:41:20 1854  ) AS
13:41:20 1855  -- VARIABLES
13:41:20 1856  SPROC_NAME		 CONSTANT VARCHAR2(18) := 'HAS_FUTURE_LICENSE';
13:41:20 1857  var_subscription_id	 NUMBER;
13:41:20 1858  var_future_licenses_count NUMBER;
13:41:20 1859  -- EXCEPTIONS
13:41:20 1860  BAD_LICENSE_ID	    EXCEPTION;
13:41:20 1861  BEGIN
13:41:20 1862  
13:41:20 1863  	 BEGIN
13:41:20 1864  	   SELECT
13:41:20 1865  	     LICENSE.SUBSCRIPTION_ID into var_subscription_id
13:41:20 1866  	   FROM
13:41:20 1867  	     LICENSE
13:41:20 1868  	   WHERE
13:41:20 1869  	     LICENSE.ID = in_license_id;
13:41:20 1870  	   EXCEPTION
13:41:20 1871  	     WHEN NO_DATA_FOUND THEN
13:41:20 1872  	       RAISE BAD_LICENSE_ID;
13:41:20 1873  	 END;
13:41:20 1874  
13:41:20 1875  	 SELECT
13:41:20 1876  	   COUNT(*) into var_future_licenses_count
13:41:20 1877  	 FROM
13:41:20 1878  	   LICENSE
13:41:20 1879  	 WHERE
13:41:20 1880  	   LICENSE.ID != in_license_id
13:41:20 1881  	   AND LICENSE.SUBSCRIPTION_ID = var_subscription_id
13:41:20 1882  	   AND LICENSE.END_DATE > sysdate;
13:41:20 1883  
13:41:20 1884  	 IF var_future_licenses_count > 0 THEN
13:41:20 1885  	   out_result := GLOBAL_CONSTANTS_V21.TRUE;
13:41:20 1886  	 ELSE
13:41:20 1887  	   out_result := GLOBAL_CONSTANTS_V21.FALSE;
13:41:20 1888  	 END IF;
13:41:20 1889  
13:41:20 1890  EXCEPTION
13:41:20 1891  WHEN BAD_LICENSE_ID THEN
13:41:20 1892  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 1893  	   SPROC_NAME, 'No such license');
13:41:20 1894  WHEN OTHERS THEN
13:41:20 1895  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 1896  	   SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 1897  END HAS_FUTURE_LICENSE;
