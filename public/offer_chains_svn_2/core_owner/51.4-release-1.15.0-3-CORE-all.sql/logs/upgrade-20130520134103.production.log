13:41:20 2641  
13:41:20 2642  PROCEDURE GET_SUBSCR_PROD_OFFERRINGS (
13:41:20 2643  /*
13:41:20 2644  APP_EXCEPTION_CODES_V21.NOT_FOUND
13:41:20 2645  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
13:41:20 2646  */
13:41:20 2647  	 in_subscription_id IN NUMBER,
13:41:20 2648  	 out_result_set     OUT SYS_REFCURSOR
13:41:20 2649  ) AS
13:41:20 2650  SPROC_NAME CONSTANT VARCHAR2(26) := 'GET_SUBSCR_PROD_OFFERRINGS';
13:41:20 2651  -- VARIABLES
13:41:20 2652  var_offer NUMBER;
13:41:20 2653  -- EXCEPTIONS
13:41:20 2654  BAD_SUBSCRIPTION_ID EXCEPTION;
13:41:20 2655  BEGIN
13:41:20 2656  
13:41:20 2657  	 OPEN out_result_set FOR
13:41:20 2658  	 SELECT DISTINCT
13:41:20 2659  	   PRODUCT_OFFERING.ID,
13:41:20 2660  	   PRODUCT_OFFERING.PRODUCT_ID,
13:41:20 2661  	   PRODUCT_OFFERING.TAX_CATEGORY_ID,
13:41:20 2662  	   PRODUCT_OFFERING.UNIT_PRICE,
13:41:20 2663  	   PRODUCT_OFFERING.QUANTITY,
13:41:20 2664  	   PRODUCT_OFFERING.CREATE_DATE,
13:41:20 2665  	   PRODUCT_OFFERING.CREATED_BY,
13:41:20 2666  	   CAPABILITY.ID CAP_ID,
13:41:20 2667  	   CAPABILITY.CODE CAP_CODE,
13:41:20 2668  	   CAPABILITY.DESCRIPTION CAP_DESCRIPTION,
13:41:20 2669  	   CAPABILITY.SHAREABLE CAP_SHAREABLE
13:41:20 2670  	 FROM
13:41:20 2671  	   OFFER_PRODUCT_OFFERING
13:41:20 2672  	   INNER JOIN PRODUCT_OFFERING ON PRODUCT_OFFERING.ID = OFFER_PRODUCT_OFFERING.PRODUCT_OFFERING_ID
13:41:20 2673  	   INNER JOIN CAPABILITY ON PRODUCT_OFFERING.CAPABILITY_ID = CAPABILITY.ID
13:41:20 2674  	 WHERE
13:41:20 2675  	   OFFER_PRODUCT_OFFERING.OFFER_ID IN (
13:41:20 2676  	     SELECT
13:41:20 2677  	       LICENSE.OFFER_ID
13:41:20 2678  	     FROM
13:41:20 2679  	       SUBSCRIPTION
13:41:20 2680  	       JOIN LICENSE ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID AND LICENSE.NEEDS_ENTITLEMENTS = GLOBAL_CONSTANTS_V21.TRUE
13:41:20 2681  	     WHERE
13:41:20 2682  	       SUBSCRIPTION.ID = in_subscription_id
13:41:20 2683  	   );
