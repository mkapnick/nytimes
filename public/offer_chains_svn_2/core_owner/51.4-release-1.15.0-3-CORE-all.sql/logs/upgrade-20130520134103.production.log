13:41:20 1405  
13:41:20 1406  PROCEDURE REDEEM_GIFT_CERTIFICATE (
13:41:20 1407  	 in_group_id			 IN NUMBER,
13:41:20 1408  	 in_gift_certificate_code	 IN VARCHAR2,
13:41:20 1409  	 in_created_by			 IN VARCHAR2,
13:41:20 1410  	 in_redeemer_address_id 	 IN NUMBER,
