13:41:20 814  	  INNER JOIN OFFER_CHAIN ON SUBSCRIPTION.OFFER_CHAIN_ID = OFFER_CHAIN.ID
13:41:20 815  	WHERE
13:41:20 816  	  SUBSCRIPTION.ACCOUNT_ID = var_account_id
13:41:20 817  	  AND (SUBSCRIPTION.SCT_ID IS NULL OR SUBSCRIPTION.SCT_ID != GLOBAL_STATUSES_V21.REAL_TIME_CANCEL_REASON)
13:41:20 818  	  AND SUBSCRIPTION.ID = NVL(in_subscription_id, SUBSCRIPTION.ID);
