13:41:19 680  FUNCTION GET_NEXT_OFFER_INDEX (
13:41:19 681  	in_offer_chain_id	     IN NUMBER,
13:41:19 682  	in_offer_chain_current_index IN NUMBER
13:41:19 683  ) RETURN NUMBER AS
13:41:19 684  -- VARIABLES
13:41:19 685  SPROC_NAME		     CONSTANT VARCHAR2(20) := 'GET_NEXT_OFFER_INDEX';
13:41:19 686  temp_offer_chain_id	     NUMBER;
13:41:19 687  temp_offer_chain_current_index NUMBER;
13:41:19 688  var_result		     NUMBER;
13:41:19 689  -- EXCEPTIONS
13:41:19 690  BAD_OFFER_CHAIN_ID      EXCEPTION;
13:41:19 691  BAD_CURRENT_INDEX_VALUE EXCEPTION;
13:41:19 692  BEGIN
13:41:19 693  
13:41:19 694  	-- Check that offer chain exists
13:41:19 695  	BEGIN
13:41:19 696  	  SELECT
13:41:19 697  	    OFFER_CHAIN.ID into temp_offer_chain_id
13:41:19 698  	  FROM
13:41:19 699  	    OFFER_CHAIN
13:41:19 700  	  WHERE
13:41:19 701  	    OFFER_CHAIN.ID = in_offer_chain_id;
13:41:19 702  	  EXCEPTION
13:41:19 703  	    WHEN NO_DATA_FOUND THEN
13:41:19 704  	      RAISE BAD_OFFER_CHAIN_ID;
13:41:19 705  	END;
13:41:19 706  
13:41:19 707  	-- Check that current offer index exists
13:41:19 708  	BEGIN
13:41:19 709  	  SELECT
13:41:19 710  	    OFFER_OFFER_CHAIN.INDEX_VALUE into temp_offer_chain_current_index
13:41:19 711  	  FROM
13:41:19 712  	    OFFER_OFFER_CHAIN
13:41:19 713  	  WHERE
13:41:19 714  	    OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
13:41:19 715  	    AND OFFER_OFFER_CHAIN.INDEX_VALUE = in_offer_chain_current_index
13:41:19 716  	    -- TODO: delete next line
13:41:19 717  	    AND ROWNUM <= 1;
13:41:19 718  	  EXCEPTION
13:41:19 719  	    WHEN NO_DATA_FOUND THEN
13:41:19 720  	      RAISE BAD_CURRENT_INDEX_VALUE;
13:41:19 721  	END;
13:41:19 722  
13:41:19 723  	-- Get next offer index
13:41:19 724  	BEGIN
13:41:19 725  	  SELECT
13:41:19 726  	    INDEX_VALUE into var_result
13:41:19 727  	  FROM (
13:41:19 728  	    SELECT
13:41:19 729  	      OFFER_OFFER_CHAIN.INDEX_VALUE
13:41:19 730  	    FROM
13:41:19 731  	      OFFER_OFFER_CHAIN
13:41:19 732  	    WHERE
13:41:19 733  	      OFFER_OFFER_CHAIN.OFFER_CHAIN_ID = in_offer_chain_id
13:41:19 734  	      AND OFFER_OFFER_CHAIN.INDEX_VALUE > in_offer_chain_current_index
13:41:19 735  	    ORDER BY
13:41:19 736  	      OFFER_OFFER_CHAIN.INDEX_VALUE ASC
13:41:19 737  	  )
13:41:19 738  	  WHERE
13:41:19 739  	    ROWNUM <= 1;
13:41:19 740  	  EXCEPTION
13:41:19 741  	    WHEN NO_DATA_FOUND THEN
13:41:19 742  	      var_result := NULL;
13:41:19 743  	END;
13:41:19 744  
13:41:19 745  	RETURN var_result;
13:41:19 746  
13:41:19 747  EXCEPTION
13:41:19 748  WHEN BAD_OFFER_CHAIN_ID THEN
13:41:19 749  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:19 750  	  SPROC_NAME, 'No such offer chain');
13:41:19 751  WHEN BAD_CURRENT_INDEX_VALUE THEN
13:41:19 752  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:19 753  	  SPROC_NAME, 'Bad current index value');
