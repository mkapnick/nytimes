13:41:20 318  PROCEDURE DISABLE_CREDIT_CARD (
13:41:20 319  /*
13:41:20 320  Throws exceptions:
13:41:20 321  APP_EXCEPTION_CODES_V21.NOT_FOUND
13:41:20 322  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
13:41:20 323  APP_EXCEPTION_CODES_V21.INTERNAL_ERROR
13:41:20 324  */
13:41:20 325  	in_credit_card_id IN NUMBER,
13:41:20 326  	in_updated_by	  IN VARCHAR2
13:41:20 327  ) AS
13:41:20 328  SPROC_NAME		   CONSTANT VARCHAR2(19) := 'DISABLE_CREDIT_CARD';
13:41:20 329  -- VARIBLES
13:41:20 330  var_account_id		     NUMBER;
13:41:20 331  var_group_id		     NUMBER;
13:41:20 332  var_credit_card_status	     NUMBER;
13:41:20 333  var_pending_transactions_num   NUMBER;
13:41:20 334  var_pending_invoices_num	     NUMBER;
13:41:20 335  current_def_instrument_type_id NUMBER;
13:41:20 336  current_def_instrument_id      NUMBER;
13:41:20 337  -- EXCEPTIONS
13:41:20 338  BAD_CC_STATUS		   EXCEPTION;
13:41:20 339  PENDING_TRANSACTIONS_FOUNDED EXCEPTION;
13:41:20 340  CAN_NOT_GET_DEF_FINANCIAL    EXCEPTION;
13:41:20 341  CAN_NOT_DEL_DEF_FINANCIAL    EXCEPTION;
13:41:20 342  CAN_NOT_DISABLE_CREDIT_CARD  EXCEPTION;
13:41:20 343  EXCEPTION_MESSAGE 	   VARCHAR2(1024);
13:41:20 344  BEGIN
13:41:20 345  
13:41:20 346  	-- Get credit card status
13:41:20 347  	-- Get account id
13:41:20 348  	SELECT
13:41:20 349  	  CREDIT_CARD.CREDIT_CARD_STATUS_ID,
13:41:20 350  	  CREDIT_CARD.ACCOUNT_ID
13:41:20 351  	  into
13:41:20 352  	  var_credit_card_status,
13:41:20 353  	  var_account_id
13:41:20 354  	FROM
13:41:20 355  	  CREDIT_CARD
13:41:20 356  	WHERE
13:41:20 357  	  CREDIT_CARD.ID = in_credit_card_id;
13:41:20 358  
13:41:20 359  	-- Check that we can disable this credit card (STUB)
13:41:20 360  	IF F_CAN_DISABLE_CREDIT_CARD(in_credit_card_id) = GLOBAL_CONSTANTS_V21.FALSE THEN
13:41:20 361  	  RAISE CAN_NOT_DISABLE_CREDIT_CARD;
13:41:20 362  	END IF;
13:41:20 363  
13:41:20 364  	-- Get account id
13:41:20 365  	SELECT
13:41:20 366  	  ACCOUNT.GROUP_ID into var_group_id
13:41:20 367  	FROM
13:41:20 368  	  ACCOUNT
13:41:20 369  	WHERE
13:41:20 370  	  ACCOUNT.ID = var_account_id;
13:41:20 371  
13:41:20 372  	-- Card should to be active
13:41:20 373  	IF var_credit_card_status != GLOBAL_STATUSES_V21.CREDIT_CARD_ACTIVE THEN
13:41:20 374  	  RAISE BAD_CC_STATUS;
13:41:20 375  	END IF;
13:41:20 376  
13:41:20 377  	-- Looking for pending transactions associated with given credit card
13:41:20 378  	SELECT
13:41:20 379  	  COUNT(*) into var_pending_invoices_num
13:41:20 380  	FROM
13:41:20 381  	  CHARGE
13:41:20 382  	  INNER JOIN TRANSACTION ON CHARGE.TRANSACTION_ID = TRANSACTION.ID
13:41:20 383  	WHERE
13:41:20 384  	  CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V21.INSTRUMENT_CREDIT_CARD
13:41:20 385  	  AND CHARGE.INSTRUMENT_ID = in_credit_card_id
13:41:20 386  	  AND TRANSACTION.TRANSACTION_STATUS_ID = GLOBAL_STATUSES_V21.TRANSACTION_PENDING;
13:41:20 387  
13:41:20 388  	IF var_pending_invoices_num > 0 THEN
13:41:20 389  	  RAISE PENDING_TRANSACTIONS_FOUNDED;
13:41:20 390  	END IF;
13:41:20 391  
13:41:20 392  	-- Getting current default financial instrument
13:41:20 393  	BEGIN
13:41:20 394  	  GET_DEF_FINANCIAL_INSTRUMENT(
13:41:20 395  	    in_group_id 	   => var_group_id,
13:41:20 396  	    out_instrument_type_id => current_def_instrument_type_id,
13:41:20 397  	    out_instrument_id	   => current_def_instrument_id
13:41:20 398  	  );
13:41:20 399  	  EXCEPTION
13:41:20 400  	    WHEN OTHERS THEN
13:41:20 401  	      EXCEPTION_MESSAGE := SQLERRM;
13:41:20 402  	      RAISE CAN_NOT_GET_DEF_FINANCIAL;
13:41:20 403  	END;
13:41:20 404  
13:41:20 405  	-- Checking that credit card is not default
13:41:20 406  	IF current_def_instrument_type_id = GLOBAL_ENUMS_V21.INSTRUMENT_CREDIT_CARD
13:41:20 407  	  AND current_def_instrument_id = in_credit_card_id THEN
13:41:20 408  	  BEGIN
13:41:20 409  	    DEL_DEF_FINANCIAL_INSTRUMENT(
13:41:20 410  	      in_group_id => var_group_id
13:41:20 411  	    );
13:41:20 412  	  EXCEPTION
13:41:20 413  	    WHEN OTHERS THEN
13:41:20 414  	      EXCEPTION_MESSAGE := SQLERRM;
13:41:20 415  	      RAISE CAN_NOT_DEL_DEF_FINANCIAL;
13:41:20 416  	  END;
13:41:20 417  	END IF;
13:41:20 418  
13:41:20 419  	-- Update credit card status
13:41:20 420  	PROCS_FIN_INSTRUMENTS_V21.UPDATE_CREDIT_CARD_STATUS(
13:41:20 421  	  in_credit_card_id	   => in_credit_card_id,
13:41:20 422  	  in_updated_by 	   => in_updated_by,
13:41:20 423  	  in_credit_card_status_id => GLOBAL_STATUSES_V21.CREDIT_CARD_DISABLED
13:41:20 424  	);
13:41:20 425  
13:41:20 426  EXCEPTION
13:41:20 427  WHEN NO_DATA_FOUND THEN
13:41:20 428  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 429  	  SPROC_NAME, 'No such credit card');
13:41:20 430  WHEN CAN_NOT_GET_DEF_FINANCIAL THEN
13:41:20 431  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 432  	  SPROC_NAME, 'Could not get current default financial instrument', EXCEPTION_MESSAGE);
13:41:20 433  WHEN CAN_NOT_DEL_DEF_FINANCIAL THEN
13:41:20 434  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 435  	  SPROC_NAME, 'Could not delete information about default financial instrument from account', EXCEPTION_MESSAGE);
13:41:20 436  WHEN BAD_CC_STATUS THEN
13:41:20 437  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
13:41:20 438  	  SPROC_NAME, 'Credit card is not active');
13:41:20 439  WHEN PENDING_TRANSACTIONS_FOUNDED THEN
13:41:20 440  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
13:41:20 441  	  SPROC_NAME, 'Account has pending charge which is using this card');
13:41:20 442  WHEN CAN_NOT_DISABLE_CREDIT_CARD THEN
13:41:20 443  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 444  	  SPROC_NAME, 'Could not disable this credit card', EXCEPTION_MESSAGE);
13:41:20 445  WHEN OTHERS THEN
13:41:20 446  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 447  	  SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 448  END DISABLE_CREDIT_CARD;
13:41:20 449  
13:41:20 450  /******************************************************************************/
13:41:20 451  
13:41:20 452  PROCEDURE DISABLE_PAYPAL (
13:41:20 453  /*
