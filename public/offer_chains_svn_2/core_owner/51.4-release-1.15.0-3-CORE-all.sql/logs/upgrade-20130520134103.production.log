13:41:20 400  FUNCTION IS_CHARGE_COLLECTED (
13:41:20 401  /*
13:41:20 402  Throws:
13:41:20 403  APP_EXCEPTION_CODES_V21.NOT_FOUND
13:41:20 404  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
13:41:20 405  Returns:
13:41:20 406  GLOBAL_CONST.TRUE if transaction collected,
13:41:20 407  GLOBAL_CONST.FALSE else
13:41:20 408  */
13:41:20 409  	in_charge_id IN NUMBER
13:41:20 410  ) RETURN NUMBER AS
13:41:20 411  SPROC_NAME CONSTANT VARCHAR2(19) := 'IS_CHARGE_COLLECTED';
13:41:20 412  -- VARIABLES
13:41:20 413  var_transaction_id NUMBER;
13:41:20 414  is_transaction_collected NUMBER;
13:41:20 415  -- EXCEPTIONS
13:41:20 416  BAD_CHARGE_ID	       EXCEPTION;
13:41:20 417  CAN_NOT_CHECK_TRANSACTION EXCEPTION;
13:41:20 418  EXCEPTION_MESSAGE        VARCHAR2(1024);
13:41:20 419  BEGIN
13:41:20 420  
13:41:20 421  	BEGIN
13:41:20 422  	  SELECT
13:41:20 423  	    CHARGE.TRANSACTION_ID into var_transaction_id
13:41:20 424  	  FROM
13:41:20 425  	    CHARGE
13:41:20 426  	  WHERE
13:41:20 427  	    CHARGE.ID = in_charge_id;
13:41:20 428  	  EXCEPTION
13:41:20 429  	    WHEN NO_DATA_FOUND THEN
13:41:20 430  	      RAISE BAD_CHARGE_ID;
13:41:20 431  	END;
13:41:20 432  
13:41:20 433  	BEGIN
13:41:20 434  	  is_transaction_collected := PROCS_TRANSACTION_V21.IS_TRANSACTION_COLLECTED(var_transaction_id);
13:41:20 435  	  EXCEPTION
13:41:20 436  	    WHEN OTHERS THEN
13:41:20 437  	      EXCEPTION_MESSAGE := SQLERRM;
13:41:20 438  	      RAISE CAN_NOT_CHECK_TRANSACTION;
13:41:20 439  	END;
13:41:20 440  
13:41:20 441  	RETURN is_transaction_collected;
13:41:20 442  
13:41:20 443  EXCEPTION
13:41:20 444  WHEN BAD_CHARGE_ID THEN
