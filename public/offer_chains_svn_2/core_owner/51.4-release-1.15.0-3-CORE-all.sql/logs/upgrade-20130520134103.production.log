13:41:19 1076  FUNCTION CHECK_FOR_SAME_PRODUCTS (
13:41:19 1077  	 in_offer_chain_1	  IN OFFER_CHAIN.ID%TYPE,
13:41:19 1078  	 in_offer_chain_2	  IN OFFER_CHAIN.ID%TYPE,
13:41:19 1079  	 in_use_eligibility_rules IN NUMBER DEFAULT GLOBAL_CONSTANTS_V21.FALSE
13:41:19 1080  ) RETURN NUMBER AS
13:41:19 1081  SPROC_NAME CONSTANT VARCHAR2(23) := 'CHECK_FOR_SAME_PRODUCTS';
13:41:19 1082  -- CONSTANTS
13:41:19 1083  PRODUCT_ELIGIBILITY_NAME CONSTANT VARCHAR2(19) := 'MAX_CONCURRENT_SUBS';
13:41:19 1084  -- VARIABLES
13:41:19 1085  temp_offer_chain_id	OFFER_CHAIN.ID%TYPE;
13:41:19 1086  var_same_products	SYS_REFCURSOR;
13:41:19 1087  var_same_product_id	NUMBER;
13:41:19 1088  same_product_count	NUMBER;
13:41:19 1089  var_product_eligibility_limit NUMBER;
13:41:19 1090  s_product_eligibility_limit   VARCHAR2(100);
13:41:19 1091  -- EXCEPTIONS
13:41:19 1092  BAD_FIRST_OFFER_CHAIN	      EXCEPTION;
13:41:19 1093  BAD_SECOND_OFFER_CHAIN	      EXCEPTION;
13:41:19 1094  BEGIN
13:41:19 1095  
13:41:19 1096  	 -- Check that first offer chain exists
13:41:19 1097  	 BEGIN
13:41:19 1098  	   SELECT
13:41:19 1099  	     OFFER_CHAIN.ID into temp_offer_chain_id
13:41:19 1100  	   FROM
13:41:19 1101  	     OFFER_CHAIN
13:41:19 1102  	   WHERE
13:41:19 1103  	     OFFER_CHAIN.ID = in_offer_chain_1;
13:41:19 1104  	   EXCEPTION
13:41:19 1105  	     WHEN NO_DATA_FOUND THEN
13:41:19 1106  	       RAISE BAD_FIRST_OFFER_CHAIN;
13:41:19 1107  	 END;
13:41:19 1108  
13:41:19 1109  	 -- Check that second offer chain exists
13:41:19 1110  	 BEGIN
13:41:19 1111  	   SELECT
13:41:19 1112  	     OFFER_CHAIN.ID into temp_offer_chain_id
13:41:19 1113  	   FROM
13:41:19 1114  	     OFFER_CHAIN
13:41:19 1115  	   WHERE
13:41:19 1116  	     OFFER_CHAIN.ID = in_offer_chain_2;
13:41:19 1117  	   EXCEPTION
13:41:19 1118  	     WHEN NO_DATA_FOUND THEN
13:41:19 1119  	       RAISE BAD_SECOND_OFFER_CHAIN;
13:41:19 1120  	 END;
13:41:19 1121  
13:41:19 1122  	 PROCS_OFFER_CHAIN_V21.GET_OFF_CHAINS_SAME_PRODUCTS(
13:41:19 1123  	   in_offer_chain_1 => in_offer_chain_1,
13:41:19 1124  	   in_offer_chain_2 => in_offer_chain_2,
13:41:19 1125  	   out_result_set   => var_same_products
13:41:19 1126  	 );
13:41:19 1127  
13:41:19 1128  	 LOOP
13:41:19 1129  	   FETCH var_same_products INTO var_same_product_id, same_product_count;
13:41:19 1130  	   EXIT WHEN var_same_products%NOTFOUND;
13:41:19 1131  
13:41:19 1132  	   IF in_use_eligibility_rules = GLOBAL_CONSTANTS_V21.FALSE THEN
13:41:19 1133  	     -- Return false because this offer chains having same products
13:41:19 1134  	     RETURN GLOBAL_CONSTANTS_V21.TRUE;
13:41:19 1135  	   ELSE
13:41:19 1136  
13:41:19 1137  	     -- Get eligibility rule for given product
13:41:19 1138  	     BEGIN
13:41:19 1139  	       SELECT
13:41:19 1140  		 PRODUCT_ELIGIBILITY.VALUE into s_product_eligibility_limit
13:41:19 1141  	       FROM
13:41:19 1142  		 PRODUCT_ELIGIBILITY
13:41:19 1143  	       WHERE
13:41:19 1144  		 PRODUCT_ELIGIBILITY.PRODUCT_ID = var_same_product_id
13:41:19 1145  		 AND PRODUCT_ELIGIBILITY.NAME = PRODUCT_ELIGIBILITY_NAME;
13:41:19 1146  
13:41:19 1147  	       -- REVU: What should to be here? 1?
13:41:19 1148  	       EXCEPTION
13:41:19 1149  		 WHEN NO_DATA_FOUND THEN
13:41:19 1150  		   s_product_eligibility_limit := '1';
13:41:19 1151  	     END;
13:41:19 1152  
13:41:19 1153  	     IF UPPER(s_product_eligibility_limit) = GLOBAL_CONSTANTS_V21.MAX_CONSURRENT_PRD_UNLIM THEN
13:41:19 1154  	       RETURN GLOBAL_CONSTANTS_V21.FALSE;
13:41:19 1155  	     END IF;
13:41:19 1156  
13:41:19 1157  	     var_product_eligibility_limit := TO_NUMBER(s_product_eligibility_limit);
13:41:19 1158  
13:41:19 1159  	     -- Check for limit
13:41:19 1160  	     IF var_product_eligibility_limit < same_product_count THEN
13:41:19 1161  	       RETURN GLOBAL_CONSTANTS_V21.TRUE;
13:41:19 1162  	     END IF;
13:41:19 1163  
13:41:19 1164  	   END IF;
13:41:19 1165  	 END LOOP;
13:41:19 1166  
13:41:19 1167  	 RETURN GLOBAL_CONSTANTS_V21.FALSE;
13:41:19 1168  
