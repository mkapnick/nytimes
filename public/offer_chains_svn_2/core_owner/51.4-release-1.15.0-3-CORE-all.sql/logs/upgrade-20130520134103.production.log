13:41:20 141  SPROC_NAME	      CONSTANT VARCHAR2(15) := 'DISABLE_ACCOUNT';
13:41:20 142  var_account_id	      NUMBER;
13:41:20 143  current_account_status  NUMBER;
13:41:20 144  
13:41:20 145  var_active_subscriptions_num NUMBER;
