13:41:19 458  ) AS
13:41:19 459  -- VARIABLES
13:41:19 460  SPROC_NAME		CONSTANT VARCHAR2(14) := 'GET_NEXT_OFFER';
13:41:19 461  var_offer_chain_id	NUMBER;
