13:41:20 1691  TOT_REF_IS_GRATER_THAN_CHARGES EXCEPTION;
13:41:20 1692  EXCEPTION_MESSAGE	      VARCHAR2(1024);
13:41:20 1693  BEGIN
13:41:20 1694  
13:41:20 1695  	 BEGIN
13:41:20 1696  	   SELECT
13:41:20 1697  	     INVOICE.INVOICE_STATUS_ID into var_invoice_status_id
13:41:20 1698  	   FROM
13:41:20 1699  	     INVOICE
13:41:20 1700  	   WHERE
13:41:20 1701  	     INVOICE.ID = in_invoice_id;
13:41:20 1702  	   EXCEPTION
13:41:20 1703  	     WHEN NO_DATA_FOUND THEN
13:41:20 1704  	       RAISE BAD_INVOICE_ID;
13:41:20 1705  	 END;
13:41:20 1706  
13:41:20 1707  	 -- Get instrument and subscription id if exists
13:41:20 1708  	 BEGIN
13:41:20 1709  	   SELECT
13:41:20 1710  	     SUBSCRIPTION.INSTRUMENT_ID,
13:41:20 1711  	     SUBSCRIPTION.INSTRUMENT_TYPE_ID,
13:41:20 1712  	     SUBSCRIPTION.ACCOUNT_ID,
