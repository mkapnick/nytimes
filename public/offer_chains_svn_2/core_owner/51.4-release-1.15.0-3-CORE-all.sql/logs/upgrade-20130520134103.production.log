13:41:19 127  ) AS
13:41:19 128  SPROC_NAME CONSTANT VARCHAR2(12) := 'LOCK_ACCOUNT';
13:41:19 129  -- CONSTANTS
13:41:19 130  one_second_interval CONSTANT INTERVAL DAY TO SECOND := INTERVAL '0 00:00:01' DAY TO SECOND;
13:41:19 131  -- VARIABLES
13:41:19 132  var_account_id NUMBER;
13:41:19 133  -- arosolovskiy: using data variable instead of direct get data calls (#38860)
13:41:19 134  var_date		DATE := SYSDATE;
13:41:19 135  var_lock_end_date DATE;
13:41:19 136  -- EXCEPTIONS
