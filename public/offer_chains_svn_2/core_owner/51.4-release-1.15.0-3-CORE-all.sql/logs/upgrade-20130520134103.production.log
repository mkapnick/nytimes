13:41:20 324  PROCEDURE GET_INVOICE_DETAILS (
13:41:20 325  /*
13:41:20 326  Throws exceptions:
13:41:20 327  APP_EXCEPTION_CODES_V21.NOT_FOUND
13:41:20 328  APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR
13:41:20 329  */
13:41:20 330  	in_invoice_id	   IN  NUMBER,
13:41:20 331  	out_group_id	   OUT NUMBER,
13:41:20 332  	out_status_id	   OUT NUMBER,
13:41:20 333  	out_line_items_set OUT SYS_REFCURSOR,
13:41:20 334  	out_pp_charges_set OUT SYS_REFCURSOR,
13:41:20 335  	out_cc_charges_set OUT SYS_REFCURSOR,
13:41:20 336  	out_gc_charges_set OUT SYS_REFCURSOR
13:41:20 337  ) AS
13:41:20 338  -- VARIABLES
13:41:20 339  SPROC_NAME CONSTANT VARCHAR2(19) := 'GET_INVOICE_DETAILS';
13:41:20 340  
13:41:20 341  -- EXCEPTIONS
13:41:20 342  BAD_INVOICE_ID	     EXCEPTION;
13:41:20 343  CAN_NOT_FIND_ACCOUNT   EXCEPTION;
13:41:20 344  EXCEPTION_MESSAGE      VARCHAR2(1024);
13:41:20 345  BEGIN
13:41:20 346  
13:41:20 347  	-- get invoice status
13:41:20 348  	BEGIN
13:41:20 349  	  SELECT
13:41:20 350  	    INVOICE.INVOICE_STATUS_ID into out_status_id
13:41:20 351  	  FROM
13:41:20 352  	    INVOICE
13:41:20 353  	  WHERE
13:41:20 354  	    INVOICE.ID = in_invoice_id;
13:41:20 355  	  EXCEPTION
13:41:20 356  	    WHEN NO_DATA_FOUND THEN
13:41:20 357  	      RAISE BAD_INVOICE_ID;
13:41:20 358  	END;
13:41:20 359  
13:41:20 360  	-- get group id
13:41:20 361  	BEGIN
13:41:20 362  	  PROCS_INVOICE_V21.GET_ACCOUNT_BY_INVOICE_ID(in_invoice_id, out_group_id);
13:41:20 363  	  EXCEPTION
13:41:20 364  	    WHEN OTHERS THEN
13:41:20 365  	      EXCEPTION_MESSAGE := SQLERRM;
13:41:20 366  	      RAISE CAN_NOT_FIND_ACCOUNT;
13:41:20 367  	END;
13:41:20 368  
13:41:20 369  	-- get all line items for given invoice
13:41:20 370  	OPEN out_line_items_set FOR
13:41:20 371  	SELECT
13:41:20 372  	  LINE_ITEM.AMOUNT,
13:41:20 373  	  LINE_ITEM.ID,
13:41:20 374  	  LINE_ITEM.CREATED_BY,
13:41:20 375  	  LINE_ITEM.CREATE_DATE,
13:41:20 376  	  LINE_ITEM.DISCOUNT_AMOUNT,
13:41:20 377  	  LINE_ITEM.TAXES_AMOUNT,
13:41:20 378  	  LINE_ITEM.PRODUCT_OFFER_ID,
13:41:20 379  	  LINE_ITEM.INVOICE_ID
13:41:20 380  	FROM
13:41:20 381  	  LINE_ITEM
13:41:20 382  	WHERE
13:41:20 383  	  LINE_ITEM.INVOICE_ID = in_invoice_id;
13:41:20 384  
13:41:20 385  	-- get all pp charges for given invoice
13:41:20 386  	OPEN out_pp_charges_set FOR
13:41:20 387  	SELECT
13:41:20 388  	  CHARGE.ID as "CHARGE_ID",
13:41:20 389  	  CHARGE_AMOUNT,
13:41:20 390  	  CHARGE.INSTRUMENT_ID,
13:41:20 391  	  CHARGE.CHARGE_STATUS_ID
13:41:20 392  	FROM
13:41:20 393  	  CHARGE
13:41:20 394  	WHERE
13:41:20 395  	  CHARGE.INVOICE_ID = in_invoice_id
13:41:20 396  	  AND CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V21.INSTRUMENT_PAYPAL;
13:41:20 397  
13:41:20 398  	-- get all credir cards for given invoice
13:41:20 399  	OPEN out_cc_charges_set FOR
13:41:20 400  	SELECT
13:41:20 401  	  CHARGE.ID as "CHARGE_ID",
13:41:20 402  	  CHARGE.CHARGE_AMOUNT,
13:41:20 403  	  CHARGE.INSTRUMENT_ID,
13:41:20 404  	  CHARGE.CHARGE_STATUS_ID
13:41:20 405  	FROM
13:41:20 406  	  CHARGE
13:41:20 407  	WHERE
13:41:20 408  	  CHARGE.INVOICE_ID = in_invoice_id
13:41:20 409  	  AND CHARGE.INSTRUMENT_TYPE_ID = GLOBAL_ENUMS_V21.INSTRUMENT_CREDIT_CARD;
13:41:20 410  
13:41:20 411  	OPEN out_gc_charges_set FOR
