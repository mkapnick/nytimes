13:41:20 435  		    AND GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID = GLOBAL_STATUSES_V21.GIFT_CERTIFICATE_ACTIVE
13:41:20 436  		  )
13:41:20 437  		)
13:41:20 438  		OR
13:41:20 439  		(
13:41:20 440  		  in_instr_status = GLOBAL_CONSTANTS_V21.FALSE AND
13:41:20 441  		  (
13:41:20 442  		    GIFT_CERTIFICATE.EXPIRATION_DATE < current_date
13:41:20 443  		    OR GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID != GLOBAL_STATUSES_V21.GIFT_CERTIFICATE_ACTIVE
13:41:20 444  		  )
13:41:20 445  		)
13:41:20 446  	      );
13:41:20 447  
13:41:20 448  EXCEPTION
13:41:20 449  WHEN NO_DATA_FOUND THEN
13:41:20 450  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 451  	  SPROC_NAME, 'Can not find account with given group id');
