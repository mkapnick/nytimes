13:41:20 1016  
13:41:20 1017  PROCEDURE GET_MAX_REFUND (
13:41:20 1018  	 in_invoice_id IN NUMBER,
13:41:20 1019  	 out_amount    OUT NUMBER
13:41:20 1020  ) AS
13:41:20 1021  SPROC_NAME CONSTANT VARCHAR2(14) := 'GET_MAX_REFUND';
13:41:20 1022  -- VARIABLES
13:41:20 1023  temp_invoice_id	       NUMBER;
13:41:20 1024  var_invoice_refunds_sum NUMBER(10,2);
13:41:20 1025  var_invoice_charges_sum NUMBER(10,2);
13:41:20 1026  -- EXCEPTIONS
13:41:20 1027  BAD_INVOICE_ID EXCEPTION;
13:41:20 1028  BEGIN
13:41:20 1029  
13:41:20 1030  	 BEGIN
13:41:20 1031  	   SELECT
13:41:20 1032  	     INVOICE.ID into temp_invoice_id
13:41:20 1033  	   FROM
13:41:20 1034  	     INVOICE
13:41:20 1035  	   WHERE
13:41:20 1036  	     INVOICE.ID = in_invoice_id;
13:41:20 1037  	   EXCEPTION
13:41:20 1038  	     WHEN NO_DATA_FOUND THEN
13:41:20 1039  	       RAISE BAD_INVOICE_ID;
13:41:20 1040  	 END;
13:41:20 1041  
13:41:20 1042  	 var_invoice_refunds_sum := 0;
13:41:20 1043  	 var_invoice_charges_sum := 0;
13:41:20 1044  
13:41:20 1045  	 FOR f_charge IN (
13:41:20 1046  	   SELECT
13:41:20 1047  	     CHARGE.ID,
13:41:20 1048  	     CHARGE.CHARGE_STATUS_ID,
13:41:20 1049  	     CHARGE.CHARGE_AMOUNT,
13:41:20 1050  	     CHARGE.TRANSACTION_ID
13:41:20 1051  	   FROM
13:41:20 1052  	     CHARGE
13:41:20 1053  	   WHERE
13:41:20 1054  	     CHARGE.INVOICE_ID = in_invoice_id
13:41:20 1055  	 )
13:41:20 1056  	 LOOP
13:41:20 1057  	   -- If charge.status = canceled then continue
13:41:20 1058  	   IF f_charge.CHARGE_STATUS_ID != GLOBAL_STATUSES_V21.CHARGE_CANCELED THEN
13:41:20 1059  
13:41:20 1060  	     IF f_charge.CHARGE_AMOUNT > 0 THEN
13:41:20 1061  	       IF f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V21.CHARGE_PROCESSED
13:41:20 1062  		  AND PROCS_TRANSACTION_V21.IS_TRANSACTION_COLLECTED(f_charge.TRANSACTION_ID) = GLOBAL_CONSTANTS_V21.TRUE THEN
13:41:20 1063  		 -- Transaction collected
13:41:20 1064  		 var_invoice_charges_sum := var_invoice_charges_sum + f_charge.CHARGE_AMOUNT;
13:41:20 1065  	       ELSE
13:41:20 1066  		 -- Transaction is not collected. Do nothing
13:41:20 1067  		 NULL;
13:41:20 1068  	       END IF;
13:41:20 1069  	     ELSE
13:41:20 1070  	       IF f_charge.CHARGE_STATUS_ID = GLOBAL_STATUSES_V21.CHARGE_PROCESSED
13:41:20 1071  		  AND PROCS_TRANSACTION_V21.IS_TRANSACTION_COLLECTED(f_charge.TRANSACTION_ID) = GLOBAL_CONSTANTS_V21.FALSE THEN
13:41:20 1072  		 -- If charge is processed transaction is not collected then do nothing
13:41:20 1073  		 NULL;
13:41:20 1074  	       ELSE
13:41:20 1075  		 var_invoice_refunds_sum := var_invoice_refunds_sum + f_charge.CHARGE_AMOUNT;
13:41:20 1076  	       END IF;
13:41:20 1077  	     END IF;
13:41:20 1078  
13:41:20 1079  	   END IF;
13:41:20 1080  	 END LOOP;
13:41:20 1081  
13:41:20 1082  	 -- Refunds are negative
13:41:20 1083  	 var_invoice_refunds_sum := 0 - var_invoice_refunds_sum;
13:41:20 1084  
13:41:20 1085  	 out_amount := var_invoice_charges_sum - var_invoice_refunds_sum;
13:41:20 1086  
13:41:20 1087  EXCEPTION
13:41:20 1088  WHEN BAD_INVOICE_ID THEN
13:41:20 1089  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 1090  	   SPROC_NAME, 'No such invoice');
13:41:20 1091  WHEN OTHERS THEN
13:41:20 1092  	 PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 1093  	   SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 1094  END GET_MAX_REFUND;
13:41:20 1095  
