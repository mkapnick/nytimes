13:41:20 414  		  ss.CREATED_BY,
13:41:20 415  		  ss.CREATE_DATE,
13:41:20 416  		  ss.UPDATED_BY,
13:41:20 417  		  ss.UPDATE_DATE,
13:41:20 418  		  a.GROUP_ID AS BORROWER_GROUP_ID
13:41:20 419  		FROM
13:41:20 420  		  SUBSCRIPTION_SHARE ss,
13:41:20 421  		  ACCOUNT a,
13:41:20 422  		  LICENSE l,
13:41:20 423  		  GROUP_ACCOUNT ga
13:41:20 424  		WHERE
13:41:20 425  		  ss.GROUP_ACCOUNT_ID = in_group_account_id AND
13:41:20 426  		  ss.GROUP_ACCOUNT_ID = ga.ID AND
13:41:20 427  		  GA.SUBSCRIPTION_ID = l.SUBSCRIPTION_ID AND
13:41:20 428  		  SYSDATE BETWEEN l.START_DATE AND l.ENTITLEMENT_END_DATE AND
13:41:20 429  		  SYSDATE BETWEEN ss.START_DATE AND ss.END_DATE AND
13:41:20 430  		  ss.BORROWER_ACCOUNT_ID  = a.ID
13:41:20 431  	      ) Q
13:41:20 432  	    WHERE rownum <= l_end)
13:41:20 433  	  WHERE rnum >= l_start;
13:41:20 434  	END;
13:41:20 435  EXCEPTION
13:41:20 436  WHEN OTHERS THEN
13:41:20 437  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 438  	  SPROC_NAME, 'Unknown error while retrieving subscription shares', SQLERRM);
