13:41:20 682  PROCEDURE FREEZE_ACCOUNT (
13:41:20 683  	in_group_id   IN NUMBER,
13:41:20 684  	in_updated_by IN VARCHAR2,
13:41:20 685  	in_note       IN VARCHAR2,
13:41:20 686  	in_agent_id   IN NUMBER
13:41:20 687  ) AS
13:41:20 688  SPROC_NAME	    CONSTANT VARCHAR2(14) := 'FREEZE_ACCOUNT';
13:41:20 689  -- VARIABLES
13:41:20 690  var_account_id	    NUMBER;
13:41:20 691  var_account_status_id NUMBER;
13:41:20 692  -- EXCEPTIONS
13:41:20 693  BAD_GROUP_ID	    EXCEPTION;
13:41:20 694  BAD_ACCOUNT_STATUS_ID EXCEPTION;
13:41:20 695  CAN_NOT_CREATE_NOTE   EXCEPTION;
13:41:20 696  EXCEPTION_MESSAGE     VARCHAR2(1024);
13:41:20 697  BEGIN
13:41:20 698  
13:41:20 699  	-- Get account status, account id
13:41:20 700  	BEGIN
13:41:20 701  	  SELECT
13:41:20 702  	    ACCOUNT.ID,
13:41:20 703  	    ACCOUNT.ACCOUNT_STATUS_ID
13:41:20 704  	    into
13:41:20 705  	    var_account_id,
13:41:20 706  	    var_account_status_id
13:41:20 707  	  FROM
13:41:20 708  	    ACCOUNT
13:41:20 709  	  WHERE
13:41:20 710  	    ACCOUNT.GROUP_ID = in_group_id;
13:41:20 711  	  EXCEPTION
13:41:20 712  	    WHEN NO_DATA_FOUND THEN
13:41:20 713  	      RAISE BAD_GROUP_ID;
13:41:20 714  	END;
13:41:20 715  
13:41:20 716  	-- We can freeze only ACTIVE accounts
13:41:20 717  	IF var_account_status_id != GLOBAL_STATUSES_V21.ACCOUNT_ACTIVE
13:41:20 718  	  AND var_account_status_id != GLOBAL_STATUSES_V21.ACCOUNT_FROZEN THEN
13:41:20 719  	  RAISE BAD_ACCOUNT_STATUS_ID;
13:41:20 720  	END IF;
13:41:20 721  
13:41:20 722  	-- Set account status
13:41:20 723  	PROCS_ACCOUNT_V21.UPDATE_ACCOUNT_STATUS(
13:41:20 724  	  in_account_id        => var_account_id,
13:41:20 725  	  in_updated_by        => in_updated_by,
13:41:20 726  	  in_account_status_id => GLOBAL_STATUSES_V21.ACCOUNT_FROZEN
13:41:20 727  	);
13:41:20 728  
13:41:20 729  	-- Annotate account
13:41:20 730  	BEGIN
13:41:20 731  	  PROCS_ACCOUNT_V21.ANNOTATE_ACCOUNT(
13:41:20 732  	    in_group_id   => in_group_id,
13:41:20 733  	    in_agent_id   => in_agent_id,
13:41:20 734  	    in_note	  => in_note,
13:41:20 735  	    in_created_by => in_updated_by
13:41:20 736  	  );
13:41:20 737  	  EXCEPTION
13:41:20 738  	    WHEN OTHERS THEN
13:41:20 739  	      EXCEPTION_MESSAGE := SQLERRM;
13:41:20 740  	      RAISE CAN_NOT_CREATE_NOTE;
13:41:20 741  	END;
13:41:20 742  
13:41:20 743  EXCEPTION
13:41:20 744  WHEN BAD_GROUP_ID THEN
13:41:20 745  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.NOT_FOUND,
13:41:20 746  	  SPROC_NAME, 'No such group id');
13:41:20 747  WHEN BAD_ACCOUNT_STATUS_ID THEN
13:41:20 748  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.STATE_ERROR,
13:41:20 749  	  SPROC_NAME, 'Could not update this account. Status should to be active or frozen');
13:41:20 750  WHEN CAN_NOT_CREATE_NOTE THEN
13:41:20 751  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.INTERNAL_ERROR,
13:41:20 752  	  SPROC_NAME, 'Could not annotate account', EXCEPTION_MESSAGE);
13:41:20 753  WHEN OTHERS THEN
13:41:20 754  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 755  	  SPROC_NAME, 'Unknown error', SQLERRM);
13:41:20 756  END FREEZE_ACCOUNT;
