13:41:19 845  PROCEDURE GET_GRACE_LICE_FOR_FINAL_TRANS (
13:41:19 846  	in_days_before_close	 IN NUMBER,
13:41:19 847  	in_num_licenses_to_fetch IN NUMBER,
13:41:19 848  	out_result_set		 OUT SYS_REFCURSOR
13:41:19 849  ) AS
13:41:19 850  SPROC_NAME CONSTANT VARCHAR2(32) := 'GET_GRACE_LICE_FOR_FINAL_TRANS';
13:41:19 851  BEGIN
13:41:19 852  	OPEN out_result_set FOR
13:41:19 853  	SELECT
13:41:19 854  	    *
13:41:19 855  	FROM
13:41:19 856  	    (
13:41:19 857  		SELECT
13:41:19 858  		    l.id
13:41:19 859  		FROM
13:41:19 860  		    license l
13:41:19 861  		JOIN
13:41:19 862  		    invoice i
13:41:19 863  		ON
13:41:19 864  		    l.invoice_id = i.id
13:41:19 865  		WHERE
13:41:19 866  		    i.invoice_status_id = GLOBAL_STATUSES_V21.INVOICE_OPEN
13:41:19 867  		AND l.license_status_id = GLOBAL_STATUSES_V21.LICENSE_IN_GRACE_PERIOD
13:41:19 868  		AND SYSDATE + in_days_before_close >= l.grace_end_date
13:41:19 869  		AND NOT EXISTS
13:41:19 870  		    (
13:41:19 871  			SELECT
13:41:19 872  			    1
13:41:19 873  			FROM
13:41:19 874  			    charge c
13:41:19 875  			WHERE
13:41:19 876  			    c.invoice_id = i.id
13:41:19 877  			AND c.charge_status_id = GLOBAL_STATUSES_V21.CHARGE_OPENED)
13:41:19 878  		AND rownum <= in_num_licenses_to_fetch * 10
13:41:19 879  		ORDER BY
13:41:19 880  		    dbms_random.value)
13:41:19 881  	WHERE
13:41:19 882  	    rownum <= in_num_licenses_to_fetch;
13:41:19 883  END GET_GRACE_LICE_FOR_FINAL_TRANS;
