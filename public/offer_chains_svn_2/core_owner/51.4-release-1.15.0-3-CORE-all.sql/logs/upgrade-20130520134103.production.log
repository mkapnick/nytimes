13:41:20 635  range_diff NUMBER := 0;
13:41:20 636  upper_bond_diff NUMBER := 0;
13:41:20 637  l_start NUMBER := 0;
13:41:20 638  l_end   NUMBER := 0;
13:41:20 639  BEGIN
13:41:20 640  	--Normalize the end points [START]
13:41:20 641  	IF (in_start IS NULL OR in_start < 0) Then
13:41:20 642  	  l_start := 0;
13:41:20 643  	ELSE
13:41:20 644  	  l_start := in_start;
13:41:20 645  	END IF;
13:41:20 646  
13:41:20 647  	IF (in_end IS NULL) Then
13:41:20 648  	  l_end := 11;
13:41:20 649  	ELSE
13:41:20 650  	  l_end := in_end;
13:41:20 651  	END IF;
13:41:20 652  
13:41:20 653  	l_start := l_start + 1;
13:41:20 654  	l_end	:= l_end   + 1;
13:41:20 655  
13:41:20 656  	range_diff := l_end - l_start;
13:41:20 657  	upper_bond_diff :=  range_diff - GLOBAL_CONSTANTS_V21.MAX_RETURN_COUNT;
13:41:20 658  
13:41:20 659  	IF (upper_bond_diff > 0) Then
13:41:20 660  	  l_end := l_end - upper_bond_diff;
13:41:20 661  	END IF;
13:41:20 662  	--Normalize the end points [END]
13:41:20 663  
13:41:20 664  	--Total count of records [START]
13:41:20 665  	SELECT
13:41:20 666  	  COUNT(1) INTO out_record_count
13:41:20 667  	FROM
13:41:20 668  	  GROUP_ACCOUNT_EMAIL_DOMAIN
13:41:20 669  	WHERE
13:41:20 670  	  GROUP_ACCOUNT_ID = in_group_account_id AND
13:41:20 671  	  (IS_ACTIVE IS NULL OR IS_ACTIVE = in_status);
13:41:20 672  	--Total count of records [END]
13:41:20 673  
13:41:20 674  	OPEN out_result_set FOR
13:41:20 675  	SELECT
13:41:20 676  	  *
13:41:20 677  	FROM
13:41:20 678  	  (SELECT rownum rnum, q.*
13:41:20 679  	   FROM
13:41:20 680  	    (SELECT
13:41:20 681  	      ID,
13:41:20 682  	      GROUP_ACCOUNT_ID,
13:41:20 683  	      EMAIL_DOMAIN,
13:41:20 684  	      IS_ACTIVE
13:41:20 685  	     FROM
13:41:20 686  	       GROUP_ACCOUNT_EMAIL_DOMAIN
13:41:20 687  	     WHERE
13:41:20 688  	       GROUP_ACCOUNT_ID = in_group_account_id AND
13:41:20 689  	       (in_status IS NULL OR
13:41:20 690  		IS_ACTIVE = in_status)
13:41:20 691  	    ) Q
13:41:20 692  	  WHERE rownum <= l_end)
13:41:20 693  	WHERE rnum >= l_Start;
13:41:20 694  
13:41:20 695  EXCEPTION
13:41:20 696  WHEN OTHERS THEN
13:41:20 697  	PROCS_COMMON_V21.THROW_EXCEPTION(APP_EXCEPTION_CODES_V21.UNKNOWN_ERROR,
13:41:20 698  	  SPROC_NAME, 'Unknown error while retrieving Email Domains ', SQLERRM);
