13:41:20 1416  ) AS
13:41:20 1417  SPROC_NAME	       CONSTANT VARCHAR2(23) := 'REDEEM_GIFT_CERTIFICATE';
13:41:20 1418  -- VARIABLES
13:41:20 1419  var_gift_certificate_id NUMBER;
13:41:20 1420  -- norlov: #38151 var_offer_chain_id replaced by var_purchased_oc_id and var_oc_id_to_redeem
13:41:20 1421  var_purchased_oc_id     NUMBER;
13:41:20 1422  var_oc_id_to_redeem     NUMBER := in_redemption_offer_chain_id;
13:41:20 1423  var_offer_duration      VARCHAR2(30);
13:41:20 1424  var_invoice_id	       NUMBER;
13:41:20 1425  var_succ_purch_attempts_num NUMBER;
