13:41:20 1475  	     ACCOUNT
13:41:20 1476  	   WHERE
13:41:20 1477  	     ACCOUNT.GROUP_ID = in_group_id
13:41:20 1478  	     AND ROWNUM <= 1;
13:41:20 1479  
13:41:20 1480  	   EXCEPTION
13:41:20 1481  	   WHEN NO_DATA_FOUND THEN
13:41:20 1482  	     RAISE BAD_GROUP_ID;
13:41:20 1483  	 END;
13:41:20 1484  
13:41:20 1485  	 -- Get gift certificate data
13:41:20 1486  	 BEGIN
13:41:20 1487  	   SELECT
13:41:20 1488  	     GIFT_CERTIFICATE.ID,
13:41:20 1489  	     GIFT_CERTIFICATE.OFFER_CHAIN_ID,
13:41:20 1490  	     GIFT_CERTIFICATE.GIFT_CERTIFICATE_STATUS_ID,
13:41:20 1491  	     GIFT_CERTIFICATE.PURCHASER_GROUP_ID,
13:41:20 1492  	     GIFT_CERTIFICATE.EXPIRATION_DATE,
13:41:20 1493  	     GIFT_CERTIFICATE.REDEEMER_GROUP_ID,
13:41:20 1494  	     GIFT_CERTIFICATE.PURCHASE_INVOICE_ID
13:41:20 1495  	     into
13:41:20 1496  	     var_gift_certificate_id,
13:41:20 1497  	     var_purchased_oc_id,
13:41:20 1498  	     var_gc_status_id,
13:41:20 1499  	     var_purchaser_group_id,
13:41:20 1500  	     var_gc_expiration_date,
13:41:20 1501  	     var_gc_redeemer_group_id,
13:41:20 1502  	     var_gc_purchase_invoice_id
13:41:20 1503  	   FROM
13:41:20 1504  	     GIFT_CERTIFICATE
13:41:20 1505  	   WHERE
13:41:20 1506  	     GIFT_CERTIFICATE.CODE = in_gift_certificate_code
13:41:20 1507  	     AND ROWNUM <= 1;
13:41:20 1508  
13:41:20 1509  	   EXCEPTION
13:41:20 1510  	   WHEN NO_DATA_FOUND THEN
13:41:20 1511  	     RAISE BAD_GIFT_CERTIFICATE_CODE;
13:41:20 1512  	 END;
13:41:20 1513  
13:41:20 1514  	 -- get redemption oc id from meta data if it wasn't passed in, parsing will fail for gcs with multiple redemption offer chains,
13:41:20 1515  	 -- but in that case a redemption offer chain id should always be passed in
13:41:20 1516  	 IF var_oc_id_to_redeem IS NULL THEN
13:41:20 1517  	   BEGIN
13:41:20 1518  	     SELECT
13:41:20 1519  	       to_number(OFFER_CHAIN_META_DATA.VALUE)
13:41:20 1520  	       into
13:41:20 1521  	       var_oc_id_to_redeem
13:41:20 1522  	     FROM
13:41:20 1523  	       OFFER_CHAIN_META_DATA
13:41:20 1524  	     WHERE
13:41:20 1525  	       OFFER_CHAIN_META_DATA.OFFER_CHAIN_ID = var_purchased_oc_id
13:41:20 1526  	       AND OFFER_CHAIN_META_DATA.NAME = GLOBAL_CONSTANTS_V21.REDEMPTION_OC_ID
13:41:20 1527  	       AND ROWNUM = 1;
13:41:20 1528  -- requested by ticket so (but above is correct for the actual migrated data):
13:41:20 1529  --  SELECT
13:41:20 1530  --      OFFER_CHAIN.ID
13:41:20 1531  --      into
13:41:20 1532  --      var_oc_id_to_redeem
13:41:20 1533  --    FROM
13:41:20 1534  --      OFFER_CHAIN
13:41:20 1535  --	 INNER JOIN ELIGIBILITY ON OFFER_CHAIN.ID = ELIGIBILITY.OFFER_CHAIN_ID
13:41:20 1536  --	 INNER JOIN OFFER_CHAIN_META_DATA ON OFFER_CHAIN.ID = OFFER_CHAIN_META_DATA.OFFER_CHAIN_ID
13:41:20 1537  --    WHERE
13:41:20 1538  --      ELIGIBILITY.OFFER_CHAIN_ID = OFFER_CHAIN.ID
13:41:20 1539  --      AND ELIGIBILITY.NAME = GLOBAL_CONSTANTS_V21.GIFT_CERTIFICATE_REQUIRED
13:41:20 1540  --      AND ELIGIBILITY.VALUE = GLOBAL_CONSTANTS_V21.ELIGIBILITY_FLAG_SET
13:41:20 1541  --      AND OFFER_CHAIN_META_DATA.OFFER_CHAIN_ID = OFFER_CHAIN.ID
13:41:20 1542  --      AND OFFER_CHAIN_META_DATA.NAME = GLOBAL_CONSTANTS_V21.REDEMPTION_OC_ID
13:41:20 1543  --      AND to_number(OFFER_CHAIN_META_DATA.VALUE) = var_purchased_oc_id
13:41:20 1544  --      AND ROWNUM = 1;
13:41:20 1545  
13:41:20 1546  	     EXCEPTION
13:41:20 1547  	     WHEN NO_DATA_FOUND THEN
13:41:20 1548  	       RAISE OC_TO_REDEEM_NOT_FOUND;
13:41:20 1549  	   END;
13:41:20 1550  	 END IF;
13:41:20 1551  
13:41:20 1552  	 -- Check that purchase invoice for this GC was closed
13:41:20 1553  	 SELECT
13:41:20 1554  	   INVOICE.INVOICE_STATUS_ID into var_gc_purchase_inv_status_id
13:41:20 1555  	 FROM
13:41:20 1556  	   INVOICE
13:41:20 1557  	 WHERE
13:41:20 1558  	   INVOICE.ID = var_gc_purchase_invoice_id;
13:41:20 1559  
13:41:20 1560  	 IF var_gc_purchase_inv_status_id != GLOBAL_STATUSES_V21.INVOICE_CLOSED THEN
13:41:20 1561  	   RAISE GC_PURCHASE_INVOICE_NOT_CLOSED;
13:41:20 1562  	 END IF;
13:41:20 1563  
13:41:20 1564  	 -- Check that this invoice was successfully processed by billing
13:41:20 1565  	 SELECT
13:41:20 1566  	   COUNT(1) into var_succ_purch_attempts_num
13:41:20 1567  	 FROM
13:41:20 1568  	   TRANSACTION_ATTEMPT TA
13:41:20 1569  	   INNER JOIN TRANSACTION T ON T.ID = TA.TRANSACTION_ID
13:41:20 1570  	   INNER JOIN CHARGE CH ON CH.TRANSACTION_ID = T.ID
13:41:20 1571  	 WHERE
13:41:20 1572  	   CH.INVOICE_ID = var_gc_purchase_invoice_id
13:41:20 1573  	   AND TA.TRANSACTION_ATTEMPT_STATUS_ID = GLOBAL_STATUSES_V21.TRANS_ATTEMPT_SUCCESS;
13:41:20 1574  
13:41:20 1575  	 IF var_succ_purch_attempts_num = 0 THEN
13:41:20 1576  	   SELECT
13:41:20 1577  	     COUNT(1) into var_succ_purch_attempts_num
13:41:20 1578  	   FROM
13:41:20 1579  	     DUAL
13:41:20 1580  	   WHERE
13:41:20 1581  	     PROCS_INVOICE_V21.F_CALCULATE_INVOICE_AMOUNT(var_gc_purchase_invoice_id) = 0;
13:41:20 1582  	 END IF;
13:41:20 1583  
13:41:20 1584  	 IF var_succ_purch_attempts_num = 0 THEN
13:41:20 1585  	   RAISE PURCHASE_INVOICES_NOT_PAID;
13:41:20 1586  	 END IF;
13:41:20 1587  
13:41:20 1588  	 -- Check limit for gc's offer chain
13:41:20 1589  	 SELECT
13:41:20 1590  	   COUNT(*) into var_same_offer_chains_num
13:41:20 1591  	 FROM
13:41:20 1592  	   SUBSCRIPTION
13:41:20 1593  	 WHERE
13:41:20 1594  	   SUBSCRIPTION.ACCOUNT_ID = var_account_id
13:41:20 1595  	   AND SUBSCRIPTION.OFFER_CHAIN_ID = var_oc_id_to_redeem
13:41:20 1596  	   AND (
13:41:20 1597  	     SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V21.SUBSCRIPTION_ACTIVE
13:41:20 1598  	     OR SUBSCRIPTION.SUBSCRIPTION_STATUS_ID = GLOBAL_STATUSES_V21.SUBSCRIPTION_IN_GRACE_PERIOD);
13:41:20 1599  IF var_same_offer_chains_num = 0 THEN
13:41:20 1600  	   -- if user does not have any active existing subscriptions to the offer chain
13:41:20 1601  	   -- and if product from the offer chain is already owned from another offer chain
13:41:20 1602  	   -- then deny purchase
13:41:20 1603  	   FOR f_account_offer_chains IN (
