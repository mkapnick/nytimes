13:41:19 467  CAN_NOT_FIND_NEXT_OFFER EXCEPTION;
13:41:19 468  EXCEPTION_MESSAGE       VARCHAR2(1024);
13:41:19 469  BEGIN
13:41:19 470  
13:41:19 471  	BEGIN
13:41:19 472  	  SELECT
13:41:19 473  	    SUBSCRIPTION.OFFER_CHAIN_ID,
13:41:19 474  	    LICENSE.OFFER_ID,
13:41:19 475  	    LICENSE.CURRENT_OFFER_INDEX
13:41:19 476  	    into
13:41:19 477  	    var_offer_chain_id,
13:41:19 478  	    var_offer_id,
13:41:19 479  	    var_license_current_index
13:41:19 480  	  FROM
13:41:19 481  	    LICENSE
13:41:19 482  	    INNER JOIN SUBSCRIPTION ON LICENSE.SUBSCRIPTION_ID = SUBSCRIPTION.ID
13:41:19 483  	  WHERE
13:41:19 484  	    LICENSE.ID = in_license_id;
13:41:19 485  	  EXCEPTION
